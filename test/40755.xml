<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>40755</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Gysbert Japicx</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn siel dy macket <rhyme label="a" type="m">great</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Him, dy 't Al macke' uwt <rhyme label="a" type="m" corresp="#s1:l1">neat</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dy greate' oer-Heer der <rhyme label="b" type="f">Heeren</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Mijn gaest forhuwghlijck <rhyme label="c" type="m">weyd't</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Yn God mijn <rhyme label="c" type="m" corresp="#s1:l4">silligheyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">O greate' Heer' God, fol <rhyme label="b" type="f" corresp="#s1:l3">eeren</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">0m 't hy d'earm' steat, so <rhyme label="a" type="m">sljuecht</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Sijns tjienst-faems het <rhyme label="a" type="m" corresp="#s2:l1">beljuecht'</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Mey God-mijlde' eag <rhyme label="b" type="f">ontlitsen</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Want, sjea! fen nu <rhyme label="c" type="m">ford-oon</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wirt sillig mijn <rhyme label="c" type="m" corresp="#s2:l4">persoon</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Fen teamm' to teamme' <rhyme label="b" type="f" corresp="#s2:l3">uwt-spritsen</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O! dingen great in <rhyme label="a" type="m">schien</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Het dy kreft'ge' oon my <rhyme label="a" type="m" corresp="#s3:l1">dien</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">O! heylig iz sijn <rhyme label="b" type="f">Namme</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">Sijn frezers hy <rhyme label="c" type="m">to-seyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Sijn great' <rhyme label="c" type="m" corresp="#s3:l4">bermhertigheyt'</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gol uwt fen stamm' to <rhyme label="b" type="f" corresp="#s3:l3">stamme</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Yn wirck fen wichte' in <rhyme label="a" type="m">lest</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Het dien sijn kreft'ge <rhyme label="a" type="m" corresp="#s4:l1">fest</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Hy het forstouwne', ijnn' <rhyme label="b" type="f">smerte</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">d'Heag-moed'ge in all' herr' <rhyme label="c" type="m">dwaen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Yn lit-tincks great <rhyme label="c" type="m" corresp="#s4:l4">op-jaen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ynn' heag op-tochtme herr's <rhyme label="b" type="f" corresp="#s4:l3">herten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hy stiett' fen <rhyme label="a" type="m">troons-steat-stoell'</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dy kreftige' ijnne <rhyme label="a" type="m" corresp="#s5:l1">poell'</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Dy kliene hy <rhyme label="b" type="f">forheget</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Dy honger-h√¥lle <rhyme label="c" type="m">buwck</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Hy foll't mey goedz <rhyme label="c" type="m" corresp="#s5:l4">gebruwck</rhyme>.</l>
        <l id="s5:l6" met="3" type="-+">d'Rijck' gier-seck hy <rhyme label="b" type="f" corresp="#s5:l3">ontleget</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy rjuecht't Isr'el <rhyme label="a" type="m">oer-eyn'</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hy schinst him yv'ge <rhyme label="a" type="m" corresp="#s6:l1">seyn'</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">In sijn bermhert'ge <rhyme label="b" type="f">tinssen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Lijcke' hy Abra'm <rhyme label="c" type="m">to-sey</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">In Abra'ms teamm', dear <rhyme label="c" type="m" corresp="#s6:l4">ney</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">'t Sill'ge' yvig wol to <rhyme label="b" type="f" corresp="#s6:l3">schinssen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
