<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156698</id>
    <date>
      <year-start>1612</year-start>
      <year-end>1612</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Och lief en leed</melody>
  <author>J. Jacobsz. Harlingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHeluck voorspoedt <rhyme label="a" type="m">ghewin</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wenscht s'menschen hert en <rhyme label="a" type="m" corresp="#s1:l1">sin</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Na tijtlijck goet, ick vindet waer dat ick <rhyme label="a" type="m" corresp="#s1:l1">bin</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">D'een soecket met <rhyme label="b" type="m">ghebou</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Des ackers op het <rhyme label="b" type="m" corresp="#s1:l4">betrou</rhyme></l>
        <l id="s1:l6" met="5" type="-+">Dat Godt die Heer wasdom gheven <rhyme label="b" type="m" corresp="#s1:l4">sou</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Die ander die <rhyme label="c" type="m">vergaert</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Met die <rhyme label="c" type="m" corresp="#s1:l7">schipvaert</rhyme>;</l>
        <l id="s1:l9" met="2" type="-+">Die derde <rhyme label="d" type="m">sterck</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Doet een <rhyme label="d" type="m" corresp="#s1:l9">hand-werck</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Die vierde gaet het <rhyme label="e" type="m">spoen</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Om koopmanschap te <rhyme label="e" type="m" corresp="#s1:l11">doen</rhyme></l>
        <l id="s1:l13" met="3" type="-+">In menigherleye <rhyme label="e" type="m" corresp="#s1:l11">fatsoen</rhyme>,</l>
        <l id="s1:l14" met="4" type="+-">Sommighe heel <rhyme label="f" type="m">desolaet</rhyme>,</l>
        <l id="s1:l15" met="4" type="+-">Dit is hier des menschen <rhyme label="f" type="m" corresp="#s1:l14">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoort nu voort wat ick <rhyme label="a" type="m">sing</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De lichamelijcke <rhyme label="a" type="m" corresp="#s2:l1">oeffening</rhyme></l>
        <l id="s2:l3" met="5" type="-+">Is weynich nut die Godsalige tot alle <rhyme label="a" type="m" corresp="#s2:l1">ding</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dese duert <rhyme label="b" type="m">eewichlijck</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dander een <rhyme label="b" type="m" corresp="#s2:l4">ooghenblijck</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Ghelijck die mensch van aerde ende <rhyme label="b" type="m" corresp="#s2:l4">slijck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Nu seyt Gods <rhyme label="c" type="m">raet</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Ons <rhyme label="c" type="m" corresp="#s2:l7">toeverlaet</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Soeckt te <rhyme label="d" type="m">ghelijck</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Gods eewighe <rhyme label="d" type="m" corresp="#s2:l9">rijck</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">En sijn <rhyme label="e" type="m">gherechticheyt</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">So is u <rhyme label="e" type="m" corresp="#s2:l11">toegheseyt</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">V nootdruft spijs en <rhyme label="e" type="m" corresp="#s2:l11">cleyt</rhyme>,</l>
        <l id="s2:l14" met="4" type="+-">Van die <rhyme label="f" type="m">barmherticheyt</rhyme>,</l>
        <l id="s2:l15" met="4" type="+-">Prijst sijnen <rhyme label="f" type="m" corresp="#s2:l14">majesteyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Recht nu na Schrifts <rhyme label="a" type="m">accoort</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat weynich nut <rhyme label="a" type="m" corresp="#s3:l1">ghehoort</rhyme></l>
        <l id="s3:l3" met="5" type="-+">Heeft oock sijn plaets voort ende <rhyme label="a" type="m" corresp="#s3:l1">voort</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Genesis drie die <rhyme label="b" type="m">seyt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Adam is op <rhyme label="b" type="m" corresp="#s3:l4">gheleyt</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Sijn brootwinninghe met sweet en <rhyme label="b" type="m" corresp="#s3:l4">arbeyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Oock s'Heeren <rhyme label="c" type="m">raedt</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Actorum <rhyme label="c" type="m" corresp="#s3:l7">staet</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Met arbeyt <rhyme label="d" type="m">saen</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Moetmen <rhyme label="d" type="m" corresp="#s3:l9">ontfaen</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Die swacke Christen <rhyme label="e" type="m">leen</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Het zijn sijn eyghen <rhyme label="e" type="m" corresp="#s3:l11">reen</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">Oock saligher <rhyme label="e" type="m" corresp="#s3:l11">ghemeen</rhyme>,</l>
        <l id="s3:l14" met="4" type="+-">Datmen aelmissen <rhyme label="f" type="m">gheeft</rhyme>,</l>
        <l id="s3:l15" met="4" type="+-">Dan dat ghy daer af <rhyme label="f" type="m" corresp="#s3:l14">leeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Int arbeyt niet <rhyme label="a" type="m">vertoeft</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die traghe knecht <rhyme label="a" type="m" corresp="#s4:l1">begroeft</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Dat ghy meucht gheven die het recht <rhyme label="a" type="m" corresp="#s4:l1">behoeft</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die met arbeyden <rhyme label="b" type="m">wil</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Scheyt hem af met <rhyme label="b" type="m" corresp="#s4:l4">gheschil</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">In alle het doen met hem houdt u <rhyme label="b" type="m" corresp="#s4:l4">stil</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">So swaren <rhyme label="c" type="m">slach</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Gods recht <rhyme label="c" type="m" corresp="#s4:l7">vermach</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Datmen aen hem <rhyme label="d" type="m">doet</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Om hem tot <rhyme label="d" type="m" corresp="#s4:l9">boet</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Te brenghen met <rhyme label="e" type="m">ootmoet</rhyme>,,</l>
        <l id="s4:l12" met="3" type="-+">Sijnder handen werck hem <rhyme label="e" type="m" corresp="#s4:l11">voedt</rhyme></l>
        <l id="s4:l13" met="3" type="-+">En vercrijcht het eewich <rhyme label="e" type="m" corresp="#s4:l11">goet</rhyme>,</l>
        <l id="s4:l14" met="4" type="+-">Over sulcke vreuchde <rhyme label="f" type="m">blijck</rhyme>,</l>
        <l id="s4:l15" met="4" type="+-">By den Enghelen in Gods <rhyme label="f" type="m" corresp="#s4:l14">rijck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Elck merck nu met <rhyme label="a" type="m">bescheyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Tot watte <rhyme label="a" type="m" corresp="#s5:l1">neersticheyt</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Om voetsel decksel'men moet zijn <rhyme label="a" type="m" corresp="#s5:l1">bereyt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat oock die luye <rhyme label="b" type="m">moet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Derven t'gheselschap <rhyme label="b" type="m" corresp="#s5:l4">soet</rhyme></l>
        <l id="s5:l6" met="5" type="-+">Van Gods Heylighen, als die meest <rhyme label="b" type="m" corresp="#s5:l4">misdoet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Is doch <rhyme label="c" type="m">verhaelt</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Wie na t'rijck Gods <rhyme label="c" type="m" corresp="#s5:l7">taelt</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Gherechtich <rhyme label="d" type="m">leeft</rhyme></l>
        <l id="s5:l10" met="2" type="-+">God diet al <rhyme label="d" type="m" corresp="#s5:l9">gheeft</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Sal hem toeworpen <rhyme label="e" type="m">al</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Dat hy van doen zijn <rhyme label="e" type="m" corresp="#s5:l11">sal</rhyme></l>
        <l id="s5:l13" met="3" type="-+">Hier in dit jammer <rhyme label="e" type="m" corresp="#s5:l11">dal</rhyme>,</l>
        <l id="s5:l14" met="4" type="+-">En een eewighen <rhyme label="f" type="m">croon</rhyme></l>
        <l id="s5:l15" met="4" type="+-">Boven in s'Hemels <rhyme label="f" type="m" corresp="#s5:l14">troon</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die lelyen siet hoe <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Zy staen in 't veldt ten <rhyme label="a" type="m" corresp="#s6:l1">toon</rhyme></l>
        <l id="s6:l3" met="5" type="-+">Sonder arbeyden, spinnen oock niet om <rhyme label="a" type="m" corresp="#s6:l1">loon</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Die voghelkens hebben <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Kelders noch schueren <rhyme label="b" type="m" corresp="#s6:l4">yet</rhyme></l>
        <l id="s6:l6" met="5" type="-+">Ons Hemelsche Vader die voetse sonder <rhyme label="b" type="m" corresp="#s6:l4">verdriet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Hoe veel meer <rhyme label="c" type="m">u</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Sorchvuldicheyt <rhyme label="c" type="m" corresp="#s6:l7">schu</rhyme>?</l>
        <l id="s6:l9" met="2" type="-+">Nae hooghen <rhyme label="d" type="m">staet</rhyme></l>
        <l id="s6:l10" met="2" type="-+">Oock niet en <rhyme label="d" type="m" corresp="#s6:l9">gaet</rhyme></l>
        <l id="s6:l11" met="3" type="+-">Daer nae die Heydenen <rhyme label="e" type="m">staen</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Maer wy moeten 't <rhyme label="e" type="m" corresp="#s6:l11">versmaen</rhyme></l>
        <l id="s6:l13" met="3" type="-+">Want het sal doch <rhyme label="e" type="m" corresp="#s6:l11">vergaen</rhyme>,</l>
        <l id="s6:l14" met="4" type="+-">'t Hemels zy u <rhyme label="f" type="m">verlang</rhyme></l>
        <l id="s6:l15" met="4" type="+-">O eeuwich is soo <rhyme label="f" type="m" corresp="#s6:l14">lang</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Tot besluyt van dit <rhyme label="a" type="m">liedt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Allen arbeydt <rhyme label="a" type="m" corresp="#s7:l1">gheschiedt</rhyme></l>
        <l id="s7:l3" met="5" type="-+">Om tytelijck goedt, is altemael doch <rhyme label="a" type="m" corresp="#s7:l1">niet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dit heeft noch naem noch <rhyme label="b" type="m">daet</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Sorchvuldicheydt gheeft <rhyme label="b" type="m" corresp="#s7:l4">schaet</rhyme>,</l>
        <l id="s7:l6" met="5" type="-+">'t Wordt ons toe worpen uyt enckel <rhyme label="b" type="m" corresp="#s7:l4">ghenaet</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Tot het eeuwighe <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Moet dat <rhyme label="c" type="m" corresp="#s7:l7">ghemoet</rhyme></l>
        <l id="s7:l9" met="2" type="-+">Met raet en <rhyme label="d" type="m">kracht</rhyme></l>
        <l id="s7:l10" met="2" type="-+">Der Schrift worden <rhyme label="d" type="m" corresp="#s7:l9">ghebracht</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Te loopen sonder <rhyme label="e" type="m">aflaet</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Nae die voorghestelde <rhyme label="e" type="m" corresp="#s7:l11">maet</rhyme></l>
        <l id="s7:l13" met="3" type="-+">Beyde vroech ende <rhyme label="e" type="m" corresp="#s7:l11">spaet</rhyme>,</l>
        <l id="s7:l14" met="4" type="+-">En Godt alleen <rhyme label="f" type="m">wijs</rhyme></l>
        <l id="s7:l15" met="4" type="+-">Altijdt gheven den <rhyme label="f" type="m" corresp="#s7:l14">prijs</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Prince u gratie <rhyme label="a" type="m">gheeft</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die gheerne nae u Woort <rhyme label="a" type="m" corresp="#s8:l1">leeft</rhyme></l>
        <l id="s8:l3" met="5" type="-+">Dat zy int' scheyden troost ende blyschap <rhyme label="a" type="m" corresp="#s8:l1">heeft</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Princesse Christus <rhyme label="b" type="m">Bruyt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Voeght u nae dit <rhyme label="b" type="m" corresp="#s8:l4">gheluyt</rhyme>,</l>
        <l id="s8:l6" met="5" type="-+">Insonderheyt wiens naem dit liedt wijst <rhyme label="b" type="m" corresp="#s8:l4">uyt</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">Wt desen <rhyme label="c" type="m">strijdt</rhyme></l>
        <l id="s8:l8" met="2" type="-+">Van swackheydt <rhyme label="c" type="m" corresp="#s8:l7">bevrijdt</rhyme></l>
        <l id="s8:l9" met="2" type="-+">Sullen wy te <rhyme label="d" type="m">gaer</rhyme></l>
        <l id="s8:l10" met="2" type="-+">By ons Hemelsche <rhyme label="d" type="m" corresp="#s8:l9">Vaer</rhyme></l>
        <l id="s8:l11" met="3" type="+-">Singhen het nieuwe <rhyme label="e" type="m">liet</rhyme></l>
        <l id="s8:l12" met="3" type="-+">Daer Esdras van <rhyme label="e" type="m" corresp="#s8:l11">bediet</rhyme>,</l>
        <l id="s8:l13" met="3" type="-+">Apocalypsis <rhyme label="e" type="m" corresp="#s8:l11">siet</rhyme>,</l>
        <l id="s8:l14" met="4" type="+-">Mijn inwendich <rhyme label="f" type="m">ghemoet</rhyme></l>
        <l id="s8:l15" met="4" type="+-">My vrolijck singhen <rhyme label="f" type="m" corresp="#s8:l14">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
