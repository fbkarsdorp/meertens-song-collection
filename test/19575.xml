        <l id="s1:l1" met="3" type="-+">O! Schoone, die my <rhyme label="a" type="m">maakt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van mijnen zlaap <rhyme label="a" type="m" corresp="#s1:l1">ontwaakt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Daar ik in zluimde; <rhyme label="b" type="m">toen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Ik deeze klagt, an u, quam <rhyme label="b" type="m" corresp="#s1:l3">doen</rhyme>.</l>
