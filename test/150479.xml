    <melody>Psalm 081 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">ZEgen-rijck <rhyme label="a" type="m">geslacht</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Singt met volle <rhyme label="b" type="f">keelen</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Iacobs Godt, ons <rhyme label="a" type="m" corresp="#s1:l1">kracht</rhyme>.</l>
        <l id="s1:l4" met="3" type="+-">Vry, tijdt aent <rhyme label="c" type="m">brommen</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Met luyt en <rhyme label="c" type="m" corresp="#s1:l4">trommen</rhyme></l>
        <l id="s1:l6" met="3" type="+-">En ’tsoet <rhyme label="b" type="f" corresp="#s1:l2">harpen-spelen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Laet trompetten <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Op gesette <rhyme label="b" type="f">stonden</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Van de nieuwe <rhyme label="a" type="m" corresp="#s2:l1">Maen</rhyme>:</l>
        <l id="s2:l4" met="3" type="+-">Het zijn <rhyme label="c" type="m">geboden</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Daer aen van <rhyme label="c" type="m" corresp="#s2:l4">Gode</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Isr’el is <rhyme label="b" type="f" corresp="#s2:l2">verbonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Op dat dese <rhyme label="a" type="m">dagh</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Alle jaer <rhyme label="b" type="f">verbreyde</rhyme></l>
        <l id="s3:l3" met="3" type="+-">’tGeen dat doe <rhyme label="a" type="m" corresp="#s3:l1">geschach</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Doe Godt door <rhyme label="c" type="m">krachten</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Abrams <rhyme label="c" type="m" corresp="#s3:l4">geslachten</rhyme></l>
        <l id="s3:l6" met="3" type="+-">uyt Egypten <rhyme label="b" type="f" corresp="#s3:l2">leyde</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Daer sy (spreeckt de <rhyme label="a" type="m">Heer</rhyme>)</l>
        <l id="s4:l2" met="3" type="+-">’sVollicks tael niet <rhyme label="b" type="f">kenden</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">Ick Godt vrijde <rhyme label="a" type="m" corresp="#s4:l1">weer</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Haer rugg’ en <rhyme label="c" type="m">handen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Van last en <rhyme label="c" type="m" corresp="#s4:l4">banden</rhyme>:</l>
        <l id="s4:l6" met="3" type="+-">’tSlaven was ten <rhyme label="b" type="f" corresp="#s4:l2">enden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Doe ghy riept, mijn <rhyme label="a" type="m">volck</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Quam ick in’t <rhyme label="b" type="f">bedroeven</rhyme></l>
        <l id="s5:l3" met="3" type="+-">uyt een <rhyme label="a" type="m" corresp="#s5:l1">donder-wolck</rhyme></l>
        <l id="s5:l4" met="3" type="+-">V antwoordt <rhyme label="c" type="m">geven</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">’kGing u daer <rhyme label="c" type="m" corresp="#s5:l4">neven</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Aen den twist-stroom <rhyme label="b" type="f" corresp="#s5:l2">proeven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Nu, mijn volck, tree <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Vaet mijns mondts <rhyme label="b" type="f">betuygen</rhyme>.</l>
        <l id="s6:l3" met="3" type="+-">Soo ghy my maer <rhyme label="a" type="m" corresp="#s6:l1">hoort</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">En tot <rhyme label="c" type="m">Af-goden</rhyme></l>
        <l id="s6:l5" met="3" type="+-">’tHert niet laet <rhyme label="c" type="m" corresp="#s6:l4">nooden</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Om de knien te <rhyme label="b" type="f" corresp="#s6:l2">buygen</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Soo ghy my <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Kent voor Heer der <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">My, die (op uw’ <rhyme label="a" type="m" corresp="#s7:l1">be’en</rhyme>)</l>
        <l id="s7:l4" met="3" type="+-">Door <rhyme label="c" type="m">wonderheden</rhyme></l>
        <l id="s7:l5" met="3" type="+-">uyt Pharoos <rhyme label="c" type="m" corresp="#s7:l4">Steden</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Vwen voet de’e <rhyme label="b" type="f" corresp="#s7:l2">keeren</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Doet u mondt dan <rhyme label="a" type="m">op</rhyme></l>
        <l id="s8:l2" met="3" type="+-">En ghy krijght uw’ <rhyme label="b" type="f">wenschen</rhyme>.</l>
        <l id="s8:l3" met="3" type="+-">’tHelpt niet wat ick <rhyme label="a" type="m" corresp="#s8:l1">klop</rhyme>.</l>
        <l id="s8:l4" met="3" type="+-">’tVolck wil niet <rhyme label="c" type="m">hooren</rhyme>:</l>
        <l id="s8:l5" met="3" type="+-">Moeyt wert <rhyme label="c" type="m" corresp="#s8:l4">verloren</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Aen veraerde <rhyme label="b" type="f" corresp="#s8:l2">menschen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">’kNam my haers niet <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s9:l2" met="3" type="+-">En in herten <rhyme label="b" type="f">hardtheydt</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Liet icks’ henen <rhyme label="a" type="m" corresp="#s9:l1">gaen</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Na eygen <rhyme label="c" type="m">rade</rhyme>.</l>
        <l id="s9:l5" met="3" type="+-">Wee, die in’t <rhyme label="c" type="m" corresp="#s9:l4">quade</rhyme></l>
        <l id="s9:l6" met="3" type="+-">Al te diep verwart <rhyme label="b" type="f" corresp="#s9:l2">leyt</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Ach! had <rhyme label="a" type="m">Israel</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Na my willen <rhyme label="b" type="f">vragen</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">En mijns wets <rhyme label="a" type="m" corresp="#s10:l1">bevel</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Niet had <rhyme label="c" type="m">verlaten</rhyme></l>
        <l id="s10:l5" met="3" type="+-">’kHad die hem <rhyme label="c" type="m" corresp="#s10:l4">haten</rhyme></l>
        <l id="s10:l6" met="3" type="+-">All’ seer haest <rhyme label="b" type="f" corresp="#s10:l2">verslagen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Die Godt vyandt <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Souden met veel <rhyme label="b" type="f">schroomen</rhyme></l>
        <l id="s11:l3" met="3" type="+-">In geveynsden <rhyme label="a" type="m" corresp="#s11:l1">schijn</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Om my te <rhyme label="c" type="m">eeren</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">Den Heer der <rhyme label="c" type="m" corresp="#s11:l4">Heeren</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">Hebben moeten <rhyme label="b" type="f" corresp="#s11:l2">komen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">’kHad haer tijdt <rhyme label="a" type="m">gereckt</rhyme>.</l>
        <l id="s12:l2" met="3" type="+-">En het merch van’t <rhyme label="b" type="f">koren</rhyme></l>
        <l id="s12:l3" met="3" type="+-">Hun ter spijs <rhyme label="a" type="m" corresp="#s12:l1">verstreckt</rhyme>:</l>
        <l id="s12:l4" met="3" type="+-">’kHad haer gaen <rhyme label="c" type="m">voeden</rhyme></l>
        <l id="s12:l5" met="3" type="+-">Met <rhyme label="c" type="m" corresp="#s12:l4">honich-vloeden</rhyme></l>
        <l id="s12:l6" met="3" type="+-">uyt een steen <rhyme label="b" type="f" corresp="#s12:l2">geboren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
