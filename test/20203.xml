    <melody>Windje daar het bos af drilt</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">A	Ls Iupijns kroost uyt Thetis <rhyme label="a" type="m">schoot</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">d' Aerd sijn kruyn, en straelen <rhyme label="a" type="m" corresp="#s1:l1">bood</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">En quam <rhyme label="b" type="f">naderen</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Aen de <rhyme label="b" type="f" corresp="#s1:l3">Bladeren</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En die koele <rhyme label="c" type="m">Water-bron</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">By 't Graf van sijn Soon <rhyme label="c" type="m" corresp="#s1:l5">PhaÃ«ton</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sach ik uyt 't West ten ren-strijd <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Kindt met Boog, en Pijl <rhyme label="a" type="m" corresp="#s2:l1">gela'en</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">En sijn <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Die de <rhyme label="b" type="f" corresp="#s2:l3">Hoeder</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Op den Berreg Ida <rhyme label="c" type="m">gaf</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Paerlen-krans voor Herders <rhyme label="c" type="m" corresp="#s2:l5">staf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'k Boog my ter aerden voor die <rhyme label="a" type="m">pronk</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Schoon my Iupijn Nectar <rhyme label="a" type="m" corresp="#s3:l1">schonk</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Sprak ik, 't <rhyme label="b" type="f">bloosen</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Van uw <rhyme label="b" type="f" corresp="#s3:l3">Roosen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Soud' my eer tot volgen <rhyme label="c" type="m">no'on</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dan al de pracht of gunst der <rhyme label="c" type="m" corresp="#s3:l5">Go'on</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gun my doch weder sulk een <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dat ik we'er genieten <rhyme label="a" type="m" corresp="#s4:l1">mag</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Van te <rhyme label="b" type="f">naken</rhyme></l>
        <l id="s4:l4" met="2" type="+-">Mond, en <rhyme label="b" type="f" corresp="#s4:l3">kaken</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Van die over-schoone <rhyme label="c" type="m">Maegt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die mijne vryheyt droeg, en <rhyme label="c" type="m" corresp="#s4:l5">draegt</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want, ach! die uur wanneer ik 't <rhyme label="a" type="m">licht</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Derf, van 't flonk'rend <rhyme label="a" type="m" corresp="#s5:l1">aengesicht</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">En haer <rhyme label="b" type="f">stralen</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Soo <rhyme label="b" type="f" corresp="#s5:l3">ontdalen</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Al mijn krachten, en ik <rhyme label="c" type="m">schijn</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Meerder mijn selven niet te <rhyme label="c" type="m" corresp="#s5:l5">zijn</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">'k Sweer, 'k sal uw bede gunstig <rhyme label="a" type="m">zijn</rhyme>:</l>
        <l id="s6:l2" met="4" type="+-">Vlecht u hooft met <rhyme label="a" type="m" corresp="#s6:l1">Roos'marijn</rhyme>.</l>
        <l id="s6:l3" met="2" type="+-">Uwe <rhyme label="b" type="f">Minne</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Sal <rhyme label="b" type="f" corresp="#s6:l3">verwinne</rhyme></l>
        <l id="s6:l5" met="4" type="+-">'t Hert, waer op u boesem <rhyme label="c" type="m">docht</rhyme>:</l>
        <l id="s6:l6" met="4" type="+-">Och! of die tijt eens wesen <rhyme label="c" type="m" corresp="#s6:l5">mocht</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
