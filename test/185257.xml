    <tags/>
    <melody>Een amoureus fier gelaat</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Liefste <rhyme label="a" type="f">uytvercoren</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Aenhoort mijn soete <rhyme label="b" type="m">stem</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Ick houd’ u staegh te <rhyme label="a" type="f" corresp="#s1:l1">voren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Het nieu <rhyme label="b" type="m" corresp="#s1:l2">Ierusalem</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Aensiet mijn lieff’lick <rhyme label="c" type="f">nooden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat uyt mijn minne <rhyme label="d" type="m">vloeyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Och staet op van den <rhyme label="c" type="f" corresp="#s1:l5">dooden</rhyme>!</l>
        <l id="s1:l8" met="3" type="-+">Daer ghy in ligt <rhyme label="d" type="m" corresp="#s1:l6">vermoeyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Bruydegom <rhyme label="a" type="f">verheven</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Die door u <rhyme label="b" type="m">goedicheyt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">My ‘t leven hebt <rhyme label="a" type="f" corresp="#s2:l1">gegeven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En met u hant <rhyme label="b" type="m" corresp="#s2:l2">bereyt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Al woud’ ick van u <rhyme label="c" type="f">wijcken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">‘k En voel in my geen <rhyme label="d" type="m">rust</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Hoe wel des werelts <rhyme label="c" type="f" corresp="#s2:l5">rijcken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Beloven soete <rhyme label="d" type="m" corresp="#s2:l6">lust</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">‘k En sal u niet <rhyme label="a" type="f">verlaten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Waer ghy u keert of <rhyme label="b" type="m">went</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wilt ghy myn straffingh <rhyme label="a" type="f" corresp="#s3:l1">haten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Soo comt ghy in <rhyme label="b" type="m" corresp="#s3:l2">ellent</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Geen vreucht en sult ghy <rhyme label="c" type="f">vinden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Buyten mijn <rhyme label="d" type="m">minlickheyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ghy sult u selfs <rhyme label="c" type="f" corresp="#s3:l5">verslinden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Volght ghy u <rhyme label="d" type="m" corresp="#s3:l6">sinlickheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dat can ick wel <rhyme label="a" type="f">bevoelen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als ick uyy dwasen <rhyme label="b" type="m">waen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Na liep verscheyden <rhyme label="a" type="f" corresp="#s4:l1">boelen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">‘k Vond my altijt <rhyme label="b" type="m" corresp="#s4:l2">verraen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">‘t Scheen wel in <rhyme label="c" type="f">d’ydelheden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat alle dinck was <rhyme label="d" type="m">wel</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Maer in stilt en in <rhyme label="c" type="f" corresp="#s4:l5">vreden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Voelde ‘t gemoet <rhyme label="d" type="m" corresp="#s4:l6">ghequel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoe soud’ ick dat <rhyme label="a" type="f">ghedooghen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Te sien mijn <rhyme label="b" type="m">evenbeelt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Verdorven voor mijn <rhyme label="a" type="f" corresp="#s5:l1">ooghen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat ick selfs heb <rhyme label="b" type="m" corresp="#s5:l2">geteelt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">K’en heb mijn eygen <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Noch noyt so lief <rhyme label="d" type="m">gehadt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Als u om u te <rhyme label="c" type="f" corresp="#s5:l5">geven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Mijn alderhoochste <rhyme label="d" type="m" corresp="#s5:l6">schat</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick ben wel <rhyme label="a" type="f">overtuyghet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van al u goetheyt, <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De reden my wel <rhyme label="a" type="f" corresp="#s6:l1">buyghet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Waer ick my wend oft <rhyme label="b" type="m" corresp="#s6:l2">keer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">De schepsels dit oock <rhyme label="c" type="f">spreken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Beneffens mijn <rhyme label="d" type="m">ghemoet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">De schrift hier van <rhyme label="c" type="f" corresp="#s6:l5">ontsteken</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Is als een minne <rhyme label="d" type="m" corresp="#s6:l6">gloet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Och ick heb sulck <rhyme label="a" type="f">vermaken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Te woonen in u <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Kost ghy mijn liefde <rhyme label="a" type="f" corresp="#s7:l1">smaken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ghy voelde pijn noch <rhyme label="b" type="m" corresp="#s7:l2">smert</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Smert sou geen smerte <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Vermidts de groote <rhyme label="d" type="m">vreucht</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Mijn soetheyt sou <rhyme label="c" type="f" corresp="#s7:l5">ghenesen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Al ‘s werelts <rhyme label="d" type="m" corresp="#s7:l6">ongheneucht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Och by ghebreck van <rhyme label="a" type="f">desen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Vind ick my dick <rhyme label="b" type="m">ontroert</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Veel dinghen doen my <rhyme label="a" type="f" corresp="#s8:l1">vreesen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Mijn sinnen <rhyme label="b" type="m" corresp="#s8:l2">omgevoert</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Van anghst en noot <rhyme label="c" type="f">bestreden</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Loopen nu hier, nu <rhyme label="d" type="m">daer</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Soeckende rust en <rhyme label="c" type="f" corresp="#s8:l5">vreden</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">En d’onrust is in <rhyme label="d" type="m" corresp="#s8:l6">haer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ghy moet u neerstigh <rhyme label="a" type="f">wachten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Als d’onrust u comt <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Te volghen u <rhyme label="a" type="f" corresp="#s9:l1">ghedachten</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Maer denckt altijt op <rhyme label="b" type="m" corresp="#s9:l2">my</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">In stilte sijn en <rhyme label="c" type="f">hopen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">V cracht gheleghen <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Mijn gracy staet u <rhyme label="c" type="f" corresp="#s9:l5">open</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Als ghyse niet loopt <rhyme label="d" type="m" corresp="#s9:l6">mis</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dat heb ick wel <rhyme label="a" type="f">bevonden</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Dat door u Liefde <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Ghenesen ‘s herten <rhyme label="a" type="f" corresp="#s10:l1">wonden</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En d’onrust van ‘t <rhyme label="b" type="m" corresp="#s10:l2">ghemoed</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Och kond ick daer by <rhyme label="c" type="f">blyven</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En hiel mijn sinnen <rhyme label="d" type="m">stil</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">En liet de werelt <rhyme label="c" type="f" corresp="#s10:l5">dryven</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Ghelijckse dryven <rhyme label="d" type="m" corresp="#s10:l6">wil</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Het geen u can <rhyme label="a" type="f">ghenesen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Is oock elcks <rhyme label="b" type="m">medecijn</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Verderfs oorsaack moet <rhyme label="a" type="f" corresp="#s11:l1">wesen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Dat sy niet achten <rhyme label="b" type="m" corresp="#s11:l2">mijn</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Can ick haer niet <rhyme label="c" type="f">verwinnen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Door noodingh in het <rhyme label="d" type="m">hert</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Hoe souden dan u <rhyme label="c" type="f" corresp="#s11:l5">sinnen</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Ghenesen hunne <rhyme label="d" type="m" corresp="#s11:l6">smert</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ghedachten niet en <rhyme label="a" type="f">baten</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Sy brengen quelling <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Maer die u Liefde <rhyme label="a" type="f" corresp="#s12:l1">vaten</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Door d’indruck van u <rhyme label="b" type="m" corresp="#s12:l2">woort</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Verblyden in ‘t <rhyme label="c" type="f">gheloove</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Vereenen in ‘t <rhyme label="d" type="m">ghebedt</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">En connen soo <rhyme label="c" type="f" corresp="#s12:l5">verdooven</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Al wat hun hier <rhyme label="d" type="m" corresp="#s12:l6">belet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Laet dan ghestadigh <rhyme label="a" type="f">blyven</rhyme></l>
        <l id="s13:l2" met="3" type="-+">In u mijn out <rhyme label="b" type="m">verbont</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">’k Wil in u herte <rhyme label="a" type="f" corresp="#s13:l1">schrijven</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Mijn aldersoetste <rhyme label="b" type="m" corresp="#s13:l2">gront</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Leeft daer mijn soet <rhyme label="c" type="f">vereenen</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">‘t Welck reyckt in <rhyme label="d" type="m">d’eeuwigheyt</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">’t Geen voormaels was in <rhyme label="c" type="f" corresp="#s13:l5">steenen</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Is nu in ‘t hert <rhyme label="d" type="m" corresp="#s13:l6">gheleyt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Den smaack van uwe <rhyme label="a" type="f">minne</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">‘t Bevinden van u <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s14:l3" met="3" type="-+">‘t Welck ghy u lief <rhyme label="a" type="f" corresp="#s14:l1">Vriendinne</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Plant in het herte <rhyme label="b" type="m" corresp="#s14:l2">soet</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Treckt haer boven <rhyme label="c" type="f">vermoghen</rhyme></l>
        <l id="s14:l6" met="3" type="-+">In u, o eeuwigh <rhyme label="d" type="m">licht</rhyme>!</l>
        <l id="s14:l7" met="3" type="-+">Dat sy blijft <rhyme label="c" type="f" corresp="#s14:l5">ingetogen</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">En lieffelijck <rhyme label="d" type="m" corresp="#s14:l6">ghesticht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
