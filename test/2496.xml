<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2496</id>
    <date>
      <year-start>1622</year-start>
      <year-end>1622</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Des morgens voor dag in het veld al bij de weg</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>G.A. Bredero</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Maar siet! sy <rhyme label="a" type="m">sluyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ach Lief, wilt soo niet <rhyme label="b" type="f">sluyten</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Siet eerst eens <rhyme label="a" type="m" corresp="#s1:l1">uyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wie dat ghy hier laet <rhyme label="b" type="f" corresp="#s1:l2">buyten</rhyme></l>
        <l id="s1:l5" met="2" type="+-">U deurtjen <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Compt fleurtjen <rhyme label="c" type="m" corresp="#s1:l5">aan</rhyme></l>
        <l id="s1:l7" met="1" type="-+">Aen 't <rhyme label="d" type="f">scheurtjen</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">Een <rhyme label="d" type="f" corresp="#s1:l7">eurtjen</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Aant deurtjen <rhyme label="c" type="m" corresp="#s1:l5">staen</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Aenhoort mijn kleyn <rhyme label="c" type="m" corresp="#s1:l5">vermaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ach! wildy <rhyme label="a" type="m">niet</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">En ist dan u <rhyme label="b" type="f">behaghen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Dat ick 't <rhyme label="a" type="m" corresp="#s2:l1">verdriet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Hier op de straet moet <rhyme label="b" type="f" corresp="#s2:l2">klaghen</rhyme>?</l>
        <l id="s2:l5" met="2" type="+-">Voor nijers <rhyme label="c" type="m">boos</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Voor vrijers <rhyme label="c" type="m" corresp="#s2:l5">loos</rhyme>,</l>
        <l id="s2:l7" met="1" type="-+">Noch <rhyme label="d" type="f">wijder</rhyme>,</l>
        <l id="s2:l8" met="1" type="-+">Voor <rhyme label="d" type="f" corresp="#s2:l7">yder</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Voor vrijers <rhyme label="c" type="m" corresp="#s2:l5">loos</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Of yemant die my <rhyme label="c" type="m" corresp="#s2:l5">siet</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ick gaf een <rhyme label="a" type="m">klop</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met vreesen en met <rhyme label="b" type="f">hopen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">En seyd doet <rhyme label="a" type="m" corresp="#s3:l1">op</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sy seyd' ick doe niet <rhyme label="b" type="f" corresp="#s3:l2">open</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Wy sorghen // <rhyme label="c" type="m">seer</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Koomt morghen // <rhyme label="c" type="m" corresp="#s3:l5">weer</rhyme>,</l>
        <l id="s3:l7" met="1" type="-+">Wilt <rhyme label="d" type="f">borghen</rhyme></l>
        <l id="s3:l8" met="1" type="-+">Tot <rhyme label="d" type="f" corresp="#s3:l7">morghen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Komt morghen <rhyme label="c" type="m" corresp="#s3:l5">weer</rhyme></l>
        <l id="s3:l10" met="3" type="-+">En eyscht dan u <rhyme label="c" type="m" corresp="#s3:l5">begeer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ick bent, mijn <rhyme label="a" type="m">roem</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">Mijn waerde <rhyme label="b" type="f">uytghelesen</rhyme>!</l>
        <l id="s4:l3" met="2" type="-+">Die ick schier <rhyme label="a" type="m" corresp="#s4:l1">noem</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Mijn eygen hart te <rhyme label="b" type="f" corresp="#s4:l2">wesen</rhyme>;</l>
        <l id="s4:l5" met="2" type="+-">Sy vluchten <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Ick stichten <rhyme label="c" type="m" corresp="#s4:l5">doe</rhyme>,</l>
        <l id="s4:l7" met="1" type="-+">Sy <rhyme label="d" type="f">vruchte</rhyme>,</l>
        <l id="s4:l8" met="1" type="-+">Ick <rhyme label="d" type="f" corresp="#s4:l7">duchte</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Ick suchte <rhyme label="c" type="m" corresp="#s4:l5">doe</rhyme>:</l>
        <l id="s4:l10" met="3" type="-+">Hoe wee was ick te <rhyme label="c" type="m" corresp="#s4:l5">moe</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">'k En kan <rhyme label="a" type="m">voorwaar</rhyme></l>
        <l id="s5:l2" met="3" type="-+">De heele nacht niet <rhyme label="b" type="f">rusten</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Maar dacht om <rhyme label="a" type="m" corresp="#s5:l1">haar</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En veel verscheyden <rhyme label="b" type="f" corresp="#s5:l2">lusten</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Temteerden // <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Het deerden // <rhyme label="c" type="m" corresp="#s5:l5">my</rhyme></l>
        <l id="s5:l7" met="1" type="-+">'t <rhyme label="d" type="f">Ontbeerde</rhyme>,</l>
        <l id="s5:l8" met="1" type="-+">
          <rhyme label="d" type="f" corresp="#s5:l7">Begeerde</rhyme>
        </l>
        <l id="s5:l9" met="2" type="+-">Dies keerd' ick <rhyme label="c" type="m" corresp="#s5:l5">my</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Van d'een op d'ander <rhyme label="c" type="m" corresp="#s5:l5">sy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Wel op mijn <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En wilt niet langher <rhyme label="b" type="f">treuren</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Mach voor u <rhyme label="a" type="m" corresp="#s6:l1">smart</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Haer jonst u niet <rhyme label="b" type="f" corresp="#s6:l2">ghebeuren</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Soo laatse // <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Maer praatse // <rhyme label="c" type="m" corresp="#s6:l5">wel</rhyme>,</l>
        <l id="s6:l7" met="1" type="-+">Soo <rhyme label="d" type="f">staatse</rhyme>,</l>
        <l id="s6:l8" met="1" type="-+">Ter <rhyme label="d" type="f" corresp="#s6:l7">plaatse</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-">Soo staatse <rhyme label="c" type="m" corresp="#s6:l5">wel</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Daermen haer eeren <rhyme label="c" type="m" corresp="#s6:l5">sel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Princesjen <rhyme label="a" type="m">eel</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">Als ick by u quam <rhyme label="b" type="f">praaten</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Wast een te <rhyme label="a" type="m" corresp="#s7:l1">veel</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Daerom sal ick u <rhyme label="b" type="f" corresp="#s7:l2">laten</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Ghy maacktet // <rhyme label="c" type="m">trots</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Ghy spraacktet // <rhyme label="c" type="m" corresp="#s7:l5">schots</rhyme></l>
        <l id="s7:l7" met="1" type="-+">Ick <rhyme label="d" type="f">laacket</rhyme>,</l>
        <l id="s7:l8" met="1" type="-+">En <rhyme label="d" type="f" corresp="#s7:l7">staacket</rhyme>,</l>
        <l id="s7:l9" met="2" type="+-">Ghy maacket <rhyme label="c" type="m" corresp="#s7:l5">trots</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">'t Hadt seker niet veel <rhyme label="c" type="m" corresp="#s7:l5">slots</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
