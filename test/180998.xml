    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Gulde <rhyme label="a" type="m">vryigheydt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat is de braefste <rhyme label="a" type="m" corresp="#s1:l1">Meyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van allen <rhyme label="b" type="f">uytghelesen</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Want sy leeft vrolijck en <rhyme label="c" type="m">gherust</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Na haer volkomen eygen <rhyme label="c" type="m" corresp="#s1:l4">lust</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">Dees is dan waert <rhyme label="b" type="f" corresp="#s1:l3">ghepresen</rhyme>.</l>
      </lg>
    </lg>
