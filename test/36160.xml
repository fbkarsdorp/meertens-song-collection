  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">DEn Heer der <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Den Schepper, ende Godt van <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Mensch t' uwer <rhyme label="a" type="f" corresp="#s1:l1">eeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En tot troost in dit traenen <rhyme label="b" type="m" corresp="#s1:l2">dal</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Heeft eenen maeltheydt gaen <rhyme label="c" type="f">oprichten</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Om uwe <rhyme label="d" type="m">smert</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Door sijn goedt <rhyme label="d" type="m" corresp="#s1:l6">hert</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Gansch te <rhyme label="c" type="f" corresp="#s1:l5">verlichten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">O wonder <rhyme label="a" type="f">Feeste</rhyme>!</l>
        <l id="s2:l2" met="4" type="-+">O on-uyt-sprekelyck <rhyme label="b" type="m">bancket</rhyme>!</l>
        <l id="s2:l3" met="2" type="-+">Loopt al om <rhyme label="a" type="f" corresp="#s2:l1">'tmeeste</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Siet wat op taeffel wordt <rhyme label="b" type="m" corresp="#s2:l2">gheset</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Het is Godt self om u te <rhyme label="c" type="f">stercken</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Onder den <rhyme label="d" type="m">schyn</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Van Broot, en <rhyme label="d" type="m" corresp="#s2:l6">wyn</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">O werck der <rhyme label="c" type="f" corresp="#s2:l5">wercken</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Syn Vleesch tot <rhyme label="a" type="f">spyse</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Syn Heyligh bloedt tot uwen <rhyme label="b" type="m">dranck</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Niet voor een <rhyme label="a" type="f" corresp="#s3:l1">reyse</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maer voor geheel uw' leven <rhyme label="b" type="m" corresp="#s3:l2">lanck</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Seer mildelijck comt hy <rhyme label="c" type="f">uyt-rycken</rhyme>:</l>
        <l id="s3:l6" met="2" type="-+">Hy is van <rhyme label="d" type="m">sin</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Tot u sijn <rhyme label="d" type="m" corresp="#s3:l6">min</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Hier doen te <rhyme label="c" type="f" corresp="#s3:l5">blycken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wie Gast comt <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Op dit Bancket met reyn <rhyme label="b" type="m">ghemoet</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Die wordt <rhyme label="a" type="f" corresp="#s4:l1">ghenesen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Van al syn quael, en soo <rhyme label="b" type="m" corresp="#s4:l2">ghevoedt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dat hy in d'eeuwigheydt sal <rhyme label="c" type="f">leven</rhyme>.</l>
        <l id="s4:l6" met="2" type="-+">Van d'helsche <rhyme label="d" type="m">pyn</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Bevrydt te <rhyme label="d" type="m" corresp="#s4:l6">zyn</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Sal Godt hem <rhyme label="c" type="f" corresp="#s4:l5">gheven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Wilt dan <rhyme label="a" type="f">aentrecken</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Wie dat ghy zyt uw' Bruyloft <rhyme label="b" type="m">kleet</rhyme></l>
        <l id="s5:l3" met="2" type="-+">'t Welck met gheen <rhyme label="a" type="f" corresp="#s5:l1">vleken</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Besmeurt magh wesen soo ghy <rhyme label="b" type="m" corresp="#s5:l2">weet</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Comt tot dees Feest Godt sal u <rhyme label="c" type="f">toeven</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Gheheel Ã´ <rhyme label="d" type="m">mensch</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Naer uwen <rhyme label="d" type="m" corresp="#s5:l6">wensch</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">En u <rhyme label="c" type="f" corresp="#s5:l5">behoeven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
