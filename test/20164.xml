  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">‘t V	Issen moet wel vreugde <rhyme label="a" type="f">baeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In de Troetel-swanger <rhyme label="b" type="m">schoot</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Want daer eertijts Baersjes <rhyme label="a" type="f" corresp="#s1:l1">waren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Geeft sy Posjes uyt haer <rhyme label="b" type="m" corresp="#s1:l2">Sloot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ach! waer vintmen soeter <rhyme label="a" type="f">Visje</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Als een Posje, bruyn, en <rhyme label="b" type="m">vet</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Holla tong! u reede <rhyme label="a" type="f" corresp="#s2:l1">misje</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Soeter vintmen wel in ‘t <rhyme label="b" type="m" corresp="#s2:l2">Bet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Meysje hebje ook <rhyme label="a" type="f">verlangen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Om eens mee te Vissen <rhyme label="b" type="m">gaen</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Gaet gy sult wel Posjes <rhyme label="a" type="f" corresp="#s3:l1">vangen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Wilt het aes aen d’Angel <rhyme label="b" type="m" corresp="#s3:l2">slaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
