<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156078</id>
    <date>
      <year-start>1700</year-start>
      <year-end>1700</year-end>
    </date>
    <tags/>
    <melody/>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Als Febus haar strale op 't Aertrijk <rhyme label="a" type="f">dalen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dan vreester den goude <rhyme label="b" type="m">Vorst</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Den tyd verslyt door storm en <rhyme label="c" type="f">vlagen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">het Water wort als een <rhyme label="b" type="m" corresp="#s1:l2">korst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">November December October <rhyme label="a" type="f">September</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dan kom ter sneeuw hagel en <rhyme label="b" type="m">Wint</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Den tijt verslyt door winterse <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Eer datter de somer <rhyme label="b" type="m" corresp="#s2:l2">begint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Siet Flora eens koomen met al Haer <rhyme label="a" type="f">boomen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En al haer min lyk <rhyme label="b" type="m">kruyt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Beminde Klarinde den tijdt sal <rhyme label="c" type="f">vinden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">om haest wese een <rhyme label="b" type="m" corresp="#s3:l2">Bruijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Diana heeft alle haer honde <rhyme label="a" type="f">ontbonden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Spionon brack hase de <rhyme label="b" type="m">wint</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">De maegd die jaegd door bossen en <rhyme label="c" type="f">wegen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">So lang als zy 't wilt rat <rhyme label="b" type="m" corresp="#s4:l2">vindt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ey sieter de beesten eens Singen en <rhyme label="a" type="f">springen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Door 't Snaren en soet <rhyme label="b" type="m">geluijt</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Diana kom aen met Bassen en <rhyme label="c" type="f">Veele</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Diana al met haer <rhyme label="b" type="m" corresp="#s5:l2">vluyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Diana laet gaen al u Bassen en <rhyme label="a" type="f">veelen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En alle u soet <rhyme label="b" type="m">geluyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Komt hier met wildt, Beminde <rhyme label="c" type="f">Klarinde</rhyme></l>
        <l id="s6:l4" met="3" type="-+">ick wilder gaen scheyden <rhyme label="b" type="m" corresp="#s6:l2">uyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
