<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183116</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb de groene straten</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Geluk <rhyme label="a" type="m">Oranje-Boom</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">En Liebanonne <rhyme label="b" type="f">bloeije</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">Zoo ziet men <rhyme label="a" type="m" corresp="#s1:l1">wederom</rhyme></l>
        <l id="s1:l4" met="3" type="-+">De Oranje Stam weer <rhyme label="b" type="f" corresp="#s1:l2">groeije</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s1:l5">Oranje-Spruit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vivat <rhyme label="a" type="m">Oranje-Vorst</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">Nu wort gy weer <rhyme label="b" type="f">verheeven</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Die u stoot op de <rhyme label="a" type="m" corresp="#s2:l1">borst</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die zullen nu weer <rhyme label="b" type="f" corresp="#s2:l2">sneeven</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s2:l5">Oranje-Spruit</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laet ons nu vrolijk <rhyme label="a" type="m">zijn</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">Oranje is <rhyme label="b" type="f">verreezen</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">Ons Land is uit de <rhyme label="a" type="m" corresp="#s3:l1">pijn</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Wilt nu maer vrolijk <rhyme label="b" type="f" corresp="#s3:l2">weezen</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s3:l5">Oranje-Spruit</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dit is een wonder <rhyme label="a" type="m">heil</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">Een onverwagte <rhyme label="b" type="f">zeegen</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Gy hebt dog weer uw <rhyme label="a" type="m" corresp="#s4:l1">regt</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">Door Godes hand <rhyme label="b" type="f" corresp="#s4:l2">verkreegen</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s4:l5">Oranje-Spruit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dus wense wy <rhyme label="a" type="m">geluk</rhyme> /</l>
        <l id="s5:l2" met="3" type="-+">Op deeze blyde <rhyme label="b" type="f">daegen</rhyme> /</l>
        <l id="s5:l3" met="3" type="-+">Om dat de Oranje <rhyme label="a" type="m" corresp="#s5:l1">Stam</rhyme> /</l>
        <l id="s5:l4" met="3" type="-+">Staet vast in Godts <rhyme label="b" type="f" corresp="#s5:l2">behaegen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s5:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s5:l5">Oranje-Spruit</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy scheen geheel <rhyme label="a" type="m">berooft</rhyme> /</l>
        <l id="s6:l2" met="3" type="-+">Van ieder een <rhyme label="b" type="f">verstooten</rhyme> /</l>
        <l id="s6:l3" met="3" type="-+">Nu is hy in het <rhyme label="a" type="m" corresp="#s6:l1">hert</rhyme> /</l>
        <l id="s6:l4" met="3" type="-+">Van ieder een <rhyme label="b" type="f" corresp="#s6:l2">geslooten</rhyme> /</l>
        <l id="s6:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s6:l6" met="3" type="-+">Vivat Oranje <rhyme label="c" type="m" corresp="#s6:l5">Spruit</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Gy waert onlangs <rhyme label="a" type="m">gedreigt</rhyme> /</l>
        <l id="s7:l2" met="3" type="-+">Met zekre <rhyme label="b" type="f">ongevalle</rhyme> /</l>
        <l id="s7:l3" met="3" type="-+">Waer van wy in 't <rhyme label="a" type="m" corresp="#s7:l1">geheel</rhyme> /</l>
        <l id="s7:l4" met="3" type="-+">Ook nu niet spreeken <rhyme label="b" type="f" corresp="#s7:l2">zallen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s7:l6" met="3" type="-+">Vivat Oranje <rhyme label="c" type="m" corresp="#s7:l5">Spruit</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Komt Vrinden weest <rhyme label="a" type="m">verheugt</rhyme>/</l>
        <l id="s8:l2" met="3" type="-+">Laet ons Oranje <rhyme label="b" type="f">zingen</rhyme> /</l>
        <l id="s8:l3" met="3" type="-+">En dus met nieuwe <rhyme label="a" type="m" corresp="#s8:l1">vreugt</rhyme> /</l>
        <l id="s8:l4" met="3" type="-+">Van vreugde ook <rhyme label="b" type="f" corresp="#s8:l2">opspringen</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s8:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s8:l5">Oranje-Spruit</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Komt hier dog menschen <rhyme label="a" type="m">al</rhyme>/</l>
        <l id="s9:l2" met="3" type="-+">Laet ons tot vreede <rhyme label="b" type="f">keeren</rhyme> /</l>
        <l id="s9:l3" met="3" type="-+">En laet ons onze <rhyme label="a" type="m" corresp="#s9:l1">Prins</rhyme> /</l>
        <l id="s9:l4" met="3" type="-+">Met zingen ook <rhyme label="b" type="f" corresp="#s9:l2">vereeren</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Laet ons nu zingen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s9:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s9:l5">Oranje-Spruit</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ons Vorst is op den <rhyme label="a" type="m">Troon</rhyme>/</l>
        <l id="s10:l2" met="3" type="-+">Laet ons hem nu <rhyme label="b" type="f">vereeren</rhyme> /</l>
        <l id="s10:l3" met="3" type="-+">En met een deugdsaem <rhyme label="a" type="m" corresp="#s10:l1">hert</rhyme> /</l>
        <l id="s10:l4" met="3" type="-+">Tot hem ons weder <rhyme label="b" type="f" corresp="#s10:l2">keeren</rhyme> /</l>
        <l id="s10:l5" met="3" type="-+">Laet ons nu roepen <rhyme label="c" type="m">luit</rhyme> /</l>
        <l id="s10:l6" met="3" type="-+">Vivat <rhyme label="c" type="m" corresp="#s10:l5">Oranje-Spruit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
