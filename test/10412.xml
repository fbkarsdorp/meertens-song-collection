<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>10412</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De stille nacht genaakt</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VRaecht Venus ghy <rhyme label="a" type="m">godin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Niet selver nae de <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die syt der minne <rhyme label="b" type="f">voeder</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">De schalcke <rhyme label="c" type="m">Cupido</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Meynt ghy is veel te <rhyme label="c" type="m" corresp="#s1:l4">bloo</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En vreest syn lieve <rhyme label="b" type="f" corresp="#s1:l3">moeder</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Neen t' boefjes gayle <rhyme label="a" type="m">lust</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Moet al soo wel <rhyme label="a" type="m" corresp="#s2:l1">geblust</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Syn inde hooge <rhyme label="b" type="f">steeden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als in het laech ge <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Te meer verwinning <rhyme label="c" type="m" corresp="#s2:l4">doet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hy in geachte <rhyme label="b" type="f" corresp="#s2:l3">leeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een hooch verheeven <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hy soo wel onder <rhyme label="a" type="m" corresp="#s3:l1">gaet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En weet dat sy <rhyme label="b" type="f">behoeven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De dorsticheyt van <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die hy haer blaset <rhyme label="c" type="m" corresp="#s3:l4">in</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En schencktse eens te <rhyme label="b" type="f" corresp="#s3:l3">proeven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De dartele Nymphe <rhyme label="a" type="m">licht</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die t' vierich <rhyme label="a" type="m" corresp="#s4:l1">aengesicht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dees goude sons <rhyme label="b" type="f">verschuylen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Achter de schadu <rhyme label="c" type="m">milt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Vant bruyne bossen <rhyme label="c" type="m" corresp="#s4:l4">wilt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Staeg om de minne <rhyme label="b" type="f" corresp="#s4:l3">pruylen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De harder in het <rhyme label="a" type="m">velt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">VVeet van syn soet <rhyme label="a" type="m" corresp="#s5:l1">gewelt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Soo wel als elck te <rhyme label="b" type="f">spreeken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De harderinne <rhyme label="c" type="m">bly</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Dencken vast weeten <rhyme label="c" type="m" corresp="#s5:l4">wy</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Syn lodderlycke <rhyme label="b" type="f" corresp="#s5:l3">treeken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Met vrolyckheytjes <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hy jder een vol <rhyme label="a" type="m" corresp="#s6:l1">doet</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Doch naer veel guyere <rhyme label="b" type="f">vlagen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Belooft hy veel <rhyme label="c" type="m">genot</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Of meent ghy dat hy <rhyme label="c" type="m" corresp="#s6:l4">spot</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">In schyn van min te <rhyme label="b" type="f" corresp="#s6:l3">dragen</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
