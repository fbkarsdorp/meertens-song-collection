  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Liefde zal ons <rhyme label="a" type="f">Leiden</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Den weg <rhyme label="a" type="f" corresp="#s1:l1">bereiden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En ons haar werk <rhyme label="b" type="f">beduiden</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Tot <rhyme label="c" type="m">meen'gerly</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Of 't voor ons tyd te <rhyme label="d" type="f">stryden</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Of 't Rust-dag <rhyme label="c" type="m" corresp="#s1:l4">zy</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Wy zien bereids van <rhyme label="d" type="f" corresp="#s1:l5">wyden</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En trap en <rhyme label="d" type="f" corresp="#s1:l5">tyden</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Van 't zaalig <rhyme label="d" type="f" corresp="#s1:l5">ziels-verblyden</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Maar trouw! maar <rhyme label="e" type="m">trouw</rhyme>!</l>
      </lg>
    </lg>
  </text>
