<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35677</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Als ik uit wandelen ga</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">OCh, dat mijn hart en <rhyme label="a" type="m">tongh</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O Heer op desen <rhyme label="b" type="m">dagh</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Tot uwen lof <rhyme label="a" type="m" corresp="#s1:l1">ontsprongh</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">Geeft my nu dat ick <rhyme label="b" type="m" corresp="#s1:l2">magh</rhyme></l>
        <l id="s1:l5" met="6" type="-+">Looven na uwen wil, een deughdelijcke <rhyme label="c" type="m">Maeght</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die u, die u, die <rhyme label="d" type="m">u</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Seer oprecht heeft <rhyme label="c" type="m" corresp="#s1:l5">behaeght</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Emerens is haer <rhyme label="a" type="m">Naem</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Tot Roomen <rhyme label="b" type="m">voort-ghebracht</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Venhuysen hare <rhyme label="a" type="m" corresp="#s2:l1">Faem</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Eer, ende Feest groot <rhyme label="b" type="m" corresp="#s2:l2">acht</rhyme>.</l>
        <l id="s2:l5" met="6" type="-+">Patroners van haer Kerck, verkoren is <rhyme label="c" type="m">aldaer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Laet ons, laet ons, laet <rhyme label="d" type="m">ons</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Singen de lof van <rhyme label="c" type="m" corresp="#s2:l5">haer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sint Agniet in haer <rhyme label="a" type="m">Ieught</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een Maget seer <rhyme label="b" type="m">vermaert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Genegen tot de <rhyme label="a" type="m" corresp="#s3:l1">deught</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met yver heeft <rhyme label="b" type="m" corresp="#s3:l2">gebaert</rhyme></l>
        <l id="s3:l5" met="6" type="-+">In Emerentiaen, kennis van Godts <rhyme label="c" type="m">Geloof</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">s' Hoort toe, s' hoort toe, 's hoort <rhyme label="d" type="m">toe</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Sy was by haer niet <rhyme label="c" type="m" corresp="#s3:l5">doof</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">By Agniet langh te <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Aen haer niet is <rhyme label="b" type="m">gegunt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Want sy haest raeckt in <rhyme label="a" type="m" corresp="#s4:l1">pijn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En doodt betaelt naer <rhyme label="b" type="m" corresp="#s4:l2">munt</rhyme></l>
        <l id="s4:l5" met="6" type="-+">Door hare pijn, en doodt, gheraeckt tot 's Hemels <rhyme label="c" type="m">Throon</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Want sy, want sy, want <rhyme label="d" type="m">sy</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Waerdigh was sulcken <rhyme label="c" type="m" corresp="#s4:l5">loon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Haer Lichaem raeckt in 't <rhyme label="a" type="m">gracht</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Veel Menschen daer by <rhyme label="b" type="m">zijn</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">En daer wordt by <rhyme label="a" type="m" corresp="#s5:l1">gewacht</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">D' oorsaeck nu komt van <rhyme label="b" type="m" corresp="#s5:l2">pijn</rhyme>:</l>
        <l id="s5:l5" met="6" type="-+">De Boeven quaemen aen, en dat met groot <rhyme label="c" type="m">gewelt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Seer boos, seer boos, seer <rhyme label="d" type="m">boos</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En heftigh seer <rhyme label="c" type="m" corresp="#s5:l5">ontstelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">'t Volck bidt by Agniets <rhyme label="a" type="m">gracht</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">Aensiet nu wie is <rhyme label="b" type="m">trouw</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">Emerens spreeckt met <rhyme label="a" type="m" corresp="#s6:l1">kracht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Soo dat het scheen geen <rhyme label="b" type="m" corresp="#s6:l2">Vrouw</rhyme>,</l>
        <l id="s6:l5" met="6" type="-+">Maer als een kloecken heldt, die gewapent is <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hoort nu, hoort nu, hoort <rhyme label="d" type="m">nu</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Sy spreeckt en niemandt <rhyme label="c" type="m" corresp="#s6:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ach! Menschen heel <rhyme label="a" type="m">godloos</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ghy plaeght de Christ'nen <rhyme label="b" type="m">seer</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Met steenen wreed en <rhyme label="a" type="m" corresp="#s7:l1">boos</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Beroovers van Godts <rhyme label="b" type="m" corresp="#s7:l2">eer</rhyme>,</l>
        <l id="s7:l5" met="6" type="-+">Ghy quetst, ghy wondt, ja doodt, die dienen haeren <rhyme label="c" type="m">Godt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Houdt op, houdt op, houdt <rhyme label="d" type="m">op</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Of d'Hel sal zijn uw' <rhyme label="c" type="m" corresp="#s7:l5">lot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Haer Lichaem jongh en <rhyme label="a" type="m">teer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ontfanght geen <rhyme label="b" type="m">hagel-buyl</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">Maer steenen tot Godts <rhyme label="a" type="m" corresp="#s8:l1">eer</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Van Menschen wreed en <rhyme label="b" type="m" corresp="#s8:l2">vuyl</rhyme>.</l>
        <l id="s8:l5" met="6" type="-+">Sy als een Stephanus, ontfanght de steenen <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dit was, dit was, dit <rhyme label="d" type="m">was</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Haer glorieuse <rhyme label="c" type="m" corresp="#s8:l5">doodt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Tiberius in sijn <rhyme label="a" type="m">tijdt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Heeft gesien een sterck <rhyme label="b" type="m">glas</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Niet breeckt, waer mee men <rhyme label="a" type="m" corresp="#s9:l1">smijt</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Emerens oock soo <rhyme label="b" type="m" corresp="#s9:l2">was</rhyme>,</l>
        <l id="s9:l5" met="6" type="-+">Al was sy jongh en teer, als glas steenen niet <rhyme label="c" type="m">acht</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Want haer, want haer, want <rhyme label="d" type="m">haer</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Iesus versterckt met <rhyme label="c" type="m" corresp="#s9:l5">kracht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Door d' Ouders van <rhyme label="a" type="m">Agniet</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Haer Lichaem d'aerden <rhyme label="b" type="m">heeft</rhyme>;</l>
        <l id="s10:l3" met="3" type="-+">Maer doch haer ziel <rhyme label="a" type="m" corresp="#s10:l1">geniet</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dat sy nu eeuwigh <rhyme label="b" type="m" corresp="#s10:l2">leeft</rhyme>:</l>
        <l id="s10:l5" met="6" type="-+">Ontfanght twee Kroonen schoon, in des Hemelsche <rhyme label="c" type="m">Throon</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Van wie? van wie? van <rhyme label="d" type="m">wie</rhyme>?</l>
        <l id="s10:l7" met="3" type="-+">Van Iesus Davids <rhyme label="c" type="m" corresp="#s10:l5">Soon</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Hoort noch Godts wercken <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Den Heer nu wordt <rhyme label="b" type="m">verstoort</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">Veele slaet hy ter <rhyme label="a" type="m" corresp="#s11:l1">doodt</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Die haer hadden <rhyme label="b" type="m" corresp="#s11:l2">vermoordt</rhyme>:</l>
        <l id="s11:l5" met="6" type="-+">Op desen het dondert, en d'aerde scheurt en <rhyme label="c" type="m">beeft</rhyme>.</l>
        <l id="s11:l6" met="3" type="-+">Haet niet, haet niet, haet <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Die wel en deughdigh <rhyme label="c" type="m" corresp="#s11:l5">leeft</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Venhuysers uwe <rhyme label="a" type="m">Geest</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Bevrijt van alle <rhyme label="b" type="m">sondt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Viert altijdt hare <rhyme label="a" type="m" corresp="#s12:l1">Feest</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Volght hare deughd' <rhyme label="b" type="m" corresp="#s12:l2">terstondt</rhyme>.</l>
        <l id="s12:l5" met="6" type="-+">Vraeght ghy wat sal ick doen, om haer te volgen <rhyme label="c" type="m">nae</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Hoort toe, hoort toe, hoort <rhyme label="d" type="m">toe</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Ick sal 't u leeren <rhyme label="c" type="m" corresp="#s12:l5">drae</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Quae Menschen met haer <rhyme label="a" type="m">schout</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Voor 't Geloof strijdt, en <rhyme label="b" type="m">lijdt</rhyme>;</l>
        <l id="s13:l3" met="3" type="-+">Waerheydt spreeckt en <rhyme label="a" type="m" corresp="#s13:l1">behoudt</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Sonden verfoeyt en <rhyme label="b" type="m" corresp="#s13:l2">mijdt</rhyme>:</l>
        <l id="s13:l5" met="6" type="-+">Volght voorts haer leven na, gelijck het u <rhyme label="c" type="m">betaemt</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Dan soo, dan soo, dan <rhyme label="d" type="m">soo</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Ghy sult niet staen <rhyme label="c" type="m" corresp="#s13:l5">beschaemt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Geeft Iesu, waerden <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Aen allen hier <rhyme label="b" type="m">vergaert</rhyme>:</l>
        <l id="s14:l3" met="3" type="-+">Dat wy om gelt noch <rhyme label="a" type="m" corresp="#s14:l1">eer</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Om Steenen ofte <rhyme label="b" type="m" corresp="#s14:l2">Swaerdt</rhyme>,</l>
        <l id="s14:l5" met="6" type="-+">Verlaten 't Roomsch Geloof, en dat men 't wel <rhyme label="c" type="m">beleeft</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Niet een, niet een, niet <rhyme label="d" type="m">een</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Sonder dat d'Hemel <rhyme label="c" type="m" corresp="#s14:l5">heeft</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Iesu! die alles <rhyme label="a" type="m">geeft</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">O Iesu, godes <rhyme label="b" type="m">Soon</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Iesu, die alles <rhyme label="a" type="m" corresp="#s15:l1">heeft</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Geeft ons, u Hemels <rhyme label="b" type="m" corresp="#s15:l2">Throon</rhyme>:</l>
        <l id="s15:l5" met="6" type="-+">Alwaer blijdtschap sal zijn, die dueren sal <rhyme label="c" type="m">altijdt</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Maeckt ons, maeckt ons, maeckt <rhyme label="d" type="m">ons</rhyme></l>
        <l id="s15:l7" met="3" type="-+">Hier door eeuwigh <rhyme label="c" type="m" corresp="#s15:l5">verblijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
