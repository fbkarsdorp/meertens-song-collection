<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178385</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>O zielsverrukte avondstond</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wie zou niet met een blyde <rhyme label="a" type="m">geest</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Nu zingen in het <rhyme label="b" type="m">rond</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Op Willem Fredriks <rhyme label="a" type="m" corresp="#s1:l1">vreugdefeest</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Van zyn geboorte <rhyme label="b" type="m" corresp="#s1:l2">stond</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Nederlanders zyn <rhyme label="a" type="m">verblyd</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zy wenschen op dit <rhyme label="b" type="m">stond</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Veel zeegen op dees blyde <rhyme label="a" type="m" corresp="#s2:l1">tyd</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Verheugd met hart en <rhyme label="b" type="m" corresp="#s2:l2">mond</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">ô Nassauws Telg, ô edel' <rhyme label="a" type="m">Stam</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ons Vorst zyn <rhyme label="b" type="m">Naamgenoot</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hoe wensch'lyk was het toe gy <rhyme label="a" type="m" corresp="#s3:l1">kwam</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">In Neêrlands liefde <rhyme label="b" type="m" corresp="#s3:l2">schoot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Leef lang, ô Vorst! nog menig <rhyme label="a" type="m">jaar</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Tot nut voor Kerk en <rhyme label="b" type="m">Staat</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat wysheid, magt, zig zamen <rhyme label="a" type="m" corresp="#s4:l1">paar</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">ô Neêrlands <rhyme label="b" type="m" corresp="#s4:l2">toeverlaat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Blyft gy beschut door Neêrlands <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In deeze droeve <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Welhaast vlugt weg dat schand'lyk <rhyme label="a" type="m" corresp="#s5:l1">rot</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat hoonen, en die <rhyme label="b" type="m" corresp="#s5:l2">nyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dan blyft Oranjes Huis in <rhyme label="a" type="m">stand</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Tot lust voor groot en <rhyme label="b" type="m">kleen</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Den zegen op het <rhyme label="a" type="m" corresp="#s6:l1">Vaderland</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En liefde in 't <rhyme label="b" type="m" corresp="#s6:l2">gemeen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Schenk vreede Heer door uw <rhyme label="a" type="m">bestuur</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Geef eendragts liefde <rhyme label="b" type="m">band</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dan blyft die Neêrlands <rhyme label="a" type="m" corresp="#s7:l1">Palinuur</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">De hoop van 't gantsche <rhyme label="b" type="m" corresp="#s7:l2">Land</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Bewaar nog lang dit Vorst'lyk <rhyme label="a" type="m">Kroost</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Bevreid van ramp en <rhyme label="b" type="m">druk</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Dan strekt dit Huis tot heil en <rhyme label="a" type="m" corresp="#s8:l1">troost</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En Nederlands <rhyme label="b" type="m" corresp="#s8:l2">geluk</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Bewaar ons groot en waarde <rhyme label="a" type="m">Vorst</rhyme></l>
        <l id="s9:l2" met="3" type="-+">En Neêrlands Voester <rhyme label="b" type="m">Vrouw</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">Hy zy, die 't Land zo bitter <rhyme label="a" type="m" corresp="#s9:l1">torst</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Beschut voor smert en <rhyme label="b" type="m" corresp="#s9:l2">rouw</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dan vieren wy eerlang de <rhyme label="a" type="m">tyd</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Van eene Prinse <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">In weerwil van de bitsen <rhyme label="a" type="m" corresp="#s10:l1">nyd</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Met een vereende <rhyme label="b" type="m" corresp="#s10:l2">magt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
