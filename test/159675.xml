<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159675</id>
    <date>
      <year-start>1612</year-start>
      <year-end>1612</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Iam satis terris nivis atque dirae</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">LAet ons verblijden // met vreesen en <rhyme label="a" type="f">beven</rhyme></l>
        <l id="s1:l2" met="5" type="+-">Boosen schijn mijden // want ons is <rhyme label="a" type="f" corresp="#s1:l1">gegheven</rhyme></l>
        <l id="s1:l3" met="5" type="+-">In onse lijden // int begin der <rhyme label="b" type="f">tijden</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Macht om te <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">In sondt gheboren // als de <rhyme label="a" type="f">canenijten</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">En gantsch verlooren // ginck met ons <rhyme label="a" type="f" corresp="#s2:l1">versmijten</rhyme></l>
        <l id="s2:l3" met="5" type="+-">Al soud wy smoren // niemant can ons <rhyme label="b" type="f">hooren</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Priester <rhyme label="a" type="f" corresp="#s2:l1">Levijten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Stramen en wonden // waren al ons <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s3:l2" met="5" type="+-">Doot in der sonden // t'hert was <rhyme label="a" type="f" corresp="#s3:l1">onbesneden</rhyme>,</l>
        <l id="s3:l3" met="5" type="+-">Onreyne wonden // onghesouten <rhyme label="b" type="f">gronden</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Godloose <rhyme label="a" type="f" corresp="#s3:l1">zeden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Bloot ende naken // lagen wy met <rhyme label="a" type="f">schanden</rhyme>,</l>
        <l id="s4:l2" met="5" type="+-">In alle saken // was niet <rhyme label="a" type="f" corresp="#s4:l1">voorhanden</rhyme>,</l>
        <l id="s4:l3" met="5" type="+-">Maer Godt liet maecken // rocken wie sal 't <rhyme label="b" type="f">laecken</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Van ons <rhyme label="a" type="f" corresp="#s4:l1">vyanden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">En ginck ons proncken // met spansels <rhyme label="a" type="f">oorringhen</rhyme>,</l>
        <l id="s5:l2" met="5" type="+-">Hy heeft ons schoncken // sijden costel <rhyme label="a" type="f" corresp="#s5:l1">dingen</rhyme>,</l>
        <l id="s5:l3" met="5" type="+-">Van gout wy bloncken // int riet gelijc <rhyme label="b" type="f">voncken</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Cierlijck wy <rhyme label="a" type="f" corresp="#s5:l1">ginghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Tot alle nacy // is dees faem <rhyme label="a" type="f">gheresen</rhyme>,</l>
        <l id="s6:l2" met="5" type="+-">Van dese gracy // worden wy <rhyme label="a" type="f" corresp="#s6:l1">ghepresen</rhyme>,</l>
        <l id="s6:l3" met="5" type="+-">Maer wy och lacy // hoe van <rhyme label="b" type="f">estimacy</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Wort hier <rhyme label="a" type="f" corresp="#s6:l1">bewesen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">In die Propheten // leeftment <rhyme label="a" type="f">overvloedich</rhyme>,</l>
        <l id="s7:l2" met="5" type="+-">Hoe't wort verweten // Israhel <rhyme label="a" type="f" corresp="#s7:l1">stoutmoedich</rhyme></l>
        <l id="s7:l3" met="5" type="+-">Dat sy vergheten // hoe sy had <rhyme label="b" type="f">gheseten</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Onreyn en <rhyme label="a" type="f" corresp="#s7:l1">bloedich</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Alle die dieren // cranen ende <rhyme label="a" type="f">swalven</rhyme>,</l>
        <l id="s8:l2" met="5" type="+-">Holt sijn manieren sijner comste <rhyme label="a" type="f" corresp="#s8:l1">halven</rhyme>,</l>
        <l id="s8:l3" met="5" type="+-">Maer te regieren // mijn volck recht te <rhyme label="b" type="f">stieren</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Helpter gheen <rhyme label="a" type="f" corresp="#s8:l1">salven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">Kermen en clagen // hoe God heeft <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s9:l2" met="5" type="+-">Na sijn behaghen // Israhel van <rhyme label="a" type="f" corresp="#s9:l1">leven</rhyme>,</l>
        <l id="s9:l3" met="5" type="+-">Maer sy ontsagen // weldaet nochte <rhyme label="b" type="f">plagen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Teghen te <rhyme label="a" type="f" corresp="#s9:l1">streven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">Ossen en muylen // kennen hare <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s10:l2" met="5" type="+-">Maer ick moet huylen // mijn volck wil niet <rhyme label="a" type="f" corresp="#s10:l1">leeren</rhyme>,</l>
        <l id="s10:l3" met="5" type="+-">Het brist als guylen // t'graeft malcander <rhyme label="b" type="f">cuylen</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Om te <rhyme label="a" type="f" corresp="#s10:l1">verheeren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="-+">Priesters en Heeren // die't volc souden <rhyme label="a" type="f">straffen</rhyme></l>
        <l id="s11:l2" met="5" type="+-">Zijn stom in't leeren // honden die niet <rhyme label="a" type="f" corresp="#s11:l1">blaffen</rhyme></l>
        <l id="s11:l3" met="5" type="+-">T'goet sy verkeeren t'boose sy <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Terwe moet kaf <rhyme label="a" type="f" corresp="#s11:l1">zijn</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="5" type="-+">Soud ick niet wreken // sulcke boose <rhyme label="a" type="f">daden</rhyme>,</l>
        <l id="s12:l2" met="5" type="+-">Ick ginck uytreecken // alle mijn <rhyme label="a" type="f" corresp="#s12:l1">ghenaden</rhyme>,</l>
        <l id="s12:l3" met="5" type="+-">Met loose treecken // met hoveerdich <rhyme label="b" type="f">spreken</rhyme></l>
        <l id="s12:l4" met="2" type="-+">Sy't al <rhyme label="a" type="f" corresp="#s12:l1">versmaden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="5" type="-+">Daerom met banden // wil ickse <rhyme label="a" type="f">versenden</rhyme>,</l>
        <l id="s13:l2" met="5" type="+-">Tot haerder schanden // met grooter <rhyme label="a" type="f" corresp="#s13:l1">ellenden</rhyme>,</l>
        <l id="s13:l3" met="5" type="+-">In vreemde landen, onder haer <rhyme label="b" type="f">vyanden</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">Aenschout net <rhyme label="a" type="f" corresp="#s13:l1">enden</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="5" type="-+">Och wat al plaghen // hongher <rhyme label="a" type="f">pestilency</rhyme>,</l>
        <l id="s14:l2" met="5" type="+-">Mosten sy draghen // daer toe <rhyme label="a" type="f" corresp="#s14:l1">vyolency</rhyme>,</l>
        <l id="s14:l3" met="5" type="+-">So vele daghen // dat Godt mocht <rhyme label="b" type="f">behagen</rhyme>,</l>
        <l id="s14:l4" met="2" type="-+">Haer <rhyme label="a" type="f" corresp="#s14:l1">penitency</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="5" type="-+">Comt weer uyt Babel // nu alle <rhyme label="a" type="f">ghelijcke</rhyme>,</l>
        <l id="s15:l2" met="5" type="+-">Acht niet als fabel // Gods woort en sijn <rhyme label="a" type="f" corresp="#s15:l1">rijcke</rhyme></l>
        <l id="s15:l3" met="5" type="+-">Weert den diabel // weest niet <rhyme label="b" type="f">variabel</rhyme></l>
        <l id="s15:l4" met="2" type="-+">Maer <rhyme label="a" type="f" corresp="#s15:l1">autentijcke</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="5" type="-+">Hier condy sporen // tweederleye <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s16:l2" met="5" type="+-">Dat was verlooren // ghinck Godt eerst <rhyme label="a" type="f" corresp="#s16:l1">verheven</rhyme>,</l>
        <l id="s16:l3" met="5" type="+-">Daer na met toren // ginck hys al <rhyme label="b" type="f">verstooren</rhyme>,</l>
        <l id="s16:l4" met="2" type="-+">Die quaet <rhyme label="a" type="f" corresp="#s16:l1">bedreven</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="5" type="-+">Tot alle beyden // lieven ende <rhyme label="a" type="f">vreesen</rhyme>,</l>
        <l id="s17:l2" met="5" type="+-">Laet u toch leyden // door singhen en <rhyme label="a" type="f" corresp="#s17:l1">lesen</rhyme>,</l>
        <l id="s17:l3" met="5" type="+-">Wiltse recht scheyden // u oock recht <rhyme label="b" type="f">bereyden</rhyme></l>
        <l id="s17:l4" met="2" type="-+">Tot 'sHemels <rhyme label="a" type="f" corresp="#s17:l1">wesen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="5" type="-+">Eermen u dringen // wilt van liefde <rhyme label="a" type="f">loopen</rhyme></l>
        <l id="s18:l2" met="5" type="+-">Ter bruyloft ringen // wilt gheen ossen <rhyme label="a" type="f" corresp="#s18:l1">coopen</rhyme></l>
        <l id="s18:l3" met="5" type="+-">Noch onschult bringen // van tijtlijcke <rhyme label="b" type="f">dingen</rhyme></l>
        <l id="s18:l4" met="2" type="-+">T'eewich wilt <rhyme label="a" type="f" corresp="#s18:l1">hopen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="5" type="-+">Recht wort ghepresen // lieven ende <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s19:l2" met="5" type="+-">Laet zijn geresen // haer vruchten van <rhyme label="a" type="f" corresp="#s19:l1">binnen</rhyme>,</l>
        <l id="s19:l3" met="5" type="+-">Sy sal ghenesen // al uwe <rhyme label="b" type="f">vreesen</rhyme>,</l>
        <l id="s19:l4" met="2" type="-+">Ghy sult <rhyme label="a" type="f" corresp="#s19:l1">verwinnen</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="5" type="-+">Wel hen die't vaten // in een goe <rhyme label="a" type="f">conscienty</rhyme>,</l>
        <l id="s20:l2" met="5" type="+-">Tijtelijck verlaten // doende <rhyme label="a" type="f" corresp="#s20:l1">penitenty</rhyme>,</l>
        <l id="s20:l3" met="5" type="+-">Haer leven haten // t'sal haer vele <rhyme label="b" type="f">baten</rhyme>,</l>
        <l id="s20:l4" met="2" type="-+">In Gods <rhyme label="a" type="f" corresp="#s20:l1">sententy</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="5" type="-+">Eenen Eleaser // seven sonen <rhyme label="a" type="f">tellen</rhyme>,</l>
        <l id="s21:l2" met="5" type="+-">Eenen Beltsaser // met sijn drie <rhyme label="a" type="f" corresp="#s21:l1">ghesellen</rhyme>,</l>
        <l id="s21:l3" met="5" type="+-">Nebucadnaser // Antiochus <rhyme label="b" type="f">dwaser</rhyme>,</l>
        <l id="s21:l4" met="2" type="-+">Woudense <rhyme label="a" type="f" corresp="#s21:l1">vellen</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="5" type="-+">Doot nochte Helle // can die liefde <rhyme label="a" type="f">crencken</rhyme>,</l>
        <l id="s22:l2" met="5" type="+-">Stroomen rebelle // canse niet <rhyme label="a" type="f" corresp="#s22:l1">verdrencken</rhyme>,</l>
        <l id="s22:l3" met="5" type="+-">Al woudy telle // al u goet daer <rhyme label="b" type="f">stelle</rhyme>,</l>
        <l id="s22:l4" met="2" type="-+">Daer voor toe <rhyme label="a" type="f" corresp="#s22:l1">schencken</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="5" type="-+">Want ellick strijder // worstelt in sijn <rhyme label="a" type="f">percken</rhyme></l>
        <l id="s23:l2" met="5" type="+-">Daer salmen yder // loonen na sijn <rhyme label="a" type="f" corresp="#s23:l1">wercken</rhyme>,</l>
        <l id="s23:l3" met="5" type="+-">Quade met lijden, goede met <rhyme label="b" type="f">verblijden</rhyme>,</l>
        <l id="s23:l4" met="2" type="-+">Och wilt toch <rhyme label="a" type="f" corresp="#s23:l1">mercken</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="5" type="-+">Waerom wilt hooren // des Apostels <rhyme label="a" type="f">raden</rhyme></l>
        <l id="s24:l2" met="5" type="+-">Laet niet verlooren // den tijt der <rhyme label="a" type="f" corresp="#s24:l1">ghenaden</rhyme>,</l>
        <l id="s24:l3" met="5" type="+-">Of Godes tooren // na t'gheclanckx <rhyme label="b" type="f">labooren</rhyme>,</l>
        <l id="s24:l4" met="2" type="-+">Soud u veel <rhyme label="a" type="f" corresp="#s24:l1">schaden</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="5" type="-+">Eer dese dingen // comt die toecomen <rhyme label="a" type="f">moete</rhyme></l>
        <l id="s25:l2" met="5" type="+-">Wilt doch gheringhen // doen oprechte <rhyme label="a" type="f" corresp="#s25:l1">boete</rhyme>,</l>
        <l id="s25:l3" met="5" type="+-">So suldy singhen // en van vreuchde <rhyme label="b" type="f">springen</rhyme>,</l>
        <l id="s25:l4" met="2" type="-+">Dit is mijn <rhyme label="a" type="f" corresp="#s25:l1">groete</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
