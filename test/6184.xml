<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6184</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hansje sneed het koren</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">NV soete Ieucht// schept nieuwe <rhyme label="a" type="m">vreucht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Schort op de naere <rhyme label="b" type="f">suchjes</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">De somer soet// brengt ons weer <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gras, kruyt, bloem, loof en <rhyme label="b" type="f" corresp="#s1:l2">vruchjes</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Treet uyt de poort// west, zuydt en <rhyme label="a" type="m">noordt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Besoeckt de nieuwe <rhyme label="b" type="f">blaetjes</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Het wintje soel// niet stuer niet <rhyme label="c" type="m">koel</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Drooght op de <rhyme label="b" type="f" corresp="#s2:l2">Wandel-paedtjes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Langhs Duyne-kant// ist soet <rhyme label="a" type="m">beplant</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met veelderleye <rhyme label="b" type="f">Boompjes</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Dan sietmen daer// noch dalen <rhyme label="c" type="m">klaer</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">De Christalijne <rhyme label="b" type="f" corresp="#s3:l2">stroompjes</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het wilt Gediert// ooc veylig <rhyme label="a" type="m">swiert</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Rontsom de <rhyme label="b" type="f">Bergen-kruyntjes</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Daer achter vint// men Brack en <rhyme label="c" type="m">Wint</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die kruyssen dese <rhyme label="b" type="f" corresp="#s4:l2">Duyntjes</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De geyle Ieucht// vol dartel <rhyme label="a" type="m">vreucht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die hebben oock <rhyme label="b" type="f">verlanghen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">En haer vermaeck// in dese <rhyme label="c" type="m">saeck</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Om 't Minne-wilt te <rhyme label="b" type="f" corresp="#s5:l2">vanghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sy passen op// met Handt, en <rhyme label="a" type="m">Strop</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En soecken veel <rhyme label="b" type="f">practijcken</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Doch 't Minne-wicht// wel af <rhyme label="c" type="m">ghericht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Weet al haer doen t' <rhyme label="b" type="f" corresp="#s6:l2">ontwijcken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Het strijckt sijn hayr// uyt d'oogjes <rhyme label="a" type="m">klaer</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Voorts Wieckjes <rhyme label="b" type="f">uyt-ghetoghen</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">Daer mee is hy// nu ver, dan <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En strackx weer uyt haer <rhyme label="b" type="f" corresp="#s7:l2">ooghen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Schoon ghy hem hadt// en eens <rhyme label="a" type="m">vervat</rhyme>:</l>
        <l id="s8:l2" met="3" type="-+">Quijt zijt ghy uwe <rhyme label="b" type="f">vangste</rhyme>!</l>
        <l id="s8:l3" met="4" type="-+">Sijn Tovery// die blijft u <rhyme label="c" type="m">by</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Daer staetmen dan met <rhyme label="b" type="f" corresp="#s8:l2">anghste</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dus, stelt ghy sin// op soete <rhyme label="a" type="m">Min</rhyme>:</l>
        <l id="s9:l2" met="3" type="-+">Soo laet u niet <rhyme label="b" type="f">verleyen</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Tot dese Iacht// maer vlytigh <rhyme label="c" type="m">tracht</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Nae ware <rhyme label="b" type="f" corresp="#s9:l2">Minnes-weyen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
