<song>
  <meta>
    <id>34439</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Laat ons met deze tijd</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAet ons met desen <rhyme label="a" type="m">tijdt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Cecilia <rhyme label="b" type="f">vereeren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Haer deught met vreught <rhyme label="a" type="m" corresp="#s1:l1">verbrijdt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Aen Princen ende <rhyme label="b" type="f" corresp="#s1:l2">Heeren</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Want sy een spieghel <rhyme label="c" type="m">klaer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Voor alle menschen <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Haer deught in't <rhyme label="c" type="m" corresp="#s1:l5">openbaer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Die gheeft <rhyme label="d" type="m" corresp="#s1:l6">ghetuyghenis</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dees alderschoonste <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van edel stam <rhyme label="b" type="f">gheboren</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Heeft Godt altijdt <rhyme label="a" type="m" corresp="#s2:l1">behaeght</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die haer had <rhyme label="b" type="f" corresp="#s2:l2">uytverkoren</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Want sy met all' <rhyme label="c" type="m">oodtmoet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Haer op-ghedraeghen <rhyme label="d" type="m">heeft</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Aen Godt het hooghste <rhyme label="c" type="m" corresp="#s2:l5">goet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Soo langhe als sy <rhyme label="d" type="m" corresp="#s2:l6">leeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Van veel wirt sy <rhyme label="a" type="m">bemint</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Versoght, tot echten <rhyme label="b" type="f">staet</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">Maer sy anders <rhyme label="a" type="m" corresp="#s3:l1">ghesint</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Heeft alle eer <rhyme label="b" type="f" corresp="#s3:l2">versmaet</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Ja heeft oock selfs <rhyme label="c" type="m">bekeert</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Haeren Bruydegom seer <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die Godt oock heeft <rhyme label="c" type="m" corresp="#s3:l5">vereert</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Met eenen <rhyme label="d" type="m" corresp="#s3:l6">Lauwer-kroon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sy dede haer <rhyme label="a" type="m">ghebedt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met liefde ende <rhyme label="b" type="f">traenen</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Op dat sy <rhyme label="a" type="m" corresp="#s4:l1">onbesmet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den rechten-wegh soud' <rhyme label="b" type="f" corresp="#s4:l2">baenen</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Dat altijdt mijn <rhyme label="c" type="m">ghemoet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">(Sonck sy tot haeren <rhyme label="d" type="m">Godt</rhyme>)</l>
        <l id="s4:l7" met="3" type="-+">Bereedt sy met <rhyme label="c" type="m" corresp="#s4:l5">oodtmoet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Te doen naer sijn <rhyme label="d" type="m" corresp="#s4:l6">ghebodt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Jck wil een suyver <rhyme label="a" type="m">maeght</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Mijn leven lanck hier <rhyme label="b" type="f">blijven</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">En doen wat u <rhyme label="a" type="m" corresp="#s5:l1">behaeght</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Al soud'men my <rhyme label="b" type="f" corresp="#s5:l2">ontlijven</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Jck bid' dan hooghste <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ontfanght my voor u <rhyme label="d" type="m">Bruydt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ontfanght mijn diep <rhyme label="c" type="m" corresp="#s5:l5">oodtmoet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Als een onvruchtbaer <rhyme label="d" type="m" corresp="#s5:l6">kruydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dat haecken naghels, <rhyme label="a" type="m">stael</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mijn lichaem heel <rhyme label="b" type="f">verschooren</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">K'en acht' niet <rhyme label="a" type="m" corresp="#s6:l1">altemael</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ten is maer tijdt <rhyme label="b" type="f" corresp="#s6:l2">verlooren</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Als ghy met my maer <rhyme label="c" type="m">sijt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En k'u ghedienen <rhyme label="d" type="m">can</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Dat acht ick meer <rhyme label="c" type="m" corresp="#s6:l5">altijdt</rhyme>:</l>
        <l id="s6:l8" met="3" type="-+">Als d'alderschoonsten <rhyme label="d" type="m" corresp="#s6:l6">man</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Den President der <rhyme label="a" type="m">stadt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Als hy had' vast <rhyme label="b" type="f">vernomen</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Dat sy den heelen <rhyme label="a" type="m" corresp="#s7:l1">schat</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Haers Bruyd'goms had <rhyme label="b" type="f" corresp="#s7:l2">bekomen</rhyme>;</l>
        <l id="s7:l5" met="3" type="-+">En dat sy door <rhyme label="c" type="m">t'ghebedt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Twee Broeders had <rhyme label="d" type="m">bekeert</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">Met rasernye <rhyme label="c" type="m" corresp="#s7:l5">beset</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wirt als een Tygher <rhyme label="d" type="m" corresp="#s7:l6">vreedt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Heeft dan dees Maeghet <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Door pijnen vol <rhyme label="b" type="f">vreedtheden</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Doen swemmen in haer <rhyme label="a" type="m" corresp="#s8:l1">bloet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Door-slaeghen al haer <rhyme label="b" type="f" corresp="#s8:l2">leden</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Maer sy als een <rhyme label="c" type="m">pilaer</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Bleef staen in allen <rhyme label="d" type="m">strijdt</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">Want sy wel wist <rhyme label="c" type="m" corresp="#s8:l5">voorwaer</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Den loon voor haer <rhyme label="d" type="m" corresp="#s8:l6">berijdt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Lof sy Cecilia <rhyme label="a" type="m">rijn</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En U, Ã´ Godt <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die door torment' en <rhyme label="a" type="m" corresp="#s9:l1">pijn</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dees Blom hebt <rhyme label="b" type="f" corresp="#s9:l2">uyt-ghelesen</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Sy heeft verwonnen <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">De wereldt, vleesch, en <rhyme label="d" type="m">bloedt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Den duyvel met de <rhyme label="c" type="m" corresp="#s9:l5">hel</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Heeft sy onder den <rhyme label="d" type="m" corresp="#s9:l6">voedt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
