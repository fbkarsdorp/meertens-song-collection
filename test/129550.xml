  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Hier sitte wy in <rhyme label="a" type="m">Gloria</rhyme></l>
        <l id="s1:l2" met="4" type="-+">En drincke daar <rhyme label="a" type="m" corresp="#s1:l1">Vinaria</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Soo laat ons <rhyme label="b" type="f">drincke</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Soo laat ons <rhyme label="b" type="f" corresp="#s1:l3">klinke</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Rondi Ronda in <rhyme label="a" type="m" corresp="#s1:l1">Cellula</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">‘Ik breng het uw, <rhyme label="a" type="m">Gregorius</rhyme>.’</l>
        <l id="s2:l2" met="4" type="-+">‘Ik danck het u <rhyme label="a" type="m" corresp="#s2:l1">Confloribus</rhyme>.’</l>
        <l id="s2:l3" met="2" type="-+">Nu set het <rhyme label="b" type="f">mondelingh</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">En drinckt het <rhyme label="b" type="f" corresp="#s2:l3">grondelingh</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Rondi Ronda in <rhyme label="a" type="m" corresp="#s2:l1">Cellula</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Is ‘t niet Claarouws,   <rhyme label="a" type="m">inspicite</rhyme>.</l>
        <l id="s3:l2" met="4" type="-+">O socii, <rhyme label="a" type="m" corresp="#s3:l1">advertite</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Wilt <rhyme label="b" type="f">indicere</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Niet <rhyme label="b" type="f" corresp="#s3:l3">favoriseere</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Rondi Ronda in <rhyme label="a" type="m" corresp="#s3:l1">Cellula</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ja, ‘t is Claarouws, <rhyme label="a" type="m">gratulamini</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">O Socii, <rhyme label="a" type="m" corresp="#s4:l1">letamini</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Sic nos <rhyme label="b" type="f">oportet</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Sic nos <rhyme label="b" type="f" corresp="#s4:l3">convortet</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Rondi Ronda in <rhyme label="a" type="m" corresp="#s4:l1">Cellula</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">‘O Neen, mijn Heer, ‘t mag soo niet <rhyme label="a" type="m">zyn</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Aanbiet myn eerst u <rhyme label="a" type="m" corresp="#s5:l1">handelyn</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Amoris <rhyme label="b" type="f">signum</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Favore <rhyme label="b" type="f" corresp="#s5:l3">dignum</rhyme>’,</l>
        <l id="s5:l5" met="4" type="+-">Rondi Ronda in <rhyme label="a" type="m" corresp="#s5:l1">Cellula</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
