  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HOe, mijn Engel, Jaeght den <rhyme label="a" type="f">bengel</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van de Min U dit <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat Liefander Lieft een <rhyme label="c" type="f">ander</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Soo jaeght, Engel U den <rhyme label="a" type="f">Bengel</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Van de Min Leugens <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want Liefander Lieft geen <rhyme label="c" type="f">ander</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Segh dan, Engel, Als den <rhyme label="a" type="f">Bengel</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Van de Min Weer blaest <rhyme label="b" type="m">in</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Dat Liefander Lieft een <rhyme label="c" type="f">ander</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Weg, jou bengel, 'k Ben den <rhyme label="a" type="f">Engel</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Die door min 't Hert heb <rhyme label="b" type="m">in</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Van Liefander, En geen <rhyme label="c" type="f">ander</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
