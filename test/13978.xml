<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13978</id>
    <date>
      <year-start>1670</year-start>
      <year-end>1670</year-end>
    </date>
    <location>
      <orginal-name>middelburg</orginal-name>
      <standard-name>Middelburg, The Netherlands</standard-name>
      <coordinates>
        <lon>51.4997302</lon>
        <lat>3.6136962</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Wilhelmus</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GOd met ons, uyt den <rhyme label="a" type="f">Throone</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghenaemt <rhyme label="b" type="m">Emanuël</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Des Vaders eenigh <rhyme label="a" type="f" corresp="#s1:l1">Sone</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Een Vorst in <rhyme label="b" type="m" corresp="#s1:l2">Israël</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Is komen hier <rhyme label="c" type="f">beneden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Al uyt sijn Rijck <rhyme label="d" type="m">eerbaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En heeft ons gebracht <rhyme label="c" type="f" corresp="#s1:l5">mede</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Een zaligh <rhyme label="d" type="m" corresp="#s1:l6">Nieuwe-jaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als eenen Helt vol <rhyme label="a" type="f">trouwen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gordde hy aen sijn <rhyme label="b" type="m">zweert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om ons uyt Serpents <rhyme label="a" type="f" corresp="#s2:l1">klouwen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Te brengen <rhyme label="b" type="m" corresp="#s2:l2">ongedeert</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Doen wy al verkocht <rhyme label="c" type="f">waren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Onder de sonde <rhyme label="d" type="m">zwaer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Quam hy ons <rhyme label="c" type="f" corresp="#s2:l5">openbaren</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Een zaligh <rhyme label="d" type="m" corresp="#s2:l6">Nieuwe-jaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Den Engel was <rhyme label="a" type="f">gesonden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In een Stadt <rhyme label="b" type="m">Nazareth</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Een Maget te <rhyme label="a" type="f" corresp="#s3:l1">verkonden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van sijn Geboorte <rhyme label="b" type="m" corresp="#s3:l2">net</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Jesus sult ghy hem <rhyme label="c" type="f">noemen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Want hy sal sijn volck <rhyme label="d" type="m">klaer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Vry maken van 't <rhyme label="c" type="f" corresp="#s3:l5">verdoemen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In 't zaligh <rhyme label="d" type="m" corresp="#s3:l6">Nieuwe-jaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sy baerden haren <rhyme label="a" type="f">eersten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sone, soo 't was <rhyme label="b" type="m">voorseyt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Al in een stal van <rhyme label="a" type="f" corresp="#s4:l1">Beesten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In de kribbe <rhyme label="b" type="m" corresp="#s4:l2">geleyt</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">En sy wondt hem in <rhyme label="c" type="f">doecken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De Wijse quamen <rhyme label="d" type="m">daer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Van verre hem <rhyme label="c" type="f" corresp="#s4:l5">besoecken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In 't zaligh <rhyme label="d" type="m" corresp="#s4:l6">Nieuwe-jaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Herders sonder <rhyme label="a" type="f">slapen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Waren doen op het <rhyme label="b" type="m">velt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">'s Nachts hoedende haer <rhyme label="a" type="f" corresp="#s5:l1">schapen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De boodtschap wert <rhyme label="b" type="m" corresp="#s5:l2">vertelt</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Sy gingen haest <rhyme label="c" type="f">vertrecken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Naer Bethlehem te <rhyme label="d" type="m">gaer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En vonden tot dier <rhyme label="c" type="f" corresp="#s5:l5">plecken</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Een zaligh <rhyme label="d" type="m" corresp="#s5:l6">Nieuwe-jaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Oorlof al die nu <rhyme label="a" type="f">trachten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Om te soecken met <rhyme label="b" type="m">vlijt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">'t Zy by daegh, of by <rhyme label="a" type="f" corresp="#s6:l1">nachte</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Sy vinden hem <rhyme label="b" type="m" corresp="#s6:l2">altijt</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Sijn Woort is de <rhyme label="c" type="f">Leyd-sterre</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die brenght ons <rhyme label="d" type="m">allegaer</rhyme></l>
        <l id="s6:l7" met="3" type="-+">In 't eeuwigh Licht soo <rhyme label="c" type="f" corresp="#s6:l5">verre</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">In 't zaligh <rhyme label="d" type="m" corresp="#s6:l6">Nieuwe-jaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
