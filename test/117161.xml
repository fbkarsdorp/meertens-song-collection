    <melody>Besneeuwde velden doorluchtige bomen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">KOmt nu gy menschen <rhyme label="a" type="f">algelijcke</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Aenschout de soete Somer <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die so lieffelijck tot ons komt <rhyme label="a" type="f" corresp="#s1:l1">kijken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Met gulde stralen van <rhyme label="b" type="m" corresp="#s1:l2">sonne-schijn</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Die koude dagen die <rhyme label="c" type="f">verjaget</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">De soete somer komt ons <rhyme label="d" type="m">by</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Met veele vrogten tot ons <rhyme label="e" type="f">genugten</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Die Godt ons geeft al met de <rhyme label="d" type="m" corresp="#s1:l6">Mey</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt gy pleysier en vermaeck <rhyme label="a" type="f">aenschouwe</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Neemt u soete liefje al by der <rhyme label="b" type="m">hant</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Spanceeren vroeg al door den <rhyme label="a" type="f" corresp="#s2:l1">douwe</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Daer sult gy sien aen alle <rhyme label="b" type="m" corresp="#s2:l2">kant</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">De Koetjes grasen met melk <rhyme label="c" type="f">beladen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Al in de groene <rhyme label="d" type="m">klaverwey</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Daer de Boertjes komen al sonder <rhyme label="e" type="f">schrome</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Halen de Melck al in de <rhyme label="d" type="m" corresp="#s2:l6">Mey</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dan gaet eens na de <rhyme label="a" type="f">Water-stroomen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Daer sult gy sien de Eentjes <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Al met de Swane groot en <rhyme label="a" type="f" corresp="#s3:l1">schoone</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Swemme sy daer in ’t <rhyme label="b" type="m" corresp="#s3:l2">openbaer</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">En nemen vermake in ’t water sy <rhyme label="c" type="f">knaken</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Elk al op syn tael so <rhyme label="d" type="m">bly</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Daer sy vereeren den Heer der <rhyme label="e" type="f">Heeren</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Loven den Schepper al in de <rhyme label="d" type="m" corresp="#s3:l6">Mey</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Aensien de Vogeltjens in de <rhyme label="a" type="f">luchte</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die zwieren onder de Hemel <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">De Nachtegael met bly <rhyme label="a" type="f" corresp="#s4:l1">genugten</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Singen met een soo schoone <rhyme label="b" type="m" corresp="#s4:l2">toon</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En gaen den Heer met sang <rhyme label="c" type="f">vereeren</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Elk al op zijn Tael so <rhyme label="d" type="m">bly</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Alle de beeste van minste en <rhyme label="e" type="f">meeste</rhyme>;</l>
        <l id="s4:l8" met="4" type="-+">Loven den schepper al in de <rhyme label="d" type="m" corresp="#s4:l6">Mey</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Men siet oock aen de duyn-kant <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Veel Hasen en Konyntjes <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die den Iager al sonder <rhyme label="a" type="f" corresp="#s5:l1">schroomen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Voor ’s menschen voetsel schieten <rhyme label="b" type="m" corresp="#s5:l2">moet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Gaet dan met eenen wat verder <rhyme label="c" type="f">treden</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Langes de zee kant met <rhyme label="d" type="m">pleysier</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En wilt aenschouwen, sonder <rhyme label="e" type="f">benouwen</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Alle de Vissertjes also <rhyme label="d" type="m" corresp="#s5:l6">fier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sy halen de Visjens so veer wilt <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Al uyt de Zee voor ’s menschen <rhyme label="b" type="m">spijs</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En nemens in de Krieltjes als <rhyme label="a" type="f" corresp="#s6:l1">vooren</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dragen te koop voor arm en <rhyme label="b" type="m" corresp="#s6:l2">rijck</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">komen te samen al sonder <rhyme label="c" type="f">blamen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Al in den Haeg en Delft <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">En andere steede komen sy <rhyme label="e" type="f">meede</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Koopt Cabbeljauw en Schellevis vers en <rhyme label="d" type="m" corresp="#s6:l6">soet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dan gaet met u liefje <rhyme label="a" type="f">vermeyden</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Langes dees linden met <rhyme label="b" type="m">vermaeek</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dan siet gy aen de groene <rhyme label="a" type="f" corresp="#s7:l1">weyde</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Daer men de bloempjes in siet <rhyme label="b" type="m" corresp="#s7:l2">staen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Cierlijck en schoon al voor ons <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Tarwe en Rogge, Garst, Haver daer <rhyme label="d" type="m">by</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">En Erten, Boonen alle, soo <rhyme label="e" type="f">schoone</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Dus looft den Schepper al in de <rhyme label="d" type="m" corresp="#s7:l6">Mey</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">En nog soo veel andere <rhyme label="a" type="f">dingen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Die Godt ons geeft al in de <rhyme label="b" type="m">Mey</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Tot ons vermaeck al <rhyme label="a" type="f" corresp="#s8:l1">sonderlingen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Wy gingen sitten al in de <rhyme label="b" type="m" corresp="#s8:l2">wey</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En loofden den Heer met lof en <rhyme label="c" type="f">eere</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Voor al sijn gaven <rhyme label="d" type="m">meenigerley</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Laet ons dan paren en vreugde <rhyme label="e" type="f">vergaren</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Nu samen in de groene <rhyme label="d" type="m" corresp="#s8:l6">Mey</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
