<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185412</id>
    <date>
      <year-start>1720</year-start>
      <year-end>1720</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik zucht zucht op zucht</melody>
  <author>Johannes Stichter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe! werkje niet <rhyme label="a" type="m">Leeuwerik</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Hoe komt dat gy niet <rhyme label="b" type="f">orgelt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In dese Tiende <rhyme label="a" type="m" corresp="#s1:l1">Kerk</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">U strotje dat soo <rhyme label="b" type="f" corresp="#s1:l2">gorgelt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer ’t niemant hoort te <rhyme label="c" type="f">velde</rhyme>?</l>
        <l id="s1:l6" met="3" type="-+">Wel aen dan, laet u <rhyme label="d" type="m">gunst</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Aen ons gehoor ook <rhyme label="c" type="f" corresp="#s1:l5">melde</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">’t Mirakel van u <rhyme label="d" type="m" corresp="#s1:l6">kunst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Toont dat gy dankbaer <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Voortaen te blijven <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van ’t broot dat gy <rhyme label="a" type="m" corresp="#s2:l1">verslijt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Van ’t zaedt dat wy u <rhyme label="b" type="f" corresp="#s2:l2">geven</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Van soo veel <rhyme label="c" type="f">Klaverzootjes</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van zulke versche <rhyme label="d" type="m">nat</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Van soo veel groene <rhyme label="c" type="f" corresp="#s2:l5">lootjes</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Als gy wel hebt <rhyme label="d" type="m" corresp="#s2:l6">gehad</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe sou ‘k ellendig <rhyme label="a" type="m">dier</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vermaken met mijn <rhyme label="b" type="f">zangen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">’t Gehoor van mijn <rhyme label="a" type="m" corresp="#s3:l1">Cypier</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die my hier houd <rhyme label="b" type="f" corresp="#s3:l2">gevangen</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">En wilt u niet <rhyme label="c" type="f">bedriegen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">‘K agt geen brood nog <rhyme label="d" type="m">zaed</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Eer dat men my laet <rhyme label="c" type="f" corresp="#s3:l5">vliegen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Te velde, voor <rhyme label="d" type="m" corresp="#s3:l6">weldaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den Aerdboom was mijn <rhyme label="a" type="m">vloer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Den Hemel mijn <rhyme label="b" type="f">verwellif</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Al ’t groen gewas mijn <rhyme label="a" type="m" corresp="#s4:l1">voer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Mijn Tafel waer Godt <rhyme label="b" type="f" corresp="#s4:l2">sellif</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hoe soude my de <rhyme label="c" type="f">kruymen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Verheugen van u <rhyme label="d" type="m">prooy</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Nu ik den Hemel <rhyme label="c" type="f" corresp="#s4:l5">ruymen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Moet voor een naeuwe <rhyme label="d" type="m" corresp="#s4:l6">Kooy</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn Schepper had mijn <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Doen baren van ’t <rhyme label="b" type="f">geveugelt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En uw gewelt heeft <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De vrye vlugt <rhyme label="b" type="f" corresp="#s5:l2">beteugelt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ik huppel en ik <rhyme label="c" type="f">wemel</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Mijn zang dient tot <rhyme label="d" type="m">geschrey</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Want Koy en is geen <rhyme label="c" type="f" corresp="#s5:l5">Hemel</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En ’t Zootjen is geen <rhyme label="d" type="m" corresp="#s5:l6">Wey</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wilt gy dan met <rhyme label="a" type="m">geneugt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Mijn gorgeltje doen <rhyme label="b" type="f">queelen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Send my weer na de <rhyme label="a" type="m" corresp="#s6:l1">Lugt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En ’t Orgeltjen sal <rhyme label="b" type="f" corresp="#s6:l2">speelen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Voor hem, die alle <rhyme label="c" type="f">dingen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Schip met een enkel <rhyme label="d" type="m">oog</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dies plagt ik niet te <rhyme label="c" type="f" corresp="#s6:l5">singen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dan als ik klim om <rhyme label="d" type="m" corresp="#s6:l6">hoog</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Het sal u dunken <rhyme label="a" type="m">nieuw</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Als g’ uw sult hooren <rhyme label="b" type="f">nooden</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Door mijn adieu, dieu, <rhyme label="a" type="m" corresp="#s7:l1">dieu</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Te Godt, te Godt, te <rhyme label="b" type="f" corresp="#s7:l2">Gode</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En tusschen ’t <rhyme label="c" type="f">tiere-lieren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Dieu, dieu, weer <rhyme label="d" type="m">t’elkemael</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Om u te leeren <rhyme label="c" type="f" corresp="#s7:l5">vieren</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Uw Schepper met uw <rhyme label="d" type="m" corresp="#s7:l6">tael</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
