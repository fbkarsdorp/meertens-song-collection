    <melody>Verblijd u gij Batavieren</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="cycle of life">levenscyclus</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VErblijt u ghy <rhyme label="a" type="f">Batavieren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Maeckt vreughden om en <rhyme label="b" type="m">tom</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Kroon nu met <rhyme label="a" type="f" corresp="#s1:l1">Lauwerieren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Den Prins d’Oranjen <rhyme label="b" type="m" corresp="#s1:l2">Blom</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Die heden sijn Trou, Aen een <rhyme label="c" type="m">Vrouw</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van Konincks bloet heeft <rhyme label="d" type="f">verbonden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Aen Ducq de Jorrec <rhyme label="e" type="m">Kint</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Die hy seer <rhyme label="e" type="m" corresp="#s1:l7">bemint</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Zoo gaf hy sijn trouwe <rhyme label="f" type="m">hant</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Binnen <rhyme label="f" type="m" corresp="#s1:l9">Engelandt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maria die Edel <rhyme label="a" type="f">Princesse</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Daer alle deught in <rhyme label="b" type="m">blinckt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Is nu Wilhelmus <rhyme label="a" type="f" corresp="#s2:l1">Meestresse</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op haer Gesontheyt <rhyme label="b" type="m" corresp="#s2:l2">drinckt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Steeckt Pectonnen aen, En laet <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Den Vierpijl door de <rhyme label="d" type="f">Wolcken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Want siet den wegh <rhyme label="e" type="m">aenvaert</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Maria <rhyme label="e" type="m" corresp="#s2:l7">Stuart</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Al met u Oranje <rhyme label="f" type="m">Blom</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Haren <rhyme label="f" type="m" corresp="#s2:l9">Bruydegom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een treyn van wel twintig <rhyme label="a" type="f">Carossen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Al met ses Peerden <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Onder ’t Canon te <rhyme label="a" type="f" corresp="#s3:l1">lossen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daer Coninck, <rhyme label="b" type="m" corresp="#s3:l2">Coningin</rhyme></l>
        <l id="s3:l5" met="4" type="+-">In d’eerste Coets sat, die door <rhyme label="c" type="m">d’Stadt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dees Edel bruyt <rhyme label="d" type="f">convoyeerden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In d’ander Coets <rhyme label="e" type="m">naer</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Daer sat <rhyme label="e" type="m" corresp="#s3:l7">openbaer</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Die Jonge Oranje <rhyme label="f" type="m">blom</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Met veel <rhyme label="f" type="m" corresp="#s3:l9">Edeldom</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al met <rhyme label="a" type="f">Magnificentie</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Reden te Kerck <rhyme label="b" type="m">alwaer</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Zy in Konincx <rhyme label="a" type="f" corresp="#s4:l1">presentie</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Beloofden trouw <rhyme label="b" type="m" corresp="#s4:l2">malkaer</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Trompet en Tamboer, Gaf <rhyme label="c" type="m">rammoer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De Klocken die luyden van <rhyme label="d" type="f">vreugden</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Al om dit Edel <rhyme label="e" type="m">Paer</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Die trouden te <rhyme label="e" type="m" corresp="#s4:l7">gaer</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Toen reedmen met grooten <rhyme label="f" type="m">Lof</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Weer naer Konincx <rhyme label="f" type="m" corresp="#s4:l9">Hof</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Alle de Heeren van <rhyme label="a" type="f">Londen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Menigh Marquies en <rhyme label="b" type="m">Ducq</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als dees Trou was <rhyme label="a" type="f" corresp="#s5:l1">ghebonden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Boden hun veel <rhyme label="b" type="m" corresp="#s5:l2">Geluck</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">De Tafel gheset, Vol <rhyme label="c" type="m">Bancket</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Violen en Bassen <rhyme label="d" type="f">kloncken</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Men dansten paer aen <rhyme label="e" type="m">paer</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Men dronck den Wijn <rhyme label="e" type="m" corresp="#s5:l7">klaer</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Al op de Gesontheyt <rhyme label="f" type="m">uyt</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Van Bruydegom en <rhyme label="f" type="m" corresp="#s5:l9">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Couragie dan met <rhyme label="a" type="f">pleysiere</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Maeckt blijtschap door het <rhyme label="b" type="m">Lant</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Laet nu de Wimpels <rhyme label="a" type="f" corresp="#s6:l1">zwieren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Den Prins is hier te <rhyme label="b" type="m" corresp="#s6:l2">Landt</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Die met sijn schoon Bruyt, In een <rhyme label="c" type="m">schuyt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Oft Gulden schip was <rhyme label="d" type="f">geseten</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Hy brenght sijn Liefste <rhyme label="e" type="m">mee</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Die ons haelt de <rhyme label="e" type="m" corresp="#s6:l7">Vree</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Sal maken met <rhyme label="f" type="m">Vranckerijck</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Door dit <rhyme label="f" type="m" corresp="#s6:l9">Houwelijck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy Princen wilt u <rhyme label="a" type="f">verblijden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Maeckt vreuhgt aen alle <rhyme label="b" type="m">kant</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">U naecken beter <rhyme label="a" type="f" corresp="#s7:l1">tijden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Heden door <rhyme label="b" type="m" corresp="#s7:l2">Engelandt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Dit Edel verbont, Sal de <rhyme label="c" type="m">Wont</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Van de Orelogh haest uyt <rhyme label="d" type="f">blussen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Sa roept dan al <rhyme label="e" type="m">viva</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Met vreughden sa <rhyme label="e" type="m" corresp="#s7:l7">sa</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Viva ons Oranje <rhyme label="f" type="m">Spruyt</rhyme>,</l>
        <l id="s7:l10" met="3" type="+-">Met sijn Edel <rhyme label="f" type="m" corresp="#s7:l9">Bruyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
