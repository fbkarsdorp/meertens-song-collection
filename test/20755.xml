<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20755</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Adieu nymphe du bois</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WY Loven U, O <rhyme label="a" type="m">God</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">O God van <rhyme label="a" type="m" corresp="#s1:l1">Sabaoth</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">U, Heer van alle <rhyme label="b" type="f">Heeren</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">De gantsche Aerd' U <rhyme label="c" type="m">eert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">U, die het al <rhyme label="c" type="m" corresp="#s1:l4">regeert</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En eeuwigh sult <rhyme label="b" type="f" corresp="#s1:l3">regeeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Eng'len <rhyme label="a" type="m">allemael</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En 's hemels gulde <rhyme label="a" type="m" corresp="#s2:l1">zael</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met al des hemels <rhyme label="b" type="f">Machten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die singen uwen <rhyme label="c" type="m">lof</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Met stemmen fijn en <rhyme label="c" type="m" corresp="#s2:l4">grof</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">By dagen en by <rhyme label="b" type="f" corresp="#s2:l3">nachten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Cherubijnsche <rhyme label="a" type="m">Choor</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Verleent hier toe haer <rhyme label="a" type="m" corresp="#s3:l1">oor</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Jae singht oock even <rhyme label="b" type="f">krachtich</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">En Seraphin die <rhyme label="c" type="m">singht</rhyme></l>
        <l id="s3:l5" met="3" type="-+">U lof, die alles <rhyme label="c" type="m" corresp="#s3:l4">dwinght</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">U lof, O God <rhyme label="b" type="f" corresp="#s3:l3">almachtich</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy heyligh, heyligh <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gantsch heyligh 't allen <rhyme label="a" type="m" corresp="#s4:l1">tijd</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dus singen all' de <rhyme label="b" type="f">Eng'len</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot danck, tot lof, tot <rhyme label="c" type="m">eer</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Van hunnen <rhyme label="c" type="m" corresp="#s4:l4">opper-Heer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Sy dus haer stemmen <rhyme label="b" type="f" corresp="#s4:l3">meng'len</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De glans uw's <rhyme label="a" type="m">Majesteyts</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De glans uw's <rhyme label="a" type="m" corresp="#s5:l1">deftigheyts</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Den hemel vult, en d' <rhyme label="b" type="f">aerde</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Uw' glans den hemel <rhyme label="c" type="m">ciert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">De gantsche aerd' U <rhyme label="c" type="m" corresp="#s5:l4">viert</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">U, die uyt niet ons <rhyme label="b" type="f" corresp="#s5:l3">baerden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">D' Apost'len <rhyme label="a" type="m">altesaem</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Verheffen uwe <rhyme label="a" type="m" corresp="#s6:l1">Naem</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo doen oock de <rhyme label="b" type="f">Propheten</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Der Marters heyr U <rhyme label="c" type="m">prijst</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En altijd eer <rhyme label="c" type="m" corresp="#s6:l4">bewijst</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">By U nu hoogh <rhyme label="b" type="f" corresp="#s6:l3">geseten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Door 's werelts ronde <rhyme label="a" type="m">kloot</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Maeckt d' Heyl'ge Kerck, U <rhyme label="a" type="m" corresp="#s7:l1">groot</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">U, Vader van ons <rhyme label="b" type="f">allen</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Wiens hooge <rhyme label="c" type="m">Majesteyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Is over al <rhyme label="c" type="m" corresp="#s7:l4">verspreyt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Voor wien wy <rhyme label="b" type="f" corresp="#s7:l3">nedervallen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De Kerck die looft uw' <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die uyt des hemels <rhyme label="a" type="m" corresp="#s8:l1">throon</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Op d' Aerden is <rhyme label="b" type="f">gekomen</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Sy looft den Heyl'gen <rhyme label="c" type="m">Geest</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Die onse smert <rhyme label="c" type="m" corresp="#s8:l4">geneest</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">G'lijck dickmaels wordt <rhyme label="b" type="f" corresp="#s8:l3">vernomen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Der glory grooten <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Wien toekoomt alle <rhyme label="a" type="m" corresp="#s9:l1">eer</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Zijt Ghy, O Jesu <rhyme label="b" type="f">Christe</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Die ons u dierbaer <rhyme label="c" type="m">bloed</rhyme></l>
        <l id="s9:l5" met="3" type="-+">En vleys, door <rhyme label="c" type="m" corresp="#s9:l4">minne-vloed</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Op 't eynd' uw's levens <rhyme label="b" type="f" corresp="#s9:l3">dischten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ghy hebt gheensins <rhyme label="a" type="m">geschroomt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">(Waerd datmen daer af <rhyme label="a" type="m" corresp="#s10:l1">roomt</rhyme>)</l>
        <l id="s10:l3" met="3" type="-+">Ons' Menscheyd aen te <rhyme label="b" type="f">vaeren</rhyme></l>
        <l id="s10:l4" met="3" type="-+">In 's Maghets suyv're <rhyme label="c" type="m">schoot</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Om ons van d' eeuw'ge <rhyme label="c" type="m" corresp="#s10:l4">dood</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Hier naemaels te <rhyme label="b" type="f" corresp="#s10:l3">bewaeren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ghy hebt des hemels <rhyme label="a" type="m">Deur</rhyme></l>
        <l id="s11:l2" met="3" type="-+">(O wonder groot <rhyme label="a" type="m" corresp="#s11:l1">faveur</rhyme>!)</l>
        <l id="s11:l3" met="3" type="-+">Geopent door u <rhyme label="b" type="f">sterven</rhyme>; ;</l>
        <l id="s11:l4" met="3" type="-+">Door 't sterven aen het <rhyme label="c" type="m">Kruys</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Maeckt Ghy dat 's hemels <rhyme label="c" type="m" corresp="#s11:l4">huys</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Wy naemaels komen 't <rhyme label="b" type="f" corresp="#s11:l3">erven</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Aen Godes <rhyme label="a" type="m">Rechter-zy</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Sit tegenwoordigh <rhyme label="a" type="m" corresp="#s12:l1">Ghy</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">In God uw's Vaders <rhyme label="b" type="f">glory</rhyme>.</l>
        <l id="s12:l4" met="3" type="-+">Ghy sult ons oord'len <rhyme label="c" type="m">all'</rhyme></l>
        <l id="s12:l5" met="3" type="-+">In 't Josaphatsche <rhyme label="c" type="m" corresp="#s12:l4">dal</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">G'lijck tuyght de kercksch' <rhyme label="b" type="f" corresp="#s12:l3">history</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Daerom wy bidden <rhyme label="a" type="m">U</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Helpt uwe dienaers <rhyme label="a" type="m" corresp="#s13:l1">nu</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Helpt hen ten allen <rhyme label="b" type="f">tijden</rhyme>;</l>
        <l id="s13:l4" met="3" type="-+">Die door u dierbaer <rhyme label="c" type="m">bloed</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Van 't vuyle helsch' <rhyme label="c" type="m" corresp="#s13:l4">gebroed</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Eertijds hen vroom <rhyme label="b" type="f" corresp="#s13:l3">bevrijden</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Maeckt dat tot 's hemels <rhyme label="a" type="m">wolck</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Werdt opgevoert u <rhyme label="a" type="m" corresp="#s14:l1">Volck</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Begaeft hen met uw' <rhyme label="b" type="f">glory</rhyme>;</l>
        <l id="s14:l4" met="3" type="-+">Op dat hier naemaels <rhyme label="c" type="m">Sy</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Met d' Heyligen zijn <rhyme label="c" type="m" corresp="#s14:l4">bly</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">En singen steeds <rhyme label="b" type="f" corresp="#s14:l3">victory</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Regeert u <rhyme label="a" type="m">Errifdeel</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Gebenedijdt haer <rhyme label="a" type="m" corresp="#s15:l1">veel</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Maeckt datze zijn <rhyme label="b" type="f">verheven</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Tot 's hemels hooge <rhyme label="c" type="m">hof</rhyme>;</l>
        <l id="s15:l5" met="3" type="-+">Laet singen hen u <rhyme label="c" type="m" corresp="#s15:l4">lof</rhyme></l>
        <l id="s15:l6" met="3" type="-+">In 't eeuwigh duerend' <rhyme label="b" type="f" corresp="#s15:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Wy loven met <rhyme label="a" type="m">jolijt</rhyme></l>
        <l id="s16:l2" met="3" type="-+">U, Heer, ten hooghst' <rhyme label="a" type="m" corresp="#s16:l1">verblijdt</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Van dage 't allen <rhyme label="b" type="f">dagen</rhyme>.</l>
        <l id="s16:l4" met="3" type="-+">Wy loven uwe <rhyme label="c" type="m">Naem</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Uw' Naem die alle <rhyme label="c" type="m" corresp="#s16:l4">faem</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Te boven wordt <rhyme label="b" type="f" corresp="#s16:l3">gedragen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Gewaerdicht 't allen <rhyme label="a" type="m">stond</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Te vrijden ons van <rhyme label="a" type="m" corresp="#s17:l1">sond</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Ontfermt U onser, <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Ontfermt U onser, <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Laet uw's genadens <rhyme label="c" type="m" corresp="#s17:l4">eer</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Op ons van boven <rhyme label="b" type="f" corresp="#s17:l3">keeren</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Laet uw' <rhyme label="a" type="m">bermhartigheyd</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Op ons zijn <rhyme label="a" type="m" corresp="#s18:l1">uytgespreyt</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">G'lijck wy op U <rhyme label="b" type="f">betrouden</rhyme>.</l>
        <l id="s18:l4" met="3" type="-+">Ick heb in U <rhyme label="c" type="m">betrouwt</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Die zijt mijn <rhyme label="c" type="m" corresp="#s18:l4">onderhoudt</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Och of ick U <rhyme label="b" type="f" corresp="#s18:l3">aenschouwden</rhyme>!</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">'k Sal worden dan <rhyme label="a" type="m">beschaemt</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Geensins, maer wel <rhyme label="a" type="m" corresp="#s19:l1">befaemt</rhyme></l>
        <l id="s19:l3" met="3" type="-+">In alle <rhyme label="b" type="f">eeuwigheden</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Wanneer ick komen <rhyme label="c" type="m">sal</rhyme></l>
        <l id="s19:l5" met="3" type="-+">Uyt 's werelts droeve <rhyme label="c" type="m" corresp="#s19:l4">dal</rhyme></l>
        <l id="s19:l6" met="3" type="-+">Tot 's hemels blijde <rhyme label="b" type="f" corresp="#s19:l3">Stede</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
