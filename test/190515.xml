<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190515</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Van de nachtegaal</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Juicht nu, brave <rhyme label="a" type="f">Nederlanders</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zwaaid vol vreugd de <rhyme label="a" type="f" corresp="#s1:l1">Oranjestanders</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">'t Is den blyden Agtsten <rhyme label="b" type="m">Maart</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">'t Is Oranje die <rhyme label="b" type="m" corresp="#s1:l3">verjaard</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt uw Huizen weer <rhyme label="a" type="f">verçieren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En Oranjes jaardag <rhyme label="a" type="f" corresp="#s2:l1">vieren</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Rigt nu Eerepoorten <rhyme label="b" type="m">op</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hyst de Oranje-Vlagt ten <rhyme label="b" type="m" corresp="#s2:l3">top</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vlegt nu Boogen, Kranssen, <rhyme label="a" type="f">Kroonen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wilt uw liefde nu <rhyme label="a" type="f" corresp="#s3:l1">betoonen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Voor Oranje onzen <rhyme label="b" type="m">Vorst</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Zingt nu helder uit uw <rhyme label="b" type="m" corresp="#s3:l3">borst</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Koomt, oprechte <rhyme label="a" type="f">Oranjevrinden</rhyme>!</l>
        <l id="s4:l2" met="4" type="+-">Koomt, ô waare <rhyme label="a" type="f" corresp="#s4:l1">Prinsgezinden</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Vierd den teugel aan uw <rhyme label="b" type="m">vreugd</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En weest op deez' dag <rhyme label="b" type="m" corresp="#s4:l3">verheugd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Lang genoeg in druk <rhyme label="a" type="f">gezeeten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Wilt uw voorig druk <rhyme label="a" type="f" corresp="#s5:l1">vergeeten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Nu den brave <rhyme label="b" type="m">Oranjeheld</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Weer in Luister is <rhyme label="b" type="m" corresp="#s5:l3">hersteld</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wilt u op deez' dag <rhyme label="a" type="f">verblyjen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Gaat weer vrolyk aan de <rhyme label="a" type="f" corresp="#s6:l1">Ryjen</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Zingt tot lof van Nassauws <rhyme label="b" type="m">bloed</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met Oranje op den <rhyme label="b" type="m" corresp="#s6:l3">Hoed</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Koomt nu, Stad- en <rhyme label="a" type="f">Dorpelingen</rhyme>!</l>
        <l id="s7:l2" met="4" type="+-">Wilt met vreugd het Liedje <rhyme label="a" type="f" corresp="#s7:l1">zingen</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">In den Haag en op het <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Zingt men van Oranjes <rhyme label="b" type="m" corresp="#s7:l3">lof</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Niemand zal uw blydschap <rhyme label="a" type="f">stooren</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Zingt dan weer gelyk te <rhyme label="a" type="f" corresp="#s8:l1">vooren</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Breekt den Tak en buigt het <rhyme label="b" type="m">Riet</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Egter treurd Oranje <rhyme label="b" type="m" corresp="#s8:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Onzen Vorst wierd nooit <rhyme label="a" type="f">verlaaten</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Vuige laster kon niet <rhyme label="a" type="f" corresp="#s9:l1">baten</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Want al is de leugen <rhyme label="b" type="m">snel</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Waarheid agterhaald ze <rhyme label="b" type="m" corresp="#s9:l3">wel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Wie zouw thans zyn blyschap <rhyme label="a" type="f">spaaren</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Nu Prins Willem Veertig <rhyme label="a" type="f" corresp="#s10:l1">jaaren</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Op deez' aangenaamen <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Tot ons heil bereiken <rhyme label="b" type="m" corresp="#s10:l3">mag</rhyme>?</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Gy die Neerlands onheil <rhyme label="a" type="f">stigten</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Moet nu voor Oranje <rhyme label="a" type="f" corresp="#s11:l1">zwigten</rhyme>?</l>
        <l id="s11:l3" met="4" type="+-">Wykt van hier, ô snood <rhyme label="b" type="m">gespuis</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Haaters van 't <rhyme label="b" type="m" corresp="#s11:l3">Oranjehuis</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Had gy 't ooit wel durven <rhyme label="a" type="f">denken</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Dat men u genaê zou <rhyme label="a" type="f" corresp="#s12:l1">schenken</rhyme>?</l>
        <l id="s12:l3" met="4" type="+-">Dankt den Vorst en onzen <rhyme label="b" type="m">Staat</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">En beteugelt uwen <rhyme label="b" type="m" corresp="#s12:l3">haat</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Hebt berouw, erkend uw <rhyme label="a" type="f">schanden</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Geeft Oranje hart en <rhyme label="a" type="f" corresp="#s13:l1">handen</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Die verleid zyt en <rhyme label="b" type="m">verblind</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">En blyft altyd <rhyme label="b" type="m" corresp="#s13:l3">Prinsgezind</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Laat ons dan in vreede <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">En vergeeten en <rhyme label="a" type="f" corresp="#s14:l1">vergeeven</rhyme>;</l>
        <l id="s14:l3" met="4" type="+-">Wordt tog nooit weer dol en <rhyme label="b" type="m">zot</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">En geen valsche <rhyme label="b" type="m" corresp="#s14:l3">Patriot</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Legt dan af uw <rhyme label="a" type="f">moordgeweeren</rhyme>;</l>
        <l id="s15:l2" met="4" type="+-">Wilt Oranje met ons <rhyme label="a" type="f" corresp="#s15:l1">eeren</rhyme>;</l>
        <l id="s15:l3" met="4" type="+-">Eerd met ons 't <rhyme label="b" type="m">Oranjebloed</rhyme>;</l>
        <l id="s15:l4" met="4" type="+-">Draagt Oranje op uw <rhyme label="b" type="m" corresp="#s15:l3">hoed</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Laat ons zaamen dan eens <rhyme label="a" type="f">klinken</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">En op WILLEMS welzyn <rhyme label="a" type="f" corresp="#s16:l1">drinken</rhyme>;</l>
        <l id="s16:l3" met="4" type="+-">Doet met ons dan altoos <rhyme label="b" type="m">meê</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Roepen wy Hoezée! <rhyme label="b" type="m" corresp="#s16:l3">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Eert den Prins en <rhyme label="a" type="f">Prinsgezinden</rhyme>.</l>
        <l id="s17:l2" met="4" type="+-">Laat u nooit by Muiters <rhyme label="a" type="f" corresp="#s17:l1">vinden</rhyme>;</l>
        <l id="s17:l3" met="4" type="+-">Leeft met ons in vrede en <rhyme label="b" type="m">rust</rhyme>;</l>
        <l id="s17:l4" met="4" type="+-">Laat het twist-vuur zyn <rhyme label="b" type="m" corresp="#s17:l3">geblust</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Wilt met ons in Oranje <rhyme label="a" type="f">looven</rhyme>!</l>
        <l id="s18:l2" met="4" type="+-">Roept met ons Oranje <rhyme label="a" type="f" corresp="#s18:l1">booven</rhyme>!</l>
        <l id="s18:l3" met="4" type="+-">Toont dat gy thans zyt <rhyme label="b" type="m">verblyd</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">En ook braave Burgers <rhyme label="b" type="m" corresp="#s18:l3">zyt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Zyt gehoorzaam aan de <rhyme label="a" type="f">Staaten</rhyme>;</l>
        <l id="s19:l2" met="4" type="+-">Wilt Oranje nooit <rhyme label="a" type="f" corresp="#s19:l1">verlaaten</rhyme>.</l>
        <l id="s19:l3" met="4" type="+-">Als uw Regt ooit wordt <rhyme label="b" type="m">verkragt</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Doet aan Hem dan uwe <rhyme label="b" type="m" corresp="#s19:l3">klagt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">WILLEM zal het vonnis <rhyme label="a" type="f">vellen</rhyme>,</l>
        <l id="s20:l2" met="4" type="+-">En uw Regten weer <rhyme label="a" type="f" corresp="#s20:l1">herstellen</rhyme>;</l>
        <l id="s20:l3" met="4" type="+-">Want hy heeft een Eed <rhyme label="b" type="m">gedaan</rhyme>,</l>
        <l id="s20:l4" met="4" type="+-">Om 's Lands Regten voor te <rhyme label="b" type="m" corresp="#s20:l3">staan</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Hy is 't Hoofd der <rhyme label="a" type="f">Burgerschaaren</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Om hun Regten te <rhyme label="a" type="f" corresp="#s21:l1">bewaaren</rhyme>;</l>
        <l id="s21:l3" met="4" type="+-">'t Is uw Voorspraak by den <rhyme label="b" type="m">Staat</rhyme>;</l>
        <l id="s21:l4" met="4" type="+-">'t Is uw Hulp en <rhyme label="b" type="m" corresp="#s21:l3">Toeverlaat</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Dat hy leev tot onze <rhyme label="a" type="f">blyheid</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Voor de Godsdienst, voor de <rhyme label="a" type="f" corresp="#s22:l1">Vryheld</rhyme>!</l>
        <l id="s22:l3" met="4" type="+-">Dat geen ramp of <rhyme label="b" type="m">tegenspoed</rhyme></l>
        <l id="s22:l4" met="4" type="+-">Onze braven Vorst <rhyme label="b" type="m" corresp="#s22:l3">ontmoet</rhyme>!</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Dat Oranje's <rhyme label="a" type="f">Gemalinne</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Neêrlands Moeder en <rhyme label="a" type="f" corresp="#s23:l1">Vorstinne</rhyme>,</l>
        <l id="s23:l3" met="4" type="+-">Heil en Zegen, Roem en <rhyme label="b" type="m">Eer</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Moog verkrygen van den <rhyme label="b" type="m" corresp="#s23:l3">Heer</rhyme>!</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Dat de Vorstelyke <rhyme label="a" type="f">Looten</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Uit de Oranje stam <rhyme label="a" type="f" corresp="#s24:l1">gesprooten</rhyme>;</l>
        <l id="s24:l3" met="4" type="+-">Vol van Heldenmoed en <rhyme label="b" type="m">Deugd</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">Leeven tot 's Lands heil en <rhyme label="b" type="m" corresp="#s24:l3">vreugd</rhyme>!</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Dat de Oranjestam moog <rhyme label="a" type="f">groeijen</rhyme>!</l>
        <l id="s25:l2" met="4" type="+-">Dat Hy geurig staa te <rhyme label="a" type="f" corresp="#s25:l1">bloeijen</rhyme>!</l>
        <l id="s25:l3" met="4" type="+-">Dat Hy vrugten draagen <rhyme label="b" type="m">mag</rhyme>,</l>
        <l id="s25:l4" met="4" type="+-">Tot aan 's Weerelds jongsten <rhyme label="b" type="m" corresp="#s25:l3">dag</rhyme>!</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">Lang leev Willem van <rhyme label="a" type="f">Oranje</rhyme>!</l>
        <l id="s26:l2" met="4" type="+-">Met Fred'rika van <rhyme label="a" type="f" corresp="#s26:l1">Germanje</rhyme>!</l>
        <l id="s26:l3" met="4" type="+-">Drinken wy tot een <rhyme label="b" type="m">besluit</rhyme>,</l>
        <l id="s26:l4" met="4" type="+-">Deezen vollen Beker <rhyme label="b" type="m" corresp="#s26:l3">uit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
