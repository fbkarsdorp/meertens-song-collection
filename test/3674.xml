    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 031 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYN hop' stell' ick op v <rhyme label="a" type="f">ghestaedigh</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wilt my bewaeren <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Voor een eewigh' <rhyme label="b" type="m" corresp="#s1:l2">oneer</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Verlost my O myn Godt <rhyme label="a" type="f" corresp="#s1:l1">ghenaedigh</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Doer v' goedtheit <rhyme label="c" type="f">ghepresen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wilt my bystandigh <rhyme label="c" type="f" corresp="#s1:l5">wesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Helpt my end' opent my v' <rhyme label="a" type="f">ooren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Weest doch myn <rhyme label="b" type="m">toeuerlaet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">In den noot my <rhyme label="b" type="m" corresp="#s2:l2">bystaet</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Ghy hebt my te helpen <rhyme label="a" type="f" corresp="#s2:l1">ghesworen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Gheen sterckten groot noch <rhyme label="c" type="f">kleyne</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hebb' ick, dan v <rhyme label="c" type="f" corresp="#s2:l5">alleyne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wt der boosen handen <rhyme label="a" type="f">onreene</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wt der tyrannen <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die gheweldt doen in't <rhyme label="b" type="m" corresp="#s3:l2">landt</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Helpt my: Want troost noch helpe <rhyme label="a" type="f" corresp="#s3:l1">gheene</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En hadd' ick al myn <rhyme label="c" type="f">leuen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dan v O Heer <rhyme label="c" type="f" corresp="#s3:l5">verheuen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">So haest als ick hier was <rhyme label="a" type="f">gheboren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wt myns moeders <rhyme label="b" type="m">lichaem</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hebb' ick op uwen <rhyme label="b" type="m" corresp="#s4:l2">naem</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Myn hop' ghestelt Heer <rhyme label="a" type="f" corresp="#s4:l1">wtverkoren</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Vwen naem t'allen <rhyme label="c" type="f">tyden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Verbreydd' ick met <rhyme label="c" type="f" corresp="#s4:l5">verblyden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Men houdt my voor een seer vremt <rhyme label="a" type="f">wonder</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Doch ghy sijt Heer myn <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Myn toevlucht dagh end <rhyme label="b" type="m" corresp="#s5:l2">nacht</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Maeckt dat ick magh sijn een <rhyme label="a" type="f" corresp="#s5:l1">verkonder</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Vw's lofs end uwer <rhyme label="c" type="f">eeren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">End die steedts magh <rhyme label="c" type="f" corresp="#s5:l5">vermeeren</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als ick Heer oudt end koudt sal <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">End swack, vol van <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wilt my verwerpen <rhyme label="b" type="m" corresp="#s6:l2">niet</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Als ick oock sal sijn Heer' <rhyme label="a" type="f" corresp="#s6:l1">ghepresen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Elendigh bouen <rhyme label="c" type="f">maeten</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Wilt my dan niet <rhyme label="c" type="f" corresp="#s6:l5">verlaeten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Want teghen my houden te <rhyme label="a" type="f">saeme</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Myn vianden seer <rhyme label="b" type="m">quaet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Eenen listighen <rhyme label="b" type="m" corresp="#s7:l2">raet</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">End teghen my wreedt end <rhyme label="a" type="f" corresp="#s7:l1">grousaeme</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Met een se hen <rhyme label="c" type="f">verbinden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die my willen <rhyme label="c" type="f" corresp="#s7:l5">verslinden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Sy spreken: Haest laet ons hem <rhyme label="a" type="f">vanghen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Want gheen hulpe <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Vindt hy verre noch <rhyme label="b" type="m" corresp="#s8:l2">naer</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Dies wilt van my in dit <rhyme label="a" type="f" corresp="#s8:l1">verlanghen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Niet wycken, maer v <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Tot myn hulp' haesten <rhyme label="c" type="f" corresp="#s8:l5">seere</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die haeters die my staen nae <rhyme label="a" type="f">t'leuen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Moeten beschaemt sijn <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">End haest kommen ten <rhyme label="b" type="m" corresp="#s9:l2">val</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Die tot myn schaede hen <rhyme label="a" type="f" corresp="#s9:l1">begheuen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Laets O Heer sijn <rhyme label="c" type="f">mispresen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">End met spot bedeckt <rhyme label="c" type="f" corresp="#s9:l5">wesen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Bouen de daeghlicksche <rhyme label="a" type="f">lofsanghen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Sal van my sijn <rhyme label="b" type="m">verbreydt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Heer v' <rhyme label="b" type="m" corresp="#s10:l2">gherechticheit</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Die gaeuen die ick hebb' <rhyme label="a" type="f" corresp="#s10:l1">ontfanghen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Die niet sijn om <rhyme label="c" type="f">doergronden</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Sal ick altyt <rhyme label="c" type="f" corresp="#s10:l5">verkonden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ick sal vrymoedighlick daer <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">End sien die wercken <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Die ghy Heer hebt <rhyme label="b" type="m" corresp="#s11:l2">ghedaen</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Van my werden altyt <rhyme label="a" type="f" corresp="#s11:l1">beleden</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">V' oprechte <rhyme label="c" type="f">voetpaeden</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">End v' groote <rhyme label="c" type="f" corresp="#s11:l5">weldaeden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Van jonghsaen hebb'ick Heer <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Vwe wercken <rhyme label="b" type="m">verstaen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Daer van doende <rhyme label="b" type="m" corresp="#s12:l2">vermaen</rhyme>.</l>
        <l id="s12:l4" met="4" type="-+">Dies als ick oudt end grys sal <rhyme label="a" type="f" corresp="#s12:l1">wesen</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Wyckt dan niet van my <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Dien ick alleyn <rhyme label="c" type="f" corresp="#s12:l5">vereere</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Tot dat ick allen die nu <rhyme label="a" type="f">leuen</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">End' oock haeren <rhyme label="b" type="m">gheslacht</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Verklaert hebb'uwe <rhyme label="b" type="m" corresp="#s13:l2">macht</rhyme>.</l>
        <l id="s13:l4" met="4" type="-+">V' oordeelen sijn hoogh' <rhyme label="a" type="f" corresp="#s13:l1">verheuen</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Daer doer ghy, so wy <rhyme label="c" type="f">mercken</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Doedt seer veel <rhyme label="c" type="f" corresp="#s13:l5">wonderwercken</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Wie is met v te <rhyme label="a" type="f">verghelycken</rhyme>?</l>
        <l id="s14:l2" met="3" type="-+">Die my proeft met angst <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Kruys end' allerley <rhyme label="b" type="m" corresp="#s14:l2">noot</rhyme>.</l>
        <l id="s14:l4" met="4" type="-+">Die my geeft van nieuws soo'tmagh <rhyme label="a" type="f" corresp="#s14:l1">blycken</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">T'leuen tot myn <rhyme label="c" type="f">oorboren</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">T'welck scheen tesijn <rhyme label="c" type="f" corresp="#s14:l5">verloren</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Wt den diepen put onder <rhyme label="a" type="f">d'ęerde</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Hebt ghy my gheuoert <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">End myn ryck verbreydt <rhyme label="b" type="m" corresp="#s15:l2">seer</rhyme>.</l>
        <l id="s15:l4" met="4" type="-+">End'als ick ouerweldight <rhyme label="a" type="f" corresp="#s15:l1">werde</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Met v' aenghesicht <rhyme label="c" type="f">krachtigh</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Troost ghy my, Heer <rhyme label="c" type="f" corresp="#s15:l5">almachtigh</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Dies sal ick op den Psalter <rhyme label="a" type="f">spelen</rhyme>.</l>
        <l id="s16:l2" met="3" type="-+">End singhen met <rhyme label="b" type="m">bescheydt</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Van v' <rhyme label="b" type="m" corresp="#s16:l2">ghetrauwicheit</rhyme>.</l>
        <l id="s16:l4" met="4" type="-+">Godt Israels, sonder <rhyme label="a" type="f" corresp="#s16:l1">vervelen</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Sal ick v' lof <rhyme label="c" type="f">voortbringhen</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Op die harp' end v <rhyme label="c" type="f" corresp="#s16:l5">singhen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Myn lippen haer in v <rhyme label="a" type="f">verblyden</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">End prysen oock <rhyme label="b" type="m">eenpaer</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Vwe heerlickheit <rhyme label="b" type="m" corresp="#s17:l2">klaer</rhyme>.</l>
        <l id="s17:l4" met="4" type="-+">Myn siel' die ghy hebt wt dat <rhyme label="a" type="f" corresp="#s17:l1">lyden</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Ghebroght, die is <rhyme label="c" type="f">ontsteken</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Met vrueghdt Heer <rhyme label="c" type="f" corresp="#s17:l5">onbesweken</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Myn tongh' wilick v <rhyme label="a" type="f">ouergheuen</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Om v' <rhyme label="b" type="m">gherechticheit</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Te melden <rhyme label="b" type="m" corresp="#s18:l2">ouerbreydt</rhyme>.</l>
        <l id="s18:l4" met="4" type="-+">Ghy verderft die boos' <rhyme label="a" type="f" corresp="#s18:l1">onghenaedigh</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Dat se met schanden <rhyme label="c" type="f">steruen</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Die soecken myn <rhyme label="c" type="f" corresp="#s18:l5">verderuen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
