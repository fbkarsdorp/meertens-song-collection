        <l id="s2:l4" met="3" type="-+">Je fais quil faut <rhyme label="c" type="m">marcher</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Consolez vous <rhyme label="d" type="f">machere</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s2:l2">mirontaine</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Consolez vous <rhyme label="d" type="f" corresp="#s2:l5">machere</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dans peu je <rhyme label="c" type="m" corresp="#s2:l4">reviendrai</rhyme></l>
      </lg>
