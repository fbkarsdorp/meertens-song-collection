        <l id="s3:l4" met="3" type="-+">Vous m’allez voir <rhyme label="c" type="m">marcher</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Je reviendrai a <rhyme label="d" type="f">pâques</rhyme>,</l>
