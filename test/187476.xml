        <l id="s6:l3" met="3" type="-+">Je vous dirai <rhyme label="a" type="f" corresp="#s6:l1">Madame</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vos beaux yeux vont <rhyme label="c" type="m">pleurer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Quittez vos habits <rhyme label="d" type="f">Roses</rhyme>,</l>
