  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Malbrouk s’en va en <rhyme label="a" type="f">Guerre</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Malbrouk s’en va en <rhyme label="a" type="f" corresp="#s1:l1">guerre</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Avec sa grande <rhyme label="c" type="m">armeé</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Pour sa femme quelle <rhyme label="d" type="f">peine</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s1:l2">mirontaine</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Pour sa femme quelle <rhyme label="d" type="f" corresp="#s1:l5">peine</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Fout il nous <rhyme label="c" type="m" corresp="#s1:l4">séparer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
