<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178412</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Liefste lief wanneer zal het wezen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Juicht nu vry, verdrukte <rhyme label="a" type="f">Schaaren</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Juicht nu <rhyme label="a" type="f" corresp="#s1:l1">Rotterdammenaaren</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Bly van geest, bly van <rhyme label="b" type="m">geest</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">'t Is van daag een vrolyk <rhyme label="b" type="m" corresp="#s1:l3">Feest</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'s Lands Vorstin, die wy <rhyme label="c" type="f">beminnen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Vaart den Maas stroom weder <rhyme label="c" type="f" corresp="#s1:l5">binnen</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Z'is weerom: Z'is <rhyme label="d" type="m">weerom</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Vivat, zy is <rhyme label="d" type="m" corresp="#s1:l7">wellekom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ziet de Maas met Schepen <rhyme label="a" type="f">pryken</rhyme>!</l>
        <l id="s2:l2" met="4" type="+-">Vlag en Wimpels doen nog <rhyme label="a" type="f" corresp="#s2:l1">blyken</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Waar men vind, waar men <rhyme label="b" type="m">vind</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Daar Oranje word <rhyme label="b" type="m" corresp="#s2:l3">bemind</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Ziet! dat Jagt, wie kan het <rhyme label="c" type="f">deeren</rhyme>?</l>
        <l id="s2:l6" met="4" type="+-">Vlagt, die dierb're Vrouw ter <rhyme label="c" type="f" corresp="#s2:l5">eeren</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Al 't gedruis, al 't <rhyme label="d" type="m">gedruis</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Spyt het Loevesteins <rhyme label="d" type="m" corresp="#s2:l7">gespuis</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat de Muitzucht ons <rhyme label="a" type="f">bestryden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">God zy dank voor Zyn <rhyme label="a" type="f" corresp="#s3:l1">gelyden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">In deez' tyd, in deez' <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Vol van tweedracht, haat en <rhyme label="b" type="m" corresp="#s3:l3">nyd</rhyme>!</l>
        <l id="s3:l5" met="4" type="+-">Hy heeft haar nu weer <rhyme label="c" type="f">beveiligd</rhyme>.</l>
        <l id="s3:l6" met="4" type="+-">Van ons zy Zyn doen <rhyme label="c" type="f" corresp="#s3:l5">geheiligd</rhyme>!</l>
        <l id="s3:l7" met="4" type="+-">'t Is zyn hand, 't Is zyn <rhyme label="d" type="m">hand</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Die nog stryd voor 't <rhyme label="d" type="m" corresp="#s3:l7">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kunnen wy meer vreugd <rhyme label="a" type="f">genieten</rhyme>?</l>
        <l id="s4:l2" met="4" type="+-">Hoord het Jagt eens helder <rhyme label="a" type="f" corresp="#s4:l1">schieten</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">'t Speel geluit, 't Speel <rhyme label="b" type="m">geluit</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Van Wilhelmus lokt ons <rhyme label="b" type="m" corresp="#s4:l3">uit</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Wilt den Vorst nog meer <rhyme label="c" type="f">verheffen</rhyme>:</l>
        <l id="s4:l6" met="4" type="+-">Want dat kan ons harte <rhyme label="c" type="f" corresp="#s4:l5">treffen</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Gy doed goed, gy doed <rhyme label="d" type="m">goed</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Aan ons Vaderlandsch <rhyme label="d" type="m" corresp="#s4:l7">gemoed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Veerlui! laat uw vlaggens <rhyme label="a" type="f">waajen</rhyme>:</l>
        <l id="s5:l2" met="4" type="+-">'t Spyt slegts die, die oproer <rhyme label="a" type="f" corresp="#s5:l1">zaajen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Wie doed mee? Wie doed <rhyme label="b" type="m">mee</rhyme>?</l>
        <l id="s5:l4" met="4" type="+-">Hoord dat lieffelyk <rhyme label="b" type="m" corresp="#s5:l3">Hoez√©e</rhyme>!</l>
        <l id="s5:l5" met="4" type="+-">Bosch en Veld schynd te <rhyme label="c" type="f">beweegen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Alles is tot vreugd <rhyme label="c" type="f" corresp="#s5:l5">geneegen</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Ziet, de vloed! ziet de <rhyme label="d" type="m">vloed</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Speelt rondsom dien blijden <rhyme label="d" type="m" corresp="#s5:l7">stoet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die de Vryheid doen <rhyme label="a" type="f">bezwyken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Staan als Jongens weer te <rhyme label="a" type="f" corresp="#s6:l1">kijken</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Hoe bedest, hoe <rhyme label="b" type="m">bedest</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Staan zij met hun <rhyme label="b" type="m" corresp="#s6:l3">kraaijenest</rhyme>?</l>
        <l id="s6:l5" met="4" type="+-">Zij vergeeten te <rhyme label="c" type="f">exerceeren</rhyme>:</l>
        <l id="s6:l6" met="4" type="+-">Want die Nieuwerwetsche <rhyme label="c" type="f" corresp="#s6:l5">Heeren</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Zijn vol schrik, zijn vol <rhyme label="d" type="m">schrik</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Om die heuchelijk <rhyme label="d" type="m" corresp="#s6:l7">oogenblik</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wat gedaan, voor deeze <rhyme label="a" type="f">vuigen</rhyme>!</l>
        <l id="s7:l2" met="4" type="+-">Al hun arbeid legt in <rhyme label="a" type="f" corresp="#s7:l1">duigen</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Zij zien klaar, zij zien <rhyme label="b" type="m">klaar</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat hun Plan lijd groot <rhyme label="b" type="m" corresp="#s7:l3">gevaar</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Want zij kunnen niet <rhyme label="c" type="f">verkrijgen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Dat de Prins-Gezinden <rhyme label="c" type="f" corresp="#s7:l5">zwijgen</rhyme>:</l>
        <l id="s7:l7" met="4" type="+-">Wij geen voet, wij geen <rhyme label="d" type="m">voet</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Wijken voor dat zwart <rhyme label="d" type="m" corresp="#s7:l7">gebroed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Nu tot lof met dankb're <rhyme label="a" type="f">tongen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Voor Oranjes naam <rhyme label="a" type="f" corresp="#s8:l1">gezongen</rhyme>!</l>
        <l id="s8:l3" met="4" type="+-">Braave Stad! braave <rhyme label="b" type="m">Stad</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Och! dat gij veel volgers <rhyme label="b" type="m" corresp="#s8:l3">had</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Dan zou haast het eind <rhyme label="c" type="f">gelukken</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Van 's Lands braven te <rhyme label="c" type="f" corresp="#s8:l5">onderdrukken</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Blijft getrouw! blijft <rhyme label="d" type="m">getrouw</rhyme>!</l>
        <l id="s8:l8" met="4" type="+-">Waak voor Willem en zijn <rhyme label="d" type="m" corresp="#s8:l7">Vrouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
