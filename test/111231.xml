    <melody>Van Paul Jonas en de Engelsman</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Wel <rhyme label="a" type="m">Engeland</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat hadje nooit <rhyme label="b" type="m">gedagt</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Maer je moest dog <rhyme label="c" type="f">vallen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">In de Franse <rhyme label="b" type="m" corresp="#s1:l2">magt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ge meende na u <rhyme label="d" type="m">sin</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Alles te neemen <rhyme label="d" type="m" corresp="#s1:l5">in</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Den Haen heeft <rhyme label="e" type="f">Spooren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Geeft nog geen moet <rhyme label="e" type="f" corresp="#s1:l7">verlooren</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Agt hem niet te <rhyme label="d" type="m" corresp="#s1:l5">min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ik ben <rhyme label="a" type="m">Engeland</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Nog evenwel <rhyme label="b" type="m">konstant</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ik zal nog Koning <rhyme label="c" type="f">blyven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Of 't waer eens <rhyme label="b" type="m" corresp="#s2:l2">chand</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Daerom vrees ik u <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ik heb nog geen <rhyme label="d" type="m" corresp="#s2:l5">verdriet</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Laet nu de Haen maer <rhyme label="e" type="f">kraeijen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">De oranje Vlaggen <rhyme label="e" type="f" corresp="#s2:l7">waeijen</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Daerom agt ik u <rhyme label="d" type="m" corresp="#s2:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Met drie <rhyme label="a" type="m">Fregatten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Trokken wy <rhyme label="b" type="m">gelaen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met een bly <rhyme label="c" type="f">gemoet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">op St. Eustatius <rhyme label="b" type="m" corresp="#s3:l2">aen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">In 't midden van de <rhyme label="d" type="m">Nagt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Daer niemant op en <rhyme label="d" type="m" corresp="#s3:l5">dagt</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Gy meende te <rhyme label="e" type="f">ontkomen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">U Volk zoo <rhyme label="e" type="f" corresp="#s3:l7">genoomen</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">In de Prison <rhyme label="d" type="m" corresp="#s3:l5">gebragt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ach Koning <rhyme label="a" type="m">Lodewyk</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik ben daerom niet <rhyme label="b" type="m">ontsteld</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gy vegt <rhyme label="c" type="f">Republiek</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Om 't Hollands <rhyme label="b" type="m" corresp="#s4:l2">geld</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Doet al maer watje <rhyme label="d" type="m">kan</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ik blyf den <rhyme label="d" type="m" corresp="#s4:l5">Engelsman</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Niemand kan myn <rhyme label="e" type="f">deeren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">myn Volk zal u <rhyme label="e" type="f" corresp="#s4:l7">keeren</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">'t Kruid is op de <rhyme label="d" type="m" corresp="#s4:l5">pan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Zwyg <rhyme label="a" type="m">Engeland</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Voor my in korten <rhyme label="b" type="m">stond</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Of ik zal u doen <rhyme label="c" type="f">zwemmen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Als een <rhyme label="b" type="m" corresp="#s5:l2">water-Hond</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Myn Helden staen <rhyme label="d" type="m">bereid</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Voor Engeland ten <rhyme label="d" type="m" corresp="#s5:l5">stryd</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Ik heb 300 <rhyme label="e" type="f">Schepen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die u nog zullen <rhyme label="e" type="f" corresp="#s5:l7">sleepen</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">'t Is uw een groote <rhyme label="d" type="m" corresp="#s5:l5">spyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Den Fransen <rhyme label="a" type="m">Haen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">ben ik niet voor <rhyme label="b" type="m">vervaert</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Al schoon is <rhyme label="c" type="f">Amerika</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met u <rhyme label="b" type="m" corresp="#s6:l2">gepaert</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Gibralter is <rhyme label="d" type="m">assurant</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die vreesen geen <rhyme label="d" type="m" corresp="#s6:l5">vijand</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">En Port-mahon <rhyme label="e" type="f">beneeven</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Daer ieder voor moet <rhyme label="e" type="f" corresp="#s6:l7">beeven</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Zy vegten <rhyme label="d" type="m" corresp="#s6:l5">florisant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Zeg <rhyme label="a" type="m">Engeland</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">'t moet nog anders <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Al benje zo <rhyme label="c" type="f">assurant</rhyme></l>
        <l id="s7:l4" met="3" type="-+">'t gaet 'er weer op <rhyme label="b" type="m" corresp="#s7:l2">aen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Met de Hollandse <rhyme label="d" type="m">Vloot</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Versien van kruid en <rhyme label="d" type="m" corresp="#s7:l5">lood</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Bentink, <rhyme label="e" type="f">Kingsbergen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Van Braem liet hem niet <rhyme label="e" type="f" corresp="#s7:l7">tergen</rhyme></l>
        <l id="s7:l9" met="3" type="+-">Wy hebben nog geen <rhyme label="d" type="m" corresp="#s7:l5">noot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Lodewyk den <rhyme label="a" type="m">Koning</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Sprak dappere <rhyme label="b" type="m">Zoutman</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ik slae hem uit zyn <rhyme label="c" type="f">woning</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Toont maer wat je <rhyme label="b" type="m" corresp="#s8:l2">kan</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Deedel en <rhyme label="d" type="m">Braek</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Sy vogten met <rhyme label="d" type="m" corresp="#s8:l5">vermaek</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">De Hollandse <rhyme label="e" type="f">Schepen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Die zullen hem weer <rhyme label="e" type="f" corresp="#s8:l7">sleepen</rhyme></l>
        <l id="s8:l9" met="3" type="+-">En neemen <rhyme label="d" type="m" corresp="#s8:l5">wederwraek</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Zeg <rhyme label="a" type="m">Groot-Brittanje</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Gy hebt ons vals <rhyme label="b" type="m">bediend</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wy meende dat <rhyme label="c" type="f">Oranje</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Was u beste <rhyme label="b" type="m" corresp="#s9:l2">vriend</rhyme></l>
        <l id="s9:l5" met="3" type="-+">'t Komt nu anders <rhyme label="d" type="m">uit</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Gy krygt wat op u <rhyme label="d" type="m" corresp="#s9:l5">Huid</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">wilt tog maer <rhyme label="e" type="f">komen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Op de Zoute <rhyme label="e" type="f" corresp="#s9:l7">stroomen</rhyme>?</l>
        <l id="s9:l9" met="3" type="+-">Hyst maer de Bloed-Vlag <rhyme label="d" type="m" corresp="#s9:l5">uit</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Ik ben de <rhyme label="a" type="m">man</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Genaamt de <rhyme label="b" type="m">Lelie-Vorst</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">De Dog die zal der <rhyme label="c" type="f">an</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Ik heb na 't bloed <rhyme label="b" type="m" corresp="#s10:l2">gedorst</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Ik draeg getrouw 't <rhyme label="d" type="m">zwaerd</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Holland weest niet <rhyme label="d" type="m" corresp="#s10:l5">vervaerd</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Men zag 't aen <rhyme label="e" type="f">Parker</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Den valsen Hond was <rhyme label="e" type="f" corresp="#s10:l7">starker</rhyme>,</l>
        <l id="s10:l9" met="3" type="+-">Is ons geen Oortje <rhyme label="d" type="m" corresp="#s10:l5">waert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
