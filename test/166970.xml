<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>166970</id>
    <date>
      <year-start>1671</year-start>
      <year-end>1671</year-end>
    </date>
    <location>
      <orginal-name>harlingen</orginal-name>
      <standard-name>Harlingen, The Netherlands</standard-name>
      <coordinates>
        <lon>53.1752103</lon>
        <lat>5.4141998</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Les Feuillantines</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlje wercken op goe <rhyme label="a" type="m">voet</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Dan soo <rhyme label="a" type="m" corresp="#s1:l1">moet</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Ghy wel letten wat elck <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Ghy moet yder Beest <rhyme label="b" type="f">bestuure</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Na de <rhyme label="c" type="m">drift</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">na di <rhyme label="c" type="m" corresp="#s1:l5">drift</rhyme></l>
        <l id="s1:l7" met="2" type="-+">van sijn <rhyme label="b" type="f" corresp="#s1:l4">natuure</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Heb je eenen moedigh <rhyme label="a" type="m">Paert</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Schatten <rhyme label="a" type="m" corresp="#s2:l1">waert</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Laet het volgen sijnen <rhyme label="a" type="m" corresp="#s2:l1">aert</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En laet het u selven <rhyme label="b" type="f">dragen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Of het <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s2:l6" met="2" type="+-">of het <rhyme label="c" type="m" corresp="#s2:l5">moet</rhyme></l>
        <l id="s2:l7" met="2" type="-+">gaen voor de <rhyme label="b" type="f" corresp="#s2:l4">wagen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hebj' een Os, soo stelt hem <rhyme label="a" type="m">vroegh</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Voor de <rhyme label="a" type="m" corresp="#s3:l1">ploegh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Want het is hem spuls <rhyme label="a" type="m" corresp="#s3:l1">genoegh</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Maer kan hy dat niet <rhyme label="b" type="f">betrachten</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Weyt hem <rhyme label="c" type="m">ver</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">weyt hem <rhyme label="c" type="m" corresp="#s3:l5">ver</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">en wilt hem <rhyme label="b" type="f" corresp="#s3:l4">slachten</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hebje Koeyen, geeft haer <rhyme label="a" type="m">Gras</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Op haer <rhyme label="a" type="m" corresp="#s4:l1">pas</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Anders het noyt voordeel <rhyme label="a" type="m" corresp="#s4:l1">was</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Want sy moeten der van <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">En dan <rhyme label="c" type="m">oock</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">en dan <rhyme label="c" type="m" corresp="#s4:l5">oock</rhyme></l>
        <l id="s4:l7" met="2" type="-+">haer Melleck <rhyme label="b" type="f" corresp="#s4:l4">geven</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hebje Verckens, geeft haer <rhyme label="a" type="m">veel</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Garsten <rhyme label="a" type="m" corresp="#s5:l1">meel</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Yder rijckelijck sijn <rhyme label="a" type="m" corresp="#s5:l1">deel</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Geeft haer wey van uwe <rhyme label="b" type="f">Koeyen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Want sy <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">want sy <rhyme label="c" type="m" corresp="#s5:l5">moet</rhyme></l>
        <l id="s5:l7" met="2" type="-+">er vet van <rhyme label="b" type="f" corresp="#s5:l4">groeyen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maer indien ghy hebt een <rhyme label="a" type="m">Kat</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Geeft het <rhyme label="a" type="m" corresp="#s6:l1">wat</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Van het witte Koeye <rhyme label="a" type="m" corresp="#s6:l1">nat</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Laet het woonen in u <rhyme label="b" type="f">huysen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Want het <rhyme label="c" type="m">vanght</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">want het <rhyme label="c" type="m" corresp="#s6:l5">vangt</rhyme></l>
        <l id="s6:l7" met="2" type="-+">er vele <rhyme label="b" type="f" corresp="#s6:l4">Muysen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">En indien ghy hebt een <rhyme label="a" type="m">Hoen</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Wilt het <rhyme label="a" type="m" corresp="#s7:l1">voen</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Wat sal 't arme diertjen <rhyme label="a" type="m" corresp="#s7:l1">doen</rhyme>?</l>
        <l id="s7:l4" met="4" type="+-">Daer en valt niet op te <rhyme label="b" type="f">seggen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Want het <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">want het <rhyme label="c" type="m" corresp="#s7:l5">doet</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">ons Eyers <rhyme label="b" type="f" corresp="#s7:l4">leggen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ende hebje noch wat <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Als een <rhyme label="a" type="m" corresp="#s8:l1">Heer</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Houdt u Beestjes frays in <rhyme label="a" type="m" corresp="#s8:l1">eer</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Want men moet na reden <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Self een <rhyme label="c" type="m">Beest</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">self een <rhyme label="c" type="m" corresp="#s8:l5">Beest</rhyme></l>
        <l id="s8:l7" met="2" type="-+">het sijne <rhyme label="b" type="f" corresp="#s8:l4">geven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
