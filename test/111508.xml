<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>111508</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kinderlied" translation="children's song">kinderlied</tag>
    </tags>
    <melody>Tircis dans un bois ténébreux</melody>
    <categories>
      <category translation="children">kinderen</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALs Thirsis sig in 't Bosch <rhyme label="a" type="m">bevond</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">ALs Thirsis sig in 't Bosch <rhyme label="a" type="m" corresp="#s1:l1">bevond</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Sprak hy tot die hem had <rhyme label="a" type="m" corresp="#s1:l1">doorwond</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ik sterf, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ik sterf, myn zoete lieve <rhyme label="b" type="f" corresp="#s1:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Iris neem dog myn sugjes <rhyme label="a" type="m">an</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Iris neem dog myn sugjes <rhyme label="a" type="m" corresp="#s2:l1">an</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want niemant my genesen <rhyme label="a" type="m" corresp="#s2:l1">kan</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Als gy, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Als gy, myn zoete lieve <rhyme label="b" type="f" corresp="#s2:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want waar sag men ooit sulk een <rhyme label="a" type="m">Maagd</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Want waar sag men ooit sulk een <rhyme label="a" type="m" corresp="#s3:l1">Maagd</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die myne ziel dus heeft <rhyme label="a" type="m" corresp="#s3:l1">behaagd</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Als gy, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Als gy, myn zoete lieve <rhyme label="b" type="f" corresp="#s3:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dit tuygen zelf de <rhyme label="a" type="m">Rooseblaan</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dit tuygen zelf de <rhyme label="a" type="m" corresp="#s4:l1">Rooseblaan</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die op de lieve kaakjes <rhyme label="a" type="m" corresp="#s4:l1">staan</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f" corresp="#s4:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Het Elpenbeen en <rhyme label="a" type="m">Lelie-wit</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Het Elpenbeen en <rhyme label="a" type="m" corresp="#s5:l1">Lelie-wit</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dat op het gladde Voorhooft <rhyme label="a" type="m" corresp="#s5:l1">sit</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f" corresp="#s5:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Geen Incarnaat in 's <rhyme label="a" type="m">Werelds-rond</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Geen Incarnaat in 's <rhyme label="a" type="m" corresp="#s6:l1">Werelds-rond</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Is schoonder, dan de rode <rhyme label="a" type="m" corresp="#s6:l1">mond</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f" corresp="#s6:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Veel schoonder als de <rhyme label="a" type="m">Dageraad</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Veel schoonder als de <rhyme label="a" type="m" corresp="#s7:l1">Dageraad</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Is 't Zielbetoverend <rhyme label="a" type="m" corresp="#s7:l1">gelaat</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f" corresp="#s7:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Indien Jupyn had 's Menschen <rhyme label="a" type="m">oog</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Indien Jupyn had 's Menschen <rhyme label="a" type="m" corresp="#s8:l1">oog</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Hy haalde wis uyt 's Hemels <rhyme label="a" type="m" corresp="#s8:l1">Boog</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Om u, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Om u, myn zoete lieve <rhyme label="b" type="f" corresp="#s8:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hoe hoog men d'Ed'le wyn ook <rhyme label="a" type="m">schat</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Hoe hoog men d'Ed'le wyn ook <rhyme label="a" type="m" corresp="#s9:l1">schat</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Veel liever ik klaar water <rhyme label="a" type="m" corresp="#s9:l1">had</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Met u, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Met u, myn zoete lieve <rhyme label="b" type="f" corresp="#s9:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">'k Lach met een anders gunst of <rhyme label="a" type="m">druk</rhyme></l>
        <l id="s10:l2" met="4" type="-+">'k Lach met een anders gunst of <rhyme label="a" type="m" corresp="#s10:l1">druk</rhyme></l>
        <l id="s10:l3" met="4" type="-+">En laat af hangen myn <rhyme label="a" type="m" corresp="#s10:l1">geluk</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>:</l>
        <l id="s10:l5" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f" corresp="#s10:l4">Kind</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Veel Minnaars zoeken geld en <rhyme label="a" type="m">goed</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Veel Minnaars zoeken geld en <rhyme label="a" type="m" corresp="#s11:l1">goed</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Maar niemand my myn lust <rhyme label="a" type="m" corresp="#s11:l1">voldoet</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Dan gy, myn zoete lieve <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Dan gy, myn zoete lieve <rhyme label="b" type="f" corresp="#s11:l4">kind</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">ô Ja, myn kuische <rhyme label="a" type="m">Ziels-Vriendin</rhyme></l>
        <l id="s12:l2" met="4" type="-+">ô Ja, myn kuische <rhyme label="a" type="m" corresp="#s12:l1">Ziels-Vriendin</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Ik tragt alleen na <rhyme label="a" type="m" corresp="#s12:l1">weder-min</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f">meysje</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f" corresp="#s12:l4">kind</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Sint dat gy waard myn eenig <rhyme label="a" type="m">but</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Sint dat gy waard myn eenig <rhyme label="a" type="m" corresp="#s13:l1">but</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Kon ik niet rusten een <rhyme label="a" type="m" corresp="#s13:l1">Minut</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f">meysje</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Van u, myn zoete lieve <rhyme label="b" type="f" corresp="#s13:l4">kind</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Tot ik myn dagen eindigen <rhyme label="a" type="m">zal</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Tot ik myn dagen eindigen <rhyme label="a" type="m" corresp="#s14:l1">zal</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Sal niemand zyn myn lief myn <rhyme label="a" type="m" corresp="#s14:l1">Al</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Dan gy, myn zoete lieve <rhyme label="b" type="f">meysje</rhyme>,</l>
        <l id="s14:l5" met="4" type="-+">Dan gy, myn zoete lieve <rhyme label="b" type="f" corresp="#s14:l4">kind</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Wanneer de dood my heefft <rhyme label="a" type="m">doorgrieft</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Wanneer de dood my heefft <rhyme label="a" type="m" corresp="#s15:l1">doorgrieft</rhyme></l>
        <l id="s15:l3" met="4" type="-+">En dat door al te groote <rhyme label="a" type="m" corresp="#s15:l1">liefd'</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Tot u myn zoete lieve <rhyme label="b" type="f">meysje</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Tot u myn zoete lieve <rhyme label="b" type="f" corresp="#s15:l4">kind</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">'t Sal zeker op myn Tombe <rhyme label="a" type="m">staan</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">'t Sal zeker op myn Tombe <rhyme label="a" type="m" corresp="#s16:l1">staan</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Thirsis is door de min <rhyme label="a" type="m" corresp="#s16:l1">vergaan</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Helaas! om Iris 't lieve <rhyme label="b" type="f">meysje</rhyme>,</l>
        <l id="s16:l5" met="4" type="-+">Helaas! om Iris 't zoete <rhyme label="b" type="f" corresp="#s16:l4">kind</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
