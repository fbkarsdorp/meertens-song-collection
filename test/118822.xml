  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'k WAar laast op de kittel <rhyme label="a" type="m">jagt</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">By de <rhyme label="a" type="m" corresp="#s1:l1">nagt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Met een Meisje jé zo <rhyme label="a" type="m" corresp="#s1:l1">zagt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Dat my liflyk <rhyme label="b" type="f">streelden</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Dus ik my <rhyme label="b" type="f" corresp="#s1:l4">verbeelden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zy waar eene <rhyme label="c" type="m">Engelin</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Alles waar my na myn <rhyme label="c" type="m" corresp="#s1:l6">zin</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">Maar <rhyme label="d" type="f">laasje</rhyme>!</l>
        <l id="s1:l9" met="1" type="-+"><rhyme label="d" type="f" corresp="#s1:l8">Temtaasje</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ik ging met haar in een <rhyme label="a" type="m">huis</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Niet te <rhyme label="a" type="m" corresp="#s2:l1">pluis</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daar zy my gelyk een <rhyme label="a" type="m" corresp="#s2:l1">muis</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">In haar val deed <rhyme label="b" type="f">vangen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Maar wat vreemde <rhyme label="b" type="f" corresp="#s2:l4">gangen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ik verloor van dit en <rhyme label="c" type="m">dat</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ja 't geen ik in myn beursje <rhyme label="c" type="m" corresp="#s2:l6">had</rhyme>,</l>
        <l id="s2:l8" met="1" type="-+">Sta schrap <rhyme label="d" type="f">maar</rhyme>,</l>
        <l id="s2:l9" met="1" type="-+">Was krap <rhyme label="d" type="f" corresp="#s2:l8">daar</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">s' Morgens vroeg, ô! groote <rhyme label="a" type="m">spyt</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Droeve <rhyme label="a" type="m" corresp="#s3:l1">tyd</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Waar ik al myn platen <rhyme label="a" type="m" corresp="#s3:l1">kwyt</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">'t Meisje uit myn <rhyme label="b" type="f">ogen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Ik waar mooi <rhyme label="b" type="f" corresp="#s3:l4">bedrogen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Met het krieke van den <rhyme label="c" type="m">dag</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Riep de Waard om zyn <rhyme label="c" type="m" corresp="#s3:l6">gelag</rhyme>,</l>
        <l id="s3:l8" met="1" type="-+">Geen <rhyme label="d" type="f">praate</rhyme>,</l>
        <l id="s3:l9" met="1" type="-+">Kon <rhyme label="d" type="f" corresp="#s3:l8">baate</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Liet myn rok en hoed te <rhyme label="a" type="m">pand</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">ô! Wat <rhyme label="a" type="m" corresp="#s4:l1">schand</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Nu moet ik na 't <rhyme label="a" type="m" corresp="#s4:l1">Apen-Land</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Voor myn dwaze <rhyme label="b" type="f">nukken</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">En myn <rhyme label="b" type="f" corresp="#s4:l4">boevestukken</rhyme>:</l>
        <l id="s4:l6" met="4" type="+-">Jonkers zyt dog nooit zo <rhyme label="c" type="m">zot</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Of gy werd als ik <rhyme label="c" type="m" corresp="#s4:l6">bedot</rhyme>,</l>
        <l id="s4:l8" met="1" type="-+">Bewaard <rhyme label="d" type="f">geld</rhyme>,</l>
        <l id="s4:l9" met="1" type="-+">En spaard <rhyme label="d" type="f" corresp="#s4:l8">geld</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
