    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IN 't diepste van de <rhyme label="a" type="m">middernagt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wanneer 't gestarnte <rhyme label="b" type="m">blinkt</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">En 't ligt der Zonne <rhyme label="b" type="m" corresp="#s1:l2">zinkt</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">En al 't menschelyk <rhyme label="a" type="m" corresp="#s1:l1">Geslagt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">In 't zwanedons <rhyme label="c" type="f">verborgen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Violen slegs laet <rhyme label="c" type="f" corresp="#s1:l5">zorgen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Kwam Venus Zoon, dat wufte <rhyme label="a" type="m">kind</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Dat kind dat al wat <rhyme label="b" type="m">leeft</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En al wat adem <rhyme label="b" type="m" corresp="#s2:l2">heeft</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">In brand zee en geheel <rhyme label="a" type="m" corresp="#s2:l1">verwind</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat kind dat met zyn <rhyme label="c" type="f">schigten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Doet alles voor hem <rhyme label="c" type="f" corresp="#s2:l5">Zwigten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat Fieltje klopte lustig <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En riep als met <rhyme label="b" type="m">getreur</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Ey opend uwe <rhyme label="b" type="m" corresp="#s3:l2">Deur</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ik ben een kind, Ey zie my <rhyme label="a" type="m" corresp="#s3:l1">Staen</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Ik druip schier van den <rhyme label="c" type="f">regen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En ben geheel <rhyme label="c" type="f" corresp="#s3:l5">verlegen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het deerde my van 't kleine <rhyme label="a" type="m">Wicht</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik opende myn <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Daer zag ik door een <rhyme label="b" type="m" corresp="#s4:l2">scheur</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Het Boefje staen met boog en <rhyme label="a" type="m" corresp="#s4:l1">schigt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En vleug'len aen de <rhyme label="c" type="f">armen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wie zou zig niet <rhyme label="c" type="f" corresp="#s4:l5">ontfarmen</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy droop, en trilde van het <rhyme label="a" type="m">nat</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Ik zette met 'er <rhyme label="b" type="m">vaert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Het halsje by den <rhyme label="b" type="m" corresp="#s5:l2">haerdt</rhyme>;</l>
        <l id="s5:l4" met="4" type="-+">Daar 't neffens my wel warmpjes <rhyme label="a" type="m" corresp="#s5:l1">zat</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Ik droogden hem zyn <rhyme label="c" type="f">hairen</rhyme>.</l>
        <l id="s5:l6" met="3" type="-+">Maer hoord myn <rhyme label="c" type="f" corresp="#s5:l5">wedervaren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als 't Guitje was gewarmt en <rhyme label="a" type="m">droog</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Kom zeid het za wel <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Laet my hier eens <rhyme label="b" type="m" corresp="#s6:l2">begaen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En zien of myn gevreesden <rhyme label="a" type="m" corresp="#s6:l1">Boog</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Door zulk een zwaren <rhyme label="c" type="f">regen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Niet heeft een krak <rhyme label="c" type="f" corresp="#s6:l5">gekregen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy spantse in een <rhyme label="a" type="m">ogenblik</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En zet voor aen op 't <rhyme label="b" type="m">spits</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Een scharp gesleepen <rhyme label="b" type="m" corresp="#s7:l2">flits</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En zonder eenig lang <rhyme label="a" type="m" corresp="#s7:l1">gemik</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Treft hy myn <rhyme label="c" type="f">ingewanden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En doet myn zinnen <rhyme label="c" type="f" corresp="#s7:l5">branden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Toen springt hy op, als heel goet <rhyme label="a" type="m">schiks</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Nu ziet gy hier voor 't <rhyme label="b" type="m">oog</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">(Zegt hy) dat myne <rhyme label="b" type="m" corresp="#s8:l2">boog</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Nog ongeschonden is en <rhyme label="a" type="m" corresp="#s8:l1">fiks</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Weest bly van mynent <rhyme label="c" type="f">wegen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Voor zulk een groten <rhyme label="c" type="f" corresp="#s8:l5">zegen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer armen Sul gy zult hier <rhyme label="a" type="m">na</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In uw getroffen <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Haest voelen welk een <rhyme label="b" type="m" corresp="#s9:l2">smart</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Ik u aenzetten kon zoo <rhyme label="a" type="m" corresp="#s9:l1">dra</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Gy brand nu reeds van <rhyme label="c" type="f">binnen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Ga spot nu vry met 't <rhyme label="c" type="f" corresp="#s9:l5">Minnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
