        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Questa dolce sirena</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ALs een niet wel <rhyme label="a" type="m">bedacht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Heb ick dikmaals <rhyme label="a" type="m" corresp="#s1:l1">veracht</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Cupido,, Los en <rhyme label="b" type="m">snoo</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Daar sijn <rhyme label="c" type="f">moeder</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hem schimpigh om <rhyme label="a" type="m" corresp="#s1:l1">belaght</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy grimmig om het <rhyme label="a" type="m">hoon</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wierp voor sijn <rhyme label="a" type="m" corresp="#s2:l1">Elpethroon</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Pyl en boogh,, En hy <rhyme label="b" type="m">vloogh</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Veel <rhyme label="c" type="f">vermoeder</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Als een Dedales <rhyme label="a" type="m" corresp="#s2:l1">soon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En koos, voor boogh en <rhyme label="a" type="m">schigt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Mijn Engel uw <rhyme label="a" type="m" corresp="#s3:l1">gesicht</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Voor dien glans,, 's <rhyme label="b" type="m">Hemelstrans</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">In moet <rhyme label="c" type="f">halen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Het helder <rhyme label="a" type="m" corresp="#s3:l1">sonnelicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Toen tart hy my in 't <rhyme label="a" type="m">velt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ick troost my sijn <rhyme label="a" type="m" corresp="#s4:l1">gewelt</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Maar hy boot,, met een <rhyme label="b" type="m">schoot</rhyme></l>
        <l id="s4:l4" met="2" type="+-">my uw <rhyme label="c" type="f">stralen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Waar door ick ben <rhyme label="a" type="m" corresp="#s4:l1">gevelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hy treft mijn vrye <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Calista, ach ick <rhyme label="a" type="m" corresp="#s5:l1">wert</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Gans bevreest,, 'k Stond <rhyme label="b" type="m">bedeest</rhyme></l>
        <l id="s5:l4" met="2" type="+-">En <rhyme label="c" type="f">bedrogen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">maar hy lacht om mijn <rhyme label="a" type="m" corresp="#s5:l1">smert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy sagh my toornig <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En riep soo moet het <rhyme label="a" type="m" corresp="#s6:l1">gaan</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Die bespot,, Sulcken <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Uyt mijn <rhyme label="c" type="f">oogen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Was hy van stonden <rhyme label="a" type="m" corresp="#s6:l1">aan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nog naulijx dekt de <rhyme label="a" type="m">lucht</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dien boef, of een <rhyme label="a" type="m" corresp="#s7:l1">gerucht</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Quam van hem,, Als een <rhyme label="b" type="m">stem</rhyme></l>
        <l id="s7:l4" met="2" type="+-"><rhyme label="c" type="f">Nederdalen</rhyme>;</l>
        <l id="s7:l5" met="3" type="-+">En riep gy droeve <rhyme label="a" type="m" corresp="#s7:l1">sucht</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Om uw gedwongen <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Maar sulk een <rhyme label="a" type="m" corresp="#s8:l1">Engelin</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Sou een borst,, Schoon <rhyme label="b" type="m">getorst</rhyme></l>
        <l id="s8:l4" met="2" type="+-">Met <rhyme label="c" type="f">metalen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Bewegen totter <rhyme label="a" type="m" corresp="#s8:l1">min</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Haar tongen soet van <rhyme label="a" type="m">taal</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Haar lipjes als <rhyme label="a" type="m" corresp="#s9:l1">coraal</rhyme>;</l>
        <l id="s9:l3" met="4" type="+-">Haar gesicht,, Dat <rhyme label="b" type="m">verlicht</rhyme></l>
        <l id="s9:l4" met="2" type="+-">Als twee <rhyme label="c" type="f">zonnen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Ons goddelijke <rhyme label="a" type="m" corresp="#s9:l1">zaal</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ia selfs Iuppijn om <rhyme label="a" type="m">hoogh</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">Wiert door een lonkend <rhyme label="a" type="m" corresp="#s10:l1">oog</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Van Calist,, Eer hy't <rhyme label="b" type="m">wist</rhyme>,</l>
        <l id="s10:l4" met="2" type="+-"><rhyme label="c" type="f">Overwonnen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Veel eer als door mijn <rhyme label="a" type="m" corresp="#s10:l1">boog</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Gy schoone die de <rhyme label="a" type="m">Goôn</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Kund' quetsen op den <rhyme label="a" type="m" corresp="#s11:l1">throon</rhyme>;</l>
        <l id="s11:l3" met="4" type="+-">'k Bidt u neemt Dan niet <rhyme label="b" type="m">vreemt</rhyme></l>
        <l id="s11:l4" met="2" type="+-">Dat <rhyme label="c" type="f">Lisander</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Om uw lijd duysent <rhyme label="a" type="m" corresp="#s11:l1">doôn</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Nochtans sal hy <rhyme label="a" type="m">Godin</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Verkeeren noyt van <rhyme label="a" type="m" corresp="#s12:l1">sin</rhyme>;</l>
        <l id="s12:l3" met="4" type="+-">Schoon 't geviel,, Dat sijn <rhyme label="b" type="m">ziel</rhyme></l>
        <l id="s12:l4" met="2" type="+-">Op een <rhyme label="c" type="f">ander</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Kon krijgen <rhyme label="a" type="m" corresp="#s12:l1">wedermin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
