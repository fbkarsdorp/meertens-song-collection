    <melody>Psalm 075 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O Wy dancken dy O <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Onser God, wy dancken <rhyme label="b" type="m">dy</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Want dijns hoogen naemes <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Is gecomen ons na <rhyme label="b" type="m" corresp="#s1:l2">by</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Alom spreecktmen vroeg end <rhyme label="c" type="m">laet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Van dijn menig <rhyme label="c" type="m" corresp="#s1:l5">wonderdaet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ick sal ten bestemden <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Billick richten ende <rhyme label="b" type="m">recht</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Al is d'eirtrijck breet ende <rhyme label="a" type="m" corresp="#s2:l1">wijt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van een ander heel <rhyme label="b" type="m" corresp="#s2:l2">onthecht</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">End d'inwoinderen in <rhyme label="c" type="m">last</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ick maeck haere stijlen <rhyme label="c" type="m" corresp="#s2:l5">vast</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ick heb tdwaes volck dus <rhyme label="a" type="m">vermaent</rhyme>:</l>
        <l id="s3:l2" met="4" type="+-">Volgt niet uwen sotten <rhyme label="b" type="m">kop</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ghy godloosen hoop <rhyme label="a" type="m" corresp="#s3:l1">verwaent</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Steeckt v hoirnen niet hoog <rhyme label="b" type="m" corresp="#s3:l2">op</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Steecktse niet op, wat ghy <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Noch spreeckt niet in <rhyme label="c" type="m" corresp="#s3:l5">overmoet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Want dat yemant klimt om <rhyme label="a" type="m">hoog</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dat en komt van Oost noch <rhyme label="b" type="m">West</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Noch van tNoirden cout end <rhyme label="a" type="m" corresp="#s4:l1">droog</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Maer God, die richt <rhyme label="b" type="m" corresp="#s4:l2">alderbest</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">End die alles wel <rhyme label="c" type="m">regeert</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Is die hoogt end die <rhyme label="c" type="m" corresp="#s4:l5">verneert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">God de heer hout in sijn <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Eenen kroes vol droeven <rhyme label="b" type="m">wijn</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daer hy tvolck van allen <rhyme label="a" type="m" corresp="#s5:l1">standt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">In laet schencken smert end <rhyme label="b" type="m" corresp="#s5:l2">pijn</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Doch het grondtsop voir haer <rhyme label="c" type="m">deel</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Suypt het godloos volck <rhyme label="c" type="m" corresp="#s5:l5">geheel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dies sal ick in mijn <rhyme label="a" type="m">gesanck</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Iacobs God, end al sijn <rhyme label="b" type="m">werck</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Prijsen met lof ende <rhyme label="a" type="m" corresp="#s6:l1">danck</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">End der boosen hoirnen <rhyme label="b" type="m" corresp="#s6:l2">sterck</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Breken: maer der vromen <rhyme label="c" type="m">eer</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Sal toenemen lancx so <rhyme label="c" type="m" corresp="#s6:l5">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
