  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EEn zoet lief en aerdig <rhyme label="a" type="f">Meisje</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Op den Amstel in haer <rhyme label="b" type="m">schuit</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">die daer Riep zo meenig <rhyme label="a" type="f" corresp="#s1:l1">Reissie</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En dat met een zoet <rhyme label="b" type="m" corresp="#s1:l2">geluit</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Over, over, over <rhyme label="c" type="m">hee</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wie wil met myn Schuitje <rhyme label="c" type="m" corresp="#s1:l5">meê</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ik liet myn dikmaels <rhyme label="a" type="f">oversette</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Van den een na de andere <rhyme label="b" type="m">kant</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Lieve Vrind wild hier op <rhyme label="a" type="f" corresp="#s2:l1">lette</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hoe wy gekomen zyn aen het <rhyme label="b" type="m" corresp="#s2:l2">land</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Want als ik wil over <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Neem ik geen ander schuitje <rhyme label="c" type="m" corresp="#s2:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Met zo trapten ik in dat <rhyme label="a" type="f">schuitje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Op de bank al nevens <rhyme label="b" type="m">haer</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Het ontleede van haer <rhyme label="a" type="f" corresp="#s3:l1">kleeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Terwyl het nog Warm <rhyme label="b" type="m" corresp="#s3:l2">waer</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Over, over waeren <rhyme label="c" type="m">Wy</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Zo dra zy haer Riempjes <rhyme label="c" type="m" corresp="#s3:l5">Ly</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">met zo trapten ik op het <rhyme label="a" type="f">rantje</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Van het schuitje op de <rhyme label="b" type="m">plegt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ik gaf 'er een kopstuk in haer <rhyme label="a" type="f" corresp="#s4:l1">hantje</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">dat zy in haer schootje <rhyme label="b" type="m" corresp="#s4:l2">legt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Wel is dat voor de vragt myn <rhyme label="c" type="m">heer</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Riep zy vaer dan nog een <rhyme label="c" type="m" corresp="#s4:l5">keer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Nog een reisje met u te gaen <rhyme label="a" type="f">vaere</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Is een zaek die ik wel <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Lieve vriend wil geen tyd <rhyme label="a" type="f" corresp="#s5:l1">spaere</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Terwyl gy reeds in jou schuitje <rhyme label="b" type="m" corresp="#s5:l2">bind</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Zet my over, heen en <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">dat is goed sprak zy myn <rhyme label="c" type="m" corresp="#s5:l5">heer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Og hoe zoet is dan het <rhyme label="a" type="f">roeije</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">In het schuitje met <rhyme label="b" type="m">Gelatée</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Lieve Vriend wild u wat <rhyme label="a" type="f" corresp="#s6:l1">spoeje</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Want ik roei ook zelfs <rhyme label="b" type="m" corresp="#s6:l2">meê</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Roei maer meisie, roei maer <rhyme label="c" type="m">rat</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">dat gy komt al in de <rhyme label="c" type="m" corresp="#s6:l5">stad</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
