  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">UNe nuit dans la <rhyme label="a" type="f">prairië</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Colinet m'y <rhyme label="b" type="m">rencontra</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Bonsoir me dit il, <rhyme label="a" type="f" corresp="#s1:l1">m'amië</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">J'y Repondis qui vat <rhyme label="b" type="m" corresp="#s1:l2">la</rhyme></l>
        <l id="s1:l5" met="4" type="+-">C'est dit il un berger <rhyme label="c" type="f">tendre</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Qui voudroit <rhyme label="d" type="m">t'entretenir</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Je consensis à <rhyme label="c" type="f" corresp="#s1:l5">l'entendre</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Et J'y pris bien du <rhyme label="d" type="m" corresp="#s1:l6">plaisir</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">D'un ton Naif &amp; <rhyme label="a" type="f">sincere</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Il m'explicá son <rhyme label="b" type="m">ardeur</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">L'esprit n'est pas <rhyme label="a" type="f" corresp="#s2:l1">necessaire</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Pour faire jaser le <rhyme label="b" type="m" corresp="#s2:l2">coeur</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Un baiser fut <rhyme label="c" type="f">l'intreprete</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">De son amoureux <rhyme label="d" type="m">desir</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">J'en devint presque <rhyme label="c" type="f" corresp="#s2:l5">muëtte</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Et j'y pris bien du <rhyme label="d" type="m" corresp="#s2:l6">plaisir</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Pour m'engager <rhyme label="a" type="f">d'avantage</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ce berger un autre <rhyme label="b" type="m">fois</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Fit retantir le <rhyme label="a" type="f" corresp="#s3:l1">bocage</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Des doux accens de sa <rhyme label="b" type="m" corresp="#s3:l2">voix</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Tout de suite sur <rhyme label="c" type="f">l'herbette</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Enfin de me <rhyme label="d" type="m">rejouir</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Il fit Enfler sa <rhyme label="c" type="f" corresp="#s3:l5">musette</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Et j'y pris bien du <rhyme label="d" type="m" corresp="#s3:l6">plaisir</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">J'avois reçu de ma <rhyme label="a" type="f">mere</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Un Certain anneau de <rhyme label="b" type="m">prix</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Il avoit grace à son <rhyme label="a" type="f" corresp="#s4:l1">pere</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Un bijou des plus <rhyme label="b" type="m" corresp="#s4:l2">Jolis</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Trocquons les tous deux mon <rhyme label="c" type="f">ange</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dit il avec nn <rhyme label="d" type="m">soupir</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Je consentis à <rhyme label="c" type="f" corresp="#s4:l5">l'Echange</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Et j'y pris bien du <rhyme label="d" type="m" corresp="#s4:l6">plaisir</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">De mon anneau le <rhyme label="a" type="f">passage</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Luy parut d'abord <rhyme label="b" type="m">Etroit</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Mais en peu de temps <rhyme label="a" type="f" corresp="#s5:l1">l'usage</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Luy rendit propre a son <rhyme label="b" type="m" corresp="#s5:l2">doigt</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Je voulus bien, luy <rhyme label="c" type="f">permettre</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Quelquefois de s'en <rhyme label="d" type="m">servir</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Il s'amusat de le <rhyme label="c" type="f" corresp="#s5:l5">mettre</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Et j'y pris bien du <rhyme label="d" type="m" corresp="#s5:l6">plaisir</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
