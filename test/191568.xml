  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ach stookert van myn <rhyme label="a" type="m">vlam</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die door dit schoon <rhyme label="b" type="m">gezicht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Weleer zyn oorsprong <rhyme label="a" type="m" corresp="#s1:l1">nam</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En heeft dit hart <rhyme label="b" type="m" corresp="#s1:l2">verpligt</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Geeft u een andere <rhyme label="c" type="m">naam</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Tot wedermin <rhyme label="c" type="m" corresp="#s1:l5">bekwaam</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Of eindigd myn <rhyme label="d" type="m">verdriet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Daar gy my nu in <rhyme label="d" type="m" corresp="#s1:l7">ziet</rhyme>.</l>
      </lg>
    </lg>
