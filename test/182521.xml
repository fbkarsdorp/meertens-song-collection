<song>
  <meta>
    <id>182521</id>
    <date>
      <year-start>1793</year-start>
      <year-end>1793</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Daar was laatst een meisje loos</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Met dees zoeten kermis <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Mietje doet open Mietje doet <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Met dees zoeten Kermis <rhyme label="b" type="f" corresp="#s1:l2">tyd</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Mietje doet open ik ben <rhyme label="a" type="m" corresp="#s1:l1">bereid</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Om te geven myn hand op <rhyme label="c" type="m">trouw</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En zo met lusten, in vreugd te <rhyme label="d" type="f">rusten</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Door de liefd' als man en <rhyme label="c" type="m" corresp="#s1:l5">vrouw</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">In u Hof zonder druk of <rhyme label="c" type="m" corresp="#s1:l5">rouw</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s1:l1">tyd</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Mietje doet open, Mietje doet <rhyme label="b" type="f" corresp="#s1:l2">open</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s1:l1">tyd</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Mietje doet open,'k zal zyn <rhyme label="a" type="m" corresp="#s1:l1">verblyd</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wie hoor ik hier voor myn <rhyme label="a" type="m">deur</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Die heeft geroepen Mietje doet <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">My dunkt het is een <rhyme label="b" type="f" corresp="#s2:l2">Serbiteur</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Met bloempjes zoet van <rhyme label="a" type="m" corresp="#s2:l1">geur</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En dan die met deese <rhyme label="c" type="m">tyd</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">In myn Hof planten, en 't alle <rhyme label="d" type="f">kanten</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Door de liefd' en <rhyme label="c" type="m" corresp="#s2:l5">minsaemheid</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Besproeijen, ja, Mietje met <rhyme label="c" type="m" corresp="#s2:l5">vlyd</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s2:l1">tyd</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Mietje doet open, Mietje doet <rhyme label="b" type="f" corresp="#s2:l2">open</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s2:l1">tyd</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Mietje doet open,'k zal zyn <rhyme label="a" type="m" corresp="#s2:l1">verblyd</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Mietje doet open laat m' in <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ik ben een Kerel net als een <rhyme label="b" type="f">perel</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ook van geld zeer zwaer <rhyme label="b" type="f" corresp="#s3:l2">gelaen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Mietje doet open en ziet my <rhyme label="a" type="m" corresp="#s3:l1">aen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ik ben eenen <rhyme label="c" type="m">Hovenier</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Die wel kan zaijen, en ool wel <rhyme label="d" type="f">maeijen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Mietje voldoet my dat <rhyme label="c" type="m" corresp="#s3:l5">plysier</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Want uwen liefsten die is <rhyme label="c" type="m" corresp="#s3:l5">hier</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s3:l1">tyd</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Mietje doet open, Mietje doet <rhyme label="b" type="f" corresp="#s3:l2">open</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s3:l1">tyd</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Mietje doet open,'k zal zyn <rhyme label="a" type="m" corresp="#s3:l1">verblyd</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Lief ik hoor uwe <rhyme label="a" type="m">spraek</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Wie heeft u geroepen Mietje doet <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Lief ik hoor uwen <rhyme label="b" type="f" corresp="#s4:l2">spraek</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'t Baert my een zoet <rhyme label="a" type="m" corresp="#s4:l1">vermaak</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Kom nu dan ook met liefste <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Maer binnen treden, ik ben te <rhyme label="d" type="f">vreden</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Mietje gy verheugd heel myn <rhyme label="c" type="m" corresp="#s4:l5">gemoed</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Ziet wat de liefde <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s4:l1">tyd</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Mietje doet open, Mietje doet <rhyme label="b" type="f" corresp="#s4:l2">open</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s4:l1">tyd</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Mietje doet open,'k zal zyn <rhyme label="a" type="m" corresp="#s4:l1">verblyd</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Mietje hoe zoet is 't in u <rhyme label="a" type="m">Hof</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">'k Zal die planten, van alle <rhyme label="b" type="f">kanten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En die vercieren tot u <rhyme label="b" type="f" corresp="#s5:l2">lof</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Met bloemekens van edel <rhyme label="a" type="m" corresp="#s5:l1">stof</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Mietje myn lief myn <rhyme label="c" type="m">Engelin</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Laet ons nu danssen en vlegten <rhyme label="d" type="f">kransen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">En uwen Hof is naer myn <rhyme label="c" type="m" corresp="#s5:l5">zin</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Laet me altyd blyven daer <rhyme label="c" type="m" corresp="#s5:l5">in</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s5:l1">tyd</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Mietje doet open, Mietje doet <rhyme label="b" type="f" corresp="#s5:l2">open</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s5:l1">tyd</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">Mietje doet open,'k zal zyn <rhyme label="a" type="m" corresp="#s5:l1">verblyd</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ach myn liefsten <rhyme label="a" type="m">Hovenier</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Zo g'had geroepen, myn hof was <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Trekt daer in, neemt u <rhyme label="b" type="f" corresp="#s6:l2">plaisier</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Mietje wat vreugden vind ik <rhyme label="a" type="m" corresp="#s6:l1">hier</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">'k Zal naer reden en <rhyme label="c" type="m">behoor</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Daer bloempjes plukken zonder <rhyme label="d" type="f">verdrukken</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">My zyn zamen nu <rhyme label="c" type="m" corresp="#s6:l5">accoor</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Mietje ik doen wel myn <rhyme label="c" type="m" corresp="#s6:l5">devoor</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Met dees zoeten Kermis <rhyme label="a" type="m" corresp="#s6:l1">tyd</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Mietje doet open, Mietje doet <rhyme label="b" type="f" corresp="#s6:l2">open</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Met dees zoeten <rhyme label="a" type="m" corresp="#s6:l1">Kermis-tyd</rhyme>,</l>
        <l id="s6:l12" met="4" type="+-">Mietje doet open ik ben <rhyme label="a" type="m" corresp="#s6:l1">verblyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
