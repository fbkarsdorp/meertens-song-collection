    <tags/>
    <melody>Windje daar het bos af drilt</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SPytige Claertje, dartele <rhyme label="a" type="m">Pry</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Kom ereys aen myn groene <rhyme label="a" type="m" corresp="#s1:l1">zy</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">‘k Zal jou streelen Met jou <rhyme label="b" type="f">speelen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En so gy gedoogd myn <rhyme label="c" type="m">Hand</rhyme>,</l>
        <l id="s1:l5" met="8" type="+-">Steken u, steken u, steken u, steken <rhyme label="d" type="m">u</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Steken uw levendig in den <rhyme label="c" type="m" corresp="#s1:l4">Brand</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">‘k Zal jou wyzen zoete <rhyme label="a" type="m">Meyd</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Waer den Haen syn Eyeren <rhyme label="a" type="m" corresp="#s2:l1">leyd</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">‘k Zal jou knoeyen Met jou <rhyme label="b" type="f">stoeyen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En jou voeren lekk’re <rhyme label="c" type="m">Pap</rhyme>,</l>
        <l id="s2:l5" met="8" type="+-">In je mond, in je mond, in je mond, in je <rhyme label="d" type="m">mond</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">‘k Wed je speelen sult hap, <rhyme label="c" type="m" corresp="#s2:l4">hap</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Meysje het is so smeerig <rhyme label="a" type="m">Vet</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">‘k Wedjer jou aen te barsten <rhyme label="a" type="m" corresp="#s3:l1">Vret</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">‘t Zal jou geven, Vrolik <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En een Krieuweling in je <rhyme label="c" type="m">Buyk</rhyme>,</l>
        <l id="s3:l5" met="8" type="+-">Ik wedje, ik wedje, ik wedje, ik <rhyme label="d" type="m">wedje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ik wedje se snoeyen sou sult ter <rhyme label="c" type="m" corresp="#s3:l4">sluyk</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">En de Lepel zoete <rhyme label="a" type="m">hond</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Daer ik mee voer past inje <rhyme label="a" type="m" corresp="#s4:l1">mond</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">‘k Bid jou Meysje Gaep een <rhyme label="b" type="f">reysje</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En probeer ‘t, ‘t is sacht als <rhyme label="c" type="m">Was</rhyme>,</l>
        <l id="s4:l5" met="8" type="+-">Niet te groot, niet te kleyn, niet te dik, niet te <rhyme label="d" type="m">dun</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Niet te lang, maer bekwaem van <rhyme label="c" type="m" corresp="#s4:l4">pas</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Meysje soje myn <rhyme label="a" type="m">geloofd</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Iou Oogjes draeyen in jou <rhyme label="a" type="m" corresp="#s5:l1">Hoofd</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Iy sult hikken, suchten, <rhyme label="b" type="f">slikken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En van wellust buyten <rhyme label="c" type="m">spraek</rhyme>,</l>
        <l id="s5:l5" met="8" type="+-">Noyt selje, noyt selje, noyt selje, noyt <rhyme label="d" type="m">selje</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Noyt selje eens sat zyn van de <rhyme label="c" type="m" corresp="#s5:l4">smaek</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
