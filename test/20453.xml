<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20453</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Het meisje van zeventien jaar</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy treckt my, Heer, ick koom tot <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Treckt stijf, ick wil hard' <rhyme label="b" type="f">loopen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ey ! gheef my moed: Soo sal ick <rhyme label="a" type="m" corresp="#s1:l1">bly</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Volkomen op u <rhyme label="b" type="f" corresp="#s1:l2">hopen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick ben op 't heyligh-padt <rhyme label="a" type="m">ghegaen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Door uwen Geest <rhyme label="b" type="f">ghedreven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">O ! Heer, wilt altijdt dien <rhyme label="a" type="m" corresp="#s2:l1">voortaen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">My tot een Leydts-man <rhyme label="b" type="f" corresp="#s2:l2">gheven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Poort is engh, de Wech is <rhyme label="a" type="m">smal</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vol Door'nen zijn de <rhyme label="b" type="f">weghen</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Daer door ick wel passeren <rhyme label="a" type="m" corresp="#s3:l1">sal</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Vertrou ick, door u <rhyme label="b" type="f" corresp="#s3:l2">Zeghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Och ! selven, Heer, vermach ick <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghy moet mijn swackheyt <rhyme label="b" type="f">stijven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">(Want ick ben swacker als een <rhyme label="a" type="m" corresp="#s4:l1">Riedt</rhyme>)</l>
        <l id="s4:l4" met="3" type="-+">Soo sal ick staende <rhyme label="b" type="f" corresp="#s4:l2">blijven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy hebt u hulp my <rhyme label="a" type="m">toe-geseyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">In u Woordt <rhyme label="b" type="f">voor-geschreven</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">U handt heeft my hier <rhyme label="a" type="m" corresp="#s5:l1">op-geleydt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Och ! wilt my noyt <rhyme label="b" type="f" corresp="#s5:l2">begheven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">GHy zijt mijn rechterhandt, mijn <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die my kan <rhyme label="b" type="f">onderstutten</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Och ! slaet steeds op u Dienaer <rhyme label="a" type="m" corresp="#s6:l1">acht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En wilt sijn val <rhyme label="b" type="f" corresp="#s6:l2">af-schutten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">U reyne Wet die is mijn <rhyme label="a" type="m">licht</rhyme>:</l>
        <l id="s7:l2" met="3" type="-+">Wilt in mijn hert die <rhyme label="b" type="f">schrijven</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Op dat die my steeds <rhyme label="a" type="m" corresp="#s7:l1">onderricht</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Om op u wech te <rhyme label="b" type="f" corresp="#s7:l2">blijven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De hope van het soetste <rhyme label="a" type="m">Soet</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die 'k eynd'lijck hoop te <rhyme label="b" type="f">smaecken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Die kan in al mijn <rhyme label="a" type="m" corresp="#s8:l1">teghenspoet</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">My altijdt moedigh <rhyme label="b" type="f" corresp="#s8:l2">maecken</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Soo dat mijn Ziel in 't minst niet <rhyme label="a" type="m">vreest</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Gheen druck, pijn, anghst, of <rhyme label="b" type="f">lijden</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Maer sal door kracht van uwen <rhyme label="a" type="m" corresp="#s9:l1">Geest</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ter doodt ghetrouw'lijck <rhyme label="b" type="f" corresp="#s9:l2">strijden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De liefde die sy tot u <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Doet haer na u <rhyme label="b" type="f">verlanghen</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">U troost die ghy haer stadigh <rhyme label="a" type="m" corresp="#s10:l1">gheeft</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Werdt danckbaerlijck <rhyme label="b" type="f" corresp="#s10:l2">ontfanghen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Die is voor haren mondt soo <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">En heer'lijck in haer <rhyme label="b" type="f">ooghen</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">Daer door haer hert, sin, en <rhyme label="a" type="m" corresp="#s11:l1">ghemoet</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">Na meerder komt te <rhyme label="b" type="f" corresp="#s11:l2">poghen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Sint dat sy die eens heeft <rhyme label="a" type="m">ghesmaeckt</rhyme></l>
        <l id="s12:l2" met="3" type="-+">En na waerd' leeren <rhyme label="b" type="f">achten</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Sy wierdt als gaende doen <rhyme label="a" type="m" corresp="#s12:l1">ghemaeckt</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Om staegh na meer te <rhyme label="b" type="f" corresp="#s12:l2">trachten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
