  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ONs Vader God <rhyme label="a" type="f">gepreesen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die in den Hemel <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Geheyligt zoo moet <rhyme label="a" type="f" corresp="#s1:l1">weesen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Uw' Naam <rhyme label="b" type="m" corresp="#s1:l2">gebenedijt</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Geheyligt zoo moet <rhyme label="a" type="f" corresp="#s1:l1">weesen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Uw' Naam <rhyme label="b" type="m" corresp="#s1:l2">gebenedijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Uw Rijk van grooter <rhyme label="a" type="f">waarden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Laat komen metter <rhyme label="b" type="m">spoet</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">In Hemel en op <rhyme label="a" type="f" corresp="#s2:l1">Aarden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Geschied' uw' wille <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">In Hemel en op <rhyme label="a" type="f" corresp="#s2:l1">Aarden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Geschied' uw' wille <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wilt ons, Ã´ Heer! <rhyme label="a" type="f">verleenen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">(Koning van magten <rhyme label="b" type="m">groot</rhyme>)</l>
        <l id="s3:l3" met="3" type="-+">Peys, Vreed' en ook met <rhyme label="a" type="f" corresp="#s3:l1">eenen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Den Kost, tot 's levens <rhyme label="b" type="m" corresp="#s3:l2">noot</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Peys, Vreed' en ook met <rhyme label="a" type="f" corresp="#s3:l1">eenen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Den Kost, tot 's levens <rhyme label="b" type="m" corresp="#s3:l2">noot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vergeeft ons Heer <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al dat wy schuldig <rhyme label="b" type="m">staan</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Gelijk wy ook <rhyme label="a" type="f" corresp="#s4:l1">vergeven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die ons hebben <rhyme label="b" type="m" corresp="#s4:l2">misdaan</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Gelijk wy ook <rhyme label="a" type="f" corresp="#s4:l1">vergeven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die ons hebben <rhyme label="b" type="m" corresp="#s4:l2">misdaan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En laat ons tog niet <rhyme label="a" type="f">vallen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ook in tentatie <rhyme label="b" type="m">quaat</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Maar Heer verlost ons <rhyme label="a" type="f" corresp="#s5:l1">allen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van 's vyants boosen <rhyme label="b" type="m" corresp="#s5:l2">raat</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Maar Heer verlost ons <rhyme label="a" type="f" corresp="#s5:l1">allen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van 's vyants boosen <rhyme label="b" type="m" corresp="#s5:l2">raat</rhyme>.</l>
      </lg>
    </lg>
  </text>
