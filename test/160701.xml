<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160701</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het was een fraai rijk burgerskind</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt minst wort hier vaeck meest <rhyme label="a" type="m">geacht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat sietmen vaeck <rhyme label="b" type="f">gebeuren</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Nae 's Werelts goedt is 't dat men <rhyme label="a" type="m" corresp="#s1:l1">tracht</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Al soud' men 't nae <rhyme label="b" type="f" corresp="#s1:l2">betreuren</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Het Goudt is groot van <rhyme label="c" type="f">krachten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die 't heeft hoortmen hoogh <rhyme label="c" type="f" corresp="#s1:l5">achten</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">'t Is Hoer of Dief, Ghy zijt my <rhyme label="d" type="m">Lief</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Heb ghy de ronde <rhyme label="d" type="m" corresp="#s1:l7">Schijf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">By Medenblick en daer <rhyme label="a" type="m">omtrent</rhyme></l>
        <l id="s2:l2" met="3" type="-+">In seker plaetse <rhyme label="b" type="f">waren</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Een Knecht en Meyt, my wel <rhyme label="a" type="m" corresp="#s2:l1">bekent</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Een middelbaer van <rhyme label="b" type="f" corresp="#s2:l2">Iaren</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">De een had geen <rhyme label="c" type="f">behoeder</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Beyd' Suster nochte <rhyme label="c" type="f" corresp="#s2:l5">Broeder</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Al d' Ouders goedt, Hoe vol en <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Haer eygen worden <rhyme label="d" type="m" corresp="#s2:l7">moet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Vryer, laegh en slecht van <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gaet nae een <rhyme label="b" type="f">Hijlke-maecker</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En seydt, mijn Vrient, ick koom om <rhyme label="a" type="m" corresp="#s3:l1">raet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Weest ghy nu mijn <rhyme label="b" type="f" corresp="#s3:l2">voor-spraecker</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ick soud' wel garen <rhyme label="c" type="f">Trouwen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Maer 't most my niet <rhyme label="c" type="f" corresp="#s3:l5">berouwen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Weet ghy een Meyt, Die schijven <rhyme label="d" type="m">heyt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ick ben daer toe <rhyme label="d" type="m" corresp="#s3:l7">bereyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De man seyt jae, ick weet u <rhyme label="a" type="m">slach</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Maer most haer daer af <rhyme label="b" type="f">spreecken</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Wel haest u soo veel wesen <rhyme label="a" type="m" corresp="#s4:l1">mach</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Aen 't loon sal 't niet <rhyme label="b" type="f" corresp="#s4:l2">gebreecken</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Help ons eens by den <rhyme label="c" type="f">ander</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Op dat wy met <rhyme label="c" type="f" corresp="#s4:l5">malkander</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Van dese saeck 't Saem houden <rhyme label="d" type="m">spraeck</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En voor de Preeck-soel <rhyme label="d" type="m" corresp="#s4:l7">raeck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy heeft in 't kort een plaets <rhyme label="a" type="m">bestelt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Om t'samen daer te <rhyme label="b" type="f">komen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">De Knecht is met sijn Oom <rhyme label="a" type="m" corresp="#s5:l1">verselt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hy heeft haer waer <rhyme label="b" type="f" corresp="#s5:l2">genomen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Een Glaesjen eerst <rhyme label="c" type="f">gedroncken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Om 't hert wat te <rhyme label="c" type="f" corresp="#s5:l5">ontfoncken</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Op dat de saeck, Met goet <rhyme label="d" type="m">vermaeck</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">In 't kort ten eynde <rhyme label="d" type="m" corresp="#s5:l7">raeck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Des Vryers Oom sprack tot haer <rhyme label="a" type="m">Vaer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Wat geefje met de <rhyme label="b" type="f">Vryster</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">Twee hondert ponden, dat is <rhyme label="a" type="m" corresp="#s6:l1">waer</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Niet meer, dat is te <rhyme label="b" type="f" corresp="#s6:l2">byster</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Geef duysent Kroonen <rhyme label="c" type="f">even</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Die sal ick haer niet <rhyme label="c" type="f" corresp="#s6:l5">geven</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Wat krijght u Knecht Meed' in de <rhyme label="d" type="m">Echt</rhyme>?</l>
        <l id="s6:l8" met="3" type="-+">My dat eens open <rhyme label="d" type="m" corresp="#s6:l7">seght</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wat, sprack de Oom, praet ghy van <rhyme label="a" type="m">goet</rhyme>?</l>
        <l id="s7:l2" met="3" type="-+">De Man moet 't alles <rhyme label="b" type="f">winnen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Door hem de Schoor-steen roocken <rhyme label="a" type="m" corresp="#s7:l1">moet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Of 't Wijf en kan niet <rhyme label="b" type="f" corresp="#s7:l2">Spinnen</rhyme>;</l>
        <l id="s7:l5" met="3" type="-+">Hy is gesont van <rhyme label="c" type="f">Leeden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En wel versien met <rhyme label="c" type="f" corresp="#s7:l5">Kleeden</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Dat acht ick best, Voort van de <rhyme label="d" type="m">rest</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Sijn huys staet Oost en <rhyme label="d" type="m" corresp="#s7:l7">West</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Kom seyt de Vader van de <rhyme label="a" type="m">Meyt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">'k Sal sestien honder <rhyme label="b" type="f">geven</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Neen, seyt de Oom, dat 's eens <rhyme label="a" type="m" corresp="#s8:l1">geseyt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Gaaf achtien hondert <rhyme label="b" type="f" corresp="#s8:l2">even</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Twee hondert gulden <rhyme label="c" type="f">meerder</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dan Trouwen sy te <rhyme label="c" type="f" corresp="#s8:l5">eerder</rhyme>:</l>
        <l id="s8:l7" met="4" type="-+">Wat dunckt u Quant, Is 't jou <rhyme label="d" type="m">verstant</rhyme>?</l>
        <l id="s8:l8" met="3" type="-+">Soo slae my in de <rhyme label="d" type="m" corresp="#s8:l7">handt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Geen stuyver biedt ick meerder <rhyme label="a" type="m">uyt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">'t Is langh genoegh <rhyme label="b" type="f">gedongen</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Wat dunckt u Trijn? Niet eenen <rhyme label="a" type="m" corresp="#s9:l1">duyt</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Dit sijn al loose <rhyme label="b" type="f" corresp="#s9:l2">sprongen</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">Laet desen droogert <rhyme label="c" type="f">loopen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En ons sijn Liefd' niet <rhyme label="c" type="f" corresp="#s9:l5">koopen</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Dan valtse duur, En dickmael <rhyme label="d" type="m">stuur</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Ick krijgh licht mijn <rhyme label="d" type="m" corresp="#s9:l7">partuur</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Daer is dit Minne-spel <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En niet een sier <rhyme label="b" type="f">gewonne</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">'t Is sonder Liefde <rhyme label="a" type="m" corresp="#s10:l1">uyt-gegaen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Gelijck het was <rhyme label="b" type="f" corresp="#s10:l2">begonnen</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Soo sullen alle <rhyme label="c" type="f">varen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Die om het goet <rhyme label="c" type="f" corresp="#s10:l5">vergaren</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Want die soo vleyt, 't Sy Knecht of <rhyme label="d" type="m">Meyt</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Veeltijdts de Kluppel <rhyme label="d" type="m" corresp="#s10:l7">scheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
