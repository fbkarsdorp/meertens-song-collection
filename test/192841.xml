  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een Dorpje niet wijd van <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ging ik jagen al voor mijn <rhyme label="a" type="m" corresp="#s1:l1">plaizier</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s1:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s1:l3">tradera</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ging ik jagen al voor mijn <rhyme label="a" type="m" corresp="#s1:l1">plaizier</rhyme>;</l>
        <l id="s1:l6" met="4" type="-+">En op een schoone vroegen <rhyme label="c" type="f">morgen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Ging ik alleen welgemoed op de <rhyme label="d" type="m">jagt</rhyme>.</l>
        <l id="s1:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s1:l3">tradera</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s1:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik jaagde door bosch en <rhyme label="a" type="m">hei</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Op het laatst kwam ik in een <rhyme label="a" type="m" corresp="#s2:l1">vallei</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s2:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s2:l3">tradera</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Op het laatst kwam ik in een <rhyme label="a" type="m" corresp="#s2:l1">vallei</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Met mijn haasje dat ik had <rhyme label="c" type="f">geschoten</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Kwam ik aan een zeer schoone <rhyme label="d" type="m">waterstroom</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s2:l3">tradera</rhyme></l>
        <l id="s2:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s2:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Aan het einde der stroom <rhyme label="a" type="m">gegaan</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zag ik daar een watermolen <rhyme label="a" type="m" corresp="#s3:l1">staan</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s3:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s3:l3">tradera</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zag ik daar een watermolen <rhyme label="a" type="m" corresp="#s3:l1">staan</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Ach hoe verrukt waren mijn <rhyme label="c" type="f">zinnen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Toen ik daar een schoon molennarinnetje <rhyme label="d" type="m">zag</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s3:l3">tradera</rhyme></l>
        <l id="s3:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s3:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ach, meisje! mijn <rhyme label="a" type="m">engelin</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Verrukt is op u mijn <rhyme label="a" type="m" corresp="#s4:l1">zin</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s4:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s4:l3">tradera</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Verrukt is op u mijn <rhyme label="a" type="m" corresp="#s4:l1">zin</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Mag ik bij u in den <rhyme label="c" type="f">molen</rhyme>?</l>
        <l id="s4:l7" met="4" type="-+">Mij vermaken al voor mijn <rhyme label="d" type="m">plaizier</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s4:l3">tradera</rhyme></l>
        <l id="s4:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s4:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ach, Jager! gaat toch <rhyme label="a" type="m">voorbij</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik ben bang voor u <rhyme label="a" type="m" corresp="#s5:l1">leverij</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s5:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s5:l3">tradera</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ik ben bang voor u <rhyme label="a" type="m" corresp="#s5:l1">leverij</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Want mijn moeder heeft mij <rhyme label="c" type="f">geboden</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dat ik mij moet wachten voor <rhyme label="d" type="m">jagerij</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s5:l3">tradera</rhyme></l>
        <l id="s5:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s5:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ach, meisje! gelooft mij <rhyme label="a" type="m">vrij</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Weest niet bang voor mijn <rhyme label="a" type="m" corresp="#s6:l1">leverij</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s6:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s6:l3">tradera</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Weest niet bang voor mijn <rhyme label="a" type="m" corresp="#s6:l1">leverij</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Voelt hieronder mijn hart eens <rhyme label="c" type="f">kloppen</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Gij zoudt zeker gaan zitten naast <rhyme label="d" type="m">mij</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s6:l3">tradera</rhyme></l>
        <l id="s6:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s6:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Geeft mij een kusje op <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mijn haasje geef ik u tot <rhyme label="a" type="m" corresp="#s7:l1">pand</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s7:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s7:l3">tradera</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Mijn haasje geef ik u tot <rhyme label="a" type="m" corresp="#s7:l1">pand</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Geen Jager zal u <rhyme label="c" type="f">verlaten</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Zoo lang hij aan het jagen <rhyme label="d" type="m">zij</rhyme>.</l>
        <l id="s7:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s7:l3">tradera</rhyme></l>
        <l id="s7:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s7:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ach, Jager! u <rhyme label="a" type="m">gekust</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat is mij reeds <rhyme label="a" type="m" corresp="#s8:l1">bewust</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s8:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s8:l3">tradera</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Dat is mij reeds <rhyme label="a" type="m" corresp="#s8:l1">bewust</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Uw hoorngeluid verrukt mijn <rhyme label="c" type="f">zin</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">En dat doet streelen mijne <rhyme label="d" type="m">min</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s8:l3">tradera</rhyme></l>
        <l id="s8:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s8:l3">tradera</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Kom, meisjes! in 't <rhyme label="a" type="m">generaal</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Geloof de Jagers al te <rhyme label="a" type="m" corresp="#s9:l1">maal</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Traderi <rhyme label="b" type="m">tradera</rhyme></l>
        <l id="s9:l4" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s9:l3">tradera</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Geloof de Jagers al te <rhyme label="a" type="m" corresp="#s9:l1">maal</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Als een Jager een haasje kan <rhyme label="c" type="f">jagen</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Dat is de Jager zijn grootse <rhyme label="d" type="m">plaizier</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">Traderi <rhyme label="b" type="m" corresp="#s9:l3">tradera</rhyme></l>
        <l id="s9:l9" met="2" type="-+">traderi <rhyme label="b" type="m" corresp="#s9:l3">tradera</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
