<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100092</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie ik niet onder de bomen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VVEl wat flicker heete <rhyme label="a" type="f">stralen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Komt bestryden mijn <rhyme label="b" type="m">gesigt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Ist de Son? neen s'is gaen <rhyme label="a" type="f" corresp="#s1:l1">dalen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wel hoe komt het dan soo <rhyme label="b" type="m" corresp="#s1:l2">ligt</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Want mijn dunckt dat yder <rhyme label="c" type="m">strael</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Mijn tot in het herte <rhyme label="c" type="m" corresp="#s1:l5">dwael</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">De Zon komt niet uyt haer <rhyme label="a" type="f">salen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Alsment Vee in d'hutte <rhyme label="b" type="m">jaegt</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Nochtans komtse mijn <rhyme label="a" type="f" corresp="#s2:l1">bestralen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Hoe komt datse mijn <rhyme label="b" type="m" corresp="#s2:l2">behaegt</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">Wijcktse van haer gulden <rhyme label="c" type="m">Throon</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Voor een lomp en slegt <rhyme label="c" type="m" corresp="#s2:l5">persoon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Neen ick kan dat niet <rhyme label="a" type="f">bedencken</rhyme>:</l>
        <l id="s3:l2" met="4" type="+-">Dat de Zon wiens goude <rhyme label="b" type="m">hooft</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Mijn sou flicker straeltjens <rhyme label="a" type="f" corresp="#s3:l1">schencken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ben ick van mijn sin <rhyme label="b" type="m" corresp="#s3:l2">berooft</rhyme>?</l>
        <l id="s3:l5" met="4" type="+-">Ach 't is spoock of vuyl <rhyme label="c" type="m">bejagh</rhyme>?</l>
        <l id="s3:l6" met="4" type="+-">Dat ick stracx soo vlamme <rhyme label="c" type="m" corresp="#s3:l5">sagh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Nu kruypt sy by <rhyme label="a" type="f">Lommer-boomen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Die beschaduwt haer met <rhyme label="b" type="m">blaen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Gode ginder sie ick <rhyme label="a" type="f" corresp="#s4:l1">komen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">D'aersche Zon met glans <rhyme label="b" type="m" corresp="#s4:l2">belaen</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Ach! het is mijn <rhyme label="c" type="m">Rosament</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Die my flicker-straeltjens <rhyme label="c" type="m" corresp="#s4:l5">sont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zon wijckt nu voor gulder <rhyme label="a" type="f">haire</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Maen wijckt nu voor helder <rhyme label="b" type="m">kleur</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Sterren wijckt voor oogjes <rhyme label="a" type="f" corresp="#s5:l1">klare</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Bloemties wijckt voor soeter <rhyme label="b" type="m" corresp="#s5:l2">geur</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Wat in glans en soetheyt <rhyme label="c" type="m">leeft</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Rosamont noch beter <rhyme label="c" type="m" corresp="#s5:l5">heeft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Prutse Maegt die door u <rhyme label="a" type="f">loncken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Hebt mijn sinnen gansch <rhyme label="b" type="m">vervoert</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Soo dat stadigh uwe <rhyme label="a" type="f" corresp="#s6:l1">voncke</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Mijn tot aen mijn Zieltje <rhyme label="b" type="m" corresp="#s6:l2">roert</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Daerom blussing soete <rhyme label="c" type="m">Vrou</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Ick voor brant wel hebbe <rhyme label="c" type="m" corresp="#s6:l5">Wou</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
