  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WEl lustighe <rhyme label="a" type="f">jongelinghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dit edele meysken <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat wil ick een kroesken <rhyme label="a" type="f" corresp="#s1:l1">bringhen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met klare koele <rhyme label="b" type="m" corresp="#s1:l2">Wijn</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En een vreughdelijck Liedeken <rhyme label="a" type="f" corresp="#s1:l1">singhen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ter eeren die liefste <rhyme label="b" type="m" corresp="#s1:l2">mijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dit glaesken wel toe <rhyme label="a" type="f">drincken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dit edele meysken <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Want haer bruyn ooghskens <rhyme label="a" type="f" corresp="#s2:l1">blincken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Soo klaer als <rhyme label="b" type="m" corresp="#s2:l2">cristalijn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wilt elck een kusken om <rhyme label="a" type="f" corresp="#s2:l1">schincken</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ter eeren die liefste <rhyme label="b" type="m" corresp="#s2:l2">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy houtet my heel van <rhyme label="a" type="f">rouwen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dit Edel <rhyme label="b" type="m">Maeghdekijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dus sal ick haer altijdt <rhyme label="a" type="f" corresp="#s3:l1">houwen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Voor mijn <rhyme label="b" type="m" corresp="#s3:l2">Medecijn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">en beminnen oock alle <rhyme label="a" type="f" corresp="#s3:l1">Ionckvrouwen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ter eeren de liefste <rhyme label="b" type="m" corresp="#s3:l2">mijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Princesse fraey van <rhyme label="a" type="f">gronde</rhyme></l>
        <l id="s4:l2" met="3" type="-+">O edele <rhyme label="b" type="m">maeghdekijn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ontfanght dit Bekerken <rhyme label="a" type="f" corresp="#s4:l1">ronde</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met klare koele <rhyme label="b" type="m" corresp="#s4:l2">Wijn</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En een kusjen aen uwen <rhyme label="a" type="f" corresp="#s4:l1">monde</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ter eeren de Liefste <rhyme label="b" type="m" corresp="#s4:l2">mijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
