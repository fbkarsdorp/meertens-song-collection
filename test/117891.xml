    <melody>Venus gij en uw kind</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MIts gy mijn hert en <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met koorden uwer <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">O Jesu! hebt <rhyme label="b" type="f">getoogen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gevoel ick my <rhyme label="b" type="f" corresp="#s1:l3">bewoogen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Om noyt na aerdsche <rhyme label="c" type="f">zaeken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Maer staeg na u te <rhyme label="c" type="f" corresp="#s1:l5">haeken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ey laet doch nimmer <rhyme label="a" type="m">toe</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat ick ooit anders <rhyme label="a" type="m" corresp="#s2:l1">doe</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Laet dit voornemen <rhyme label="b" type="f">blijven</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">En in my vast <rhyme label="b" type="f" corresp="#s2:l3">beklijven</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat ick in vreugt en <rhyme label="c" type="f">pijne</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Magh uw zijn en gy <rhyme label="c" type="f" corresp="#s2:l5">mijne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Geeft my een blijden <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wanneer ick strijden <rhyme label="a" type="m" corresp="#s3:l1">moet</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Strijd met my, en strijd door <rhyme label="b" type="f">my</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Strijd in my, en strijd voor <rhyme label="b" type="f" corresp="#s3:l3">my</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Doet my den pijs <rhyme label="c" type="f">behalen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En moedigh <rhyme label="c" type="f" corresp="#s3:l5">zege-pralen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Mijn heyl, mijn hulp, mijn <rhyme label="a" type="m">raed</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn troost, mijn <rhyme label="a" type="m" corresp="#s4:l1">toeverlaet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Laet selfs de doot niet <rhyme label="b" type="f">scheyden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De liefde van ons <rhyme label="b" type="f" corresp="#s4:l3">beyden</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Maer deese vlam der <rhyme label="c" type="f">minnen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Het alles <rhyme label="c" type="f" corresp="#s4:l5">overwinnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O liefdens soet <rhyme label="a" type="m">gewelt</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Hoe voel ick my <rhyme label="a" type="m" corresp="#s5:l1">gestelt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wanneer gy my komt <rhyme label="b" type="f">dringen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En lieffelick <rhyme label="b" type="f" corresp="#s5:l3">bespringen</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">Waer by sal 't zijn <rhyme label="c" type="f">geleken</rhyme>?</l>
        <l id="s5:l6" met="3" type="-+">'t En is niet uyt te <rhyme label="c" type="f" corresp="#s5:l5">spreeken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mijn ziel aen u vast <rhyme label="a" type="m">kleeft</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Gy zijt, gy hebt, gy <rhyme label="a" type="m" corresp="#s6:l1">geeft</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Al wat ick ooit <rhyme label="b" type="f">behoeve</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">O! die my 't hert <rhyme label="b" type="f" corresp="#s6:l3">door-groeve</rhyme>!</l>
        <l id="s6:l5" met="3" type="-+">Men soud aldaer niet <rhyme label="c" type="f">vinden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Als Iesus mijn <rhyme label="c" type="f" corresp="#s6:l5">beminden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
