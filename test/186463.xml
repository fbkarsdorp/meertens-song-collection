<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186463</id>
    <date>
      <year-start>1701</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Het huwelijk is als een meloen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IK zal myn Philis hulde <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want zy alleen is het <rhyme label="b" type="m">waard</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ach mogt zy zig doch derwaards <rhyme label="a" type="m" corresp="#s1:l1">spoen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Myn grootste vreugd op <rhyme label="b" type="m" corresp="#s1:l2">Aard</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Hoe zalig is het by haar te <rhyme label="c" type="m">zyn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wyl haar <rhyme label="d" type="m">bekoorlykheên</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Myn ontheffe van myn <rhyme label="c" type="m" corresp="#s1:l5">Minnepyn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Die als onbluschbaar <rhyme label="d" type="m" corresp="#s1:l6">scheen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Aanschouw ik haar sneeuwitte <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het wonder der <rhyme label="b" type="m">Natuur</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Haar zuivre borst stookt <rhyme label="a" type="m" corresp="#s2:l1">Minnebrand</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Van 't Goddelyk <rhyme label="b" type="m" corresp="#s2:l2">Minnevuur</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">En als die op het hevigst <rhyme label="c" type="m">blaakt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dan weet zy myne <rhyme label="d" type="m">lust</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Te koesteren op haar Boesem <rhyme label="c" type="m" corresp="#s2:l5">naakt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die van my werd <rhyme label="d" type="m" corresp="#s2:l6">gekust</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ik haar boesem blank en <rhyme label="a" type="m">rond</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Als Goddelyk <rhyme label="b" type="m">aanschouw</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Dan blaast zy uyt haar lieve <rhyme label="a" type="m" corresp="#s3:l1">mond</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Op my een frisse <rhyme label="b" type="m" corresp="#s3:l2">douw</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Waar by een opgespalkte <rhyme label="c" type="m">Roos</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Verfrist met al haar <rhyme label="d" type="m">geur</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">En de alderaangenaamste <rhyme label="c" type="m" corresp="#s3:l5">bloos</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Dat straalt haar koontje <rhyme label="d" type="m" corresp="#s3:l6">deur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Geen tintelende Ster <rhyme label="a" type="m">gelykt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ook by haar schitterend <rhyme label="b" type="m">Oog</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat zelfs van boven is <rhyme label="a" type="m" corresp="#s4:l1">verrykt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met een zeer fraaje <rhyme label="b" type="m" corresp="#s4:l2">boog</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ja zy is in alles zoo <rhyme label="c" type="m">volmaakt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Zy tergd het Venus <rhyme label="d" type="m">Beeld</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Dat in geen schoonheid haar <rhyme label="c" type="m" corresp="#s4:l5">genaakt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Waar van zy is <rhyme label="d" type="m" corresp="#s4:l6">geteeld</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ach hoe vol zalig is hy <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Te zyn van haar <rhyme label="b" type="m">bemind</rhyme>?</l>
        <l id="s5:l3" met="4" type="-+">'k Bid u ô schoone neem my <rhyme label="a" type="m" corresp="#s5:l1">aan</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Voor Hymens Echt <rhyme label="b" type="m" corresp="#s5:l2">Altaar</rhyme>!</l>
        <l id="s5:l5" met="4" type="-+">Vergunt die gunst aan 't <rhyme label="c" type="m">Godendom</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Haar kransjes zyn <rhyme label="d" type="m">gevlegt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Die om het Hooft als <rhyme label="c" type="m" corresp="#s5:l5">Bruidegom</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Voor eeuwig zyn <rhyme label="d" type="m" corresp="#s5:l6">gehegt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
