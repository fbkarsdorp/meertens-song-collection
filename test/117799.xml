      <category translation="narratives">verhalen</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik Droomden op een <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat ik geheel <rhyme label="a" type="m" corresp="#s1:l1">verblyd</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Reê op een Drie gepootten <rhyme label="b" type="m">Os</rhyme></l>
        <l id="s1:l4" met="4" type="-+">De Wereld om, wyl hy waar <rhyme label="b" type="m" corresp="#s1:l3">los</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s1:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Bloed wat zag ik <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zo dra ik uyt de <rhyme label="a" type="m" corresp="#s2:l1">Stal</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Sprong op myn Beesjen als <rhyme label="b" type="m">Quichot</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Gewapent als een <rhyme label="b" type="m" corresp="#s2:l3">Hottentot</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s2:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik Reê eerst langs een <rhyme label="a" type="m">Heg</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Tot ik kwam op een <rhyme label="a" type="m" corresp="#s3:l1">Weg</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die liep regt na <rhyme label="b" type="m">Batavia</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ik kwam daar met myn Beesje <rhyme label="b" type="m" corresp="#s3:l3">dra</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s3:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'k Zag daar een heele <rhyme label="a" type="m">Rey</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ezels, die Schransten <rhyme label="a" type="m" corresp="#s4:l1">Brey</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Elk had een Lepel in de <rhyme label="b" type="m">Poot</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ik lagte my wel half <rhyme label="b" type="m" corresp="#s4:l3">Doot</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s4:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'k Reê weêr een weynig <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Tot Spanjen in de <rhyme label="a" type="m" corresp="#s5:l1">Poort</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Daar zat een Beesje als een <rhyme label="b" type="m">Klop</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die Spon van Paardehaar een <rhyme label="b" type="m" corresp="#s5:l3">Strop</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s5:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat verder zag ik <rhyme label="a" type="m">weêr</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Een Stier, die menig <rhyme label="a" type="m" corresp="#s6:l1">keer</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Vloog als een Vogel door de <rhyme label="b" type="m">Lugt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Hy had twee Vleugels 't leek een <rhyme label="b" type="m" corresp="#s6:l3">Klugt</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s6:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">'k Ging spoedig daar van <rhyme label="a" type="m">daan</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wyl 't Beest niet Stil wou <rhyme label="a" type="m" corresp="#s7:l1">staan</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Tot ik gezwint in Polen <rhyme label="b" type="m">kwam</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En hoor wat dat ik daar <rhyme label="b" type="m" corresp="#s7:l3">Vernam</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s7:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Daar Las een Hont de <rhyme label="a" type="m">Krant</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En een die Blafte <rhyme label="a" type="m" corresp="#s8:l1">Brant</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Vermits die een Trompetter <rhyme label="b" type="m">waar</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">O ramp! Wat blies dat Hontje <rhyme label="b" type="m" corresp="#s8:l3">naar</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s8:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Voort trok ik na <rhyme label="a" type="m">Parys</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Daar zat een Aap heel <rhyme label="a" type="m" corresp="#s9:l1">Grys</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Die vrat zyn Neus heel van zyn <rhyme label="b" type="m">Bek</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">'t Lykt dat de Apen daar zyn <rhyme label="b" type="m" corresp="#s9:l3">Gek</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s9:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">'k Zag ook een <rhyme label="a" type="m">Ojevaar</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Maar dat stont Duyvels <rhyme label="a" type="m" corresp="#s10:l1">raar</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Die Droeg een Oxhooft vol met <rhyme label="b" type="m">Wyn</rhyme></l>
        <l id="s10:l4" met="4" type="-+">En Vloog daar meê waar hy wou <rhyme label="b" type="m" corresp="#s10:l3">zyn</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s10:l5">zel</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Mit viel myn Osjen <rhyme label="a" type="m">neêr</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Ik deê my Duyvels <rhyme label="a" type="m" corresp="#s11:l1">zeer</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Waar door ik haastig Wakker <rhyme label="b" type="m">wier</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Het Dromen is dikmaals <rhyme label="b" type="m" corresp="#s11:l3">Pleysier</rhyme>,</l>
        <l id="s11:l5" met="2" type="-+">Let wel, let <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">Wyl ik noyt Liegen <rhyme label="c" type="m" corresp="#s11:l5">zel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
