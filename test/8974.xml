    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Dies sind die heiligen zehn Gebote</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MEnsch wilt ghy Leven <rhyme label="a" type="m">Salichlijck</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">End by Godt blijven <rhyme label="a" type="m" corresp="#s1:l1">Eewichlijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">So sult ghy houden Thien <rhyme label="b" type="m">Ghebodt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die ons ghebiedet onse <rhyme label="b" type="m" corresp="#s1:l3">GOdt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+"><rhyme label="c" type="f">Kirieleyson</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dijn Godt alleyn end HEEr ick <rhyme label="a" type="m">sy</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Gheen ander God sal arg'ren <rhyme label="a" type="m" corresp="#s2:l1">dy</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Trouwen sal my dat Herte <rhyme label="b" type="m">dijn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Mijn eyghen Rijck sult ghy <rhyme label="b" type="m" corresp="#s2:l3">zijn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+"><rhyme label="c" type="f">Kirieleyson</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy sult mijn Name Eeren <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">End in der Noot my Roepen <rhyme label="a" type="m" corresp="#s3:l1">aen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ghy sult heyligen den Sabbath <rhyme label="b" type="m">Dach</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat ick in dy wercken <rhyme label="b" type="m" corresp="#s3:l3">mach</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+"><rhyme label="c" type="f">Kirieleyson</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Vader en der Moeder <rhyme label="a" type="m">dijn</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Sult ghy nae my ghehoorsaem <rhyme label="a" type="m" corresp="#s4:l1">zijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Niemant Dooden noch Thoornich <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s4:l4" met="4" type="-+">End dijne Trouwe houden <rhyme label="b" type="m" corresp="#s4:l3">reyn</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+"><rhyme label="c" type="f">Kirieleyson</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy sult gheen And'ren stelen <rhyme label="a" type="m">yet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Op niemant Valscheyt tuyghen <rhyme label="a" type="m" corresp="#s5:l1">niet</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dijns Naesten wijf oock niet <rhyme label="b" type="m">begeer'n</rhyme></l>
        <l id="s5:l4" met="4" type="-+">End all' zijns Goets geern <rhyme label="b" type="m" corresp="#s5:l3">Ontbeern'n</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+"><rhyme label="c" type="f">Kirieleyson</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
