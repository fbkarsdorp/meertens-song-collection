  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALs Daifilo speelden en Filida <rhyme label="a" type="m">zong</rhyme>,</l>
        <l id="s1:l2" met="1" type="-+">Van <rhyme label="b" type="f">vreugden</rhyme>,</l>
        <l id="s1:l3" met="1" type="-+">Vol <rhyme label="b" type="f" corresp="#s1:l2">jeugden</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">'t Gantsch bosje schier <rhyme label="a" type="m" corresp="#s1:l1">sprong</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Pan liet zijn <rhyme label="c" type="m">gefluit</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">En quam op 't <rhyme label="c" type="m" corresp="#s1:l5">geluit</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Door doornen en hagen tot by haar en <rhyme label="a" type="m" corresp="#s1:l1">drong</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Elk Hardertje quam met zijn Zoetertjen <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s2:l2" met="1" type="-+">Haar <rhyme label="b" type="f">tuitjes</rhyme>,</l>
        <l id="s2:l3" met="1" type="-+">Met <rhyme label="b" type="f" corresp="#s2:l2">kruitjes</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">En blompjes <rhyme label="a" type="m" corresp="#s2:l1">belaan</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">VVaer van dat den <rhyme label="c" type="m">daauw</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Haar sprenkeltjes, <rhyme label="c" type="m" corresp="#s2:l5">naauw</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Op 't Ã§ierlikst' gepeerelt, af waren <rhyme label="a" type="m" corresp="#s2:l1">gegaan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ten lijkte geen aartsch' maar een Hemelsche <rhyme label="a" type="m">toon</rhyme>,</l>
        <l id="s3:l2" met="1" type="-+">De <rhyme label="b" type="f">bomen</rhyme>,</l>
        <l id="s3:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s3:l2">stromen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Zijn tuigen; den <rhyme label="a" type="m" corresp="#s3:l1">Doon</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Verzuimde zijn <rhyme label="c" type="m">zwier</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Door 't juichend' <rhyme label="c" type="m" corresp="#s3:l5">getier</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Dog Filidaas gorgeltje spanden de <rhyme label="a" type="m" corresp="#s3:l1">kroon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
