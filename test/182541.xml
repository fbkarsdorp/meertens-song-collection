  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'k Wensch u gesondheid zoete <rhyme label="a" type="f">liefste</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">'k Wensch u gesondheid harte <rhyme label="a" type="f" corresp="#s1:l1">diefje</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Hoe heerlyk smaakt, hoe heerlyk <rhyme label="b" type="m">smaakt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">De wyn die ik met Damon <rhyme label="c" type="f">drinken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hoe kan de wyn de liefde <rhyme label="a" type="f">kweeken</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Hoe kan de wyn de min <rhyme label="a" type="f" corresp="#s2:l1">ontsteeken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hoe kan de wyn, hoe dan de <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Hoe kan de wyn de liefde <rhyme label="c" type="f">kweeken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat glaesje schenk ik u ter eere <rhyme label="a" type="f">in</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dat glaasje schenk ik u <rhyme label="a" type="f" corresp="#s3:l1">Koningin</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Godin ons klinke, ons kussen <rhyme label="b" type="m">klinke</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Daer door bekroond gy onse <rhyme label="c" type="f">min</rhyme>.</l>
      </lg>
    </lg>
  </text>
