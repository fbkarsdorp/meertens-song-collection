<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178409</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Blijf liever ongetrouwd</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'k Bemin Oranjes Prins <rhyme label="a" type="m">opregt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">'k Heb mijn hart hem <rhyme label="a" type="m" corresp="#s1:l1">toegezegd</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s1:l2">Oranje</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s1:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'k Heb alles over voor dien <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Hij heeft nooit zijn gemoed <rhyme label="a" type="m" corresp="#s2:l1">bemorst</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s2:l2">Oranje</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s2:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het heeft 'er al te vuil <rhyme label="a" type="m">gegaan</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Men heeft met hoon hem <rhyme label="a" type="m" corresp="#s3:l1">overlaên</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s3:l2">Oranje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s3:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy waakte als Neêrlands <rhyme label="a" type="m">Admiraal</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Nu blinkt alom zyn <rhyme label="a" type="m" corresp="#s4:l1">zegepraal</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s4:l2">Oranje</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s4:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Men schold nooit iemand zo als <rhyme label="a" type="m">hem</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">De Duivel haal die laster <rhyme label="a" type="m" corresp="#s5:l1">stem</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s5:l2">Oranje</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s5:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De loogens zoogenz' uit hun <rhyme label="a" type="m">poot</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Om 't kwaad te redden uit den <rhyme label="a" type="m" corresp="#s6:l1">nood</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s6:l2">Oranje</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s6:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Zy kreegen voor dat schelden <rhyme label="a" type="m">loon</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Nu staan de guiten al ten <rhyme label="a" type="m" corresp="#s7:l1">toon</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s7:l2">Oranje</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s7:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Zy waaren daar toe <rhyme label="a" type="m">omgekogt</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Zy hebben Willems val <rhyme label="a" type="m" corresp="#s8:l1">gezogt</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s8:l2">Oranje</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s8:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hun lastersmoel is al <rhyme label="a" type="m">gestopt</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Hun eige list heeft hen <rhyme label="a" type="m" corresp="#s9:l1">gefopt</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s9:l2">Oranje</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s9:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Des Prinsen daan verdienen <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Hy blinkt na 't lasteren des te <rhyme label="a" type="m" corresp="#s10:l1">meer</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s10:l2">Oranje</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s10:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Nu vlugten zy als rekels <rhyme label="a" type="m">heen</rhyme>.</l>
        <l id="s11:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Zy kruipen weg van bang <rhyme label="a" type="m" corresp="#s11:l1">geween</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Ja, ja, 'k bemin <rhyme label="b" type="f" corresp="#s11:l2">Oranje</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s11:l3">Prins</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Elk hunner word een helsche <rhyme label="a" type="m">draak</rhyme>,</l>
        <l id="s12:l2" met="2" type="-+">'k Bemin <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s12:l3" met="2" type="-+">Viva de <rhyme label="c" type="m">Prins</rhyme>.</l>
        <l id="s12:l4" met="4" type="-+">Hier mee geeft hen de Satan <rhyme label="a" type="m" corresp="#s12:l1">taak</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Ja, ja, 'k Bemin <rhyme label="b" type="f" corresp="#s12:l2">Oranje</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Ja, ja, ja, de <rhyme label="c" type="m" corresp="#s12:l3">Prins</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
