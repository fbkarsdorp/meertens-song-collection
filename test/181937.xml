  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Juffrouw ik kom hier <rhyme label="a" type="f">verkoped</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Al die soort van <rhyme label="b" type="m">Savonet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Skoon ik mit een Kruk mot <rhyme label="a" type="f" corresp="#s1:l1">loped</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Ik bid daar niet eens op <rhyme label="b" type="m" corresp="#s1:l2">let</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">’k Eb veel soort van <rhyme label="c" type="m">wateren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Om te was de <rhyme label="c" type="m" corresp="#s1:l5">snateren</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Eb ik mooye <rhyme label="d" type="f">Savonette</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Eb ik mooye <rhyme label="e" type="m">Savonet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vouler vous l’eau <rhyme label="a" type="f">d’Orange</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Sekke my ik ebze <rhyme label="b" type="m">bon</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ou vouler vous l’eau d’ <rhyme label="a" type="f" corresp="#s2:l1">Almange</rhyme> :</l>
        <l id="s2:l4" met="4" type="+-">Die soo klaer is als de <rhyme label="b" type="m" corresp="#s2:l2">Son</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">’t Is kemaekte in <rhyme label="c" type="m">Parys</rhyme>:</l>
        <l id="s2:l6" met="4" type="+-">Het maek al de Kekken <rhyme label="c" type="m" corresp="#s2:l5">wys</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Maer nok meer myn <rhyme label="d" type="f">Savonette</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Maer nok meer myn <rhyme label="e" type="m">Savonet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">L’eau de Reine eb ik <rhyme label="a" type="f">skonen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ook Pumad voor de <rhyme label="b" type="m">Madam</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Om te krygen een Jonke <rhyme label="a" type="f" corresp="#s3:l1">zonen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Eb ik nog wat in myn <rhyme label="b" type="m" corresp="#s3:l2">Kraem</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En dat is soo <rhyme label="c" type="m">delicaet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">’t Eeeft in ’t k’eel keen bien nog <rhyme label="c" type="m" corresp="#s3:l5">kraet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En dat is die <rhyme label="d" type="f">Savonette</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En dat is die <rhyme label="e" type="m">Savonet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wilke moesjes van myn <rhyme label="a" type="f">koped</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Om te setten op de <rhyme label="b" type="m">Kin</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Neem die doosje maek jy <rhyme label="a" type="f" corresp="#s4:l1">oped</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Daer syn al die moesjes <rhyme label="b" type="m" corresp="#s4:l2">in</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Mais vous prenes rein de <rhyme label="c" type="m">tout</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dittes mooi que vouler <rhyme label="c" type="m" corresp="#s4:l5">vous</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Mais je crooi mon <rhyme label="d" type="f">Savonette</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">’t Is van die <rhyme label="e" type="m">Savonet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Voila daer eb je se <rhyme label="a" type="f">allen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Die ik eb in myne <rhyme label="b" type="m">Kraem</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">’k Eb maer twee van deese <rhyme label="a" type="f" corresp="#s5:l1">Ballen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Daer eb jy ze nou <rhyme label="b" type="m" corresp="#s5:l2">madaem</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Maer kebruikse niet te <rhyme label="c" type="m">veel</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">’t Breekt zuur op by myn <rhyme label="c" type="m" corresp="#s5:l5">keel</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Want dat het die <rhyme label="d" type="f">Savonette</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Want dat het die <rhyme label="e" type="m">Savonet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
