<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>192759</id>
    <date>
      <year-start>1748</year-start>
      <year-end>1748</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
