      </coordinates>
    </location>
    <tags>
      <tag standard-name="moralistisch lied" translation="moralistic song">moralistisch lied</tag>
    </tags>
    <melody>Wilhelmus</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOort vrienden na dees <rhyme label="a" type="f">lesse</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En wiltse wel <rhyme label="b" type="m">verstaen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Al die ter <rhyme label="a" type="f" corresp="#s1:l1">Kerremisse</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Na Siecken willen <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wilt den buydel wel <rhyme label="c" type="f">voeren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">met Cruys munte van <rhyme label="d" type="m">gelt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Eer ghy komt by de <rhyme label="c" type="f" corresp="#s1:l5">Boeren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Te Siecken op dat <rhyme label="d" type="m" corresp="#s1:l6">Velt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Trompetten hoordt ghy <rhyme label="a" type="f">klincken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ende de Trommels <rhyme label="b" type="m">slaen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Veel prijsen siet ghy <rhyme label="a" type="f" corresp="#s2:l1">blincken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met Vaendels wijt <rhyme label="b" type="m" corresp="#s2:l2">ontdaen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dinghen vreemt om <rhyme label="c" type="f">ghelijcken</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sijn daer met groot <rhyme label="d" type="m">ghedruys</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Oock over al te <rhyme label="c" type="f" corresp="#s2:l5">kijcken</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Voor ghelt in menich <rhyme label="d" type="m" corresp="#s2:l6">huys</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">D'een roept komt in twee <rhyme label="a" type="f">blancken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sult ghy maer gheven <rhyme label="b" type="m">reen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">D'ander sou hem <rhyme label="a" type="f" corresp="#s3:l1">bedancken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al kregher hy maer <rhyme label="b" type="m" corresp="#s3:l2">een</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Dan komt den armen <rhyme label="c" type="f">cluyver</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Oock aen zijn poortgen <rhyme label="d" type="m">staen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Komt om een halve <rhyme label="c" type="f" corresp="#s3:l5">stuyver</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">D'ander een Oortgen <rhyme label="d" type="m" corresp="#s3:l6">saen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Brieven, History <rhyme label="a" type="f">Boecken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Draeyt, werpt, treckt hier om <rhyme label="b" type="m">prijs</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Noten, Appels en <rhyme label="a" type="f" corresp="#s4:l1">Koecken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Poppegoet seer <rhyme label="b" type="m" corresp="#s4:l2">logijs</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Kinder Molentgens <rhyme label="c" type="f">bouwen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Zy daer oock, sonder <rhyme label="d" type="m">jock</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wilt ghy een Koecxken <rhyme label="c" type="f" corresp="#s4:l5">houwen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Aenveert Bijl ende <rhyme label="d" type="m" corresp="#s4:l6">Block</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt peyster Korfkens <rhyme label="a" type="f">vullen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met Broot, Spec, Ham, en <rhyme label="b" type="m">vleys</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">wilt Paes Eyerkens <rhyme label="a" type="f" corresp="#s5:l1">smullen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Kaes, Boter is den <rhyme label="b" type="m" corresp="#s5:l2">eys</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Tis voor den Buyck sou'c <rhyme label="c" type="f">dincken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Soo goet een <rhyme label="d" type="m">Medecijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">maer ghy moet daer op <rhyme label="c" type="f" corresp="#s5:l5">drincken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Goet Bier oft klaren <rhyme label="d" type="m" corresp="#s5:l6">Wijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Veel menschen daer <rhyme label="a" type="f">vergaren</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Out, jongh, groot ende <rhyme label="b" type="m">kleen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Vryers en Vrysters <rhyme label="a" type="f" corresp="#s6:l1">paren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Sy weten van gheen <rhyme label="b" type="m" corresp="#s6:l2">scheen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat komt door 'tlustich <rhyme label="c" type="f">quelen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van Liedekens <rhyme label="d" type="m">excellent</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Boven dien hoort ghy <rhyme label="c" type="f" corresp="#s6:l5">spelen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Op menich <rhyme label="d" type="m" corresp="#s6:l6">Instrument</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Om de spijs te <rhyme label="a" type="f">vertreden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">wilt 'tHuys ter spijt aen <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">En dan tsamen met <rhyme label="a" type="f" corresp="#s7:l1">vreden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Trect deur de <rhyme label="b" type="m" corresp="#s7:l2">Kleverlaen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">maect dan met <rhyme label="c" type="f">triumpheren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">'tOverveen u zeyl <rhyme label="d" type="m">nat</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En comt also <rhyme label="c" type="f" corresp="#s7:l5">laveren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">weer soetjens na de <rhyme label="d" type="m" corresp="#s7:l6">Stadt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof Edel <rhyme label="a" type="f">Princieren</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Princessen <rhyme label="b" type="m">onbevlect</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Liefde reyn van <rhyme label="a" type="f" corresp="#s8:l1">manieren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Heeft my hier toe <rhyme label="b" type="m" corresp="#s8:l2">verweckt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En wilt my niet <rhyme label="c" type="f">vergeten</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Als ghy banct Speck of <rhyme label="d" type="m">Ham</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Ick meen oock Koec te <rhyme label="c" type="f" corresp="#s8:l5">eten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Haest binnen <rhyme label="d" type="m" corresp="#s8:l6">Amsterdam</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
