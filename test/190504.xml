<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190504</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">A la Messieurs sa wilse <rhyme label="a" type="f">kyken</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">La Rareté, la <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dingen jou siet nooit zyns <rhyme label="a" type="f" corresp="#s1:l1">kelyke</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s1:l2">Curiosité</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Alle soorte van kekke <rhyme label="c" type="f">sotten</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die wil pronk met de skone <rhyme label="d" type="m">naam</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Van koede brave <rhyme label="c" type="f" corresp="#s1:l5">Patriotten</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Daar zy zyn vuilken al te <rhyme label="d" type="m" corresp="#s1:l6">zaam</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s1:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s1:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hier zel jou ras en klaar <rhyme label="a" type="f">ontdekken</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Iets dat jou tot een les moet <rhyme label="a" type="f" corresp="#s2:l1">strekken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s2:l2">Curiosité</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dat als de aap hoog klimmen <rhyme label="c" type="f">willen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Men, kelyk als de spreekwoord <rhyme label="d" type="m">zek</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Straks ziet hun kaale naakte <rhyme label="c" type="f" corresp="#s2:l5">billen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Hun toonen duvels kwaat of <rhyme label="d" type="m" corresp="#s2:l6">kek</rhyme>,</l>
        <l id="s2:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s2:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s2:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daar zie jou eerst van Berkel <rhyme label="a" type="f">koomen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">'t Krote Kopstuk van Hollands <rhyme label="a" type="f" corresp="#s3:l1">Rome</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s3:l2">Curiosité</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Zie hum daar eed en plikt <rhyme label="c" type="f">verzaken</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En stout, wat onkehoorde <rhyme label="d" type="m">zaak</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Propria Auctoritate <rhyme label="c" type="f" corresp="#s3:l5">maken</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Een verdrak met <rhyme label="d" type="m" corresp="#s3:l6">d'Amerikaan</rhyme>.</l>
        <l id="s3:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s3:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s3:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daar heb se Jonker van <rhyme label="a" type="f">Capellen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Le Demagoge des <rhyme label="a" type="f" corresp="#s4:l1">Rebellen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s4:l2">Curiosité</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Hy ontfank van een troup <rhyme label="c" type="f">Canaille</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Die voor hum mooy Soldaatje <rhyme label="d" type="m">speel</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Een kroote skone gou <rhyme label="c" type="f" corresp="#s4:l5">Medailje</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Hankt met de lind die aan zyn <rhyme label="d" type="m" corresp="#s4:l6">keel</rhyme>,</l>
        <l id="s4:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s4:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s4:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Zie daar de Amsterdamsche <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hoe druk zy ben aan 't <rhyme label="a" type="f" corresp="#s5:l1">besoigneeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s5:l2">Curiosité</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Zy weet niet hoe zy 't maken <rhyme label="c" type="f">zullen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Nu zy zien, hoe zy als de <rhyme label="d" type="m">sot</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Zik van d'Amerikaan liet <rhyme label="c" type="f" corresp="#s5:l5">sullen</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">En al den duvle heb <rhyme label="d" type="m" corresp="#s5:l6">verbrod</rhyme>,</l>
        <l id="s5:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s5:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s5:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Daar heb ze nou het <rhyme label="a" type="f">Dortenaartje</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">'t Kwaat en keffende <rhyme label="a" type="f" corresp="#s6:l1">Gyselaartje</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s6:l2">Curiosité</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Uit zyn kezigt, men kan wel <rhyme label="c" type="f">leezen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Hy vrees voor duvle nok voor <rhyme label="d" type="m">dood</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Zoo hum maar aankezien mag <rhyme label="c" type="f" corresp="#s6:l5">weezen</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Pour Jean de Wit, of Huig de <rhyme label="d" type="m" corresp="#s6:l6">Kroot</rhyme>,</l>
        <l id="s6:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s6:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s6:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daar wil hum door de Hofpoort <rhyme label="a" type="f">ryden</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dat wil Monsr. Morant niet <rhyme label="a" type="f" corresp="#s7:l1">lyden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s7:l2">Curiosité</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Hy pakt de Paart en hout se <rhyme label="c" type="f">tegen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Mais voyez vous, de man is <rhyme label="d" type="m">sat</rhyme>;</l>
        <l id="s7:l7" met="4" type="-+">Monsr. van Nispen trek de <rhyme label="c" type="f" corresp="#s7:l5">deegen</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Schoon men hier nix te vreezen <rhyme label="d" type="m" corresp="#s7:l6">had</rhyme>.</l>
        <l id="s7:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s7:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s7:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ok! ok! daar word Morant <rhyme label="a" type="f">kevangen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">d'Armen drommel wil men <rhyme label="a" type="f" corresp="#s8:l1">hangen</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s8:l2">Curiosité</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Daar ziet se de skavot al <rhyme label="c" type="f">bouwen</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Wat zyn de Patriotten <rhyme label="d" type="m">bly</rhyme>!</l>
        <l id="s8:l7" met="4" type="-+">Maar ze bekryp dit zou hun <rhyme label="c" type="f" corresp="#s8:l5">rouwen</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Morant blyf van de kalge <rhyme label="d" type="m" corresp="#s8:l6">vry</rhyme>,</l>
        <l id="s8:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s8:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s8:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Daar heb se nou die krote <rhyme label="a" type="f">baazen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Jean de Roye van <rhyme label="a" type="f" corresp="#s9:l1">Westmase</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s9:l2">Curiosité</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Kent ze de Vent niet koeje <rhyme label="c" type="f">vruitje</rhyme>.</l>
        <l id="s9:l6" met="4" type="-+">ô Het is zo en keleerden <rhyme label="d" type="m">Heer</rhyme>.</l>
        <l id="s9:l7" met="4" type="-+">Die om de kleln onnosel <rhyme label="c" type="f" corresp="#s9:l5">pruitje</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">De Staatsverkadring <rhyme label="d" type="m" corresp="#s9:l6">convoieer</rhyme>.</l>
        <l id="s9:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s9:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s9:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Daar hebt se Spaan met zyn twee <rhyme label="a" type="f">Jongen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Wat doen die aapen kekke <rhyme label="a" type="f" corresp="#s10:l1">sprongen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s10:l2">Curiosité</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Zie hoe die dronke duvele <rhyme label="c" type="f">zwayen</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Hoe hum staat als een <rhyme label="d" type="m">Oojevaar</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Op de Stadhous, romtom te <rhyme label="c" type="f" corresp="#s10:l5">drayen</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">ô Fraai curieus, ô dat is <rhyme label="d" type="m" corresp="#s10:l6">raar</rhyme>!</l>
        <l id="s10:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s10:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s10:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">ô Le Prevôt, by volle <rhyme label="a" type="f">flessen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Zie daar zyn Wyf die mal <rhyme label="a" type="f" corresp="#s11:l1">Princesse</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s11:l2">Curiosité</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Zie hun daar zitten met hun <rhyme label="c" type="f">beiden</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Zoo opkeblazen als de <rhyme label="d" type="m">Pad</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Door de Stadhoudre Poort hem <rhyme label="c" type="f" corresp="#s11:l5">ryden</rhyme>,</l>
        <l id="s11:l8" met="4" type="-+">Tot walkens toe van glory <rhyme label="d" type="m" corresp="#s11:l6">zat</rhyme>.</l>
        <l id="s11:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s11:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s11:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Daar zie jou veel kebeste <rhyme label="a" type="f">knapen</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Kunsjes doen net kelyk de <rhyme label="a" type="f" corresp="#s12:l1">aapen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s12:l2">Curiosité</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Snaphaan op schoer: Eerwaarde <rhyme label="c" type="f">Heeren</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Die thans den brui heb te <rhyme label="d" type="m">studeer</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Maar als Soldaat wil <rhyme label="c" type="f" corresp="#s12:l5">exerceeren</rhyme>,</l>
        <l id="s12:l8" met="4" type="-+">Of leer behandlen het <rhyme label="d" type="m" corresp="#s12:l6">keweer</rhyme>.</l>
        <l id="s12:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s12:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s12:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Zie! zie! waar zag ze ooit huur <rhyme label="a" type="f">kelyken</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Zie! zie! hoe boos zy staan te <rhyme label="a" type="f" corresp="#s13:l1">kyken</rhyme>:</l>
        <l id="s13:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s13:l2">Curiosité</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Elk pronkt, Hemel welk een <rhyme label="c" type="f">wonder</rhyme>!</l>
        <l id="s13:l6" met="4" type="-+">Met een postiche <rhyme label="d" type="m">knevelbaart</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">Ziet zoo brutaal als een Hans <rhyme label="c" type="f" corresp="#s13:l5">donder</rhyme>,</l>
        <l id="s13:l8" met="4" type="-+">Of als Schoppe Boer in de <rhyme label="d" type="m" corresp="#s13:l6">Kaart</rhyme>.</l>
        <l id="s13:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s13:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s13:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Sa prenez garde op dees <rhyme label="a" type="f">vertooning</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Voila le plus grand <rhyme label="a" type="f" corresp="#s14:l1">Apekoning</rhyme>;</l>
        <l id="s14:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s14:l2">Curiosité</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">Monsr. van Salm, zie zie hum <rhyme label="c" type="f">springen</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">Hy stap of niemant hum kom <rhyme label="d" type="m">deer</rhyme>,</l>
        <l id="s14:l7" met="4" type="-+">'t Is of hum zou den duvle <rhyme label="c" type="f" corresp="#s14:l5">dwingen</rhyme>,</l>
        <l id="s14:l8" met="4" type="-+">Hun zie zoo bars kelyk een <rhyme label="d" type="m" corresp="#s14:l6">beer</rhyme>.</l>
        <l id="s14:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s14:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s14:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Zie! zie! hum daar in kroote <rhyme label="a" type="f">sakken</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Al zyn kestool ducaren <rhyme label="a" type="f" corresp="#s15:l1">pakken</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s15:l2">Curiosité</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Zie! zie! wat maak hum kekke <rhyme label="c" type="f">grappe</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Hy krap de kop, hum wou graag <rhyme label="d" type="m">vort</rhyme>,</l>
        <l id="s15:l7" met="4" type="-+">Uit Holland na Parys <rhyme label="c" type="f" corresp="#s15:l5">opstappen</rhyme>,</l>
        <l id="s15:l8" met="4" type="-+">Eer hum hier opgehangen <rhyme label="d" type="m" corresp="#s15:l6">word</rhyme>.</l>
        <l id="s15:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s15:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s15:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Wie ziet ze daar zoo trots <rhyme label="a" type="f">aankoomen</rhyme>?</l>
        <l id="s16:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Le Dictateur van Amstels <rhyme label="a" type="f" corresp="#s16:l1">Roomen</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s16:l2">Curiosité</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Monsr. Goudoure met zyn <rhyme label="c" type="f">benden</rhyme>,</l>
        <l id="s16:l6" met="4" type="-+">Qui les Elits du <rhyme label="d" type="m">Magistraat</rhyme>,</l>
        <l id="s16:l7" met="4" type="-+">Schop van 't Stadhous, en henen <rhyme label="c" type="f" corresp="#s16:l5">zenden</rhyme>,</l>
        <l id="s16:l8" met="4" type="-+">ô! die verduvelde <rhyme label="d" type="m" corresp="#s16:l6">Democraat</rhyme>!</l>
        <l id="s16:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s16:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s16:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Ha! ha! nu zal ze moeten <rhyme label="a" type="f">lakken</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Dat ze schier in de broek zou <rhyme label="a" type="f" corresp="#s17:l1">kakken</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s17:l2">Curiosité</rhyme>,</l>
        <l id="s17:l5" met="4" type="+-">Ken jou daar niet die maagre <rhyme label="c" type="f">Ritter</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">Die Balnewis zoo hep <rhyme label="d" type="m">keplaagt</rhyme>,</l>
        <l id="s17:l7" met="4" type="-+">Die Pagattre, die van <rhyme label="c" type="f" corresp="#s17:l5">Sitter</rhyme>,</l>
        <l id="s17:l8" met="4" type="-+">Die de Soldaat hep <rhyme label="d" type="m" corresp="#s17:l6">wekgejaagt</rhyme>.</l>
        <l id="s17:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s17:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s17:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Zie hum daar daar in zyn hemt staan <rhyme label="a" type="f">pronke</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Hum is parbleu half slape <rhyme label="a" type="f" corresp="#s18:l1">drenke</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s18:l2">Curiosité</rhyme>,</l>
        <l id="s18:l5" met="4" type="+-">Kyk! kyk! wat trekt hum viese <rhyme label="c" type="f">bekke</rhyme>,</l>
        <l id="s18:l6" met="4" type="-+">Nu hum Balnewis schoon <rhyme label="d" type="m">badot</rhyme>,</l>
        <l id="s18:l7" met="4" type="-+">En uit zyn slaapt hem op komt <rhyme label="c" type="f" corresp="#s18:l5">wekken</rhyme>,</l>
        <l id="s18:l8" met="4" type="-+">Hum Conduiseer na het <rhyme label="d" type="m" corresp="#s18:l6">Casjot</rhyme>.</l>
        <l id="s18:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s18:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s18:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Schrikke non niet c'est <rhyme label="a" type="f">detestable</rhyme>!</l>
        <l id="s19:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Le pourtrait du plus laide <rhyme label="a" type="f" corresp="#s19:l1">Diable</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s19:l2">Curiosité</rhyme>,</l>
        <l id="s19:l5" met="4" type="+-">Quel visage de <rhyme label="c" type="f">Contrabande</rhyme>!</l>
        <l id="s19:l6" met="4" type="-+">Quel foutu nez! twee vuisten <rhyme label="d" type="m">lang</rhyme>,</l>
        <l id="s19:l7" met="4" type="-+">Kyk wat een bult, kyk wat voor <rhyme label="c" type="f" corresp="#s19:l5">handen</rhyme>,</l>
        <l id="s19:l8" met="4" type="-+">Een figuur als een kromme <rhyme label="d" type="m" corresp="#s19:l6">tang</rhyme>;</l>
        <l id="s19:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s19:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s19:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Se kent hun wel dat 's vast en <rhyme label="a" type="f">zekere</rhyme>,</l>
        <l id="s20:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">Suljart de grootste <rhyme label="a" type="f" corresp="#s20:l1">Eedverbrekere</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s20:l2">Curiosité</rhyme>,</l>
        <l id="s20:l5" met="4" type="+-">Zie, heb se van jou leeve <rhyme label="c" type="f">daagen</rhyme>,</l>
        <l id="s20:l6" met="4" type="-+">Wel kezien wat die beul <rhyme label="d" type="m">bedryf</rhyme>?</l>
        <l id="s20:l7" met="4" type="-+">Hy laat de braaf Sergeant <rhyme label="c" type="f" corresp="#s20:l5">wegjaagen</rhyme>,</l>
        <l id="s20:l8" met="4" type="-+">Kort hem de tres, snyd de kleed van 't <rhyme label="d" type="m" corresp="#s20:l6">lyf</rhyme>.</l>
        <l id="s20:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s20:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s20:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Daar zied ze nou die kroote <rhyme label="a" type="f">baazen</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">Die als verwoede duvle <rhyme label="a" type="f" corresp="#s21:l1">raasen</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s21:l2">Curiosité</rhyme>,</l>
        <l id="s21:l5" met="4" type="+-">Alles zy zal tot Huspot <rhyme label="c" type="f">kappen</rhyme>,</l>
        <l id="s21:l6" met="4" type="-+">Eer ze verlaat de Stat van <rhyme label="d" type="m">Wyk</rhyme>,</l>
        <l id="s21:l7" met="4" type="-+">Kyk ze daar nou de broek vol <rhyme label="c" type="f" corresp="#s21:l5">kakken</rhyme>,</l>
        <l id="s21:l8" met="4" type="-+">A la Messieurs, sa kyk, sa <rhyme label="d" type="m" corresp="#s21:l6">kyk</rhyme>.</l>
        <l id="s21:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s21:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s21:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Ha! ziet ze daar den Oproer <rhyme label="a" type="f">krayer</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>!</l>
        <l id="s22:l3" met="4" type="-+">Van der Kempe die Twedragt <rhyme label="a" type="f" corresp="#s22:l1">zayer</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s22:l2">Curiosité</rhyme>,</l>
        <l id="s22:l5" met="4" type="+-">Een Dominé en <rhyme label="c" type="f">Uniforme</rhyme>.</l>
        <l id="s22:l6" met="4" type="-+">En dat nok wel een styf <rhyme label="d" type="m">Menist</rhyme>,</l>
        <l id="s22:l7" met="4" type="-+">Die de Hel zelf wel zou <rhyme label="c" type="f" corresp="#s22:l5">bestormen</rhyme>,</l>
        <l id="s22:l8" met="4" type="-+">Zie hoe hum zik van angst <rhyme label="d" type="m" corresp="#s22:l6">bepist</rhyme>,</l>
        <l id="s22:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s22:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s22:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Non, non Monsieur, hier valt keen <rhyme label="a" type="f">preeke</rhyme>,</l>
        <l id="s23:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">Nous connoissons jou duvle <rhyme label="a" type="f" corresp="#s23:l1">streeken</rhyme>,</l>
        <l id="s23:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s23:l2">Curiosité</rhyme>,</l>
        <l id="s23:l5" met="4" type="+-">Jy zelt ons hier niet ligt <rhyme label="c" type="f">ontglippen</rhyme>,</l>
        <l id="s23:l6" met="4" type="-+">Non, non parbleu kom spoedig <rhyme label="d" type="m">voort</rhyme>,</l>
        <l id="s23:l7" met="4" type="-+">Kyk hoe 2 hum pakken by de <rhyme label="c" type="f" corresp="#s23:l5">slippen</rhyme>,</l>
        <l id="s23:l8" met="4" type="-+">Hum conduiseer naar <rhyme label="d" type="m" corresp="#s23:l6">Amersfoort</rhyme>.</l>
        <l id="s23:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s23:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s23:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Ha! nog een blyk hoe dat de <rhyme label="a" type="f">Keesen</rhyme>,</l>
        <l id="s24:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Voor spook, nok dood, nok duvele <rhyme label="a" type="f" corresp="#s24:l1">vreeze</rhyme>,</l>
        <l id="s24:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s24:l2">Curiosité</rhyme>,</l>
        <l id="s24:l5" met="4" type="+-">Daar gaan hum Soesdyk <rhyme label="c" type="f">attaqueeren</rhyme>,</l>
        <l id="s24:l6" met="4" type="-+">Tien tegen een. ha! dat is <rhyme label="d" type="m">puik</rhyme>,</l>
        <l id="s24:l7" met="4" type="-+">Maar zien hun schielyk <rhyme label="c" type="f" corresp="#s24:l5">retireeren</rhyme>,</l>
        <l id="s24:l8" met="4" type="-+">Hals over kop, in heg, en <rhyme label="d" type="m" corresp="#s24:l6">struik</rhyme>.</l>
        <l id="s24:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s24:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s24:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Zie jou daar wel die <rhyme label="a" type="f">Pruikebollen</rhyme>,</l>
        <l id="s25:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s25:l3" met="4" type="-+">Parbleu hum kop ze zyn op <rhyme label="a" type="f" corresp="#s25:l1">hollen</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s25:l2">Curiosité</rhyme>,</l>
        <l id="s25:l5" met="4" type="+-">Ventre saint gris, se kryg de <rhyme label="c" type="f">stuipen</rhyme>,</l>
        <l id="s25:l6" met="4" type="-+">Ze vrees, ze raken haast van <rhyme label="d" type="m">kant</rhyme>,</l>
        <l id="s25:l7" met="4" type="-+">Zy willen mensch en beest <rhyme label="c" type="f" corresp="#s25:l5">verzuipen</rhyme>,</l>
        <l id="s25:l8" met="4" type="-+">Gaan Inundeer de gansche <rhyme label="d" type="m" corresp="#s25:l6">Land</rhyme>!</l>
        <l id="s25:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s25:l10" met="5" type="+-">Oh la merveille sans <rhyme label="e" type="f" corresp="#s25:l9">pareille</rhyme>!</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Mais quel bruit komt my <rhyme label="a" type="f">verbaazen</rhyme>,</l>
        <l id="s26:l2" met="4" type="-+">La <rhyme label="b" type="m">Rareté</rhyme>,</l>
        <l id="s26:l3" met="4" type="-+">Parbleu se hoor trompetten <rhyme label="a" type="f" corresp="#s26:l1">blaazen</rhyme>,</l>
        <l id="s26:l4" met="3" type="-+">La <rhyme label="b" type="m" corresp="#s26:l2">Curiosité</rhyme>,</l>
        <l id="s26:l5" met="4" type="+-">Hoor! hoor Messieurs de Paarden <rhyme label="c" type="f">bruissche</rhyme>,</l>
        <l id="s26:l6" met="4" type="-+">Se loop eens kyken gins en <rhyme label="d" type="m">weêr</rhyme>.</l>
        <l id="s26:l7" met="4" type="-+">Ventre saint gris, het zyn de <rhyme label="c" type="f" corresp="#s26:l5">Pruissen</rhyme>,</l>
        <l id="s26:l8" met="4" type="-+">Sufficit dan, voor deeze <rhyme label="d" type="m" corresp="#s26:l6">keer</rhyme>.</l>
        <l id="s26:l9" met="5" type="+-">Oh! oh! oh! la <rhyme label="e" type="f">merveille</rhyme>,</l>
        <l id="s26:l10" met="5" type="+-">Oh! la merveille sans <rhyme label="e" type="f" corresp="#s26:l9">pareille</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
