    <melody>Psalm 015 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WIe stijght soo hoogh in heyl en <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat hy met Dy, de grootste <rhyme label="b" type="f">Koning</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Met Dy, de alderhooghste <rhyme label="a" type="m" corresp="#s1:l1">Heer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Op uw’ gewijden bergh <rhyme label="a" type="m" corresp="#s1:l1">verkeer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En plaets be-erv’ in uwe <rhyme label="b" type="f" corresp="#s1:l2">wooning</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die is’t, die, louter van <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Noch afgerecht op slimme <rhyme label="b" type="f">streken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sijn gang van quade paden <rhyme label="a" type="m" corresp="#s2:l1">hoedt</rhyme>;</l>
        <l id="s2:l4" met="4" type="-+">Niet anders oyt dan recht en <rhyme label="a" type="m" corresp="#s2:l1">doet</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Niet anders oyt dan waer wil <rhyme label="b" type="f" corresp="#s2:l2">spreken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die is’t, die niet los-mondigh <rhyme label="a" type="m">snapt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Maer houdt sijn tong alsoo in <rhyme label="b" type="f">banden</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Dat sy van niemandt <rhyme label="a" type="m" corresp="#s3:l1">achterklapt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En niet lichtvaerdigh sich <rhyme label="a" type="m" corresp="#s3:l1">verrapt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Tot ’s even-mensches leet off <rhyme label="b" type="f" corresp="#s3:l2">schande</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die is’t, die, edel van <rhyme label="a" type="m">gedacht</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">De Godt-vergeten snoode <rhyme label="b" type="f">zielen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Al zijnse schoon in staet en <rhyme label="a" type="m" corresp="#s4:l1">pracht</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Geen eer noch achtens waerdigh <rhyme label="a" type="m" corresp="#s4:l1">acht</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En niet voor haer geluck wil <rhyme label="b" type="f" corresp="#s4:l2">knielen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die is’t, die, Goddelijck <rhyme label="a" type="m">gesint</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Sijn met-gesinde houdt in <rhyme label="b" type="f">waerden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En ’tvrome hert van herten <rhyme label="a" type="m" corresp="#s5:l1">mint</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Off ’tschoon by niemant gunst en <rhyme label="a" type="m" corresp="#s5:l1">vindt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En een uytvaeghsel is op <rhyme label="b" type="f" corresp="#s5:l2">aerden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die is’t, die, reyn en trouw van <rhyme label="a" type="m">grondt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">’tGesproken woordt noyt laet <rhyme label="b" type="f">verloren</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Wiens tong met ’thert is in <rhyme label="a" type="m" corresp="#s6:l1">verbondt</rhyme>;</l>
        <l id="s6:l4" met="4" type="-+">Die, schoon hy daer al scha’ by <rhyme label="a" type="m" corresp="#s6:l1">vondt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Niet swackt in’t geen hy heeft <rhyme label="b" type="f" corresp="#s6:l2">gesworen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die is’t, die, niet verdwaest op <rhyme label="a" type="m">geldt</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Den Goudt-gier noyt in’t hert laet <rhyme label="b" type="f">sluypen</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">En ’tsijne niet op woecker <rhyme label="a" type="m" corresp="#s7:l1">stelt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Om, door een ongevoeldt <rhyme label="a" type="m" corresp="#s7:l1">geweldt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Sijns even-naestens bloet te <rhyme label="b" type="f" corresp="#s7:l2">suypen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die is’t, die, vast aen Deuchdt <rhyme label="a" type="m">verknecht</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Door geenich ding sich laet <rhyme label="b" type="f">verrucken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Waer met men ’sRechters hert <rhyme label="a" type="m" corresp="#s8:l1">bevecht</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Om van onrecht te maken <rhyme label="a" type="m" corresp="#s8:l1">recht</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En die geen schuldt heeft te <rhyme label="b" type="f" corresp="#s8:l2">verdrucken</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die soo in hert en daden <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Wat goedts off soets kan hem <rhyme label="b" type="f">ontbreken</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">Sijn doen en laet is wijs en <rhyme label="a" type="m" corresp="#s9:l1">wis</rhyme>?</l>
        <l id="s9:l4" met="4" type="-+">Godts wooning, sijne <rhyme label="a" type="m" corresp="#s9:l1">erffenis</rhyme>:</l>
        <l id="s9:l5" met="4" type="-+">Noyt bleeff reyn Deughdt van loon <rhyme label="b" type="f" corresp="#s9:l2">versteken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
