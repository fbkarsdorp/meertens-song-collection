    <tags/>
    <melody>Psalm 021 Datheen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Heer, de Koning is <rhyme label="a" type="m">verblydt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat uwe hand en <rhyme label="b" type="f">sterkte</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Voor hem het heil <rhyme label="b" type="f" corresp="#s1:l2">uitwerkte</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">Hoe zeer is hy in dezen <rhyme label="a" type="m" corresp="#s1:l1">tijdt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Verheugt, nu gy zyn <rhyme label="c" type="m">hart</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ontheft van alle <rhyme label="c" type="m" corresp="#s1:l5">smart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gy gaaft hem zynes harten <rhyme label="a" type="m">wensch</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">En d'uitspraak zyner <rhyme label="b" type="f">lippen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Liet gy hem niet <rhyme label="b" type="f" corresp="#s2:l2">ontglippen</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Geen quaad treft hem van eenig <rhyme label="a" type="m" corresp="#s2:l1">mensch</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dewyl gy zynen <rhyme label="c" type="m">staat</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Bevestigt, naar uw' <rhyme label="c" type="m" corresp="#s2:l5">raadt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want gy komt hem gedurig <rhyme label="a" type="m">voor</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met zegening van 't <rhyme label="b" type="f">goede</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Uw hand is zyne <rhyme label="b" type="f" corresp="#s3:l2">hoede</rhyme>;</l>
        <l id="s3:l4" met="4" type="-+">'t Blykt dat gy hem uw gunst <rhyme label="a" type="m" corresp="#s3:l1">beschoor</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Want gy zet hem op 't <rhyme label="c" type="m">hoofdt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">De kroon, aan hem <rhyme label="c" type="m" corresp="#s3:l5">belooft</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een goude kroon die gloeit en <rhyme label="a" type="m">straalt</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Hy eischte van u 't <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gy hebt het hem <rhyme label="b" type="f" corresp="#s4:l2">gegeeven</rhyme>;</l>
        <l id="s4:l4" met="4" type="-+">Terwyl hy op den ryksstoel <rhyme label="a" type="m" corresp="#s4:l1">praalt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hebt gy in <rhyme label="c" type="m">eeuwigheit</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Zyn dagen <rhyme label="c" type="m" corresp="#s4:l5">uitgebreidt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Groot is zyn eere door uw <rhyme label="a" type="m">heil</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Zyn Majesteit en <rhyme label="b" type="f">glanssen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Zyn tot aan 's hemels <rhyme label="b" type="f" corresp="#s5:l2">transsen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Verhoogt; zyn heerlykheit is <rhyme label="a" type="m" corresp="#s5:l1">steil</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Geklommen in den <rhyme label="c" type="m">top</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En houd ook nimmer <rhyme label="c" type="m" corresp="#s5:l5">op</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Want gy zet hem tot <rhyme label="a" type="m">zegening</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Alle eeuwen door en <rhyme label="b" type="f">tyden</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Gy koomt zyn hart <rhyme label="b" type="f" corresp="#s6:l2">verblyden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Als gy uit <rhyme label="a" type="m" corresp="#s6:l1">d'opperhemelkring</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Hem met uw oog <rhyme label="c" type="m">bestraalt</rhyme>;</l>
        <l id="s6:l6" met="3" type="-+">Een zon die nimmer <rhyme label="c" type="m" corresp="#s6:l5">daalt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Want 's Konings hart erkent den <rhyme label="a" type="m">Heer</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Godt is zyn <rhyme label="b" type="f">zielsbetrouwen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Die steeds zijn ryk zal <rhyme label="b" type="f" corresp="#s7:l2">bouwen</rhyme>;</l>
        <l id="s7:l4" met="4" type="-+">Door 's Hoogstens goedheit word zyne <rhyme label="a" type="m" corresp="#s7:l1">eer</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Gestaaft, zoodat hy <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Zal wanklen in 't <rhyme label="c" type="m" corresp="#s7:l5">verdriet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Alle uwe haters zal uw <rhyme label="a" type="m">hand</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Wel vinden; in de <rhyme label="b" type="f">landen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Vergaan ze in smaat en <rhyme label="b" type="f" corresp="#s8:l2">schanden</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Gy legt ze in 't woeden aan den <rhyme label="a" type="m" corresp="#s8:l1">band</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Uw rechte hand <rhyme label="c" type="m">verwint</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Al dien ze als vyand <rhyme label="c" type="m" corresp="#s8:l5">vindt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Gy zult ze zetten als een <rhyme label="a" type="m">vier</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Als eenen viergen <rhyme label="b" type="f">oven</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Te lesschen noch te <rhyme label="b" type="f" corresp="#s9:l2">dooven</rhyme>:</l>
        <l id="s9:l4" met="4" type="-+">Uw toornig aanschyn zal ze <rhyme label="a" type="m" corresp="#s9:l1">fier</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Verslinden, en de <rhyme label="c" type="m">kracht</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Des viers verteert hun <rhyme label="c" type="m" corresp="#s9:l5">magt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Gy zult hun vruchten van der <rhyme label="a" type="m">aard'</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Verdoen, en gansch <rhyme label="b" type="f">verdelgen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">'t Graf zal hunne eer <rhyme label="b" type="f" corresp="#s10:l2">verzwelgen</rhyme>;</l>
        <l id="s10:l4" met="4" type="-+">Hun kinderen, veracht, <rhyme label="a" type="m" corresp="#s10:l1">onwaard</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Ruktge uit den <rhyme label="c" type="m">levensstaat</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Der menschen, met hun <rhyme label="c" type="m" corresp="#s10:l5">zaadt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Want, Heer, zy hebben stout <rhyme label="a" type="m">bestaan</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Quaad tegens u <rhyme label="b" type="f">smeeden</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">Een daad vol <rhyme label="b" type="f" corresp="#s11:l2">schendlykheden</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Bedacht, maar zy zal haast <rhyme label="a" type="m" corresp="#s11:l1">vergaan</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Hun aanslag en <rhyme label="c" type="m">gewelt</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Wordt door uw hand <rhyme label="c" type="m" corresp="#s11:l5">gevelt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Dewyl gy hen ten doelwit <rhyme label="a" type="m">zet</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Van uwen booge en <rhyme label="b" type="f">peezen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Daar zy den schoot van <rhyme label="b" type="f" corresp="#s12:l2">vreezen</rhyme>:</l>
        <l id="s12:l4" met="4" type="-+">Uw wraakzwaard is op hen <rhyme label="a" type="m" corresp="#s12:l1">gewet</rhyme>;</l>
        <l id="s12:l5" met="3" type="-+">Gy hebt den pyl <rhyme label="c" type="m">gericht</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Recht op hun <rhyme label="c" type="m" corresp="#s12:l5">aangezicht</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Verhoog u, Heer, in uwe <rhyme label="a" type="m">magt</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">En goddelyke <rhyme label="b" type="f">sterkte</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Die 's Konings heil <rhyme label="b" type="f" corresp="#s13:l2">uitwerkte</rhyme>:</l>
        <l id="s13:l4" met="4" type="-+">Dan wordt u van ons <rhyme label="a" type="m" corresp="#s13:l1">toegebragt</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Met psalmen en <rhyme label="c" type="m">gezang</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Eere al ons leven <rhyme label="c" type="m" corresp="#s13:l5">lang</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
