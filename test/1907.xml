    <melody>Iets moet ik u Laura vragen</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WOnderbare Echtens <rhyme label="a" type="f">stricken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Door des hooghe Hemels <rhyme label="a" type="f" corresp="#s1:l1">schicken</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Vaeck op’t onversienst <rhyme label="b" type="m">gheschien</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Liefde vlecht de Trouwens <rhyme label="c" type="f">banden</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Eendracht cierdt de gulden <rhyme label="c" type="f" corresp="#s1:l4">randen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Soo’t van Gode is <rhyme label="b" type="m" corresp="#s1:l3">versien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Godt gun dees Ghehuvvde <rhyme label="a" type="f">seeghen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Nu hen vvensch is lief <rhyme label="a" type="f" corresp="#s2:l1">verkreghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Bruydegom en Bruydt <rhyme label="b" type="m">eersaem</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dat sy tvveedracht gantsch <rhyme label="c" type="f">vernielen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En hun herten en hun <rhyme label="c" type="f" corresp="#s2:l4">zielen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Sluyten beyd’ in een <rhyme label="b" type="m" corresp="#s2:l3">lichaem</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Tracht hier na door Godes <rhyme label="a" type="f">vreesen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Parend’ paer soo sult ghy <rhyme label="a" type="f" corresp="#s3:l1">vvesen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Vreedich in u eyghen <rhyme label="b" type="m">rust</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Vvve vreuchd’ en teghen <rhyme label="c" type="f">strijden</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Matight voort tot allen <rhyme label="c" type="f" corresp="#s3:l4">tijden</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Om ghenieten vvare <rhyme label="b" type="m" corresp="#s3:l3">lust</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als ghy teeld’ u Echtens <rhyme label="a" type="f">vruchten</rhyme></l>
        <l id="s4:l2" met="4" type="+-">VViltse met vermaen op <rhyme label="a" type="f" corresp="#s4:l1">tuchten</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En hun buyghen na Gods <rhyme label="b" type="m">VVet</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat als ghy door ’s Herrefst <rhyme label="c" type="f">vlaghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Langher niet kunt vruchten <rhyme label="c" type="f" corresp="#s4:l4">draghen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Heure gunst dan hebt te <rhyme label="b" type="m" corresp="#s4:l3">bet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
