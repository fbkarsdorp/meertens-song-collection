    <melody>La grédeline</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAg Teuntie Teeuvvis <rhyme label="a" type="f">Kaértie</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe gaéie soo <rhyme label="b" type="m">allien</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Of wachie, na je <rhyme label="a" type="f" corresp="#s1:l1">Vaértie</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Neen die is quaét te <rhyme label="b" type="m" corresp="#s1:l2">bien</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Seper kom dan op men <rhyme label="c" type="f">Waégen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Seper ey, ick hou al <rhyme label="d" type="m">staén</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">'t Is te <rhyme label="e" type="m">beslickt</rhyme></l>
        <l id="s1:l8" met="2" type="-+">'k Heb plaéts <rhyme label="e" type="m" corresp="#s1:l7">eschickt</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Jou Maa is al te swaer te <rhyme label="c" type="f" corresp="#s1:l5">draégen</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Hoe me uchie doch soo lang <rhyme label="d" type="m" corresp="#s1:l6">beraén</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Neen Aris Corse 'k <rhyme label="a" type="f">danckie</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">'k Ben nou niet vart van <rhyme label="b" type="m">Steé</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ey op dit leege <rhyme label="a" type="f" corresp="#s2:l1">banckie</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Kennie nou moyties <rhyme label="b" type="m" corresp="#s2:l2">meé</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Ick selie soo voorsichtig <rhyme label="c" type="f">rije</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Mijn Paérden loopen op ien <rhyme label="d" type="m">haér</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Isset wel <rhyme label="e" type="m">slach</rhyme>?</l>
        <l id="s2:l8" met="2" type="-+">Oft yement <rhyme label="e" type="m" corresp="#s2:l7">sagh</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Se souwen mene datwe <rhyme label="c" type="f" corresp="#s2:l5">Vrije</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Wel Meijsie offet dan soor <rhyme label="d" type="m" corresp="#s2:l6">waér</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Heer Aris rijt wat <rhyme label="a" type="f">steegies</rhyme></l>
        <l id="s3:l2" met="3" type="-+">'k Heb Eijers in men <rhyme label="b" type="m">schoot</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Ey Teuntie 't zien maér <rhyme label="a" type="f" corresp="#s3:l1">blaéchies</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Seker je hebt gien <rhyme label="b" type="m" corresp="#s3:l2">noot</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Gans lyden 'k worrder dol en geck <rhyme label="c" type="f">op</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Als ick een Heultie <rhyme label="d" type="m">Oogh</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">'k Soen dat ick <rhyme label="e" type="m">lil</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Nou Aris <rhyme label="e" type="m" corresp="#s3:l7">stil</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Me dunckt ie hebter dapper beck <rhyme label="c" type="f" corresp="#s3:l5">op</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Wel knecht ie sopt je Lever <rhyme label="d" type="m" corresp="#s3:l6">drooch</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
