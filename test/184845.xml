<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184845</id>
    <date>
      <year-start>1639</year-start>
      <year-end>1639</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Baise-moi ma Jeanneton</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs ick denck aen u O <rhyme label="a" type="m">Ieught</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Soo moet ick u <rhyme label="b" type="f">smeecken</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ieught verlaet u yd'le <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme></l>
        <l id="s1:l4" met="3" type="+-">En des Werelts <rhyme label="b" type="f" corresp="#s1:l2">treecken</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'t Werelts wesen haest <rhyme label="c" type="m">vervlieght</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat soo menigh mensch <rhyme label="c" type="m" corresp="#s1:l5">bedrieght</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Die met hert en <rhyme label="d" type="f">sinnen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Haer te seer <rhyme label="d" type="f" corresp="#s1:l7">beminnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Soo ghy d'Heere Christo <rhyme label="a" type="m">dient</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Dient hem uyt u <rhyme label="b" type="f">krachten</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wordt doch niet des Werelts <rhyme label="a" type="m" corresp="#s2:l1">vrient</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Maer wiltse <rhyme label="b" type="f" corresp="#s2:l2">verachten</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Draeght altijdt ghedurigh <rhyme label="c" type="m">haet</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Teghens die u ziele <rhyme label="c" type="m" corresp="#s2:l5">schaet</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">En soeckt te <rhyme label="d" type="f">beroven</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Van den Schat daer <rhyme label="d" type="f" corresp="#s2:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want den Vyandt stelt ons <rhyme label="a" type="m">voor</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Rijckdom, lustigh <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wilt ghy hem geven <rhyme label="a" type="m" corresp="#s3:l1">ghehoor</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Soo ghenaeckt u <rhyme label="b" type="f" corresp="#s3:l2">beven</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Gheeft hem nimmermeer <rhyme label="c" type="m">consent</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Wat hy u oock vooren <rhyme label="c" type="m" corresp="#s3:l5">went</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Maer wilt hem <rhyme label="d" type="f">af-keeren</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Met het Woort des <rhyme label="d" type="f" corresp="#s3:l7">Heeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'Tis bedroch en <rhyme label="a" type="m">ydelheyt</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Wat hy gaet <rhyme label="b" type="f">beschicken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Aen veel kanten zijn <rhyme label="a" type="m" corresp="#s4:l1">gheleydt</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Van zijn loose <rhyme label="b" type="f" corresp="#s4:l2">stricken</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Wie niet neerstigh voor hem <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Raeckt soo haest in het <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Van des Vyandts <rhyme label="d" type="f">netten</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Die hy ons gaet <rhyme label="d" type="f" corresp="#s4:l7">setten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hy doet veel beloften <rhyme label="a" type="m">hoogh</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Om u te <rhyme label="b" type="f">bekoren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Anders niet dan schoon in 't <rhyme label="a" type="m" corresp="#s5:l1">oogh</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">'t Beste gaet <rhyme label="b" type="f" corresp="#s5:l2">verloren</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Dat is 't Eeuwigh blijvend' <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dat de Heer wil gheven <rhyme label="c" type="m" corresp="#s5:l5">vroet</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Die hier al haer <rhyme label="d" type="f">leden</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Tot sijn dienst <rhyme label="d" type="f" corresp="#s5:l7">besteden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wilt dan oock o Ionghe <rhyme label="a" type="m">Ieught</rhyme></l>
        <l id="s6:l2" met="3" type="+-">V tot deughden <rhyme label="b" type="f">keeren</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Kiest voor ydelijck <rhyme label="a" type="m" corresp="#s6:l1">gheneught</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Het Ghebodt des <rhyme label="b" type="f" corresp="#s6:l2">Heeren</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Wilt beleven sijnen <rhyme label="c" type="m">wil</rhyme></l>
        <l id="s6:l6" met="4" type="+-">In een kuysch, reyn leven <rhyme label="c" type="m" corresp="#s6:l5">stil</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">In u jonghe <rhyme label="d" type="f">daghen</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Wilt u Cruyce <rhyme label="d" type="f" corresp="#s6:l7">draghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Cruyst u lusten, cruyst u <rhyme label="a" type="m">vleysch</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Doodt u aerdtsche <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Gheeft het vleesch niet sijnen <rhyme label="a" type="m" corresp="#s7:l1">eysch</rhyme></l>
        <l id="s7:l4" met="3" type="+-">In <rhyme label="b" type="f" corresp="#s7:l2">wellustigheden</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Oock niet nae 's vleeschs kloeckheydt <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Dat ghy sijne lusten <rhyme label="c" type="m" corresp="#s7:l5">boet</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">V soo te <rhyme label="d" type="f">vermaken</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Sout ghy Godt <rhyme label="d" type="f" corresp="#s7:l7">versaken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Want den Steen der <rhyme label="a" type="m">Saligheydt</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Soud'men dan kleyn <rhyme label="b" type="f">achten</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Daer de Vrome nae <rhyme label="a" type="m" corresp="#s8:l1">verbeydt</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Soud'men soo niet <rhyme label="b" type="f" corresp="#s8:l2">wachten</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Want als een Dief inder <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En als men daer minst op <rhyme label="c" type="m" corresp="#s8:l5">acht</rhyme>.</l>
        <l id="s8:l7" met="3" type="+-">Sal de Heere <rhyme label="d" type="f">komen</rhyme></l>
        <l id="s8:l8" met="3" type="+-">Tot blijdtschap der <rhyme label="d" type="f" corresp="#s8:l7">Vromen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Elck de Heer dan gheven <rhyme label="a" type="m">sal</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Nae sijn werck sal <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Die bewandelt den Wegh <rhyme label="a" type="m" corresp="#s9:l1">smal</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Wordt van hem <rhyme label="b" type="f" corresp="#s9:l2">ghepresen</rhyme>:</l>
        <l id="s9:l5" met="4" type="+-">Die met ghedooghsaemheydt <rhyme label="c" type="m">saen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Nae dat eeuwigh leven <rhyme label="c" type="m" corresp="#s9:l5">staen</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">Naem'lijck prijs en <rhyme label="d" type="f">eere</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">Krijght hy van den <rhyme label="d" type="f" corresp="#s9:l7">Heere</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">O! wat een groot <rhyme label="a" type="m">onderscheydt</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Salmen sien en <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Tusschen die wel is <rhyme label="a" type="m" corresp="#s10:l1">bereydt</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">En 't best heeft <rhyme label="b" type="f" corresp="#s10:l2">verkoren</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">En die onnutt'lijck werdt <rhyme label="c" type="m">quijt</rhyme></l>
        <l id="s10:l6" met="4" type="+-">De rechte ghenaden <rhyme label="c" type="m" corresp="#s10:l5">tijdt</rhyme>,</l>
        <l id="s10:l7" met="3" type="+-">Die hem gaf den <rhyme label="d" type="f">Heere</rhyme></l>
        <l id="s10:l8" met="3" type="+-">Tot zijnen <rhyme label="d" type="f" corresp="#s10:l7">bekeere</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Schrick'lijck hem Godt wreken <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Over sijn <rhyme label="b" type="f">Vyanden</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">En sal haer dan werpen <rhyme label="a" type="m" corresp="#s11:l1">al</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Om Eeuwigh te <rhyme label="b" type="f" corresp="#s11:l2">branden</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">In het on-uyt-bluschlijck <rhyme label="c" type="m">Vyer</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">En het grouwelijck <rhyme label="c" type="m" corresp="#s11:l5">ghetier</rhyme>,</l>
        <l id="s11:l7" met="3" type="+-">Met wringhen der <rhyme label="d" type="f">handen</rhyme>,</l>
        <l id="s11:l8" met="3" type="+-">Oogh-weenen, <rhyme label="d" type="f" corresp="#s11:l7">kners-tanden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Een Prinslijcke blyde <rhyme label="a" type="m">Dagh</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Is bereydt den <rhyme label="b" type="f">Vromen</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Een vreught die noyt oogh en <rhyme label="a" type="m" corresp="#s12:l1">sagh</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">Noch oyt is <rhyme label="b" type="f" corresp="#s12:l2">ghekomen</rhyme></l>
        <l id="s12:l5" met="4" type="+-">In des Menschen herte <rhyme label="c" type="m">hier</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Al werdt dan als't Gout in 't <rhyme label="c" type="m" corresp="#s12:l5">Vyer</rhyme></l>
        <l id="s12:l7" met="3" type="+-">Beproeft u <rhyme label="d" type="f">Gheloove</rhyme></l>
        <l id="s12:l8" met="3" type="+-">Laet dien schat niet <rhyme label="d" type="f" corresp="#s12:l7">roove</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
