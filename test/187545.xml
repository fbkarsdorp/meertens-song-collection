<song>
  <meta>
    <id>187545</id>
    <date>
      <year-start>1693</year-start>
      <year-end>1693</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik ging nog hedenmorgen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAeslandt-sluys groot van <rhyme label="a" type="f">waerden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gelegen in <rhyme label="b" type="m">Hollandt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Noyt uws gelijck op <rhyme label="a" type="f" corresp="#s1:l1">Aerden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ick in Europa <rhyme label="b" type="m" corresp="#s1:l2">vant</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Van Visschery in <rhyme label="c" type="m">overvloet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Gelijck ick lest <rhyme label="d" type="f">aenschouwde</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">’t Welck my nu singen <rhyme label="c" type="m" corresp="#s1:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Voor eerst veel hondert <rhyme label="a" type="f">zielen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Soo t’samen groot en <rhyme label="b" type="m">kleen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die door malkander <rhyme label="a" type="f" corresp="#s2:l1">krielen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Rijck,arm en oock <rhyme label="b" type="m" corresp="#s2:l2">gemeen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Daer onder menigh nobel <rhyme label="c" type="m">geest</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wanneer sy t’zeewaert <rhyme label="d" type="f">wenden</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Het schijnt sy gaen ter <rhyme label="c" type="m" corresp="#s2:l5">feest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Maes sy uyt <rhyme label="a" type="f">braveren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Het schijnt een heele <rhyme label="b" type="m">vloot</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Komt na de Noort-zee <rhyme label="a" type="f" corresp="#s3:l1">keeren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Van Hoeckers meenigh <rhyme label="b" type="m" corresp="#s3:l2">Boot</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hondert vijftigh omtrent <rhyme label="c" type="m">getelt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die met haer houte <rhyme label="d" type="f">kielen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Door-ploegen ’t <rhyme label="c" type="m" corresp="#s3:l5">Golve-velt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Over Nepthunis <rhyme label="a" type="f">baren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Varen sy wel <rhyme label="b" type="m">gemoet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En uyt de Zee <rhyme label="a" type="f" corresp="#s4:l1">vergaren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En al haer schat en <rhyme label="b" type="m" corresp="#s4:l2">goet</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Wanneer den Heer sijn zegen <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dan singht den vromen <rhyme label="d" type="f">Visscher</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">’t Schijnt daer geen blijder <rhyme label="c" type="m" corresp="#s4:l5">leeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Te Land sy weder <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met Schelvis, <rhyme label="b" type="m">Cabeljauw</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Oock Haringh <rhyme label="a" type="f" corresp="#s5:l1">uytgenomen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">d’Vent-jagers komen <rhyme label="b" type="m" corresp="#s5:l2">gaen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Van Leyden,en noch Steden <rhyme label="c" type="m">meer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">De Vissers waer af <rhyme label="d" type="f">halen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Brengen ’t dan heen en <rhyme label="c" type="m" corresp="#s5:l5">veer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O Sluysenaers <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Door hulp van Godes <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo spijst gy in dit <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Verscheyde <rhyme label="b" type="m" corresp="#s6:l2">Steden,want</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Waer datmen komt in Hollandt <rhyme label="c" type="m">wis</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">In Vranckrijck of in <rhyme label="d" type="f">Spanjen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Vint men noch Sluysse <rhyme label="c" type="m" corresp="#s6:l5">Vis</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wie sou het al <rhyme label="a" type="f">verhalen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Den Neeringh die hier <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Nu oock te recht <rhyme label="a" type="f" corresp="#s7:l1">af-malen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dus swijgh ick eer ick <rhyme label="b" type="m" corresp="#s7:l2">mis</rhyme>;</l>
        <l id="s7:l5" met="4" type="-+">Schiedam,Vlaerdingh,bidt <rhyme label="c" type="m">allegaer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Dat Godt u voortaen <rhyme label="d" type="f">zegent</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Gelijck den <rhyme label="c" type="m" corresp="#s7:l5">Sluysenaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hoe hoort men alle <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ter Veer en <rhyme label="b" type="m">Zirrick-zee</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">De Visschers droevigh <rhyme label="a" type="f" corresp="#s8:l1">klagen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Op and’re plaetsen <rhyme label="b" type="m" corresp="#s8:l2">mee</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En hier op Sluys is ’t na <rhyme label="c" type="m">begeer</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Daer voor moet yder <rhyme label="d" type="f">dancken</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Alleen den <rhyme label="c" type="m" corresp="#s8:l5">Opper-Heer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Waer vint men uws <rhyme label="a" type="f">gelijcken</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Want Wijck ende <rhyme label="b" type="m">Zantvoort</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Noortwijck moet voor u <rhyme label="a" type="f" corresp="#s9:l1">wijcken</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En Katwijck soo ’t <rhyme label="b" type="m" corresp="#s9:l2">behoort</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Met Schevelingh,en oock de <rhyme label="c" type="m">Hey</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Sluys spant deKroone van <rhyme label="d" type="f">alle</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Rontom aen dees <rhyme label="c" type="m" corresp="#s9:l5">Contrey</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Godts Woort siet men hier <rhyme label="a" type="f">bloeyen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Als in een <rhyme label="b" type="m">Lust-prieel</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En oock in deught <rhyme label="a" type="f" corresp="#s10:l1">op-groeyen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">De jonge Jeught seer <rhyme label="b" type="m" corresp="#s10:l2">veel</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Een Hemels Lust-hof en <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">De Luyden veel hier <rhyme label="d" type="f">woonen</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Drie dubbelt by <rhyme label="c" type="m" corresp="#s10:l5">malkaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De Kruys-kerck hoogh <rhyme label="a" type="f">vermeten</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Heel cier’lijck wel <rhyme label="b" type="m">gemaeckt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">En dient oock niet <rhyme label="a" type="f" corresp="#s11:l1">vergeten</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Waer in dat doorgaens <rhyme label="b" type="m" corresp="#s11:l2">blaeckt</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Des Heeren Woort,en oock sijn <rhyme label="c" type="m">Wet</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Waer op den Visscher <rhyme label="d" type="f">vreugdigh</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Al sijn vertrouwen <rhyme label="c" type="m" corresp="#s11:l5">set</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Oorlof Sluysenaers <rhyme label="a" type="f">moedigh</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ey neemt dit doch in <rhyme label="b" type="m">danck</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Want door een yver <rhyme label="a" type="f" corresp="#s12:l1">spoedigh</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Maeckten ick dit <rhyme label="b" type="m" corresp="#s12:l2">gesanck</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Na Godes wil weest staegh <rhyme label="c" type="m">bereyt</rhyme>:</l>
        <l id="s12:l6" met="3" type="-+">En leeft in rust,brenght <rhyme label="d" type="f">vrede</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">In alle <rhyme label="c" type="m" corresp="#s12:l5">eeuwigheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
