<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156076</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Jonkvrouw ik u vragen laat</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALs Fobert had sijn tijdt <rhyme label="a" type="m">vergist</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tot op den Ouden <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Oock al sijn jonge jeught <rhyme label="a" type="m" corresp="#s1:l1">verquist</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En nu niet beter <rhyme label="b" type="m" corresp="#s1:l2">mach</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sprack met berou, Had ick een <rhyme label="c" type="m">Vrouw</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat was de rechte <rhyme label="b" type="m" corresp="#s1:l2">slach</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nu ben ick al te Oudt <rhyme label="a" type="m">bedaeght</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En weet voor my geen <rhyme label="b" type="m">raet</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Vraegh ick 't een jonge brave <rhyme label="a" type="m" corresp="#s2:l1">Maeght</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die in mijn Liefde <rhyme label="b" type="m" corresp="#s2:l2">staet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ick vrees dat sy Niet eens na <rhyme label="c" type="m">my</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sal hooren wat ick <rhyme label="b" type="m" corresp="#s2:l2">praet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wel waerom staen ick dus <rhyme label="a" type="m">verheert</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Mijn Gelt, mijn Schat, mijn <rhyme label="b" type="m">Goud</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat sal my maecken liefde <rhyme label="a" type="m" corresp="#s3:l1">weert</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al ben ick loom en <rhyme label="b" type="m" corresp="#s3:l2">koud</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Want yeder een Soeckt dat <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En menigh hier op <rhyme label="b" type="m" corresp="#s3:l2">bouwt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick gae met dese Sack vol <rhyme label="a" type="m">gelt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">nae Teet, dit is mijn <rhyme label="b" type="m">Sin</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En segh dat sy dees Schijven <rhyme label="a" type="m" corresp="#s4:l1">telt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Sy weet wie dat ick <rhyme label="b" type="m" corresp="#s4:l2">bin</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Soo sy het doet, Dan krijgh ick <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En maeck een goedt <rhyme label="b" type="m" corresp="#s4:l2">begin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Houw Teetje, 'k wensch u goeden <rhyme label="a" type="m">dagh</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dus kloeck, is daer geen <rhyme label="b" type="m">rust</rhyme>?</l>
        <l id="s5:l3" met="4" type="-+">Hoor Fobert, die niet anders <rhyme label="a" type="m" corresp="#s5:l1">magh</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Windt soo sijn Broodt met <rhyme label="b" type="m" corresp="#s5:l2">lust</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En sonder leet, Al is 't met <rhyme label="c" type="m">sweet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Het loon de leden <rhyme label="b" type="m" corresp="#s5:l2">sust</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy kond wel beter, waer 't u <rhyme label="a" type="m">wil</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ick wist daer toe wel <rhyme label="b" type="m">raet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Om met gemack te leven <rhyme label="a" type="m" corresp="#s6:l1">stil</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Of proncken by de <rhyme label="b" type="m" corresp="#s6:l2">straet</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Versien met goedt, In <rhyme label="c" type="m">overvloed</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Een huys gepropt van <rhyme label="b" type="m" corresp="#s6:l2">staet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maer Fobert, seyt sy, dat was <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hoe raeck ik best daer <rhyme label="b" type="m">an</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Als ghy nu mijn begeeren <rhyme label="a" type="m" corresp="#s7:l1">doet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En neemt my tot u <rhyme label="b" type="m" corresp="#s7:l2">Man</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Dan is mijn Gelt In u <rhyme label="c" type="m">gewelt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ick scheyd terstondt daer <rhyme label="b" type="m" corresp="#s7:l2">van</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Al wat mijn Huys en Landt <rhyme label="a" type="m">bezit</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat maeck ick u goedt <rhyme label="b" type="m">rond</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Sie daer een Sack, besiet eens <rhyme label="a" type="m" corresp="#s8:l1">dit</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die geef ick u <rhyme label="b" type="m" corresp="#s8:l2">terstond</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En voort al 't geen Is u <rhyme label="c" type="m">gemeen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En eygen tot de <rhyme label="b" type="m" corresp="#s8:l2">grond</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat is een saeck om aen te <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Seyt sy, waert ghy noch <rhyme label="b" type="m">fris</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Ick woud' met u de reys <rhyme label="a" type="m" corresp="#s9:l1">bestaen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Maer nu soo is het <rhyme label="b" type="m" corresp="#s9:l2">mis</rhyme>;</l>
        <l id="s9:l5" met="4" type="-+">Want soo een Vrient My niet en <rhyme label="c" type="m">dient</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Soeck een voor u daer s' <rhyme label="b" type="m" corresp="#s9:l2">is</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Al waerje, Fobert, noch soo <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ick wil u nimmer <rhyme label="b" type="m">niet</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">Het goedt acht ick maer stof en <rhyme label="a" type="m" corresp="#s10:l1">slijck</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Vol moeyten en <rhyme label="b" type="m" corresp="#s10:l2">verdriet</rhyme>:</l>
        <l id="s10:l5" met="4" type="-+">Want een qua Trouw Baert vaeck <rhyme label="c" type="m">berouw</rhyme>;</l>
        <l id="s10:l6" met="3" type="-+">Als men aen Treusel <rhyme label="b" type="m" corresp="#s10:l2">siet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ick ben een Iongh en aerdigh <rhyme label="a" type="m">Dier</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">My dient geen oude <rhyme label="b" type="m">Grijs</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Maer Suyer is al mijn <rhyme label="a" type="m" corresp="#s11:l1">playsier</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Die is nog Iongh en <rhyme label="b" type="m" corresp="#s11:l2">wijs</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Daer reys ick mee In Dorp en <rhyme label="c" type="m">Stee</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En 's Winters op het <rhyme label="b" type="m" corresp="#s11:l2">Ys</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Hoor Vrysters, hoogh of laegh van <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Sie voorje watje <rhyme label="b" type="m">doet</rhyme>;</l>
        <l id="s12:l3" met="4" type="-+">Het oogh op uw's gelijcken <rhyme label="a" type="m" corresp="#s12:l1">slaet</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Dat is het beste <rhyme label="b" type="m" corresp="#s12:l2">goedt</rhyme>:</l>
        <l id="s12:l5" met="4" type="-+">Mijn naem is Teet, Daerom soo <rhyme label="c" type="m">weet</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Ick Suyer hebben <rhyme label="b" type="m" corresp="#s12:l2">moet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
