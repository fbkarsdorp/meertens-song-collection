  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">REaeltje, die door <rhyme label="a" type="m">d'Echte-bandt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Tot Bedt-strijdt zijt <rhyme label="b" type="f">verbonden</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Beeft hier voor de <rhyme label="b" type="f" corresp="#s1:l2">wonden</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als een morgen <rhyme label="a" type="m" corresp="#s1:l1">landt</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Zijt ghy <rhyme label="c" type="f">onbedreven</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">In dit vreedts <rhyme label="d" type="m">gevecht</rhyme>?</l>
        <l id="s1:l7" met="3" type="-+">Mooght ghy tot morgen <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">'k Wedt ghy anders <rhyme label="d" type="m" corresp="#s1:l6">seght</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Als beter <rhyme label="d" type="m" corresp="#s1:l6">onderrecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
