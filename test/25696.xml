    <melody>O Venus der minne godin</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Recht doch lief Heere nu mijn <rhyme label="a" type="f">ganghen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En laet my u ghenaed’ <rhyme label="a" type="f" corresp="#s1:l1">ontfanghen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Mijn jeucht, in deucht, nu door te <rhyme label="b" type="f">bringhen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Voor alle <rhyme label="b" type="f" corresp="#s1:l3">dinghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ingheplant zijnde om te <rhyme label="a" type="f">draghen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Als een spruytken nae u <rhyme label="a" type="f" corresp="#s2:l1">behaghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Vrucht soet // met spoet, soo is mijn <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s2:l4" met="2" type="-+">V nu <rhyme label="b" type="f" corresp="#s2:l3">begheven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In u laet my ghewortelt <rhyme label="a" type="f">blijven</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">D’oprecht voornemen wilt <rhyme label="a" type="f" corresp="#s3:l1">verstijven</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Heel fijn // in mijn, herte deur <rhyme label="b" type="f">claerheyt</rhyme></l>
        <l id="s3:l4" met="2" type="-+">In uwe <rhyme label="b" type="f" corresp="#s3:l3">waerheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Comt te helpen met uwen <rhyme label="a" type="f">crachten</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ghy zijt een rechter der <rhyme label="a" type="f" corresp="#s4:l1">ghedachten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En kent // present, het hert’ van <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">En ons <rhyme label="b" type="f" corresp="#s4:l3">versinnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Kent mijn droogheyt met uwen <rhyme label="a" type="f">rade</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Maeckt my vochtich door u <rhyme label="a" type="f" corresp="#s5:l1">ghenade</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Soo can // ick dan, wassen, <rhyme label="b" type="f">vermeeren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Tot uwer <rhyme label="b" type="f" corresp="#s5:l3">eeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Qualijck can ick Heer om <rhyme label="a" type="f">verheughen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Het volbringhen deur mijn <rhyme label="a" type="f" corresp="#s6:l1">vermeughen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Mijn ziel // ghehiel, dorst nae u <rhyme label="b" type="f">goedich</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Alst droogh landt <rhyme label="b" type="f" corresp="#s6:l3">spoedich</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Vrucht te brenghen in al mijn <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Sendt my u hulpe hier <rhyme label="a" type="f" corresp="#s7:l1">beneden</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En voort // soo’t hoort, in goede <rhyme label="b" type="f">wercken</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Wilt my <rhyme label="b" type="f" corresp="#s7:l3">verstercken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">En dat mijn hert u mach <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Den mondt u loven en <rhyme label="a" type="f" corresp="#s8:l1">beginnen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Met daet // en raet, drincken en <rhyme label="b" type="f">spijsen</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Liefde <rhyme label="b" type="f" corresp="#s8:l3">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Iae dat ick mach met mijnen <rhyme label="a" type="f">ooren</rhyme></l>
        <l id="s9:l2" met="4" type="-+">V Goddelijcke waerheyt <rhyme label="a" type="f" corresp="#s9:l1">hooren</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Wat vry // in my, is Heer hier <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Dat moet u <rhyme label="b" type="f" corresp="#s9:l3">loven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Alsoo vrucht brenghende int <rhyme label="a" type="f">pooghen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Laet mijn hert neder zijn <rhyme label="a" type="f" corresp="#s10:l1">ghebooghen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Verniert // gheciert, nu <rhyme label="b" type="f">overvloedich</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Gheheel <rhyme label="b" type="f" corresp="#s10:l3">ootmoedich</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Naer dat wy doen sonder <rhyme label="a" type="f">beswaren</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Al dat wy te doen schuldich <rhyme label="a" type="f" corresp="#s11:l1">waren</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Zijn wy // noch by, onse <rhyme label="b" type="f">uytrechten</rhyme></l>
        <l id="s11:l4" met="2" type="-+">Onnutte <rhyme label="b" type="f" corresp="#s11:l3">knechten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Soet-rieckende laet my doch <rhyme label="a" type="f">groeyen</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Den goeden wil als bloemkens <rhyme label="a" type="f" corresp="#s12:l1">bloeyen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Op dat // soo wat, ghy wilt, O <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Ick ook <rhyme label="b" type="f" corresp="#s12:l3">begeere</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Dat ick lief Heer in uwer <rhyme label="a" type="f">vresen</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Een soeten reuck Christi mach <rhyme label="a" type="f" corresp="#s13:l1">wesen</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Die saen // nu gaen, naemaels <rhyme label="b" type="f">verloren</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">En den <rhyme label="b" type="f" corresp="#s13:l3">vercoren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Och mocht ick bloeyen als een <rhyme label="a" type="f">roose</rhyme></l>
        <l id="s14:l2" met="4" type="-+">In mijnen tijdt die corte <rhyme label="a" type="f" corresp="#s14:l1">poose</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Deuchtsaem // bequaem, in al mijn  <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s14:l4" met="2" type="-+">Nae Christi <rhyme label="b" type="f" corresp="#s14:l3">zeden</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Tot dat ick soete vruchten <rhyme label="a" type="f">draghe</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Wercken der liefden alle <rhyme label="a" type="f" corresp="#s15:l1">daghe</rhyme></l>
        <l id="s15:l3" met="4" type="-+">My sterckt // en werckt, doch in my <rhyme label="b" type="f">crachtich</rhyme>.</l>
        <l id="s15:l4" met="2" type="-+">O Godt <rhyme label="b" type="f" corresp="#s15:l3">almachtich</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">En hoedt my doch voor quade <rhyme label="a" type="f">winden</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Voor coud’ en hitte naer’t <rhyme label="a" type="f" corresp="#s16:l1">bevinden</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Waer deur // mijn fleur, mocht crijgen <rhyme label="b" type="f">hinder</rhyme>,</l>
        <l id="s16:l4" met="2" type="-+">Oft worden <rhyme label="b" type="f" corresp="#s16:l3">minder</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Recht ghy alsoo, lief Heer, mijn <rhyme label="a" type="f">saken</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Soo sult ghy mijn herte <rhyme label="a" type="f" corresp="#s17:l1">vermaken</rhyme></l>
        <l id="s17:l3" met="4" type="-+">Int hof // tot lof, uwer <rhyme label="b" type="f">ghenaden</rhyme></l>
        <l id="s17:l4" met="2" type="-+">Wilt my <rhyme label="b" type="f" corresp="#s17:l3">beraden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
