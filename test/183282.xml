<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183282</id>
    <date>
      <year-start>1752</year-start>
      <year-end>1752</year-end>
    </date>
    <location>
      <orginal-name>gorinchem</orginal-name>
      <standard-name>Gorinchem, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8296133</lon>
        <lat>4.9738743</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mijn vader die mijn smart ziet</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Vader is u teere <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">En reyne <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En liefden <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En op zoo snoo <rhyme label="b" type="m">gedrogt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Bevatten eer de <rhyme label="c" type="m">gront</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Bevatten eer de <rhyme label="c" type="m" corresp="#s1:l5">gront</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Des werelds van u was <rhyme label="d" type="m">gelegt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ia eer den Hemel <rhyme label="c" type="m" corresp="#s1:l5">stont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O vrye liefde zonder <rhyme label="a" type="m">maat</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Die hooger <rhyme label="a" type="m" corresp="#s2:l1">gaat</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">En vaster <rhyme label="a" type="m" corresp="#s2:l1">staat</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dan Zon of <rhyme label="b" type="m">Firmament</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ik zondig Adams <rhyme label="c" type="m">lant</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ik zondig Adams <rhyme label="c" type="m" corresp="#s2:l5">lant</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Zou ik van u o reyne <rhyme label="d" type="m">God</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Met liefde zijn <rhyme label="c" type="m" corresp="#s2:l5">bemint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wat was het dog dat u <rhyme label="a" type="m">bewoog</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Tot <rhyme label="a" type="m" corresp="#s3:l1">mededoog</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">En dat u <rhyme label="a" type="m" corresp="#s3:l1">oog</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Op my juyst neder <rhyme label="b" type="m">viel</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">My zondig snood <rhyme label="c" type="m">rebel</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">My zondig snood <rhyme label="c" type="m" corresp="#s3:l5">rebel</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Die niet als straffe had <rhyme label="d" type="m">verdient</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ia toorn vloek en <rhyme label="c" type="m" corresp="#s3:l5">hel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ik was een eyge duyvels <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Niet waard <rhyme label="a" type="m" corresp="#s4:l1">bemint</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Met niemant <rhyme label="a" type="m" corresp="#s4:l1">vrint</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Als met den helschen <rhyme label="b" type="m">geest</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Men zag in my wel <rhyme label="c" type="m">ras</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Men zag in my wel <rhyme label="c" type="m" corresp="#s4:l5">ras</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Dat ik het beeld des Zatans <rhyme label="d" type="m">droeg</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En hy mijn Vader <rhyme label="c" type="m" corresp="#s4:l5">was</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mijn zin mijn wil mijne ziele <rhyme label="a" type="m">lust</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Was niet <rhyme label="a" type="m" corresp="#s5:l1">geblust</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Nog had geen <rhyme label="a" type="m" corresp="#s5:l1">rust</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dan in mijn vaders <rhyme label="b" type="m">wil</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zoo zondig boos en <rhyme label="c" type="m">snoot</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Zoo zondig boos en <rhyme label="c" type="m" corresp="#s5:l5">snoot</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Was ik o reyn en zalig <rhyme label="d" type="m">God</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Van alle goet <rhyme label="c" type="m" corresp="#s5:l5">ontbloot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maar niet te min wanneer de <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">In <rhyme label="a" type="m" corresp="#s6:l1">d’eeuwigheyd</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Van God <rhyme label="a" type="m" corresp="#s6:l1">bereyd</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Om my te trekken <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Quam hy met reyne <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Quam hy met reyne <rhyme label="c" type="m" corresp="#s6:l5">min</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">My snood vervloekte adams <rhyme label="d" type="m">kindt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Geheelijk nemen <rhyme label="c" type="m" corresp="#s6:l5">in</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Zijn liefde hemel hoog en <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Nam my zoo <rhyme label="a" type="m" corresp="#s7:l1">snood</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">In zonde <rhyme label="a" type="m" corresp="#s7:l1">dood</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Verlooren en <rhyme label="b" type="m">onreijn</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Voor eeuwig tot zijn <rhyme label="c" type="m">kint</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Voor eeuwig tot zijn <rhyme label="c" type="m" corresp="#s7:l5">kint</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">En zedert heeft mijn hert <rhyme label="d" type="m">alleen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Als Vader hem <rhyme label="c" type="m" corresp="#s7:l5">bemint</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O Vader die mijn herte <rhyme label="a" type="m">ziet</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Is ’t niet <rhyme label="a" type="m" corresp="#s8:l1">geschiet</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Hebt gy my <rhyme label="a" type="m" corresp="#s8:l1">niet</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Gegeven uwen <rhyme label="b" type="m">geest</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Die ’t kinderlijk <rhyme label="c" type="m">ontzag</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die ’t kinderlijk <rhyme label="c" type="m" corresp="#s8:l5">ontzag</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">En min tot u in ’t hert <rhyme label="d" type="m">ontstak</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Gelijk een kintje <rhyme label="c" type="m" corresp="#s8:l5">plag</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Vader was mijn hert <rhyme label="a" type="m">bereyd</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Om t’aller <rhyme label="a" type="m" corresp="#s9:l1">tijd</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Met <rhyme label="a" type="m" corresp="#s9:l1">vierigheyd</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">U wil alleen te <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">U wil en niet de <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">U wil en niet de <rhyme label="c" type="m" corresp="#s9:l5">mijn</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Ia laat mijn boos en eyge <rhyme label="d" type="m">wil</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Geheel verbrooken <rhyme label="c" type="m" corresp="#s9:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Regeer my Vader als u <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">In my <rhyme label="a" type="m" corresp="#s10:l1">verslint</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Al wat nog <rhyme label="a" type="m" corresp="#s10:l1">mint</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Het Schepsel zonder <rhyme label="b" type="m">u</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Neemt gy mijn hert <rhyme label="c" type="m">geheel</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Neemt gy mijn hert <rhyme label="c" type="m" corresp="#s10:l5">geheel</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">En stort daar in u liefden <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">O God mijn zalig <rhyme label="c" type="m" corresp="#s10:l5">deel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
