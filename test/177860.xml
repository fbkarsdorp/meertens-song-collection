<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>177860</id>
    <date>
      <year-start>1774</year-start>
      <year-end>1774</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 081 Datheen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Zingt nu blij te <rhyme label="a" type="m">moê</rhyme></l>
        <l id="s1:l2" met="3" type="+-">'t Magtig <rhyme label="b" type="f">Opperwezen</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Eenen lofzang <rhyme label="a" type="m" corresp="#s1:l1">toe</rhyme>:</l>
        <l id="s1:l4" met="3" type="+-">Om ons <rhyme label="c" type="m">heilgenot</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Worde Iakobs <rhyme label="c" type="m" corresp="#s1:l4">God</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Met gejuich <rhyme label="b" type="f" corresp="#s1:l2">gepreezen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Zingt een' psalm, en <rhyme label="a" type="m">geeft</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Trommels aan de <rhyme label="b" type="f">reien</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Wat in Isrel <rhyme label="a" type="m" corresp="#s2:l1">leeft</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Roep' zijn grootheid <rhyme label="c" type="m">uit</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">Harp en zachte <rhyme label="c" type="m" corresp="#s2:l4">luit</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Moet' zijn' roem <rhyme label="b" type="f" corresp="#s2:l2">verbreien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">'t Blij <rhyme label="a" type="m">bazuingeschal</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Klink' in Isrels <rhyme label="b" type="f">ooren</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Doe nu <rhyme label="a" type="m" corresp="#s3:l1">overal</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Deze maar <rhyme label="c" type="m">verstaan</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">“'t Feest der nieuwe <rhyme label="c" type="m" corresp="#s3:l4">maan</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">'t Feest-uur is <rhyme label="b" type="f" corresp="#s3:l2">gebooren</rhyme>.”</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Want dit is 't <rhyme label="a" type="m">bevel</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Van den Heer der <rhyme label="b" type="f">heeren</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Aan zijn <rhyme label="a" type="m" corresp="#s4:l1">Israël</rhyme>;</l>
        <l id="s4:l4" met="3" type="+-">Dit is 't hoog <rhyme label="c" type="m">gebod</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">'t Recht van Iakobs <rhyme label="c" type="m" corresp="#s4:l4">God</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Dat wij billijk <rhyme label="b" type="f" corresp="#s4:l2">eeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Dit doet Iosefs <rhyme label="a" type="m">zaad</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Aan Egypten <rhyme label="b" type="f">denken</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">En in welk een' <rhyme label="a" type="m" corresp="#s5:l1">staat</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Daar 't een spraake <rhyme label="c" type="m">vond</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Die het niet <rhyme label="c" type="m" corresp="#s5:l4">verstond</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">God zijn heil wou <rhyme label="b" type="f" corresp="#s5:l2">schenken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">'k Heb hunn' hals <rhyme label="a" type="m">bevrijd</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Van den last te <rhyme label="b" type="f">draagen</rhyme>;</l>
        <l id="s6:l3" met="3" type="+-">'t Was die blijde <rhyme label="a" type="m" corresp="#s6:l1">tijd</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Toen hun moede <rhyme label="c" type="m">hand</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Werd in 's vijands <rhyme label="c" type="m" corresp="#s6:l4">land</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Van de pot <rhyme label="b" type="f" corresp="#s6:l2">ontslagen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Op uw <rhyme label="a" type="m">noodgeschrei</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Deed ik groote <rhyme label="b" type="f">wondren</rhyme>.</l>
        <l id="s7:l3" met="3" type="+-">Onder mijn <rhyme label="a" type="m" corresp="#s7:l1">gelei</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Vondt gij hulp; mijn <rhyme label="c" type="m">woord</rhyme></l>
        <l id="s7:l5" met="3" type="+-">Werd van u <rhyme label="c" type="m" corresp="#s7:l4">gehoord</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Uit de plaats der <rhyme label="b" type="f" corresp="#s7:l2">dondren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">'k Nam te <rhyme label="a" type="m">Meriba</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Proef van uw <rhyme label="b" type="f">vertrouwen</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Of g' op mijn <rhyme label="a" type="m" corresp="#s8:l1">genaê</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">In uw <rhyme label="c" type="m">tegenheên</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Op mijn' naam <rhyme label="c" type="m" corresp="#s8:l4">alleen</rhyme></l>
        <l id="s8:l6" met="3" type="+-">En mijn woord zoudt <rhyme label="b" type="f" corresp="#s8:l2">bouwen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Hoort mij, zei ik <rhyme label="a" type="m">toen</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Onder u <rhyme label="b" type="f">betuigen</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Wat gij hebt te <rhyme label="a" type="m" corresp="#s9:l1">doen</rhyme>:</l>
        <l id="s9:l4" met="3" type="+-">Och dat <rhyme label="c" type="m">Israël</rhyme></l>
        <l id="s9:l5" met="3" type="+-">Zich, op mijn <rhyme label="c" type="m" corresp="#s9:l4">bevel</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Onder wij wou <rhyme label="b" type="f" corresp="#s9:l2">buigen</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Eert geen' uitlandsch' <rhyme label="a" type="m">god</rhyme>;</l>
        <l id="s10:l2" met="3" type="+-">Wacht u voor uw <rhyme label="b" type="f">zielen</rhyme>;</l>
        <l id="s10:l3" met="3" type="+-">Wilt naar mijn <rhyme label="a" type="m" corresp="#s10:l1">gebod</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Mijnen naam ten <rhyme label="c" type="m">hoon</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Voor geen valsche <rhyme label="c" type="m" corresp="#s10:l4">goôn</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Voor geen vreemde, <rhyme label="b" type="f" corresp="#s10:l2">knielen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Ik, ik ben de <rhyme label="a" type="m">HEER</rhyme>;</l>
        <l id="s11:l2" met="3" type="+-">'k Ben uw God, die <rhyme label="b" type="f">heilig</rhyme></l>
        <l id="s11:l3" met="3" type="+-">Yvre voor mijn <rhyme label="a" type="m" corresp="#s11:l1">eer</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Die u door mijn <rhyme label="c" type="m">hand</rhyme></l>
        <l id="s11:l5" met="3" type="+-">Uit <rhyme label="c" type="m" corresp="#s11:l4">Egyptenland</rhyme></l>
        <l id="s11:l6" met="3" type="+-">Leidde vrij en <rhyme label="b" type="f" corresp="#s11:l2">veilig</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Opent uwen <rhyme label="a" type="m">mond</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Eischt van mij <rhyme label="b" type="f">vrijmoedig</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">Op mijn <rhyme label="a" type="m" corresp="#s12:l1">trouwverbond</rhyme>:</l>
        <l id="s12:l4" met="3" type="+-">Al wat u <rhyme label="c" type="m">ontbreekt</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Schenk ik, zo gij 't <rhyme label="c" type="m" corresp="#s12:l4">smeekt</rhyme>,</l>
        <l id="s12:l6" met="3" type="+-">Mild en <rhyme label="b" type="f" corresp="#s12:l2">overvloedig</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Maar mijn volk wou <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s13:l2" met="3" type="+-">Naar mijn stemme <rhyme label="b" type="f">hooren</rhyme>;</l>
        <l id="s13:l3" met="3" type="+-">Israël <rhyme label="a" type="m" corresp="#s13:l1">verliet</rhyme></l>
        <l id="s13:l4" met="3" type="+-">Mij en mijn <rhyme label="c" type="m">geboôn</rhyme>;</l>
        <l id="s13:l5" met="3" type="+-">'t Heeft zich andre <rhyme label="c" type="m" corresp="#s13:l4">goôn</rhyme>,</l>
        <l id="s13:l6" met="3" type="+-">Naar zijn' lust, <rhyme label="b" type="f" corresp="#s13:l2">verkooren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">'k Liet hen dies, <rhyme label="a" type="m">veracht</rhyme>,</l>
        <l id="s14:l2" met="3" type="+-">Naar 't hun goeddocht, <rhyme label="b" type="f">handlen</rhyme>;</l>
        <l id="s14:l3" met="3" type="+-">'k Liet dit boos <rhyme label="a" type="m" corresp="#s14:l1">geslacht</rhyme>,</l>
        <l id="s14:l4" met="3" type="+-">Naar de keuze <rhyme label="c" type="m">viel</rhyme></l>
        <l id="s14:l5" met="3" type="+-">Van hun dwaaze <rhyme label="c" type="m" corresp="#s14:l4">ziel</rhyme>,</l>
        <l id="s14:l6" met="3" type="+-">In hun wegen <rhyme label="b" type="f" corresp="#s14:l2">wandlen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="+-">Och! had, naar mijn' <rhyme label="a" type="m">raad</rhyme>,</l>
        <l id="s15:l2" met="3" type="+-">Zich mijn volk <rhyme label="b" type="f">gedraagen</rhyme>!</l>
        <l id="s15:l3" met="3" type="+-">Och! had Isrels <rhyme label="a" type="m" corresp="#s15:l1">zaad</rhyme></l>
        <l id="s15:l4" met="3" type="+-">Op mijn effen <rhyme label="c" type="m">paên</rhyme></l>
        <l id="s15:l5" met="3" type="+-">Yvrig willen <rhyme label="c" type="m" corresp="#s15:l4">gaan</rhyme>,</l>
        <l id="s15:l6" met="3" type="+-">Naar mijn <rhyme label="b" type="f" corresp="#s15:l2">welbehagen</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="+-">'k Had hun haaters <rhyme label="a" type="m">ras</rhyme></l>
        <l id="s16:l2" met="3" type="+-">En geheel <rhyme label="b" type="f">verslonden</rhyme>;</l>
        <l id="s16:l3" met="3" type="+-">Wie hun tegen <rhyme label="a" type="m" corresp="#s16:l1">was</rhyme></l>
        <l id="s16:l4" met="3" type="+-">Had, aan alle <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s16:l5" met="3" type="+-">Mijn geduchte <rhyme label="c" type="m" corresp="#s16:l4">hand</rhyme></l>
        <l id="s16:l6" met="3" type="+-">Zeker <rhyme label="b" type="f" corresp="#s16:l2">ondervonden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="+-">Haaters van den <rhyme label="a" type="m">HEER</rhyme></l>
        <l id="s17:l2" met="3" type="+-">Hadden Hem <rhyme label="b" type="f">gegeeven</rhyme>,</l>
        <l id="s17:l3" met="3" type="+-">Schoon geveinsd, zijn <rhyme label="a" type="m" corresp="#s17:l1">eer</rhyme>;</l>
        <l id="s17:l4" met="3" type="+-">Ook zou Isrels <rhyme label="c" type="m">tijd</rhyme>,</l>
        <l id="s17:l5" met="3" type="+-">Van de smert <rhyme label="c" type="m" corresp="#s17:l4">bevrijd</rhyme>,</l>
        <l id="s17:l6" met="3" type="+-">Eeuwig zijn <rhyme label="b" type="f" corresp="#s17:l2">gebleeven</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="+-">'k Had u dan tot <rhyme label="a" type="m">spijz'</rhyme></l>
        <l id="s18:l2" met="3" type="+-">Vette tarw doen <rhyme label="b" type="f">groeien</rhyme>,</l>
        <l id="s18:l3" met="3" type="+-">En u ten <rhyme label="a" type="m" corresp="#s18:l1">bewijz'</rhyme>,</l>
        <l id="s18:l4" met="3" type="+-">Hoe ik u kon <rhyme label="c" type="m">voên</rhyme>,</l>
        <l id="s18:l5" met="3" type="+-">Honingbeeken <rhyme label="c" type="m" corresp="#s18:l4">doen</rhyme></l>
        <l id="s18:l6" met="3" type="+-">Uit de rotsen <rhyme label="b" type="f" corresp="#s18:l2">vloeien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
