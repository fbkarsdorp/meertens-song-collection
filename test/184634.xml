    <date>
      <year-start>1740</year-start>
      <year-end>1740</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Vlucht winter wil ons zoet weer geven</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">TOt uw spyt gy, helsch serpent, moet <rhyme label="a" type="f">vluchten</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Onder u en moet dit kindt niet <rhyme label="a" type="f" corresp="#s1:l1">suchten</rhyme>;</l>
        <l id="s1:l3" met="3" type="+-">Van uw' <rhyme label="b" type="m">slaverny</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Is dese Maghet <rhyme label="b" type="m" corresp="#s1:l3">vry</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Maria is sonder vleck <rhyme label="c" type="f">ontfangen</rhyme>,</l>
        <l id="s1:l6" met="5" type="+-">Sy de Moeder is, die wy <rhyme label="c" type="f" corresp="#s1:l5">verlangen</rhyme>.</l>
        <l id="s1:l7" met="2" type="+-">Ons heeft <rhyme label="d" type="m">al</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Den droeven Adams <rhyme label="d" type="m" corresp="#s1:l7">val</rhyme></l>
        <l id="s1:l9" met="2" type="-+">In uwe <rhyme label="e" type="m">macht</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Vol druck <rhyme label="e" type="m" corresp="#s1:l9">gebracht</rhyme>:</l>
        <l id="s1:l11" met="3" type="+-">Maer Godt reyckt-uyt syn <rhyme label="f" type="f">handen</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">Dat Maria noyt komt in uw' <rhyme label="f" type="f" corresp="#s1:l11">banden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Als voor u moet heel de wereldt <rhyme label="a" type="f">beven</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">Heeft haer Godt genaed' alleen <rhyme label="a" type="f" corresp="#s2:l1">gegeven</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Weest gy vry van <rhyme label="b" type="m">schult</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gy, Maeght, niet sterven <rhyme label="b" type="m" corresp="#s2:l3">sult</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Draeght op d'aerd' (seght hy haer) mynen <rhyme label="c" type="f">vrede</rhyme>,</l>
        <l id="s2:l6" met="5" type="+-">Dat uw' voet het helsch serpent <rhyme label="c" type="f" corresp="#s2:l5">betrede</rhyme>:</l>
        <l id="s2:l7" met="2" type="+-">Dat noyt <rhyme label="d" type="m">dagh</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Den vyandt seggen <rhyme label="d" type="m" corresp="#s2:l7">magh</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Dat gy <rhyme label="e" type="m">slaevin</rhyme></l>
        <l id="s2:l10" met="2" type="-+">In uw' <rhyme label="e" type="m" corresp="#s2:l9">begin</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Van d'hel, hebt moeten <rhyme label="f" type="f">wesen</rhyme>,</l>
        <l id="s2:l12" met="4" type="-+">Die ick had' voor Moeder <rhyme label="f" type="f" corresp="#s2:l11">uyt-gelesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
