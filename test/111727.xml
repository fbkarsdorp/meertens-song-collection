  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Goeden avond, <rhyme label="a" type="f">Jansje</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Alderliefste <rhyme label="b" type="m">meid</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Nu een reys een <rhyme label="a" type="f" corresp="#s1:l1">dansje</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Samen <rhyme label="b" type="m" corresp="#s1:l2">afgeleid</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Kom laat jou <rhyme label="c" type="f">beleezen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">'t Danssen is zoo <rhyme label="d" type="m">zoet</rhyme>.</l>
        <l id="s1:l7" met="2" type="+-">'k Wierd voor <rhyme label="c" type="f" corresp="#s1:l5">deezen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Wel <rhyme label="c" type="f" corresp="#s1:l5">geprezen</rhyme>;</l>
        <l id="s1:l9" met="2" type="+-">'k Ben nog <rhyme label="d" type="m" corresp="#s1:l6">goed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Neen, Govert <rhyme label="a" type="f">Gyzen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Dat durf ik niet <rhyme label="b" type="m">doen</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Besje zou 't <rhyme label="a" type="f" corresp="#s2:l1">mispryzen</rhyme>:</l>
        <l id="s2:l4" met="3" type="+-">'t Is ook geen <rhyme label="b" type="m" corresp="#s2:l2">fatzoen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Gut wat zou zy <rhyme label="c" type="f">baren</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Wiert haar dit <rhyme label="d" type="m">vertelt</rhyme>.</l>
        <l id="s2:l7" met="2" type="+-">'k Heb niet <rhyme label="c" type="f" corresp="#s2:l5">garen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Dat haar <rhyme label="c" type="f" corresp="#s2:l5">snaren</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Zyn <rhyme label="d" type="m" corresp="#s2:l6">ontsteld</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Vrees, toch niet, myn <rhyme label="a" type="f">hartje</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Govert is te <rhyme label="b" type="m">loos</rhyme>.</l>
        <l id="s3:l3" met="3" type="+-">Hy weet meenig <rhyme label="a" type="f" corresp="#s3:l1">partje</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Al was besje <rhyme label="b" type="m" corresp="#s3:l2">boos</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Om haar te <rhyme label="c" type="f">versagten</rhyme>:</l>
        <l id="s3:l6" met="3" type="+-">Kom, myn Guytje, <rhyme label="d" type="m">gaa</rhyme></l>
        <l id="s3:l7" met="2" type="+-">'t Is by <rhyme label="c" type="f" corresp="#s3:l5">achten</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Langer <rhyme label="c" type="f" corresp="#s3:l5">wagten</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Is maar <rhyme label="d" type="m" corresp="#s3:l6">schaa</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Ik zal my <rhyme label="a" type="f">verkleeden</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Na de spaanse <rhyme label="b" type="m">swier</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">En met jou gaan <rhyme label="a" type="f" corresp="#s4:l1">treeden</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Als een Paauw zoo <rhyme label="b" type="m" corresp="#s4:l2">fier</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">'k Zal een Wambuis <rhyme label="c" type="f">dragen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Lubbens op myn <rhyme label="d" type="m">hand</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Van haar <rhyme label="c" type="f" corresp="#s4:l5">dagen</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Nooyt zy <rhyme label="c" type="f" corresp="#s4:l5">zagen</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Zulken <rhyme label="d" type="m" corresp="#s4:l6">trant</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Jy zult met een <rhyme label="a" type="f">kraagje</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Rontom vol met <rhyme label="b" type="m">kant</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Koomen, zoete <rhyme label="a" type="f" corresp="#s5:l1">Maagje</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Als een franse <rhyme label="b" type="m" corresp="#s5:l2">dant</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Ik heb van myn <rhyme label="c" type="f">vaartje</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Noch zo'n hoopen <rhyme label="d" type="m">lint</rhyme>.</l>
        <l id="s5:l7" met="2" type="+-">Een <rhyme label="c" type="f" corresp="#s5:l5">samaartje</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Van peet <rhyme label="c" type="f" corresp="#s5:l5">Klaartje</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Voor jou <rhyme label="d" type="m" corresp="#s5:l6">KINT</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Wie zal ons zoo <rhyme label="a" type="f">kennen</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Is die avond niet <rhyme label="b" type="m">goed</rhyme>?</l>
        <l id="s6:l3" met="3" type="+-">'k Zal twee <rhyme label="a" type="f" corresp="#s6:l1">haane-pennen</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Planten op myn <rhyme label="b" type="m" corresp="#s6:l2">hoed</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">'k Zal een deegen <rhyme label="c" type="f">steeken</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Op myn slinker <rhyme label="d" type="m">zy</rhyme>.</l>
        <l id="s6:l7" met="2" type="+-"><rhyme label="c" type="f" corresp="#s6:l5">Sellerweken</rhyme>!</l>
        <l id="s6:l8" met="2" type="+-">'k Kan niet <rhyme label="c" type="f" corresp="#s6:l5">spreeken</rhyme>:</l>
        <l id="s6:l9" met="2" type="+-">'k Ben zoo <rhyme label="d" type="m" corresp="#s6:l6">bly</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Wel ik zal het <rhyme label="a" type="f">wagen</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Jonge: ik wou maar <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Dat ons menschen <rhyme label="a" type="f" corresp="#s7:l1">zagen</rhyme>:</l>
        <l id="s7:l4" met="3" type="+-">Dan was't in't <rhyme label="b" type="m" corresp="#s7:l2">verdriet</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Wat wil jy <rhyme label="c" type="f">beginnen</rhyme>?</l>
        <l id="s7:l6" met="3" type="+-">Foey! dat is niet <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s7:l7" met="2" type="+-">Zijn jou <rhyme label="c" type="f" corresp="#s7:l5">zinnen</rhyme>,</l>
        <l id="s7:l8" met="2" type="+-">Nog wel <rhyme label="c" type="f" corresp="#s7:l5">binnen</rhyme></l>
        <l id="s7:l9" met="2" type="+-">In jouw <rhyme label="d" type="m" corresp="#s7:l6">hoed</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Vrees niet, poezel <rhyme label="a" type="f">Zusje</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">'t Is van vreugde, <rhyme label="b" type="m">dat</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Ik jou, met een <rhyme label="a" type="f" corresp="#s8:l1">kusje</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Om jou halsje <rhyme label="b" type="m" corresp="#s8:l2">vat</rhyme>?</l>
        <l id="s8:l5" met="3" type="+-">Govert, kryg jy <rhyme label="c" type="f">stuypen</rhyme>?</l>
        <l id="s8:l6" met="3" type="+-">Zachtjes met <rhyme label="d" type="m">gemak</rhyme>.</l>
        <l id="s8:l7" met="2" type="+-">'k Wou wel <rhyme label="c" type="f" corresp="#s8:l5">sluypen</rhyme>,</l>
        <l id="s8:l8" met="2" type="+-">En wel <rhyme label="c" type="f" corresp="#s8:l5">kruypen</rhyme></l>
        <l id="s8:l9" met="2" type="+-">In jouw... <rhyme label="d" type="m" corresp="#s8:l6">zak</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">'k Loof, dat jy <rhyme label="a" type="f">bezeten</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Dezen avond <rhyme label="b" type="m">wort</rhyme>.</l>
        <l id="s9:l3" met="3" type="+-">'k Heb nog noyt <rhyme label="a" type="f" corresp="#s9:l1">geweten</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Dat jou Gekheyt <rhyme label="b" type="m" corresp="#s9:l2">schort</rhyme>.</l>
        <l id="s9:l5" met="3" type="+-">Laet die parten <rhyme label="c" type="f">varen</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Eer ik jouw <rhyme label="d" type="m">verlaet</rhyme>.</l>
        <l id="s9:l7" met="2" type="+-">'k Heb niet <rhyme label="c" type="f" corresp="#s9:l5">garen</rhyme>.</l>
        <l id="s9:l8" met="2" type="+-">Al dat <rhyme label="c" type="f" corresp="#s9:l5">baaren</rhyme>,</l>
        <l id="s9:l9" met="2" type="+-">Op de <rhyme label="d" type="m" corresp="#s9:l6">straat</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Dat ik met mijn <rhyme label="a" type="f">vinger</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Raak jou borsjes <rhyme label="b" type="m">aan</rhyme>;</l>
        <l id="s10:l3" met="3" type="+-">Wat is 'er <rhyme label="a" type="f" corresp="#s10:l1">geringer</rhyme>?</l>
        <l id="s10:l4" met="3" type="+-">Zou jy daarom <rhyme label="b" type="m" corresp="#s10:l2">slaen</rhyme>.</l>
        <l id="s10:l5" met="3" type="+-">Dat ik kus jou <rhyme label="c" type="f">montje</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Is dat <rhyme label="d" type="m">ongemeen</rhyme>?</l>
        <l id="s10:l7" met="2" type="+-">Kom myn <rhyme label="c" type="f" corresp="#s10:l5">hondje</rhyme>.</l>
        <l id="s10:l8" met="2" type="+-">Yder <rhyme label="c" type="f" corresp="#s10:l5">stontje</rhyme></l>
        <l id="s10:l9" met="2" type="+-">Is 'er <rhyme label="d" type="m" corresp="#s10:l6">een</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
