    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EEns ick aensagh des hemels <rhyme label="a" type="m">throon</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Blincken <rhyme label="a" type="m" corresp="#s1:l1">schoon</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Verlicht door de stralen der <rhyme label="b" type="f">Sonnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">De boomkens ende velden <rhyme label="c" type="m">groen</rhyme></l>
        <l id="s1:l5" met="2" type="+-">In't <rhyme label="c" type="m" corresp="#s1:l4">saysoen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">De voghels haer musieck <rhyme label="b" type="f" corresp="#s1:l3">begonnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Naer haer natuer houde sy <rhyme label="a" type="m">choor</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Soet <rhyme label="a" type="m" corresp="#s2:l1">ghehoor</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer wat doen wy arme <rhyme label="b" type="f">sondaren</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Die naer Gods beelde zijn <rhyme label="c" type="m">ghedaen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Blijven <rhyme label="c" type="m" corresp="#s2:l4">staen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Al oft wy alle steenen <rhyme label="b" type="f" corresp="#s2:l3">waren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Oock d'Elementen houden <rhyme label="a" type="m">ganck</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">In <rhyme label="a" type="m" corresp="#s3:l1">bedwanck</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Sy loopen u en't allen <rhyme label="b" type="f">tijden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Hoe comt, daer ons is schoon <rhyme label="c" type="m">verstant</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">In <rhyme label="c" type="m" corresp="#s3:l4">gheplant</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Dat wy de sonden niet en <rhyme label="b" type="f" corresp="#s3:l3">mijden</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">d'Aerd' brenght oock haer vruchten <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Door een <rhyme label="a" type="m" corresp="#s4:l1">woort</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">De wateren climmen en <rhyme label="b" type="f">dalen</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">'T vier en Locht neemt voor <rhyme label="c" type="m">loghement</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">'T <rhyme label="c" type="m" corresp="#s4:l4">Firmament</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Maer wy ind'aerde altijts <rhyme label="b" type="f" corresp="#s4:l3">dralen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Diversche menschen nu ter <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Godt ten <rhyme label="a" type="m" corresp="#s5:l1">spijt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Doen sonden met de <rhyme label="b" type="f">Creaturen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">De wereldt sot volghen sy <rhyme label="c" type="m">naer</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">'T blijckt <rhyme label="c" type="m" corresp="#s5:l4">voorwaer</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Eylaes ten sal niet langhe <rhyme label="b" type="f" corresp="#s5:l3">dueren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Veel dwasen loopen al <rhyme label="a" type="m">verkeert</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s6:l1">Ongheleert</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En soecken hier de breede <rhyme label="b" type="f">paden</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Den enghen wech is haer te <rhyme label="c" type="m">strangh</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">En te <rhyme label="c" type="m" corresp="#s6:l4">langh</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Wellusten en can haer niet <rhyme label="b" type="f" corresp="#s6:l3">versaden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Christus voor onse deure <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Vroech en <rhyme label="a" type="m" corresp="#s7:l1">laet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Hy clopt, hy waerder gheerne <rhyme label="b" type="f">inne</rhyme>;</l>
        <l id="s7:l4" met="4" type="-+">Ontsluyt u herteken met <rhyme label="c" type="m">spoet</rhyme></l>
        <l id="s7:l5" met="2" type="+-">En weest <rhyme label="c" type="m" corresp="#s7:l4">vroet</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ontfanght hem met een soete <rhyme label="b" type="f" corresp="#s7:l3">minne</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Hy roept tot u als tot zijn <rhyme label="a" type="m">bruydt</rhyme></l>
        <l id="s8:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s8:l1">Overluydt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Komt al tot my die sijn <rhyme label="b" type="f">beladen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ick sal u in mijn wonden <rhyme label="c" type="m">root</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Kleyn en <rhyme label="c" type="m" corresp="#s8:l4">groot</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Ontfanghen uyt milde <rhyme label="b" type="f" corresp="#s8:l3">ghenade</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Den Phenix die daer hooghe <rhyme label="a" type="m">was</rhyme></l>
        <l id="s9:l2" met="2" type="+-">Daelde <rhyme label="a" type="m" corresp="#s9:l1">ras</rhyme></l>
        <l id="s9:l3" met="4" type="-+">In bed schoot der suyver <rhyme label="b" type="f">Kersouwe</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Ontsteken met der liefden <rhyme label="c" type="m">brandt</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Heeft hem <rhyme label="c" type="m" corresp="#s9:l4">pandt</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Ghegheven tot onse <rhyme label="b" type="f" corresp="#s9:l3">behouwe</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Als een bermhertich <rhyme label="a" type="m">Pellicaen</rhyme></l>
        <l id="s10:l2" met="2" type="+-">Heeft <rhyme label="a" type="m" corresp="#s10:l1">ontfaen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Voor ons soo veel bloedighe <rhyme label="b" type="f">wonden</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Voldaen uyt liefden menich <rhyme label="c" type="m">fout</rhyme></l>
        <l id="s10:l5" met="2" type="+-">Aen het <rhyme label="c" type="m" corresp="#s10:l4">hout</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Des cruycen, voor alle onse <rhyme label="b" type="f" corresp="#s10:l3">sonden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
