    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Schoonste herderin</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrick Jansz. Prins</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">OCh! wat is het van de <rhyme label="a" type="m">mensch</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Schoon dat hy sijn volle <rhyme label="a" type="m" corresp="#s1:l1">wensch</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Hier gheniet op <rhyme label="b" type="f">aerden</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">'t Is soo ras daer mee <rhyme label="c" type="m">ghedaen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Als hy we'er moet hene <rhyme label="c" type="m" corresp="#s1:l4">gaen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">En de reys <rhyme label="b" type="f" corresp="#s1:l3">aenvaerden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wat is van sijn grooten <rhyme label="a" type="m">roem</rhyme>?</l>
        <l id="s2:l2" met="4" type="+-">Anders niet als van een <rhyme label="a" type="m" corresp="#s2:l1">bloem</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En noch min te <rhyme label="b" type="f">seggen</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Als de Noorden windt eens <rhyme label="c" type="m">blaest</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En daer vinnigh tegen <rhyme label="c" type="m" corresp="#s2:l4">raest</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Moet s' haer <rhyme label="b" type="f" corresp="#s2:l3">neder-leggen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer, wat is des menschen <rhyme label="a" type="m">kindt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Alsser maer een kleyne <rhyme label="a" type="m" corresp="#s3:l1">windt</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Van hem werdt <rhyme label="b" type="f">gedreven</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Alsser maer een schrale <rhyme label="c" type="m">lucht</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Sich gaet geven op de <rhyme label="c" type="m" corresp="#s3:l4">vlucht</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Eyndicht niet sijn <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Daer leyt dan het doode <rhyme label="a" type="m">lijf</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Stom, en doof, en koudt, en <rhyme label="a" type="m" corresp="#s4:l1">stijf</rhyme>;</l>
        <l id="s4:l3" met="3" type="+-">'t Leven is daer <rhyme label="b" type="f">henen</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Alle pracht, en <rhyme label="c" type="m">hoovaerdy</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Alle schoonheyt is <rhyme label="c" type="m" corresp="#s4:l4">voor-by</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">En als roock <rhyme label="b" type="f" corresp="#s4:l3">verdwenen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ia, daer leyt het nietigh <rhyme label="a" type="m">dier</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Soo gantsch yslijck, dat men 't <rhyme label="a" type="m" corresp="#s5:l1">schier</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Niet en durf <rhyme label="b" type="f">aenschouwen</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Daer de Wormen, daer de <rhyme label="c" type="m">Pier</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Onder haer, op haer <rhyme label="c" type="m" corresp="#s5:l4">manier</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Feest mee sullen <rhyme label="b" type="f" corresp="#s5:l3">houwen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Door de stanck, en <rhyme label="a" type="m">vuyligheyt</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Werdt de doode <rhyme label="a" type="m" corresp="#s6:l1">wegh-geleyt</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">En na 't graf <rhyme label="b" type="f">gedraghen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">In een kuyl van seven <rhyme label="c" type="m">voet</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Daer hy voort in rusten <rhyme label="c" type="m" corresp="#s6:l4">moet</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Tot het weer sal <rhyme label="b" type="f" corresp="#s6:l3">dagen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hoort dan hier ghy grootse <rhyme label="a" type="m">lie'n</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Als ghy dit komt aen te <rhyme label="a" type="m" corresp="#s7:l1">sien</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Waer de dooden <rhyme label="b" type="f">leggen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Wilt my dan het <rhyme label="c" type="m">onderscheyt</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Tusschen Vrouwe, tusschen <rhyme label="c" type="m" corresp="#s7:l4">Meyt</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Boer en Adel, <rhyme label="b" type="f" corresp="#s7:l3">seggen</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Scheydt my nu den naeckten <rhyme label="a" type="m">Slaef</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Van den hoogh-gebooren <rhyme label="a" type="m" corresp="#s8:l1">Graef</rhyme></l>
        <l id="s8:l3" met="3" type="+-">En de grootste <rhyme label="b" type="f">Koningh</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Van den meesten <rhyme label="c" type="m">Bedelaer</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Onderscheydtse van me <rhyme label="c" type="m" corresp="#s8:l4">ka'er</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">In haer donckere <rhyme label="b" type="f" corresp="#s8:l3">wooningh</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Ghy die nu gedurigh <rhyme label="a" type="m">lacht</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Ghy zijt oock van dit <rhyme label="a" type="m" corresp="#s9:l1">geslacht</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Dat ghy moet <rhyme label="b" type="f">bederven</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Want dit is een stale <rhyme label="c" type="m">Wet</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Alle menschen is <rhyme label="c" type="m" corresp="#s9:l4">gheset</rhyme></l>
        <l id="s9:l6" met="3" type="+-">Eens te moeten <rhyme label="b" type="f" corresp="#s9:l3">sterven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Laet ons dan voor alle <rhyme label="a" type="m">dingh</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Dese vaste <rhyme label="a" type="m" corresp="#s10:l1">reeckeningh</rhyme></l>
        <l id="s10:l3" met="3" type="+-">Voor ons selven <rhyme label="b" type="f">stellen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Dat het graf ons oock <rhyme label="c" type="m">verwacht</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">En daeromme wel <rhyme label="c" type="m" corresp="#s10:l4">verdacht</rhyme></l>
        <l id="s10:l6" met="3" type="+-">Onse daghen <rhyme label="b" type="f" corresp="#s10:l3">tellen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
