<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188359</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Branle de Metz</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AVec l'aimable <rhyme label="a" type="f">Clorine</rhyme></l>
        <l id="s1:l2" met="4" type="+-">L'autre jour faisant le <rhyme label="b" type="m">fou</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">J'apperçûs certain <rhyme label="b" type="m" corresp="#s1:l2">Bijou</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Sous une gaze <rhyme label="a" type="f" corresp="#s1:l1">très-fine</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Exprès pour qu'on le <rhyme label="d" type="f">devine</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s1:l5">nom</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s1:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Près d'une vive <rhyme label="a" type="f">fontaine</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Se voit un double <rhyme label="b" type="m">valon</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Là s'éleve un petit <rhyme label="b" type="m" corresp="#s2:l2">mont</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Où croît le lin &amp; la <rhyme label="a" type="f" corresp="#s2:l1">laine</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Sans cesse Amour s'y <rhyme label="d" type="f">proméne</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s2:l5">nom</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Or écoutez, ma <rhyme label="c" type="m" corresp="#s2:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">En vain la vertu <rhyme label="a" type="f">sévere</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Deffend-t-elle d'en <rhyme label="b" type="m">user</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Quand Jupin pour y <rhyme label="b" type="m" corresp="#s3:l2">passer</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Ose tout tenter &amp; <rhyme label="a" type="f" corresp="#s3:l1">faire</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Junon seroit en <rhyme label="d" type="f">colere</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s3:l5">nom</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s3:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Vos Mamans gardent <rhyme label="a" type="f">l'entrée</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Leurs yeux sons toûjours <rhyme label="b" type="m">dessus</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Maîs en vain pour un <rhyme label="b" type="m" corresp="#s4:l2">Argus</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Il en est cinq cens <rhyme label="a" type="f" corresp="#s4:l1">trompées</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Amour est un vray <rhyme label="d" type="f">Prothée</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s4:l5">nom</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s4:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Chaque Belle en sa <rhyme label="a" type="f">puissance</rhyme></l>
        <l id="s5:l2" met="4" type="+-">A ce Bijou si <rhyme label="b" type="m">vanté</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Chaque Amant en a <rhyme label="b" type="m" corresp="#s5:l2">clef</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Rarement la <rhyme label="a" type="f" corresp="#s5:l1">jouïssance</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Ce seroit trop de <rhyme label="d" type="f">licence</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s5:l5">nom</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s5:l5">chanson</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">On le doit mettre en <rhyme label="a" type="f">usage</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Dès qu'on atteint les beaux <rhyme label="b" type="m">ans</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Autant qu'il brille au <rhyme label="b" type="m" corresp="#s6:l2">Printems</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Autant il est laid sur <rhyme label="a" type="f" corresp="#s6:l1">l'âge</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Plus d'une Fille en <rhyme label="d" type="f">enrage</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s6:l5">nom</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s6:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">On feroit un fort gros <rhyme label="a" type="f">Livre</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Des effets de son <rhyme label="b" type="m">pouvoir</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Il échauffe, il fait <rhyme label="b" type="m" corresp="#s7:l2">pleuvoir</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Il attire, il se fait <rhyme label="a" type="f" corresp="#s7:l1">suivre</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>;</l>
        <l id="s7:l6" met="4" type="+-">Il fait mourir &amp; <rhyme label="d" type="f">revivre</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s7:l5">nom</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s7:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Autrefois, pour une <rhyme label="a" type="f">pomme</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Venus donna ce <rhyme label="b" type="m">trésor</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Mais cette pomme étoit <rhyme label="b" type="m" corresp="#s8:l2">d'or</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Et Paris étoit bel <rhyme label="a" type="f" corresp="#s8:l1">homme</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">On en a pour moindre <rhyme label="d" type="f">somme</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s8:l5">nom</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s8:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Il causa sa <rhyme label="a" type="f">decadence</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Des lieux où Priam <rhyme label="b" type="m">régnoit</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En Espagne on en a <rhyme label="b" type="m" corresp="#s9:l2">fait</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Un Ordre de <rhyme label="a" type="f" corresp="#s9:l1">conséquence</rhyme>;</l>
        <l id="s9:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Jadis il sauva la <rhyme label="d" type="f">France</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s9:l5">nom</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s9:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Dans l'amoureux se <rhyme label="a" type="f">folie</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Son commerce est des plus <rhyme label="b" type="m">doux</rhyme>;</l>
        <l id="s10:l3" met="4" type="+-">Mais si-tôt qu'on est <rhyme label="b" type="m" corresp="#s10:l2">Epoux</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">On s'en lasse, on s'en <rhyme label="a" type="f" corresp="#s10:l1">ennuye</rhyme>;</l>
        <l id="s10:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">On le grille en <rhyme label="d" type="f">Italie</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s10:l5">nom</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Or échoutez ma <rhyme label="c" type="m" corresp="#s10:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Plus sa figure est <rhyme label="a" type="f">petite</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Plus ce Bijou est de <rhyme label="b" type="m">prix</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Chaque belle dans <rhyme label="b" type="m" corresp="#s11:l2">Paris</rhyme></l>
        <l id="s11:l4" met="4" type="+-">S'en dispute le <rhyme label="a" type="f" corresp="#s11:l1">mérite</rhyme>;</l>
        <l id="s11:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">La Prude n'est <rhyme label="d" type="f">qu'hypocrite</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s11:l5">nom</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s11:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">On a beau le mettre en <rhyme label="a" type="f">gage</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Jamais il ne e'est <rhyme label="b" type="m">perdu</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Sans ôter de sa <rhyme label="b" type="m" corresp="#s12:l2">vertu</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Aisément on le <rhyme label="a" type="f" corresp="#s12:l1">partage</rhyme>;</l>
        <l id="s12:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">C'est le plus commun <rhyme label="d" type="f">usage</rhyme>,</l>
        <l id="s12:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s12:l5">nom</rhyme>,</l>
        <l id="s12:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s12:l5">chanson</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
