  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SPeelt op, <rhyme label="a" type="f">vreughde-snaren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Slaet nu op uw' hooghste <rhyme label="b" type="m">toon</rhyme>;</l>
        <l id="s1:l3" met="3" type="+-">Wilt nu <rhyme label="a" type="f" corresp="#s1:l1">openbaren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat ick nu aen d'Amstel <rhyme label="b" type="m" corresp="#s1:l2">woon'</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Barster nu uyt tot een <rhyme label="c" type="m">Bruylofts-Liedt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Over het Huwelijck 't welck <rhyme label="c" type="m" corresp="#s1:l5">geschiedt</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Tusschen Heer Rombouts, en 't <rhyme label="d" type="f">Reaeltje</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">'t Beste <rhyme label="d" type="f" corresp="#s1:l7">staeltje</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Pronck en <rhyme label="d" type="f" corresp="#s1:l7">praaltje</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Van dat den Amstel <rhyme label="c" type="m" corresp="#s1:l5">siet</rhyme>.</l>
      </lg>
    </lg>
  </text>
