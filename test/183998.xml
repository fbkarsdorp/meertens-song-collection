<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183998</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1634</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Jamais une si belle dame</melody>
  <author>Jacob Cats</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O God, wy sien dat onse <rhyme label="a" type="f">jaren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat onse tijt staegh henen <rhyme label="b" type="m">gaet</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">De jeught verloopt gelijck de <rhyme label="a" type="f" corresp="#s1:l1">baren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Als wy dat leeren uyter <rhyme label="b" type="m" corresp="#s1:l2">daet</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Maer, Heer, u <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Noyt recht <rhyme label="c" type="f" corresp="#s1:l5">gepresen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Dat blijft alleen in vasten <rhyme label="b" type="m" corresp="#s1:l2">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het oude jaer dat is <rhyme label="a" type="f">verloopen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Het nieuw dat neemt nusyn <rhyme label="b" type="m">begin</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Leert ons den tijt recht uyt te <rhyme label="a" type="f" corresp="#s2:l1">koopen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat is voor-al een groot <rhyme label="b" type="m" corresp="#s2:l2">gewin</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">O Heer, wy <rhyme label="c" type="f">wenschen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Als nieuwe <rhyme label="c" type="f" corresp="#s2:l5">menschen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Van u voortaen een reynen <rhyme label="b" type="m" corresp="#s2:l2">sin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De gulde son begint te <rhyme label="a" type="f">rijsen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Den dagh die wast, gelijck men <rhyme label="b" type="m">siet</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Myn ziel, ghy dient oock aen te <rhyme label="a" type="f" corresp="#s3:l1">wijsen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">De gunst die ghy van God <rhyme label="b" type="m" corresp="#s3:l2">geniet</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">O laet u <rhyme label="c" type="f">sinnen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Eens <rhyme label="c" type="f" corresp="#s3:l5">overwinnen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Al wat de sond'en vleech <rhyme label="b" type="m" corresp="#s3:l2">gebiet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Verlaet doch eens u fondigh <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Den ouden mensch dient <rhyme label="b" type="m">uyt-gedaen</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Koom met de son om-hoogh <rhyme label="a" type="f" corresp="#s4:l1">geresen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En wilt gestaegh noch hooger <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Wilt God u <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Gansch <rhyme label="c" type="f" corresp="#s4:l5">overgeven</rhyme>;</l>
        <l id="s4:l7" met="4" type="-+">Dat is tot heyl de rechte <rhyme label="b" type="m" corresp="#s4:l2">baen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wie weet of wy noch <rhyme label="a" type="f">ander-werven</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Eens sullen sien het nieuwe <rhyme label="b" type="m">Iaer</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Want out en jongh moet beyde <rhyme label="a" type="f" corresp="#s5:l1">sterven</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Wy leven al in dit <rhyme label="b" type="m" corresp="#s5:l2">gevaer</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Wel aen myn <rhyme label="c" type="f">sinnen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Die gaen <rhyme label="c" type="f" corresp="#s5:l5">beginnen</rhyme>;</l>
        <l id="s5:l7" met="4" type="-+">Ick word het in den geest <rhyme label="b" type="m" corresp="#s5:l2">gewaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
