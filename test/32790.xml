<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>32790</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Cupido triomfant</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHegroet soo moet ghy <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Maria hooch <rhyme label="b" type="f">gepresen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Vol ootmoet Dienst maeght <rhyme label="a" type="m" corresp="#s1:l1">fijn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van dry voudich <rhyme label="b" type="f" corresp="#s1:l2">wesen</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Ghy zijt vol gracy <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Moeder en Maghet <rhyme label="c" type="m" corresp="#s1:l5">goet</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">O Maria weest dan <rhyme label="c" type="m" corresp="#s1:l5">ghegroet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O uytvercoren <rhyme label="a" type="m">jeucht</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Van Godt den Vader <rhyme label="b" type="f">machtich</rhyme></l>
        <l id="s2:l3" met="3" type="-+">O Liefste Bruydt vol <rhyme label="a" type="m" corresp="#s2:l1">deucht</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Des Heylighs Gheest <rhyme label="b" type="f" corresp="#s2:l2">waerachtich</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Ghy zijt vol gracy <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Moeder en Maghet <rhyme label="c" type="m" corresp="#s2:l5">goet</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">O Maria weest dan <rhyme label="c" type="m" corresp="#s2:l5">ghegroet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O Koninghinne <rhyme label="a" type="m">eenpaer</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Der Hemelen en <rhyme label="b" type="f">Eerden</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">O Moeder Godts <rhyme label="a" type="m" corresp="#s3:l1">voorwaer</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">O Maget groot van <rhyme label="b" type="f" corresp="#s3:l2">weerden</rhyme>!</l>
        <l id="s3:l5" met="3" type="-+">Ghy zijt vol gracy <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Moeder en Maghet <rhyme label="c" type="m" corresp="#s3:l5">goet</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">O Maria weest dan <rhyme label="c" type="m" corresp="#s3:l5">ghegroet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O Croon en schoon <rhyme label="a" type="m">cieraet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Der Maeghdelijck <rhyme label="b" type="f">Chooren</rhyme>!</l>
        <l id="s4:l3" met="3" type="-+">O reyn Maeghdelijck <rhyme label="a" type="m" corresp="#s4:l1">saet</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">Ontsluyt ons u <rhyme label="b" type="f" corresp="#s4:l2">tresooren</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Ghy zijt vol gracy <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Moeder en Maghet <rhyme label="c" type="m" corresp="#s4:l5">goet</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">O Maria weest dan <rhyme label="c" type="m" corresp="#s4:l5">ghegroet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Blijft by ons in't <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">In druck, en alle <rhyme label="b" type="f">lijden</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Och en verlaet ons <rhyme label="a" type="m" corresp="#s5:l1">niet</rhyme>!</l>
        <l id="s5:l4" met="3" type="-+">Ons eynde wilt <rhyme label="b" type="f" corresp="#s5:l2">verblijden</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Ghy zijt vol gracy <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Moeder en Maghet <rhyme label="c" type="m" corresp="#s5:l5">goet</rhyme>:</l>
        <l id="s5:l7" met="4" type="+-">O Maria weest dan <rhyme label="c" type="m" corresp="#s5:l5">ghegroet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
