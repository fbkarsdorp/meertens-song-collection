    <melody>Hoor Kees mijn vrijer</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="amusement">vermaak</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">‘Hoort Kees, myn <rhyme label="a" type="f">Vryer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wat hebt gy daar te <rhyme label="b" type="m">koop</rhyme>:</l>
        <l id="s1:l3" met="2" type="+-">Boter of <rhyme label="a" type="f" corresp="#s1:l1">Eyer</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Laet mijn eens sien den <rhyme label="b" type="m" corresp="#s1:l2">hoop</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Wat geeft gy het <rhyme label="c" type="m">dosijn</rhyme>?</l>
        <l id="s1:l6" met="3" type="-+">Of hebt gy sin in <rhyme label="c" type="m" corresp="#s1:l5">mijn</rhyme>?</l>
        <l id="s1:l7" met="3" type="-+">Kom geeftse mijn op <rhyme label="d" type="f">Trouwe</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En slaapt een nagt by <rhyme label="e" type="m">mijn</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">‘t Sal u niet <rhyme label="d" type="f" corresp="#s1:l7">rouwe</rhyme>.’</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">‘Juffrou, u bruyn <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Locken mijn tot de <rhyme label="b" type="m">min</rhyme>:</l>
        <l id="s2:l3" met="2" type="+-">Gij   zijt <rhyme label="a" type="f" corresp="#s2:l1">bedrogen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">‘k Hou meer van een <rhyme label="b" type="m" corresp="#s2:l2">Boerin</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Ik soek geen Juffrouws <rhyme label="c" type="m">belt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Sij syn te ligt voor ‘t <rhyme label="c" type="m" corresp="#s2:l5">gelt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ik trou met mijn <rhyme label="d" type="f">boerinne</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Die werke dag en <rhyme label="e" type="m">nagt</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Naayen en <rhyme label="d" type="f" corresp="#s2:l7">spinne</rhyme>.’</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">‘Waar zijn u <rhyme label="a" type="f">zinne</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Acht gy geen Juffrouw <rhyme label="b" type="m">meer</rhyme></l>
        <l id="s3:l3" met="2" type="+-">Als een <rhyme label="a" type="f" corresp="#s3:l1">boerinne</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">Dan zoud gy zijn een <rhyme label="b" type="m" corresp="#s3:l2">Heer</rhyme>!</l>
        <l id="s3:l5" met="3" type="-+">Gy   zijt een botten <rhyme label="c" type="m">boer</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Een Kinkel of een <rhyme label="c" type="m" corresp="#s3:l5">Loer</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Uw Fortuyn is <rhyme label="d" type="f">gebooren</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Grijpt het geluk by ‘t <rhyme label="e" type="m">hayr</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">‘k Segt u te <rhyme label="d" type="f" corresp="#s3:l7">vooren</rhyme>.’</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">‘Ben niet <rhyme label="a" type="f">hoovaardig</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik acht geen Hoofse <rhyme label="b" type="m">zwier</rhyme>.</l>
        <l id="s4:l3" met="2" type="+-">Een Meysje, <rhyme label="a" type="f" corresp="#s4:l1">aardig</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met gelt, dat is <rhyme label="b" type="m" corresp="#s4:l2">pleyzier</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Al waart een Boeren <rhyme label="c" type="m">meyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Met duysent gulden <rhyme label="c" type="m" corresp="#s4:l5">ryk</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Ik souse <rhyme label="d" type="f">respeeteren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Meer als een kaal <rhyme label="e" type="m">Iuffrou</rhyme></l>
        <l id="s4:l9" met="2" type="+-">In Zyde <rhyme label="d" type="f" corresp="#s4:l7">kleeren</rhyme>.’</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">‘Mijn blanke <rhyme label="a" type="f">leden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die yeder een <rhyme label="b" type="m">behaagt</rhyme>:</l>
        <l id="s5:l3" met="2" type="+-">‘t Is tegen <rhyme label="a" type="f" corresp="#s5:l1">reden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat daar een Boer van <rhyme label="b" type="m" corresp="#s5:l2">klaagt</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Mijn kleeren, schoon in ‘t <rhyme label="c" type="m">oog</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">‘k Draag een Fontangie <rhyme label="c" type="m" corresp="#s5:l5">hoog</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Daar een boerin gaet <rhyme label="d" type="f">slonse</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met de Kap in de <rhyme label="e" type="m">nek</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Op houte <rhyme label="d" type="f" corresp="#s5:l7">klonse</rhyme>.’</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">‘Wilt my <rhyme label="a" type="f">excuseren</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Juffrou, ik ben te <rhyme label="b" type="m">grof</rhyme>.</l>
        <l id="s6:l3" met="2" type="+-">Blyft by de <rhyme label="a" type="f" corresp="#s6:l1">Heeren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ik hou van geen <rhyme label="b" type="m" corresp="#s6:l2">gestof</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Ik agt geen Zeyd of <rhyme label="c" type="m">Zits</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Van een stuyver, vyf, <rhyme label="c" type="m" corresp="#s6:l5">ses</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Die vodden kogt gy <rhyme label="d" type="f">heden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Op d’ Oude Luyse <rhyme label="e" type="m">merkt</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-">Daar pronkt gy <rhyme label="d" type="f" corresp="#s6:l7">mede</rhyme>.’</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">‘Het sal u <rhyme label="a" type="f">rouwe</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat gy my soo <rhyme label="b" type="m">versmaat</rhyme>.</l>
        <l id="s7:l3" met="2" type="+-">Trouwt een <rhyme label="a" type="f" corresp="#s7:l1">Juffrouwe</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Gy eet niet als <rhyme label="b" type="m" corresp="#s7:l2">Gebraat</rhyme></l>
        <l id="s7:l5" met="3" type="-+">En Gy drinkt Wijn en <rhyme label="c" type="m">Mee</rhyme></l>
        <l id="s7:l6" met="3" type="-+">En Soccolaat of <rhyme label="c" type="m" corresp="#s7:l5">Thee</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Verlaat die vuyle <rhyme label="d" type="f">sloore</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Die na de Koe-stront <rhyme label="e" type="m">stinkt</rhyme>,</l>
        <l id="s7:l9" met="2" type="+-">Wilt na mijn <rhyme label="d" type="f" corresp="#s7:l7">hoore</rhyme>.’</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">‘U <rhyme label="a" type="f">presentatie</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En acht ik niet een <rhyme label="b" type="m">spelt</rhyme>:</l>
        <l id="s8:l3" met="2" type="+-">‘t Is maar <rhyme label="a" type="f" corresp="#s8:l1">flattatie</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ik hou al meer van ‘t <rhyme label="b" type="m" corresp="#s8:l2">gelt</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Wy koken Worst en <rhyme label="c" type="m">Ham</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En somtyds een vet <rhyme label="c" type="m" corresp="#s8:l5">Lam</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Ik hou ‘t met myn <rhyme label="d" type="f">Boerinnen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Die maaken Botter en <rhyme label="e" type="m">Kaas</rhyme></l>
        <l id="s8:l9" met="2" type="+-">Om gelt te <rhyme label="d" type="f" corresp="#s8:l7">winnen</rhyme>.’</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="+-">‘Gaet, Boere <rhyme label="a" type="f">lompert</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Ik blijf in mijn <rhyme label="b" type="m">Salet</rhyme>.</l>
        <l id="s9:l3" met="2" type="+-">Gy zijt een <rhyme label="a" type="f" corresp="#s9:l1">plompert</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Gy acht geen schoon of <rhyme label="b" type="m" corresp="#s9:l2">net</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Ik seg het in ‘t <rhyme label="c" type="m">gemeyn</rhyme>:</l>
        <l id="s9:l6" met="3" type="-+">Stroyt Roosen voor een <rhyme label="c" type="m" corresp="#s9:l5">Swijn</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Geen Kraaye paart met <rhyme label="d" type="f">Uylen</rhyme>.</l>
        <l id="s9:l8" met="3" type="-+">Blijft dan by u <rhyme label="e" type="m">boerin</rhyme>,</l>
        <l id="s9:l9" met="2" type="+-">Morsig en <rhyme label="d" type="f" corresp="#s9:l7">vuyle</rhyme>.’</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="+-">‘Weg, Juffrou <rhyme label="a" type="f">sober</rhyme>!</l>
        <l id="s10:l2" met="3" type="-+">Trout gy een ligte <rhyme label="b" type="m">vink</rhyme></l>
        <l id="s10:l3" met="2" type="+-">Of Ionker <rhyme label="a" type="f" corresp="#s10:l1">pover</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">‘t Is al geen Goud dat <rhyme label="b" type="m" corresp="#s10:l2">blinkt</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">‘t Is ‘Juffrou’ op de <rhyme label="c" type="m">straat</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Wie weet hoe ‘t in huys <rhyme label="c" type="m" corresp="#s10:l5">gaat</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Ik trou met myns <rhyme label="d" type="f">gelyke</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Met Truytje myn <rhyme label="e" type="m">Boerin</rhyme></l>
        <l id="s10:l9" met="2" type="+-">Die ‘s schoon en <rhyme label="d" type="f" corresp="#s10:l7">ryke</rhyme>.’</l>
      </lg>
    </lg>
  </text>
</song>
