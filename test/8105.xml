<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8105</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Ik klom de boom al op die mij te hoog</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LIeve vrienden <rhyme label="a" type="f">eenparen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghelijck wy in Godts <rhyme label="b" type="m">naem</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Eenmoedelijck <rhyme label="a" type="f" corresp="#s1:l1">vergaren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Soo ist oock seer <rhyme label="b" type="m" corresp="#s1:l2">bequaem</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Dat wy nu wederom in zijn naem <rhyme label="c" type="f">scheyden</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Heer wil ons <rhyme label="a" type="f">gheleyden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Waer dat wy hene <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En in zijn groene <rhyme label="a" type="f" corresp="#s2:l1">weyden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Van 't Goddelijck <rhyme label="b" type="m" corresp="#s2:l2">vermaen</rhyme></l>
        <l id="s2:l5" met="5" type="-+">Ons in 't afwesen geestelijck <rhyme label="c" type="f">regeeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Op dat wy in zijn <rhyme label="a" type="f">leere</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Weder komen by <rhyme label="b" type="m">ien</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Al met veel deughde <rhyme label="a" type="f" corresp="#s3:l1">meere</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dan voormaels is <rhyme label="b" type="m" corresp="#s3:l2">ghesien</rhyme></l>
        <l id="s3:l5" met="5" type="-+">En met veel minder sonde in het <rhyme label="c" type="f">enden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Waer dat wy heene <rhyme label="a" type="f">wenden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In 't scheyden vrienden <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Onder vreemde <rhyme label="a" type="f" corresp="#s4:l1">bekenden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Laet ons toch een licht <rhyme label="b" type="m" corresp="#s4:l2">zijn</rhyme></l>
        <l id="s4:l5" met="5" type="-+">In al ons wandel, leven, wercken, <rhyme label="c" type="f">woorden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Tghene dat wy hier <rhyme label="a" type="f">hoorden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En wilt vergheten <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Waert dat het woordt <rhyme label="a" type="f" corresp="#s5:l1">versmoorde</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Soo souder groot <rhyme label="b" type="m" corresp="#s5:l2">verdriet</rhyme></l>
        <l id="s5:l5" met="5" type="-+">Seer klaghelijck nae onse scheyden <rhyme label="c" type="f">komen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al met vreesen en <rhyme label="a" type="f">schromen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Elck sijn ziele <rhyme label="b" type="m">bewaert</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">By gherechtighe en <rhyme label="a" type="f" corresp="#s6:l1">vromen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Alle tijden <rhyme label="b" type="m" corresp="#s6:l2">vergaert</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Met den lichame of met den <rhyme label="c" type="f">ghemoede</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Heer wil ons <rhyme label="a" type="f">behoeden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">In zijn Godtlijcke <rhyme label="b" type="m">vreen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En deurt gheloof op <rhyme label="a" type="f" corresp="#s7:l1">voeden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Werwaerts wy keeren <rhyme label="b" type="m" corresp="#s7:l2">heen</rhyme>,</l>
        <l id="s7:l5" met="5" type="-+">Terwijle het nu moet ghescheyden <rhyme label="c" type="f">wesen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof vrienden <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ick beveel u Godt den <rhyme label="b" type="m">Vaer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Die wil ons al na <rhyme label="a" type="f" corresp="#s8:l1">desen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">By den Hemelsche <rhyme label="b" type="m" corresp="#s8:l2">schaer</rhyme>,</l>
        <l id="s8:l5" met="5" type="-+">Eeuwigh in sijn vreughd'lijcke rijck <rhyme label="c" type="f">vergaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
