  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Sie hoe waggelen de <rhyme label="a" type="f">staten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van den trotsen Lely <rhyme label="b" type="m">vorst</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Die hoogmoedig en <rhyme label="a" type="f" corresp="#s1:l1">verwaten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Yder een braveren <rhyme label="b" type="m" corresp="#s1:l2">dorst</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Sie hoe beeft zijn trotse <rhyme label="c" type="m">troon</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Sie hoe waggeld sijne <rhyme label="c" type="m" corresp="#s1:l5">Kroon</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Van haar luyster gants <rhyme label="a" type="f" corresp="#s1:l1">verlaten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Die wel eertyds blonk soo <rhyme label="c" type="m" corresp="#s1:l5">schoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Laat ons dan volmondig <rhyme label="a" type="f">prysen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Alle helden die soo <rhyme label="b" type="m">vroom</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Aan den Staat haar trouw <rhyme label="a" type="f" corresp="#s2:l1">bewysen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die 's lands vyand brengt in <rhyme label="b" type="m" corresp="#s2:l2">toom</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Laat ons drinken tot haar <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat haar luyster, meer en <rhyme label="c" type="m" corresp="#s2:l5">meer</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Mag tot aen de Serren <rhyme label="a" type="f" corresp="#s2:l1">rysen</rhyme></l>
        <l id="s2:l8" met="4" type="+-">En des Seines magt <rhyme label="c" type="m" corresp="#s2:l5">verneer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vat, vat aan dees volle <rhyme label="a" type="f">glasen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Op 't geluk van 't <rhyme label="b" type="m">vaderland</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Trots 's lands vyands heiloos <rhyme label="a" type="f" corresp="#s3:l1">rasen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Om het groeijen van sijn <rhyme label="b" type="m" corresp="#s3:l2">schand</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Roemen wy der helden <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Die ons vrolijk Juychen <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme></l>
        <l id="s3:l7" met="4" type="+-">En sijn heyr kragt doet <rhyme label="a" type="f" corresp="#s3:l1">verbasen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Daar sijn spyt vergeefs om <rhyme label="c" type="m" corresp="#s3:l5">woed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
