    <tags/>
    <melody>Cloris mijn lief mijn tweede leven</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Kloris lief myn tweede <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Waer in heb ik uw <rhyme label="a" type="f" corresp="#s1:l1">misdreven</rhyme>?</l>
        <l id="s1:l3" met="3" type="+-">Dat gy wreed van <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Uw stoord aen myn <rhyme label="b" type="m" corresp="#s1:l3">min</rhyme></l>
        <l id="s1:l5" met="5" type="+-">Ik zal door die straf nog sneeden, <rhyme label="a" type="f" corresp="#s1:l1">sneeden</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Engel gaf ik daer toe <rhyme label="a" type="f">reeden</rhyme>?</l>
        <l id="s2:l2" met="4" type="+-">Zo waert gy met regt <rhyme label="a" type="f" corresp="#s2:l1">t'onvreeden</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">'k Heb uw te allen <rhyme label="b" type="m">tyd</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Myn hert <rhyme label="b" type="m" corresp="#s2:l3">toegeweid</rhyme></l>
        <l id="s2:l5" met="5" type="+-">Schuw van de onstandvastigheden, <rhyme label="a" type="f" corresp="#s2:l1">heden</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Lief laet my dog beÃª <rhyme label="a" type="f">verwinnen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En verlaet uw straffen <rhyme label="a" type="f" corresp="#s3:l1">zinnen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Kom rykt my uw <rhyme label="b" type="m">Mond</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">En heelt weer myn <rhyme label="b" type="m" corresp="#s3:l3">Wond</rhyme></l>
        <l id="s3:l5" met="5" type="+-">Dan zal ik uw eeuwig minnen, <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer die wreede wilniet <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sy stopt voor myn klagt haer <rhyme label="a" type="f" corresp="#s4:l1">ooren</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En lacht met myn <rhyme label="b" type="m">smert</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Kupido treft geen <rhyme label="b" type="m" corresp="#s4:l3">hert</rhyme></l>
        <l id="s4:l5" met="5" type="+-">Want myn zuchten gaen verlooren <rhyme label="a" type="f" corresp="#s4:l1">looren</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
