  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GElijck de Sonne uyt een <rhyme label="a" type="m">Roos</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Op haelt de silvere dauwe o! <rhyme label="b" type="f">Tipgens</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Suych ick uan u <rhyme label="b" type="f" corresp="#s1:l2">Lipjens</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Nectar en <rhyme label="a" type="m" corresp="#s1:l1">Ambroos</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Maer als ghy mijn weerom <rhyme label="c" type="m">begeeft</rhyme></l>
        <l id="s1:l6" met="5" type="-+">Ben ick als een Salmander ach die <rhyme label="c" type="m" corresp="#s1:l5">leeft</rhyme></l>
        <l id="s1:l7" met="3" type="-+">En wentel in den <rhyme label="d" type="m">Brant</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Versengt door 't Min <rhyme label="d" type="m" corresp="#s1:l7">verstant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Geen frissche Sephijrs lieve <rhyme label="a" type="m">lucht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Noch soete Waessem Ziels <rhyme label="b" type="f">verssing</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Sweeftet als door <rhyme label="b" type="f" corresp="#s2:l2">perssing</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Van mijn diep <rhyme label="a" type="m" corresp="#s2:l1">gesucht</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Het preutel trampje swelt oock <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Als door de brave dropgens die ick <rhyme label="c" type="m" corresp="#s2:l5">giet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ten deed mijn traentjes <rhyme label="d" type="m">dou</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Het beeckje drooch zijn <rhyme label="d" type="m" corresp="#s2:l7">sou</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het morgen root heeft geen <rhyme label="a" type="m">begin</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ten is geen dach die ghy siet <rhyme label="b" type="f">pralen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Maer het zijn de <rhyme label="b" type="f" corresp="#s3:l2">stralen</rhyme>;</l>
        <l id="s3:l4" met="3" type="+-">Van mijn trouwe <rhyme label="a" type="m" corresp="#s3:l1">Min</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Want waer de Fackel niet <rhyme label="c" type="m">verschijnt</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Den dach versmoort en 't licht geheel <rhyme label="c" type="m" corresp="#s3:l5">verdwijnt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">De Son verliest zijn <rhyme label="d" type="m">kracht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Het toont sich altijt <rhyme label="d" type="m" corresp="#s3:l7">Nacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wel aen mijn soo veel Cusjes <rhyme label="a" type="m">jont</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Als kleyne Wichjens minne <rhyme label="b" type="f">swermen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Om u lieve <rhyme label="b" type="f" corresp="#s4:l2">Nermen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">En u Kaeckjes <rhyme label="a" type="m" corresp="#s4:l1">ront</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Gelooft dan vry mijn <rhyme label="c" type="m">Af-godin</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Hoe trou stant-vast ick u vermogen <rhyme label="c" type="m" corresp="#s4:l5">Min</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En laet u wreetheyt <rhyme label="d" type="m">af</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Voor die zijn Hert u <rhyme label="d" type="m" corresp="#s4:l7">gaf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick smoock ick smeul ick blaeck en <rhyme label="a" type="m">brant</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Hoe lange sult ghy noch <rhyme label="b" type="f">kont-oorstich</rhyme>?</l>
        <l id="s5:l3" met="3" type="+-">En drie dubbelt <rhyme label="b" type="f" corresp="#s5:l2">korstich</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Braen mijn <rhyme label="a" type="m" corresp="#s5:l1">Inge-want</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Is kan u Hert soo kout als <rhyme label="c" type="m">loot</rhyme>,</l>
        <l id="s5:l6" met="5" type="-+">Van Ys en Snee of als de kille <rhyme label="c" type="m" corresp="#s5:l5">Doot</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Neem van het Vier me <rhyme label="d" type="m">wat</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">'t Geen langh mijn Ziel <rhyme label="d" type="m" corresp="#s5:l7">besat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
