<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9169</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Waarom ach schone</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Secht soete beckje, waer wilje <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Verwacht my wat 'ksal u <rhyme label="b" type="f">versellen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wech, wech dit geckje laet my met <rhyme label="a" type="m" corresp="#s1:l1">vreen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En hout vry op van my te <rhyme label="b" type="f" corresp="#s1:l2">quellen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ick heb in u doch gheen <rhyme label="c" type="f">behaegen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Soetertje mach ickt u niet <rhyme label="c" type="f" corresp="#s1:l5">vraghen</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Of ghy wilt dat ick u gheleyden <rhyme label="d" type="m">sal</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Wech wijs hooft, ghy zijt wel half <rhyme label="d" type="m" corresp="#s1:l7">mal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laet my gheniete van u <rhyme label="a" type="m">weermin</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Wilt met mijn smart hebben <rhyme label="b" type="f">meedoghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">'t Sou my verdrieten teghen mijn <rhyme label="a" type="m" corresp="#s2:l1">sin</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Te minne u, gaet uyt mijn <rhyme label="b" type="f" corresp="#s2:l2">ooghen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Het sal doch nimmermeer <rhyme label="c" type="f">geschiede</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Schoone wilt my doch weermin <rhyme label="c" type="f" corresp="#s2:l5">biede</rhyme>,</l>
        <l id="s2:l7" met="5" type="-+">En laeft mijn hert, versacht mijn sware <rhyme label="d" type="m">pijn</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Iongman gaet wech, 'tsal doch nimmer <rhyme label="d" type="m" corresp="#s2:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hebt ghy dan reden te haten <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Secht schoonst der schoon aen uwe <rhyme label="b" type="f">slave</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Vol boertse seede Iongman zijt <rhyme label="a" type="m" corresp="#s3:l1">ghy</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wanneer ick mijn eer aen u <rhyme label="b" type="f" corresp="#s3:l2">gave</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Ick sou altijdt in droefheyt <rhyme label="c" type="f">treuren</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Soetertje ghy doet mijn hart <rhyme label="c" type="f" corresp="#s3:l5">scheuren</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">Vermidts dat ghy alsulcke reden <rhyme label="d" type="m">spreeckt</rhyme>.</l>
        <l id="s3:l8" met="4" type="-+">Iongman 'tis om dat u van als <rhyme label="d" type="m" corresp="#s3:l7">ontbreeckt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Princes mijn leven staet in u <rhyme label="a" type="m">hant</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">'k Verwacht van u 'tleven <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">'k Sal geen troost geven aen u 'twaer <rhyme label="a" type="m" corresp="#s4:l1">schant</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Met u sou ick veel smarte <rhyme label="b" type="f" corresp="#s4:l2">erven</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dus weest te vreen laet my in <rhyme label="c" type="f">vreden</rhyme>.</l>
        <l id="s4:l6" met="4" type="-+">Soetertje comt wat naerder <rhyme label="c" type="f" corresp="#s4:l5">treden</rhyme>,</l>
        <l id="s4:l7" met="5" type="-+">In deucht een eer, ach! schoon ic swijm <rhyme label="d" type="m">neer</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Iongman 'tis tijt dat ick van u <rhyme label="d" type="m" corresp="#s4:l7">keer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
