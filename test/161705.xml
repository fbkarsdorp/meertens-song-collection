<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161705</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Veni creator spiritus</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">MYn Ziele looft den Heer uyt alle uwe <rhyme label="a" type="m">kracht</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Die u in zyner wooninghe heeft <rhyme label="a" type="m" corresp="#s1:l1">ghebracht</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dach ende nacht wilt danc <rhyme label="b" type="f">bewijsen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die Ziel en Lijf kan <rhyme label="b" type="f" corresp="#s1:l3">spijsen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Wilt hem doch <rhyme label="b" type="f" corresp="#s1:l3">prijsen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Ghy sijt wel prysens weert, o lieve Heere <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s2:l2" met="5" type="-+">want ghy sijt mijn Ziel een <rhyme label="a" type="m" corresp="#s2:l1">medecijn</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Oly en wijn na schrifs <rhyme label="b" type="f">vermonden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Goot ghy in onse <rhyme label="b" type="f" corresp="#s2:l3">wonden</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Wiest af ons <rhyme label="b" type="f" corresp="#s2:l3">sonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Wie heeft doch meerder barmhertigheden <rhyme label="a" type="m">gedaen</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Dan ghy o rechten <rhyme label="a" type="m" corresp="#s3:l1">Samaritaen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">ons by gestaen ons schult <rhyme label="b" type="f">ghedragen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die wy ten eeuwigen <rhyme label="b" type="f" corresp="#s3:l3">dagen</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Mosten <rhyme label="b" type="f" corresp="#s3:l3">beklagen</rhyme>.,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">U moeten wy wel prijsen, o Heere boven <rhyme label="a" type="m">al</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Die ons heeft genesen van Adams <rhyme label="a" type="m" corresp="#s4:l1">val</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Int jammer dal, zijt <rhyme label="b" type="f">ghecomen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En hebt ons aen <rhyme label="b" type="f" corresp="#s4:l3">genomen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Wt dat <rhyme label="b" type="f" corresp="#s4:l3">verdomen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="-+">Want al wat // dat // de Wet niet en <rhyme label="a" type="m">vermocht</rhyme></l>
        <l id="s5:l2" met="5" type="-+">dat heeft Christus met zijn doot <rhyme label="a" type="m" corresp="#s5:l1">volbrocht</rhyme></l>
        <l id="s5:l3" met="4" type="-+">dat schaep gesocht dat was <rhyme label="b" type="f">verloren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Om ons te vrijden al <rhyme label="b" type="f" corresp="#s5:l3">vooren</rhyme></l>
        <l id="s5:l5" met="2" type="-+">uyt Godts <rhyme label="b" type="f" corresp="#s5:l3">toren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="-+">Ghy hebt dat hantschrift geweert tot onsen <rhyme label="a" type="m">behoet</rhyme></l>
        <l id="s6:l2" met="5" type="-+">En voor ons allen gestort u <rhyme label="a" type="m" corresp="#s6:l1">bloet</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Den Vader goet ghestelt te <rhyme label="b" type="f">vreden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">De Wijn Persse <rhyme label="b" type="f" corresp="#s6:l3">getreden</rhyme></l>
        <l id="s6:l5" met="2" type="-+">En vele <rhyme label="b" type="f" corresp="#s6:l3">gheleden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="-+">Om ons te bevryden uyt den eeuwighen <rhyme label="a" type="m">noodt</rhyme></l>
        <l id="s7:l2" met="5" type="-+">So waert ghy o Heer de doot, een <rhyme label="a" type="m" corresp="#s7:l1">doot</rhyme></l>
        <l id="s7:l3" met="4" type="-+">dit vintmen bloot seer klaer <rhyme label="b" type="f">geschreven</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En ons sonden <rhyme label="b" type="f" corresp="#s7:l3">vergeven</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Op dat wy <rhyme label="b" type="f" corresp="#s7:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="6" type="-+">Wie heeft meerder liefde soo ghy o Heer <rhyme label="a" type="m">vertelt</rhyme></l>
        <l id="s8:l2" met="5" type="-+">Dan die zijn leven voor zijn schapen <rhyme label="a" type="m" corresp="#s8:l1">steldt</rhyme></l>
        <l id="s8:l3" met="4" type="-+">sDuyvels gewelt hebt gy <rhyme label="b" type="f">gewroken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Die zeven zegels <rhyme label="b" type="f" corresp="#s8:l3">gebroken</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Dat Boeck <rhyme label="b" type="f" corresp="#s8:l3">ontloken</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="6" type="-+">Want soo lief hadde God de Werelt <rhyme label="a" type="m">ydoon</rhyme></l>
        <l id="s9:l2" met="5" type="-+">Dat hy Christus zynen sone <rhyme label="a" type="m" corresp="#s9:l1">schoon</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">uyt zynen Troon hier heeft <rhyme label="b" type="f">gesonden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Den sterken man <rhyme label="b" type="f" corresp="#s9:l3">gevonden</rhyme></l>
        <l id="s9:l5" met="2" type="-+">En vast <rhyme label="b" type="f" corresp="#s9:l3">gebonden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="6" type="-+">Ghy hebt voor ons o Heer so veel <rhyme label="a" type="m">gheleen</rhyme></l>
        <l id="s10:l2" met="5" type="-+">Ende den Duyvel zynen cop <rhyme label="a" type="m" corresp="#s10:l1">vertreen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Voor ons gebeen, in u <rhyme label="b" type="f">verklaren</rhyme></l>
        <l id="s10:l4" met="3" type="-+">dat ons God der <rhyme label="b" type="f" corresp="#s10:l3">heyrscharen</rhyme></l>
        <l id="s10:l5" met="2" type="-+">Soude <rhyme label="b" type="f" corresp="#s10:l3">bewaren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="6" type="-+">O Princelijck God vol alder <rhyme label="a" type="m">heerschappijen</rhyme></l>
        <l id="s11:l2" met="5" type="-+">die voor des werelts begin was <rhyme label="a" type="m" corresp="#s11:l1">versien</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Ten laetsten tyen, vertoont int <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">En met u bloet <rhyme label="b" type="f" corresp="#s11:l3">gepresen</rhyme></l>
        <l id="s11:l5" met="2" type="-+">Ons al <rhyme label="b" type="f" corresp="#s11:l3">genesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
