  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Neen <rhyme label="a" type="m">Rozaniere</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Denk dat myn <rhyme label="b" type="f">Liefde</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Nooit zal <rhyme label="c" type="f">verdooven</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Myn trouw staet <rhyme label="d" type="m">vast</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Ik eer en <rhyme label="a" type="m" corresp="#s1:l1">viere</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">U, die my <rhyme label="b" type="f" corresp="#s1:l2">griefde</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Ik zal steeds <rhyme label="c" type="f" corresp="#s1:l3">looven</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Dien zoeten <rhyme label="d" type="m" corresp="#s1:l4">last</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ik kus de <rhyme label="a" type="m">banden</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Waer mee ge Ã´ <rhyme label="b" type="f">Schoone</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Myn hart woudt <rhyme label="c" type="f">boeyen</rhyme>.</l>
        <l id="s2:l4" met="2" type="-+">Die geven <rhyme label="d" type="m">vreugd</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Wil ook zoo <rhyme label="a" type="m" corresp="#s2:l1">branden</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Als ik, en <rhyme label="b" type="f" corresp="#s2:l2">toone</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Dat trouw te <rhyme label="c" type="f" corresp="#s2:l3">gloeyen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Uw hart <rhyme label="d" type="m" corresp="#s2:l4">verheugt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Een trouwe <rhyme label="a" type="m">Minnaer</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Veracht de <rhyme label="b" type="f">schatten</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Die elk op <rhyme label="c" type="f">aerde</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Zoo dierbaer <rhyme label="d" type="m">noemt</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Maek my <rhyme label="a" type="m" corresp="#s3:l1">verwinnaer</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">En wil <rhyme label="b" type="f" corresp="#s3:l2">bevatten</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Dat gy naer <rhyme label="c" type="f" corresp="#s3:l3">waerde</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Myn trouwheid <rhyme label="d" type="m" corresp="#s3:l4">roemt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
