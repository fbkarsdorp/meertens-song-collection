    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Corydon ontstoken</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">KLagende <rhyme label="a" type="f">Goddinne</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Slaat een oog op 't <rhyme label="b" type="m">zeyl</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Siet: nu krijgt de <rhyme label="a" type="f" corresp="#s1:l1">Minne</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Een volkomen <rhyme label="b" type="m" corresp="#s1:l2">heyl</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Daar mijn <rhyme label="c" type="f">Blinde-Soontje</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Goude pijlen <rhyme label="d" type="m">schoot</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Vlecht ik nu een <rhyme label="c" type="f" corresp="#s1:l5">kroontje</rhyme>:</l>
        <l id="s1:l8" met="3" type="+-">Van veel Roosjes, <rhyme label="d" type="m" corresp="#s1:l6">rood</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Roosjes, vars <rhyme label="a" type="f">ontloken</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Met den dau <rhyme label="b" type="m">ver-eend</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Segt gy, zijn <rhyme label="a" type="f" corresp="#s2:l1">gebroken</rhyme>:</l>
        <l id="s2:l4" met="3" type="+-">So gy waan-wijs <rhyme label="b" type="m" corresp="#s2:l2">meend</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Neen, een roosje <rhyme label="c" type="f">geurig</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Dat men Echt'lijk <rhyme label="d" type="m">plukt</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Acht men waard, en <rhyme label="c" type="f" corresp="#s2:l5">keurig</rhyme>:</l>
        <l id="s2:l8" met="3" type="+-">En word niet <rhyme label="d" type="m" corresp="#s2:l6">verdrukt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">'k Heb den prijs <rhyme label="a" type="f">verkregen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Voor des Hemels <rhyme label="b" type="m">-rey</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">En in Ida, <rhyme label="a" type="f" corresp="#s3:l1">zegen</rhyme>:</l>
        <l id="s3:l4" met="3" type="+-">Juno Pallas, <rhyme label="b" type="m" corresp="#s3:l2">bey</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Gaven 't my <rhyme label="c" type="f">gewonnen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">In het <rhyme label="d" type="m">groene-dal</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Hier uyt is <rhyme label="c" type="f" corresp="#s3:l5">begonnen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">'t Eerste <rhyme label="d" type="m" corresp="#s3:l6">Troyens-val</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">'t Is u wel een <rhyme label="a" type="f">quelling</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Dat dit <rhyme label="b" type="m">Edel-paar</rhyme></l>
        <l id="s4:l3" met="3" type="+-">(Na des <rhyme label="a" type="f" corresp="#s4:l1">ziels-verselling</rhyme>)</l>
        <l id="s4:l4" met="3" type="+-">Beyde zijn te <rhyme label="b" type="m" corresp="#s4:l2">gaar</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Nu ver-eend: van <rhyme label="c" type="f">Hijmen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Dio de trou <rhyme label="d" type="m">beveeld</rhyme>:</l>
        <l id="s4:l7" met="3" type="+-">Daar men <rhyme label="c" type="f" corresp="#s4:l5">soete-rijmen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Schone-deuntjes <rhyme label="d" type="m" corresp="#s4:l6">queeld</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Schrijft dan vry een <rhyme label="a" type="f">cedel</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">En onthoud den <rhyme label="b" type="m">tijd</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Doen gy d' Edle <rhyme label="a" type="f" corresp="#s5:l1">Edel</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Zijt geworden <rhyme label="b" type="m" corresp="#s5:l2">quijt</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Want des Huw'lijks <rhyme label="c" type="f">banden</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Worden niet <rhyme label="d" type="m">ontknoopt</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Dan door Atrophs <rhyme label="c" type="f" corresp="#s5:l5">handen</rhyme>:</l>
        <l id="s5:l8" met="3" type="+-">Daar men al na <rhyme label="d" type="m" corresp="#s5:l6">loopt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Daarom laat maar <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Zeylen, d' <rhyme label="b" type="m">Ed'le-Hulk</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Want sy door dit <rhyme label="a" type="f" corresp="#s6:l1">paren</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">En ook over <rhyme label="b" type="m" corresp="#s6:l2">sulk</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Niet en word <rhyme label="c" type="f">versminderd</rhyme>:</l>
        <l id="s6:l6" met="3" type="+-">Maar (veel meer <rhyme label="d" type="m">(vermeÃªrd</rhyme>:</l>
        <l id="s6:l7" met="3" type="+-">Als sy (vruchtbaar) <rhyme label="c" type="f" corresp="#s6:l5">kinderd</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">'t Geen den aart haar <rhyme label="d" type="m" corresp="#s6:l6">leerd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Want men noemd haar <rhyme label="a" type="f">namen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">beyde, Man, en <rhyme label="b" type="m">Wijf</rhyme>:</l>
        <l id="s7:l3" met="3" type="+-">Om dat sy te <rhyme label="a" type="f" corresp="#s7:l1">samen</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Zijn een ziel, een <rhyme label="b" type="m" corresp="#s7:l2">lijf</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Niemand kan my <rhyme label="c" type="f">keren</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Wat ik laat, of <rhyme label="d" type="m">doe</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">En ik sal dit <rhyme label="c" type="f" corresp="#s7:l5">leren</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Tot het eynde <rhyme label="d" type="m" corresp="#s7:l6">toe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
