  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O minne <rhyme label="a" type="f">pyne</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Van waer komt die strenghe <rhyme label="b" type="m">wet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat ick moet in min <rhyme label="a" type="f" corresp="#s1:l1">verdwynen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Nu ick my to 'tminne <rhyme label="b" type="m" corresp="#s1:l2">set</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Liefde my jongh hert <rhyme label="c" type="m">versmacht</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Mits gheen liefde my en <rhyme label="c" type="m" corresp="#s1:l5">acht</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Moght ick hem noemen den <rhyme label="a" type="f" corresp="#s1:l1">mynen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Die mischien naer my oock <rhyme label="c" type="m" corresp="#s1:l5">tracht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Uyt wat een <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Heeft natuer die wet <rhyme label="b" type="m">ghestelt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat een Maeght moet zyn <rhyme label="a" type="f" corresp="#s2:l1">ghebeden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Eer sy haere liefde <rhyme label="b" type="m" corresp="#s2:l2">melt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Schoon de stercke minne <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Roert myn hert, sin, en <rhyme label="c" type="m" corresp="#s2:l5">ghedacht</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Moets ick teghen danck <rhyme label="a" type="f" corresp="#s2:l1">vertreden</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Soo ick d'Maeghden plichtjen <rhyme label="c" type="m" corresp="#s2:l5">acht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Moght ick <rhyme label="a" type="f">bethoone</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">'tGheen een Minnaer dickwils <rhyme label="b" type="m">thoont</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ick sou in syn hertjen <rhyme label="a" type="f" corresp="#s3:l1">woonen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Soo hy in het myn nu <rhyme label="b" type="m" corresp="#s3:l2">woont</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Syne deucht, en <rhyme label="c" type="m">soetigheyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Voeyt in my een vremden <rhyme label="c" type="m" corresp="#s3:l5">stryt</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Ach! moet ick de deucht <rhyme label="a" type="f" corresp="#s3:l1">bekroone</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Soo waer ick myn droefheyt <rhyme label="c" type="m" corresp="#s3:l5">quyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Spraeck loose <rhyme label="a" type="f">mueren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Daer hy dickwils komt <rhyme label="b" type="m">voor-by</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Melt hem hoe ick d'moet <rhyme label="a" type="f" corresp="#s4:l1">besuren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat hy soo langh blyft van <rhyme label="b" type="m" corresp="#s4:l2">my</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Vraeght hy wie u dat <rhyme label="c" type="m">ghebiet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Seght dat ghy het selfs wel <rhyme label="c" type="m" corresp="#s4:l5">siet</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Soo sal d'Maeghde plichte <rhyme label="a" type="f" corresp="#s4:l1">dueren</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En oock eynden myn <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme>.</l>
      </lg>
    </lg>
  </text>
