    <melody>Ik stort nu mijn klachten</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Bent gy 't zoete <rhyme label="a" type="f">Bekje</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Wel Clorisje myn <rhyme label="b" type="m">Maat</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Ey wat doet gy <rhyme label="a" type="f" corresp="#s1:l1">Gekje</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Zo laat op de <rhyme label="b" type="m" corresp="#s1:l2">Straat</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Jou Moer zal <rhyme label="c" type="f">verlegen</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Zyn wat begind <rhyme label="d" type="m">gy</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Terwyl dat de <rhyme label="c" type="f" corresp="#s1:l5">wegen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Nu zyn heel <rhyme label="d" type="m" corresp="#s1:l6">onvry</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">'t Is om u myn <rhyme label="a" type="f">Zoetje</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Myn Schaapje myn <rhyme label="b" type="m">Lam</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Zo veer kruypt het <rhyme label="a" type="f" corresp="#s2:l1">Bloedje</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Daar 't niet gaan en <rhyme label="b" type="m" corresp="#s2:l2">kan</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Doet u Deurtje <rhyme label="c" type="f">open</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Laat myn hier niet <rhyme label="d" type="m">staan</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Of ik gâ weer <rhyme label="c" type="f" corresp="#s2:l5">lopen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Daar ik kom van <rhyme label="d" type="m" corresp="#s2:l6">daan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wel Cloris hoe <rhyme label="a" type="f">Praatje</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Myn Lief kom maar <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Myn zoete <rhyme label="a" type="f" corresp="#s3:l1">Maatje</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">'t Is wel myn <rhyme label="b" type="m" corresp="#s3:l2">zin</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Weest wellekom <rhyme label="c" type="f">binnen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">En zetje wat <rhyme label="d" type="m">neer</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Wy zullen <rhyme label="c" type="f" corresp="#s3:l5">beginnen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Wat is u <rhyme label="d" type="m" corresp="#s3:l6">begeer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wat zou ik <rhyme label="a" type="f">begeren</rhyme>?</l>
        <l id="s4:l2" met="3" type="-+">Wel ik begeer <rhyme label="b" type="m">jou</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">In Deugd en <rhyme label="a" type="f" corresp="#s4:l1">Eeren</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Tot myn Egte <rhyme label="b" type="m" corresp="#s4:l2">Vrouw</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Bent gy niet <rhyme label="c" type="f">genegen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Te wezen myn <rhyme label="d" type="m">Bruyd</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Zo gaan ik myn <rhyme label="c" type="f" corresp="#s4:l5">wegen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">En ik schyer <rhyme label="d" type="m" corresp="#s4:l6">uyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Wel Cloris hoe <rhyme label="a" type="f">Praatje</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Te haastig is <rhyme label="b" type="m">quaad</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Meend gy zoete <rhyme label="a" type="f" corresp="#s5:l1">Maatje</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Dat gy zult te <rhyme label="b" type="m" corresp="#s5:l2">laat</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Komen om te <rhyme label="c" type="f">Trouwen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Het kan u myn <rhyme label="d" type="m">Vrind</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">In 't kort haast <rhyme label="c" type="f" corresp="#s5:l5">berouwen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">U dog wel <rhyme label="d" type="m" corresp="#s5:l6">verzind</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Myn Broek die moet <rhyme label="a" type="f">wyer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Indien ik zo <rhyme label="b" type="m">blyf</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">'k Moet na de <rhyme label="a" type="f" corresp="#s6:l1">Snyer</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Raak ik aan geen <rhyme label="b" type="m" corresp="#s6:l2">Wyf</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">'k Kan niet langer <rhyme label="c" type="f">wagten</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Nu zyt gy te <rhyme label="d" type="m">vreên</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">'k Wil u niet <rhyme label="c" type="f" corresp="#s6:l5">verkragten</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Zeg maar ja of <rhyme label="d" type="m" corresp="#s6:l6">neen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Gy lykt het te <rhyme label="a" type="f">menen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ik hoor 't aan jou <rhyme label="b" type="m">reên</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Gaan wy maar <rhyme label="a" type="f" corresp="#s7:l1">henen</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Ik ben wel te <rhyme label="b" type="m" corresp="#s7:l2">vreên</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Doet zo Jonge <rhyme label="c" type="f">Maagjes</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Dan bent gy uyt de <rhyme label="d" type="m">nood</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Elk Trouwt al eens <rhyme label="c" type="f" corresp="#s7:l5">gragjes</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">En dat voor zyn <rhyme label="d" type="m" corresp="#s7:l6">Dood</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
