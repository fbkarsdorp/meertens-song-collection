<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183501</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Kom brave Keezen, w√©lgemoed, <rhyme label="a" type="m">hoezee</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Spaar nimmer uw Batavisch bloed, <rhyme label="a" type="m" corresp="#s1:l1">hoezee</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Toont, hoe verdrukt g'ook wezen <rhyme label="b" type="m">moogt</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Dat gy geen Slaverny gedoogt, <rhyme label="a" type="m" corresp="#s1:l1">hoezee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Blyv steeds standvastig in 't gevaar, <rhyme label="a" type="m">hoezee</rhyme></l>
        <l id="s2:l2" met="5" type="-+">En blyv vereenigt met elkaar, <rhyme label="a" type="m" corresp="#s2:l1">hoezee</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Zet nog, voor 't lieve <rhyme label="b" type="m">Vaderland</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Uw Goed, uw Bloed, uw Lyf te pand, <rhyme label="a" type="m" corresp="#s2:l1">hoezee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">De Vryheid zy Uw grootste Loon, <rhyme label="a" type="m">hoezee</rhyme></l>
        <l id="s3:l2" met="5" type="-+">(Schoon al uw yv'ren is verboon), <rhyme label="a" type="m" corresp="#s3:l1">hoezee</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Blyv haar toch tot uw dood <rhyme label="b" type="m">getrouw</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Dat nimmer uwe Hoop verflauw! <rhyme label="a" type="m" corresp="#s3:l1">hoezee</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Vreest nimmer snood' Aristocraat, <rhyme label="a" type="m">hoezee</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Schoon 't thans Oranje boven! gaat: <rhyme label="a" type="m" corresp="#s4:l1">hoezee</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ten dienst van Stad en <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Heeft elk Bataav zyn Lyf verpand, <rhyme label="a" type="m" corresp="#s4:l1">hoezee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
