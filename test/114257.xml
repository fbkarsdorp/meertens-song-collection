  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Spieghel aller <rhyme label="a" type="m">deught</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met wat een wonder <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In u Godt is <rhyme label="b" type="f">ghecomen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">U, die om u <rhyme label="c" type="m">nedricheydt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">(Die u tot hem heeft <rhyme label="c" type="m" corresp="#s1:l4">bereyt</rhyme>)</l>
        <l id="s1:l6" met="3" type="-+">Tot Moeder heeft <rhyme label="b" type="f" corresp="#s1:l3">ghenomen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer met wat groot <rhyme label="a" type="m">jolijt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hebt ghy soo langhen <rhyme label="a" type="m" corresp="#s2:l1">tijdt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die soetste vrucht <rhyme label="b" type="f">ghedraghen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">In u lichaem en u <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Vry van ghewalgh en van <rhyme label="c" type="m" corresp="#s2:l4">smert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">In die ghewenschte <rhyme label="b" type="f" corresp="#s2:l3">daghen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maer als de ure <rhyme label="a" type="m">quam</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat u schoon hemelsch <rhyme label="a" type="m" corresp="#s3:l1">lam</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wert sonder wee <rhyme label="b" type="f">gheboren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Uwen Schepper en u <rhyme label="c" type="m">kindt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Van u soo vierich <rhyme label="c" type="m" corresp="#s3:l4">bemint</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Uyt dusenden <rhyme label="b" type="f" corresp="#s3:l3">vercoren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat aldergrootste <rhyme label="a" type="m">vreucht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Heeft u doen 't hert <rhyme label="a" type="m" corresp="#s4:l1">verheucht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Doen swemmen in <rhyme label="b" type="f">welluste</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Als ghevoeght hy van u <rhyme label="c" type="m">wert</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Vast aen u moederlijck <rhyme label="c" type="m" corresp="#s4:l4">hert</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En ghy hem soetjens <rhyme label="b" type="f" corresp="#s4:l3">kuste</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy saeght hem sijnen <rhyme label="a" type="m">dorst</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Versaden aen u <rhyme label="a" type="m" corresp="#s5:l1">borst</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die hy weer moest <rhyme label="b" type="f">vervullen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Hem, die 't all' hier kleeden <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Hebt ghy ghewonden met <rhyme label="c" type="m" corresp="#s5:l4">spoet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In lijnen en in <rhyme label="b" type="f" corresp="#s5:l3">wullen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy waert sijn <rhyme label="a" type="m">onderdaen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Waer van ghy had <rhyme label="a" type="m" corresp="#s6:l1">ontfaen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ziel, lichaem en 'theel <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Maer hoe kost hy dus <rhyme label="c" type="m">bekent</rhyme></l>
        <l id="s6:l5" met="4" type="+-">U, hem t' aenbidden <rhyme label="c" type="m" corresp="#s6:l4">ghewent</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Oock onderdanich <rhyme label="b" type="f" corresp="#s6:l3">wesen</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Lof, alle eer zy <rhyme label="a" type="m">hem</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hem prijse alle <rhyme label="a" type="m" corresp="#s7:l1">stem</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Die u dus heeft <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Op dat ghy, door u <rhyme label="c" type="m">ghebedt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Weirt all' dat ons hier <rhyme label="c" type="m" corresp="#s7:l4">belet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Om eeuwelijck te <rhyme label="b" type="f" corresp="#s7:l3">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
