  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Een goede man, een brave <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hy laat zyn Vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Hy kookt de <rhyme label="c" type="m">Pot</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Mooi <rhyme label="c" type="m" corresp="#s1:l3">aangebrand</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Maar laat zyn Vrouwtje <rhyme label="b" type="f" corresp="#s1:l2">dansen</rhyme>;</l>
        <l id="s1:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een goede Man, een brave <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hy laat zyn Vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Hy wiegt het <rhyme label="c" type="m">Kind</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">By dag en <rhyme label="c" type="m" corresp="#s2:l3">nagt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Maar laat zyn Vrouwtje <rhyme label="b" type="f" corresp="#s2:l2">dansen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En goede Man, een brave <rhyme label="a" type="m">Man</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hy laat zyn Vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Hy veegt de <rhyme label="c" type="m">Vloer</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Zonder <rhyme label="c" type="m" corresp="#s3:l3">rumoer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die aars niet doet als <rhyme label="b" type="f" corresp="#s3:l2">dansen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een goede man, een braave <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hy laat zyn vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Hy spoeld en <rhyme label="c" type="m">wast</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">En veegt de <rhyme label="c" type="m" corresp="#s4:l3">Kast</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En laat zyn Vrouwtje <rhyme label="b" type="f" corresp="#s4:l2">dansen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een goede man, een braave <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hy laat zyn vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Hy schuurt de <rhyme label="c" type="m">plaat</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">En 's avond <rhyme label="c" type="m" corresp="#s5:l3">laat</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Laat hy zyn Vrouwtje <rhyme label="b" type="f" corresp="#s5:l2">dansen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Een goede man, een braave <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hy laat zyn vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Hy word <rhyme label="c" type="m">gesuld</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Het hoofd <rhyme label="c" type="m" corresp="#s6:l3">gekruld</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Met hoorens onder het <rhyme label="b" type="f" corresp="#s6:l2">dansen</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Een goede man, een braave <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die laat zyn Vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">En na die <rhyme label="c" type="m">pret</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Gaat zy na <rhyme label="c" type="m" corresp="#s7:l3">bed</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Vermoe id van al het <rhyme label="b" type="f" corresp="#s7:l2">dansen</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Een goede man, een braave <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die laat zyn Vrouwtje <rhyme label="b" type="f">dansen</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Die arme <rhyme label="c" type="m">bloed</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Zyn geld en <rhyme label="c" type="m" corresp="#s8:l3">goed</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Verzwierd zyn Wyf met <rhyme label="b" type="f" corresp="#s8:l2">dansen</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Hup, hup, zeid <rhyme label="d" type="m">Jannetje</rhyme>.</l>
      </lg>
