<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156343</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <tags/>
    <melody>Florida zo het wezen mag</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GEnooden, die gekomen <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ter bruyloft om te zijn <rhyme label="a" type="m" corresp="#s1:l1">verblijd</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ick bid wilt stadigh <rhyme label="b" type="f">dencken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Om 't eeuwigh vreughdigh <rhyme label="c" type="m">bruyloftsmael</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat Godt in s'Hemels <rhyme label="c" type="m" corresp="#s1:l4">sael</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sijn lieve Bruydt sal <rhyme label="b" type="f" corresp="#s1:l3">schencken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ey! en vergaept u hier doch <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Aen 't geen ghy smaeckt, oft hoort, oft <rhyme label="a" type="m" corresp="#s2:l1">siet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">'Zijn al maer yd'le <rhyme label="b" type="f">dingen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Te laegh voor 't Christelijcke <rhyme label="c" type="m">gemoedt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat sich met hooger <rhyme label="c" type="m" corresp="#s2:l4">voed</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En voor sulcks moet <rhyme label="b" type="f" corresp="#s2:l3">bedwingen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als u de spijs en dranck wel <rhyme label="a" type="m">smaeckt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Of singers keel u 't oor <rhyme label="a" type="m" corresp="#s3:l1">vermaeckt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wilt dan aendachtigh <rhyme label="b" type="f">dencken</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Soo Godt de Mensch dees soetheydt <rhyme label="c" type="m">geeft</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Die steedts in boosheydt <rhyme label="c" type="m" corresp="#s3:l4">leeft</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Och wat sal hy wel <rhyme label="b" type="f" corresp="#s3:l3">schencken</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Aen die noch vleck noch rimpel <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En steeds aendachtigh suyver <rhyme label="a" type="m" corresp="#s4:l1">leeft</rhyme></l>
        <l id="s4:l3" met="4" type="-+">In heyl'ge <rhyme label="b" type="f">wandelingen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die in 't forneys als goudt sal <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Alst 't alles sal <rhyme label="c" type="m" corresp="#s4:l4">vergaen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De aerd en aerdsche <rhyme label="b" type="f" corresp="#s4:l3">dingen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O onbegrijpelijcke <rhyme label="a" type="m">vreught</rhyme>!</l>
        <l id="s5:l2" met="4" type="-+">O onuytsprekelijck <rhyme label="a" type="m" corresp="#s5:l1">geneught</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">O soetheydt noyt vol <rhyme label="b" type="f">presen</rhyme>!</l>
        <l id="s5:l4" met="4" type="-+">O rijck! o kroon! o hooge <rhyme label="c" type="m">throon</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">O alderschoonste <rhyme label="c" type="m" corresp="#s5:l4">schoon</rhyme>!</l>
        <l id="s5:l6" met="3" type="-+">Dien daer sal eeuwigh <rhyme label="b" type="f" corresp="#s5:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wegh, wegh dan met het aerdtsch <rhyme label="a" type="m">onthael</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ick hoop op beter <rhyme label="a" type="m" corresp="#s6:l1">bruylofts-mael</rhyme></l>
        <l id="s6:l3" met="4" type="-+">My namaels te <rhyme label="b" type="f">vermaken</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Met mijnen lieven <rhyme label="c" type="m">Bruydegom</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">O soete Iesu <rhyme label="c" type="m" corresp="#s6:l4">kom</rhyme>!</l>
        <l id="s6:l6" met="3" type="-+">En laet my't Hemels <rhyme label="b" type="f" corresp="#s6:l3">smaken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Mijn Ziel met vlijdt u derwaerdts <rhyme label="a" type="m">spoet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">En't witte Bruylofts kleedt <rhyme label="a" type="m" corresp="#s7:l1">aendoet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Niet flau, maer ras en <rhyme label="b" type="f">vaerdigh</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Star-ooght gestaegh op 't Hemels <rhyme label="c" type="m">heyl</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Altijdt een oogh in 't <rhyme label="c" type="m" corresp="#s7:l4">seyl</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">De saeck is dubbeldt <rhyme label="b" type="f" corresp="#s7:l3">waerdigh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
