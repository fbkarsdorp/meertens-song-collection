    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ICk Cupido Goden <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die <rhyme label="a" type="m" corresp="#s1:l1">versot</rhyme></l>
        <l id="s1:l3" met="4" type="+-">D’aldersterckster mannen <rhyme label="b" type="f">herten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Iae het cloeckste dat men <rhyme label="c" type="m">vindt</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Maeck ick <rhyme label="c" type="m" corresp="#s1:l4">blindt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Door mijn wulpse dertel <rhyme label="b" type="f" corresp="#s1:l3">perten</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Door mijn wulpse dertel <rhyme label="b" type="f" corresp="#s1:l3">perten</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ist my eer, dat ick een <rhyme label="a" type="m">Landt</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Steeck in <rhyme label="a" type="m" corresp="#s2:l1">brandt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als ick werck Helenae <rhyme label="b" type="f">treken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">D’Hemel kennet mijnen <rhyme label="c" type="m">schicht</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">D’Hel <rhyme label="c" type="m" corresp="#s2:l4">onlicht</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Can ick met mijn vyer <rhyme label="b" type="f" corresp="#s2:l3">ontsteken</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Can ick met mijn vyer <rhyme label="b" type="f" corresp="#s2:l3">ontsteken</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waerom mimert Phoebus <rhyme label="a" type="m">soo</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">End’ <rhyme label="a" type="m" corresp="#s3:l1">Pluto</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Waerom giert Pan door de <rhyme label="b" type="f">berghen</rhyme>?</l>
        <l id="s3:l4" met="4" type="+-">Selfs mijn Moeder treurich <rhyme label="c" type="m">is</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Alles <rhyme label="c" type="m" corresp="#s3:l4">wis</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Can ick met mijn boghe <rhyme label="b" type="f" corresp="#s3:l3">terghen</rhyme>.</l>
        <l id="s3:l7" met="4" type="+-">Can ick met mijn boghe <rhyme label="b" type="f" corresp="#s3:l3">terghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als ick schiet maer eenen <rhyme label="a" type="m">pijl</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Metter <rhyme label="a" type="m" corresp="#s4:l1">ijl</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Can ick haest de Ieucht <rhyme label="b" type="f">ontstellen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Ionghmans krielen dan <rhyme label="c" type="m">terstont</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Om den <rhyme label="c" type="m" corresp="#s4:l4">mont</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Van de Meyskens diese <rhyme label="b" type="f" corresp="#s4:l3">quellen</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">Van de Meyskens diese <rhyme label="b" type="f" corresp="#s4:l3">quellen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Menich Paris noch by <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s5:l2" met="2" type="+-">De deur <rhyme label="a" type="m" corresp="#s5:l1">wacht</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Van sijn soet-suur <rhyme label="b" type="f">uytvercoren</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Oock soo kan ick het <rhyme label="c" type="m">ghemoet</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Door mijn <rhyme label="c" type="m" corresp="#s5:l4">gloet</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Van die weygher-Meyskens <rhyme label="b" type="f" corresp="#s5:l3">storen</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Van die weygher-Meyskens <rhyme label="b" type="f" corresp="#s5:l3">storen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Doch dit alles acht ick <rhyme label="a" type="m">kleyn</rhyme></l>
        <l id="s6:l2" met="2" type="+-">End’ <rhyme label="a" type="m" corresp="#s6:l1">ghemeyn</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wilder oock gheen eer af <rhyme label="b" type="f">trecken</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Dat ick Hercles minlijck <rhyme label="c" type="m">ree</rhyme></l>
        <l id="s6:l5" met="2" type="+-">Spinnen <rhyme label="c" type="m" corresp="#s6:l4">dee</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Oft oock Goden kan <rhyme label="b" type="f" corresp="#s6:l3">verwecken</rhyme>.</l>
        <l id="s6:l7" met="4" type="+-">Oft oock Goden kan <rhyme label="b" type="f" corresp="#s6:l3">verwecken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Meerder eer verdien ick <rhyme label="a" type="m">nu</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Dat ick <rhyme label="a" type="m" corresp="#s7:l1">u</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Anna, die wel seven <rhyme label="b" type="f">jaren</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Mijnen boghe volder <rhyme label="c" type="m">kracht</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Hebt <rhyme label="c" type="m" corresp="#s7:l4">veracht</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Eyndlijck noch met Pauw doe <rhyme label="b" type="f" corresp="#s7:l3">paren</rhyme>.</l>
        <l id="s7:l7" met="4" type="+-">Eyndlijck noch met Pauw doe <rhyme label="b" type="f" corresp="#s7:l3">paren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dijne pruytsheyt, dijn <rhyme label="a" type="m">beval</rhyme></l>
        <l id="s8:l2" met="2" type="+-">
          <rhyme label="a" type="m" corresp="#s8:l1">Liefghetal</rhyme>
        </l>
        <l id="s8:l3" met="4" type="+-">Is nu eenmael <rhyme label="b" type="f">t’ondercomen</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Wat voor Meyskens zijnder <rhyme label="c" type="m">noch</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Die ick <rhyme label="c" type="m" corresp="#s8:l4">toch</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Nu voortaen niet en sal <rhyme label="b" type="f" corresp="#s8:l3">toomen</rhyme>?</l>
        <l id="s8:l7" met="4" type="+-">Nu voortaen niet en sal <rhyme label="b" type="f" corresp="#s8:l3">toomen</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Noch een Anna isser, <rhyme label="a" type="m">welck</rhyme></l>
        <l id="s9:l2" met="2" type="+-">Trots van <rhyme label="a" type="m" corresp="#s9:l1">elck</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Oock mijn pijlen derf <rhyme label="b" type="f">versmaden</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Die ick doch eer langhen <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s9:l5" met="2" type="+-">Sal te <rhyme label="c" type="m" corresp="#s9:l4">spijt</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Met mijns Liefdens dauw <rhyme label="b" type="f" corresp="#s9:l3">versaden</rhyme>.</l>
        <l id="s9:l7" met="4" type="+-">Met mijns Liefdens dauw <rhyme label="b" type="f" corresp="#s9:l3">versaden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Vlecht my Flora eenen <rhyme label="a" type="m">Crans</rhyme></l>
        <l id="s10:l2" met="2" type="+-">Dat ick <rhyme label="a" type="m" corresp="#s10:l1">dans</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Met dees lieves Bruyts soet <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Bruydgom Pauw, daer is u <rhyme label="c" type="m">Bruyt</rhyme></l>
        <l id="s10:l5" met="2" type="+-">Die ick <rhyme label="c" type="m" corresp="#s10:l4">uyt</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Ander, voor u hebb’ <rhyme label="b" type="f" corresp="#s10:l3">ghelesen</rhyme>.</l>
        <l id="s10:l7" met="4" type="+-">Ander, voor u hebb’ <rhyme label="b" type="f" corresp="#s10:l3">ghelesen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Treckt in uwen <rhyme label="a" type="m">Ruytenborgh</rhyme>,</l>
        <l id="s11:l2" met="2" type="+-">Sonder <rhyme label="a" type="m" corresp="#s11:l1">sorgh</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Om mijns pijlens loon te <rhyme label="b" type="f">smaken</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Dommelt, stommelt vrolijck <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s11:l5" met="2" type="+-">Lieflijck <rhyme label="c" type="m" corresp="#s11:l4">paer</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Ofter schoon wat Ruyten <rhyme label="b" type="f" corresp="#s11:l3">braken</rhyme>.</l>
        <l id="s11:l7" met="4" type="+-">Ofter schoon wat Ruyten <rhyme label="b" type="f" corresp="#s11:l3">braken</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Ghy zijt t’saem wel, ick <rhyme label="a" type="m">vertreck</rhyme></l>
        <l id="s12:l2" met="2" type="+-">Tot <rhyme label="a" type="m" corresp="#s12:l1">verweck</rhyme></l>
        <l id="s12:l3" met="4" type="+-">Van dees ander Bruylofts <rhyme label="b" type="f">karen</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">Bruylofts-gasten rontsom <rhyme label="c" type="m">kust</rhyme></l>
        <l id="s12:l5" met="2" type="+-">Metter <rhyme label="c" type="m" corresp="#s12:l4">lust</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Kuskens mach men doch niet <rhyme label="b" type="f" corresp="#s12:l3">sparen</rhyme></l>
        <l id="s12:l7" met="4" type="+-">Kuskens mach men doch niet <rhyme label="b" type="f" corresp="#s12:l3">sparen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
