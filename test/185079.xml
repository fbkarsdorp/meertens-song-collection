<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185079</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Un matin brusquement</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Terwyl wy nu <rhyme label="a" type="m">verheugd</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Den agtsten Maart, in <rhyme label="a" type="m" corresp="#s1:l1">vreugd</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Als eertyds, vieren <rhyme label="b" type="f">mogen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Zo zullen wy, met <rhyme label="a" type="m" corresp="#s1:l1">vreugd</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dan vieren deezen <rhyme label="c" type="m">dag</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van blydschap <rhyme label="b" type="f" corresp="#s1:l3">opgetoogen</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s1:l7">eensgezind</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s1:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s1:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy mogen nu weer <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">echt vergenoegd en <rhyme label="a" type="m" corresp="#s2:l1">bly</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Oranje – strikken <rhyme label="b" type="f">draagen</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Nu leeven wy weer <rhyme label="a" type="m" corresp="#s2:l1">bly</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Daar Neêrlands God, in <rhyme label="c" type="m">vreugd</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Verandren wil ons <rhyme label="b" type="f" corresp="#s2:l3">klaagen</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s2:l7">eensgezind</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s2:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s2:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Prins wierd lang <rhyme label="a" type="m">ontëerd</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gelasterd en <rhyme label="a" type="m" corresp="#s3:l1">verneêrd</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Tot smart van zyne <rhyme label="b" type="f">Vrinden</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Maar, die ’t Heelal <rhyme label="a" type="m" corresp="#s3:l1">regeerd</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Deed hem, in nood en <rhyme label="c" type="m">druk</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Vertroosting <rhyme label="b" type="f" corresp="#s3:l3">ondervinden</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s3:l7">eensgezind</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s3:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s3:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Prins was <rhyme label="a" type="m">welgemoed</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Toen hy in <rhyme label="a" type="m" corresp="#s4:l1">tegenspoed</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En ongeval moest <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">De Prins was <rhyme label="a" type="m" corresp="#s4:l1">welgemoed</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Vertrouwende dat <rhyme label="c" type="m">God</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Hem redding zoude <rhyme label="b" type="f" corresp="#s4:l3">geeven</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s4:l7">eensgezind</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s4:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s4:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En Die heeft hem <rhyme label="a" type="m">gered</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Verhoorende ’t <rhyme label="a" type="m" corresp="#s5:l1">gebed</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Van hem, en zyne <rhyme label="b" type="f">Vrinden</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">God heeft de Prins <rhyme label="a" type="m" corresp="#s5:l1">gered</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Tot vreugd der Vroomen, <rhyme label="c" type="m">die</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Geduurig hem <rhyme label="b" type="f" corresp="#s5:l3">beminden</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s5:l7">eensgezind</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s5:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s5:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dewyl hy deezen <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Tot onze blydschap, <rhyme label="a" type="m" corresp="#s6:l1">mag</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Bereiken veertig <rhyme label="b" type="f">jaaren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Zo laat ons deezen <rhyme label="a" type="m" corresp="#s6:l1">dag</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Zyn Redden danken, <rhyme label="c" type="m">dat</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Hy hem in gunst wou <rhyme label="b" type="f" corresp="#s6:l3">spaaren</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s6:l7">eensgezind</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s6:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s6:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Gy, Die hem heeft <rhyme label="a" type="m">bevryd</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van Rampen, en <rhyme label="a" type="m" corresp="#s7:l1">verblyd</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Door Uwe groote <rhyme label="b" type="f">daaden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Toon verder dat Gy <rhyme label="a" type="m" corresp="#s7:l1">zyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Zyn Helper, wil hem <rhyme label="c" type="m">steeds</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Met zegen <rhyme label="b" type="f" corresp="#s7:l3">overlaaden</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s7:l7">eensgezind</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s7:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s7:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wil toch, tot nut van <rhyme label="a" type="m">Land</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En Kerk, door Uwe <rhyme label="a" type="m" corresp="#s8:l1">Hand</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Goedgunstig hem <rhyme label="b" type="f">geleiden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Wil, met Uw Regte <rhyme label="a" type="m" corresp="#s8:l1">Hand</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Hem hoeden, en voor <rhyme label="c" type="m">druk</rhyme></l>
        <l id="s8:l6" met="3" type="-+">En ramp altoos <rhyme label="b" type="f" corresp="#s8:l3">bevryden</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s8:l7">eensgezind</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s8:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s8:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wil hem, zyn <rhyme label="a" type="m">Gemaalin</rhyme></l>
        <l id="s9:l2" met="3" type="-+">En Kroost, uit tedre <rhyme label="a" type="m" corresp="#s9:l1">min</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Door Uwen Geest <rhyme label="b" type="f">geleiden</rhyme>;</l>
        <l id="s9:l4" met="3" type="-+">Wil toch, uit tedre <rhyme label="a" type="m" corresp="#s9:l1">min</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Dit Vorstelyke <rhyme label="c" type="m">Huis</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Door uw Genâ <rhyme label="b" type="f" corresp="#s9:l3">verblyden</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s9:l7">eensgezind</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s9:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s9:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ei, laat de Prins <rhyme label="a" type="m">verheugd</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Tot aller Braaven <rhyme label="a" type="m" corresp="#s10:l1">vreugd</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Nog veele jaaren <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Wil hem, tot onzen <rhyme label="a" type="m" corresp="#s10:l1">vreugd</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Lang spaaren in uw <rhyme label="c" type="m">gunst</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En heil en voorspoed <rhyme label="b" type="f" corresp="#s10:l3">geeven</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s10:l7">eensgezind</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s10:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s10:l7">Vrind</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Laat hem, zyn <rhyme label="a" type="m">Gemaalin</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En Kroost, voor uwe <rhyme label="a" type="m" corresp="#s11:l1">min</rhyme></l>
        <l id="s11:l3" met="3" type="-+">En zorg, U altoos <rhyme label="b" type="f">looven</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Dat elk hen teder <rhyme label="a" type="m" corresp="#s11:l1">min</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">En met ons, <rhyme label="c" type="m">ongeveinsd</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Steeds roep’, Oranje <rhyme label="b" type="f" corresp="#s11:l3">Boven</rhyme>!</l>
        <l id="s11:l7" met="3" type="-+">Elk die ’s Lands Welvaard <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Zingt met ons <rhyme label="d" type="m" corresp="#s11:l7">eensgezind</rhyme>,</l>
        <l id="s11:l9" met="3" type="-+">Lang moet Prins willem <rhyme label="e" type="f">leeven</rhyme>!</l>
        <l id="s11:l10" met="3" type="-+">Hy is onz’ beste <rhyme label="d" type="m" corresp="#s11:l7">Vrind</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
