    <melody>Mijn hartje is heel doorwond</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SIlvester en <rhyme label="a" type="m">Coridon</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die gingen eens nae de <rhyme label="b" type="f">Linden</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ontrent de <rhyme label="a" type="m" corresp="#s1:l1">Zuyder-bron</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Om Graefjes 't saem te binden Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s1:l4">la</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sy quamen ter plaetsen <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">By eenen seer hoogen <rhyme label="b" type="f">Ceder</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Waer 't seer genuchlijck <rhyme label="a" type="m" corresp="#s2:l1">waer</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Sy gingen sitten neder, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s2:l4">la</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy vleyden haer om een <rhyme label="a" type="m">Kus</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En om haer borsjes te <rhyme label="b" type="f">voelen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Sy sey: ay Coridon <rhyme label="a" type="m" corresp="#s3:l1">flus</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ick voel mijn hertje woelen, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s3:l4">la</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy sagh haer soo vriendelijck <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En voelden daer met sijn <rhyme label="b" type="f">Handen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Of 't Polsje began te <rhyme label="a" type="m" corresp="#s4:l1">slaen</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Maer ach' haer Hartje branden, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s4:l4">la</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Haer Borsjes heeft hy der <rhyme label="a" type="m">ontdaen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daer sagh hy de blancke <rhyme label="b" type="f">Leden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Met twee roo Tepeltjes <rhyme label="a" type="m" corresp="#s5:l1">staen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En voelde voort nae beneden, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s5:l4">la</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Silvestertje (och mijn <rhyme label="a" type="m">Lief</rhyme>)</l>
        <l id="s6:l2" met="3" type="-+">Sey Coridon met een <rhyme label="b" type="f">Kusje</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ay! doet eens mjin <rhyme label="a" type="m" corresp="#s6:l1">gerief</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Mijn heet op-steygent lufje, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s6:l4">la</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Och seyt sy mijn <rhyme label="a" type="m">Coridon</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Mins-tochje moet icker <rhyme label="b" type="f">bedwingen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">De Ceder en dese <rhyme label="a" type="m" corresp="#s7:l1">Bron</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">De Vogels souden 't uyt-singen, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s7:l4">la</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Geen Vogeltjes noch <rhyme label="a" type="m">Geboomt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Geen water sal ons <rhyme label="b" type="f">beklappen</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Het Beeckje dat ruysend' <rhyme label="a" type="m" corresp="#s8:l1">stroomt</rhyme></l>
        <l id="s8:l4" met="4" type="-+">En sal ons niet beklappen, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s8:l4">la</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maer Coridon sal u <rhyme label="a" type="m">trouw</rhyme></l>
        <l id="s9:l2" met="3" type="-+">My nimmer of oyt <rhyme label="b" type="f">begeven</rhyme>?</l>
        <l id="s9:l3" met="3" type="-+">Dat ick als Herders <rhyme label="a" type="m" corresp="#s9:l1">Vrouw</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Ay u magh zijn verheven, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s9:l4">la</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Betroutme dat vryelijck <rhyme label="a" type="m">toe</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">Silvestertje, (och! mijn <rhyme label="b" type="f">waerde</rhyme>;)</l>
        <l id="s10:l3" met="3" type="-+">Soo ick u oyt ontrouw <rhyme label="a" type="m" corresp="#s10:l1">doe</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Soo schen my Locht, en Aerde, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s10:l4">la</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O Hemel wat ick daer <rhyme label="a" type="m">sagh</rhyme>!</l>
        <l id="s11:l2" met="3" type="-+">Sy speelden (het was een <rhyme label="b" type="f">wonder</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Tot Coridon boven <rhyme label="a" type="m" corresp="#s11:l1">lagh</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Silvestertje raeckten onder, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s11:l4">la</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">De Blaertjes maeckten <rhyme label="a" type="m">gedril</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Mit gaf Silvester een <rhyme label="b" type="f">suchje</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Doch Cupido sat <rhyme label="a" type="m" corresp="#s12:l1">stil</rhyme></l>
        <l id="s12:l4" met="4" type="-+">En sagh dit geestigh kluchje, Fa, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Fa, la, la, la, la, la, <rhyme label="c" type="m" corresp="#s12:l4">la</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
