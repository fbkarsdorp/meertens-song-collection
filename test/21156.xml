    <melody>Amarilli mia bella</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">AL-bestierder der <rhyme label="a" type="f">Menschen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Groot-maghtighst <rhyme label="b" type="m">Bruylofs-Heer</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Die al ons <rhyme label="a" type="f" corresp="#s1:l1">wenschen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Vyt-voert na u <rhyme label="b" type="m" corresp="#s1:l2">begeer</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Opent ons mondt, laet onsen lof-zangh <rhyme label="c" type="f">rijsen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door al u <rhyme label="d" type="f">Hemel-paden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Om die <rhyme label="d" type="f" corresp="#s1:l6">weldaden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die wy hier sien, te <rhyme label="c" type="f" corresp="#s1:l5">prijsen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">En u <rhyme label="e" type="f">name</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">en u <rhyme label="e" type="f" corresp="#s1:l9">name</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">en u <rhyme label="e" type="f" corresp="#s1:l9">name</rhyme></l>
        <l id="s1:l12" met="2" type="+-">Eer <rhyme label="c" type="f" corresp="#s1:l5">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wie kan waerdigh <rhyme label="a" type="f">uyt-spreecken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">'t Goet dat dees twee <rhyme label="b" type="m">geschiet</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Die ghy met <rhyme label="a" type="f" corresp="#s2:l1">beecken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van uwe gunst <rhyme label="b" type="m" corresp="#s2:l2">begiet</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">In haer gemoet hebt ghy Heer <rhyme label="c" type="f">af-gesonden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">V Heyl'ge Geest, die <rhyme label="d" type="f">beyde</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Soo t'samen <rhyme label="d" type="f" corresp="#s2:l6">leyde</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat sy met hart en <rhyme label="c" type="f" corresp="#s2:l5">monde</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Nu met <rhyme label="e" type="f">trouwe</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">nu met <rhyme label="e" type="f" corresp="#s2:l9">trouwe</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">nu met <rhyme label="e" type="f" corresp="#s2:l9">trouwe</rhyme></l>
        <l id="s2:l12" met="2" type="+-">Zijn <rhyme label="c" type="f" corresp="#s2:l5">verbonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Bint haer zielen te <rhyme label="a" type="f">gader</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met ware liefd' en <rhyme label="b" type="m">deught</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">En brenght haer <rhyme label="a" type="f" corresp="#s3:l1">nader</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Tot uw' <rhyme label="b" type="m" corresp="#s3:l2">genaden-vreught</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Wijst haer den wegh, die sy met heyl'ge <rhyme label="c" type="f">ganghen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En met <rhyme label="d" type="f">gelovens-hoopen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Christelijck <rhyme label="d" type="f" corresp="#s3:l6">loopen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Tot datse na 't <rhyme label="c" type="f" corresp="#s3:l5">verlangen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-"><rhyme label="e" type="f">d'Hemel-kroone</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-"><rhyme label="e" type="f" corresp="#s3:l9">d'Hemel-kroone</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">
          <rhyme label="e" type="f" corresp="#s3:l9">d'Hemel-kroone</rhyme>
        </l>
        <l id="s3:l12" met="2" type="+-">Eeuwigh <rhyme label="c" type="f" corresp="#s3:l5">vangen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
