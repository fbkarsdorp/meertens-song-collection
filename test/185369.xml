    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pronkje van de maagden</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">AAn een rouwig <rhyme label="a" type="f">herte</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Toont, ô Heer! <rhyme label="b" type="f">genaden</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Dat lijdt zware <rhyme label="a" type="f" corresp="#s1:l1">smerte</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Over zijn <rhyme label="b" type="f" corresp="#s1:l2">misdaden</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Wilt dog niet <rhyme label="b" type="f" corresp="#s1:l2">versmaden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Een Ziel die schult <rhyme label="c" type="m">bekent</rhyme>:</l>
        <l id="s1:l7" met="2" type="+-">Mijn zugten, mijn <rhyme label="d" type="f">dugten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Mijn tranen vermanen, u mijn <rhyme label="c" type="m" corresp="#s1:l6">ellendt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Door des Werelts <rhyme label="a" type="f">smeecken</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">En des Vyands <rhyme label="b" type="f">raden</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Ben ik, Heer! <rhyme label="a" type="f" corresp="#s2:l1">geweecken</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Van u soete <rhyme label="b" type="f" corresp="#s2:l2">paden</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">En heel <rhyme label="b" type="f" corresp="#s2:l2">onberaden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Heb ik, die eerst was <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Niet schroomen, te <rhyme label="d" type="f">komen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Door sonden, gebonden, in <rhyme label="c" type="m" corresp="#s2:l6">slaverny</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Duysent quae <rhyme label="a" type="f">manieren</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Passien en <rhyme label="b" type="f">sonden</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Nu mijn ziel <rhyme label="a" type="f" corresp="#s3:l1">regieren</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Die tot allen <rhyme label="b" type="f" corresp="#s3:l2">stonden</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Trecken als <rhyme label="b" type="f" corresp="#s3:l2">gebonden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Mijn ongeluckig <rhyme label="c" type="m">Mensch</rhyme>:</l>
        <l id="s3:l7" met="2" type="+-">En reden, <rhyme label="d" type="f">vertreden</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Als slaven, doen draven, naer hunnen <rhyme label="c" type="m" corresp="#s3:l6">wensch</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Heer gy zijt mijn <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">En ik uwen <rhyme label="b" type="f">Soone</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Hoe wel ik <rhyme label="a" type="f" corresp="#s4:l1">misdader</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Verre ben gaen <rhyme label="b" type="f" corresp="#s4:l2">woonen</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Wilt tog aen mijn <rhyme label="b" type="f" corresp="#s4:l2">toonen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">U Vaderlijke <rhyme label="c" type="m">sugt</rhyme>:</l>
        <l id="s4:l7" met="2" type="+-">U oogen, wilt <rhyme label="d" type="f">hoogen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Op ’t hert, vol smerte, dat tot u <rhyme label="c" type="m" corresp="#s4:l6">vlugt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Wilt de snoode <rhyme label="a" type="f">banden</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Die mijn arme <rhyme label="b" type="f">ziele</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">t’Haerder groote <rhyme label="a" type="f" corresp="#s5:l1">schande</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Lang gebonden <rhyme label="b" type="f" corresp="#s5:l2">hielen</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Breecken en <rhyme label="b" type="f" corresp="#s5:l2">vernielen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En kragt verleenen <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">U raden, en <rhyme label="d" type="f">paden</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">U deugden, met vreugden, te volgen <rhyme label="c" type="m" corresp="#s5:l6">bly</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Toont dog eens hoe <rhyme label="a" type="f">machtig</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Dat zijn uwe <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">En u hulpe <rhyme label="a" type="f" corresp="#s6:l1">krachtig</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Tegen mijn <rhyme label="b" type="f" corresp="#s6:l2">vyanden</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Brengtse heel tot <rhyme label="b" type="f" corresp="#s6:l2">schande</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die mijn ziel soeken <rhyme label="c" type="m">Heer</rhyme>:</l>
        <l id="s6:l7" met="2" type="+-">’t Sal wesen, van <rhyme label="d" type="f">vreesen</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">En klagen, ontslagen, door u <rhyme label="c" type="m" corresp="#s6:l6">verweer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Laet mijn u <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Altijd sonder <rhyme label="b" type="f">flouwen</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">En in ’t hert van <rhyme label="a" type="f" corresp="#s7:l1">binnen</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">U altijd <rhyme label="b" type="f" corresp="#s7:l2">behouwen</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Gy zijt mijn <rhyme label="b" type="f" corresp="#s7:l2">betrouwen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daer geheel mijn hoop op <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s7:l7" met="2" type="+-">Mijn Voeder, <rhyme label="d" type="f">Behoeder</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Weldader, en Vader, mijn <rhyme label="c" type="m" corresp="#s7:l6">toeverlaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
