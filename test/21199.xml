  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoe Neeltje, benje <rhyme label="a" type="m">sieck</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Wel Heer! hoe sien jou <rhyme label="b" type="f">kaecken</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Soo schrael, soo hol, soo <rhyme label="a" type="m" corresp="#s1:l1">blieck</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Ghy moet gheen hartseer <rhyme label="b" type="f" corresp="#s1:l2">maecken</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Jy soudt in Teeringh <rhyme label="b" type="f" corresp="#s1:l2">raecken</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Of treurt ghy om de <rhyme label="c" type="m">Knecht</rhyme>?</l>
        <l id="s1:l7" met="3" type="-+">Jy siet ghelijck een <rhyme label="b" type="f" corresp="#s1:l2">Laecken</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wat schort jou Neeltje, <rhyme label="c" type="m" corresp="#s1:l6">seght</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Iae Trijn, al heb ick <rhyme label="a" type="m">deur</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Mijn droefheydt wel <rhyme label="b" type="f">ghekreten</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">D'oorsaeck daer ick om <rhyme label="a" type="m" corresp="#s2:l1">treur</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Heeft noyt gheen mensch <rhyme label="b" type="f" corresp="#s2:l2">gheweten</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Maer nou wy zijn <rhyme label="b" type="f" corresp="#s2:l2">gheseten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En t'samen Speelnoots <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Soo sal ick mijn <rhyme label="b" type="f" corresp="#s2:l2">secreten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Iou gaen verhalen, <rhyme label="c" type="m" corresp="#s2:l6">Trijn</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doen wy buyten de <rhyme label="a" type="m">Stadt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">(Of 't jou noch heught) eens <rhyme label="b" type="f">saten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daer Siueuwert by ons <rhyme label="a" type="m" corresp="#s3:l1">sat</rhyme></l>
        <l id="s3:l4" met="3" type="-+">('t Swijghen mach toch niet <rhyme label="b" type="f" corresp="#s3:l2">baten</rhyme>)</l>
        <l id="s3:l5" met="3" type="-+">Die heeft my met schoon <rhyme label="b" type="f" corresp="#s3:l2">praten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ghevrijt vier of vijf <rhyme label="c" type="m">Iaer</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Nou heeft hy my <rhyme label="b" type="f" corresp="#s3:l2">verlaten</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En ick gae by hem <rhyme label="c" type="m" corresp="#s3:l6">Swaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Jy Swaer! wat ick <rhyme label="a" type="m">ontstel</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Iae Trijn, dat doet mijn <rhyme label="b" type="f">trueren</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Weet dat jou Moeder <rhyme label="a" type="m" corresp="#s4:l1">wel</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Of yemandt van jou <rhyme label="b" type="f" corresp="#s4:l2">Bueren</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">Gheen mensch tot desen <rhyme label="b" type="f" corresp="#s4:l2">uren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Als ghy, in <rhyme label="c" type="m">vryigheydt</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Maer of het langh sal <rhyme label="b" type="f" corresp="#s4:l2">dueren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dat waer te veel <rhyme label="c" type="m" corresp="#s4:l6">gheseydt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Seghtet jou Moeder <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Maer wilt haer niet <rhyme label="b" type="f">beswaren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En raedt haer datse <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Met jou laet speele <rhyme label="b" type="f" corresp="#s5:l2">varen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wy sullen 't dan wel <rhyme label="b" type="f" corresp="#s5:l2">klaren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wanneer jy maer <rhyme label="c" type="m">verlost</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Soo sal ick jou <rhyme label="b" type="f" corresp="#s5:l2">bewaren</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Besorghen voor de <rhyme label="c" type="m" corresp="#s5:l6">kost</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Trijntje sou dat wel <rhyme label="a" type="m">gaen</rhyme>?</l>
        <l id="s6:l2" met="3" type="-+">Wat sou daer aen <rhyme label="b" type="f">ghebreecken</rhyme>?</l>
        <l id="s6:l3" met="3" type="-+">Dat wordt wel meer <rhyme label="a" type="m" corresp="#s6:l1">ghedaen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Daer wordt niet op <rhyme label="b" type="f" corresp="#s6:l2">ghekeecken</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Men kan dat wel <rhyme label="b" type="f" corresp="#s6:l2">besteecken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Al reysen wy niet <rhyme label="c" type="m">veer</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Jy krijght in vijf ses <rhyme label="b" type="f" corresp="#s6:l2">weecken</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Suyver jou maeghdom <rhyme label="c" type="m" corresp="#s6:l6">weer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Jy spreeckt nae u <rhyme label="a" type="m">verstandt</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Maer 't Kindt, waer daer mee <rhyme label="b" type="f">henen</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Dat most nae <rhyme label="a" type="m" corresp="#s7:l1">Water-landt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Of erghens in de <rhyme label="b" type="f" corresp="#s7:l2">Veenen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Soo hoor jy 't hier niet <rhyme label="b" type="f" corresp="#s7:l2">weenen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Of schreyen om de <rhyme label="c" type="m">Mam</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En als 't de Min sal <rhyme label="b" type="f" corresp="#s7:l2">speenen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Besteet 'et <rhyme label="c" type="m" corresp="#s7:l6">t'Amsterdam</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wel Neeltje ick moet <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">De Klock is over <rhyme label="b" type="f">vijven</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Hoor Trijntje noch een <rhyme label="a" type="m" corresp="#s8:l1">woordt</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Ey laet het by u <rhyme label="b" type="f" corresp="#s8:l2">blijven</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Praet daer van met gheen <rhyme label="b" type="f" corresp="#s8:l2">Wijven</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">Neen, steldt dat uyt <rhyme label="c" type="m">ghedacht</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Hoe sal mijn Besje <rhyme label="b" type="f" corresp="#s8:l2">kijven</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Nou Neeltje goeden <rhyme label="c" type="m" corresp="#s8:l6">nacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
