        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Cette Anne si belle</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">KOmt mijn Oyen <rhyme label="a" type="f">Lamm’ren</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Treet te veltwaert <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Staeckt het droevigh <rhyme label="a" type="f" corresp="#s1:l1">jamm’ren</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Van uw’ <rhyme label="b" type="m" corresp="#s1:l2">Harderin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Gaet en knabbelt <rhyme label="a" type="f">blaetjes</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Langhs de dorre <rhyme label="b" type="m">Hey</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Terwijl ick dees <rhyme label="a" type="f" corresp="#s2:l1">paetjes</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Tree met naer <rhyme label="b" type="m" corresp="#s2:l2">geschrey</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Al dees Beemden en <rhyme label="a" type="f">Bomen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Zijn met my <rhyme label="b" type="m">begaen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Ia dees <rhyme label="a" type="f" corresp="#s3:l1">water-stroomen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Blyven stille <rhyme label="b" type="m" corresp="#s3:l2">staen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Al dees wilde <rhyme label="a" type="f">dieren</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Worden tam en <rhyme label="b" type="m">mack</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Sy niet <rhyme label="a" type="f" corresp="#s4:l1">tierelieren</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Vreughdigh by op een <rhyme label="b" type="m" corresp="#s4:l2">tack</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Mits sy vreesent <rhyme label="a" type="f">derven</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Mijn trouw Harder <rhyme label="b" type="m">Pan</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Soo moet ick ’t <rhyme label="a" type="f" corresp="#s5:l1">besterven</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Komt hy niet meer <rhyme label="b" type="m" corresp="#s5:l2">an</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Tuygt vry Noortse <rhyme label="a" type="f">stranden</rhyme></l>
        <l id="s6:l2" met="3" type="+-">En ghy <rhyme label="b" type="m">Amstel-kust</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Oft mijn hart niet <rhyme label="a" type="f" corresp="#s6:l1">branden</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Nae mijn Harders <rhyme label="b" type="m" corresp="#s6:l2">lust</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">En ghy mede <rhyme label="a" type="f">wooningh</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Daer men heeft <rhyme label="b" type="m">gespeelt</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">De droeve <rhyme label="a" type="f" corresp="#s7:l1">vertoningh</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Van Dorethas <rhyme label="b" type="m" corresp="#s7:l2">beelt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Tot de duyster <rhyme label="a" type="f">wolcken</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Deckten d’Aertrijck, <rhyme label="b" type="m">dan</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Brochten Venus <rhyme label="a" type="f" corresp="#s8:l1">tolcken</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Ons vreught weder <rhyme label="b" type="m" corresp="#s8:l2">an</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Tot Auroraes <rhyme label="a" type="f">luyster</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Klopten voor het <rhyme label="b" type="m">glas</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">En Morpheus <rhyme label="a" type="f" corresp="#s9:l1">duyster</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Gantsch verdweenen <rhyme label="b" type="m" corresp="#s9:l2">was</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Nu so moet ick <rhyme label="a" type="f">treuren</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Eensaem vol <rhyme label="b" type="m">geween</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Mach ’t my niet <rhyme label="a" type="f" corresp="#s10:l1">gebeuren</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Dat wy worden <rhyme label="b" type="m" corresp="#s10:l2">een</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Adieu ontrou <rhyme label="a" type="f">Harder</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">Spiegelt u aen <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Nimphjes, eer ghy <rhyme label="a" type="f" corresp="#s11:l1">varder</rhyme></l>
        <l id="s11:l4" met="3" type="+-">Valt in <rhyme label="b" type="m" corresp="#s11:l2">minnery</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
