    <melody>Zoetje laat mij altoos vrijen</melody>
  <author>Hermanus van den Burg</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ZOetje laat my altoos <rhyme label="a" type="f">vryen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Houw my altoos vo or den <rhyme label="b" type="m">zot</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Doe my slechts gemaatigt <rhyme label="a" type="f" corresp="#s1:l1">lyen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dan wens ik geen beter <rhyme label="b" type="m" corresp="#s1:l2">lot</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Want in 't begee <rhyme label="c" type="f">ren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Steekt meer vreugd dan in 't <rhyme label="b" type="m" corresp="#s1:l2">genot</rhyme>.</l>
      </lg>
    </lg>
  </text>
