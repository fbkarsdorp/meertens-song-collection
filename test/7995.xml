    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 009 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEer ick wil u uyt 'sherten <rhyme label="a" type="m">gront</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Prijsen en over al doen <rhyme label="a" type="m" corresp="#s1:l1">cont</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">V wonderen in allen <rhyme label="b" type="f">wijcken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die niet en zijn om <rhyme label="b" type="f" corresp="#s1:l3">vergelijcken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In U wil ick wesen <rhyme label="a" type="m">verblijt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dat is mijn vreucht tot deser <rhyme label="a" type="m" corresp="#s2:l1">tijdt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Vwen naem schoon wil ick oock <rhyme label="b" type="f">prijsen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">End' u met lof-sangh eer <rhyme label="b" type="f" corresp="#s2:l3">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Om dat door uwe groote <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Mijn vyant wijckt en vliet met <rhyme label="a" type="m" corresp="#s3:l1">kracht</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En dat hy neder leyt <rhyme label="b" type="f">geslaghen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Door u aenschijn zwaer om <rhyme label="b" type="f" corresp="#s3:l3">verdragen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy zijt soo goedt in desen <rhyme label="a" type="m">standt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat ghy mijn saeck neemt by der <rhyme label="a" type="m" corresp="#s4:l1">hant</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ghy sit tot mijn hulpe <rhyme label="b" type="f">volveerdigh</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Op den stoel als Rechter <rhyme label="b" type="f" corresp="#s4:l3">rechtveerdigh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mijn vyanden hebt ghy <rhyme label="a" type="m">verdaen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En de boos' al haest doen <rhyme label="a" type="m" corresp="#s5:l1">vergaen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ghy hebt haer gedachtenis <rhyme label="b" type="f">prachtigh</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Te niet ghedaen door u handt <rhyme label="b" type="f" corresp="#s5:l3">krachtigh</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wel aen ghy loos mensch wel <rhyme label="a" type="m">bekent</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Hebt ghy u voornemen <rhyme label="a" type="m" corresp="#s6:l1">volent</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">En vernielt ons steden <rhyme label="b" type="f">verheven</rhyme>?</l>
        <l id="s6:l4" met="4" type="-+">Is haren naem gantsch t'onder <rhyme label="b" type="f" corresp="#s6:l3">bleven</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Och neen: want Godt met grooter <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die eeuwigh heerschet hoogh <rhyme label="a" type="m" corresp="#s7:l1">gheacht</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Is op den Stoel soo 't is <rhyme label="b" type="f">gebleken</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Gheseten, om 't recht uyt te <rhyme label="b" type="f" corresp="#s7:l3">spreken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer sal hy rechten met <rhyme label="a" type="m">bescheyt</rhyme></l>
        <l id="s8:l2" met="4" type="-+">D'aertrijck dat hier beneden <rhyme label="a" type="m" corresp="#s8:l1">leyt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Weghende de saecken met <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Van alle Menschen hier <rhyme label="b" type="f" corresp="#s8:l3">beneden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dan werdt Godt de toevlucht <rhyme label="a" type="m">alleyn</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Des armen diemen acht seer <rhyme label="a" type="m" corresp="#s9:l1">kleyn</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Ia zijn toevlucht die hem in 't <rhyme label="b" type="f">lijden</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Sal verquicken end' doen <rhyme label="b" type="f" corresp="#s9:l3">verblijden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dies zy dien ghy Heer u maeckt <rhyme label="a" type="m">cont</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Sullen op u vast staen <rhyme label="a" type="m" corresp="#s10:l1">ghegront</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Want die tot u gaen Heer <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Sullen hier niet verlaten <rhyme label="b" type="f" corresp="#s10:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Looft nu met lof-sangen seer <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Godt die tot Zyon woont <rhyme label="a" type="m" corresp="#s11:l1">eenpaer</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Vertelt zijn groote <rhyme label="b" type="f">wonderwercken</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Maeckt dats' alle Menschen <rhyme label="b" type="f" corresp="#s11:l3">bemercken</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Hy gedenckt aen der vromen <rhyme label="a" type="m">bloet</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">End' wreeckt dat met een gram <rhyme label="a" type="m" corresp="#s12:l1">gemoet</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Dat geschrey wil hy niet <rhyme label="b" type="f">verachten</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Der benauder, noch oock haer <rhyme label="b" type="f" corresp="#s12:l3">klachten</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Heer, ghy die mijn Godt zijt in <rhyme label="a" type="m">noot</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Siet aen mijn kruys end' lijden <rhyme label="a" type="m" corresp="#s13:l1">groot</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Dat mijn vyanden my op <rhyme label="b" type="f">dringhen</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Wt den wegh des doodts wilt my <rhyme label="b" type="f" corresp="#s13:l3">bringen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Dat ick midden in u <rhyme label="a" type="m">gemeen</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Vwen lof singh', end' anders <rhyme label="a" type="m" corresp="#s14:l1">geen</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Zijnde verblijdt, end' oock <rhyme label="b" type="f">gedachtigh</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Dat ghy my verlost hebt <rhyme label="b" type="f" corresp="#s14:l3">waerachtich</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">De boose zijn haestelijck <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">In haer stricken komen ten <rhyme label="a" type="m" corresp="#s15:l1">val</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Haer voeten zijn in 't net <rhyme label="b" type="f">ghevanghen</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Dat sy voor my hadden <rhyme label="b" type="f" corresp="#s15:l3">ghehanghen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Soo is Godt geworden <rhyme label="a" type="m">bekent</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Hebbende dit oordeel <rhyme label="a" type="m" corresp="#s16:l1">ghe-ent</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Daer in dat de schalck heeft <rhyme label="b" type="f">bevonden</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">'Tquaedt sijner listen niet om <rhyme label="b" type="f" corresp="#s16:l3">gronden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Dits seecker dat de boose <rhyme label="a" type="m">saen</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Haest sullen vallen end' <rhyme label="a" type="m" corresp="#s17:l1">vergaen</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Godt sal die straffen <rhyme label="b" type="f">onvertogen</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Die hem niet en hebben voor <rhyme label="b" type="f" corresp="#s17:l3">ooghen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Maer de Mensch ootmoedigh, o <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Sult ghy vergeten <rhyme label="a" type="m" corresp="#s18:l1">nemmermeer</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Sijn hope die hier is <rhyme label="b" type="f">mispresen</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">En sal by u niet ydel <rhyme label="b" type="f" corresp="#s18:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">O Heer die daer eeuwighlijck <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Maeckt dat der boosen herte <rhyme label="a" type="m" corresp="#s19:l1">beeft</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Doet hen verstaen, dit is mijn <rhyme label="b" type="f">wenschen</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">Dat sy niet zijn dan swacke <rhyme label="b" type="f" corresp="#s19:l3">Menschen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
