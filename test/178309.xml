<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178309</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Wilhelmus</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Juicht nog ons eerlyk <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Al moet Euroop <rhyme label="a" type="f" corresp="#s1:l1">aanschouwen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hoe men zyn Naneef <rhyme label="b" type="m" corresp="#s1:l2">tart</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Dien Prince van <rhyme label="c" type="f">Oranje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door zyne deugd <rhyme label="d" type="m">bekend</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Nooit heeft hy om <rhyme label="c" type="f" corresp="#s1:l5">Brittanje</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Zyn Eed en Plicht <rhyme label="d" type="m" corresp="#s1:l6">geschend</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In God is zyn <rhyme label="a" type="f">vertrouwen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zyn hart voor 't <rhyme label="b" type="m">Vaderland</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Laat vry die Vorst dan <rhyme label="a" type="f" corresp="#s2:l1">bouwen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op 's Hemels <rhyme label="b" type="m" corresp="#s2:l2">onderstand</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">'Er zyn in onze <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Nog Mannen, kloek en <rhyme label="d" type="m">fier</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die alles zullen <rhyme label="c" type="f" corresp="#s2:l5">waagen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Voor 't vrye <rhyme label="d" type="m" corresp="#s2:l6">Staatsbestier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laat vry den helhond <rhyme label="a" type="f">bassen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wy schroomen geen <rhyme label="b" type="m">geschreeuw</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Geen hond is <rhyme label="a" type="f" corresp="#s3:l1">opgewassen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Te stryden met een <rhyme label="b" type="m" corresp="#s3:l2">Leeuw</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">w'Erkennen <rhyme label="c" type="f">Landverraders</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In een geschoor dat <rhyme label="d" type="m">woedt</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Maar Neêrlands echte <rhyme label="c" type="f" corresp="#s3:l5">Vaders</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In Prinsen van dat <rhyme label="d" type="m" corresp="#s3:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Laat nu de duivel <rhyme label="a" type="f">bloozen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die 't zaat van oproer <rhyme label="b" type="m">zaait</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hier worden schoone <rhyme label="a" type="f" corresp="#s4:l1">roozen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In Hollands Tuin <rhyme label="b" type="m" corresp="#s4:l2">gemaait</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Nu wy een <rhyme label="c" type="f">Vaderlander</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De Vorstelyke <rhyme label="d" type="m">jeugd</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">By onzen Eendragts <rhyme label="c" type="f" corresp="#s4:l5">stander</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zien leiden na de <rhyme label="d" type="m" corresp="#s4:l6">deugd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een Edle <rhyme label="a" type="f">Hooggebooren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Een Lynde vroom en <rhyme label="b" type="m">trouw</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Een Vriend der <rhyme label="a" type="f" corresp="#s5:l1">Tempelchooren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Een Zuil van 't <rhyme label="b" type="m" corresp="#s5:l2">Staatsgebouw</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zal voor de Zoonen <rhyme label="c" type="f">waaken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Geteeld uit Nassouws <rhyme label="d" type="m">Bloed</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En Willems lof <rhyme label="c" type="f" corresp="#s5:l5">volmaaken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat dit geen Vreemdling <rhyme label="d" type="m" corresp="#s5:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Myn Vrienden! laat ons <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ofschoon de Laster <rhyme label="b" type="m">blaft</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ofschoon de Prins moet drin <rhyme label="a" type="f" corresp="#s6:l1">en</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat men hem recht <rhyme label="b" type="m" corresp="#s6:l2">verschaft</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ofschoon men 's Prinsen <rhyme label="c" type="f">Vrinden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Veracht tot in de <rhyme label="d" type="m">Hel</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Geen vrees moet lippen <rhyme label="c" type="f" corresp="#s6:l5">binden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Die 't echter meenen <rhyme label="d" type="m" corresp="#s6:l6">wel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Vaart voort, schoon men <rhyme label="a" type="f">geschriften</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Vervult van <rhyme label="b" type="m">dankbaarheid</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Vergalt, door booze <rhyme label="a" type="f" corresp="#s7:l1">driften</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En 't kwaad word <rhyme label="b" type="m" corresp="#s7:l2">uitgeleid</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wen men den Prins wil <rhyme label="c" type="f">danken</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die meer doet dan zyn <rhyme label="d" type="m">plicht</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Ve vangt het door uw <rhyme label="c" type="f" corresp="#s7:l5">klanken</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En zingt dit vrolyk <rhyme label="d" type="m" corresp="#s7:l6">dicht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Aan Steedsche <rhyme label="a" type="f">Magistraaten</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Staat u wel danken <rhyme label="b" type="m">vry</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">Zoud gy Oranje <rhyme label="a" type="f" corresp="#s8:l1">haaten</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Daar hy uw Redder <rhyme label="b" type="m" corresp="#s8:l2">zy</rhyme>?</l>
        <l id="s8:l5" met="3" type="-+">Neen, trouwe <rhyme label="c" type="f">Burgerye</rhyme>!</l>
        <l id="s8:l6" met="3" type="-+">Voldoe uw dankbaar <rhyme label="d" type="m">hart</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Wil Hem uw Zangen <rhyme label="c" type="f" corresp="#s8:l5">wye</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Vervloekt al wie hem <rhyme label="d" type="m" corresp="#s8:l6">smart</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Niets, niets moet uw <rhyme label="a" type="f">vertzaagen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dank Willem voor zyn <rhyme label="b" type="m">trouw</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Herdenk aan d'oude <rhyme label="a" type="f" corresp="#s9:l1">dagen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En 't wag'lend <rhyme label="b" type="m" corresp="#s9:l2">Staatsgebouw</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Wy moeten hem <rhyme label="c" type="f">verjaaren</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Al is 't maar met een <rhyme label="d" type="m">Lied</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Een Vaderlief ziet <rhyme label="c" type="f" corresp="#s9:l5">gaaren</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Al wat het Kind hem <rhyme label="d" type="m" corresp="#s9:l6">bied</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">O God! wil ons <rhyme label="a" type="f">ontdekken</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Wie dat de Monsters <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Die hem zyn leed <rhyme label="a" type="f" corresp="#s10:l1">verwekken</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Hem, Neêrlands <rhyme label="b" type="m" corresp="#s10:l2">oogenlyn</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">En ach, verschoon dees' <rhyme label="c" type="f">galmen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Niet ik, zy zyn in <rhyme label="d" type="m">schuld</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">'Er zyn ook nog Vloek <rhyme label="c" type="f" corresp="#s10:l5">psalmen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">In uwe Kerk <rhyme label="d" type="m" corresp="#s10:l6">geduld</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Roey uit, roey uit de <rhyme label="a" type="f">boozen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">De haters van zyn <rhyme label="b" type="m">Huis</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Straf, straf de <rhyme label="a" type="f" corresp="#s11:l1">Goddeloozen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Verdelg dat <rhyme label="b" type="m" corresp="#s11:l2">Vloekgespuis</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Dat, eer Hy weer <rhyme label="c" type="f">Verjaare</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En ik weer Zangen <rhyme label="d" type="m">biê</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Hy wel zyn Vyand <rhyme label="c" type="f" corresp="#s11:l5">spaare</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Maar hem vernedert <rhyme label="d" type="m" corresp="#s11:l6">zie</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Aan u, ô <rhyme label="a" type="f">Nederlanders</rhyme>!</l>
        <l id="s12:l2" met="3" type="-+">Aan u, o vry <rhyme label="b" type="m">Geslacht</rhyme>!</l>
        <l id="s12:l3" met="3" type="-+">Zweer ik by <rhyme label="a" type="f" corresp="#s12:l1">d'Eendragts-standers</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Dit hoore <rhyme label="b" type="m" corresp="#s12:l2">d'Oppermacht</rhyme>;</l>
        <l id="s12:l5" met="3" type="-+">Geen Prins is ooit <rhyme label="c" type="f">gebooren</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Die meer u heeft <rhyme label="d" type="m">bemind</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Laat dan uw blydschap <rhyme label="c" type="f" corresp="#s12:l5">hooren</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Weest Prinsch, niet Fransch <rhyme label="d" type="m" corresp="#s12:l6">gezind</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Neem Hem voor echten <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">En Grootvaers echten <rhyme label="b" type="m">Zoon</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Wie, Vrienden! is ons <rhyme label="a" type="f" corresp="#s13:l1">nader</rhyme>?</l>
        <l id="s13:l4" met="3" type="-+">Wie helpt de Deugd ten <rhyme label="b" type="m" corresp="#s13:l2">troon</rhyme>?</l>
        <l id="s13:l5" met="3" type="-+">De Vryheid gaat <rhyme label="c" type="f">verlooren</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Waakt Hy niet voor dat <rhyme label="d" type="m">Pand</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Waar Willem 't snoer wit <rhyme label="c" type="f" corresp="#s13:l5">schooren</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Daar bloeit het <rhyme label="d" type="m" corresp="#s13:l6">vaderland</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Ja! Godsdienst gaat te <rhyme label="a" type="f">gronden</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Door veel <rhyme label="b" type="m">toegeeflijkheid</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Het hart, verlokt tot <rhyme label="a" type="f" corresp="#s14:l1">zonden</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Dient in een band <rhyme label="b" type="m" corresp="#s14:l2">geleid</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Alleen in Willems <rhyme label="c" type="f">handen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Is 't kleinnood wel <rhyme label="d" type="m">bewaard</rhyme>:</l>
        <l id="s14:l7" met="3" type="-+">Laat ons voor Willem <rhyme label="c" type="f" corresp="#s14:l5">branden</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Ja! al bevriest al de <rhyme label="d" type="m" corresp="#s14:l6">aard</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Eeuwig'! aij hoor deez' <rhyme label="a" type="f">beede</rhyme>!</l>
        <l id="s15:l2" met="3" type="-+">Bescherm ons <rhyme label="b" type="m">Vaderland</rhyme>!</l>
        <l id="s15:l3" met="3" type="-+">In Oorlog en in <rhyme label="a" type="f" corresp="#s15:l1">Vreede</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Lei WILLEM by der <rhyme label="b" type="m" corresp="#s15:l2">hand</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Ja Hemel, elk roept <rhyme label="c" type="f">Amen</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Elk wuif den vryen <rhyme label="d" type="m">hoed</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Godsdienst, Prins, Vryheid <rhyme label="c" type="f" corresp="#s15:l5">zaamen</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Zyn Een by 't echte <rhyme label="d" type="m" corresp="#s15:l6">bloed</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
