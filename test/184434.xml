  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WY leggen alle <rhyme label="a" type="m">zwarigheit</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O Held! op uwe <rhyme label="b" type="f">Krachten</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dog dragen wy uw Jok <rhyme label="a" type="m" corresp="#s1:l1">bereidt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En zullen hulp <rhyme label="b" type="f" corresp="#s1:l2">verwagten</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">O rust dog niet met Geest en <rhyme label="c" type="m">Kracht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Tot Gy de Broeders <rhyme label="d" type="f">alle</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Beweegt tot uwe <rhyme label="c" type="m" corresp="#s1:l5">Ridderschap</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dat waar' ons <rhyme label="d" type="f" corresp="#s1:l6">welgevallen</rhyme>.</l>
      </lg>
    </lg>
  </text>
