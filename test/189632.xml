  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LYsje wel myn zoete <rhyme label="a" type="f">sloofje</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Wil je <rhyme label="b" type="m">me</rhyme></l>
        <l id="s1:l3" met="2" type="+-">In myn <rhyme label="b" type="m" corresp="#s1:l2">sle</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Ik heb voorwaar een warm <rhyme label="a" type="f" corresp="#s1:l1">stoofje</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">'t Zal u <rhyme label="c" type="f">warmen</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Tot uw <rhyme label="c" type="f" corresp="#s1:l5">darmen</rhyme>.</l>
        <l id="s1:l7" met="2" type="+-">Koom maar <rhyme label="d" type="m">in</rhyme></l>
        <l id="s1:l8" met="2" type="+-"><rhyme label="d" type="m" corresp="#s1:l7">Engelin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wel hoe zou 'k dat durven <rhyme label="a" type="f">wagen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">In uw <rhyme label="b" type="m">sle</rhyme>.</l>
        <l id="s2:l3" met="2" type="+-">'k ben te <rhyme label="b" type="m" corresp="#s2:l2">vre</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Wil uw schaasjes maar aan <rhyme label="a" type="f" corresp="#s2:l1">binden</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Aan uw <rhyme label="c" type="f">voetjes</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Ryd wat <rhyme label="c" type="f" corresp="#s2:l5">zoetjes</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">'k Ben van <rhyme label="d" type="m">aart</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Wat <rhyme label="d" type="m" corresp="#s2:l7">vervaart</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gaat dat nou niet wel myn <rhyme label="a" type="f">Lysje</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Dat ik <rhyme label="b" type="m">jou</rhyme></l>
        <l id="s3:l3" met="2" type="+-">Zoetjes <rhyme label="b" type="m" corresp="#s3:l2">dou</rhyme>?</l>
        <l id="s3:l4" met="4" type="+-">Wel ik dagt voorwaar niet <rhyme label="a" type="f" corresp="#s3:l1">Gysje</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Dat aie <rhyme label="c" type="f">dingen</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Zoo wel <rhyme label="c" type="f" corresp="#s3:l5">gingen</rhyme></l>
        <l id="s3:l7" met="2" type="+-">'k Waar in 't <rhyme label="d" type="m">eerst</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Wat <rhyme label="d" type="m" corresp="#s3:l7">bevreest</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Met zoo kreeg de sle een <rhyme label="a" type="f">hortje</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Op een <rhyme label="b" type="m">steen</rhyme></l>
        <l id="s4:l3" met="2" type="+-">Zoo ik <rhyme label="b" type="m" corresp="#s4:l2">meen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En zy zei wel Gys wat <rhyme label="a" type="f" corresp="#s4:l1">schortje</rhyme>?</l>
        <l id="s4:l5" met="2" type="+-">Toen viel <rhyme label="c" type="f">matje</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Met haar <rhyme label="c" type="f" corresp="#s4:l5">gatje</rhyme></l>
        <l id="s4:l7" met="2" type="+-">Uyt de <rhyme label="d" type="m">sle</rhyme></l>
        <l id="s4:l8" met="2" type="+-">In de <rhyme label="d" type="m" corresp="#s4:l7">sneu</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wel hoe vuyl is nou myn <rhyme label="a" type="f">stoofje</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Zoo <rhyme label="b" type="m">beslik</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">En aan <rhyme label="b" type="m" corresp="#s5:l2">stik</rhyme></l>
        <l id="s5:l4" met="4" type="+-">'t Was voorwaar zoo 'n moye <rhyme label="a" type="f" corresp="#s5:l1">stoofje</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Door dat <rhyme label="c" type="f">vallen</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">En dat <rhyme label="c" type="f" corresp="#s5:l5">mallen</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Dat ik <rhyme label="d" type="m">deÃª</rhyme></l>
        <l id="s5:l8" met="2" type="+-">Uyt de <rhyme label="d" type="m" corresp="#s5:l7">slee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Zal my voor dat finnig <rhyme label="a" type="f">ryen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Op het <rhyme label="b" type="m">eys</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Ben je <rhyme label="b" type="m" corresp="#s6:l2">wys</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Op een andertyt wel <rhyme label="a" type="f" corresp="#s6:l1">myen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Loop jou <rhyme label="c" type="f">kwant</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-"><rhyme label="c" type="f" corresp="#s6:l5">Asturant</rhyme>.</l>
        <l id="s6:l7" met="2" type="+-">Loop jou <rhyme label="d" type="m">vent</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">Als je <rhyme label="d" type="m" corresp="#s6:l7">bent</rhyme>.</l>
      </lg>
    </lg>
  </text>
