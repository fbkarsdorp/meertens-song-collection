    <melody>Il me suffit</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Tjaert Sonnema</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DEs Heeren wil geschiê <rhyme label="a" type="m">altyd</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En d'onse gae te <rhyme label="b" type="f">boven</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Hy helpt die vaerdig uit den <rhyme label="a" type="m" corresp="#s1:l1">stryd</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die vast aen Hem <rhyme label="b" type="f" corresp="#s1:l2">gelooven</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Schoon onse <rhyme label="c" type="m">nood</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Zy <rhyme label="c" type="m" corresp="#s1:l5">over-groot</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hy troost ons boven <rhyme label="d" type="f">maeten</rhyme>;</l>
        <l id="s1:l8" met="2" type="-+">Wie God <rhyme label="e" type="m">vertrouwt</rhyme></l>
        <l id="s1:l9" met="2" type="-+">En op Hem <rhyme label="e" type="m" corresp="#s1:l8">bouwt</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Die sal Hy niet <rhyme label="d" type="f" corresp="#s1:l7">verlaeten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">DEs Heeren wil geschiê <rhyme label="a" type="m">altyd</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En d'onse gae te <rhyme label="b" type="f">boven</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Hy helpt die vaerdig uit dê <rhyme label="a" type="m" corresp="#s2:l1">stryd</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die vast aen Hem <rhyme label="b" type="f" corresp="#s2:l2">gelooven</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Schoon onse <rhyme label="c" type="m">nood</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Zy <rhyme label="c" type="m" corresp="#s2:l5">over-groot</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Hy troost ons boven <rhyme label="d" type="f">maeten</rhyme>;</l>
        <l id="s2:l8" met="2" type="-+">Wie God <rhyme label="e" type="m">vertrouwt</rhyme></l>
        <l id="s2:l9" met="2" type="-+">En op Hem <rhyme label="e" type="m" corresp="#s2:l8">bouwt</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Die sal Hy niet <rhyme label="d" type="f" corresp="#s2:l7">verlaten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">God is mijn heil, mijn raed en <rhyme label="a" type="m">daed</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hy is mijn hoop en <rhyme label="b" type="f">leven</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Aen God alleen mijn wille <rhyme label="a" type="m" corresp="#s3:l1">staet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wie kan Hem <rhyme label="b" type="f" corresp="#s3:l2">tegenstreeven</rhyme>?</l>
        <l id="s3:l5" met="2" type="-+">Die 't al <rhyme label="c" type="m">besiet</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Mijn hairen <rhyme label="c" type="m" corresp="#s3:l5">weet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Te tellen net in <rhyme label="d" type="f">allen</rhyme>;</l>
        <l id="s3:l8" met="2" type="-+">Sy zijn in <rhyme label="e" type="m">klem</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Dat sonder <rhyme label="e" type="m" corresp="#s3:l8">Hem</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Niet een en kan <rhyme label="d" type="f" corresp="#s3:l7">uitvallen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
