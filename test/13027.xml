  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'t ZOete gezicht van <rhyme label="a" type="m">Katarijn</rhyme></l>
        <l id="s1:l2" met="5" type="+-">Maakt mijn hertje dronken zonder <rhyme label="a" type="m" corresp="#s1:l1">Wijn</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Ja mijn <rhyme label="b" type="m">bol</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Die is heel op de <rhyme label="b" type="m" corresp="#s1:l3">rol</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Door zakker-eerze <rhyme label="c" type="m">Min</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Tot die <rhyme label="c" type="m" corresp="#s1:l5">Zottin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want om haar braafheid is zy 't <rhyme label="a" type="m">waard</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">Ja geen schoonheid leeft op deze <rhyme label="a" type="m" corresp="#s2:l1">aard</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Die, <rhyme label="b" type="m">Katrijn</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">By u gestelt mach <rhyme label="b" type="m" corresp="#s2:l3">zijn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Of by u <rhyme label="c" type="m">aardigheid</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Foei fraye <rhyme label="c" type="m" corresp="#s2:l5">Meid</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want op haar veur-hooft, zonder <rhyme label="a" type="m">kleur</rhyme>,</l>
        <l id="s3:l2" met="5" type="+-">Leest me de Story van <rhyme label="a" type="m" corresp="#s3:l1">Blance-fleur</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">En heel <rhyme label="b" type="m">fraay</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Geel hair, gelijk een <rhyme label="b" type="m" corresp="#s3:l3">Kraay</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Verçiert dat schoone <rhyme label="c" type="m">hooft</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Wel waard <rhyme label="c" type="m" corresp="#s3:l5">gelooft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Want door haar wangen <rhyme label="a" type="m">Ezels-grauw</rhyme>,</l>
        <l id="s4:l2" met="5" type="+-">En bruin' oogen als een <rhyme label="a" type="m" corresp="#s4:l1">Kabeljauw</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Munt zy <rhyme label="b" type="m">uit</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Gelijk een vuile <rhyme label="b" type="m" corresp="#s4:l3">Bruidt</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">En met alzulk <rhyme label="c" type="m">blanket</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Is zy <rhyme label="c" type="m" corresp="#s4:l5">besmet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ja dat ik noch meer zeggen <rhyme label="a" type="m">wou</rhyme></l>
        <l id="s5:l2" met="5" type="+-">Van die overschoone zoete <rhyme label="a" type="m" corresp="#s5:l1">Vrouw</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-"><rhyme label="b" type="m">Katarijn</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Ghy wierdt licht, tot mijn <rhyme label="b" type="m" corresp="#s5:l3">pijn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Van d'een of d'aâr <rhyme label="c" type="m">geschaakt</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Die na u <rhyme label="c" type="m" corresp="#s5:l5">haakt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Want boven al die schoone <rhyme label="a" type="m">leên</rhyme></l>
        <l id="s6:l2" met="5" type="+-">Heeft zy noch veel and're <rhyme label="a" type="m" corresp="#s6:l1">waardigheên</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">z'Is niet <rhyme label="b" type="m">luy</rhyme>;</l>
        <l id="s6:l4" met="3" type="+-">Maar al heeft z' een goê <rhyme label="b" type="m" corresp="#s6:l3">buy</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Werkt zy niet garen <rhyme label="c" type="m">veel</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Dat lief <rhyme label="c" type="m" corresp="#s6:l5">juweel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">'t Zoete gezicht van <rhyme label="a" type="m">Katarijn</rhyme></l>
        <l id="s7:l2" met="5" type="+-">Maakt mijn hertje dronken zonder <rhyme label="a" type="m" corresp="#s7:l1">Wijn</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Ja mijn <rhyme label="b" type="m">bol</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Die is heel op de <rhyme label="b" type="m" corresp="#s7:l3">rol</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Door zakker-eerze <rhyme label="c" type="m">Min</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Tot de <rhyme label="c" type="m" corresp="#s7:l5">zottin</rhyme>.</l>
      </lg>
    </lg>
  </text>
