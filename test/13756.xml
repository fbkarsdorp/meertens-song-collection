    <tags>
      <tag standard-name="gezelschapslied" translation="social song">gezelschapslied</tag>
    </tags>
    <melody>Cette cruelle</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">KOmt helden die bemint het <rhyme label="a" type="f">smoocken</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Doet naer de wijs u <rhyme label="b" type="m">offerhandt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En wilt de godt Vulcanus <rhyme label="a" type="f" corresp="#s1:l1">roocken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Met heyligh en ghewijde <rhyme label="b" type="m" corresp="#s1:l2">brandt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Op u <rhyme label="c" type="f">altaeren</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Steets u dienst laet <rhyme label="d" type="m">sien</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Aenvaert de <rhyme label="c" type="f" corresp="#s1:l5">klaeren</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Menghtse onder <rhyme label="d" type="m" corresp="#s1:l6">ien</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Als Bacchus u komt <rhyme label="d" type="m" corresp="#s1:l6">bien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Toont u als wackere <rhyme label="a" type="f">soldaten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Bequaem te zijn tot Vochtes <rhyme label="b" type="m">dienst</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Laed’ u Musquet met dub’le <rhyme label="a" type="f" corresp="#s2:l1">maeten</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En niemant toont sich <rhyme label="b" type="m" corresp="#s2:l2">ongesienst</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Maer wilt als <rhyme label="c" type="f">Helden</rhyme></l>
        <l id="s2:l6" met="3" type="+-">V vyandt <rhyme label="d" type="m">verslaen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Laet u niet <rhyme label="c" type="f" corresp="#s2:l5">schelden</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Dat de <rhyme label="d" type="m" corresp="#s2:l6">Natte-vaen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Met Haerlem is <rhyme label="d" type="m" corresp="#s2:l6">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Yeder stelt sich nu in <rhyme label="a" type="f">geleden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En blijcken laet te zijn een <rhyme label="b" type="m">Man</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ons vyandt komt ons tegen <rhyme label="a" type="f" corresp="#s3:l1">treden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Versien met Toeback, Pijp en <rhyme label="b" type="m" corresp="#s3:l2">Kan</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Blaest u <rhyme label="c" type="f">Trompetten</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Laet den Trommel <rhyme label="d" type="m">slaen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">‘t Kan ons niet <rhyme label="c" type="f" corresp="#s3:l5">letten</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Swiert de <rhyme label="d" type="m" corresp="#s3:l6">Natte-vaen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Dit vechten kan niet <rhyme label="d" type="m" corresp="#s3:l6">schaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Lof Bachus voor u milde <rhyme label="a" type="f">tranen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En Cerus voor u deftigh <rhyme label="b" type="m">nat</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ghy die ons vreughde eerst gingh <rhyme label="a" type="f" corresp="#s4:l1">banen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Doen ons de droefheyt noch <rhyme label="b" type="m" corresp="#s4:l2">besat</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">En ghy die’t <rhyme label="c" type="f">smoocken</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Eerst heb aen <rhyme label="d" type="m">gevaen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En ons in’t <rhyme label="c" type="f" corresp="#s4:l5">roocken</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Deftigh <rhyme label="d" type="m" corresp="#s4:l6">voorgegaen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Dies danckt men u <rhyme label="d" type="m" corresp="#s4:l6">Vulcaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De roem van Sparens brave <rhyme label="a" type="f">Basen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Het werelts vack om is <rhyme label="b" type="m">bekent</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Vermits de Faem haer roem gaet <rhyme label="a" type="f" corresp="#s5:l1">blasen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Maeckt dat u roem noyt wert <rhyme label="b" type="m" corresp="#s5:l2">geschent</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Dus <rhyme label="c" type="f">Amstel-lieden</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Geeft mal-kaer de <rhyme label="d" type="m">handt</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Sparen komt <rhyme label="c" type="f" corresp="#s5:l5">bieden</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Hulp trouw, en <rhyme label="d" type="m" corresp="#s5:l6">pant</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Tot dienst van’t <rhyme label="d" type="m" corresp="#s5:l6">Amstel-landt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
