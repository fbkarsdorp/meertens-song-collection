  <author>Jacobus de Ruyter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEest verblijdt nu al <rhyme label="a" type="m">ghelijck</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Door g'heel 't <rhyme label="a" type="m" corresp="#s1:l1">Christenrijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Boerey en wilt niet meer <rhyme label="b" type="f">treuren</rhyme>,</l>
        <l id="s1:l4" met="6" type="+-">Al uw' droefheyt is ghedaen, Wilt nu <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Naer de Stee, Drincken een <rhyme label="d" type="m">Santee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Koninck Carels <rhyme label="a" type="m">Testament</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Heeft voor Poirs <rhyme label="a" type="m" corresp="#s2:l1">bekent</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Duc d'Anjou den tweeden <rhyme label="b" type="f">Sone</rhyme>,</l>
        <l id="s2:l4" met="6" type="+-">Van den Dolphijn te Bourbon, Die hy <rhyme label="c" type="m">won</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">Tot zijn loon, Heeft de Spaensche <rhyme label="d" type="m">Troon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">T'is gheluck voor g'heel het <rhyme label="a" type="m">Lant</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">'t Is een stercken <rhyme label="a" type="m" corresp="#s3:l1">bant</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Vranckerijck sal mer met Spaignien <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l4" met="6" type="+-">Nu in vrede voor altijdt, weest <rhyme label="c" type="m">verblijdt</rhyme>,</l>
        <l id="s3:l5" met="5" type="+-">Wilt al gaen, Steecke vier-wercken <rhyme label="d" type="m">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Vlecht Croonen van <rhyme label="a" type="m">Lauwerier</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">En maeckt vreughde <rhyme label="a" type="m" corresp="#s4:l1">vier</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om hier med' te <rhyme label="b" type="f">vereeren</rhyme>,</l>
        <l id="s4:l4" met="6" type="+-">Al die Dorsten kloeck en stout, Een van <rhyme label="c" type="m">gout</rhyme>,</l>
        <l id="s4:l5" met="5" type="+-">Voor dien Helt, 't Lant in ruste <rhyme label="d" type="m">stelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Nu sal alle Coop-mans <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Reysen metter <rhyme label="a" type="m" corresp="#s5:l1">spoet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Al sonder rechten te <rhyme label="b" type="f">gheven</rhyme>,</l>
        <l id="s5:l4" met="6" type="+-">Of voor Comisen te vlien, wilt <rhyme label="c" type="m">verblijen</rhyme>,</l>
        <l id="s5:l5" met="5" type="+-">Alle gaer, Over dese <rhyme label="d" type="m">maer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Vranckerijck en dien jonghen <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Naer ons welvaert <rhyme label="a" type="m" corresp="#s6:l1">dorst</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Bidt dat Godt hun een langh <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s6:l4" met="6" type="+-">Magh verleenen al ghelijck, In zijn <rhyme label="c" type="m">Rijck</rhyme>,</l>
        <l id="s6:l5" met="5" type="+-">Plaets bereyt, In de <rhyme label="d" type="m">Saligheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Oorlof Christen Menschen <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Hier op s'wereldts <rhyme label="a" type="m" corresp="#s7:l1">dal</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En aenbidt den Heer der <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s7:l4" met="6" type="+-">En zijn lieve Moeder mee, Dat s'ons <rhyme label="c" type="m">vre</rhyme>,</l>
        <l id="s7:l5" met="5" type="+-">Gheeft te gaer, Nu en oock hier <rhyme label="d" type="m">naer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
