<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24516</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Ik klom de boom al op die mij te hoog</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hy truer die trueren <rhyme label="a" type="f">wil</rhyme></l>
        <l id="s1:l2" met="3" type="-+">myn troeren heeft <rhyme label="b" type="m">gedaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">ic heb geswegen <rhyme label="a" type="f" corresp="#s1:l1">stylle</rhyme></l>
        <l id="s1:l4" met="3" type="-+">maria wilt <rhyme label="b" type="m" corresp="#s1:l2">verstaen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hi truere die trueren <rhyme label="a" type="f" corresp="#s1:l1">wil</rhyme></l>
        <l id="s1:l6" met="3" type="-+">myn trueren heeft <rhyme label="b" type="m" corresp="#s1:l2">gedaen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">ic heb ge  swegen <rhyme label="a" type="f" corresp="#s1:l1">stylle</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Maria wilt <rhyme label="b" type="m" corresp="#s1:l2">verstaen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Waer om soe soldic <rhyme label="a" type="f">trueren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">of sorgen alte <rhyme label="b" type="m">seer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">ic weet  ic byn <rhyme label="a" type="f" corresp="#s2:l1">vercaeren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">van Jhesus mynen <rhyme label="b" type="m" corresp="#s2:l2">heer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Hi truere die trueren <rhyme label="a" type="f" corresp="#s2:l1">wil</rhyme></l>
        <l id="s2:l6" met="3" type="-+">myn trueren heeft <rhyme label="b" type="m" corresp="#s2:l2">gedaen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">ic heb ge  swegen <rhyme label="a" type="f" corresp="#s2:l1">stylle</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Maria wilt <rhyme label="b" type="m" corresp="#s2:l2">verstaen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dat ic hem byn <rhyme label="a" type="f">vercaeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">heeft hi my gedaen <rhyme label="b" type="m">aenschyn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Want ic moste syn <rhyme label="a" type="f" corresp="#s3:l1">verlaeren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">dede my syn doet ende <rhyme label="b" type="m" corresp="#s3:l2">pyn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Hi truere die trueren <rhyme label="a" type="f" corresp="#s3:l1">wil</rhyme></l>
        <l id="s3:l6" met="3" type="-+">myn trueren heeft <rhyme label="b" type="m" corresp="#s3:l2">gedaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">ic heb ge  swegen <rhyme label="a" type="f" corresp="#s3:l1">stylle</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Maria wilt <rhyme label="b" type="m" corresp="#s3:l2">verstaen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hi is hier neder <rhyme label="a" type="f">gecoemen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">daer ic in byn <rhyme label="b" type="m">verblyt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">myn mysdaet solde my <rhyme label="a" type="f" corresp="#s4:l1">verdoemen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">syn doet heeft my <rhyme label="b" type="m" corresp="#s4:l2">gequyt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hi truere die trueren <rhyme label="a" type="f" corresp="#s4:l1">wil</rhyme></l>
        <l id="s4:l6" met="3" type="-+">myn trueren heeft <rhyme label="b" type="m" corresp="#s4:l2">gedaen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">ic heb ge  swegen <rhyme label="a" type="f" corresp="#s4:l1">stylle</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Maria wilt <rhyme label="b" type="m" corresp="#s4:l2">verstaen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dus wil ic my <rhyme label="a" type="f">verblyden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">seer vroemelic in <rhyme label="b" type="m">hem</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ende dragen tot allen <rhyme label="a" type="f" corresp="#s5:l1">tyden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">syn doet in mynen <rhyme label="b" type="m" corresp="#s5:l2">syn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Hi truere die trueren <rhyme label="a" type="f" corresp="#s5:l1">wil</rhyme></l>
        <l id="s5:l6" met="3" type="-+">myn trueren heeft <rhyme label="b" type="m" corresp="#s5:l2">gedaen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">ic heb ge  swegen <rhyme label="a" type="f" corresp="#s5:l1">stylle</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Maria wilt <rhyme label="b" type="m" corresp="#s5:l2">verstaen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Des byn ic wal <rhyme label="a" type="f">ontwaere</rhyme></l>
        <l id="s6:l2" met="3" type="-+">dede my syn pyn ende <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">al bade ic dusent <rhyme label="a" type="f" corresp="#s6:l1">jaer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">twaer al verlaren <rhyme label="b" type="m" corresp="#s6:l2">guet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Hi truere die trueren <rhyme label="a" type="f" corresp="#s6:l1">wil</rhyme></l>
        <l id="s6:l6" met="3" type="-+">myn trueren heeft <rhyme label="b" type="m" corresp="#s6:l2">gedaen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">ic heb ge  swegen <rhyme label="a" type="f" corresp="#s6:l1">stylle</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Maria wilt <rhyme label="b" type="m" corresp="#s6:l2">verstaen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hier om wil ic hem <rhyme label="a" type="f">mynnen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">die my dus heeft <rhyme label="b" type="m">verloest</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ende myn heer van <rhyme label="a" type="f" corresp="#s7:l1">bynnen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">holden voer alle myn <rhyme label="b" type="m" corresp="#s7:l2">troest</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Hi truere die trueren <rhyme label="a" type="f" corresp="#s7:l1">wil</rhyme></l>
        <l id="s7:l6" met="3" type="-+">myn trueren heeft <rhyme label="b" type="m" corresp="#s7:l2">gedaen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">ic heb ge  swegen <rhyme label="a" type="f" corresp="#s7:l1">stylle</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Maria wilt <rhyme label="b" type="m" corresp="#s7:l2">verstaen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
