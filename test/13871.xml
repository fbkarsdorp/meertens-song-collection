        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Rein maagdje met eer</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AMbachten zijnder <rhyme label="a" type="f">veele</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Yder prijst het <rhyme label="b" type="m">zijn</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Een Schilder sijn <rhyme label="a" type="f" corresp="#s1:l1">Penceelen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">En ick oock het <rhyme label="b" type="m" corresp="#s1:l2">mijn</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">En ick segh het wel <rhyme label="c" type="m">rondt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Gheen Ambacht mach ‘er <rhyme label="d" type="f">wesen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Dan wercken in het <rhyme label="c" type="m" corresp="#s1:l5">Bont</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dat dient noch meer <rhyme label="d" type="f" corresp="#s1:l6">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al in de eerste <rhyme label="a" type="f">Tijden</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Vondt hem Adam <rhyme label="b" type="m">Naeckt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En om hem te <rhyme label="a" type="f" corresp="#s2:l1">verblijden</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Heeft Godt <rhyme label="b" type="m" corresp="#s2:l2">ghemaeckt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Van Vellen een <rhyme label="c" type="m">Habeyt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Het gheen hy ginck aen <rhyme label="d" type="f">trecken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Al om sijn <rhyme label="c" type="m" corresp="#s2:l5">Schamelheydt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En Lichaem mee te <rhyme label="d" type="f" corresp="#s2:l6">decken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dus ist een oudt <rhyme label="a" type="f">gheslachte</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">En schier alle <rhyme label="b" type="m">man</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die siet men daer nae <rhyme label="a" type="f" corresp="#s3:l1">trachten</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Om te raecken <rhyme label="b" type="m" corresp="#s3:l2">an</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Aoo wel arrem als <rhyme label="c" type="m">rijck</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Soo wel vrijsters als <rhyme label="d" type="f">vrouwen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Sy alle te <rhyme label="c" type="m" corresp="#s3:l5">ghelijck</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van het Bont-wercken <rhyme label="d" type="f" corresp="#s3:l6">houwen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men kan ‘t niet wel <rhyme label="a" type="f">ontbeeren</rhyme></l>
        <l id="s4:l2" met="3" type="+-">‘t Edel <rhyme label="b" type="m">Beeste-vel</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Of Kou die sou ons <rhyme label="a" type="f" corresp="#s4:l1">deeren</rhyme>:</l>
        <l id="s4:l4" met="3" type="+-">Want het kleedt seer <rhyme label="b" type="m" corresp="#s4:l2">wel</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Het kan een <rhyme label="c" type="m">Vrouw-persoon</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Heel wonder schoon <rhyme label="d" type="f">vercieren</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">‘t Bont-wercken spant de <rhyme label="c" type="m" corresp="#s4:l5">Kroon</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In alderley <rhyme label="d" type="f" corresp="#s4:l6">manieren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
