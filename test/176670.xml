<song>
  <meta>
    <id>176670</id>
    <date>
      <year-start>1683</year-start>
      <year-end>1683</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Cupido triomfant</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">In zonden ben ick <rhyme label="a" type="m">ontfaen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En oock daer in <rhyme label="b" type="f">ghebooren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Qualijck kan ickse <rhyme label="a" type="m" corresp="#s1:l1">ontgaen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sy komt my altijt <rhyme label="b" type="f" corresp="#s1:l2">vooren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En toont my <rhyme label="c" type="m">snel</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Haer Leeuwen tanden <rhyme label="c" type="m" corresp="#s1:l5">fel</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Mijn ziele maecktse groot <rhyme label="c" type="m" corresp="#s1:l5">ghequel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zonden is mijn <rhyme label="a" type="m">ghepeys</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">By dach en oock by <rhyme label="b" type="f">nachte</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Sy soeckt altijt haer <rhyme label="a" type="m" corresp="#s2:l1">eys</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ick kan my niet wel <rhyme label="b" type="f" corresp="#s2:l2">wachte</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">O Heere my <rhyme label="c" type="m">gheneest</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dat ick heb <rhyme label="c" type="m" corresp="#s2:l5">onbevreest</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Eenen nieuwen ghewissen <rhyme label="c" type="m" corresp="#s2:l5">Gheest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ben ick in een <rhyme label="a" type="m">propoost</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van nieuws my te <rhyme label="b" type="f">bekeeren</rhyme></l>
        <l id="s3:l3" met="3" type="-+">In den eeuwighen <rhyme label="a" type="m" corresp="#s3:l1">troost</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Altijdt te <rhyme label="b" type="f" corresp="#s3:l2">mediteeren</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Heer eer yet <rhyme label="c" type="m">langh</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ontgaet my sulck <rhyme label="c" type="m" corresp="#s3:l5">aenvangh</rhyme></l>
        <l id="s3:l7" met="4" type="-+">’t Welck my maeckt van herten <rhyme label="c" type="m" corresp="#s3:l5">bangh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick wilde wel o <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al ick van wil sal <rhyme label="b" type="f">spreecken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat ick kond’ nae u <rhyme label="a" type="m" corresp="#s4:l1">Leer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Recht bet’ren mijn <rhyme label="b" type="f" corresp="#s4:l2">ghebreecken</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Maer uyt mijn <rhyme label="c" type="m">kracht</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ick so veel niet <rhyme label="c" type="m" corresp="#s4:l5">verwacht</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Bequaem tot een goet <rhyme label="c" type="m" corresp="#s4:l5">ghedaght</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ontfaen heb ick u <rhyme label="a" type="m">Gheest</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En dat tot mijn <rhyme label="b" type="f">behoeve</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Maer och dit isset <rhyme label="a" type="m" corresp="#s5:l1">meest</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Oft ickse niet <rhyme label="b" type="f" corresp="#s5:l2">bedroeve</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Want sy is ‘t <rhyme label="c" type="m">pant</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van het beloofde <rhyme label="c" type="m" corresp="#s5:l5">Lant</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Inder eeuwigheyt <rhyme label="c" type="m" corresp="#s5:l5">triumphant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">En isser niet <rhyme label="a" type="m">gheseyt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Haer vrught is liefde en <rhyme label="b" type="f">vreughde</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Vreedt en <rhyme label="a" type="m" corresp="#s6:l1">lanckmoedigheyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vriendelijckheyt en <rhyme label="b" type="f" corresp="#s6:l2">deughde</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Gheloove <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ende <rhyme label="c" type="m" corresp="#s6:l5">sachtmoedigheyt</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Matigh in allen hoort dit <rhyme label="c" type="m" corresp="#s6:l5">bescheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Oock is des gheestes <rhyme label="a" type="m">vrucht</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Vol alderleye <rhyme label="b" type="f">goetheyt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hierom is ‘t dat ick <rhyme label="a" type="m" corresp="#s7:l1">sucht</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Want quaet in mijn ghemoet <rhyme label="b" type="f" corresp="#s7:l2">leyt</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">En dit <rhyme label="c" type="m">present</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Is ‘t begin en het <rhyme label="c" type="m" corresp="#s7:l5">endt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Een boom wort aen sijn vrucht <rhyme label="c" type="m" corresp="#s7:l5">bekent</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Daer wort oock voort <rhyme label="a" type="m">vermelt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Och mochtet ons doch <rhyme label="b" type="f">vroomen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat de Bijl is <rhyme label="a" type="m" corresp="#s8:l1">ghestelt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Aen den wortel der <rhyme label="b" type="f" corresp="#s8:l2">boomen</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Die gheen goet <rhyme label="c" type="m">kruyt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Draeght, gaetmen roepen <rhyme label="c" type="m" corresp="#s8:l5">uyt</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">In ‘t Helsche vyer men hem dan <rhyme label="c" type="m" corresp="#s8:l5">sluyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">In des Heeren dach <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Sal yeghelijck moeten <rhyme label="b" type="f">gheven</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Reeckenschap van elck <rhyme label="a" type="m" corresp="#s9:l1">woort</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">O Heer wie soude niet <rhyme label="b" type="f" corresp="#s9:l2">beven</rhyme>?</l>
        <l id="s9:l5" met="2" type="-+">Iae oock van elck <rhyme label="c" type="m">ghedacht</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Welckmen in ‘t hert <rhyme label="c" type="m" corresp="#s9:l5">betracht</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Och wee! alle die hem niet <rhyme label="c" type="m" corresp="#s9:l5">wacht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Geboren uyt het <rhyme label="a" type="m">ghebloet</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ben ick van d’eerste <rhyme label="b" type="f">mensche</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Verdorven is ‘t <rhyme label="a" type="m" corresp="#s10:l1">ghemoedt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Onrecht is al sijn <rhyme label="b" type="f" corresp="#s10:l2">wensche</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Al isser ghenaed’ <rhyme label="c" type="m">gheschiet</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Oock die plant en <rhyme label="c" type="m" corresp="#s10:l5">begiet</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Sonder u wasdom is ‘t al <rhyme label="c" type="m" corresp="#s10:l5">niet</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Prinse Godt aller <rhyme label="a" type="m">deugt</rhyme></l>
        <l id="s11:l2" met="3" type="-+">U Gheest laet in my <rhyme label="b" type="f">wercken</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Dat hy my van der <rhyme label="a" type="m" corresp="#s11:l1">jeugt</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Tot in outheyt wil <rhyme label="b" type="f" corresp="#s11:l2">stercken</rhyme>.</l>
        <l id="s11:l5" met="2" type="-+">Op dat in mijn <rhyme label="c" type="m">elent</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Ick van u sy <rhyme label="c" type="m" corresp="#s11:l5">bekent</rhyme></l>
        <l id="s11:l7" met="4" type="-+">Een Olijf-Tacxken <rhyme label="c" type="m" corresp="#s11:l5">excelent</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
