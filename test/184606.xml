  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MAria weerde <rhyme label="a" type="m">Koningin</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van Hemel en <rhyme label="b" type="m">aerd'</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Geeft dat ick uwen lof <rhyme label="a" type="m" corresp="#s1:l1">begin</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die heel myn hert <rhyme label="b" type="m" corresp="#s1:l2">bewaert</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">En uw' weldaeden, die gy <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Uw' jonst, en sorge, die gy <rhyme label="d" type="m">thoont</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Aen al die <rhyme label="c" type="m" corresp="#s1:l5">leeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gy Moeder syt van 't hooghste <rhyme label="a" type="m">goedt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Godts Soon, het eeuwigh <rhyme label="b" type="m">Woordt</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Die eerst u hadde self <rhyme label="a" type="m" corresp="#s2:l1">gevoedt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gy suyver Maeght <rhyme label="b" type="m" corresp="#s2:l2">brenght-voort</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Gy Dochter van den Vader <rhyme label="c" type="m">syt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En Bruyt van Godt den Heyligh' <rhyme label="d" type="m">Geest</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Van vleck <rhyme label="c" type="m" corresp="#s2:l5">bevrydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gy schoonder als de sonn' en <rhyme label="a" type="m">maen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gy uytverkore <rhyme label="b" type="m">vat</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Gy s'Hemels poort, en rechte <rhyme label="a" type="m" corresp="#s3:l1">baen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Godts throon, en s'Hemels <rhyme label="b" type="m" corresp="#s3:l2">stadt</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Gy voor den mensch gewaepent <rhyme label="c" type="m">staer</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Op dat gy s'vyants helsche <rhyme label="d" type="m">macht</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Te <rhyme label="c" type="m" corresp="#s3:l5">neder-slaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O levend' waeter, klaer <rhyme label="a" type="m">fonteyn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ververscht doch mynen <rhyme label="b" type="m">mondt</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Maeckt myn gemoet van sonden <rhyme label="a" type="m" corresp="#s4:l1">reyn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ah! maeckt myn Ziel <rhyme label="b" type="m" corresp="#s4:l2">gesondt</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Dat in u, toren van <rhyme label="c" type="m">Ivoor</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ick woon, daer ick u Soon magh <rhyme label="d" type="m">fien</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Met d'Engels <rhyme label="c" type="m" corresp="#s4:l5">Choor</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
