    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik breng u op een dronkje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOm, kom myn lieve <rhyme label="a" type="f">zusje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat gelt u op een <rhyme label="a" type="f" corresp="#s1:l1">kusje</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">'t Smaakt by een dronk zoo <rhyme label="b" type="m">zoet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Myn allerzoetste <rhyme label="c" type="f">bekje</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En honing zoete <rhyme label="c" type="f" corresp="#s1:l4">gekje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ei wat doet my dat <rhyme label="b" type="m" corresp="#s1:l3">goet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ei wat doet my dat <rhyme label="b" type="m" corresp="#s1:l3">goet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zie daar ik kant niet <rhyme label="a" type="f">laten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Waar toe zoo veel te <rhyme label="a" type="f" corresp="#s2:l1">praten</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Gy maakt u immers <rhyme label="b" type="m">moe</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Drie kusjes ben je <rhyme label="c" type="f">schuldig</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Kom leyd se maar <rhyme label="c" type="f" corresp="#s2:l4">geduldig</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En houd uw montje <rhyme label="b" type="m" corresp="#s2:l3">toe</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En houd uw montje <rhyme label="b" type="m" corresp="#s2:l3">toe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maar hoe! myn lieve <rhyme label="a" type="f">roosje</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">My dunt Gy krygt een <rhyme label="a" type="f" corresp="#s3:l1">bloosje</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Of deet het uw wat <rhyme label="b" type="m">zeer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wel zoo het u niet <rhyme label="c" type="f">luste</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat ik u driemaal <rhyme label="c" type="f" corresp="#s3:l4">kuste</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Geef my myn kusjes <rhyme label="b" type="m" corresp="#s3:l3">weer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Geef my myn kusjes <rhyme label="b" type="m" corresp="#s3:l3">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
