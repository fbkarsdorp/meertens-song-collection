  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ick ben <rhyme label="a" type="m">deurwont</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Al met Copidos <rhyme label="b" type="f">stralen</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Heel <rhyme label="a" type="m" corresp="#s1:l1">onghesont</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wie sal mijn gheuen <rhyme label="c" type="m">raet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">V rooder <rhyme label="a" type="m" corresp="#s1:l1">Mont</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Daer ick niet aen sal <rhyme label="b" type="f" corresp="#s1:l2">falen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">In corter <rhyme label="a" type="m" corresp="#s1:l1">stont</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Wantet v luttel <rhyme label="c" type="m" corresp="#s1:l4">schaet</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Ick roep om <rhyme label="c" type="m" corresp="#s1:l4">baet</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Dies mijn toch nu <rhyme label="d" type="m">verhoort</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Ghy zijt mijn troost en <rhyme label="e" type="m">medecijn</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Daerom voldoet de bede <rhyme label="e" type="m" corresp="#s1:l11">mijn</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Duert minlijck <rhyme label="d" type="m" corresp="#s1:l10">accoort</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Int nieuwe Jaer nv <rhyme label="d" type="m" corresp="#s1:l10">voort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Niemant dan <rhyme label="a" type="m">ghy</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En can mijn quael <rhyme label="b" type="f">ghenesen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Dat segt ick <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dus compt mijn Lief <rhyme label="c" type="m">ontrent</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Nyders <rhyme label="a" type="m" corresp="#s2:l1">gheschry</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daer en wilt niet voor <rhyme label="b" type="f" corresp="#s2:l2">vreesen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Maer brengt nu <rhyme label="a" type="m" corresp="#s2:l1">by</rhyme></l>
        <l id="s2:l8" met="3" type="-+">V troost seer <rhyme label="c" type="m" corresp="#s2:l4">excelent</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Of heel <rhyme label="c" type="m" corresp="#s2:l4">gheschent</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Blijf ick tot deser <rhyme label="d" type="m">tijdt</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Dat waer v leet dat weet ick <rhyme label="e" type="m">claer</rhyme></l>
        <l id="s2:l12" met="4" type="-+">Weest mijn ghetrou vergeten <rhyme label="e" type="m" corresp="#s2:l11">aer</rhyme></l>
        <l id="s2:l13" met="3" type="-+">So wort mijn Hart <rhyme label="d" type="m" corresp="#s2:l10">verblijt</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Int nieuwe Jaer met <rhyme label="d" type="m" corresp="#s2:l10">vlijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Gheen schoon <rhyme label="a" type="m">samblant</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Al wt gheueynsder <rhyme label="b" type="f">harten</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Toont mijn Lief, <rhyme label="a" type="m" corresp="#s3:l1">want</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat zijn die Cruyden <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Maer <rhyme label="a" type="m" corresp="#s3:l1">abondant</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Verlicht mijn pijne <rhyme label="b" type="f" corresp="#s3:l2">smarten</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Biet mijn v <rhyme label="a" type="m" corresp="#s3:l1">hant</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Helpt mijn wt dit <rhyme label="c" type="m" corresp="#s3:l4">verdriet</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Troostbaricht <rhyme label="c" type="m" corresp="#s3:l4">ziet</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Op mijn v Dienaer <rhyme label="d" type="m">trou</rhyme></l>
        <l id="s3:l11" met="4" type="+-">So is my hart ghenesen <rhyme label="e" type="m">wis</rhyme></l>
        <l id="s3:l12" met="4" type="-+">Wantet wt alle vreesen <rhyme label="e" type="m" corresp="#s3:l11">is</rhyme></l>
        <l id="s3:l13" met="3" type="-+">En ick v voor mijn <rhyme label="d" type="m" corresp="#s3:l10">Vrou</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Int Nyeuwe Iaer nu <rhyme label="d" type="m" corresp="#s3:l10">hou</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Princesse <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Bloem bouen alle <rhyme label="b" type="f">wijuen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">In swerelts <rhyme label="a" type="m" corresp="#s4:l1">pleyn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Gheen lieuer lief <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Dan ghy <rhyme label="a" type="m" corresp="#s4:l1">alleyn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">V eyghen sal ick <rhyme label="b" type="f" corresp="#s4:l2">blijuen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Ick segt <rhyme label="a" type="m" corresp="#s4:l1">certeyn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In u weet ick gheen <rhyme label="c" type="m" corresp="#s4:l4">maer</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Want v naem <rhyme label="c" type="m" corresp="#s4:l4">claer</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Staet in mijn Jonck hart <rhyme label="d" type="m">siet</rhyme></l>
        <l id="s4:l11" met="4" type="+-">En is bequaem en <rhyme label="e" type="m">Lieflijck</rhyme></l>
        <l id="s4:l12" met="4" type="-+">Dies send ick v seer <rhyme label="e" type="m" corresp="#s4:l11">brieffelijck</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Dit Nieuwe Jaer <rhyme label="d" type="m" corresp="#s4:l10">liet</rhyme></l>
        <l id="s4:l14" met="3" type="-+">Dwelck wt Liefden <rhyme label="d" type="m" corresp="#s4:l10">gheschiet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
