<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156822</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik wil van deze avond</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DOen Jehu quam te <rhyme label="a" type="f">dooden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Het Godloos Ahabs, <rhyme label="b" type="m">Zaedt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Deed haer Isebel <rhyme label="c" type="f">blancketten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Of Jehu noch 't hert mocht <rhyme label="c" type="f" corresp="#s1:l3">setten</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Op haer opgetoyt <rhyme label="b" type="m" corresp="#s1:l2">Cieraet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dus blonck zy uyt haer <rhyme label="a" type="f">venster</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ghelijck een <rhyme label="b" type="m">Somer-Son</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Krielende van <rhyme label="c" type="f">Floncker-stralen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Daer door dat zy menigh <rhyme label="c" type="f" corresp="#s2:l3">malen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Geyle herten <rhyme label="b" type="m" corresp="#s2:l2">overwon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maer hy seydt: Stootze <rhyme label="a" type="f">neder</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Zy viel, en brack de <rhyme label="b" type="m">neck</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Siet, daer laghze op de <rhyme label="c" type="f">straten</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Daer de Honden haer voort <rhyme label="c" type="f" corresp="#s3:l3">aten</rhyme></l>
        <l id="s3:l5" met="4" type="+-">En verschiepen haer in <rhyme label="b" type="m" corresp="#s3:l2">dreck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wanneer wy mede <rhyme label="a" type="f">trachten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">'t Godtloose <rhyme label="b" type="m">Hof-gesin</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Van de sonden, uyt te <rhyme label="c" type="f">royen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Doet de wellust haer op <rhyme label="c" type="f" corresp="#s4:l3">toyen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Als een schoone <rhyme label="b" type="m" corresp="#s4:l2">Koningin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En glinstert voor ons' <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met schijn van soet <rhyme label="b" type="m">vermaeck</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En met Laghjes, en <rhyme label="c" type="f">Genuchtjes</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Duysendt Leck're soete <rhyme label="c" type="f" corresp="#s5:l3">Kluchtjes</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Al door-suyckert voor de <rhyme label="b" type="m" corresp="#s5:l2">smaeck</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer die met heyl'ghe <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">De weelde teghen <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Om haer onder voet te <rhyme label="c" type="f">storten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Sal haest sien wat haer sal <rhyme label="c" type="f" corresp="#s6:l3">schorten</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Want zy toont Schijn, voor de <rhyme label="b" type="m" corresp="#s6:l2">Daedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Haer soet' en minlijck <rhyme label="a" type="f">loncken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Verdwijnt ghelijck een <rhyme label="b" type="m">roock</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En haer glorend blinckend <rhyme label="c" type="f">glimmen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Vlieght daer heen ghelijck de <rhyme label="c" type="f" corresp="#s7:l3">schimmen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Of een droomend <rhyme label="b" type="m" corresp="#s7:l2">Nacht-ghespoock</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
