<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183578</id>
    <date>
      <year-start>1774</year-start>
      <year-end>1774</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Toen ik nog in het hartje was</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WAerom of soo meenig <rhyme label="a" type="m">Mensch</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Niet vernoegt kan <rhyme label="b" type="f">leven</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Wat kan dog het woord van <rhyme label="a" type="m" corresp="#s1:l1">Mensch</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Adams Kinderen <rhyme label="b" type="f" corresp="#s1:l2">geven</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Of men Wenschen wil of <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Het geschiet dog, dat <rhyme label="c" type="m" corresp="#s1:l5">geschiet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Laet dan elk vry <rhyme label="d" type="f">praten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s1:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Of men Wenscht en of men <rhyme label="a" type="m">zwijgt</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Als een aerdig <rhyme label="b" type="f">Meisje</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Dog geen zin in iemand <rhyme label="a" type="m" corresp="#s2:l1">krygt</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Schoon haer jeugdig <rhyme label="b" type="f" corresp="#s2:l2">Vleisje</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Graeg is naer het Huwelijks <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">'t Is vergeefs; al wat hy <rhyme label="c" type="m" corresp="#s2:l5">doed</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Laet hy vry wat <rhyme label="d" type="f">praeten</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Het wenschen kan niet <rhyme label="d" type="f" corresp="#s2:l7">baeten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Schoon men Wenscht al om een <rhyme label="a" type="m">prys</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Als het niet sal <rhyme label="b" type="f">weesen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Leg maer in, van rijs tot <rhyme label="a" type="m" corresp="#s3:l1">rijs</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Gaet de Lysten <rhyme label="b" type="f" corresp="#s3:l2">lesen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Sie het Contraboek vry <rhyme label="c" type="m">na</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Voor de meeste is het <rhyme label="c" type="m" corresp="#s3:l5">schaa</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Nieten voor uw <rhyme label="d" type="f">Platen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s3:l7">baeten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Schoon dat meenig frisse <rhyme label="a" type="m">Meid</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Wenscht om eens te <rhyme label="b" type="f">Trouwen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En het besten van haer <rhyme label="a" type="m" corresp="#s4:l1">tyd</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Slijt gelyk de <rhyme label="b" type="f" corresp="#s4:l2">Vrouwen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Van de zoete Huwelyks <rhyme label="c" type="m">Taert</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Wat te krygen om den <rhyme label="c" type="m" corresp="#s4:l5">Haert</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wat helpt al dat <rhyme label="d" type="f">praeten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s4:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Schoon een Ionggetrouwde <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Wel een mooy <rhyme label="b" type="f">Iuweeltje</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om een kleyntje geven <rhyme label="a" type="m" corresp="#s5:l1">sou</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Een van beidens <rhyme label="b" type="f" corresp="#s5:l2">beeltje</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Schoon sy goed is voor het <rhyme label="c" type="m">spel</rhyme></l>
        <l id="s5:l6" met="4" type="-+">En genoeg trekt aen de <rhyme label="c" type="m" corresp="#s5:l5">schel</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Laet de Vrouw maer <rhyme label="d" type="f">praten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s5:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Schoon myn buurvrouws swarte <rhyme label="a" type="m">Kat</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Laest myn draeijers <rhyme label="b" type="f">Duyfje</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Dat soo trouw te broeijen <rhyme label="a" type="m" corresp="#s6:l1">zat</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Wegnam voor een <rhyme label="b" type="f" corresp="#s6:l2">Kluyfje</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Of hy nou al wenscht of <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Dat het nooit mogt syn <rhyme label="c" type="m" corresp="#s6:l5">geschied</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Wat helpt al dat <rhyme label="d" type="f">praten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s6:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Schoon dat Niesje rondom <rhyme label="a" type="m">Net</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Gort smult van een <rhyme label="b" type="f">stoter</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Of sy dik word en niet <rhyme label="a" type="m" corresp="#s7:l1">vet</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Sy word geen duijm breed <rhyme label="b" type="f" corresp="#s7:l2">groter</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En daer word regt uit <rhyme label="c" type="m">geseid</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Onvergenoegt is dan de <rhyme label="c" type="m" corresp="#s7:l5">Meid</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Laet de buert maer <rhyme label="d" type="f">praten</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Het wenschen kan niet <rhyme label="d" type="f" corresp="#s7:l7">baeten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Schoon een Weeuwtje dag en <rhyme label="a" type="m">nagt</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Niet en doet als <rhyme label="b" type="f">treuren</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Of een ander Man <rhyme label="a" type="m" corresp="#s8:l1">verwagt</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Sal het niet <rhyme label="b" type="f" corresp="#s8:l2">gebeuren</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Schoon sy selfs met veel <rhyme label="c" type="m">verdriet</rhyme>:</l>
        <l id="s8:l6" met="4" type="-+">'t Klimmen van haer Kater <rhyme label="c" type="m" corresp="#s8:l5">siet</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Laet de Sloof maer <rhyme label="d" type="f">praeten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Het wenschen kan niet <rhyme label="d" type="f" corresp="#s8:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Schreid een Susje jong en <rhyme label="a" type="m">teer</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Om haer <rhyme label="b" type="f">Maegderoosje</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Nooit krygt sy het Bloempje <rhyme label="a" type="m" corresp="#s9:l1">weer</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Had sy haer <rhyme label="b" type="f" corresp="#s9:l2">Tonteldoosje</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Regt bewaerd, 't waer nooit <rhyme label="c" type="m">geschied</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Maer 't klaegen helpt dan <rhyme label="c" type="m" corresp="#s9:l5">niet</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Laet de ziel maer <rhyme label="d" type="f">praeten</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s9:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Schoon dat menig frisse <rhyme label="a" type="m">Maegd</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Niet voorsien van <rhyme label="b" type="f">schyven</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Met verdriet haer lot <rhyme label="a" type="m" corresp="#s10:l1">beklaegd</rhyme>;</l>
        <l id="s10:l4" met="3" type="+-">Ongetrouwt moet <rhyme label="b" type="f" corresp="#s10:l2">blyven</rhyme>;</l>
        <l id="s10:l5" met="4" type="-+">Schoon sy dagelyks al haer <rhyme label="c" type="m">schat</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Aen haer lyf draegt door de <rhyme label="c" type="m" corresp="#s10:l5">Stad</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Laet de sloof maer <rhyme label="d" type="f">praeten</rhyme>;</l>
        <l id="s10:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s10:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Daerom best is <rhyme label="a" type="m">vergenoegt</rhyme>?</l>
        <l id="s11:l2" met="3" type="+-">In dees Baen van 't <rhyme label="b" type="f">leven</rhyme>;</l>
        <l id="s11:l3" met="4" type="+-">Sig maer na den tyd <rhyme label="a" type="m" corresp="#s11:l1">gevoegt</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Of wat het Lot wil <rhyme label="b" type="f" corresp="#s11:l2">geven</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Rykdom, Armoe, Vreugt, <rhyme label="c" type="m">Verdriet</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Beter leven weet ik <rhyme label="c" type="m" corresp="#s11:l5">niet</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Want al wat wy <rhyme label="d" type="f">praeten</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s11:l7">baten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Vergenoegsaemheid is <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">ô Soo groot van <rhyme label="b" type="f">waerden</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Het is een Balsem voor 't <rhyme label="a" type="m" corresp="#s12:l1">gemoed</rhyme>;</l>
        <l id="s12:l4" met="3" type="+-">Het maekt ons sterck als <rhyme label="b" type="f" corresp="#s12:l2">Paerden</rhyme>;</l>
        <l id="s12:l5" met="4" type="-+">Water heeft de smaek van <rhyme label="c" type="m">Wyn</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Als men maer vergenoegt mag <rhyme label="c" type="m" corresp="#s12:l5">syn</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Laet dan elk maer <rhyme label="d" type="f">praeten</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Het Wenschen kan niet <rhyme label="d" type="f" corresp="#s12:l7">baten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
