<song>
  <meta>
    <id>185077</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pantalone door kunst verheven</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ach! wat baat uw kragtloos <rhyme label="a" type="f">woeden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zogenaamde <rhyme label="b" type="m">Patriot</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Zoek uw troost in <rhyme label="a" type="f" corresp="#s1:l1">tegenspoeden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zyt te vreeden met uw <rhyme label="b" type="m" corresp="#s1:l2">lot</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Moet uw macht en dwangzucht <rhyme label="c" type="f">buigen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dit ’s der Oproermaakers <rhyme label="d" type="m">deel</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Laat dit barneveld <rhyme label="c" type="f" corresp="#s1:l5">getuigen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s1:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Gy zogt steeds de Prins te <rhyme label="a" type="f">drukken</rhyme>.</l>
        <l id="s2:l2" met="4" type="+-">Gy woud Meesters zyn van ’t <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ieder moest hier voor u <rhyme label="a" type="f" corresp="#s2:l1">bukken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Vyfde willem moest aan <rhyme label="b" type="m" corresp="#s2:l2">kant</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Gy verbrak gezwooren <rhyme label="c" type="f">Eeden</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Om maar Baas te zyn <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Gy ging ’t Heiligst Recht <rhyme label="c" type="f" corresp="#s2:l5">vertreeden</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">’t Was uw beurt, maar ach! te <rhyme label="d" type="m" corresp="#s2:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zogenaamde Vryheids – <rhyme label="a" type="f">Zoonen</rhyme>!</l>
        <l id="s3:l2" met="4" type="+-">Gy woud ons in <rhyme label="b" type="m">slaverny</rhyme></l>
        <l id="s3:l3" met="4" type="+-">In dit vrye Land doen <rhyme label="a" type="f" corresp="#s3:l1">woonen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Door uw snoode <rhyme label="b" type="m" corresp="#s3:l2">dwingelandy</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Gy wou ’t Vaderland <rhyme label="c" type="f">regeeren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Niet ten deele, maar <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Doch nu zal men u <rhyme label="c" type="f" corresp="#s3:l5">beheeren</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s3:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dikwils hoorde men u <rhyme label="a" type="f">schreeuwen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Gy zoud al ’t Oranje – <rhyme label="b" type="m">vee</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Hier uitroeijen, en, als <rhyme label="a" type="f" corresp="#s4:l1">Leeuwen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Dapper vegten, maar, ô <rhyme label="b" type="m" corresp="#s4:l2">jee</rhyme>!</l>
        <l id="s4:l5" met="4" type="+-">Toen ’t de tyd was om te <rhyme label="c" type="f">stryden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Zag men u niet op ’t <rhyme label="d" type="m">tooneel</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Daarom moet gy smaad nu <rhyme label="c" type="f" corresp="#s4:l5">lyden</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s4:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">O, wat waart gy dappre <rhyme label="a" type="f">Helden</rhyme>!</l>
        <l id="s5:l2" met="4" type="+-">In het zuipen van de <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">In het lastren, in het <rhyme label="a" type="f" corresp="#s5:l1">schelden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">In het spuuwen van <rhyme label="b" type="m" corresp="#s5:l2">fenyn</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">In het vloeken, in het <rhyme label="c" type="f">liegen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En gy zogt hier, in ’t <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Ieder een maar te <rhyme label="c" type="f" corresp="#s5:l5">bedriegen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">’t Was uw beurt, maar ach! te <rhyme label="d" type="m" corresp="#s5:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Nu de Prins weer is <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">In ’t door u vernederd <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Nu hy weer in eer mag <rhyme label="a" type="f" corresp="#s6:l1">leeven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En hersteld is in zyn <rhyme label="b" type="m" corresp="#s6:l2">stand</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Nu weer Braaven ’t Land <rhyme label="c" type="f">regeeren</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Zucht gy, om dat zy <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">U, en uwe macht, <rhyme label="c" type="f" corresp="#s6:l5">verneêren</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s6:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Nu is ook het <rhyme label="a" type="f">Exerceeren</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Uwer Helpers <rhyme label="b" type="m">afgedaan</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Zy, die u, met haar <rhyme label="a" type="f" corresp="#s7:l1">Geweeren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Ievrig hebben <rhyme label="b" type="m" corresp="#s7:l2">bygestaan</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Moest ’t alles <rhyme label="c" type="f">overgeeven</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Zo verdween uw macht <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Zo moest uwe dwangzucht <rhyme label="c" type="f" corresp="#s7:l5">sneeven</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s7:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Wy, die eertyds stil hier <rhyme label="a" type="f">zweegen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Zingen vrolyk <rhyme label="b" type="m">overluid</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Om dat wy die Vryheid <rhyme label="a" type="f" corresp="#s8:l1">kreegen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Om dat ’t nu geen Beul meer <rhyme label="b" type="m" corresp="#s8:l2">stuit</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Vivat, dat Prins willem <rhyme label="c" type="f">leeve</rhyme>!</l>
        <l id="s8:l6" met="4" type="+-">En ’t Oranje – Huis <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Tot onz’ vreugd, laat Snoodaarts <rhyme label="c" type="f" corresp="#s8:l5">beeven</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s8:l6">veel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
