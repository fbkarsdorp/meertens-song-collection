<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3153</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Nadien uw goddelijkheid</melody>
    <categories>
      <category translation="emotions">emoties</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">V Claegh ick, hooge <rhyme label="a" type="m">Goon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe my een Nemphje <rhyme label="a" type="m" corresp="#s1:l1">schoon</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Soo schielijck heeft <rhyme label="b" type="f">begeven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ha! licht-verleyde <rhyme label="c" type="m">Vrouw</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Die u Ziel en <rhyme label="b" type="f" corresp="#s1:l3">leven</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Eertijts mijn off'ren <rhyme label="c" type="m" corresp="#s1:l4">wouw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat reed ick <rhyme label="a" type="m">mennigmael</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Tot u mijn Liefs <rhyme label="a" type="m" corresp="#s2:l1">onthael</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Door d' Haegsche schoon <rhyme label="b" type="f">Landouwen</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Met spele-vares <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Om u schoonheyt t' <rhyme label="b" type="f" corresp="#s2:l3">aenschouwen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En duysent-mael <rhyme label="c" type="m" corresp="#s2:l4">gekust</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Is u (Lief, uyt <rhyme label="a" type="m">ghedacht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hoe dickmael in de <rhyme label="a" type="m" corresp="#s3:l1">Nacht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wy gingen t' samen <rhyme label="b" type="f">wand'len</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Langhs <rhyme label="c" type="m">Dorpen-woninghs-laen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Om van min-lust te <rhyme label="b" type="f" corresp="#s3:l3">hand'len</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Met suchjens en <rhyme label="c" type="m" corresp="#s3:l4">getraen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doe ick, o aertsche <rhyme label="a" type="m">pronck</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Een heete vlam <rhyme label="a" type="m" corresp="#s4:l1">in-dronck</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Want by de groene <rhyme label="b" type="f">Wooningh</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer zwoert ghy by u <rhyme label="c" type="m">trouw</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat ick was u <rhyme label="b" type="f" corresp="#s4:l3">harts-Koningh</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En eeuwigh blijven <rhyme label="c" type="m" corresp="#s4:l4">souw</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Noyt heeft ons yet <rhyme label="a" type="m">beledt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ghy hebt my dagh <rhyme label="a" type="m" corresp="#s5:l1">gheset</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En dat oock voor een <rhyme label="b" type="f">Nachje</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tot binnen <rhyme label="c" type="m">Rotterdam</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Met een vriendelijck <rhyme label="b" type="f" corresp="#s5:l3">lachje</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Tot blussing' van mijn <rhyme label="c" type="m" corresp="#s5:l4">vlam</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat had ick soete <rhyme label="a" type="m">Meyt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Met u al <rhyme label="a" type="m" corresp="#s6:l1">vriend'licheyt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Gins in de Delfsche <rhyme label="b" type="f">Landen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Door lonckjes en <rhyme label="c" type="m">gegluer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Met gedruck van u <rhyme label="b" type="f" corresp="#s6:l3">Handen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Een daghje scheen een <rhyme label="c" type="m" corresp="#s6:l4">uer</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Straeltjes uw's <rhyme label="a" type="m">Gesicht</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Doofden het <rhyme label="a" type="m" corresp="#s7:l1">Sonne-licht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wy saten in de <rhyme label="b" type="f">Weytjes</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En sagen 't Hoornigh <rhyme label="c" type="m">Vee</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En de weeldige <rhyme label="b" type="f" corresp="#s7:l3">Geytjes</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ons daer vermaken <rhyme label="c" type="m" corresp="#s7:l4">dee</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maer doen ick (soete <rhyme label="a" type="m">Bruyt</rhyme>)</l>
        <l id="s8:l2" met="3" type="-+">U bracht totaen de <rhyme label="a" type="m" corresp="#s8:l1">Schuyt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Hoe droevigh was het <rhyme label="b" type="f">scheyen</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Maer, op dat geen <rhyme label="c" type="m">getraen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">U ontsierde door 't <rhyme label="b" type="f" corresp="#s8:l3">schreyen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Volchd' ick stracks achter <rhyme label="c" type="m" corresp="#s8:l4">aen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En quam daer by <rhyme label="a" type="m">Me-vrouw</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In 't Heusdens schoon <rhyme label="a" type="m" corresp="#s9:l1">Gebouw</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Daer wy soo vriend'lijck <rhyme label="b" type="f">rusten</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dien nacht, als Arm, in <rhyme label="c" type="m">Arm</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">En wel duysentmael <rhyme label="b" type="f" corresp="#s9:l3">kusten</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Met Liefdens soet <rhyme label="c" type="m" corresp="#s9:l4">gewarm</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Heught u niet meer den <rhyme label="a" type="m">Eedt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die ghy dien Nacht my <rhyme label="a" type="m" corresp="#s10:l1">deedt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">My nimmer te <rhyme label="b" type="f">verlaten</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Ghy zwoert dat noyt geen <rhyme label="c" type="m">Prins</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Noch geen Koningh sijn <rhyme label="b" type="f" corresp="#s10:l3">staten</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Ghy ruylen wout <rhyme label="c" type="m" corresp="#s10:l4">geenssins</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wat maeckten ghy <rhyme label="a" type="m">getreur</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En lonckten om de <rhyme label="a" type="m" corresp="#s11:l1">deur</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Eer ghy van my kost <rhyme label="b" type="f">scheyen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Wat toonde ghy al <rhyme label="c" type="m">gonst</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">'t Scheen u hart wou my <rhyme label="b" type="f" corresp="#s11:l3">leyen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Door u vermonde <rhyme label="c" type="m" corresp="#s11:l4">konst</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Maer, sy! licht-vaerde <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Schaemt dat de Aerd' u <rhyme label="a" type="m" corresp="#s12:l1">draeght</rhyme>:</l>
        <l id="s12:l3" met="3" type="-+">Maer ghy hebt schaemt <rhyme label="b" type="f">verloren</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Waer is u trouw <rhyme label="c" type="m">verbondt</rhyme>?</l>
        <l id="s12:l5" met="3" type="-+">Dat ghy vals hebt <rhyme label="b" type="f" corresp="#s12:l3">ghezworen</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Wt u verweende <rhyme label="c" type="m" corresp="#s12:l4">mont</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ha! lichteloos <rhyme label="a" type="m">verleyt</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Al u <rhyme label="a" type="m" corresp="#s13:l1">volstandigheyt</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Is nu verkeert <rhyme label="b" type="f">lichtvaerdigh</rhyme>:</l>
        <l id="s13:l4" met="3" type="-+">Dat u de Son <rhyme label="c" type="m">bestraelt</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Zijt ghy snoode niet <rhyme label="b" type="f" corresp="#s13:l3">waerdigh</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Want ghy in 't Licht <rhyme label="c" type="m" corresp="#s13:l4">verdwaelt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Vreest ghy niet u <rhyme label="a" type="m">gelaet</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Te steken op de <rhyme label="a" type="m" corresp="#s14:l1">straet</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Of voor mijn Oog te <rhyme label="b" type="f">komen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Maer d' ontrouw' vals en <rhyme label="c" type="m">boos</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Heeft u schaemt' al <rhyme label="b" type="f" corresp="#s14:l3">benomen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Soo blijft dan <rhyme label="c" type="m" corresp="#s14:l4">Eede-loos</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
