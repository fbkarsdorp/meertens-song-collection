    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pronkje van de maagden</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">LIefd' en reyne <rhyme label="a" type="f">trouwe</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Komt ons hier voor <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Siet ons Bruyt en <rhyme label="a" type="f" corresp="#s1:l1">vrouwe</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Heerlijck <rhyme label="b" type="f" corresp="#s1:l2">op-getogen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Liefd' haer <rhyme label="b" type="f" corresp="#s1:l2">bewogen</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Te treden in den <rhyme label="c" type="m">Echt</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Speelnootjes, <rhyme label="d" type="f">pylootjes</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Hert-binders, <rhyme label="e" type="f">vreught-vinders</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Nu Kransjes <rhyme label="c" type="m" corresp="#s1:l6">vlecht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Helpt haer lof <rhyme label="a" type="f">vermeeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Ghy die hier ter <rhyme label="b" type="f">stede</rhyme></l>
        <l id="s2:l3" met="3" type="+-">'t Ionge Paer ter <rhyme label="a" type="f" corresp="#s2:l1">eeren</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Zijt gekomen <rhyme label="b" type="f" corresp="#s2:l2">mede</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Wenst haer toe veel <rhyme label="b" type="f" corresp="#s2:l2">vrede</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Geluck in dese <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Dat hare, <rhyme label="d" type="f">eerbare</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">In 't leve, Godt <rhyme label="e" type="f">geve</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Zijn gunst te <rhyme label="c" type="m" corresp="#s2:l6">baet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Bruydegom wilt <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Uwe Bruyt vol <rhyme label="b" type="f">eere</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Met u hert en <rhyme label="a" type="f" corresp="#s3:l1">sinnen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Soeckt haer vreughts <rhyme label="b" type="f" corresp="#s3:l2">vermeeren</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">In de vrees des <rhyme label="b" type="f" corresp="#s3:l2">Heeren</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Wilt leven na Gods <rhyme label="c" type="m">woort</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Met vreughden, in <rhyme label="d" type="f">deughden</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Wilt seyen, en <rhyme label="e" type="f">meyen</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Soo als 't <rhyme label="c" type="m" corresp="#s3:l6">behoort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wilt vrouw Bruyt <rhyme label="a" type="f">betoonen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Uwe man veel <rhyme label="b" type="f">trouwe</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Vreed'lijck by hem <rhyme label="a" type="f" corresp="#s4:l1">woonen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">d' Hemel t'samen <rhyme label="b" type="f" corresp="#s4:l2">bouwen</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Als een eerbaer <rhyme label="b" type="f" corresp="#s4:l2">vrouwe</rhyme></l>
        <l id="s4:l6" met="3" type="+-">U altoos deughd'lijck <rhyme label="c" type="m">draeght</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">De Sonde, in 't <rhyme label="d" type="f">ronde</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Wilt haten, en <rhyme label="e" type="f">laten</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Die Godt <rhyme label="c" type="m" corresp="#s4:l6">mishaeght</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Wilt al t'samen <rhyme label="a" type="f">mercken</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Op de handt des <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Op sijn <rhyme label="a" type="f" corresp="#s5:l1">wonder-wercken</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Hoe hy 't al gaet <rhyme label="b" type="f" corresp="#s5:l2">keeren</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Yder moet hem <rhyme label="b" type="f" corresp="#s5:l2">eeren</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Hier in dit aerdtsche <rhyme label="c" type="m">dal</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Dus noemen, en <rhyme label="d" type="f">roemen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Wy heden, met <rhyme label="e" type="f">reden</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Godt boven <rhyme label="c" type="m" corresp="#s5:l6">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
