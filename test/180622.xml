  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">â€˜k Gelijck u mijn vrindin by <rhyme label="a" type="f">Paerden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In wagenen van <rhyme label="b" type="f">Pharao</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">U wangen in de spangen <rhyme label="c" type="m">fijn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Lieff lief-lijck uwen hals in <rhyme label="d" type="f">peerle</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">In peerle <rhyme label="e" type="f">snoeren</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy sullen goude, goude <rhyme label="a" type="f">spangen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Met silver stipkens maken <rhyme label="b" type="f">U</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Terwijle dat de Coninck <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Sijn ronde Tafel sit, mijn <rhyme label="d" type="f">Nardus</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Geeft, geeft sijn <rhyme label="e" type="f">reucke</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn Liefste een bundelken <rhyme label="a" type="f">Myrre</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Een Myrrhe Bundelken my <rhyme label="b" type="f">is</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat tusschen mijn borsten <rhyme label="c" type="m">vernacht</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Mijn Liefste is een Cypres tros <rhyme label="d" type="f">in</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Engedys <rhyme label="e" type="f">Wijngaert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Siet gy sijt schoone mijn <rhyme label="a" type="f">Vriendinne</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Siet gy sijt schoon, U oogen <rhyme label="b" type="f">sijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">U oogen Duyven oogen <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Siet gy sijt schoon, gy syt mijn <rhyme label="d" type="f">Liefste</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Ia lieflijck, <rhyme label="e" type="f">lieflijck</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Oock groent, oock groent onse <rhyme label="a" type="f">Bedt-stede</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">De Balcken onser Huysen <rhyme label="b" type="f">fijn</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">De Balcken onser Huysen <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Sijn Cedren, onse <rhyme label="d" type="f">Galeryen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Die sijn <rhyme label="e" type="f">Cypressen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
