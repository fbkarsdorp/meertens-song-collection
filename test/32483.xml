    <melody>Corydon zucht nacht en dag</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Schoon Aurora vanden <rhyme label="a" type="m">dagh</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die smorgens met een soet <rhyme label="a" type="m" corresp="#s1:l1">gelagh</rhyme></l>
        <l id="s1:l3" met="3" type="-+">De Bloemkens doet <rhyme label="b" type="f">ver-rysen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">In Floras schoonen <rhyme label="c" type="m">Hoff</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Gy gaeft my heden rechte <rhyme label="c" type="m" corresp="#s1:l4">stoff</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Mijn Nymph te <rhyme label="b" type="f" corresp="#s1:l3">prysen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al wat den milden Hemel <rhyme label="a" type="m">geeft</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Natuer in haer vergadert <rhyme label="a" type="m" corresp="#s2:l1">heeft</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Dees ick haer magh <rhyme label="b" type="f">gelijcken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">By Cloris schoonen <rhyme label="c" type="m">glans</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Daer sy met recht den <rhyme label="c" type="m" corresp="#s2:l4">Roosen-crans</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Voor my magh <rhyme label="b" type="f" corresp="#s2:l3">strycken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Heeft oyt Apelles op 't <rhyme label="a" type="m">Panneel</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Aff-gemaelt met syn kloeck <rhyme label="a" type="m" corresp="#s3:l1">Pinseel</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Roxana naer het <rhyme label="b" type="f">leven</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">Soo heb ick reden <rhyme label="c" type="m">dan</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Mijn Nymph te Schild'ren soo ick <rhyme label="c" type="m" corresp="#s3:l4">kan</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">En loff te <rhyme label="b" type="f" corresp="#s3:l3">geven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gelijck de Maen door rechte <rhyme label="a" type="m">plicht</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Moet vande Son haer <rhyme label="a" type="m" corresp="#s4:l1">helder-licht</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En claericheyt <rhyme label="b" type="f">ontfangen</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Soo schept mijn Ziel <rhyme label="c" type="m">genucht</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En claert de Wolcken van 't <rhyme label="c" type="m" corresp="#s4:l4">gesucht</rhyme>.</l>
        <l id="s4:l6" met="2" type="-+">Door haer <rhyme label="b" type="f" corresp="#s4:l3">om-vangen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Al moght my Cesars Gulden <rhyme label="a" type="m">Troon</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Het Purper Ryck, Staff ende <rhyme label="a" type="m" corresp="#s5:l1">Kroon</rhyme>â€™</l>
        <l id="s5:l3" met="3" type="-+">Voor haer na wensch <rhyme label="b" type="f">gebeuren</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Ick achten't min als <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Want sonder haer, ick in 't <rhyme label="c" type="m" corresp="#s5:l4">verdriet</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Sou moeten <rhyme label="b" type="f" corresp="#s5:l3">treuren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ten is om Ryckdom, noch om <rhyme label="a" type="m">schat</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Dat mijn trouw ziel heeft <rhyme label="a" type="m" corresp="#s6:l1">aen-gevat</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Haer jonste te <rhyme label="b" type="f">betrachten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Maer d'eer-baerheyt, en <rhyme label="c" type="m">deught</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Dat is de recht vernoeghde <rhyme label="c" type="m" corresp="#s6:l4">vreught</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Van mijn <rhyme label="b" type="f" corresp="#s6:l3">gedachten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O schoon Diana van ons <rhyme label="a" type="m">Landt</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Waer door mijn hert in MINNE <rhyme label="a" type="m" corresp="#s7:l1">brandt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Geeft my u teere <rhyme label="b" type="f">handen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat ick die druck aen 't <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Eens magh verkoelen van mijn <rhyme label="c" type="m" corresp="#s7:l4">smert</rhyme>.</l>
        <l id="s7:l6" met="2" type="-+">En soete <rhyme label="b" type="f" corresp="#s7:l3">banden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
