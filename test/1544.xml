    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">O heyl'ge Heer! o groote waere <rhyme label="a" type="m">God</rhyme>!</l>
        <l id="s1:l2" met="5" type="-+">Hoe lang sult gy met wraek noch niet <rhyme label="b" type="f">ontsteken</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Hoe lang toch noch hoe lang ons bloed niet <rhyme label="b" type="f" corresp="#s1:l2">wreken</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Noch neder-sien van uyt dijns hemels <rhyme label="a" type="m" corresp="#s1:l1">slot</rhyme>?</l>
      </lg>
    </lg>
