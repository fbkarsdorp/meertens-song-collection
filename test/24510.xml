<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24510</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Het viel een hemelse dauw</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">God gruet v <rhyme label="a" type="f">Conyngynne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Maria suet <rhyme label="b" type="m">gheluyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">gy syt een vat vol <rhyme label="a" type="f" corresp="#s1:l1">mynnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">altoes <rhyme label="b" type="m" corresp="#s1:l2">gebenedy</rhyme></l>
        <l id="s1:l5" met="3" type="-+">o weerde bruyt der <rhyme label="c" type="f">weelden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">v lof is <rhyme label="d" type="m">ewichlanck</rhyme></l>
        <l id="s1:l7" met="3" type="-+">fonteyne des ewige  <rhyme label="c" type="f" corresp="#s1:l5">l[..]ens</rhyme></l>
        <l id="s1:l8" met="3" type="-+">schencket ons der mynnen <rhyme label="d" type="m" corresp="#s1:l6">dranck</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">God gruet v alre <rhyme label="a" type="f">claerste</rhyme></l>
        <l id="s2:l2" met="3" type="-+">die ic aenschouwen <rhyme label="b" type="m">mach</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Maria onse <rhyme label="a" type="f" corresp="#s2:l1">bewaerster</rhyme></l>
        <l id="s2:l4" met="3" type="-+">sy is een ewich <rhyme label="b" type="m" corresp="#s2:l2">dach</rhyme></l>
        <l id="s2:l5" met="3" type="-+">wal schoenheit sonder <rhyme label="c" type="f">verdrieten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">vol alre doegden <rhyme label="d" type="m">groet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">och mocht ic oer <rhyme label="c" type="f" corresp="#s2:l5">genyeten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">die edelle <rhyme label="d" type="m" corresp="#s2:l6">roseroet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">God gruet v mergen <rhyme label="a" type="f">steerne</rhyme></l>
        <l id="s3:l2" met="3" type="-+">O spiegel sonder <rhyme label="b" type="m">smyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">schoenheit sonder <rhyme label="a" type="f" corresp="#s3:l1">meeren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">O lelye <rhyme label="b" type="m" corresp="#s3:l2">onbesmyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Maria edelle <rhyme label="c" type="f">sonne</rhyme></l>
        <l id="s3:l6" met="3" type="-+">ic mach v geuen <rhyme label="d" type="m">prys</rhyme></l>
        <l id="s3:l7" met="3" type="-+">gy syt een suuer <rhyme label="c" type="f" corresp="#s3:l5">blome</rhyme></l>
        <l id="s3:l8" met="3" type="-+">een roese des <rhyme label="d" type="m" corresp="#s3:l6">paradys</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">God gruet v suuer <rhyme label="a" type="f">maget</rhyme></l>
        <l id="s4:l2" met="3" type="-+">O peerle der joncfrouwen <rhyme label="b" type="m">schoen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">gy schynt gelic der <rhyme label="a" type="f" corresp="#s4:l1">sonnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">al in des hemels <rhyme label="b" type="m" corresp="#s4:l2">throen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">daer is sy <rhyme label="c" type="f">wtuercaeren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">een spiegel al <rhyme label="d" type="m">volmaeket</rhyme></l>
        <l id="s4:l7" met="3" type="-+">als een vercierde <rhyme label="c" type="f" corresp="#s4:l5">roese</rhyme></l>
        <l id="s4:l8" met="3" type="-+">och moeder ende <rhyme label="d" type="m" corresp="#s4:l6">maget</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">God gruet v suuer <rhyme label="a" type="f">Maget</rhyme></l>
        <l id="s5:l2" met="3" type="-+">O suete <rhyme label="b" type="m">medecyne</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Och dach die ewich <rhyme label="a" type="f" corresp="#s5:l1">daget</rhyme></l>
        <l id="s5:l4" met="3" type="-+">hoe geerne waer ic by <rhyme label="b" type="m" corresp="#s5:l2">dy</rhyme></l>
        <l id="s5:l5" met="3" type="-+">en wilt my niet <rhyme label="c" type="f">vergeten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">wal edel <rhyme label="d" type="m">honichroete</rhyme></l>
        <l id="s5:l7" met="3" type="-+">als my myn here sal <rhyme label="c" type="f" corresp="#s5:l5">breken</rhyme></l>
        <l id="s5:l8" met="3" type="-+">al inder vren myns <rhyme label="d" type="m" corresp="#s5:l6">dodes</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
