<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>166467</id>
    <date>
      <year-start>1749</year-start>
      <year-end>1749</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Que vous trompettez</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">KOmt wilt <rhyme label="a" type="f">beschouwe</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">De groote vreugde ende <rhyme label="b" type="m">swier</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Die men zal <rhyme label="a" type="f" corresp="#s1:l1">houwe</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Met veel <rhyme label="b" type="m" corresp="#s1:l2">plaisier</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Nu Holland weder is <rhyme label="c" type="m">hersteld</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Door Friso den Oranje <rhyme label="c" type="m" corresp="#s1:l5">held</rhyme>;</l>
        <l id="s1:l7" met="4" type="-+">Tot Vorst gekoren, door 't Frans <rhyme label="c" type="m" corresp="#s1:l5">geweld</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hy heeft de <rhyme label="a" type="f">Vrede</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Doen bloeijen wêer gelyk <rhyme label="b" type="m">voorheen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Dus moet op <rhyme label="a" type="f" corresp="#s2:l1">heden</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Mars dadelyks <rhyme label="b" type="m" corresp="#s2:l2">heen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">'t Was buyten nauwelyks <rhyme label="c" type="m">gedaan</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Den kryg ging binne weder <rhyme label="c" type="m" corresp="#s2:l5">aan</rhyme>;</l>
        <l id="s2:l7" met="4" type="-+">De Pachters huyzen, zy plund'ren <rhyme label="c" type="m" corresp="#s2:l5">gaan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">D'Overheid der <rhyme label="a" type="f">Steeden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Wierd ook te zaame door 't <rhyme label="b" type="m">gemeen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Al mêe <rhyme label="a" type="f" corresp="#s3:l1">beleeden</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Dat daar moest <rhyme label="b" type="m" corresp="#s3:l2">heen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Die zy niet keurde dan voor <rhyme label="c" type="m">goed</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Moest men afzette met 'er <rhyme label="c" type="m" corresp="#s3:l5">spoet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ziet wat de onlust en twist al <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">'t Was al in <rhyme label="a" type="f">roere</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Waar dat men hoorde ofte <rhyme label="b" type="m">zag</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Komt Burgers en <rhyme label="a" type="f" corresp="#s4:l1">Boeren</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Viert dezen <rhyme label="b" type="m" corresp="#s4:l2">dag</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Want alle onheil is <rhyme label="c" type="m">voorby</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Batavies Haagsche <rhyme label="c" type="m" corresp="#s4:l5">Schuttery</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Gaat weder trekken, in deeze <rhyme label="c" type="m" corresp="#s4:l5">Mey</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Dingsdag <rhyme label="a" type="f">marcheeren</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">De Burgers, voor het <rhyme label="b" type="m">allereerst</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Charremant van <rhyme label="a" type="f" corresp="#s5:l1">Kleeren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">In het <rhyme label="b" type="m" corresp="#s5:l2">Kermis-Feest</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Spaart nu geen Kan met Wyn of <rhyme label="c" type="m">Bier</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Maar maakt te zaam een goede <rhyme label="c" type="m" corresp="#s5:l5">cier</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Den Prins ter eere, en 't vreugde <rhyme label="c" type="m" corresp="#s5:l5">Vier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Dat in de <rhyme label="a" type="f">Vyver</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Verrigt werd met veel heast en <rhyme label="b" type="m">spoet</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Om de Vrede <rhyme label="a" type="f" corresp="#s6:l1">styver</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Te doen nemen <rhyme label="b" type="m" corresp="#s6:l2">Voet</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">God geeve dat ze lang mag <rhyme label="c" type="m">staan</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">En 't Neêrland altyd wel mag <rhyme label="c" type="m" corresp="#s6:l5">gaan</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Onder 't bestiere van Oranjes <rhyme label="c" type="m" corresp="#s6:l5">Vaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
