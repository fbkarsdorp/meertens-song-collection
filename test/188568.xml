  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Was ons sterflyk oog niet <rhyme label="a" type="m">blind</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Was de mensch niet als een <rhyme label="a" type="m" corresp="#s1:l1">kind</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Grooter wonders zou hy <rhyme label="b" type="m">zien</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dan hy nimmer zag <rhyme label="b" type="m" corresp="#s1:l3">geschiÃªn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wonders ziet ge in 't minste <rhyme label="a" type="m">kruidt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat uit modd'rige aerde <rhyme label="a" type="m" corresp="#s2:l1">spruidt</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Wonders ziet gy <rhyme label="b" type="m">overal</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'t Zy op 't Veld, in Beemd, en <rhyme label="b" type="m" corresp="#s2:l3">Dal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waer ge u keert, of henen <rhyme label="a" type="m">wendt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">'t Zyn al wonders zonder <rhyme label="a" type="m" corresp="#s3:l1">endt</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Vier en Water, Lucht en <rhyme label="b" type="m">Aerd</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Niet dan enkle wonders <rhyme label="b" type="m" corresp="#s3:l3">baert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer de Mensch die 't alles <rhyme label="a" type="m">ziet</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Kan nogthans de Godheid <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Is dan 't sterflyk oog niet <rhyme label="b" type="m">blind</rhyme>?</l>
        <l id="s4:l4" met="4" type="+-">Is de Mensch niet als een <rhyme label="b" type="m" corresp="#s4:l3">kind</rhyme>?</l>
      </lg>
    </lg>
  </text>
