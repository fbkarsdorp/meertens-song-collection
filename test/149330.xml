    <melody>Psalm 026 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEweldelijck <rhyme label="a" type="m">geplaeght</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Argh-listelijck <rhyme label="a" type="m" corresp="#s1:l1">belaeght</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Met lasteringe dicht <rhyme label="b" type="m">om-vaen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Onreyn in <rhyme label="c" type="f">menschen-oogen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Door konst der stouter <rhyme label="c" type="f" corresp="#s1:l4">logen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Roep ick u, Godt, tot richter <rhyme label="b" type="m" corresp="#s1:l3">aen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In’t dichst van <rhyme label="a" type="m">tegen-spoedt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Geeft my mijn onschuldt <rhyme label="a" type="m" corresp="#s2:l1">moedt</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">De moedt, vertrouwen op den <rhyme label="b" type="m">Heer</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">De Heer, my, zijn <rhyme label="c" type="f">vertrouwer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En nauwe <rhyme label="c" type="f" corresp="#s2:l4">bondt-behouwer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Ten val te komen <rhyme label="b" type="m" corresp="#s2:l3">nimmermeer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doe, Heer mijn Godt, doe <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Een scherpe proev’ op <rhyme label="a" type="m" corresp="#s3:l1">my</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Off ’tbinnenst is als ’tbuytenst <rhyme label="b" type="m">toont</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Sinck ne’er met ’sgeestes <rhyme label="c" type="f">krachten</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Int donckerst der <rhyme label="c" type="f" corresp="#s3:l4">gedachten</rhyme>;</l>
        <l id="s3:l6" met="4" type="-+">Aenschouw wat in mijn diepste <rhyme label="b" type="m" corresp="#s3:l3">woont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daer sult ghy sien een <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Waer niets geherberght <rhyme label="a" type="m" corresp="#s4:l1">werdt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Dan louterheydt van geest en <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Daer klare <rhyme label="c" type="f">heugenisse</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Van uw’ <rhyme label="c" type="f" corresp="#s4:l4">erbarmenisse</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Heeft alle denckens krachten <rhyme label="b" type="m" corresp="#s4:l3">in</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daer alles wordt <rhyme label="a" type="m">gericht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Na ’tsnoer van waerheydts <rhyme label="a" type="m" corresp="#s5:l1">licht</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Dies ick noyt lossen gang en <rhyme label="b" type="m">doe</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maer met bedachte <rhyme label="c" type="f">treden</rhyme></l>
        <l id="s5:l5" met="3" type="-+">In suyverheydt van <rhyme label="c" type="f" corresp="#s5:l4">zeden</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Standtvastelijck tot uwaerts <rhyme label="b" type="m" corresp="#s5:l3">spoe</rhyme>’.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Den mensch wiens yd’le <rhyme label="a" type="m">mondt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Is oneens met sijn <rhyme label="a" type="m" corresp="#s6:l1">grondt</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Gewaerdig’ ick mijn by-zijn <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Wie isser die met <rhyme label="c" type="f">sluypers</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Off schoon-vermomde <rhyme label="c" type="f" corresp="#s6:l4">luypers</rhyme></l>
        <l id="s6:l6" met="4" type="-+">My immer my vermengen <rhyme label="b" type="m" corresp="#s6:l3">siet</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ick haet in ernst, en <rhyme label="a" type="m">my</rhyme>’</l>
        <l id="s7:l2" met="3" type="-+">De <rhyme label="a" type="m" corresp="#s7:l1">t’samen-rottery</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Van menschen, die, veraerdt van <rhyme label="b" type="m">ze’en</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In stoute <rhyme label="c" type="f">achteloosheydt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">En ongetoomde <rhyme label="c" type="f" corresp="#s7:l4">boosheydt</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Ongoddelijcke paden <rhyme label="b" type="m" corresp="#s7:l3">tre’en</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Met een gewis <rhyme label="a" type="m">gemoedt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Daer niet in woont dan <rhyme label="a" type="m" corresp="#s8:l1">goedt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daer geen verholen sonden <rhyme label="b" type="m">broen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Wil ick my, Heer, <rhyme label="c" type="f">op-maken</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En ’theyligh altaer <rhyme label="c" type="f" corresp="#s8:l4">naken</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">En een behaeghlijck offer <rhyme label="b" type="m" corresp="#s8:l3">doen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Daer sal met soet <rhyme label="a" type="m">gebrom</rhyme></l>
        <l id="s9:l2" met="3" type="-+">’tGeluydt van Luyt en <rhyme label="a" type="m" corresp="#s9:l1">trom</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Verselschapt met mijn <rhyme label="b" type="m">stem-geklanck</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Vw’ wonderhe’en doen <rhyme label="c" type="f">hooren</rhyme></l>
        <l id="s9:l5" met="3" type="-+">En d’omme-staende <rhyme label="c" type="f" corresp="#s9:l4">ooren</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Verlustigen door <rhyme label="b" type="m" corresp="#s9:l3">loff-gesanck</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De plaetse daer ghy <rhyme label="a" type="m">woont</rhyme></l>
        <l id="s10:l2" met="3" type="-+">En sichtelijck in <rhyme label="a" type="m" corresp="#s10:l1">toont</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Vws goedtheydts en uws Godtheyts <rhyme label="b" type="m">glants</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Daer d’uytgedruckte <rhyme label="c" type="f">panden</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Vws glorijs zijn <rhyme label="c" type="f" corresp="#s10:l4">voorhanden</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Vermaeckt mijn ziel en trecktse <rhyme label="b" type="m" corresp="#s10:l3">gants</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Dus hangt mijn hert aen <rhyme label="a" type="m">dy</rhyme>:</l>
        <l id="s11:l2" met="3" type="-+">Daerom en blusch <rhyme label="a" type="m" corresp="#s11:l1">t’onty</rhyme>’</l>
        <l id="s11:l3" met="4" type="-+">Des levens vonck alsoo niet <rhyme label="b" type="m">uyt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Als in haer, dien, aen <rhyme label="c" type="f">sonden</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Te slaeffelijck <rhyme label="c" type="f" corresp="#s11:l4">gebonden</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">De straff den deur der boete <rhyme label="b" type="m" corresp="#s11:l3">sluyt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ontruck my ’tleven <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Gelijckmen veeltijts <rhyme label="a" type="m" corresp="#s12:l1">siet</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Dat sy in yl ter Hellen <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Wiens keel om bloedt te <rhyme label="c" type="f">drincken</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Wiens handen tot <rhyme label="c" type="f" corresp="#s12:l4">geschincken</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Wiens le’en tot listen vaerdich <rhyme label="b" type="m" corresp="#s12:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">De dwaleloose <rhyme label="a" type="m">baen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Van mijn volmaeckte <rhyme label="a" type="m" corresp="#s13:l1">pae’n</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Sal ick, onsondigh, blijven <rhyme label="b" type="m">tre’en</rhyme>.</l>
        <l id="s13:l4" met="3" type="-+">Daerom, Heer, maeck een <rhyme label="c" type="f">ende</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Van droefheyt en <rhyme label="c" type="f" corresp="#s13:l4">ellende</rhyme>.</l>
        <l id="s13:l6" met="4" type="-+">uyt volheyt der <rhyme label="b" type="m" corresp="#s13:l3">genadiche’en</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Dat ick met vasten <rhyme label="a" type="m">tredt</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Mijn gangen richtigh <rhyme label="a" type="m" corresp="#s14:l1">set</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Is uwes goedtheyts milde <rhyme label="b" type="m">vrucht</rhyme>.</l>
        <l id="s14:l4" met="3" type="-+">Dies ick my niet sal <rhyme label="c" type="f">schromen</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Waer menschen t’samen <rhyme label="c" type="f" corresp="#s14:l4">komen</rhyme></l>
        <l id="s14:l6" met="4" type="-+">Te eeuwigen uws naems <rhyme label="b" type="m" corresp="#s14:l3">gerucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
