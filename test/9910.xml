  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WIlt yemandt rechte wijsheyt <rhyme label="a" type="f">leeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Nae des Heeren// woordt <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die moet het goet altijts <rhyme label="a" type="f" corresp="#s1:l1">vermeeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">End' hem bekeeren// van zonden <rhyme label="b" type="m" corresp="#s1:l2">swaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Want dat is <rhyme label="c" type="f">wijshede</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die hier in Gods <rhyme label="c" type="f" corresp="#s1:l5">vrede</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Wandelt op elck <rhyme label="d" type="m">termijn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Als wijnrancxkens <rhyme label="e" type="f">becleven</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Niet meer in zonden <rhyme label="e" type="f" corresp="#s1:l8">leven</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Van men herboren <rhyme label="d" type="m" corresp="#s1:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wie hem vernedert voor Gods <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Nae t'betooghen// van sijn <rhyme label="b" type="m">woordt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">End' hem om goet te doen wilt <rhyme label="a" type="f" corresp="#s2:l1">tooghen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Sal Godt weder verhooghen// <rhyme label="b" type="m" corresp="#s2:l2">hoort</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Maer daer en zijn niet <rhyme label="c" type="f">vele</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Machtighe noch <rhyme label="c" type="f" corresp="#s2:l5">ele</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Nae t'vleesch gheroepen <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Weereltsche wijse oft <rhyme label="e" type="f">rijcke</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Want sy staen al <rhyme label="e" type="f" corresp="#s2:l8">ghelijcke</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Op haer eyghen wijsheyt, <rhyme label="d" type="m" corresp="#s2:l7">siet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De wijsheyt light wilt dit <rhyme label="a" type="f">vaten</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Op der straten// ghevallen, <rhyme label="b" type="m">hoort</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wie datse op raept vry, sonder <rhyme label="a" type="f" corresp="#s3:l1">laten</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">T'sijner baten// nae Gods <rhyme label="b" type="m" corresp="#s3:l2">woordt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die wordt als <rhyme label="c" type="f">dwasen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Lieden die <rhyme label="c" type="f" corresp="#s3:l5">rasen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ter weerelt soo <rhyme label="d" type="m">ghenaemt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Want Gods <rhyme label="e" type="f">wijshede</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Dat is <rhyme label="e" type="f" corresp="#s3:l8">sottichede</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Voor de weerelt <rhyme label="d" type="m" corresp="#s3:l7">gheblaemt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die wijs wil wesen voor den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Nae de leere// van't <rhyme label="b" type="m">ghebodt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die wort ter weerelt, t'elcken <rhyme label="a" type="f" corresp="#s4:l1">keere</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Gheacht seere// als dwaes end' <rhyme label="b" type="m" corresp="#s4:l2">sot</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Want de weerelt <rhyme label="c" type="f">hoochmoedich</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Veracht Gods wijsheyt <rhyme label="c" type="f" corresp="#s4:l5">goedich</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">S'is haer selven <rhyme label="d" type="m">wijs</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Daeromme datse <rhyme label="e" type="f">dolen</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Want men leertse in gheen <rhyme label="e" type="f" corresp="#s4:l8">scholen</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Te Leuven, noch <rhyme label="d" type="m" corresp="#s4:l7">Parijs</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Christus Jesus ons <rhyme label="a" type="f">verader</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Die gaf sijnen Vader <rhyme label="b" type="m">prijs</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dat hy 't verborghen had voor die <rhyme label="a" type="f" corresp="#s5:l1">allegader</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Die waren nae de weerelt <rhyme label="b" type="m" corresp="#s5:l2">wijs</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">End' hy heeft den <rhyme label="c" type="f">cleenen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Wijsheyt willen <rhyme label="c" type="f" corresp="#s5:l5">verleenen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die altijts nae wijsheyt <rhyme label="d" type="m">vraeght</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Den armen <rhyme label="e" type="f">slechten</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Die sijn woort houdt ten <rhyme label="e" type="f" corresp="#s5:l8">rechten</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Alsoo hevet hem <rhyme label="d" type="m" corresp="#s5:l7">behaeght</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De weerelt en can op der <rhyme label="a" type="f">Aerden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Niet aenvaerden// dees <rhyme label="b" type="m">wijsheyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Want Gods Gheest hooghe van <rhyme label="a" type="f" corresp="#s6:l1">waerden</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Wijckt van den quaden [..] <rhyme label="b" type="m" corresp="#s6:l2">bereyt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Maer is by den <rhyme label="c" type="f">vromen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Sy en sal niet <rhyme label="c" type="f" corresp="#s6:l5">comen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">In een boosachtich <rhyme label="d" type="m">lijf</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dan die willen <rhyme label="e" type="f">bekeeren</rhyme></l>
        <l id="s6:l9" met="3" type="+-">Nae het woordt des <rhyme label="e" type="f" corresp="#s6:l8">Heeren</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Het zy Man ofte <rhyme label="d" type="m" corresp="#s6:l7">Wijf</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Want dese wijsheyt hoogh <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Wort ghegheven, <rhyme label="b" type="m">delicaet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die wil nae Gods gheboden <rhyme label="a" type="f" corresp="#s7:l1">leven</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Alsoo daer gheschreven// <rhyme label="b" type="m" corresp="#s7:l2">staet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Alle die den <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Lief hebben <rhyme label="c" type="f" corresp="#s7:l5">seere</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ontfanghen dees wijsheyt, <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Maer die in <rhyme label="e" type="f">zonden</rhyme></l>
        <l id="s7:l9" met="3" type="+-">Leven t'allen <rhyme label="e" type="f" corresp="#s7:l8">stonden</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Hebben dees wijsheyt <rhyme label="d" type="m" corresp="#s7:l7">niet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Gods vreese is dat begin <rhyme label="a" type="f">ydoone</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Van dees schoone// wijsheyt <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">De vreese Gods dat is een <rhyme label="a" type="f" corresp="#s8:l1">croone</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Om een persoone// wiese <rhyme label="b" type="m" corresp="#s8:l2">doet</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Naer Schrifts <rhyme label="c" type="f">bedieden</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Rechte cloecke <rhyme label="c" type="f" corresp="#s8:l5">lieden</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Maeckt dese wijsheyt <rhyme label="d" type="m">claer</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Wt haer <rhyme label="e" type="f">virtuyten</rhyme></l>
        <l id="s8:l9" met="3" type="+-">Alle deuchden <rhyme label="e" type="f" corresp="#s8:l8">spruyten</rhyme>,</l>
        <l id="s8:l10" met="3" type="-+">Sy brengt alle goet met <rhyme label="d" type="m" corresp="#s8:l7">haer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Niemant en weet hoort dit <rhyme label="a" type="f">vermonden</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Nae t'verconden// soo Job <rhyme label="b" type="m">seyt</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Waer dese wijsheyt was <rhyme label="a" type="f" corresp="#s9:l1">ghevonden</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Tot gheenen stonden// waerse <rhyme label="b" type="m" corresp="#s9:l2">leyt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Maer Godt, die alle <rhyme label="c" type="f">dinghen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Van niet conde <rhyme label="c" type="f" corresp="#s9:l5">volbrenghen</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">De Zee stelde in haer <rhyme label="d" type="m">perck</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Van hem is sy <rhyme label="e" type="f">ghesproten</rhyme>,</l>
        <l id="s9:l9" met="3" type="+-">End' hy heeftse <rhyme label="e" type="f" corresp="#s9:l8">uytghegoten</rhyme></l>
        <l id="s9:l10" met="3" type="-+">Over al sijn crachtich <rhyme label="d" type="m" corresp="#s9:l7">werck</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Gods woordt is een fonteyne <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Der wijshede// alsoo ick <rhyme label="b" type="m">las</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Dat is sijn woordt vol <rhyme label="a" type="f" corresp="#s10:l1">reynichede</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Dwelck eer des weerelts stede// <rhyme label="b" type="m" corresp="#s10:l2">was</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Dese wijsheyt <rhyme label="c" type="f">verheven</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Heeft Godt <rhyme label="c" type="f" corresp="#s10:l5">ghegheven</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Jacob sijnen <rhyme label="d" type="m">dienaer</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Israel sijnen <rhyme label="e" type="f">beminden</rhyme></l>
        <l id="s10:l9" met="3" type="+-">Mochte dees wijsheyt <rhyme label="e" type="f" corresp="#s10:l8">vinden</rhyme>,</l>
        <l id="s10:l10" met="3" type="-+">En die Godt vreest <rhyme label="d" type="m" corresp="#s10:l7">allegaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Gods vreese is wijsheyt, wilt <rhyme label="a" type="f">onthouwen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">End' quaet te schouwen// is 't <rhyme label="b" type="m">verstant</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Die Godt vreest sal den strijdt <rhyme label="a" type="f" corresp="#s11:l1">behouwen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">In sijn benouwen// zweert oft <rhyme label="b" type="m" corresp="#s11:l2">brandt</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Die vast houdt, sonder <rhyme label="c" type="f">keeren</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Aen haer, die helptse uyt met <rhyme label="c" type="f" corresp="#s11:l5">eeren</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Hierom, o Menschen, <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Bekeert u al <rhyme label="e" type="f">ghemeene</rhyme>,</l>
        <l id="s11:l9" met="3" type="+-">Leert dese wijsheyt <rhyme label="e" type="f" corresp="#s11:l8">reene</rhyme>,</l>
        <l id="s11:l10" met="3" type="-+">End' leeft der zonde <rhyme label="d" type="m" corresp="#s11:l7">niet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Sy leydt den rechten op den <rhyme label="a" type="f">pade</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Op dat niet en schade, <rhyme label="b" type="m">want</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">End' maeckt hem vruchtbaer, reyn van <rhyme label="a" type="f" corresp="#s12:l1">dade</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Vroech en spade// <rhyme label="b" type="m" corresp="#s12:l2">triumphant</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Dat sy als <rhyme label="c" type="f">boomen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Die by <rhyme label="c" type="f" corresp="#s12:l5">waterstroomen</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Staen, soo David <rhyme label="d" type="m">verhaelt</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Die altijts <rhyme label="e" type="f">groene</rhyme></l>
        <l id="s12:l9" met="3" type="+-">Staen, lustich in <rhyme label="e" type="f" corresp="#s12:l8">saysoene</rhyme>,</l>
        <l id="s12:l10" met="3" type="-+">Daer gheen drooch bladt af en <rhyme label="d" type="m" corresp="#s12:l7">daelt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Heeft yemandt ghebreck van <rhyme label="a" type="f">wijshede</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Die bede// aen Godt om wijsheyt, <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Hy gheefts' elcken in <rhyme label="a" type="f" corresp="#s13:l1">overvloedichede</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">End' hy en verwijtes niemant <rhyme label="b" type="m" corresp="#s13:l2">niet</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Die onghetwijfelt <rhyme label="c" type="f">beden</rhyme></l>
        <l id="s13:l6" met="3" type="-+">In den Gheest en <rhyme label="c" type="f" corresp="#s13:l5">waerheden</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Vernedert voor den <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Die salich willen <rhyme label="e" type="f">wesen</rhyme></l>
        <l id="s13:l9" met="3" type="+-">Gheeft hy wijsheyt <rhyme label="e" type="f" corresp="#s13:l8">ghepresen</rhyme>,</l>
        <l id="s13:l10" met="3" type="-+">Door sijn heylich woordt en <rhyme label="d" type="m" corresp="#s13:l7">leer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Hoe salich zijn wy, o <rhyme label="a" type="f">Israel</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Wy moghen wel// wesen <rhyme label="b" type="m">verblijt</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Dat ons Godt sijnen wille <rhyme label="a" type="f" corresp="#s14:l1">snel</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Nu heeft gheopenbaert <rhyme label="b" type="m" corresp="#s14:l2">subijt</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Niet ons <rhyme label="c" type="f">alleene</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Maer alle <rhyme label="c" type="f" corresp="#s14:l5">ghemeene</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Die de salicheyt <rhyme label="d" type="m">begheert</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">End' Gods woordt al <rhyme label="e" type="f">vooren</rhyme></l>
        <l id="s14:l9" met="3" type="+-">Nu oock willen <rhyme label="e" type="f" corresp="#s14:l8">hooren</rhyme>,</l>
        <l id="s14:l10" met="3" type="-+">Hem in deught <rhyme label="d" type="m" corresp="#s14:l7">gheneert</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Merckt, wie sal zijn bevonden <rhyme label="a" type="f">recht</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Soo Christus seght// al van 't <rhyme label="b" type="m">ghetal</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Die waeckt als een ghetrouwe <rhyme label="a" type="f" corresp="#s15:l1">knecht</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Tot dat den Bruydegom comen <rhyme label="b" type="m" corresp="#s15:l2">sal</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Dat zijn de <rhyme label="c" type="f">wijse</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Die ick hier <rhyme label="c" type="f" corresp="#s15:l5">prijse</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Ghecleedt met t'Bruylofts <rhyme label="d" type="m">cleedt</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Ghelijck de wijse <rhyme label="e" type="f">Maechden</rhyme>,</l>
        <l id="s15:l9" met="3" type="+-">Die den Bruygom <rhyme label="e" type="f" corresp="#s15:l8">behaechden</rhyme>,</l>
        <l id="s15:l10" met="3" type="-+">Sy waren al <rhyme label="d" type="m" corresp="#s15:l7">bereedt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Oorlof, soeckt nu dees wijsheyt <rhyme label="a" type="f">goet</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Wt recht ootmoet// alsoo <rhyme label="b" type="m">ghesint</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Wie nae des Heeren wille <rhyme label="a" type="f" corresp="#s16:l1">doet</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Tot sijn behoedt// dees wijsheyt <rhyme label="b" type="m" corresp="#s16:l2">vindt</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">T'is eenen schat hier <rhyme label="c" type="f">boven</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Dies wy u Heere <rhyme label="c" type="f" corresp="#s16:l5">loven</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">End' prijsen hooch <rhyme label="d" type="m">vermaert</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Dat ghy ons hier <rhyme label="e" type="f">beneven</rhyme></l>
        <l id="s16:l9" met="3" type="+-">Kennisse hebt <rhyme label="e" type="f" corresp="#s16:l8">ghegheven</rhyme>,</l>
        <l id="s16:l10" met="3" type="-+">V woordt <rhyme label="d" type="m" corresp="#s16:l7">gheopenbaert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
