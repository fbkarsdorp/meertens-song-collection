      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DEs boosdoenders wille seer <rhyme label="a" type="m">quaedt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ghetuyght sekerlijck met der <rhyme label="a" type="m" corresp="#s1:l1">daet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat hij niet vreest den <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Want hy hem in't boose <rhyme label="c" type="m">behaeght</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">'Twelck hy noch hatet noch <rhyme label="c" type="m" corresp="#s1:l4">beklaecht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Maer spot den vrome <rhyme label="b" type="f" corresp="#s1:l3">seere</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Seer schad'lijck is de leere <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Vol van leugen ende <rhyme label="d" type="m" corresp="#s1:l7">fenijn</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">En hy laet hem niet <rhyme label="e" type="f">leeren</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Des nachts denckt hy niet dan <rhyme label="f" type="m">schalckheydt</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Hy wil blijven inder <rhyme label="f" type="m" corresp="#s1:l10">boosheydt</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">End' van geen quaedt hem <rhyme label="e" type="f" corresp="#s1:l9">keeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Heer u goetheydt en u woort <rhyme label="a" type="m">waer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Tot der lucht en den Hemel <rhyme label="a" type="m" corresp="#s2:l1">klaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hen strecken na ons <rhyme label="b" type="f">wenschen</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">V oordeel vast als een bergh <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">V recht diep als een afgront <rhyme label="c" type="m" corresp="#s2:l4">gaet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ghy behoet Vee end' <rhyme label="b" type="f" corresp="#s2:l3">Menschen</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Hoe groot is u goetheyt <rhyme label="d" type="m">eenpaer</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Hen die onder u vleugelen <rhyme label="d" type="m" corresp="#s2:l7">haer</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Te begheven <rhyme label="e" type="f">ghedincken</rhyme>.</l>
        <l id="s2:l10" met="4" type="-+">Haer begeert vervult ghy met <rhyme label="f" type="m">goet</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">End' aen uwer wellusten <rhyme label="f" type="m" corresp="#s2:l10">vloet</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Voert ghy die datse <rhyme label="e" type="f" corresp="#s2:l9">drincken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In u is des levens <rhyme label="a" type="m">fonteyn</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">End' u klaerheyt gheeft ons <rhyme label="a" type="m" corresp="#s3:l1">alleyn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Licht end' verstant om <rhyme label="b" type="f">mercken</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Over de herten <rhyme label="c" type="m">onbevleckt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">V goedigheyt, O heere, <rhyme label="c" type="m" corresp="#s3:l4">streckt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die steets schijnt in u <rhyme label="b" type="f" corresp="#s3:l3">wercken</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Dat my de voet des menschen <rhyme label="d" type="m">wreet</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Niet en vertreed', end' my gheen <rhyme label="d" type="m" corresp="#s3:l7">leet</rhyme></l>
        <l id="s3:l9" met="3" type="-+">End' doe, noch slae mits <rhyme label="e" type="f">desen</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">Hoe de boose vergaen, doch <rhyme label="f" type="m">siet</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Sy blijven in eewigh <rhyme label="f" type="m" corresp="#s3:l10">verdriet</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Sonder verlost te <rhyme label="e" type="f" corresp="#s3:l9">wesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
