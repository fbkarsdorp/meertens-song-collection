    <tags>
      <tag standard-name="spotlied" translation="mocking song">spotlied</tag>
    </tags>
    <melody>Zoutmans glorie</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Vrienden ik zal u <rhyme label="a" type="f">verhaalen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hoe de Naaisters <rhyme label="b" type="m">komplaisant</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Fuurig na de nieuwste <rhyme label="c" type="f">Moode</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als de dames van ons <rhyme label="b" type="m" corresp="#s1:l2">Land</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Om Vrouw Venus te <rhyme label="d" type="f">behaagen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Want zy leeven in <rhyme label="e" type="m">minnepyn</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Voor de jongmans die haar <rhyme label="f" type="f">vrage</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Om van haar bemint te <rhyme label="e" type="m" corresp="#s1:l6">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wel hoe zou myn juffrouw <rhyme label="a" type="f">weeze</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Want het is ons naaisters <rhyme label="b" type="m">trant</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Om de jongmans te <rhyme label="c" type="f">geryven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hebbe wy knipjes agter de <rhyme label="b" type="m" corresp="#s2:l2">hand</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daar zy savonds by ons <rhyme label="d" type="f">koomen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Tracteeren op een Vlesje <rhyme label="e" type="m">Wyn</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Dat is de regte Naaisters <rhyme label="f" type="f">moode</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Soete lief gaat ge maar met <rhyme label="e" type="m" corresp="#s2:l6">myn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Soete susje aardig <rhyme label="a" type="f">meisje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Voelt gy somtyds niet wel <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zou gy niet wel voor een <rhyme label="c" type="f">reisje</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Eens van myn bemint te <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Speelt niet savons u <rhyme label="d" type="f">gedagten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Als gy gaat in bed <rhyme label="e" type="m">allyn</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Zou gy niet u minnaar <rhyme label="f" type="f">verwagten</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Om van hem gevreid te <rhyme label="e" type="m" corresp="#s3:l6">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Mogt ik eens u Boesem <rhyme label="a" type="f">voelen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Om te blusse myn <rhyme label="b" type="m">minnebrand</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ach wat zou myn hartje <rhyme label="c" type="f">verkoelen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Als ik streelden mee myn <rhyme label="b" type="m" corresp="#s4:l2">hand</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Ach myn alderliefste <rhyme label="d" type="f">Naaimeisje</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Was het niet een vreugde <rhyme label="e" type="m">groot</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Als ik eens mogt voor een <rhyme label="f" type="f">reisje</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Zoene voor jou Montje <rhyme label="e" type="m" corresp="#s4:l6">root</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ach myn alderliefste <rhyme label="a" type="f">hontje</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Zyt gy zoo vol <rhyme label="b" type="m">minnepyn</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En gy weet wel dat myn <rhyme label="c" type="f">montje</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Niet voor niet gezoent mag <rhyme label="b" type="m" corresp="#s5:l2">zyn</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Wel hoe kan een Naaister <rhyme label="d" type="f">werken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Waar zou ik Juffrouw van <rhyme label="e" type="m">zyn</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">En zoud gy dan nog niet <rhyme label="f" type="f">merken</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Want ons daggeld is te <rhyme label="e" type="m" corresp="#s5:l6">klein</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ach hoe speelen myn <rhyme label="a" type="f">gedagten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Och myn Engel en zoet <rhyme label="b" type="m">gelaat</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Sult gy van avond myn <rhyme label="c" type="f">verwagten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Als de klok agt uuren <rhyme label="b" type="m" corresp="#s6:l2">slaat</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Ach wat zal wy hebbe <rhyme label="d" type="f">vreugde</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Als wy dan zijn by <rhyme label="e" type="m">malkaar</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">'t Zal aan mijn niet <rhyme label="f" type="f">mankeeren</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Ik blyf uw getrouw <rhyme label="e" type="m" corresp="#s6:l6">minnaar</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">En ik zal alles wel <rhyme label="a" type="f">beryden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">En open spreien 't <rhyme label="b" type="m">Ledikant</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Daar op zal ik van <rhyme label="c" type="f">scheiden</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Minnaar geeft me maar u <rhyme label="b" type="m" corresp="#s7:l2">hand</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Ik zal wel op mijn uurtje <rhyme label="d" type="f">passen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Dat ik sierig ben <rhyme label="e" type="m">gekleet</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Mijn minnaar zal my niet <rhyme label="f" type="f">verrassen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En dat alles is <rhyme label="e" type="m" corresp="#s7:l6">gereet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
