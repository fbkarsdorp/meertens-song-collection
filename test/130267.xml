  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">‘Og, myn Lief, myn <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gaat gy my nu <rhyme label="a" type="f" corresp="#s1:l1">begeven</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ontrouwe <rhyme label="b" type="m">Minnaar</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Nu ik gaa van u <rhyme label="b" type="m" corresp="#s1:l3">swaer</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Ik klaag uw myn <rhyme label="c" type="m">verdriet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Maar gy en agt my <rhyme label="c" type="m" corresp="#s1:l5">niet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">En speelt met myne <rhyme label="d" type="f">reden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gelijk een wind met <rhyme label="c" type="m" corresp="#s1:l5">Riet</rhyme>.’</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">‘Wel zoete <rhyme label="a" type="f">Annaatje</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wat 's dat voor een <rhyme label="a" type="f" corresp="#s2:l1">praatje</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Jou malle <rhyme label="b" type="m">geklag</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat is daer ik om <rhyme label="b" type="m" corresp="#s2:l3">lag</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Wel Liefste, gaat gy <rhyme label="c" type="m">zwaar</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Loop heen en soekt de <rhyme label="c" type="m" corresp="#s2:l5">Vaar</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En laat my in mijn <rhyme label="d" type="f">wesen</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Dit komt zeeker <rhyme label="c" type="m" corresp="#s2:l5">raar</rhyme>.’</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">‘Ag, mijn waarde <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En swoert gy niet voor <rhyme label="a" type="f" corresp="#s3:l1">desen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Als dat gy mijn <rhyme label="b" type="m">zou</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Nooit laaten in <rhyme label="b" type="m" corresp="#s3:l3">rou</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">Het Schrifie dat <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">- Geschreven met u <rhyme label="c" type="m" corresp="#s3:l5">bloet</rhyme> -</l>
        <l id="s3:l7" met="3" type="-+">U selver <rhyme label="d" type="f">overtuigen</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Siet dog wat gy <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>.’</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">‘Het schrifje, zoete <rhyme label="a" type="f">meisje</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Schreef ik maer om een <rhyme label="a" type="f" corresp="#s4:l1">reisje</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Te nemen mijn <rhyme label="b" type="m">pleizier</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot uw, zoet aerdig <rhyme label="b" type="m" corresp="#s4:l3">dier</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">De Eeden, <rhyme label="c" type="m">Vrindin</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Voor 't Altaar van de <rhyme label="c" type="m" corresp="#s4:l5">min</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Zijn met de wind <rhyme label="d" type="f">vervlogen</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Stelt dat maar uit u <rhyme label="c" type="m" corresp="#s4:l5">zin</rhyme>.’</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">‘Aanschouwt eens mijn <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En hebt dog <rhyme label="a" type="f" corresp="#s5:l1">meedogen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Met mijn, onteerde <rhyme label="b" type="m">maagt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Eer u hertje nog <rhyme label="b" type="m" corresp="#s5:l3">knaagt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Al over dese <rhyme label="c" type="m">daat</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Als het sal sijn te <rhyme label="c" type="m" corresp="#s5:l5">laat</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Ag, komt mijn te <rhyme label="d" type="f">baten</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In mijn droeven <rhyme label="c" type="m" corresp="#s5:l5">staat</rhyme>.’</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">‘Wel wat helpt dit <rhyme label="a" type="f">klagen</rhyme>?</l>
        <l id="s6:l2" met="3" type="-+">Gy sult my nooit <rhyme label="a" type="f" corresp="#s6:l1">behagen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat weet gy, Liefste <rhyme label="b" type="m">zoet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat   mijn lust is <rhyme label="b" type="m" corresp="#s6:l3">geboet</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Ey, steld u dog te <rhyme label="c" type="m">vreen</rhyme>:</l>
        <l id="s6:l6" met="3" type="-+">Gy zijt het niet <rhyme label="c" type="m" corresp="#s6:l5">alleen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Die zoo een kindje <rhyme label="d" type="f">dragen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat is nu <rhyme label="c" type="m" corresp="#s6:l5">gemeen</rhyme>.’</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">‘O, gy wreede <rhyme label="a" type="f">ontrouwe</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">Ha, schender der <rhyme label="a" type="f" corresp="#s7:l1">Vrouwe</rhyme>!</l>
        <l id="s7:l3" met="3" type="-+">Hoe waar ik <rhyme label="b" type="m">helaas</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Zoo sot en soo <rhyme label="b" type="m" corresp="#s7:l3">dwaas</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">Og, og, hoe sit ik <rhyme label="c" type="m">nou</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Geen maget, nog <rhyme label="c" type="m" corresp="#s7:l5">vrou</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Dogters, wilt hier uit <rhyme label="d" type="f">leeren</rhyme>:</l>
        <l id="s7:l8" met="3" type="-+">Spiegelt u aan myn <rhyme label="c" type="m" corresp="#s7:l5">rou</rhyme>.’</l>
      </lg>
    </lg>
  </text>
</song>
