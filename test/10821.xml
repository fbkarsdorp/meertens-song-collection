    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Dewijle wy zijn <rhyme label="a" type="f">hiere</rhyme></l>
        <l id="s1:l2" met="3" type="-+">makende goede <rhyme label="a" type="f" corresp="#s1:l1">ciere</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Niet meer dan u lust <rhyme label="b" type="m">drinckt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maeckt in u t’minnen <rhyme label="a" type="f" corresp="#s1:l1">viere</rhyme></l>
        <l id="s1:l5" met="3" type="-+">In uwen tijdt seer <rhyme label="a" type="f" corresp="#s1:l1">diere</rhyme></l>
        <l id="s1:l6" met="3" type="-+">D’een d’ander tot vreucht <rhyme label="b" type="m" corresp="#s1:l3">bringt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Stelt u harteken tot <rhyme label="c" type="m">rust</rhyme></l>
        <l id="s1:l8" met="4" type="-+">V lief voedt u met goede <rhyme label="a" type="f" corresp="#s1:l1">manieren</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Soo langhe alst u beyden <rhyme label="c" type="m" corresp="#s1:l7">lust</rhyme>.</l>
      </lg>
    </lg>
