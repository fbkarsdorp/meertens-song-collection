<song>
  <meta>
    <id>188373</id>
    <date>
      <year-start>1651</year-start>
      <year-end>1651</year-end>
    </date>
    <location>
      <orginal-name>brugge</orginal-name>
      <standard-name>Brugge, Belgium</standard-name>
      <coordinates>
        <lon>51.2315785</lon>
        <lat>3.20809951603418</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Immaculatus Maria natus</melody>
  <author>Philippus Jennyn</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IMmaculatus ,, Mariae <rhyme label="a" type="f">Natus</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In balneo cruoris natat <rhyme label="b" type="m">flens</rhyme>:</l>
        <l id="s1:l3" met="2" type="-+">Est purpuratus ,, <rhyme label="c" type="f">Decorticatus</rhyme></l>
        <l id="s1:l4" met="6" type="-+">Iessaeus flos <rhyme label="b" type="m" corresp="#s1:l2">exoriens</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Pastot pastorum ,, Rex <rhyme label="a" type="f">Angelorum</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Vult propriis subesse <rhyme label="b" type="m">Legibus</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Cum amarore ,, Fuso <rhyme label="c" type="f">cruore</rhyme>,</l>
        <l id="s2:l4" met="6" type="-+">Dat potum suis <rhyme label="b" type="m" corresp="#s2:l2">gregibus</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hac sacrâ die ,, Infans <rhyme label="a" type="f">Mariae</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Cultello circumcisus <rhyme label="b" type="m">doluit</rhyme>:</l>
        <l id="s3:l3" met="2" type="-+">Est Nomen ejus ,, Vocatum <rhyme label="c" type="f">IESVS</rhyme>,</l>
        <l id="s3:l4" met="6" type="-+">Ut Pater ejus <rhyme label="b" type="m" corresp="#s3:l2">voluit</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O IESV ! Frater ,, Quem Virgo <rhyme label="a" type="f">Mater</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Privavit hodie <rhyme label="b" type="m">praeputio</rhyme>:</l>
        <l id="s4:l3" met="2" type="-+">Rivus cruoris ,, Pignus <rhyme label="c" type="f">amoris</rhyme>,</l>
        <l id="s4:l4" met="6" type="-+">Sit scelerum <rhyme label="b" type="m" corresp="#s4:l2">ablutio</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Totus anhelo ,, Amoris <rhyme label="a" type="f">zelo</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ut tibi dulcis IESV <rhyme label="b" type="m">uniat</rhyme>:</l>
        <l id="s5:l3" met="2" type="-+">IESVM suspiro ,, IESVM <rhyme label="c" type="f">requiro</rhyme>,</l>
        <l id="s5:l4" met="6" type="-+">Ut non à IESV <rhyme label="b" type="m" corresp="#s5:l2">dividat</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sancitor Legum ,, Rex summe <rhyme label="a" type="f">Regum</rhyme>.</l>
        <l id="s6:l2" met="4" type="-+">Qui circumcisus es in <rhyme label="b" type="m">stabulo</rhyme>:</l>
        <l id="s6:l3" met="2" type="-+">IESV dignare ,, Nos <rhyme label="c" type="f">saginare</rhyme></l>
        <l id="s6:l4" met="6" type="-+">Tuo caelesti <rhyme label="b" type="m" corresp="#s6:l2">pabulo</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Rubrum liquorem ,, Fusum <rhyme label="a" type="f">cruorem</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Salutis nostrae in <rhyme label="b" type="m">operibus</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Nobis propina ,, Ut <rhyme label="c" type="f">medicina</rhyme></l>
        <l id="s7:l4" met="6" type="-+">Pro nostris sit <rhyme label="b" type="m" corresp="#s7:l2">sceleribus</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Mariae Matris ,, Et Dei <rhyme label="a" type="f">Patris</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Sit aeviterna hic <rhyme label="b" type="m">memoria</rhyme>:</l>
        <l id="s8:l3" met="2" type="-+">Sit vulnerato ,, Mariae <rhyme label="c" type="f">Nato</rhyme></l>
        <l id="s8:l4" met="6" type="-+">Laus, honor, virtus, <rhyme label="b" type="m" corresp="#s8:l2">gloria</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
