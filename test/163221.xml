  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Twe Liefkens t'samen <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">By Godt geacht wert <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sijn zeegen sullen <rhyme label="a" type="f" corresp="#s1:l1">winnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Helpen malkaer in <rhyme label="b" type="m" corresp="#s1:l2">noot</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Soo Liefd' met Deught <rhyme label="c" type="m">op-wast</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Op 's Heeren Wetten <rhyme label="c" type="m" corresp="#s1:l5">past</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Sal niet <rhyme label="d" type="f">klagen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Van te <rhyme label="d" type="f" corresp="#s1:l7">dragen</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Swaren <rhyme label="c" type="m" corresp="#s1:l5">last</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ganimeed schenckt <rhyme label="a" type="f">Wijnen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Op soo waerden <rhyme label="b" type="m">bruylofs-Feest</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Roemers, Fluyten laet <rhyme label="a" type="f" corresp="#s2:l1">verschijnen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">'t Schijnt hier niemant suft of <rhyme label="b" type="m" corresp="#s2:l2">vreest</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wenschen dit nieuw <rhyme label="c" type="m">Paer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Sy moghen binnen 't <rhyme label="c" type="m" corresp="#s2:l5">Jaer</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Spruytjes <rhyme label="d" type="f">teelen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Hooren <rhyme label="d" type="f" corresp="#s2:l7">queelen</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Moer en <rhyme label="c" type="m" corresp="#s2:l5">Vaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
