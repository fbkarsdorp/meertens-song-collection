<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20558</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Visserij is waard geprezen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AScletario te <rhyme label="a" type="f">Romen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Heeft met goe <rhyme label="b" type="m">vrymoedigheyt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Hier, en daer, somtijts <rhyme label="b" type="m" corresp="#s1:l2">geseyt</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Dat den Vorst haest soud' <rhyme label="a" type="f" corresp="#s1:l1">omkomen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Die nu Romen strengh <rhyme label="c" type="m">regeert</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En een Keysers naem <rhyme label="c" type="m" corresp="#s1:l5">onteert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Men gingh dit al voort <rhyme label="a" type="f">verbreyden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Quam al me'e in 's Keysers <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Den Tyran oock hoort'er <rhyme label="b" type="m" corresp="#s2:l2">of</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Wat den Borst al van hem <rhyme label="a" type="f" corresp="#s2:l1">seyde</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Doen ontbiedt hy desen <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En hy sprack hem selver <rhyme label="c" type="m" corresp="#s2:l5">an</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Segh my Vriendt (al sonder <rhyme label="a" type="f">schromen</rhyme>)</l>
        <l id="s3:l2" met="4" type="+-">Is 't waer, dat ick heb <rhyme label="b" type="m">ghehoort</rhyme> ?</l>
        <l id="s3:l3" met="4" type="+-">Sal ick wreed'lijck zyn <rhyme label="b" type="m" corresp="#s3:l2">vermoort</rhyme> ?</l>
        <l id="s3:l4" met="4" type="+-">Och ! mijn Heer, ghy sult <rhyme label="a" type="f" corresp="#s3:l1">om-komen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">(Sprack de Man) en dat wel <rhyme label="c" type="m">haest</rhyme>.</l>
        <l id="s3:l6" met="4" type="+-">Dit heel den Tyran <rhyme label="c" type="m" corresp="#s3:l5">verbaest</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als hy nu wat had' <rhyme label="a" type="f">gesweghen</rhyme>;</l>
        <l id="s4:l2" met="4" type="+-">Sprack hy den Waerseggher <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Vriendt, ey ! laet my nu <rhyme label="b" type="m" corresp="#s4:l2">verstaen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Hoe is 't dan met u <rhyme label="a" type="f" corresp="#s4:l1">geleghen</rhyme> ?</l>
        <l id="s4:l5" met="4" type="+-">Hoe sal zijn u eyghen <rhyme label="c" type="m">endt</rhyme> ?</l>
        <l id="s4:l6" met="4" type="+-">Maeckt my nu dit oock <rhyme label="c" type="m" corresp="#s4:l5">bekent</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dat kan ick, mijn Heer, <rhyme label="a" type="f">verkonden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">(Sprack de Man) geheel <rhyme label="b" type="m">betreurt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Och ! mijn lijf sal zyn <rhyme label="b" type="m" corresp="#s5:l2">verscheurt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Op-gheeten van de <rhyme label="a" type="f" corresp="#s5:l1">Honden</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Ach ! mijn eynd' is nu <rhyme label="c" type="m">na-by</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Ick en kan niet raecken <rhyme label="c" type="m" corresp="#s5:l5">vry</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">ICk sal nu tot uwer <rhyme label="a" type="f">schanden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">(Sprack den Tyran) toonen <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat ghy zijt een <rhyme label="b" type="m" corresp="#s6:l2">Leughenaer</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Ghy sult nu door 't vyer <rhyme label="a" type="f" corresp="#s6:l1">verbranden</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Heel tot asch, in korten <rhyme label="c" type="m">stondt</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Niet tot aes van eenigh <rhyme label="c" type="m" corresp="#s6:l5">Hondt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Och ! het vonnis is <rhyme label="a" type="f">gestreecken</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Ascletaer die moet'er <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Hy moest buyten Romen <rhyme label="b" type="m" corresp="#s7:l2">gaen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Daer gingh men een vyer <rhyme label="a" type="f" corresp="#s7:l1">aen-steecken</rhyme></l>
        <l id="s7:l5" met="4" type="+-">In het Hout, aldaer <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Uyt last van sijn <rhyme label="c" type="m" corresp="#s7:l5">Majesteyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Aen een Pael werdt hy <rhyme label="a" type="f">gebonden</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Al te vooren <rhyme label="b" type="m">toe-bereydt</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Daer men 't vyer rondtsomme <rhyme label="b" type="m" corresp="#s8:l2">leydt</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Reghen heeft den Heer <rhyme label="a" type="f" corresp="#s8:l1">ghesonden</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Doen den Man was half <rhyme label="c" type="m">ghebra'en</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Is een yeder <rhyme label="c" type="m" corresp="#s8:l5">wech-gegaen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Door den over-grooten <rhyme label="a" type="f">Regen</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Kreegh een yeder grooten <rhyme label="b" type="m">haest</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Yeder liep al heel <rhyme label="b" type="m" corresp="#s9:l2">verbaest</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Sochten soo de kortste <rhyme label="a" type="f" corresp="#s9:l1">weghen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Quamen alle meest deur <rhyme label="c" type="m">nat</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Binnen Romen in de <rhyme label="c" type="m" corresp="#s9:l5">Stadt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">DOen de Keyser was <rhyme label="a" type="f">gheseten</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Vrolijck aen sijn <rhyme label="b" type="m">Avondtmael</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Deed' een Knecht een kort <rhyme label="b" type="m" corresp="#s10:l2">verhael</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Sprack, mijn Heer, ick doe u <rhyme label="a" type="f" corresp="#s10:l1">weten</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Wat den Quidam is <rhyme label="c" type="m">gheschiedt</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Die ghy heden branden <rhyme label="c" type="m" corresp="#s10:l5">liedt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Als hy heer'lijck stond' te <rhyme label="a" type="f">branden</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">(Door de last van u, mijn <rhyme label="b" type="m">Heer</rhyme>)</l>
        <l id="s11:l3" met="4" type="+-">Viel den groote Reghen <rhyme label="b" type="m" corresp="#s11:l2">ne'er</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Maeckte al het vyer te <rhyme label="a" type="f" corresp="#s11:l1">schande</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Liet alsoo den snooden <rhyme label="c" type="m">Guyt</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Voor de Honden, tot een <rhyme label="c" type="m" corresp="#s11:l5">buyt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Die voor haer was gaer <rhyme label="a" type="f">ghebraden</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">O ! sy vielen vinnigh <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Maer het was wel haest <rhyme label="b" type="m" corresp="#s12:l2">ghedaen</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">Hy mocht all' haer niet <rhyme label="a" type="f" corresp="#s12:l1">verzaden</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Want ick sagh vast yeder <rhyme label="c" type="m">een</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Lustigh knaghen aen een <rhyme label="c" type="m" corresp="#s12:l5">been</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Als den Keyser dit gingh <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Dat tot spijt van sijn <rhyme label="b" type="m">beleyt</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Even gingh, als had' <rhyme label="b" type="m" corresp="#s13:l2">gheseyt</rhyme></l>
        <l id="s13:l4" met="4" type="+-">Desen Man wel klaer te <rhyme label="a" type="f" corresp="#s13:l1">vooren</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Doen wierdt hy met vrees' <rhyme label="c" type="m">bevaen</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Dacht, het soud' hem oock soo <rhyme label="c" type="m" corresp="#s13:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Voor een yeder moest hy <rhyme label="a" type="f">schromen</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Selver voor een <rhyme label="b" type="m">Kamerlingh</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Die gestadigh om hem <rhyme label="b" type="m" corresp="#s14:l2">gingh</rhyme>:</l>
        <l id="s14:l4" met="4" type="+-">Hy liet niemandt tot hem <rhyme label="a" type="f" corresp="#s14:l1">komen</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">Och ! sijn hert was seer <rhyme label="c" type="m">beswaert</rhyme></l>
        <l id="s14:l6" met="4" type="+-">En voor yeder een <rhyme label="c" type="m" corresp="#s14:l5">vervaert</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">‘15. 't Gheen den Heere heeft <rhyme label="a" type="f">besloten</rhyme></l>
        <l id="s15:l2" met="4" type="+-">‘Dat voert hy rechtvaerdigh <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Selven me'e aen desen <rhyme label="b" type="m" corresp="#s15:l2">Guyt</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Die seer dickmael had' <rhyme label="a" type="f" corresp="#s15:l1">vergoten</rhyme></l>
        <l id="s15:l5" met="4" type="+-">(Als een wreeden Leeuw <rhyme label="c" type="m">verwoedt</rhyme>)</l>
        <l id="s15:l6" met="4" type="+-">Veel onnoosel <rhyme label="c" type="m" corresp="#s15:l5">Christen-bloedt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Daer hy meest hem op <rhyme label="a" type="f">betroude</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Wierdt door 't Roomsche Volck <rhyme label="b" type="m">bekoort</rhyme></l>
        <l id="s16:l3" met="4" type="+-">Dat hy sijnen Heer <rhyme label="b" type="m" corresp="#s16:l2">vermoort</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Die dit listigh met hem <rhyme label="a" type="f" corresp="#s16:l1">broude</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Maeckte alsoo Romen <rhyme label="c" type="m">vry</rhyme></l>
        <l id="s16:l6" met="4" type="+-">Van sijn wreede <rhyme label="c" type="m" corresp="#s16:l5">Tyranny</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Hier uyt konnen wy nu <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s17:l2" met="4" type="+-">Hoe Godt door sijn wijs <rhyme label="b" type="m">beleydt</rhyme>,</l>
        <l id="s17:l3" met="4" type="+-">Quaden, quaden loon <rhyme label="b" type="m" corresp="#s17:l2">bereydt</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Want sijn Wil kan niemandt <rhyme label="a" type="f" corresp="#s17:l1">keeren</rhyme>,</l>
        <l id="s17:l5" met="4" type="+-">‘'t Quaedt daer voor een Booswicht <rhyme label="c" type="m">schroomt</rhyme></l>
        <l id="s17:l6" met="4" type="+-">‘In 't ghemeen hem <rhyme label="c" type="m" corresp="#s17:l5">over-koomt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
