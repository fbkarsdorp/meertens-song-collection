<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>117673</id>
    <date>
      <year-start>1741</year-start>
      <year-end>1741</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatst kwam Cupido</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Lestmaal kwam Cupido myn <rhyme label="a" type="f">opwekken</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Daar ik zeer zoet lag in myn <rhyme label="b" type="m">rust</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Phoebus die kwam myn <rhyme label="a" type="f" corresp="#s1:l1">opwekken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Want ik was heel <rhyme label="b" type="m" corresp="#s1:l2">ontlust</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat ik myn moest <rhyme label="a" type="f" corresp="#s1:l1">ontdekken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door Brand van Minne <rhyme label="b" type="m" corresp="#s1:l2">lust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ik voelde myn Hertje heel <rhyme label="a" type="f">ontsteken</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dat ik moest uyt myn Bed <rhyme label="b" type="m">opstaan</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Het was om myn Liefje te <rhyme label="a" type="f" corresp="#s2:l1">spreken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ag hoort myn klagten <rhyme label="b" type="m" corresp="#s2:l2">aan</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Lief wilt u hooft <rhyme label="a" type="f" corresp="#s2:l1">uytsteken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ag! hoort myn kermen <rhyme label="b" type="m" corresp="#s2:l2">aan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Stilletjes gaat Vrouw Venus <rhyme label="a" type="f">Jonker</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Myn dunkt ik hoor daar myn <rhyme label="b" type="m">Mompeer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ik wensje genagt myn <rhyme label="a" type="f" corresp="#s3:l1">Jonker</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Stilt u <rhyme label="b" type="m" corresp="#s3:l2">gelammenteer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Gaat naar u huys 't is <rhyme label="a" type="f" corresp="#s3:l1">donker</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Want ziet het word <rhyme label="b" type="m" corresp="#s3:l2">onweer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Liefje hoe kan ik na huys toe <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ik zweerje zo waarlyk als gy <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat ik hier st√¢ <rhyme label="a" type="f" corresp="#s4:l1">beneden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gelyk een Riet dat <rhyme label="b" type="m" corresp="#s4:l2">beeft</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Liefje komt dog <rhyme label="a" type="f" corresp="#s4:l1">beneden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat ik je een Zoentje <rhyme label="b" type="m" corresp="#s4:l2">geef</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Jonkman ik houder myn <rhyme label="a" type="f">Contenanci</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gaat naar u huis ik zeg u <rhyme label="b" type="m">dat</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En maakt dog geen <rhyme label="a" type="f" corresp="#s5:l1">Samblanci</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Te kussen van dit of <rhyme label="b" type="m" corresp="#s5:l2">dat</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Want ik heb nooyt geen <rhyme label="a" type="f" corresp="#s5:l1">Allianci</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met u Perzoon <rhyme label="b" type="m" corresp="#s5:l2">gehad</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Liefje wat is 'er aan <rhyme label="a" type="f">gelegen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Dat ik hier voor u Deurtje <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Want ik ontzie Hagel of <rhyme label="a" type="f" corresp="#s6:l1">Regen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Donder of onweer <rhyme label="b" type="m" corresp="#s6:l2">aan</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Als ik naar jou <rhyme label="a" type="f" corresp="#s6:l1">behagen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Maar inder jou Graci mag <rhyme label="b" type="m" corresp="#s6:l2">staan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ha daar Jonkman leest dat <rhyme label="a" type="f">Briefje</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Dat ik daar Voor u leg op <rhyme label="b" type="m">straat</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Zeg eens myn zoete <rhyme label="a" type="f" corresp="#s7:l1">Liefje</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Al watter geschreven <rhyme label="b" type="m" corresp="#s7:l2">staat</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">Hoe komt dat zo myn <rhyme label="a" type="f" corresp="#s7:l1">Diefje</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Datje myn zo <rhyme label="b" type="m" corresp="#s7:l2">verlaat</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Hadjeu myn Lieste Lief <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Hadjeu dan myn over schoon <rhyme label="b" type="m">Godin</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dat Brief jen gy my hebt <rhyme label="a" type="f" corresp="#s8:l1">geschreven</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Daar staat myn Vonnis <rhyme label="b" type="m" corresp="#s8:l2">in</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Kom Dood beneemt myn 't <rhyme label="a" type="f" corresp="#s8:l1">leven</rhyme>;</l>
        <l id="s8:l6" met="3" type="-+">Hadjeu dan schoon <rhyme label="b" type="m" corresp="#s8:l2">Godin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
