<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181837</id>
    <date>
      <year-start>1791</year-start>
      <year-end>1791</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Bedroefde ziel laat af van klagen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Zeg maar eens <rhyme label="a" type="f">Braband</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">wilt gy met <rhyme label="b" type="m">geweld</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Gy stelt u te <rhyme label="a" type="f" corresp="#s1:l1">wapen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">En trekt zo te <rhyme label="b" type="m" corresp="#s1:l2">veld</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Gy neemt in veel <rhyme label="c" type="f">steden</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">En stelt u te <rhyme label="d" type="m">weer</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">wilt gy zo <rhyme label="e" type="f">afzweeren</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Uw Hertog en <rhyme label="d" type="m" corresp="#s1:l6">Heer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wy stryden voor <rhyme label="a" type="f">Regten</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Voor Kloosters en <rhyme label="b" type="m">Goed</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Daar voor willen wy <rhyme label="a" type="f" corresp="#s2:l1">vegten</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En waagen ons <rhyme label="b" type="m" corresp="#s2:l2">bloed</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Geweer en <rhyme label="c" type="f">Snaphaanen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Geschut, kruid en <rhyme label="d" type="m">lood</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Wy willen gaan <rhyme label="e" type="f">stryden</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Al raakt alles <rhyme label="d" type="m" corresp="#s2:l6">dood</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Gy zult nog <rhyme label="a" type="f">beklaagen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Uw gepog en <rhyme label="b" type="m">geraas</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Ik zal zorg <rhyme label="a" type="f" corresp="#s3:l1">draagen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Dat ik blyf de <rhyme label="b" type="m" corresp="#s3:l2">baas</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Wilt gy Meester <rhyme label="c" type="f">weezen</rhyme>?</l>
        <l id="s3:l6" met="3" type="+-">Neen, dat kan niet <rhyme label="d" type="m">zyn</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Wou een ander aan <rhyme label="e" type="f">Braband</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Neen, dat blijft aan <rhyme label="d" type="m" corresp="#s3:l6">myn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wy hebben <rhyme label="a" type="f">couragie</rhyme></l>
        <l id="s4:l2" met="3" type="+-">En ook goede <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Dat wy zullen <rhyme label="a" type="f" corresp="#s4:l1">winnen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Onz’ Vryheid zoo <rhyme label="b" type="m" corresp="#s4:l2">zoet</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Dan zyn wy uit <rhyme label="c" type="f">handen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">En ontslagen van ’t <rhyme label="d" type="m">juk</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Dan komen onze <rhyme label="e" type="f">Landen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Ontslagen van <rhyme label="d" type="m" corresp="#s4:l6">druk</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Wild zo hard niet <rhyme label="a" type="f">roepen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Maar myn regt <rhyme label="b" type="m">verstaan</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Als ik zend myn <rhyme label="a" type="f" corresp="#s5:l1">Troepen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Is uw Vryheid <rhyme label="b" type="m" corresp="#s5:l2">gedaan</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Dan ziet men u <rhyme label="c" type="f">vlugten</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Gy schreeuwt moord en <rhyme label="d" type="m">brand</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Dan moeten die <rhyme label="e" type="f">Helden</rhyme></l>
        <l id="s5:l8" met="3" type="+-">En Baazen van <rhyme label="d" type="m" corresp="#s5:l6">kant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Gy zoekt ons te <rhyme label="a" type="f">dreigen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Maar wy zyn niet <rhyme label="b" type="m">bang</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Wy zullen hulp <rhyme label="a" type="f" corresp="#s6:l1">krygen</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Het zal duuren niet <rhyme label="b" type="m" corresp="#s6:l2">lang</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Andere <rhyme label="c" type="f">Pootentaaten</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Bieden ons de <rhyme label="d" type="m">hand</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Wy zullen niet <rhyme label="e" type="f">laaten</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Maar blyven <rhyme label="d" type="m" corresp="#s6:l6">konstant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Gy spreekt zo van <rhyme label="a" type="f">vegten</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">En maakt veel <rhyme label="b" type="m">getier</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Als ik zend myn <rhyme label="a" type="f" corresp="#s7:l1">Knegten</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Met Bom en <rhyme label="b" type="m" corresp="#s7:l2">Mortier</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Dan zult gy <rhyme label="c" type="f">beklaagen</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Al uw wreed <rhyme label="d" type="m">bestaan</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Daarom was het <rhyme label="e" type="f">besten</rhyme></l>
        <l id="s7:l8" met="3" type="+-">In vreede te <rhyme label="d" type="m" corresp="#s7:l6">gaan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Och al uw <rhyme label="a" type="f">praaten</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Dat is <rhyme label="b" type="m">loozigheid</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Dat heb ik <rhyme label="a" type="f" corresp="#s8:l1">geweeten</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Al zoo lange <rhyme label="b" type="m" corresp="#s8:l2">tyd</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Wy zullen wel <rhyme label="c" type="f">doen</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Volbrengen uw <rhyme label="d" type="m">lust</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Dus laat ons met <rhyme label="e" type="f">vreede</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">En blyven in <rhyme label="d" type="m" corresp="#s8:l6">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
