  <meta>
    <id>9090</id>
    <date>
      <year-start>1676</year-start>
      <year-end>1676</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Gezwinde bode van de min</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jodocus van Lodenstein</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O siel! door d'onrust van Gods <rhyme label="a" type="m">soon</rhyme></l>
        <l id="s1:l2" met="3" type="+-">In de soete <rhyme label="b" type="m">rust</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Sal nu in u nog op den <rhyme label="a" type="m" corresp="#s1:l1">throon</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Staan uw vuyle <rhyme label="b" type="m" corresp="#s1:l2">lust</rhyme>?</l>
        <l id="s1:l5" met="2" type="+-">Kent uw <rhyme label="c" type="m">lot</rhyme>!</l>
        <l id="s1:l6" met="2" type="+-">Van om <rhyme label="d" type="m">hoog</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Comt uw <rhyme label="c" type="m" corresp="#s1:l5">Godt</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Met het <rhyme label="d" type="m" corresp="#s1:l6">oog</rhyme></l>
        <l id="s1:l9" met="1" type="+-">Op <rhyme label="e" type="m">u</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Doe gy gingt afkerig, <rhyme label="e" type="m" corresp="#s1:l9">schuw</rhyme>:</l>
        <l id="s1:l11" met="3" type="+-">Siet Hy laat sijn <rhyme label="f" type="m">rust</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">En sijn Hemels <rhyme label="f" type="m" corresp="#s1:l11">lust</rhyme>,</l>
        <l id="s1:l13" met="3" type="+-">Eeuwig Hemels <rhyme label="g" type="m">Soon</rhyme></l>
        <l id="s1:l14" met="4" type="+-">Om in u die lust te <rhyme label="g" type="m" corresp="#s1:l13">doo'n</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">So lang wy sijn in 't lichaam <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Sijn de lusten <rhyme label="b" type="m">soet</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Gelijck 't geknetter van het <rhyme label="a" type="m" corresp="#s2:l1">vier</rhyme></l>
        <l id="s2:l4" met="3" type="+-">In een doornen <rhyme label="b" type="m" corresp="#s2:l2">gloed</rhyme>.</l>
        <l id="s2:l5" met="2" type="+-">Dat geeft <rhyme label="c" type="m">ligt</rhyme></l>
        <l id="s2:l6" met="2" type="+-">En een <rhyme label="d" type="m">glans</rhyme>:</l>
        <l id="s2:l7" met="2" type="+-">Met een <rhyme label="c" type="m" corresp="#s2:l5">schigt</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Is het <rhyme label="d" type="m" corresp="#s2:l6">Thans</rhyme></l>
        <l id="s2:l9" met="1" type="+-"><rhyme label="e" type="m">Gebluscht</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">En een vuyle smoock voor <rhyme label="e" type="m" corresp="#s2:l9">lust</rhyme>.</l>
        <l id="s2:l11" met="3" type="+-">So een rasch <rhyme label="f" type="m">verdriet</rhyme></l>
        <l id="s2:l12" met="4" type="+-">Volgt de lusten <rhyme label="f" type="m" corresp="#s2:l11">niet</rhyme>:</l>
        <l id="s2:l13" met="3" type="+-">Veler jaren <rhyme label="g" type="m">blick</rhyme></l>
        <l id="s2:l14" met="4" type="+-">Is by 't eeuwig maar een <rhyme label="g" type="m" corresp="#s2:l13">snick</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nogtans vermaackt sig 't lichaam <rhyme label="a" type="m">dan</rhyme>:</l>
        <l id="s3:l2" met="3" type="+-">'t Is het vuyle <rhyme label="b" type="m">vleesch</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Althoos het sig vernoegen <rhyme label="a" type="m" corresp="#s3:l1">kan</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Maar in stage <rhyme label="b" type="m" corresp="#s3:l2">vrees</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Dat dien <rhyme label="c" type="m">top</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Van <rhyme label="d" type="m">vermaack</rhyme></l>
        <l id="s3:l7" met="2" type="+-">'t Grondig <rhyme label="c" type="m" corresp="#s3:l5">som</rhyme></l>
        <l id="s3:l8" met="2" type="+-">Van de <rhyme label="d" type="m" corresp="#s3:l6">wraack</rhyme></l>
        <l id="s3:l9" met="1" type="+-"><rhyme label="e" type="m">Besuyr</rhyme>.</l>
        <l id="s3:l10" met="4" type="+-">Dat en denck is <rhyme label="e" type="m" corresp="#s3:l9">niet</rhyme>.</l>
        <l id="s3:l11" met="3" type="+-">Het stuyr van 't regtvaardige <rhyme label="f" type="m">gebied</rhyme></l>
        <l id="s3:l12" met="4" type="+-">En slaapt daarom <rhyme label="f" type="m" corresp="#s3:l11">niet</rhyme>.</l>
        <l id="s3:l13" met="3" type="+-">Gods gena dat <rhyme label="g" type="m">staackt</rhyme>:</l>
        <l id="s3:l14" met="4" type="+-">Tot het dies te feller <rhyme label="g" type="m" corresp="#s3:l13">raackt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoe heerlick is het pragtig <rhyme label="a" type="m">stoff</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Boven het <rhyme label="b" type="m">gemeen</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Dog 't is geen dragt van 's Hemels <rhyme label="a" type="m" corresp="#s4:l1">hoff</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Maer van hier <rhyme label="b" type="m" corresp="#s4:l2">beneen</rhyme>.</l>
        <l id="s4:l5" met="2" type="+-">Ah! hoe <rhyme label="c" type="m">sagt</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Ist en <rhyme label="d" type="m">soet</rhyme>!</l>
        <l id="s4:l7" met="2" type="+-">Ia de <rhyme label="c" type="m" corresp="#s4:l5">pragt</rhyme></l>
        <l id="s4:l8" met="2" type="+-">Sagtjes <rhyme label="d" type="m" corresp="#s4:l6">voed</rhyme>,</l>
        <l id="s4:l9" met="1" type="+-">En <rhyme label="e" type="m">blijd</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">maar en wapent niet ten <rhyme label="e" type="m" corresp="#s4:l9">strijd</rhyme></l>
        <l id="s4:l11" met="3" type="+-">In so grooten <rhyme label="f" type="m">gevaar</rhyme>.</l>
        <l id="s4:l12" met="4" type="+-">'T is oock nett en <rhyme label="f" type="m" corresp="#s4:l11">claar</rhyme>;</l>
        <l id="s4:l13" met="3" type="+-">Wee! dat nettjes <rhyme label="g" type="m">blinckt</rhyme>,</l>
        <l id="s4:l14" met="4" type="+-">Als de siel van gruwlen <rhyme label="g" type="m" corresp="#s4:l13">stinckt</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dewijl ick hier mijn lichaam <rhyme label="a" type="m">draag</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Eyscht het spijs, en <rhyme label="b" type="m">dranck</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">De spijs verquickt, maar lust maackt <rhyme label="a" type="m" corresp="#s5:l1">traag</rhyme></l>
        <l id="s5:l4" met="3" type="+-">En het Lichaam <rhyme label="b" type="m" corresp="#s5:l2">cranck</rhyme>.</l>
        <l id="s5:l5" met="2" type="+-">Sout en <rhyme label="c" type="m">brood</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Was vol <rhyme label="d" type="m">op</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Voor ons <rhyme label="c" type="m" corresp="#s5:l5">nood</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Had den <rhyme label="d" type="m" corresp="#s5:l6">top</rhyme>,</l>
        <l id="s5:l9" met="1" type="+-">Van <rhyme label="e" type="m">Liefd</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Ons niet minlijcker <rhyme label="e" type="m" corresp="#s5:l9">gerieft</rhyme>.</l>
        <l id="s5:l11" met="3" type="+-">So gebruyck ick die <rhyme label="f" type="m">gunst</rhyme>:</l>
        <l id="s5:l12" met="4" type="+-">Gy verderft die door <rhyme label="f" type="m" corresp="#s5:l11">cunst</rhyme>,</l>
        <l id="s5:l13" met="3" type="+-">Dat uw lichaam <rhyme label="g" type="m">voedt</rhyme></l>
        <l id="s5:l14" met="4" type="+-">Nu uw lusten leven <rhyme label="g" type="m" corresp="#s5:l13">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Mijn jong hert wil niet in dien <rhyme label="a" type="m">prang</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">'t Voelt een soeter <rhyme label="b" type="m">band</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Een band die u een slaafschen <rhyme label="a" type="m" corresp="#s6:l1">dwang</rhyme></l>
        <l id="s6:l4" met="3" type="+-">legt om hert en <rhyme label="b" type="m" corresp="#s6:l2">hand</rhyme>:</l>
        <l id="s6:l5" met="2" type="+-">Arm Iong <rhyme label="c" type="m">Hert</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Dat in 't <rhyme label="d" type="m">vier</rhyme></l>
        <l id="s6:l7" met="2" type="+-">Hang <rhyme label="c" type="m" corresp="#s6:l5">verwert</rhyme>!</l>
        <l id="s6:l8" met="2" type="+-">Maar in 't <rhyme label="d" type="m" corresp="#s6:l6">vier</rhyme></l>
        <l id="s6:l9" met="1" type="+-">Van <rhyme label="e" type="m">lust</rhyme>:</l>
        <l id="s6:l10" met="4" type="+-">lust die steurt uw rust, en <rhyme label="e" type="m" corresp="#s6:l9">lust</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">En uw siel <rhyme label="f" type="m">misleydt</rhyme></l>
        <l id="s6:l12" met="4" type="+-">Van haar <rhyme label="f" type="m" corresp="#s6:l11">suyverheyd</rhyme>,</l>
        <l id="s6:l13" met="3" type="+-">Diere <rhyme label="g" type="m">suyverheyd</rhyme>!</l>
        <l id="s6:l14" met="4" type="+-">Die 't jong hert voor Godt <rhyme label="g" type="m" corresp="#s6:l13">geleyd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Iong hert! ey spaar uw reynen <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Voor Gods lieven <rhyme label="b" type="m">soon</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die door het vier van suyvre <rhyme label="a" type="m" corresp="#s7:l1">min</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Liet sijn hogen <rhyme label="b" type="m" corresp="#s7:l2">Throon</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">(Godes <rhyme label="c" type="m">soon</rhyme>?</l>
        <l id="s7:l6" met="2" type="+-">Oh ja <rhyme label="d" type="m">dog</rhyme></l>
        <l id="s7:l7" met="2" type="+-">Sijnen <rhyme label="c" type="m" corresp="#s7:l5">throon</rhyme>?</l>
        <l id="s7:l8" met="2" type="+-">K seg het <rhyme label="d" type="m" corresp="#s7:l6">nog</rhyme>)</l>
        <l id="s7:l9" met="1" type="+-">Om <rhyme label="e" type="m">u</rhyme></l>
        <l id="s7:l10" met="4" type="+-">Die nog gaet afkeerig <rhyme label="e" type="m" corresp="#s7:l9">schuw</rhyme>.</l>
        <l id="s7:l11" met="3" type="+-">Maar dat schoon! maar dien <rhyme label="f" type="m">val</rhyme>!</l>
        <l id="s7:l12" met="4" type="+-">Hy is schoonst boven <rhyme label="f" type="m" corresp="#s7:l11">al</rhyme>.</l>
        <l id="s7:l13" met="3" type="+-">Maar de lust nog <rhyme label="g" type="m">leeft</rhyme>.</l>
        <l id="s7:l14" met="4" type="+-">Hy de magt tot doding <rhyme label="g" type="m" corresp="#s7:l13">geeft</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">My dunckt ick voel een ander <rhyme label="a" type="m">vuer</rhyme></l>
        <l id="s8:l2" met="3" type="+-">In min aders <rhyme label="b" type="m">leeft</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Ah! dat Hy daal, en die brand <rhyme label="a" type="m" corresp="#s8:l1">stuer</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Diese stigt en <rhyme label="b" type="m" corresp="#s8:l2">geeft</rhyme>!</l>
        <l id="s8:l5" met="2" type="+-">Soeckt gy <rhyme label="c" type="m">baat</rhyme>?</l>
        <l id="s8:l6" met="2" type="+-">Oh! wat <rhyme label="d" type="m">raad</rhyme>!</l>
        <l id="s8:l7" met="2" type="+-">Tot hem <rhyme label="c" type="m" corresp="#s8:l5">gaat</rhyme>.</l>
        <l id="s8:l8" met="2" type="+-">'T is te <rhyme label="d" type="m" corresp="#s8:l6">laat</rhyme></l>
        <l id="s8:l9" met="1" type="+-">Te <rhyme label="e" type="m">gaan</rhyme></l>
        <l id="s8:l10" met="4" type="+-">Die so lang heb still' <rhyme label="e" type="m" corresp="#s8:l9">gestaan</rhyme>.</l>
        <l id="s8:l11" met="3" type="+-">Is't u ernst, 't is niet te <rhyme label="f" type="m">laat</rhyme>,</l>
        <l id="s8:l12" met="4" type="+-">'T is te vast een <rhyme label="f" type="m" corresp="#s8:l11">quaad</rhyme>.</l>
        <l id="s8:l13" met="3" type="+-">Hy heeft magt <rhyme label="g" type="m">genoeg</rhyme>.</l>
        <l id="s8:l14" met="4" type="+-">Voor die tijd-lijck soeckt en <rhyme label="g" type="m" corresp="#s8:l13">vroeg</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Staat gy maar op en toont u <rhyme label="a" type="m">sterck</rhyme>.</l>
        <l id="s9:l2" met="3" type="+-">In my is geen <rhyme label="b" type="m">magt</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">In Godt werdt magtig, op wijn <rhyme label="a" type="m" corresp="#s9:l1">werck</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Onbecommert <rhyme label="b" type="m" corresp="#s9:l2">wagt</rhyme>.</l>
        <l id="s9:l5" met="2" type="+-">Ick en <rhyme label="c" type="m">can</rhyme></l>
        <l id="s9:l6" met="2" type="+-">'t Valt te <rhyme label="d" type="m">bang</rhyme>.</l>
        <l id="s9:l7" met="2" type="+-">Vang maar <rhyme label="c" type="m" corresp="#s9:l5">an</rhyme></l>
        <l id="s9:l8" met="2" type="+-">Niet te <rhyme label="d" type="m" corresp="#s9:l6">lang</rhyme></l>
        <l id="s9:l9" met="1" type="+-">En <rhyme label="e" type="m">staat</rhyme></l>
        <l id="s9:l10" met="4" type="+-">Met den Hemel u <rhyme label="e" type="m" corresp="#s9:l9">beraad</rhyme>.</l>
        <l id="s9:l11" met="3" type="+-">Sonder hulp ist niet te <rhyme label="f" type="m">doen</rhyme>.</l>
        <l id="s9:l12" met="4" type="+-">Menig <rhyme label="f" type="m" corresp="#s9:l11">Legioen</rhyme></l>
        <l id="s9:l13" met="3" type="+-">Van Gods dierbaer <rhyme label="g" type="m">volck</rhyme></l>
        <l id="s9:l14" met="4" type="+-">Sal uw schaduw sijn en <rhyme label="g" type="m" corresp="#s9:l13">wolck</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Met voorgang, met gebed en <rhyme label="a" type="m">raad</rhyme>.</l>
        <l id="s10:l2" met="3" type="+-">T is maar <rhyme label="b" type="m">menschen-magt</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Het Hemelsch heyr u ten dienst <rhyme label="a" type="m" corresp="#s10:l1">staat</rhyme>.</l>
        <l id="s10:l4" met="3" type="+-">T is maar Schepsels <rhyme label="b" type="m" corresp="#s10:l2">cragt</rhyme>.</l>
        <l id="s10:l5" met="2" type="+-">Schepsels <rhyme label="c" type="m">maar</rhyme></l>
        <l id="s10:l6" met="2" type="+-">In uw <rhyme label="d" type="m">oog</rhyme>,</l>
        <l id="s10:l7" met="2" type="+-">Dat is <rhyme label="c" type="m" corresp="#s10:l5">waar</rhyme>,</l>
        <l id="s10:l8" met="2" type="+-">Maar om <rhyme label="d" type="m" corresp="#s10:l6">hoog</rhyme></l>
        <l id="s10:l9" met="1" type="+-"><rhyme label="e" type="m">Bewerckt</rhyme>,</l>
        <l id="s10:l10" met="4" type="+-">En met gunst, en magt <rhyme label="e" type="m" corresp="#s10:l9">gesterckt</rhyme>.</l>
        <l id="s10:l11" met="3" type="+-">'K ben geen strijd <rhyme label="f" type="m">gewoon</rhyme>:</l>
        <l id="s10:l12" met="4" type="+-">Maar Gods wijsen <rhyme label="f" type="m" corresp="#s10:l11">Soon</rhyme></l>
        <l id="s10:l13" met="3" type="+-">Is uw Hooft <rhyme label="g" type="m">ge-eert</rhyme>,</l>
        <l id="s10:l14" met="4" type="+-">Die uw vingren strijden <rhyme label="g" type="m" corresp="#s10:l13">leert</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">En of ick een dien slag <rhyme label="a" type="m">verloor</rhyme>?</l>
        <l id="s11:l2" met="3" type="+-">Leg af al uw <rhyme label="b" type="m">sorg</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Waarom? Die eens dit Hooft <rhyme label="a" type="m" corresp="#s11:l1">verkoor</rhyme></l>
        <l id="s11:l4" met="3" type="+-">Had oyt vasten <rhyme label="b" type="m" corresp="#s11:l2">borg</rhyme>.</l>
        <l id="s11:l5" met="2" type="+-">'k Vrees een <rhyme label="c" type="m">wond</rhyme>,</l>
        <l id="s11:l6" met="2" type="+-">'T is geen <rhyme label="d" type="m">Nood</rhyme>,</l>
        <l id="s11:l7" met="2" type="+-">Dit <rhyme label="c" type="m" corresp="#s11:l5">Verbond</rhyme></l>
        <l id="s11:l8" met="2" type="+-">In de <rhyme label="d" type="m" corresp="#s11:l6">Dood</rhyme></l>
        <l id="s11:l9" met="1" type="+-"><rhyme label="e" type="m">Verwint</rhyme>.</l>
        <l id="s11:l10" met="4" type="+-">In de Doodt die 't al <rhyme label="e" type="m" corresp="#s11:l9">verslindt</rhyme>,</l>
        <l id="s11:l11" met="3" type="+-">All vernielt 't al <rhyme label="f" type="m">verderft</rhyme>?</l>
        <l id="s11:l12" met="4" type="+-">Als het lichaam <rhyme label="f" type="m" corresp="#s11:l11">sterft</rhyme></l>
        <l id="s11:l13" met="3" type="+-">Sterft het lusten <rhyme label="g" type="m">meed</rhyme>,</l>
        <l id="s11:l14" met="4" type="+-">En de siel crijgt eeuwig <rhyme label="g" type="m" corresp="#s11:l13">vreed</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">My dunckt mijn lust my vrede <rhyme label="a" type="m">geeft</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Als ick die wat <rhyme label="b" type="m">boet</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Dats so: maar sy door uw vlugt <rhyme label="a" type="m" corresp="#s12:l1">leeft</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">En crijgt nieuwe <rhyme label="b" type="m" corresp="#s12:l2">moed</rhyme>:</l>
        <l id="s12:l5" met="2" type="+-">Sy <rhyme label="c" type="m">verwint</rhyme>,</l>
        <l id="s12:l6" met="2" type="+-">En gy <rhyme label="d" type="m">valt</rhyme>,</l>
        <l id="s12:l7" met="2" type="+-">Sy <rhyme label="c" type="m" corresp="#s12:l5">verslint</rhyme>,</l>
        <l id="s12:l8" met="2" type="+-">En gy <rhyme label="d" type="m" corresp="#s12:l6">malt</rhyme>,</l>
        <l id="s12:l9" met="1" type="+-"><rhyme label="e" type="m">Helas</rhyme>!</l>
        <l id="s12:l10" met="4" type="+-">En werd sonder slag haar <rhyme label="e" type="m" corresp="#s12:l9">aas</rhyme>.</l>
        <l id="s12:l11" met="3" type="+-">Wel wat raad! Blijf maar <rhyme label="f" type="m">staan</rhyme>,</l>
        <l id="s12:l12" met="4" type="+-">En hy sal <rhyme label="f" type="m" corresp="#s12:l11">vergaan</rhyme>.</l>
        <l id="s12:l13" met="3" type="+-">'t Strijden valt te <rhyme label="g" type="m">swaar</rhyme>.</l>
        <l id="s12:l14" met="4" type="+-">Sonder strijd geen croon hier <rhyme label="g" type="m" corresp="#s12:l13">naar</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Nu trecken my uw reden <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s13:l2" met="3" type="+-">In Gods <rhyme label="b" type="m">slaverny</rhyme>.</l>
        <l id="s13:l3" met="4" type="-+">O! vrygecogte voor Gods <rhyme label="a" type="m" corresp="#s13:l1">bloed</rhyme>!</l>
        <l id="s13:l4" met="3" type="+-">Nu werd gy eerst <rhyme label="b" type="m" corresp="#s13:l2">vry</rhyme>.</l>
        <l id="s13:l5" met="2" type="+-">Christi <rhyme label="c" type="m">Bruyd</rhyme>!</l>
        <l id="s13:l6" met="2" type="+-">Godes <rhyme label="d" type="m">kind</rhyme>!</l>
        <l id="s13:l7" met="2" type="+-">Was ick <rhyme label="c" type="m" corresp="#s13:l5">uyt</rhyme></l>
        <l id="s13:l8" met="2" type="+-">d'Aarde <rhyme label="d" type="m" corresp="#s13:l6">blind</rhyme></l>
        <l id="s13:l9" met="1" type="+-"><rhyme label="e" type="m">Geruckt</rhyme>!</l>
        <l id="s13:l10" met="4" type="+-">U des Hemels erg <rhyme label="e" type="m" corresp="#s13:l9">geluckt</rhyme>:</l>
        <l id="s13:l11" met="3" type="+-">Godt uw Vader <rhyme label="f" type="m">goed</rhyme>!</l>
        <l id="s13:l12" met="4" type="+-">Oh! mijn Vader, <rhyme label="f" type="m" corresp="#s13:l11">doet</rhyme></l>
        <l id="s13:l13" met="3" type="+-">Dat ick dat <rhyme label="g" type="m">geloov</rhyme></l>
        <l id="s13:l14" met="4" type="+-">So sijn al mijn lusten <rhyme label="g" type="m" corresp="#s13:l13">dood</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
