    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Als Bacchus eerst ter wereld is gekomen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">Dit 's ter eer en welsijn van <rhyme label="a" type="f">Germanje</rhyme></l>
        <l id="s1:l2" met="4" type="+-">En haar dapper oorlogs <rhyme label="b" type="m">held</rhyme></l>
        <l id="s1:l3" met="5" type="+-">Die voor 't huys van Oostenrijk en <rhyme label="a" type="f" corresp="#s1:l1">Spanje</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Sig vol moed in 't harnas <rhyme label="b" type="m" corresp="#s1:l2">steld</rhyme>.</l>
        <l id="s1:l5" met="6" type="+-">Wie sijn heil bemind, Set dit aan sijn <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s1:l6" met="6" type="+-">En drink sijn welvaard uyt opregte <rhyme label="c" type="m" corresp="#s1:l5">grond</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Dit is ter eer der Britse <rhyme label="a" type="f">Krygsbanieren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En den wydberoemden <rhyme label="b" type="m">vorst</rhyme></l>
        <l id="s2:l3" met="5" type="+-">Wiens kruyn ontelbare <rhyme label="a" type="f" corresp="#s2:l1">Lauwrieren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Tot des Seines schande <rhyme label="b" type="m" corresp="#s2:l2">torst</rhyme>.</l>
        <l id="s2:l5" met="6" type="+-">Wie sijn daden eerd, En sijn welvaard <rhyme label="c" type="m">agt</rhyme>,</l>
        <l id="s2:l6" met="6" type="+-">Sy dit sap te regt met blydschap toe <rhyme label="c" type="m" corresp="#s2:l5">gebragt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Dese dronk, getrouwe <rhyme label="a" type="f">Batavieren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sy tot eer van 't <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s3:l3" met="5" type="+-">Om met vreugd der helden kruyn te <rhyme label="a" type="f" corresp="#s3:l1">sieren</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Wiens moed het snood geweld <rhyme label="b" type="m" corresp="#s3:l2">verband</rhyme>.</l>
        <l id="s3:l5" met="6" type="+-">Wie 's lands vryheid agt, Drink met hert en <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s3:l6" met="6" type="+-">Op haar welvaard dese schoone druppels <rhyme label="c" type="m" corresp="#s3:l5">in</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Dese treug, o hooge <rhyme label="a" type="f">Bondgenoten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sy u in 't gemeen ter <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s4:l3" met="5" type="+-">Om uw naam en luyster te <rhyme label="a" type="f" corresp="#s4:l1">vergroten</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En 's vyands roem te ploffen <rhyme label="b" type="m" corresp="#s4:l2">neer</rhyme>.</l>
        <l id="s4:l5" met="6" type="+-">Wel aan, yder een, Is tot die dronk <rhyme label="c" type="m">verpligt</rhyme></l>
        <l id="s4:l6" met="6" type="+-">Om 't stamhuys van Bourbon te tergen in 't <rhyme label="c" type="m" corresp="#s4:l5">gesigt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
