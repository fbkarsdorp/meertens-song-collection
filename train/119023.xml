  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Winter is <rhyme label="a" type="f">vergangen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">De Mey komt met zoet <rhyme label="a" type="f" corresp="#s1:l1">verlangen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">De Velden die staan <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In dit lieffelyk <rhyme label="b" type="m" corresp="#s1:l3">Zaizoen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">De Schaapjes in de <rhyme label="c" type="f">Wyden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die ziet men zig <rhyme label="c" type="f" corresp="#s1:l5">verblyden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Den Harder met zyn <rhyme label="d" type="m">fluit</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die maakt een zoet <rhyme label="d" type="m" corresp="#s1:l7">geluid</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Visjes ziet men <rhyme label="a" type="f">zwieren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En de Vogels tiere <rhyme label="a" type="f" corresp="#s2:l1">liere</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Gedurig, hier en <rhyme label="b" type="m">daar</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Een ieder zoekt zyn <rhyme label="b" type="m" corresp="#s2:l3">Paar</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En ik zoek myn <rhyme label="c" type="f">Beminden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Om by haar troost te <rhyme label="c" type="f" corresp="#s2:l5">vinden</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Zeer bly, al met <rhyme label="d" type="m">gevly</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">In dezen koelen <rhyme label="d" type="m" corresp="#s2:l7">Mey</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">My dunkt het hart moet <rhyme label="a" type="f">breken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Zo voel ik het myn <rhyme label="a" type="f" corresp="#s3:l1">ontsteken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met een brandende <rhyme label="b" type="m">Min</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Tot myne <rhyme label="b" type="m" corresp="#s3:l3">Herderin</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Ik wil my dan <rhyme label="c" type="f">verkloeken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Om myn Lief te gaan <rhyme label="c" type="f" corresp="#s3:l5">zoeken</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Zeer ras, al op dit <rhyme label="d" type="m">pas</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hier in het groene <rhyme label="d" type="m" corresp="#s3:l7">Gras</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Van verre zie ik <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Onder deze groene <rhyme label="a" type="f" corresp="#s4:l1">Bomen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Myn liefste <rhyme label="b" type="m">Herderin</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die staat in myne <rhyme label="b" type="m" corresp="#s4:l3">zin</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zy alleen kan myn <rhyme label="c" type="f">behagen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Aanhoord myn droevig <rhyme label="c" type="f" corresp="#s4:l5">klagen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Verfoeid gy, die gy <rhyme label="d" type="m">ziet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En u zyn liefde <rhyme label="d" type="m" corresp="#s4:l7">bied</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ziet deze zoete <rhyme label="a" type="f">Tyden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Komen elk een hier <rhyme label="a" type="f" corresp="#s5:l1">verblyden</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En al wat men hier <rhyme label="b" type="m">vind</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Is nu met de Mey <rhyme label="b" type="m" corresp="#s5:l3">gezind</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zo wel 't Vee als ook de <rhyme label="c" type="f">Menszen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ziet men niet anders <rhyme label="c" type="f" corresp="#s5:l5">wenszen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Als om alhier op <rhyme label="d" type="m">Aard</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met malkaÃ¢r te zyn <rhyme label="d" type="m" corresp="#s5:l7">gepaard</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
