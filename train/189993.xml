    <melody>Daar was eens een jong meisje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Daar was eens een jong <rhyme label="a" type="f">Meisje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van Rysel uit de <rhyme label="b" type="m">Stad</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Een Heer van 't <rhyme label="c" type="m">Dorp</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Een Heer van 't <rhyme label="c" type="m" corresp="#s1:l3">Dorp</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Veel liefde <rhyme label="b" type="m" corresp="#s1:l2">had</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hy trof haar op zyn <rhyme label="d" type="f">wegen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Het Meisje eenmaal <rhyme label="e" type="m">aan</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Verneemt, <rhyme label="f" type="m">verneemt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Wat weg zy is <rhyme label="e" type="m" corresp="#s1:l7">gegaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy klom 'er af van zyn <rhyme label="a" type="f">Paard</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En zy maakt groot <rhyme label="b" type="m">geschrei</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Zyt niet <rhyme label="c" type="m">bevreesd</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Zyt niet <rhyme label="c" type="m" corresp="#s2:l3">bevreesd</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Kom, omarmt <rhyme label="b" type="m" corresp="#s2:l2">my</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En zy sprak, zonder <rhyme label="d" type="f">schroomen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ja wel, myn lieve <rhyme label="e" type="m">Heer</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Merk op, merk <rhyme label="f" type="m">op</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">'k Zal doen wat gy <rhyme label="e" type="m" corresp="#s2:l7">begeert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Myn Broer die is gints in 't <rhyme label="a" type="f">Veld</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En ziet hy ons zeer <rhyme label="b" type="m">gouw</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Dan zyt hy 't <rhyme label="c" type="m">wis</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Dan zyt hy 't <rhyme label="c" type="m" corresp="#s3:l3">wis</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Van myn en <rhyme label="b" type="m" corresp="#s3:l2">jou</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Klimt maar op deeze <rhyme label="d" type="f">Duinen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dan zult gy hem wel <rhyme label="e" type="m">zien</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Verneemt, <rhyme label="f" type="m">verneemt</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Eer hy ons komt <rhyme label="e" type="m" corresp="#s3:l7">verbiÃªn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy stond, keek heen en <rhyme label="a" type="f">weder</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat zag dat Meisje <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Hy klom van 't <rhyme label="c" type="m">Paard</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Hy klom van 't <rhyme label="c" type="m" corresp="#s4:l3">Paard</rhyme></l>
        <l id="s4:l5" met="2" type="-+">En ging zo <rhyme label="b" type="m" corresp="#s4:l2">voort</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">Adju, myn Heer van <rhyme label="d" type="f">Dorpen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Zy vlugt door 't Veld, zeer <rhyme label="e" type="m">bly</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Myn Heer, myn <rhyme label="f" type="m">Heer</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Myn Heer, stond toen <rhyme label="e" type="m" corresp="#s4:l7">alleen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Zo doen die Kruisbaans <rhyme label="a" type="f">Meisjes</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Spreeken de Jonkmans <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">En houden <rhyme label="c" type="m">dan</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">En houden <rhyme label="c" type="m" corresp="#s5:l3">dan</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Een praatje <rhyme label="b" type="m" corresp="#s5:l2">daar</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wat Man zou 't <rhyme label="d" type="f">gelyken</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">So als de Meisjes <rhyme label="e" type="m">zyn</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Om 't geld, om 't <rhyme label="f" type="m">geld</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Word men nog al <rhyme label="e" type="m" corresp="#s5:l7">geteld</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
