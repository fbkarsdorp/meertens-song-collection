<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190502</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Jan van Doorn</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Een best Lied zal ik zingen <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">'t Is van Oranje wilt <rhyme label="a" type="m" corresp="#s1:l1">verstaan</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Ho! Ho! vivat Oranje, vivat zyn <rhyme label="b" type="m">Huis</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Weg met al 't Patriotsch <rhyme label="b" type="m" corresp="#s1:l3">gespuis</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Zy zyn tot <rhyme label="b" type="m" corresp="#s1:l3">gruis</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Vivat Oranje, vivat zyn <rhyme label="b" type="m" corresp="#s1:l3">Huis</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Lang leev Oranje onzen <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Maar naar den drommel met <rhyme label="a" type="m" corresp="#s2:l1">Hansworst</rhyme>:</l>
        <l id="s2:l3" met="5" type="-+">Ho! Ho! vivat Oranje, ook de <rhyme label="b" type="m">Vorstin</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die waard is onzer aller <rhyme label="b" type="m" corresp="#s2:l3">min</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Wat <rhyme label="b" type="m" corresp="#s2:l3">Engelin</rhyme>!</l>
        <l id="s2:l6" met="4" type="-+">Vivat Oranje met zyn <rhyme label="b" type="m" corresp="#s2:l3">Vorstin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hoe zag een ieder Hun niet <rhyme label="a" type="m">graag</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Weer eindelyk koomen in den <rhyme label="a" type="m" corresp="#s3:l1">Haag</rhyme>:</l>
        <l id="s3:l3" met="5" type="-+">Ho! ho! vivat Oranje, 't Vorstlyk <rhyme label="b" type="m">Huis</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wat hoor ik thans een bly <rhyme label="b" type="m" corresp="#s3:l3">gedruis</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">'t Is niet <rhyme label="b" type="m" corresp="#s3:l3">kwansuis</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">'t Is voor Oranje en 't Vorstlyk <rhyme label="b" type="m" corresp="#s3:l3">Huis</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ik sprong van vreugde, als ik <rhyme label="a" type="m">zag</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Wat dat de Oranje min <rhyme label="a" type="m" corresp="#s4:l1">vermag</rhyme>;</l>
        <l id="s4:l3" met="5" type="-+">Ho! ho! Haagsche Burgers, tot hun <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Trokken de Koets naar 't <rhyme label="b" type="m" corresp="#s4:l3">Binnenhof</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Wat blyheids <rhyme label="b" type="m" corresp="#s4:l3">stof</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">'k Weet zulks 's Prinsen harten <rhyme label="b" type="m" corresp="#s4:l3">trof</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ik zag ook naar een korten <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dat niet min heeft myn hart <rhyme label="a" type="m" corresp="#s5:l1">verblyd</rhyme>:</l>
        <l id="s5:l3" met="5" type="-+">Ho! ho! 't was Prins Willems <rhyme label="b" type="m">Gemalin</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die z'op die wys ook haalden <rhyme label="b" type="m" corresp="#s5:l3">in</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Ã´ <rhyme label="b" type="m" corresp="#s5:l3">Kroonvorstin</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Wat kan niet onze <rhyme label="b" type="m" corresp="#s5:l3">Burgermin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Keezen zyn ter neer <rhyme label="a" type="m">geveld</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">De Prins in al zyn regt <rhyme label="a" type="m" corresp="#s6:l1">hersteld</rhyme>;</l>
        <l id="s6:l3" met="5" type="-+">Ho! ho! laaten wy nu maar vrolyk <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Zoo oud als jong, en groot en <rhyme label="b" type="m" corresp="#s6:l3">klyn</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Komt, geeft ons <rhyme label="b" type="m" corresp="#s6:l3">Wyn</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">'t Zal steeds Oranje Boven <rhyme label="b" type="m" corresp="#s6:l3">zyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
