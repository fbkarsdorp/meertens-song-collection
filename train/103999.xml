<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>103999</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb menig pint en kan</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">ICk heb menich pint en kan gelost met eenen <rhyme label="a" type="m">teugh</rhyme>Â :</l>
        <l id="s1:l2" met="3" type="-+">En weder vol <rhyme label="b" type="f">geschonken</rhyme>,</l>
        <l id="s1:l3" met="6" type="-+">Int nat ben ick een man ick neemt togh maer mijn <rhyme label="a" type="m" corresp="#s1:l1">meught</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Tot dat ick word half <rhyme label="b" type="f" corresp="#s1:l2">droncken</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">K'bin lust en smaeck daer <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Draeyt mijn daer naer den <rhyme label="d" type="m">kop</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Soo word ick bly van <rhyme label="c" type="m" corresp="#s1:l5">zin</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Door kragten van het <rhyme label="d" type="m" corresp="#s1:l6">sop</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Den Wijn doch maer en wast om dat men hem drincken <rhyme label="a" type="m">sou</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Waerom dan niet <rhyme label="b" type="f">gesoopen</rhyme>,</l>
        <l id="s2:l3" met="6" type="-+">Voorwaer ick ben een gast die veel van snypen <rhyme label="a" type="m" corresp="#s2:l1">hout</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Laet zwellighen met heele <rhyme label="b" type="f" corresp="#s2:l2">stopen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Drinct een gesontheyt <rhyme label="c" type="m">om</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En smeert de drooge <rhyme label="d" type="m">borst</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gedurich aenden <rhyme label="c" type="m" corresp="#s2:l5">brom</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Soo sterft gy noyt van <rhyme label="d" type="m" corresp="#s2:l6">dorst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Elck ogenblick een dronck laet ons niet ledich <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Want ledicheyt is <rhyme label="b" type="f">sonden</rhyme>,</l>
        <l id="s3:l3" met="6" type="-+">Die ons de Wijn hier schonck, die heeft het vreugdich <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Vyt deset nat <rhyme label="b" type="f" corresp="#s3:l2">gevonden</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Twelck deuchdich doet <rhyme label="c" type="m">deucht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Soo moet altijt den <rhyme label="d" type="m">Wijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Tot voetsel vande <rhyme label="c" type="m" corresp="#s3:l5">Jeucht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Van ons gedroncken <rhyme label="d" type="m" corresp="#s3:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">Doen ick een dronck bescheet ick slaet gesicht om <rhyme label="a" type="m">hooch</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Geduerich nae den <rhyme label="b" type="f">Hemel</rhyme>,</l>
        <l id="s4:l3" met="6" type="-+">Beweent alle mijn liet met tranen inde <rhyme label="a" type="m" corresp="#s4:l1">oogh</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Waer door ick krijch <rhyme label="b" type="f" corresp="#s4:l2">geschemel</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">My dunckt dat de aerde <rhyme label="c" type="m">beeft</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En elcke voetstap die ick <rhyme label="d" type="m">gae</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Eer my de ganck <rhyme label="c" type="m" corresp="#s4:l5">begeeft</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Breng noch een teugken <rhyme label="d" type="m" corresp="#s4:l6">na</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="-+">O Goddelijcke Nat hoe wonder is u <rhyme label="a" type="m">kracht</rhyme>?</l>
        <l id="s5:l2" met="3" type="-+">Gy drijft van ons het <rhyme label="b" type="f">suchten</rhyme>,</l>
        <l id="s5:l3" met="6" type="-+">Naer datmen u lost uyt van laeyt inden buyck u <rhyme label="a" type="m" corresp="#s5:l1">bracht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dan jaeght gy't hooft vol <rhyme label="b" type="f" corresp="#s5:l2">kluchten</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Looft dan die altijt <rhyme label="c" type="m">drinct</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En leeft met bly <rhyme label="d" type="m">gemoet</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Want die dan zinckt en <rhyme label="c" type="m" corresp="#s5:l5">springt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Als hy is vol en <rhyme label="d" type="m" corresp="#s5:l6">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
