<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8858</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Aus tiefer Not schrei ich zu dir</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">OCh Heer met dyner hulp <rhyme label="a" type="m">verschijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Help ons aen Lijf end' <rhyme label="b" type="f">Siele</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghevallen zyn int Erve <rhyme label="a" type="m" corresp="#s1:l1">dyn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die Vyanden soo <rhyme label="b" type="f" corresp="#s1:l2">veele</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Van haer dyn Godes dienst end' <rhyme label="c" type="m">Woort</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Ghelastert wort aen allen <rhyme label="c" type="m" corresp="#s1:l5">Oort</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">End' Jammerlijck <rhyme label="d" type="f">verstooret</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Oock is dijns Volcx Onschuldich <rhyme label="a" type="m">Bloet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dickwils soo veel <rhyme label="b" type="f">vergroten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat het ghelijck een <rhyme label="a" type="m" corresp="#s2:l1">Watervloet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Is over al <rhyme label="b" type="f" corresp="#s2:l2">ghevlooten</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">So menig Christ is heen <rhyme label="c" type="m">gericht</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die tot den Doot gaff Oorsaeck <rhyme label="c" type="m" corresp="#s2:l5">nicht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">End' veel syn niet <rhyme label="d" type="f">Begraven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Lichaem' dijner Heyl'gen <rhyme label="a" type="m">weerd'</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sijn worden Vogel <rhyme label="b" type="f">Spijse</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End' van 't Gediert' des Lants <rhyme label="a" type="m" corresp="#s3:l1">verteert</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Och HEer dijn Hulp <rhyme label="b" type="f" corresp="#s3:l2">bewijse</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Help doch en Troost ons lieve <rhyme label="c" type="m">GOdt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Want wy zijn worden seer te <rhyme label="c" type="m" corresp="#s3:l5">spot</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">All' dien, die om ons <rhyme label="d" type="f">Woonen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoe lang' wilt ghy so gants en <rhyme label="a" type="m">gaer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Op ons Ong'nade <rhyme label="b" type="f">Vaten</rhyme>?</l>
        <l id="s4:l3" met="4" type="-+">Wilt ghy (Heer) altoos thoornen <rhyme label="a" type="m" corresp="#s4:l1">swaer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dijn grim niet vallen <rhyme label="b" type="f" corresp="#s4:l2">laten</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">Aensiet dijn Arme <rhyme label="c" type="m">Christenheyt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ghedenckt aen dijn <rhyme label="c" type="m" corresp="#s4:l5">Barmherticheyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En strafft ons doch met <rhyme label="d" type="f">Maten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Schud' op die Vyand' dijnen <rhyme label="a" type="m">grim</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die dijnen Naem <rhyme label="b" type="f">vercleenen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Al Roemen sy met Luyder <rhyme label="a" type="m" corresp="#s5:l1">stem</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van Herten doch niet <rhyme label="b" type="f" corresp="#s5:l2">meenen</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Verstoor oock door dijn Machtig' <rhyme label="c" type="m">Handt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">All' die doen stoute <rhyme label="c" type="m" corresp="#s5:l5">Teghenstandt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">End' dijnen Name <rhyme label="d" type="f">scheynden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sy Plaghen seer die <rhyme label="a" type="m">Christenheyt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sy Woeden ende <rhyme label="b" type="f">Moorden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Sy brenghen Kerck en school in <rhyme label="a" type="m" corresp="#s6:l1">Leydt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wy syn seer Dunne <rhyme label="b" type="f" corresp="#s6:l2">worden</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ghedenckt doch niet ons <rhyme label="c" type="m">Missebaer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Die wel verdient heeft alle <rhyme label="c" type="m" corresp="#s6:l5">quaet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Ontfermt dy onser <rhyme label="d" type="f">haeste</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">HEEr om dijns Heyl'gen Names <rhyme label="a" type="m">Eer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Help ons wt dese <rhyme label="b" type="f">Nooden</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ghy trouwe Heylant weer end <rhyme label="a" type="m" corresp="#s7:l1">keer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Der Vyand' grousaem <rhyme label="b" type="f" corresp="#s7:l2">Woeden</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Verlost ons Heer van alle <rhyme label="c" type="m">quaet</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Vergheeft ons onse <rhyme label="c" type="m" corresp="#s7:l5">Missedaet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Om dijnes Namens <rhyme label="d" type="f">willen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Sy drijven bitter smaey end' <rhyme label="a" type="m">spot</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die dijn Woort teghen <rhyme label="b" type="f">strijden</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Gaen segghen waer is nu haer <rhyme label="a" type="m" corresp="#s8:l1">GOdt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hoe moechdy HEer dat <rhyme label="b" type="f" corresp="#s8:l2">lijden</rhyme>?</l>
        <l id="s8:l5" met="4" type="-+">Houdt doch dat spotten niet te <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Maer wreeckt aen haer dat Christen <rhyme label="c" type="m" corresp="#s8:l5">Bloet</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Dat so veel is <rhyme label="d" type="f">vergoten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat suchten der Ghevangen <rhyme label="a" type="m">daer</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Laet (HEER) dy gaen ter <rhyme label="b" type="f">Herten</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Troostse in haren Ancxten <rhyme label="a" type="m" corresp="#s9:l1">swaer</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Gheneest oock hare <rhyme label="b" type="f" corresp="#s9:l2">smerten</rhyme>:</l>
        <l id="s9:l5" met="4" type="-+">End' nae de Macht van dijnen <rhyme label="c" type="m">Arm</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Dijn cleyne Kudde (Heer) <rhyme label="c" type="m" corresp="#s9:l5">Bescherm</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Want sy ons willen <rhyme label="d" type="f">slachten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Sy Lastren groulijck dijnen <rhyme label="a" type="m">Soon</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Christ onsen lieven <rhyme label="b" type="f">HEEREN</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Och Godt veel langher niet <rhyme label="a" type="m" corresp="#s10:l1">verschoon</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Maer wilt haer Laster <rhyme label="b" type="f" corresp="#s10:l2">keeren</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Betaelt haer (HEEr) wel seven <rhyme label="c" type="m">Vout</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Dat sy Blameeren dy so <rhyme label="c" type="m" corresp="#s10:l5">stout</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">End' gaen daer in <rhyme label="d" type="f">verharden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Heer help dijn Volc, end' niet <rhyme label="a" type="m">verlaet</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Troost ons in allen <rhyme label="b" type="f">Leyde</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Want ghy zijt onse <rhyme label="a" type="m" corresp="#s11:l1">Toeverlaet</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Wy Schapen dijner <rhyme label="b" type="f" corresp="#s11:l2">Weyde</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Dijn eyghen Volck, ghy onse <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Dijn grooten Naem, end' dijne <rhyme label="c" type="m" corresp="#s11:l5">Eer</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Laet ons oock Eewich <rhyme label="d" type="f">Prijsen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
