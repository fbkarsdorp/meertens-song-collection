<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24578</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Es kommt ein Schiff geladen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HEt comet een scyp <rhyme label="a" type="f">geladen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">hent aen dat hoechste <rhyme label="b" type="m">boert</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Maria hielt dat <rhyme label="a" type="f" corresp="#s1:l1">roeder</rhyme></l>
        <l id="s1:l4" met="3" type="+-">die engel stuert dat <rhyme label="b" type="m" corresp="#s1:l2">voert</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Maria gades <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s2:l2" met="3" type="+-">gelaeuet moetstu <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s2:l3" met="3" type="+-">wy waren al <rhyme label="a" type="f" corresp="#s2:l1">verlaeren</rhyme></l>
        <l id="s2:l4" met="3" type="+-">dedet v lieue <rhyme label="b" type="m" corresp="#s2:l2">kyndekyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Maria gades <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s3:l2" met="3" type="+-">gelaeuet moetstu <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s3:l3" met="3" type="+-">vermeende <rhyme label="a" type="f" corresp="#s3:l1">Conynginne</rhyme></l>
        <l id="s3:l4" met="3" type="+-">der engelen <rhyme label="b" type="m" corresp="#s3:l2">aenneschyn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Maria gades <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s4:l2" met="3" type="+-">woe is v lof soe <rhyme label="b" type="m">breyt</rhyme></l>
        <l id="s4:l3" met="3" type="+-">ten kan geen man <rhyme label="a" type="f" corresp="#s4:l1">bescryuen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">v grote <rhyme label="b" type="m" corresp="#s4:l2">eerwaardicheyt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">In eenre stylre <rhyme label="a" type="f">wagen</rhyme></l>
        <l id="s5:l2" met="3" type="+-">coemet ons dat scyp aen <rhyme label="b" type="m">lant</rhyme></l>
        <l id="s5:l3" met="3" type="+-">het brenget ons ryke <rhyme label="a" type="f" corresp="#s5:l1">gaeuen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">een soen is ons <rhyme label="b" type="m" corresp="#s5:l2">gesant</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Het leget in eenre <rhyme label="a" type="f">crybben</rhyme></l>
        <l id="s6:l2" met="3" type="+-">dat lieue cleyne <rhyme label="b" type="m">kynt</rhyme></l>
        <l id="s6:l3" met="3" type="+-">myt sinen verweenden <rhyme label="a" type="f" corresp="#s6:l1">rybben</rhyme></l>
        <l id="s6:l4" met="3" type="+-">in snoeden <rhyme label="b" type="m" corresp="#s6:l2">doekerkyn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Het leget in eenre <rhyme label="a" type="f">wagen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">dat lieue cleyne <rhyme label="b" type="m">kynt</rhyme></l>
        <l id="s7:l3" met="3" type="+-">het luchtet woe een <rhyme label="a" type="f" corresp="#s7:l1">spiegel</rhyme></l>
        <l id="s7:l4" met="3" type="+-">als die kersten alle gader <rhyme label="b" type="m" corresp="#s7:l2">syn</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Mocht ic dat kyndekyn <rhyme label="a" type="f">cussen</rhyme></l>
        <l id="s8:l2" met="3" type="+-">voer synen roder <rhyme label="b" type="m">mont</rhyme></l>
        <l id="s8:l3" met="3" type="+-">des mochte my wal <rhyme label="a" type="f" corresp="#s8:l1">gelusten</rhyme></l>
        <l id="s8:l4" met="3" type="+-">van sunden worde ic <rhyme label="b" type="m" corresp="#s8:l2">gesont</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
