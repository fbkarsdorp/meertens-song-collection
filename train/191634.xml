  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEt Keurs-Lyf van de <rhyme label="a" type="f">Maagden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zeer wonder van en <rhyme label="b" type="m">bol</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Wie ziet ze niet al <rhyme label="a" type="f" corresp="#s1:l1">dragen</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Nog dikker als een <rhyme label="b" type="m" corresp="#s1:l2">bol</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Is niet de Natuur, <rhyme label="a" type="f">Natuur</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die haar dit zo doet <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Maar 't is een wulpze <rhyme label="a" type="f" corresp="#s2:l1">dragt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Door deze of gene <rhyme label="b" type="m" corresp="#s2:l2">pyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wie zou zig dan <rhyme label="a" type="f">verwonderen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wanneer hy zulke Meysjes <rhyme label="b" type="m">ziet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">'t Is een stemme als of 't <rhyme label="a" type="f" corresp="#s3:l1">dondert</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">Dat zy (ik meen haar) getrout wil <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ze getuygt het is haar <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s4:l2" met="3" type="-+">En anders zoekt ze <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Een knaap die haar wat kan <rhyme label="a" type="f" corresp="#s4:l1">geven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hier na wagt men met <rhyme label="b" type="m" corresp="#s4:l2">verdriet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het Keurs-lyf is <rhyme label="a" type="f">getuyge</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En houd het voor <rhyme label="b" type="m">gewis</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En laat het by u <rhyme label="a" type="f" corresp="#s5:l1">zyn</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Als een wet die niet Feylende en <rhyme label="b" type="m" corresp="#s5:l2">is</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Za Vryers van de <rhyme label="a" type="f">Lande</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Gy die getrout wilt <rhyme label="b" type="m">zien</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hier is wat 't <rhyme label="a" type="f" corresp="#s6:l1">verhandelen</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Hier kund gy ligt na <rhyme label="b" type="m" corresp="#s6:l2">bien</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Edog niet al te <rhyme label="a" type="f">greetig</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ziet eerst te regte <rhyme label="b" type="m">toe</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Wild gy niet zyn <rhyme label="a" type="f" corresp="#s7:l1">gegrepen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Van die niet zyn te <rhyme label="b" type="m" corresp="#s7:l2">koen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Want komt 'er een <rhyme label="a" type="f">gestoven</rhyme>;</l>
        <l id="s8:l2" met="3" type="-+">Hy is wonder <rhyme label="b" type="m">willekom</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Gy kund my vry <rhyme label="a" type="f" corresp="#s8:l1">gelooven</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ik lag 'er dikwyls <rhyme label="b" type="m" corresp="#s8:l2">om</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De Meysjes van <rhyme label="a" type="f">playzier</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Die zyn der magtig <rhyme label="b" type="m">veel</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Maar weynig goede <rhyme label="a" type="f" corresp="#s9:l1">zeen</rhyme>!</l>
        <l id="s9:l4" met="3" type="-+">Bezittende ze by myn <rhyme label="b" type="m" corresp="#s9:l2">ziel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De Meysjes van <rhyme label="a" type="f">playzier</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Die zyn der magtig <rhyme label="b" type="m">veel</rhyme>;</l>
        <l id="s10:l3" met="3" type="-+">Maar rap en gaau <rhyme label="a" type="f" corresp="#s10:l1">verstand</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dat zit haar in de <rhyme label="b" type="m" corresp="#s10:l2">hiel</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De Meysjes van <rhyme label="a" type="f">playzier</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Die zyn der magtig <rhyme label="b" type="m">veel</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">Maar 't geld is by haar <rhyme label="a" type="f" corresp="#s11:l1">hard</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Gelyk de verharde <rhyme label="b" type="m" corresp="#s11:l2">steen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">De Meysjes van <rhyme label="a" type="f">playzier</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Die zyn der magtig <rhyme label="b" type="m">veel</rhyme>;</l>
        <l id="s12:l3" met="3" type="-+">Maar goed en deftig <rhyme label="a" type="f" corresp="#s12:l1">slag</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Die zyn der weynig by myn <rhyme label="b" type="m" corresp="#s12:l2">keel</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">De Meysjes van <rhyme label="a" type="f">playzier</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Die zyn der magtig <rhyme label="b" type="m">veel</rhyme>;</l>
        <l id="s13:l3" met="3" type="-+">Edog ik houde <rhyme label="a" type="f" corresp="#s13:l1">op</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Met het tellen van haar <rhyme label="b" type="m" corresp="#s13:l2">zeen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
