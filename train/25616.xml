  <author>Leenaert Clock</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Dancken laet ons den grooten <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Des Hemels, hem hoogh <rhyme label="b" type="f">loven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die op ons het ghenadich <rhyme label="a" type="m" corresp="#s1:l1">lot</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghesonden heeft van <rhyme label="b" type="f" corresp="#s1:l2">boven</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Tot liefd’, vreed’ en <rhyme label="c" type="m">eenicheyt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ons heeft laten <rhyme label="d" type="f">vergaren</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Door gunste t’hert daer toe <rhyme label="c" type="m" corresp="#s1:l5">bereyt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Verlost uyt het <rhyme label="d" type="f" corresp="#s1:l6">beswaren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Eeuwighe Vader, ghy wel <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Weerdich los te <rhyme label="b" type="f">ontfanghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Van ons te zijn <rhyme label="a" type="m" corresp="#s2:l1">ghebenedijt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ghepresen met <rhyme label="b" type="f" corresp="#s2:l2">verlanghen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dat ghy ons ooghen hebt <rhyme label="c" type="m">verlicht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">T’was u handt die dit <rhyme label="d" type="f">dede</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Die mons gaf het Godlijck <rhyme label="c" type="m" corresp="#s2:l5">ghesticht</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Te soecken rechten <rhyme label="d" type="f" corresp="#s2:l6">vrede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Recht soo’y is u begonnen <rhyme label="a" type="m">werck</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Bereyt door uwe <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Verseghelt dat, en maket <rhyme label="a" type="m" corresp="#s3:l1">sterck</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Voortaen in alle <rhyme label="b" type="f" corresp="#s3:l2">olanden</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Door uwen goeden heyl’gen <rhyme label="c" type="m">Gheest</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Tot lof van uwer <rhyme label="d" type="f">eeren</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Dat u Ghemeynte <rhyme label="c" type="m" corresp="#s3:l5">aldermeest</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Mach wassen en <rhyme label="d" type="f" corresp="#s3:l6">vermeeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Rechte arbeyders hier toe <rhyme label="a" type="m">sent</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Begaeftse, Heer, met <rhyme label="b" type="f">crachten</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ws Gheestes, dat sy moghen <rhyme label="a" type="m" corresp="#s4:l1">jent</rhyme></l>
        <l id="s4:l4" met="3" type="-+">T’gheen aldermeest <rhyme label="b" type="f" corresp="#s4:l2">betrachten</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Wat u Vader behaghen <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">T’is dat u volck <rhyme label="d" type="f">ghepresen</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Eenich zy met hert en <rhyme label="c" type="m" corresp="#s4:l5">ghemoet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In een Christelijck <rhyme label="d" type="f" corresp="#s4:l6">wesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">In uwen Naem, en door u <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Moghen wy nu <rhyme label="b" type="f">beginnen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Gheeft ons doch sterckheyt ende <rhyme label="a" type="m" corresp="#s5:l1">cracht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Om recht te <rhyme label="b" type="f" corresp="#s5:l2">overwinnen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">O lieve Heer, ghetrou te <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Volcomen, met <rhyme label="d" type="f">jolijsen</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Daer van die daedt doenmaels gaf <rhyme label="c" type="m" corresp="#s5:l5">schijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En t’werck dede <rhyme label="d" type="f" corresp="#s5:l6">uytwijsen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Cloecklijck wel bedacht met <rhyme label="a" type="m">verstandt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hebben wy doe <rhyme label="b" type="f">verheven</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Elckanderen die rechte <rhyme label="a" type="m" corresp="#s6:l1">handt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En oock den mondt <rhyme label="b" type="f" corresp="#s6:l2">ghegheven</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Verbindt ons herten oock <rhyme label="c" type="m">bequaem</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Met reynder liefden <rhyme label="d" type="f">vyerich</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Als leden zijn aen een <rhyme label="c" type="m" corresp="#s6:l5">lichaem</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Vreedsaem en <rhyme label="d" type="f" corresp="#s6:l6">goedertierich</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Kenteyckenen van dese <rhyme label="a" type="m">daet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat wy die laten <rhyme label="b" type="f">blijcken</rhyme></l>
        <l id="s7:l3" met="4" type="-+">In lief in leet, hoe dat <rhyme label="a" type="m" corresp="#s7:l1">hetgaet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Daer nimmer van en <rhyme label="b" type="f" corresp="#s7:l2">wijcken</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Mart troulijck bystaen in der <rhyme label="c" type="m">noot</rhyme></l>
        <l id="s7:l6" met="3" type="-+">In suer en soet op <rhyme label="d" type="f">erden</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">In droefheyt als in blijschap <rhyme label="c" type="m" corresp="#s7:l5">groot</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Heer, daer in trou <rhyme label="d" type="f" corresp="#s7:l6">volherden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Proeven wat u behaeghlijck <rhyme label="a" type="m">zy</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En den Naesten mach <rhyme label="b" type="f">stichten</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En hoe wy die blinden hier <rhyme label="a" type="m" corresp="#s8:l1">by</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Souden moghen <rhyme label="b" type="f" corresp="#s8:l2">verlichten</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Heer, doet open haer ooghen <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">T’is t’ghene dat wy u <rhyme label="d" type="f">beden</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Verlichtse doch, O lieve <rhyme label="c" type="m" corresp="#s8:l5">Vaer</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Door uwen Gheest vol <rhyme label="d" type="f" corresp="#s8:l6">vreden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Int recht verstandt ons <rhyme label="a" type="m">onderhout</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En in u heylsaem <rhyme label="b" type="f">Leere</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Op Christum Iesum vast <rhyme label="a" type="m" corresp="#s9:l1">ghebout</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Tot lof van ws Naems <rhyme label="b" type="f" corresp="#s9:l2">eere</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Voor valsche leere ons <rhyme label="c" type="m">behoet</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En wat ons mocht <rhyme label="d" type="f">verleyden</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Dat wy dat moghen met der <rhyme label="c" type="m" corresp="#s9:l5">spoet</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Verlaten, daer van <rhyme label="d" type="f" corresp="#s9:l6">scheyden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">En wat ons noch van nooden <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Dat wy noch weten <rhyme label="b" type="f">sullen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Dat wilt, O Heere, gansch <rhyme label="a" type="m" corresp="#s10:l1">ghewis</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Door u goetheyt <rhyme label="b" type="f" corresp="#s10:l2">vervullen</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">T’herte om dat te nemen <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Gheeft ons tot onser <rhyme label="d" type="f">baten</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Lief Heer, wilt ons versorghen <rhyme label="c" type="m" corresp="#s10:l5">gaen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Op u wy ons <rhyme label="d" type="f" corresp="#s10:l6">verlaten</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ter eeren uwer <rhyme label="a" type="m">goedicheyt</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En vriendtlijckheyt <rhyme label="b" type="f">ghedachtich</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En oock uwer <rhyme label="a" type="m" corresp="#s11:l1">lanckmoedicheyt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Die ons maeckt <rhyme label="b" type="f" corresp="#s11:l2">levenachtich</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">En dat ghy ons gheerne <rhyme label="c" type="m">verhoort</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En ons saeck laet <rhyme label="d" type="f">ghelinghen</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Soo willen wy u met <rhyme label="c" type="m" corresp="#s11:l5">accoort</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Eenen danckoffer <rhyme label="d" type="f" corresp="#s11:l6">bringhen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Salicheyt, macht, eere en de <rhyme label="a" type="m">prijs</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Zy u t’eeuwighen <rhyme label="b" type="f">daghen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Ghy Coninck groot, machtich en <rhyme label="a" type="m" corresp="#s12:l1">wijs</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">V Goddelijck <rhyme label="b" type="f" corresp="#s12:l2">behaghen</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Moet gheschieden het leven <rhyme label="c" type="m">lanck</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Van ons alle te <rhyme label="d" type="f">samen</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">O lieve Heer laet desen <rhyme label="c" type="m" corresp="#s12:l5">sanck</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Voor u recht comen, <rhyme label="d" type="f" corresp="#s12:l6">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
