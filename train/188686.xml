<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188686</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De wereld is in rep en roer</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Triumph! Bataafsche <rhyme label="a" type="m">Broederkring</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Voor wien ik vrije toonen <rhyme label="a" type="m" corresp="#s1:l1">zing</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Uw heildag is <rhyme label="b" type="f">verscheenen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Triumph! 't onschatbaar vreÃª <rhyme label="c" type="m">verbond</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Op 's Menschen heilig regt <rhyme label="c" type="m" corresp="#s1:l4">gegrond</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mogt Gal en Belg <rhyme label="b" type="f" corresp="#s1:l3">vereenen</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Civilis Kroost, te lang <rhyme label="a" type="m">verdrukt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Gij zijt der Dwinglandij <rhyme label="a" type="m" corresp="#s2:l1">ontrukt</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Zie haar in 't stof <rhyme label="b" type="f">geboogen</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">De heerschzucht knarsetand van <rhyme label="c" type="m">spijt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Haar snood gevolg, de wraak en <rhyme label="c" type="m" corresp="#s2:l4">nijd</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Deez derven haar <rhyme label="b" type="f" corresp="#s2:l3">vermoogen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dank zij de Gal, wien <rhyme label="a" type="m">heldenstaal</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Het wreed gekluisterd <rhyme label="a" type="m" corresp="#s3:l1">Leeuwendaal</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Bij 't bliksmen der <rhyme label="b" type="f">Musketten</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">De lang gewenschte Vrijheid <rhyme label="c" type="m">bragt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Den Dwingland en zijn helsche <rhyme label="c" type="m" corresp="#s3:l4">magt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Den voet op 't harte <rhyme label="b" type="f" corresp="#s3:l3">zette</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'t Waar niet genoeg, o vrijen <rhyme label="a" type="m">Frank</rhyme>!</l>
        <l id="s4:l2" met="4" type="-+">Dat gij den Dwinger op den <rhyme label="a" type="m" corresp="#s4:l1">klank</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Van 't Krijgsklaroen deet <rhyme label="b" type="f">vlieden</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">O Neen: Gij maakte Neerland <rhyme label="c" type="m">vrij</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Verbrak haar boeij, en dorst daar <rhyme label="c" type="m" corresp="#s4:l4">bij</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ons uwe vriendschap <rhyme label="b" type="f" corresp="#s4:l3">bieden</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Belgen! welk een edel <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Bezit een Volk, dat donders <rhyme label="a" type="m" corresp="#s5:l1">tart</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En weet haar kragt te <rhyme label="b" type="f">ontrukken</rhyme>!</l>
        <l id="s5:l4" met="4" type="-+">Het durst zijn <rhyme label="c" type="m">overwonneling</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Geboogen voor zijn <rhyme label="c" type="m" corresp="#s5:l4">heldenkling</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Aan 't edel harte <rhyme label="b" type="f" corresp="#s5:l3">drukken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Triumph Bataafsche <rhyme label="a" type="m">Burgerij</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Nu spreekt uw achtbre volkstem, <rhyme label="a" type="m" corresp="#s6:l1">vrij</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">En uwe dwingren <rhyme label="b" type="f">zwijgen</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Volhard in trouw, volhard in <rhyme label="c" type="m">deugd</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">En laat uw ongeveinsde <rhyme label="c" type="m" corresp="#s6:l4">vreugd</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Naar 't heldre luchtruim <rhyme label="b" type="f" corresp="#s6:l3">stijgen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nooit zag ons Vrij <rhyme label="a" type="m">Gemeenebest</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Te wreed gekluisterd, door een <rhyme label="a" type="m" corresp="#s7:l1">pest</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Van drukkende <rhyme label="b" type="f">Tijrannen</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Zijn Dwingeland zoo kleen <rhyme label="c" type="m">vergruist</rhyme>;</l>
        <l id="s7:l5" met="4" type="-+">Noch heerschzucht, schoon zij woeld en <rhyme label="c" type="m" corresp="#s7:l4">druist</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Zoo zegerijk <rhyme label="b" type="f" corresp="#s7:l3">verbannen</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dat nu de Koopmanschap <rhyme label="a" type="m">herleev'</rhyme>!</l>
        <l id="s8:l2" met="4" type="-+">De Zeevaardij door 't pekel <rhyme label="a" type="m" corresp="#s8:l1">streev'</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Met rijk geladen <rhyme label="b" type="f">Kielen</rhyme>!</l>
        <l id="s8:l4" met="4" type="-+">Dat Gal en Belg, op Nereus <rhyme label="c" type="m">vloed</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Iet Zeeregtschendend <rhyme label="c" type="m" corresp="#s8:l4">Roosgebroed</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Door 't Heldenstaal <rhyme label="b" type="f" corresp="#s8:l3">vernielen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Zoo word de ruime Zee ook <rhyme label="a" type="m">vrij</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Zoo zal der Britten <rhyme label="a" type="m" corresp="#s9:l1">dwinglandij</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die zich Vorstin durft <rhyme label="b" type="f">noemen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Verzuipen in dien <rhyme label="c" type="m">waterplas</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">En zij, die ons ten geesfel <rhyme label="c" type="m" corresp="#s9:l4">was</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Zich zien ten afgrond <rhyme label="b" type="f" corresp="#s9:l3">doemen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Triumph! Gij smaakt de Vrijheid <rhyme label="a" type="m">weer</rhyme>.</l>
        <l id="s10:l2" met="4" type="-+">Civilis Kroost, het geldt uw <rhyme label="a" type="m" corresp="#s10:l1">eer</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Blijf, blijf dien schat <rhyme label="b" type="f">bewaaren</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">De Gal ging U op 't <rhyme label="c" type="m">Heldenspoor</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Bij 't rommlen van den Krijgstrom <rhyme label="c" type="m" corresp="#s10:l4">voor</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Pluk naast Hem <rhyme label="b" type="f" corresp="#s10:l3">Lauwerblaaren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Toon dat Gij nimmer zijt <rhyme label="a" type="m">ontaard</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Maar, dat Gij 't Hollend <rhyme label="a" type="m" corresp="#s11:l1">Oorlogspaard</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Ook in zijn vaard durft <rhyme label="b" type="f">vellen</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Verheft zich ooit de <rhyme label="c" type="m">dwinglandij</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Volgt dan de spreuk: Of dood, of <rhyme label="c" type="m" corresp="#s11:l4">vrij</rhyme>!</l>
        <l id="s11:l6" met="3" type="-+">En blikfem haar ter <rhyme label="b" type="f" corresp="#s11:l3">helle</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Leef vrolijk, als den vrijen <rhyme label="a" type="m">Gal</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Die bij het vreeslijk <rhyme label="a" type="m" corresp="#s12:l1">krijgsgeschal</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">A-ca-ira doet <rhyme label="b" type="f">hooren</rhyme>.</l>
        <l id="s12:l4" met="4" type="-+">O! de altoos blijde <rhyme label="c" type="m">heldenmoed</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Is 't die de heerschzugt vlugten <rhyme label="c" type="m" corresp="#s12:l4">doet</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Met nederhangende <rhyme label="b" type="f" corresp="#s12:l3">ooren</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Dat eeuwig 't nu gestaafd <rhyme label="a" type="m">Verbond</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Op Vrijheid, liesde en trouw <rhyme label="a" type="m" corresp="#s13:l1">gegrond</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Dat de Hovaardij doet <rhyme label="b" type="f">bukken</rhyme>.</l>
        <l id="s13:l4" met="4" type="-+">De Grondvest zij van ons <rhyme label="c" type="m">geluk</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">En wij bevrijd van zorg en <rhyme label="c" type="m" corresp="#s13:l4">druk</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">De Vrugt der Vrijheid <rhyme label="b" type="f" corresp="#s13:l3">plukken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
