<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186351</id>
    <date>
      <year-start>1672</year-start>
      <year-end>1672</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De lucht is vol geluid</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Och Heer ick ben <rhyme label="a" type="m">Getrouwt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Waer door mijn vreught <rhyme label="a" type="m" corresp="#s1:l1">verflout</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">'k Heb een Griet <rhyme label="b" type="f">gekregen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Dat my nu wel <rhyme label="a" type="m" corresp="#s1:l1">rouwt</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Want voorwaer dat <rhyme label="c" type="f">vercken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Brengt mijn in den <rhyme label="d" type="m">noot</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Sy is te luy ter <rhyme label="c" type="f" corresp="#s1:l5">wercken</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">En noch maeckt 's haer <rhyme label="d" type="m" corresp="#s1:l6">groot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Is schand al voor de <rhyme label="a" type="m">Lien</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die dees Madam <rhyme label="a" type="m" corresp="#s2:l1">aensien</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">'t Is al na de <rhyme label="b" type="f">mode</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Tot haer <rhyme label="a" type="m" corresp="#s2:l1">Hengselin</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Het huygh laet sy <rhyme label="c" type="f">sincken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En laet my <rhyme label="d" type="m">ongerieft</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Iuffrouw die gaet <rhyme label="c" type="f" corresp="#s2:l5">flincken</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Al waer 't haer <rhyme label="d" type="m" corresp="#s2:l6">belieft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoort desen <rhyme label="a" type="m">hannen-gat</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'k En drinck my noch niet <rhyme label="a" type="m" corresp="#s3:l1">sat</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Ghy zijt heele <rhyme label="b" type="f">dagen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Met den beck in 't <rhyme label="a" type="m" corresp="#s3:l1">nat</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Yder een magh <rhyme label="c" type="f">weten</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat u Kinders <rhyme label="d" type="m">zijn</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Dickwils sonder <rhyme label="c" type="f" corresp="#s3:l5">eten</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">En ghy drinckt de <rhyme label="d" type="m" corresp="#s3:l6">Wijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ey seker seght qua <rhyme label="a" type="m">Griet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Want ghy en deught doch <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">'t Zijn u eygen <rhyme label="b" type="f">fouten</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Dat ghy lijdt <rhyme label="a" type="m" corresp="#s4:l1">verdriet</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Want al won ick <rhyme label="c" type="f">schijven</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Trots iemand in de <rhyme label="d" type="m">Stad</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Daer en sou niet <rhyme label="c" type="f" corresp="#s4:l5">blijven</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">'t Moet al aen u <rhyme label="d" type="m" corresp="#s4:l6">gat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wel seght my Ioncker <rhyme label="a" type="m">Ian</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En ist niet beter <rhyme label="a" type="m" corresp="#s5:l1">dan</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Aen het gat <rhyme label="b" type="f">gehangen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Als soo by de <rhyme label="a" type="m" corresp="#s5:l1">kan</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Speelen als de <rhyme label="c" type="f">hanssen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Setten Munt en <rhyme label="d" type="m">Kruys</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">En dan moet ick <rhyme label="c" type="f" corresp="#s5:l5">danssen</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Savonts uyt den <rhyme label="d" type="m" corresp="#s5:l6">huys</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wel Menschen hoort dit <rhyme label="a" type="m">spel</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">So haest ick klop of <rhyme label="a" type="m" corresp="#s6:l1">bel</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Sy begint te <rhyme label="b" type="f">kijven</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Als een Droes van <rhyme label="a" type="m" corresp="#s6:l1">d'hel</rhyme>:</l>
        <l id="s6:l5" met="3" type="+-">Nu moet ick u <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dient haer dan geen <rhyme label="d" type="m">hout</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Lusten haer geen <rhyme label="c" type="f" corresp="#s6:l5">slagen</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Dats haer backhuys <rhyme label="d" type="m" corresp="#s6:l6">hout</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wel moet ick dan so <rhyme label="a" type="m">net</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Gelijck een <rhyme label="a" type="m" corresp="#s7:l1">pissebed</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Altijt voor u <rhyme label="b" type="f">zwijgen</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Wel voorseker 'k <rhyme label="a" type="m" corresp="#s7:l1">wed</rhyme></l>
        <l id="s7:l5" met="3" type="+-">'k Sweer u sonder <rhyme label="c" type="f">liegen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Word ick 't slaen eens <rhyme label="d" type="m">moe</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Dat ick u sal <rhyme label="c" type="f" corresp="#s7:l5">vliegen</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Na u backhuys <rhyme label="d" type="m" corresp="#s7:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Al waert ghy <rhyme label="a" type="m">obstinaet</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ia Grietje noch so <rhyme label="a" type="m" corresp="#s8:l1">quaet</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">So sal ick u <rhyme label="b" type="f">smijten</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Als ghy flincken <rhyme label="a" type="m" corresp="#s8:l1">gaet</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Och waer ick noyt <rhyme label="c" type="f">geboren</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Tot dit groot <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Ick ben wel <rhyme label="c" type="f" corresp="#s8:l5">geschoren</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Met mijn Quade <rhyme label="d" type="m" corresp="#s8:l6">Griet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Och wist ick haer <rhyme label="a" type="m">verset</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Al in de Lombert <rhyme label="a" type="m" corresp="#s9:l1">net</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Ick sou haer <rhyme label="b" type="f">verplacken</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Maer ma foy ick <rhyme label="a" type="m" corresp="#s9:l1">wed</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">'k Segh u sonder <rhyme label="c" type="f">kijven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Ick sou u laten <rhyme label="d" type="m">daer</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">Al soud ghy daer <rhyme label="c" type="f" corresp="#s9:l5">blijven</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">Vijf-en-twintigh <rhyme label="d" type="m" corresp="#s9:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">O ghy verbrilde <rhyme label="a" type="m">Lijs</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ghy zijt weer vol <rhyme label="a" type="m" corresp="#s10:l1">Annijs</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Moet het zijn <rhyme label="b" type="f">versopen</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Lustigh dan om <rhyme label="a" type="m" corresp="#s10:l1">prijs</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Nu sal ick my <rhyme label="c" type="f">weeren</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">So ghy noch iet <rhyme label="d" type="m">ruert</rhyme>,</l>
        <l id="s10:l7" met="3" type="+-">Of ick wil mee <rhyme label="c" type="f" corresp="#s10:l5">smeeren</rhyme>,</l>
        <l id="s10:l8" met="3" type="+-">So langh als het <rhyme label="d" type="m" corresp="#s10:l6">duurt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Daer mee wast her, her, <rhyme label="a" type="m">her</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Margriet die kreegh een <rhyme label="a" type="m" corresp="#s11:l1">ster</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Dat sy <rhyme label="b" type="f">sollebolden</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Doen trock sy van <rhyme label="a" type="m" corresp="#s11:l1">leer</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">Griet die most <rhyme label="c" type="f">gedogen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Al in desen <rhyme label="d" type="m">nood</rhyme>,</l>
        <l id="s11:l7" met="3" type="+-">Kreegh sy twee blau <rhyme label="c" type="f" corresp="#s11:l5">oogen</rhyme>,</l>
        <l id="s11:l8" met="3" type="+-">Met een buyl in 't <rhyme label="d" type="m" corresp="#s11:l6">hoot</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Den Huysraet moster <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En Ian hiel op van <rhyme label="a" type="m" corresp="#s12:l1">slaen</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">Griet begost te <rhyme label="b" type="f">krabben</rhyme>.</l>
        <l id="s12:l4" met="3" type="+-">Neemt dit voor <rhyme label="a" type="m" corresp="#s12:l1">vermaen</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Boeren en <rhyme label="c" type="f">Boerinnen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Lieden van de <rhyme label="d" type="m">Stadt</rhyme>,</l>
        <l id="s12:l7" met="3" type="+-">Wilt u wel <rhyme label="c" type="f" corresp="#s12:l5">versinnen</rhyme>,</l>
        <l id="s12:l8" met="3" type="+-">Eer ghy Echt <rhyme label="d" type="m" corresp="#s12:l6">aenvat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
