<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25440</id>
    <date>
      <year-start>1425</year-start>
      <year-end>1430</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ghelucke, heyl onde alles <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s1:l2" met="4" type="-+">wensche ich, yr eygen, daer ich <rhyme label="b" type="m">bin</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Zi gheeft mir tso allen tsiden <rhyme label="a" type="m" corresp="#s1:l1">moyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">in weys gheyn besser <rhyme label="b" type="m" corresp="#s1:l2">troesterin</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Wilde zi mir ghebieden <rhyme label="c" type="m">vyl</rhyme></l>
        <l id="s1:l6" met="4" type="-+">des heyls, zo ne werde ich nemmer <rhyme label="d" type="m">cranck</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">zint ich yr gherne dienen <rhyme label="c" type="m" corresp="#s1:l5">wil</rhyme>:</l>
        <l id="s1:l8" met="4" type="-+">groen ist der minnen <rhyme label="d" type="m" corresp="#s1:l6">anenvanc</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Roeder verwen bin ich <rhyme label="a" type="m">holt</rhyme>:</l>
        <l id="s2:l2" met="4" type="-+">des wil ich gherne yr dienre <rhyme label="b" type="m">ziin</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Zi lichtet claer zam al dat <rhyme label="a" type="m" corresp="#s2:l1">golt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">miin hophe, miin troyst, miin hoyste <rhyme label="b" type="m" corresp="#s2:l2">anschiin</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Blaeu das dudyt <rhyme label="c" type="m">stedicheyt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">das weys yder steden <rhyme label="d" type="m">man</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">al wert al dem luden <rhyme label="c" type="m" corresp="#s2:l5">leyt</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">ich zal yr dienen, of ich <rhyme label="d" type="m" corresp="#s2:l6">kan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gheyn man mach mir miin lief nicht <rhyme label="a" type="m">leyden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">want zi dorch lieft mich dat <rhyme label="b" type="m">iaer</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Van yren dyenst ich nemmer <rhyme label="a" type="m" corresp="#s3:l1">sceiden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">das zinghe ich weydeliiche <rhyme label="b" type="m" corresp="#s3:l2">vaer</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Tse yren dienst wer ich <rhyme label="c" type="m">gherne</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">al zulde ich liden <rhyme label="d" type="m">arbeyt</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Lief, nu lays mich zwerts <rhyme label="c" type="m" corresp="#s3:l5">onberen</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">des brinck tsorn onde <rhyme label="d" type="m" corresp="#s3:l6">leyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vrouwen dienst bleef nie <rhyme label="a" type="m">verloren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">hoert ich eyn tsartz mondeliin <rhyme label="b" type="m">gheen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">loent zi nicht huden, zi loent <rhyme label="a" type="m" corresp="#s4:l1">morghen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">daer van zal zich ghein man <rhyme label="b" type="m" corresp="#s4:l2">ontzeen</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">In myme hertzen <rhyme label="c" type="m">heymelich</rhyme></l>
        <l id="s4:l6" met="4" type="-+">will ich yr dienen ane <rhyme label="d" type="m">wanch</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Yr lieflicheyt, die vruyt mich <rhyme label="c" type="m" corresp="#s4:l5">witz</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">dat is eyn goyt <rhyme label="d" type="m" corresp="#s4:l6">ghedanch</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
