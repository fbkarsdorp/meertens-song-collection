      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Beata immaculata</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAet ons al t'samen <rhyme label="a" type="f">loven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met sanck en <rhyme label="b" type="m">snarenspel</rhyme></l>
        <l id="s1:l3" met="3" type="-+">De Borghers van hier <rhyme label="a" type="f" corresp="#s1:l1">boven</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Want sy verdienent <rhyme label="b" type="m" corresp="#s1:l2">wel</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Singht met <rhyme label="c" type="m" corresp="#s1:l5">jolijdt</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s1:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer onder veel <rhyme label="a" type="f">diversche</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soo wil ick mijnen <rhyme label="b" type="m">Patroon</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En mijnen <rhyme label="a" type="f" corresp="#s2:l1">Patronnersse</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Singhen een loff <rhyme label="b" type="m" corresp="#s2:l2">ydoon</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Singht met <rhyme label="c" type="m" corresp="#s2:l5">jolijdt</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s2:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O soet Patroon <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ã” Patronersse <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wat soud' ick u best <rhyme label="a" type="f" corresp="#s3:l1">gheven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat u sou wesen <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Singht met <rhyme label="c" type="m" corresp="#s3:l5">jolijdt</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s3:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wilt ghy ons wel <rhyme label="a" type="f">vereeren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die onsen naeme <rhyme label="b" type="m">draeght</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Voor allen, wilt dit <rhyme label="a" type="f" corresp="#s4:l1">leeren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">V reyn van sonden <rhyme label="b" type="m" corresp="#s4:l2">vaeght</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Singht met <rhyme label="c" type="m" corresp="#s4:l5">jolijdt</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s4:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daer naer soo moet ghy <rhyme label="a" type="f">talen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Naer onse deughden <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Cont ghy die <rhyme label="a" type="f" corresp="#s5:l1">achterhalen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Weest sekers s'Hemels <rhyme label="b" type="m" corresp="#s5:l2">croon</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Singht met <rhyme label="c" type="m" corresp="#s5:l5">jolijdt</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s5:l5">quijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
