    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Tien geboden</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Mijn Godt wilt my nu <rhyme label="a" type="f">bevrijden</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Voor den Menschen seer boos en <rhyme label="b" type="m">quaedt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Behoet my nu, en t' allen <rhyme label="a" type="f" corresp="#s1:l1">tijden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Voor der listigen raedt en <rhyme label="b" type="m" corresp="#s1:l2">daet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die daghelijcks met haers <rhyme label="a" type="f">ghelijcken</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Teghen my dencken schalckheydt <rhyme label="b" type="m">loos</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ende my strijt in allen <rhyme label="a" type="f" corresp="#s2:l1">wijcken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van nieus aendoen met herten <rhyme label="b" type="m" corresp="#s2:l2">boos</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer listighe tonghen <rhyme label="a" type="f">hooghmoedigh</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Scherpen sy als slanghen seer <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Onder haer lippen <rhyme label="a" type="f" corresp="#s3:l1">overvloedigh</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Hebben sy adderen, <rhyme label="b" type="m" corresp="#s3:l2">fenijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Bewaert my voor der boosen <rhyme label="a" type="f">handen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En mijn ganghen tot deser <rhyme label="b" type="m">stondt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Voor den moetwillighen <rhyme label="a" type="f" corresp="#s4:l1">vyanden</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die my willen brenghen te <rhyme label="b" type="m" corresp="#s4:l2">grondt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De grootsche hebben nu met <rhyme label="a" type="f">listen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Mijner zielen stricken <rhyme label="b" type="m">bereyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Op datse my te vanghen <rhyme label="a" type="f" corresp="#s5:l1">wisten</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">In den weghen daer ghy my <rhyme label="b" type="m" corresp="#s5:l2">leyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doe sprack ick met vasten <rhyme label="a" type="f">betrouwen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ghy zijt, o Heer, mijn Godt <rhyme label="b" type="m">alleyn</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Verhoort my in mijn swaer <rhyme label="a" type="f" corresp="#s6:l1">benouwen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Hoort de stemme mijns ghebedts <rhyme label="b" type="m" corresp="#s6:l2">reyn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Heer ghy zijt mijn wapen seer <rhyme label="a" type="f">crachtigh</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Mijn helm, als my den noot <rhyme label="b" type="m">beswaert</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Als ick in 't veldt benaut was <rhyme label="a" type="f" corresp="#s7:l1">clachtigh</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Hebt ghy my 'thooft ghedeckt <rhyme label="b" type="m" corresp="#s7:l2">bewaert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wilt den boosen, Heer, niet <rhyme label="a" type="f">toelaten</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">'Tvolbrenghen haers voornemens <rhyme label="b" type="m">fel</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Anders souden sy die my <rhyme label="a" type="f" corresp="#s8:l1">haten</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Hooghmoedigh en stout werden <rhyme label="b" type="m" corresp="#s8:l2">snel</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat den Oversten onder <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Die my quelt, toecome met <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Dat lijden end' ellendigh <rhyme label="a" type="f" corresp="#s9:l1">wesen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Dat hy my te doen was <rhyme label="b" type="m" corresp="#s9:l2">bedacht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dat hem colen op den cop <rhyme label="a" type="f">vallen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">En gantschelijck te gronde <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Slaetse met tempeest over <rhyme label="a" type="f" corresp="#s10:l1">allen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Soo datse niet meer op en <rhyme label="b" type="m" corresp="#s10:l2">staen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">De quaedt-spreeckende mensch <rhyme label="a" type="f">alommen</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Sal niet lange blijven in <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Op den moetwilligen sal <rhyme label="a" type="f" corresp="#s11:l1">commen</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Sijn eygen aenslagh en zijn <rhyme label="b" type="m" corresp="#s11:l2">raet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Godt sal zijn getrouheyt <rhyme label="a" type="f">verclaren</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Hem, die met onrecht is <rhyme label="b" type="m">gequelt</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Sy die den vromen hier <rhyme label="a" type="f" corresp="#s12:l1">beswaren</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Werden voor Godts oordeel <rhyme label="b" type="m" corresp="#s12:l2">gestelt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">De vrome sullen uwen <rhyme label="a" type="f">Name</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Die heerlijck is prijsen, o <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">D'oprechte sullen eeuwigh <rhyme label="a" type="f" corresp="#s13:l1">t'same</rhyme></l>
        <l id="s13:l4" met="4" type="-+">By u woonen in aller <rhyme label="b" type="m" corresp="#s13:l2">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
