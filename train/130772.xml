    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wel heeft Salomon <rhyme label="a" type="m">geseid</rhyme>:</l>
        <l id="s1:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s1:l1">‘Ydelheid</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ydelheid zijn alle <rhyme label="b" type="f">dingen</rhyme>.’</l>
        <l id="s1:l4" met="4" type="+-">Zijn dan alle dingen <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Dan <rhyme label="c" type="m" corresp="#s1:l4">verdriet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wat kan eensaemheid <rhyme label="b" type="f" corresp="#s1:l3">voortbringen</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wat is dog den Mens <rhyme label="a" type="m">alleen</rhyme>?</l>
        <l id="s2:l2" met="2" type="+-">In 't <rhyme label="a" type="m" corresp="#s2:l1">gemeen</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Wat is dog de mens <rhyme label="b" type="f">alleen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die dikwils hier met <rhyme label="c" type="m">geklag</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Nagt en <rhyme label="c" type="m" corresp="#s2:l4">dag</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Word besogt, met veel <rhyme label="b" type="f" corresp="#s2:l3">geween</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die slegts vreemde oogen <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Vrienden <rhyme label="a" type="m" corresp="#s3:l1">niet</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Heeft geen kinderen nog <rhyme label="b" type="f">neven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Nog met geen vriend'lijk <rhyme label="c" type="m">gemoed</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Word <rhyme label="c" type="m" corresp="#s3:l4">gegroet</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Onsoet is des mensen <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laet hem werken vroeg en <rhyme label="a" type="m">laet</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">'t Helpt nog <rhyme label="a" type="m" corresp="#s4:l1">baet</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Om het hoopjen te <rhyme label="b" type="f">vermeeren</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Wat hij wint of wat hy <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Spaert of <rhyme label="c" type="m" corresp="#s4:l4">wroet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Wie sal 't overschot   <rhyme label="b" type="f" corresp="#s4:l3">verteeren</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Is wel beter met sijn <rhyme label="a" type="m">tween</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Dan <rhyme label="a" type="m" corresp="#s5:l1">alleen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om de last te helpen <rhyme label="b" type="f">dreegen</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Want dat op twee schijven <rhyme label="c" type="m">glijd</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Beter <rhyme label="c" type="m" corresp="#s5:l4">vlijd</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dan dat eene doet <rhyme label="b" type="f" corresp="#s5:l3">bewegen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Is 't dat een van beiden <rhyme label="a" type="m">valt</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">d'Ander sal <rhyme label="a" type="m" corresp="#s6:l1">'t</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Trouw'lijk   soeken te <rhyme label="b" type="f">beletten</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Kom d' een in en d' ander <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Elk is <rhyme label="c" type="m" corresp="#s6:l4">ree</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Om terstond een stoel te <rhyme label="b" type="f" corresp="#s6:l3">setten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Een is overwonnen <rhyme label="a" type="m">haest</rhyme></l>
        <l id="s7:l2" met="2" type="+-">En <rhyme label="a" type="m" corresp="#s7:l1">verbaest</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Een alleen komt haest te <rhyme label="b" type="f">sneven</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Twee-dik snoer van kennip   of <rhyme label="c" type="m">vlas</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Breekt niet <rhyme label="c" type="m" corresp="#s7:l4">ras</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Enkelt sal 't hem haest <rhyme label="b" type="f" corresp="#s7:l3">begeven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Twee in Egt te saem <rhyme label="a" type="m">gepaert</rhyme></l>
        <l id="s8:l2" met="2" type="+-">En <rhyme label="a" type="m" corresp="#s8:l1">vergaert</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Konnen malkanderen <rhyme label="b" type="f">verwermen</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Die alleen is en wordt <rhyme label="c" type="m">oudt</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">'s Winters <rhyme label="c" type="m" corresp="#s8:l4">koudt</rhyme>.</l>
        <l id="s8:l6" met="4" type="+-">Dikmael seid hy: ‘Ey, og <rhyme label="b" type="f" corresp="#s8:l3">ermen</rhyme>!’</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wat den Prediker hier <rhyme label="a" type="m">leert</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s9:l1">Practiseert</rhyme>!</l>
        <l id="s9:l3" met="4" type="+-">'t Is een goê leer, soo ik <rhyme label="b" type="f">meene</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">Want God self sprak in <rhyme label="c" type="m">voorspoet</rhyme>:</l>
        <l id="s9:l5" met="2" type="+-">‘'t Is niet <rhyme label="c" type="m" corresp="#s9:l4">goet</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Dat den mensche zy <rhyme label="b" type="f" corresp="#s9:l3">alleene</rhyme>. ’</l>
      </lg>
    </lg>
  </text>
</song>
