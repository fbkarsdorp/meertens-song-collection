<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184501</id>
    <date>
      <year-start>1803</year-start>
      <year-end>1803</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie hoe gelukkig zijn ze dan</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Komt vrienden luisterd na dit <rhyme label="a" type="m">lied</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat ik u hier zal <rhyme label="b" type="f">zingen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Het is dog zo <rhyme label="a" type="m" corresp="#s1:l1">geschied</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Een maagd van <rhyme label="c" type="m">Amsterdam</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Die daar een reis <rhyme label="c" type="m" corresp="#s1:l4">aannam</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In Haarlem dienen <rhyme label="c" type="m" corresp="#s1:l4">kwam</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zy diende by een koopman <rhyme label="a" type="m">daar</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die had een eenig <rhyme label="b" type="f">zoone</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die sloeg zyn oog op <rhyme label="a" type="m" corresp="#s2:l1">haar</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy sprak, myn lief <rhyme label="c" type="m">vriendin</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">gy staat in mynen <rhyme label="c" type="m" corresp="#s2:l4">zin</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Anders ik niemand <rhyme label="c" type="m" corresp="#s2:l4">min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zy sprak, myn Heer gaat dog van <rhyme label="a" type="m">myn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gy kunt wel aers <rhyme label="b" type="f">aanraaken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daar zyn wel Juffers <rhyme label="a" type="m" corresp="#s3:l1">fyn</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Maar hy sprak haar <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En maakte droef <rhyme label="c" type="m" corresp="#s3:l4">geween</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Met klagen en <rhyme label="c" type="m" corresp="#s3:l4">gebeên</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy dan met vlyen niet <rhyme label="a" type="m">afliet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Tot hy de maagd <rhyme label="b" type="f">beslaapen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En haar eer beroofd <rhyme label="a" type="m" corresp="#s4:l1">ziet</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Zo dat de maagd <rhyme label="c" type="m">aldaar</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Raakte met 't kinde <rhyme label="c" type="m" corresp="#s4:l4">zwaar</rhyme></l>
        <l id="s4:l6" met="3" type="-+">By deeze zoone <rhyme label="c" type="m" corresp="#s4:l4">klaar</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Agt maanden, ach! het was <rhyme label="a" type="m">publyk</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Doe ging de zoone <rhyme label="b" type="f">reizen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Geheel al na <rhyme label="a" type="m" corresp="#s5:l1">Frankryk</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Sprak doen, myn liefje <rhyme label="c" type="m">koen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Als gy baart met <rhyme label="c" type="m" corresp="#s5:l4">fatsoen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wilt dan uw kind <rhyme label="c" type="m" corresp="#s5:l4">verdoen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Op dat het mag verholen <rhyme label="a" type="m">zyn</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Als ik wederom <rhyme label="b" type="f">kome</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Zult gy trouwen met <rhyme label="a" type="m" corresp="#s6:l1">myn</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Och lief, sprak zy met <rhyme label="c" type="m">spoed</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ik kan niet zyn <rhyme label="c" type="m" corresp="#s6:l4">verwoed</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Op myn vleesch ende <rhyme label="c" type="m" corresp="#s6:l4">bloed</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Kom lief, wilt myn beloven <rhyme label="a" type="m">stil</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat gy het zult <rhyme label="b" type="f">vermoorden</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En doet dog mynen <rhyme label="a" type="m" corresp="#s7:l1">wil</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Daar is een zak met <rhyme label="c" type="m">geld</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Het is nog <rhyme label="c" type="m" corresp="#s7:l4">ongeteld</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daar gy van leven <rhyme label="c" type="m" corresp="#s7:l4">zelt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Zy beloofde daar toe met 'er <rhyme label="a" type="m">hand</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat zy het zou <rhyme label="b" type="f">verinoorden</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En zoo helpen van <rhyme label="a" type="m" corresp="#s8:l1">kant</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Toen trok de jonkman <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">De maagd vol van <rhyme label="c" type="m" corresp="#s8:l4">geween</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Bleef zitten daar <rhyme label="c" type="m" corresp="#s8:l4">alleen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Als nu haar baarensnood kwam <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Ging zy na boven <rhyme label="b" type="f">treeden</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Heel droevig en <rhyme label="a" type="m" corresp="#s9:l1">belaan</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Baar heeft zy doe <rhyme label="c" type="m">geteeld</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Een jonge zoone <rhyme label="c" type="m" corresp="#s9:l4">eelt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Als een albaster <rhyme label="c" type="m" corresp="#s9:l4">beeld</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Als zy het kind daar hadde <rhyme label="a" type="m">nou</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ging zy te regt <rhyme label="b" type="f">bepeinzen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Of zy 't vermoorden <rhyme label="a" type="m" corresp="#s10:l1">zou</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Maar daar een stemme <rhyme label="c" type="m">kwam</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Als zy 't in haar armen <rhyme label="c" type="m" corresp="#s10:l4">nam</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Bewaard uw lieve <rhyme label="c" type="m" corresp="#s10:l4">Lam</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Zy viel doe op haar kniën <rhyme label="a" type="m">neêr</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">En stortte haar <rhyme label="b" type="f">gebeden</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">En riep aan God den <rhyme label="a" type="m" corresp="#s11:l1">Heer</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">O groote God zeer <rhyme label="c" type="m">goed</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Ik bid u met <rhyme label="c" type="m" corresp="#s11:l4">ootmoed</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">My en myn kind <rhyme label="c" type="m" corresp="#s11:l4">behoed</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Zy wond het kind in doeken <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En lag het in een <rhyme label="b" type="f">tonne</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Ging na beneden <rhyme label="a" type="m" corresp="#s12:l1">voort</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Of haar gantsch deerde <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Maar deeze maaget <rhyme label="c" type="m" corresp="#s12:l4">ziet</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Was wel in groot <rhyme label="c" type="m" corresp="#s12:l4">verdriet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Zy deed haar werk zo het <rhyme label="a" type="m">behoort</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Als zy het had <rhyme label="b" type="f">gedaane</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Ging zy naar boven <rhyme label="a" type="m" corresp="#s13:l1">voort</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">En voedde het kind <rhyme label="c" type="m">aldaar</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Tot dat het was een <rhyme label="c" type="m" corresp="#s13:l4">jaar</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Eer 't iemand weird <rhyme label="c" type="m" corresp="#s13:l4">gewaar</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Haar Meester dit op 't laatst <rhyme label="a" type="m">vernam</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Hy sprak tot zyn <rhyme label="b" type="f">huisvrouwe</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Hoe of dat zo <rhyme label="a" type="m" corresp="#s14:l1">bykwam</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Dat altyd de meid <rhyme label="c" type="m">plaan</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Als ze 't werk heeft <rhyme label="c" type="m" corresp="#s14:l4">gedaan</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Na boven toe moet <rhyme label="c" type="m" corresp="#s14:l4">gaan</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Ik moet haar stuuren eens na <rhyme label="a" type="m">buyt</rhyme>:</l>
        <l id="s15:l2" met="3" type="-+">Op Vogelwyks <rhyme label="b" type="f">Hofstede</rhyme>:</l>
        <l id="s15:l3" met="3" type="-+">Dat zy wat lang blyft <rhyme label="a" type="m" corresp="#s15:l1">uit</rhyme>?</l>
        <l id="s15:l4" met="3" type="-+">Want ik kryg kwa <rhyme label="c" type="m">gedagt</rhyme></l>
        <l id="s15:l5" met="3" type="-+">En moet dan zien met <rhyme label="c" type="m" corresp="#s15:l4">magt</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Wat zy daar heeft <rhyme label="c" type="m" corresp="#s15:l4">gebragt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Men stuurde haar na 't Hofstee <rhyme label="a" type="m">terstond</rhyme>.</l>
        <l id="s16:l2" met="3" type="-+">Vrouw en Meester gingen <rhyme label="b" type="f">boven</rhyme>;</l>
        <l id="s16:l3" met="3" type="-+">Zy zogten daar in 't <rhyme label="a" type="m" corresp="#s16:l1">rond</rhyme>;</l>
        <l id="s16:l4" met="3" type="-+">Maar wat zy zogten <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Met yver en met <rhyme label="c" type="m" corresp="#s16:l4">vliet</rhyme>?</l>
        <l id="s16:l6" met="3" type="-+">Zy en vonden daar <rhyme label="c" type="m" corresp="#s16:l4">niet</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Op 't lest zoo sprak de Meester <rhyme label="a" type="m">daar</rhyme>;</l>
        <l id="s17:l2" met="3" type="-+">Ik moet ook eens gaan <rhyme label="b" type="f">kyken</rhyme>:</l>
        <l id="s17:l3" met="3" type="-+">In deze Ton <rhyme label="a" type="m" corresp="#s17:l1">voorwaar</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">D ar zag hy het Schaapje <rhyme label="c" type="m">teer</rhyme>?</l>
        <l id="s17:l5" met="3" type="-+">Verschrikte alzo <rhyme label="c" type="m" corresp="#s17:l4">zeer</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Dat hy schier viel ter <rhyme label="c" type="m" corresp="#s17:l4">neer</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Hy riep zeer luide en <rhyme label="a" type="m">verbaast</rhyme>;</l>
        <l id="s18:l2" met="3" type="-+">Een Kind myn lieve <rhyme label="b" type="f">Vrouwe</rhyme>;</l>
        <l id="s18:l3" met="3" type="-+">Legt hier, komt met 'er <rhyme label="a" type="m" corresp="#s18:l1">haast</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Sou dit wel zyn ten <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Van onze Maaget <rhyme label="c" type="m" corresp="#s18:l4">schoon</rhyme>;</l>
        <l id="s18:l6" met="3" type="-+">Dat zy heeft by ons <rhyme label="c" type="m" corresp="#s18:l4">Soon</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Zy bragten 't kindje na <rhyme label="a" type="m">beneen</rhyme>?</l>
        <l id="s19:l2" met="3" type="-+">De Maagd kwam 't huis <rhyme label="b" type="f">gegange</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">En ging na boven <rhyme label="a" type="m" corresp="#s19:l1">treen</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Zy in de Tonne <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Maar vond haar kindje <rhyme label="c" type="m" corresp="#s19:l4">niet</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Dat was haar groot <rhyme label="c" type="m" corresp="#s19:l4">verdriet</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Sy ging weer na beneeden <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">Met oogen vol van <rhyme label="b" type="f">traanen</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">De Meester zeide <rhyme label="a" type="m" corresp="#s20:l1">hoort</rhyme>;</l>
        <l id="s20:l4" met="3" type="-+">Is u geschied veel <rhyme label="c" type="m">leed</rhyme>:</l>
        <l id="s20:l5" met="3" type="-+">Zegt het myn zoete <rhyme label="c" type="m" corresp="#s20:l4">Meid</rhyme>?</l>
        <l id="s20:l6" met="3" type="-+">Waarom dat gy zoo <rhyme label="c" type="m" corresp="#s20:l4">schreid</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Met regt is 't dat ik schreijen <rhyme label="a" type="m">mag</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Myn is myn kind <rhyme label="b" type="f">ontnomen</rhyme>?</l>
        <l id="s21:l3" met="3" type="-+">Dat stil in ruste <rhyme label="a" type="m" corresp="#s21:l1">lag</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Hy bragt de Maagd <rhyme label="c" type="m">eerbaar</rhyme>;</l>
        <l id="s21:l5" met="3" type="-+">Al by zyn Vrouw a <rhyme label="c" type="m" corresp="#s21:l4">aar</rhyme>:</l>
        <l id="s21:l6" met="3" type="-+">Dieh et haar kindje <rhyme label="c" type="m" corresp="#s21:l4">waar</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">De Meester vraagde heel <rhyme label="a" type="m">gewis</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">Wie van dat zoete <rhyme label="b" type="f">kindje</rhyme>,</l>
        <l id="s22:l3" met="3" type="-+">De regte Vader <rhyme label="a" type="m" corresp="#s22:l1">is</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">De Maagd sprak zonder <rhyme label="c" type="m">schroom</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Het is u eigen <rhyme label="c" type="m" corresp="#s22:l4">Zoon</rhyme>:</l>
        <l id="s22:l6" met="3" type="-+">Die my bedroog <rhyme label="c" type="m" corresp="#s22:l4">onvroom</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Wel zoo myn Zoon de Vader <rhyme label="a" type="m">is</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Soo moet hy u ook <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s23:l3" met="3" type="-+">Hebt gy <rhyme label="a" type="m" corresp="#s23:l1">beloftenis</rhyme>,</l>
        <l id="s23:l4" met="3" type="-+">Ja sprak zy van zyn <rhyme label="c" type="m">hand</rhyme>;</l>
        <l id="s23:l5" met="3" type="-+">Heb ik dees trouw <rhyme label="c" type="m" corresp="#s23:l4">faljant</rhyme>,</l>
        <l id="s23:l6" met="3" type="-+">Die hy my gaf te <rhyme label="c" type="m" corresp="#s23:l4">pand</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Hy schreef een brief wagte niet <rhyme label="a" type="m">lank</rhyme>,</l>
        <l id="s24:l2" met="3" type="-+">Aen Zoon wild <rhyme label="b" type="f">overkomen</rhyme>?</l>
        <l id="s24:l3" met="3" type="-+">Want wy leggen heel <rhyme label="a" type="m" corresp="#s24:l1">krank</rhyme>!</l>
        <l id="s24:l4" met="3" type="-+">En dat wel haastig <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Eer de dood ons <rhyme label="c" type="m" corresp="#s24:l4">ontmoet</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">Dus kykt dan na u <rhyme label="c" type="m" corresp="#s24:l4">goed</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">De Zoon wel haastig reizen <rhyme label="a" type="m">kwam</rhyme>,</l>
        <l id="s25:l2" met="3" type="-+">Als hy t'huis was <rhyme label="b" type="f">gekomen</rhyme>:</l>
        <l id="s25:l3" met="3" type="-+">Wel haast wat aars <rhyme label="a" type="m" corresp="#s25:l1">vernam</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">De Vader sprak hem <rhyme label="c" type="m">an</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">Segt myn nu Zoone <rhyme label="c" type="m" corresp="#s25:l4">dan</rhyme>?</l>
        <l id="s25:l6" met="3" type="-+">Bent gy der Vader <rhyme label="c" type="m" corresp="#s25:l4">van</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Hy sprak, ja Vader met 'er <rhyme label="a" type="m">spoed</rhyme></l>
        <l id="s26:l2" met="3" type="-+">Ik kan het niet <rhyme label="b" type="f">verzaken</rhyme>,</l>
        <l id="s26:l3" met="3" type="-+">'t Is myn vleesch en <rhyme label="a" type="m" corresp="#s26:l1">bloed</rhyme>;</l>
        <l id="s26:l4" met="3" type="-+">Wel hebt gy zulks <rhyme label="c" type="m">gedaan</rhyme>,</l>
        <l id="s26:l5" met="3" type="-+">Soo wil 'k van stonden <rhyme label="c" type="m" corresp="#s26:l4">aan</rhyme>,</l>
        <l id="s26:l6" met="3" type="-+">Gy haar zult trouwen <rhyme label="c" type="m" corresp="#s26:l4">gaan</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">De Zoon deede veel <rhyme label="a" type="m">tegenweer</rhyme>,</l>
        <l id="s27:l2" met="3" type="-+">Doen sprak de Vader <rhyme label="b" type="f">goeding</rhyme>,</l>
        <l id="s27:l3" met="3" type="-+">Brengt deeze Maagd in <rhyme label="a" type="m" corresp="#s27:l1">eer</rhyme>,</l>
        <l id="s27:l4" met="3" type="-+">Of ik maak met 'er <rhyme label="c" type="m">spoed</rhyme>,</l>
        <l id="s27:l5" met="3" type="-+">Uw basterd van myn <rhyme label="c" type="m" corresp="#s27:l4">goed</rhyme>,</l>
        <l id="s27:l6" met="3" type="-+">En aan 't kindje <rhyme label="c" type="m" corresp="#s27:l4">zoet</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Doe heeft de Zoon <rhyme label="a" type="m">geconzenteerd</rhyme>,</l>
        <l id="s28:l2" met="3" type="-+">Zy gingen daar te <rhyme label="b" type="f">trouwen</rhyme>?</l>
        <l id="s28:l3" met="3" type="-+">Dit was de Meids <rhyme label="a" type="m" corresp="#s28:l1">begeer</rhyme></l>
        <l id="s28:l4" met="3" type="-+">De Vader bewerkt met <rhyme label="c" type="m">vlyt</rhyme>:</l>
        <l id="s28:l5" met="3" type="-+">Dees Maagt vol <rhyme label="c" type="m" corresp="#s28:l4">trouwigheid</rhyme>,</l>
        <l id="s28:l6" met="3" type="-+">Die was ook zeer <rhyme label="c" type="m" corresp="#s28:l4">verblyd</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Gy Gogters als gy komt ten <rhyme label="a" type="m">val</rhyme>,</l>
        <l id="s29:l2" met="3" type="-+">Wilt God voor oogen <rhyme label="b" type="f">houden</rhyme>,</l>
        <l id="s29:l3" met="3" type="-+">Hy u dan helpen <rhyme label="a" type="m" corresp="#s29:l1">zal</rhyme>:</l>
        <l id="s29:l4" met="3" type="-+">Gelyk als deze <rhyme label="c" type="m">meid</rhyme>,</l>
        <l id="s29:l5" met="3" type="-+">Onnozel was <rhyme label="c" type="m" corresp="#s29:l4">verlied</rhyme>?</l>
        <l id="s29:l6" met="3" type="-+">Maar God heeft haar <rhyme label="c" type="m" corresp="#s29:l4">verblyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
