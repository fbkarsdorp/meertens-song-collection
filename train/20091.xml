  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LVstig Uylenborger <rhyme label="a" type="f">Brakjes</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Toontje vrolijk by de <rhyme label="b" type="m">vocht</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Wat, we moete met je <rhyme label="a" type="f" corresp="#s1:l1">strakjes</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Mee eens springen in de <rhyme label="b" type="m" corresp="#s1:l2">bocht</rhyme>.</l>
        <l id="s1:l5" met="2" type="+-">Hy za, <rhyme label="c" type="m">za</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Fa, la, <rhyme label="c" type="m" corresp="#s1:l5">la</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Sakker Benjamijn, dat <rhyme label="d" type="m">zijn</rhyme></l>
        <l id="s1:l8" met="2" type="+-"><rhyme label="e" type="f">Kabbeljautjes</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Kijk, hoe <rhyme label="e" type="f" corresp="#s1:l8">gautjes</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Steven slingert met sijn <rhyme label="d" type="m" corresp="#s1:l7">Trijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Stap wech met je zye <rhyme label="a" type="f">kousjes</rhyme>:</l>
        <l id="s2:l2" met="4" type="+-">Wilt je vlugge voetjes <rhyme label="b" type="m">spoen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wel wat zelle deuze <rhyme label="a" type="f" corresp="#s2:l1">Smousjes</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Hier in ons gezelschop <rhyme label="b" type="m" corresp="#s2:l2">doen</rhyme>:</l>
        <l id="s2:l5" met="2" type="+-">Laat ze <rhyme label="c" type="m">gaan</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Hier van <rhyme label="c" type="m" corresp="#s2:l5">daan</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Wakker Zaar en Maay, laet <rhyme label="d" type="m">ons</rhyme>.</l>
        <l id="s2:l8" met="2" type="+-">Met Jan <rhyme label="e" type="f">Franse</rhyme></l>
        <l id="s2:l9" met="2" type="+-">tâ€™Zaam eens <rhyme label="e" type="f" corresp="#s2:l8">danse</rhyme>:</l>
        <l id="s2:l10" met="4" type="+-">Wil je niet: loop heen Moer <rhyme label="d" type="m" corresp="#s2:l7">Slons</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Piet, hoe duikje zoo veur <rhyme label="a" type="f">over</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Presenteertje als een <rhyme label="b" type="m">Man</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Gijs, al is je beurs wat <rhyme label="a" type="f" corresp="#s3:l1">pover</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Scheyt daarom niet van de <rhyme label="b" type="m" corresp="#s3:l2">kan</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Hoor je <rhyme label="c" type="m">niet</rhyme>?</l>
        <l id="s3:l6" met="2" type="+-">Tap <rhyme label="c" type="m" corresp="#s3:l5">Margriet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Frans, ik breng je een hallef <rhyme label="d" type="m">bier</rhyme>:</l>
        <l id="s3:l8" met="2" type="+-">Dat gaat <rhyme label="e" type="f">wakker</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Zeg eend <rhyme label="e" type="f" corresp="#s3:l8">Makker</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Waer is meer vermaak als <rhyme label="d" type="m" corresp="#s3:l7">hier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
