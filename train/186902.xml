<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186902</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Wees niet bedrukt maat Dries</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom! Fatavieren <rhyme label="a" type="m">kom</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Matroozen en <rhyme label="b" type="f">Soldaaten</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Men slaat alom de <rhyme label="a" type="m" corresp="#s1:l1">trom</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Men bied u nu <rhyme label="b" type="f" corresp="#s1:l2">Ducaaten</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En Goude Ryders op de <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Om, met het Zwaard op <rhyme label="d" type="f">zyde</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Vol heldenmoed te <rhyme label="d" type="f" corresp="#s1:l6">stryden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Voor 't lieve <rhyme label="c" type="m" corresp="#s1:l5">Vaderland</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gy ziet hoe de <rhyme label="a" type="m">Engelsman</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het al wil <rhyme label="b" type="f">overheeren</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En waant, hy mag en <rhyme label="a" type="m" corresp="#s2:l1">kan</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">De Zee alleen <rhyme label="b" type="f" corresp="#s2:l2">regeeren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">En trots op zyn <rhyme label="c" type="m">heerschappy</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Kent zyne waan geen <rhyme label="d" type="f">paalen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Hoe kan de Brit zo <rhyme label="d" type="f" corresp="#s2:l6">dwalen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Is dan de Zee niet <rhyme label="c" type="m" corresp="#s2:l5">vry</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat heeft hy <rhyme label="a" type="m">menigmaal</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De trouw en eer <rhyme label="b" type="f">geschonden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Op Zee, met wreed <rhyme label="a" type="m" corresp="#s3:l1">onthaal</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Uw goed en bloed <rhyme label="b" type="f" corresp="#s3:l2">verslonden</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Hoe heeft hy, tegen recht en <rhyme label="c" type="m">rêen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">De Scheepen steeds <rhyme label="d" type="f">beslagen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wie kan dien hoon <rhyme label="d" type="f" corresp="#s3:l6">verdraagen</rhyme>?</l>
        <l id="s3:l8" met="3" type="-+">Geen Batavier, ô <rhyme label="c" type="m" corresp="#s3:l5">neen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hier mee nog niet te <rhyme label="a" type="m">vreê</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Beslaat hy nu de <rhyme label="b" type="f">Scheepen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gelyk een gier op <rhyme label="a" type="m" corresp="#s4:l1">zee</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In zyn roofnest te <rhyme label="b" type="f" corresp="#s4:l2">sleepen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Hy plundert, met een dollen <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De ryke <rhyme label="d" type="f">Koopvaersvlooten</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van ons zyn <rhyme label="d" type="f" corresp="#s4:l6">Bondgenooten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dat duld geen Neerlands <rhyme label="c" type="m" corresp="#s4:l5">bloed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Of zou dan Hollands <rhyme label="a" type="m">Leeuw</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Voor Britsche Doggen <rhyme label="b" type="f">wyken</rhyme>?</l>
        <l id="s5:l3" met="3" type="-+">Hun Demmen en <rhyme label="a" type="m" corresp="#s5:l1">geschreeuw</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hem doen de moed <rhyme label="b" type="f" corresp="#s5:l2">bezwyken</rhyme>?</l>
        <l id="s5:l5" met="4" type="-+">ô Neen! hy vreest niet hun <rhyme label="c" type="m">getier</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hy zal zig haast wel <rhyme label="d" type="f">wreeken</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Van hoon en valsche <rhyme label="d" type="f" corresp="#s5:l6">streeken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Hy is nog even <rhyme label="c" type="m" corresp="#s5:l5">fier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Welaen dan flux ten <rhyme label="a" type="m">stryd</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Matroozen en <rhyme label="b" type="f">Krygsknegten</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">Toord dat gy vaardig <rhyme label="a" type="m" corresp="#s6:l1">zyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Om onversaagt te <rhyme label="b" type="f" corresp="#s6:l2">vegten</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Voor de eer en 't recht van 't <rhyme label="c" type="m">Vaderland</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">De Prins en Heeren <rhyme label="d" type="f">Staaten</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die zich op u <rhyme label="d" type="f" corresp="#s6:l6">verlaaten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">De Brit die moet aan <rhyme label="c" type="m" corresp="#s6:l5">kant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wy moeten nu niet <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Lafhartig ons <rhyme label="b" type="f">gedraagen</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Geen Vreede dan met <rhyme label="a" type="m" corresp="#s7:l1">eer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Moet onzen Staat <rhyme label="b" type="f" corresp="#s7:l2">behaagen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">De drommel, neen. Het moet 'er <rhyme label="c" type="m">deur</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ons laadt moet <rhyme label="d" type="f">schaa-verhaalen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">De Brit 't gelag <rhyme label="d" type="f" corresp="#s7:l6">betaalen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wy zyn nog in ons <rhyme label="c" type="m" corresp="#s7:l5">fleur</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Laat u hun <rhyme label="a" type="m">overmagt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En vreedheid niet <rhyme label="b" type="f">vervaaren</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">De Rus en Deensche <rhyme label="a" type="m" corresp="#s8:l1">kragt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Zal haest met ons zich <rhyme label="b" type="f" corresp="#s8:l2">paeren</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Wy hebben ook nog kruit en <rhyme label="c" type="m">loot</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Nog Ruyters en nog <rhyme label="d" type="f">Trompen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Om nu dien Dog te <rhyme label="d" type="f" corresp="#s8:l6">stompen</rhyme>:</l>
        <l id="s8:l8" met="3" type="-+">Tot dat hy is mors <rhyme label="c" type="m" corresp="#s8:l5">dood</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
