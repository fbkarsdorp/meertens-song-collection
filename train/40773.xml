  <author>Gysbert Japicx</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O! Wol-lock-silligh' <rhyme label="a" type="f">minsche</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dy de' HEER' God hosck <rhyme label="b" type="m">ontsjocht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In giet (ô wâd in <rhyme label="a" type="f" corresp="#s1:l1">winsche</rhyme>!)</l>
        <l id="s1:l4" met="3" type="-+">Goads wey, yn 't jinge' hy <rhyme label="b" type="m" corresp="#s1:l2">docht</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">God sil dijn hann'-wirck <rhyme label="c" type="f">seyngje</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Du ytste 'er fied-swiet <rhyme label="d" type="m">fen</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Dijn wolfert' sil naet <rhyme label="c" type="f" corresp="#s1:l5">eyngje</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Wol-sillig blieuwste <rhyme label="d" type="m" corresp="#s1:l6">den</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dijn wijv', dijn ljeave' <rhyme label="a" type="f">uwt-lezzen'</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dy wille fen dijn <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sil dy, az wijn-stoack', <rhyme label="a" type="f" corresp="#s2:l1">wezze</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Teel-sillig, ney <rhyme label="b" type="m" corresp="#s2:l2">bejeartt'</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Dijn bern'-teamm' schil him <rhyme label="c" type="f">schanssje</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ruwn om dy, great in <rhyme label="d" type="m">klien</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">In om dijn yt-disck' <rhyme label="c" type="f" corresp="#s2:l5">kranssje</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Az Olijve' altijd <rhyme label="d" type="m" corresp="#s2:l6">grien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sjea, sijcker! so sil <rhyme label="a" type="f">firdde</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Goads wol-feart', goed in <rhyme label="b" type="m">spoed</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ney him (Goads ljeav' in <rhyme label="a" type="f" corresp="#s3:l1">wirdde</rhyme>)</l>
        <l id="s3:l4" met="3" type="-+">Dy God frees't, yn 't <rhyme label="b" type="m" corresp="#s3:l2">gemoed</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Uwt Syon sil dy de' <rhyme label="c" type="f">HEERE</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Oer-schoddje', oer-mijld, mey' <rhyme label="d" type="m">seyn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Goads goe, sill'g Salems <rhyme label="c" type="f" corresp="#s3:l5">eere</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Begloerreste' oon jelds <rhyme label="d" type="m" corresp="#s3:l6">eyn'</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
