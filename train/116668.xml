<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>116668</id>
    <date>
      <year-start>1732</year-start>
      <year-end>1748</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Minzaam beeldje amoureus</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Heer! verschoont het <rhyme label="a" type="m">Nederlant</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En dat om uwen <rhyme label="b" type="m">Naem</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">U Toorn als een Oven <rhyme label="a" type="m" corresp="#s1:l1">brant</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wy hebben al te <rhyme label="b" type="m" corresp="#s1:l2">saem</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Lighaem:,,: en Ziel <rhyme label="c" type="f">bedorven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">'t Verstorven:,,: weder <rhyme label="d" type="m">wert</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">De <rhyme label="e" type="f">Grooten</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">Die <rhyme label="e" type="f" corresp="#s1:l7">stooten</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Ontblooten:,,: de <rhyme label="d" type="m" corresp="#s1:l6">smert</rhyme>,</l>
        <l id="s1:l10" met="1" type="-+">U <rhyme label="f" type="f">Namen</rhyme>,</l>
        <l id="s1:l11" met="1" type="-+"><rhyme label="f" type="f" corresp="#s1:l10">Beschamen</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Wy samen in 't <rhyme label="d" type="m" corresp="#s1:l6">hert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De beste middel die ik <rhyme label="a" type="m">weet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Om ons van straf <rhyme label="b" type="m">t'ontslaen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Is dat wy met een nieuwen <rhyme label="a" type="m" corresp="#s2:l1">Eedt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot Godt, door menig <rhyme label="b" type="m" corresp="#s2:l2">traen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Toegaen:,,: en ons <rhyme label="c" type="f">verbinden</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Wy vinden:,,: Godt <rhyme label="d" type="m">opregt</rhyme>,</l>
        <l id="s2:l7" met="1" type="-+">Wilt <rhyme label="e" type="f">ridden</rhyme>,</l>
        <l id="s2:l8" met="1" type="-+">In 't <rhyme label="e" type="f" corresp="#s2:l7">midden</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Door 't bidden:,,: U <rhyme label="d" type="m" corresp="#s2:l6">Knegt</rhyme>;</l>
        <l id="s2:l10" met="1" type="-+">Van <rhyme label="f" type="f">binnen</rhyme>,</l>
        <l id="s2:l11" met="1" type="-+">U <rhyme label="f" type="f" corresp="#s2:l10">minnen</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">En winnen 't <rhyme label="d" type="m" corresp="#s2:l6">Gevegt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laet ons soo met een Leeuwen <rhyme label="a" type="m">kragt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met hem te Velde <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dewijl het van de Hoge <rhyme label="a" type="m" corresp="#s3:l1">Magt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Voorheen niet is <rhyme label="b" type="m" corresp="#s3:l2">gedaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Laet staen:,,: U eygen <rhyme label="c" type="f">kragten</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Betragten:,,: goet voor <rhyme label="d" type="m">quaet</rhyme>,</l>
        <l id="s3:l7" met="1" type="-+">Door <rhyme label="e" type="f">rijden</rhyme>,</l>
        <l id="s3:l8" met="1" type="-+">En <rhyme label="e" type="f" corresp="#s3:l7">strijden</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Bevrijden:,,: den <rhyme label="d" type="m" corresp="#s3:l6">Staet</rhyme>,</l>
        <l id="s3:l10" met="1" type="-+">Niet <rhyme label="f" type="f">kijven</rhyme>,</l>
        <l id="s3:l11" met="1" type="-+">Als <rhyme label="f" type="f" corresp="#s3:l10">Wijven</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Maer blijven <rhyme label="d" type="m" corresp="#s3:l6">Soldaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Erkent den Staet als <rhyme label="a" type="m">Opper-Hooft</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Brengt Schatten in de <rhyme label="b" type="m">Kist</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Betaelt Soldaten en <rhyme label="a" type="m" corresp="#s4:l1">gelooft</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat gy dan niet en <rhyme label="b" type="m" corresp="#s4:l2">mist</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Laet Twist:,,: en Tweedragt <rhyme label="c" type="f">steeken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wilt Preken:,,: En <rhyme label="d" type="m">Gebe'en</rhyme>,</l>
        <l id="s4:l7" met="1" type="-+">Nooyt <rhyme label="e" type="f">staken</rhyme>,</l>
        <l id="s4:l8" met="1" type="-+">Maer <rhyme label="e" type="f" corresp="#s4:l7">maken</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Te raken:,,: als <rhyme label="d" type="m" corresp="#s4:l6">een</rhyme>,</l>
        <l id="s4:l10" met="1" type="-+">En <rhyme label="f" type="f">trouwen</rhyme>,</l>
        <l id="s4:l11" met="1" type="-+"><rhyme label="f" type="f" corresp="#s4:l10">Behouwen</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+">En bouwen 't <rhyme label="d" type="m" corresp="#s4:l6">Gemeen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Godt des Hemels en sijn <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De Geest van bey daer <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Aensiet ons uyt den Hoogen <rhyme label="a" type="m" corresp="#s5:l1">Throon</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En sal ons maken <rhyme label="b" type="m" corresp="#s5:l2">vry</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Soo wy:,,: Ons maer <rhyme label="c" type="f">bekeeren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Vermeeren:,,: Trouw en <rhyme label="d" type="m">Deugt</rhyme>,</l>
        <l id="s5:l7" met="1" type="-+">En <rhyme label="e" type="f">geven</rhyme>,</l>
        <l id="s5:l8" met="1" type="-+">Ons <rhyme label="e" type="f" corresp="#s5:l7">Leven</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Voorschreven:,,: Met <rhyme label="d" type="m" corresp="#s5:l6">vreugt</rhyme>,</l>
        <l id="s5:l10" met="1" type="-+">Godts <rhyme label="f" type="f">Tooren</rhyme>,</l>
        <l id="s5:l11" met="1" type="-+">Sal <rhyme label="f" type="f" corresp="#s5:l10">smooren</rhyme>,</l>
        <l id="s5:l12" met="2" type="-+">Als vooren ons <rhyme label="d" type="m" corresp="#s5:l6">heugt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
