  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VOOR alle menschen <rhyme label="a" type="f">kinders</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En is maer <rhyme label="b" type="m">eenderley</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ingangh, end' uytgangh <rhyme label="c" type="m">schier</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In dees Wereldt vol <rhyme label="a" type="f" corresp="#s1:l1">hinders</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Gheboorte met <rhyme label="b" type="m" corresp="#s1:l2">gheschrey</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Gheschiedt, soo wel <rhyme label="c" type="m" corresp="#s1:l3">Princier</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Als arme mensche <rhyme label="c" type="m" corresp="#s1:l3">hier</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Wy zijn al bloot <rhyme label="d" type="f">verschenen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Iae naeckt, ghelijck <rhyme label="c" type="m" corresp="#s1:l3">bijstier</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En naeckt moet ghy oock <rhyme label="d" type="f" corresp="#s1:l8">henen</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">O mensch ellendigh <rhyme label="c" type="m" corresp="#s1:l3">dier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Aldus dan de kindtsche <rhyme label="a" type="f">Iaren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Voorby zijn tot in 't <rhyme label="b" type="m">velt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van redelijck <rhyme label="c" type="m">verstandt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Twee weghen <rhyme label="a" type="f" corresp="#s2:l1">openbaren</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In desen keur <rhyme label="b" type="m" corresp="#s2:l2">gestelt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Al heel verscheyden, <rhyme label="c" type="m" corresp="#s2:l3">want</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Den wegh ten rechten <rhyme label="c" type="m" corresp="#s2:l3">kant</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hooren wy 't leven <rhyme label="d" type="f">noemen</rhyme>:</l>
        <l id="s2:l9" met="3" type="-+">Maer die ter <rhyme label="c" type="m" corresp="#s2:l3">slinckerhandt</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Die doodt is, of 't <rhyme label="d" type="f" corresp="#s2:l8">verdoemen</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Al schijnt hy seer <rhyme label="c" type="m" corresp="#s2:l3">playsant</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Elck doolt meest in 't <rhyme label="a" type="f">verkiesen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Want 'svleeschs verblinden <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En 'sWerelts loop <rhyme label="c" type="m">ghemeen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Goedt oordeel doen <rhyme label="a" type="f" corresp="#s3:l1">verliesen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dies weynigh gaender <rhyme label="b" type="m" corresp="#s3:l2">in</rhyme></l>
        <l id="s3:l6" met="3" type="-+">'sLevens wegh nauw en <rhyme label="c" type="m" corresp="#s3:l3">kleen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Iae van duysent nauw <rhyme label="c" type="m" corresp="#s3:l3">een</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Men moeter klimmen <rhyme label="d" type="f">steyligh</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">En selden tijdt oft <rhyme label="c" type="m" corresp="#s3:l3">gheen</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Is hy van vervolgh <rhyme label="d" type="f" corresp="#s3:l8">veyligh</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Maer veel tijdt vol <rhyme label="c" type="m" corresp="#s3:l3">gheween</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Lucas beschrijft na <rhyme label="a" type="f">desen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Datmen door veel <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Moet in Godts Rijcke <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maer in haer ydel <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">De Werelt vreught <rhyme label="b" type="m" corresp="#s4:l2">gheschiet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Hier in dees ruyme <rhyme label="c" type="m" corresp="#s4:l3">baen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dan 'tis so haest <rhyme label="c" type="m" corresp="#s4:l3">gedaen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">By dat eeuwigh sal <rhyme label="d" type="f">dueren</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Al leven <rhyme label="c" type="m" corresp="#s4:l3">onbelaen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">De sondaers sonder <rhyme label="d" type="f" corresp="#s4:l8">trueren</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Te niet worden sy <rhyme label="c" type="m" corresp="#s4:l3">saen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'T Heeft voormaels den <rhyme label="a" type="f">Propheten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Verwondert <rhyme label="b" type="m">menighfout</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hoe die Godtloos' <rhyme label="c" type="m">onvroet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die haer van Godt <rhyme label="a" type="f" corresp="#s5:l1">vermeten</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">End' hem temteren <rhyme label="b" type="m" corresp="#s5:l2">stout</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hebben soo veel <rhyme label="c" type="m" corresp="#s5:l3">voorspoet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Met pijp en harpen <rhyme label="c" type="m" corresp="#s5:l3">soet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Haer kinderen die <rhyme label="d" type="f">springhen</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Sy hebben <rhyme label="c" type="m" corresp="#s5:l3">overvloet</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Sy vreesen voor gheen <rhyme label="d" type="f" corresp="#s5:l8">dingen</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Ghelijck den vromen <rhyme label="c" type="m" corresp="#s5:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Godts roede, noch 't <rhyme label="a" type="f">kastijden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Sijnder handt, en is <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ter Werelt over <rhyme label="c" type="m">haer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">'Tvolck loopt aen allen <rhyme label="a" type="f" corresp="#s6:l1">zijden</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Haer toe, gelijck een <rhyme label="b" type="m" corresp="#s6:l2">vliet</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Van vloeyend' water <rhyme label="c" type="m" corresp="#s6:l3">klaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Doch wat sy spreecken <rhyme label="c" type="m" corresp="#s6:l3">maer</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Moet ghelden op der <rhyme label="d" type="f">Aerden</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Als oft gesproken <rhyme label="c" type="m" corresp="#s6:l3">waer</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Vanden Hemel van <rhyme label="d" type="f" corresp="#s6:l8">waerden</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Sy lasteren <rhyme label="c" type="m" corresp="#s6:l3">eenpaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Een licht-teecken <rhyme label="a" type="f">versmadigh</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Is in haer stout <rhyme label="b" type="m">geacht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Den vromen 'tis zijn <rhyme label="c" type="m">lot</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat hy ter werelt <rhyme label="a" type="f" corresp="#s7:l1">stadigh</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Veracht is, en <rhyme label="b" type="m" corresp="#s7:l2">belacht</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En gereeckent voor <rhyme label="c" type="m" corresp="#s7:l3">sot</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Sy segghen, wat sou <rhyme label="c" type="m" corresp="#s7:l3">Godt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Nae sulcke lieden <rhyme label="d" type="f">vraghen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Wat baet ons zijn <rhyme label="c" type="m" corresp="#s7:l3">gebodt</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Te houden met <rhyme label="d" type="f" corresp="#s7:l8">vertsagen</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">En drijven haren <rhyme label="c" type="m" corresp="#s7:l3">spot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu troosten <rhyme label="a" type="f">onderlingen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Inden Heere <rhyme label="b" type="m">gesterckt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Haer die Godt vreesen <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hoe dat Godt <rhyme label="a" type="f" corresp="#s8:l1">sonderlingen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Alles aensiet en <rhyme label="b" type="m" corresp="#s8:l2">merckt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat een <rhyme label="c" type="m" corresp="#s8:l3">gedenck-cedel</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Voor hem geschreven <rhyme label="c" type="m" corresp="#s8:l3">wel</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Is, die voor 's Heeren <rhyme label="d" type="f">Name</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Vreesen en zijn <rhyme label="c" type="m" corresp="#s8:l3">bevel</rhyme>,</l>
        <l id="s8:l10" met="3" type="-+">En die op hem <rhyme label="d" type="f" corresp="#s8:l8">bequame</rhyme></l>
        <l id="s8:l11" met="3" type="+-">Dencken met goedt <rhyme label="c" type="m" corresp="#s8:l3">opstel</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Vrienden, Godt sal <rhyme label="a" type="f">verthoonen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Ten laetsten daghe <rhyme label="b" type="m">dan</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wat voor een <rhyme label="c" type="m">onderscheydt</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat sal zijn in 't <rhyme label="a" type="f" corresp="#s9:l1">beloonen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Tusschen den vromen <rhyme label="b" type="m" corresp="#s9:l2">Man</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En Godtloosen <rhyme label="c" type="m" corresp="#s9:l3">bereydt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Iae so de Heere <rhyme label="c" type="m" corresp="#s9:l3">seydt</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Tusschen die hier seer <rhyme label="d" type="f">milde</rhyme></l>
        <l id="s9:l9" met="3" type="-+">Godt dient met <rhyme label="c" type="m" corresp="#s9:l3">neerstigheyt</rhyme>,</l>
        <l id="s9:l10" met="3" type="-+">En tusschen die niet <rhyme label="d" type="f" corresp="#s9:l8">wilde</rhyme></l>
        <l id="s9:l11" met="3" type="+-">Dienen zijn <rhyme label="c" type="m" corresp="#s9:l3">Majesteyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ick wil den vromen <rhyme label="a" type="f">sparen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">In mijnen dagh seer <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Maecken mijn <rhyme label="c" type="m">eyghendom</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Als oft mijn sonen <rhyme label="a" type="f" corresp="#s10:l1">waren</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En van de tweede <rhyme label="b" type="m" corresp="#s10:l2">doodt</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Haer vrijden sonder <rhyme label="c" type="m" corresp="#s10:l3">schrom</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Ick als de <rhyme label="c" type="m" corresp="#s10:l3">Bruydegom</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Sal haer als Bruydt <rhyme label="d" type="f">ontfangen</rhyme></l>
        <l id="s10:l9" met="3" type="-+">Mijn uytverkoren <rhyme label="c" type="m" corresp="#s10:l3">kom</rhyme>,</l>
        <l id="s10:l10" met="3" type="-+">Na u staet mijn <rhyme label="d" type="f" corresp="#s10:l8">verlanghen</rhyme>,</l>
        <l id="s10:l11" met="3" type="+-">Ghy zijt al mijnen <rhyme label="c" type="m" corresp="#s10:l3">rom</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Niet soo, niet soo, ghy <rhyme label="a" type="f">boose</rhyme></l>
        <l id="s11:l2" met="3" type="-+">'t Werdt niet met u <rhyme label="b" type="m">also</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Ten zy dat ghy staet <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Verachters en <rhyme label="a" type="f" corresp="#s11:l1">Godloose</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Ghy sult worden als <rhyme label="b" type="m" corresp="#s11:l2">stroo</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En buyten schuyr als <rhyme label="c" type="m" corresp="#s11:l3">kaf</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Verbrandt, met vyere <rhyme label="c" type="m" corresp="#s11:l3">straf</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">En tot een eeuwigh <rhyme label="d" type="f">sterven</rhyme></l>
        <l id="s11:l9" met="3" type="-+">Gheweckt werden uyt 't <rhyme label="c" type="m" corresp="#s11:l3">graf</rhyme>,</l>
        <l id="s11:l10" met="3" type="-+">En al ghemaeckt tot <rhyme label="d" type="f" corresp="#s11:l8">scherven</rhyme></l>
        <l id="s11:l11" met="3" type="+-">Met een yseren <rhyme label="c" type="m" corresp="#s11:l3">staf</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Coud' om te knerssen <rhyme label="a" type="f">tanden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Met lijden fel en <rhyme label="b" type="m">wreedt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Sal haer komen <rhyme label="c" type="m">aenboort</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Een vyer // een eeuwigh <rhyme label="a" type="f" corresp="#s12:l1">branden</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Tooren, verdriet en <rhyme label="b" type="m" corresp="#s12:l2">leedt</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Ia druck sonder <rhyme label="c" type="m" corresp="#s12:l3">confoort</rhyme></l>
        <l id="s12:l7" met="3" type="-+">En datmen noode <rhyme label="c" type="m" corresp="#s12:l3">hoort</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Eeuwigh gheween en <rhyme label="d" type="f">huylen</rhyme>,</l>
        <l id="s12:l9" met="3" type="-+">Valt berghen, werdt het <rhyme label="c" type="m" corresp="#s12:l3">woordt</rhyme>,</l>
        <l id="s12:l10" met="3" type="-+">Bedeckt ons, om te <rhyme label="d" type="f" corresp="#s12:l8">schuylen</rhyme></l>
        <l id="s12:l11" met="3" type="+-">Voor 'sLams aenschijn <rhyme label="c" type="m" corresp="#s12:l3">verstoort</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Eer, <rhyme label="a" type="f">onvergancklijckhede</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Sal terwijlen in <rhyme label="b" type="m">vreught</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Den oprechten <rhyme label="c" type="m">gheschien</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">In grooter eer en <rhyme label="a" type="f" corresp="#s13:l1">vrede</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Lachende seer <rhyme label="b" type="m" corresp="#s13:l2">verheught</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Want Godts aenschijn te <rhyme label="c" type="m" corresp="#s13:l3">sien</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Werdt hem een sulck <rhyme label="c" type="m" corresp="#s13:l3">verblien</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Gheen hert en mach 't <rhyme label="d" type="f">bedincken</rhyme>,</l>
        <l id="s13:l9" met="3" type="-+">Noch tongh en kan 't <rhyme label="c" type="m" corresp="#s13:l3">belien</rhyme>,</l>
        <l id="s13:l10" met="3" type="-+">Hy sal klaer sonnigh <rhyme label="d" type="f" corresp="#s13:l8">blincken</rhyme>,</l>
        <l id="s13:l11" met="3" type="+-">Godts Naem <rhyme label="c" type="m" corresp="#s13:l3">ghebenedien</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Nu sal terwijlen <rhyme label="a" type="f">moeten</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Den boosen <rhyme label="b" type="m">hopeloos</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">En oock sonder <rhyme label="c" type="m">quijtscheldt</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Eeuwigh sijn sonden <rhyme label="a" type="f" corresp="#s14:l1">boeten</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">En met d'Engelen <rhyme label="b" type="m" corresp="#s14:l2">boos</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Seer grouwelijck <rhyme label="c" type="m" corresp="#s14:l3">verselt</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">In duysterheyt <rhyme label="c" type="m" corresp="#s14:l3">ghequelt</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Van Godts aenschijn <rhyme label="d" type="f">verdreven</rhyme>,</l>
        <l id="s14:l9" met="3" type="-+">Ghelijck hy hem <rhyme label="c" type="m" corresp="#s14:l3">ghestelt</rhyme></l>
        <l id="s14:l10" met="3" type="-+">Hadt in een duyster <rhyme label="d" type="f" corresp="#s14:l8">leven</rhyme>,</l>
        <l id="s14:l11" met="3" type="+-">Alsoo werdt zijn <rhyme label="c" type="m" corresp="#s14:l3">verghelt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">'T Wert in aller <rhyme label="a" type="f">manieren</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Seer onghelijck een <rhyme label="b" type="m">dingh</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">In 't beloonen der <rhyme label="c" type="m">daedt</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Tusschen die hier nu <rhyme label="a" type="f" corresp="#s15:l1">vieren</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Godts wille <rhyme label="b" type="m" corresp="#s15:l2">sonderlingh</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">En die daer buyten <rhyme label="c" type="m" corresp="#s15:l3">gaet</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Soo van Lazaro <rhyme label="c" type="m" corresp="#s15:l3">staet</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Hy werdt getroost ten <rhyme label="d" type="f">fijne</rhyme>,</l>
        <l id="s15:l9" met="3" type="-+">Al was zijn deel hier <rhyme label="c" type="m" corresp="#s15:l3">quaet</rhyme>,</l>
        <l id="s15:l10" met="3" type="-+">Daer teghen quam in <rhyme label="d" type="f" corresp="#s15:l8">pijne</rhyme></l>
        <l id="s15:l11" met="3" type="+-">Den vrecken, <rhyme label="c" type="m" corresp="#s15:l3">delicaet</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Siet, aldus salt eens <rhyme label="a" type="f">werden</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Ghewisset al <rhyme label="b" type="m">gheheel</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Och vrienden laet ons <rhyme label="c" type="m">dan</rhyme></l>
        <l id="s16:l4" met="3" type="-+">In 'sHeeren Wet <rhyme label="a" type="f" corresp="#s16:l1">volherden</rhyme>.</l>
        <l id="s16:l5" met="3" type="-+">Al mach hier zijn ons <rhyme label="b" type="m" corresp="#s16:l2">deel</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Doodt // noodt, vervolgh, oft <rhyme label="c" type="m" corresp="#s16:l3">ban</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Want wie volherden <rhyme label="c" type="m" corresp="#s16:l3">kan</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Door die macht Gods, wert <rhyme label="d" type="f">saligh</rhyme>,</l>
        <l id="s16:l9" met="3" type="-+">Daer teghen den <rhyme label="c" type="m" corresp="#s16:l3">Tyran</rhyme></l>
        <l id="s16:l10" met="3" type="-+">Werdt Godts genade <rhyme label="d" type="f" corresp="#s16:l8">saligh</rhyme>,</l>
        <l id="s16:l11" met="3" type="+-">Als ongeluckigh <rhyme label="c" type="m" corresp="#s16:l3">Man</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Doch‘ter den mensch <rhyme label="a" type="f">gestadigh</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Wel op met vlijt, o <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Het waer hem goedt <rhyme label="c" type="m">ghewis</rhyme>:</l>
        <l id="s17:l4" met="3" type="-+">Gheeft, o Prince <rhyme label="a" type="f" corresp="#s17:l1">grootdadigh</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Dat wy in uwe <rhyme label="b" type="m" corresp="#s17:l2">Leer</rhyme></l>
        <l id="s17:l6" met="3" type="-+">Volherden mogen <rhyme label="c" type="m" corresp="#s17:l3">fris</rhyme>,</l>
        <l id="s17:l7" met="3" type="-+">Keert van ons al wat <rhyme label="c" type="m" corresp="#s17:l3">mis</rhyme></l>
        <l id="s17:l8" met="3" type="-+">De waerheyt komt <rhyme label="d" type="f">bestrijden</rhyme>,</l>
        <l id="s17:l9" met="3" type="-+">En sonder <rhyme label="c" type="m" corresp="#s17:l3">erghernis</rhyme>,</l>
        <l id="s17:l10" met="3" type="-+">Bewaert ons t'allen <rhyme label="d" type="f" corresp="#s17:l8">tijden</rhyme></l>
        <l id="s17:l11" met="3" type="+-">In een dat noodigh <rhyme label="c" type="m" corresp="#s17:l3">is</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
