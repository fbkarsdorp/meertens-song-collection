<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182570</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Zwart wil ik wel wezen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Myn lieffelyke <rhyme label="a" type="f">Roosje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wat aangenaame <rhyme label="b" type="m">geur</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Moog ik uw voor een <rhyme label="a" type="f" corresp="#s1:l1">poosje</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ontrooven van uw <rhyme label="b" type="m" corresp="#s1:l2">kleur</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wat zoet <rhyme label="c" type="m">aandoenlykheid</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wanneer het open <rhyme label="c" type="m" corresp="#s1:l5">splyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En ik uw Roosje <rhyme label="d" type="f">lukken</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Zo zoetjes myn <rhyme label="d" type="f" corresp="#s1:l7">verrukken</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Daar alles voor moet <rhyme label="d" type="f" corresp="#s1:l7">bukken</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Zo ouderdom als <rhyme label="e" type="m">jeugd</rhyme>,</l>
        <l id="s1:l11" met="2" type="-+">Wat vreugd, wat <rhyme label="e" type="m" corresp="#s1:l10">vreugd</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Die een teder hart <rhyme label="e" type="m" corresp="#s1:l10">verheugd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon dat de <rhyme label="a" type="f">Angelieren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ook lieflyk zyn van <rhyme label="b" type="m">lugt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat geeft doch geen <rhyme label="a" type="f" corresp="#s2:l1">plaizieren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Aan Venus <rhyme label="b" type="m" corresp="#s2:l2">Minnezugt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Geen schoonder Bachus <rhyme label="c" type="m">zoon</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die overtreft dat <rhyme label="c" type="m" corresp="#s2:l5">schoon</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Nog kan dat zoet niet <rhyme label="d" type="f">smaaken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Van aan die Roos te <rhyme label="d" type="f" corresp="#s2:l7">raaken</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Waar alles na moet <rhyme label="d" type="f" corresp="#s2:l7">haaken</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Om die aangenaame <rhyme label="e" type="m">geur</rhyme>,</l>
        <l id="s2:l11" met="2" type="-+">Een kleur, een <rhyme label="e" type="m" corresp="#s2:l10">kleur</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Daar straald een schoouheid <rhyme label="e" type="m" corresp="#s2:l10">deur</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De roode Roosen <rhyme label="a" type="f">blaaden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van binnen in dat <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met wit albast <rhyme label="a" type="f" corresp="#s3:l1">belaaden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met een gevlegte <rhyme label="b" type="m" corresp="#s3:l2">kroon</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ik aanschouw het op myn <rhyme label="c" type="m">kniÃ«n</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En kon van vreugd niet <rhyme label="c" type="m" corresp="#s3:l5">zien</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En niemand kwam my <rhyme label="d" type="f">moeijen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ik ging dien Roos <rhyme label="d" type="f" corresp="#s3:l7">besproeijen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Waardoor haar schoonheid <rhyme label="d" type="f" corresp="#s3:l7">bloeijen</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">En meerder Roosjes <rhyme label="e" type="m">teeld</rhyme>,</l>
        <l id="s3:l11" met="2" type="-+">Hoe streeld, hoe <rhyme label="e" type="m" corresp="#s3:l10">streeld</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">My dat aangenaame <rhyme label="e" type="m" corresp="#s3:l10">beeld</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
