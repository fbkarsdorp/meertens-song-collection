    <melody>Psalm 028 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOdt, sterckt’ en bolwerck van mijn <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Door diepen noodt en angst <rhyme label="a" type="f" corresp="#s1:l1">gedreven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Laet ick mijn klacht ten Hemel <rhyme label="b" type="f">stijgen</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Ach! laetse gunstigh’ ooren <rhyme label="b" type="f" corresp="#s1:l3">krijgen</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Ach! laet mijn droevigh <rhyme label="c" type="m">stem-getreur</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Niet kloppen voor een doove <rhyme label="c" type="m" corresp="#s1:l5">deur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want spreeckt ghy niet, door klare <rhyme label="a" type="f">daden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Tot redding uyt na-bye <rhyme label="a" type="f" corresp="#s2:l1">quaden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Soo sta ick, wanckelijck en <rhyme label="b" type="f">onveyl</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Op d’oever-kant van’t hoochste <rhyme label="b" type="f" corresp="#s2:l3">onheyl</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Niet ongelijck het koude <rhyme label="c" type="m">lijck</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Dat henen gaet na ’tHellen <rhyme label="c" type="m" corresp="#s2:l5">rijck</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat henen gaet, en, niets van <rhyme label="a" type="f">waerden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Een eygendom wordt van der <rhyme label="a" type="f" corresp="#s3:l1">aerden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Daerom verhoor het smeeck’lijck <rhyme label="b" type="f">karmen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dat ick met opgeheven <rhyme label="b" type="f" corresp="#s3:l3">armen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Laet komen voor de hooge <rhyme label="c" type="m">Door</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Van’t Godlijck drymael-heylich <rhyme label="c" type="m" corresp="#s3:l5">Choor</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Tel my niet met den <rhyme label="a" type="f">Goddeloosen</rhyme>.</l>
        <l id="s4:l2" met="4" type="-+">Laet my, als metgesel der <rhyme label="a" type="f" corresp="#s4:l1">boosen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Vw’ straffe ’tleven niet <rhyme label="b" type="f">ontrucken</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Als hen, wiens hert, vol boose <rhyme label="b" type="f" corresp="#s4:l3">stucken</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">De tonge, met een soet <rhyme label="c" type="m">gelaet</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Verleydt en vleydt door <rhyme label="c" type="m" corresp="#s4:l5">vreden-praet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Laet die den loon haers moetwils <rhyme label="a" type="f">smaken</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En ’tstoute quaedt sijn Heere <rhyme label="a" type="f" corresp="#s5:l1">raken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Laet die haers herten raedt selfs <rhyme label="b" type="f">hoonen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En ’twerck sijn dwasen meester <rhyme label="b" type="f" corresp="#s5:l3">loonen</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Laet die der wel-verschulder <rhyme label="c" type="m">pijn</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Rechtveerdelijck gëeygent <rhyme label="c" type="m" corresp="#s5:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Want sonder onderwijs <rhyme label="a" type="f">t’ontfangen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Vol-treden sy haer slimme <rhyme label="a" type="f" corresp="#s6:l1">gangen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Terwijlen sy, niet wel <rhyme label="b" type="f">beraden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Den luyster uwer hooger <rhyme label="b" type="f" corresp="#s6:l3">daden</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Domsinnighlijck geen acht en <rhyme label="c" type="m">slaen</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Noch oyt in’t herte laten <rhyme label="c" type="m" corresp="#s6:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daerom sal Godt sich aen haer <rhyme label="a" type="f">wreken</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Des levens draedt ontijdigh <rhyme label="a" type="f" corresp="#s7:l1">breken</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Haer zaedt na wensch niet laten <rhyme label="b" type="f">dijen</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Om voort te slaen tot lange <rhyme label="b" type="f" corresp="#s7:l3">rijen</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Loff Gode, die mijn <rhyme label="c" type="m">smeeck-gebedt</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Niet onverhoort heeft <rhyme label="c" type="m" corresp="#s7:l5">aff-geset</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Van Hem heb ick mijn moedt, mijn <rhyme label="a" type="f">krachten</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Mijn schut, mijn stut, mijn nut te <rhyme label="a" type="f" corresp="#s8:l1">wachten</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Ick was bela’en, en hy <rhyme label="b" type="f">verblijde</rhyme>:</l>
        <l id="s8:l4" met="4" type="-+">Met ramp om-vaen, en hy <rhyme label="b" type="f" corresp="#s8:l3">bevrijde</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Dies my de geest inwendigh <rhyme label="c" type="m">springt</rhyme></l>
        <l id="s8:l6" met="4" type="-+">En tot een vrolijck danck-liedt <rhyme label="c" type="m" corresp="#s8:l5">bringt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hy is, (getuyge zijn sijn <rhyme label="a" type="f">wercken</rhyme>)</l>
        <l id="s9:l2" met="4" type="-+">Hy is de sterckheydt aller <rhyme label="a" type="f" corresp="#s9:l1">stercken</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">Hy is’t die sijnen <rhyme label="b" type="f">uytverkoren</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">In nooden niet en laet <rhyme label="b" type="f" corresp="#s9:l3">verloren</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Maer hem met stercke handen <rhyme label="c" type="m">redt</rhyme></l>
        <l id="s9:l6" met="4" type="-+">En in behouden stande <rhyme label="c" type="m" corresp="#s9:l5">set</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Help, Heer, dijn volck. giet uyt een <rhyme label="a" type="f">regen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Van on-op-houdelijcken <rhyme label="a" type="f" corresp="#s10:l1">zegen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Op haer die ghy ten <rhyme label="b" type="f">eygendomme</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Verkoren hebt. Weest haer <rhyme label="b" type="f" corresp="#s10:l3">rondtsomme</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Geleydtse, weydtse, voertse <rhyme label="c" type="m">op</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Een eyndeloosen <rhyme label="c" type="m" corresp="#s10:l5">eeren-top</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
