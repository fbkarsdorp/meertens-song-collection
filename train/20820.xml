    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Windje daar het bos af drilt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SAncta Theresa! Spaensche <rhyme label="a" type="m">Maeghd</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Sancta Theresa! Spaensche <rhyme label="a" type="m" corresp="#s1:l1">Maeght</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Nae uw' lof mijn hartje <rhyme label="a" type="m" corresp="#s1:l1">jaeght</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">'t Wordt gedreven, om u <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Te verhaelen, en uw' <rhyme label="c" type="m">deughd</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die God Almachtigh heeft <rhyme label="c" type="m" corresp="#s1:l5">verheught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">G' hebt van uw' jonckheyd graegh <rhyme label="a" type="m">getracht</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">G' hebt van uw' jonckheyd graegh <rhyme label="a" type="m" corresp="#s2:l1">getracht</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Nae 't Martely dagh en <rhyme label="a" type="m" corresp="#s2:l1">nacht</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Ghy woudt sterven // Ghy woudt <rhyme label="b" type="f">derven</rhyme></l>
        <l id="s2:l5" met="4" type="+-">'t Jonge leven voor den <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ghy stelden hier in uwe <rhyme label="c" type="m" corresp="#s2:l5">eer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Sancta Theresa! Ghy <rhyme label="a" type="m">verkoos</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sancta Theresa! Ghy <rhyme label="a" type="m" corresp="#s3:l1">verkoos</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Tot uw' Moeder // 's Hemels <rhyme label="a" type="m" corresp="#s3:l1">Roos</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">'k Meen de Moeder, van ons' <rhyme label="b" type="f">Hoeder</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Dese wond're schoone <rhyme label="c" type="m">Maen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Naemt Ghy Therees, tot Moeder <rhyme label="c" type="m" corresp="#s3:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Op dese alderwaerdste <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Op dese alderwaerdste <rhyme label="a" type="m" corresp="#s4:l1">Vrouw</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Stelden Ghy all' u <rhyme label="a" type="m" corresp="#s4:l1">betrouw</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Jae door dese // (soo wy <rhyme label="b" type="f">lesen</rhyme>)</l>
        <l id="s4:l5" met="4" type="+-">Hebt Ghy g'wonnen Satans <rhyme label="c" type="m">macht</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Want Sy verkreegh U dese <rhyme label="c" type="m" corresp="#s4:l5">kracht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">O! dese Maeght heeft U <rhyme label="a" type="m">gebrocht</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">O! dese Maeght heeft U <rhyme label="a" type="m" corresp="#s5:l1">gebrocht</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Tot dien Staet die Ghy <rhyme label="a" type="m" corresp="#s5:l1">versocht</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Sy U leyden // tot de <rhyme label="b" type="f">weyden</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Van een Kloorsterlijck <rhyme label="c" type="m">gebiedt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Sy U, Theresa, haer toe <rhyme label="c" type="m" corresp="#s5:l5">riedt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Als Ghy van troosting was <rhyme label="a" type="m">ontbloot</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Als Ghy van troostingh was <rhyme label="a" type="m" corresp="#s6:l1">ontbloot</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Ghy door Haer die we'er <rhyme label="a" type="m" corresp="#s6:l1">genoot</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">G' hadt uw' sinnen // G' hadt uw' <rhyme label="b" type="f">minne</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Op die Maget vast <rhyme label="c" type="m">geset</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Die was ontfangen sonder <rhyme label="c" type="m" corresp="#s6:l5">smet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">U, Godes Moeder troosten <rhyme label="a" type="m">quam</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">U, Godes Moeder troosten <rhyme label="a" type="m" corresp="#s7:l1">quam</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Met haer soet onnoosel <rhyme label="a" type="m" corresp="#s7:l1">Lam</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Godes Engel // (niet den <rhyme label="b" type="f">bengel</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Cupid') heeft u hert <rhyme label="c" type="m">gewondt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Die Jesvs Christus tot U <rhyme label="c" type="m" corresp="#s7:l5">sondt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">G' hadt uwe liefde vast <rhyme label="a" type="m">gestelt</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">G' Had uwe liefde vast <rhyme label="a" type="m" corresp="#s8:l1">gestelt</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Op dien aldervroomsten <rhyme label="a" type="m" corresp="#s8:l1">Held</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Op den Hoeder // op den <rhyme label="b" type="f">Voeder</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Van Mariaes waerdsten <rhyme label="c" type="m">Soon</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Op Sint Josephvs uw' <rhyme label="c" type="m" corresp="#s8:l5">Patroon</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Ghy zijt geweest by Hem soo <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Ghy zijt geweest by Hem soo <rhyme label="a" type="m" corresp="#s9:l1">groot</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Dat Hy U in geene <rhyme label="a" type="m" corresp="#s9:l1">nood</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Oyt liet blijven // maer <rhyme label="b" type="f">gerijven</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Quam Hy U ten allen <rhyme label="c" type="m">tijd</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Soo Ghy Theresa selfs <rhyme label="c" type="m" corresp="#s9:l5">belijdt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Dees met Maria sijne <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Dees met Maria sijne <rhyme label="a" type="m" corresp="#s10:l1">Vrouw</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Brochten U in 't hoogh <rhyme label="a" type="m" corresp="#s10:l1">gebouw</rhyme>;</l>
        <l id="s10:l4" met="4" type="+-">'k Meen hier boven // waer dat <rhyme label="b" type="f">loven</rhyme></l>
        <l id="s10:l5" met="4" type="+-">All' de Eng'len God den <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Waer Ghy gehouden wordt in <rhyme label="c" type="m" corresp="#s10:l5">eer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Och of ick oock in 's Hemels <rhyme label="a" type="m">hof</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Och of ick oock in 's Hemels <rhyme label="a" type="m" corresp="#s11:l1">hof</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Aen mocht hooren Godes <rhyme label="a" type="m" corresp="#s11:l1">lof</rhyme>!</l>
        <l id="s11:l4" met="4" type="+-">Wilt verwerven // dat ick <rhyme label="b" type="f">erven</rhyme></l>
        <l id="s11:l5" met="4" type="+-">Mach, Theresa! dit <rhyme label="c" type="m">geluck</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Dat ick ontga den helschen <rhyme label="c" type="m" corresp="#s11:l5">druck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Wilt met Maria spannen <rhyme label="a" type="m">t'saem</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Wilt met Maria spannen <rhyme label="a" type="m" corresp="#s12:l1">t'saem</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">'t Sal my wesen <rhyme label="a" type="m" corresp="#s12:l1">aengenaem</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">'k Sal dan springen // 'k sal dan <rhyme label="b" type="f">singen</rhyme></l>
        <l id="s12:l5" met="4" type="+-">Godes lof met bly <rhyme label="c" type="m">gemoed</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Als ick sal erven 't <rhyme label="c" type="m" corresp="#s12:l5">Opper-goed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
