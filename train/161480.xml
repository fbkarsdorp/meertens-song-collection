<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161480</id>
    <date>
      <year-start>1612</year-start>
      <year-end>1612</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Rijs uit de slaap nu niet en vaak</melody>
  <author>J. Jacobsz. Harlingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk roep tot u o Heere <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Al met ootmoet // in mijnen <rhyme label="a" type="m" corresp="#s1:l1">noet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Helpt my o God al metter <rhyme label="a" type="m" corresp="#s1:l1">spoet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt over my <rhyme label="b" type="f">ontfermen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sterckt mijnen gheest en mijn <rhyme label="a" type="m" corresp="#s1:l1">ghemoedt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Mijn vleesch en bloet // ter neder <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">En wilt my helpen Heere <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Tot u is al mijn <rhyme label="b" type="f" corresp="#s1:l4">kermen</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Toont mijn ghenade <rhyme label="b" type="f" corresp="#s1:l4">ochermen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Bedroeft ben ick in mijnen <rhyme label="a" type="m">gheest</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En seer bevreest // in dit <rhyme label="a" type="m" corresp="#s2:l1">foreest</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">O Heere my ghenadich <rhyme label="a" type="m" corresp="#s2:l1">weest</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat ick niet heel <rhyme label="b" type="f">verflouwe</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Als ick mijn aenvanck <rhyme label="a" type="m" corresp="#s2:l1">eest</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Hoese is gheweest // verstaet den <rhyme label="a" type="m" corresp="#s2:l1">keest</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Seer lieffelijck heel <rhyme label="a" type="m" corresp="#s2:l1">onbeteest</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Nu isset groote <rhyme label="b" type="f" corresp="#s2:l4">rouwe</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Helpt my o God <rhyme label="b" type="f" corresp="#s2:l4">ghetrouwe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Isser nu gheen ghenade <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">O lieve Heer // die leyt ter <rhyme label="a" type="m" corresp="#s3:l1">neer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sult ghy nu helpen <rhyme label="a" type="m" corresp="#s3:l1">nimmermeer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sult ghy altijt <rhyme label="b" type="f">verstooten</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">O God neemt ghy gheheel een <rhyme label="a" type="m" corresp="#s3:l1">keer</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En wilt niet weer // my helpen <rhyme label="a" type="m" corresp="#s3:l1">teer</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Is u barmherticheyt so <rhyme label="a" type="m" corresp="#s3:l1">seer</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Met toorne <rhyme label="b" type="f" corresp="#s3:l4">overgoten</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">En gantschelijck <rhyme label="b" type="f" corresp="#s3:l4">besloten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer doch spreeck ick met David <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Van dit verdriet // dat my <rhyme label="a" type="m" corresp="#s4:l1">gheschiet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ick moet het lijden vaet dit <rhyme label="a" type="m" corresp="#s4:l1">bediet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gods rechter hant cant <rhyme label="b" type="f">wenden</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ick denck voort watter is <rhyme label="a" type="m" corresp="#s4:l1">gheschiet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Door u ghebiedt // sonder u <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme></l>
        <l id="s4:l7" met="4" type="-+">V volck dat ernstich tot u <rhyme label="a" type="m" corresp="#s4:l1">riep</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Ginckt ghy ghenade <rhyme label="b" type="f" corresp="#s4:l4">senden</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">En hielptse uyt <rhyme label="b" type="f" corresp="#s4:l4">ellenden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghelijck oock Achior naeckt <rhyme label="a" type="m">verclaert</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Hoe dat ghy spaert // en hebt <rhyme label="a" type="m" corresp="#s5:l1">bewaert</rhyme></l>
        <l id="s5:l3" met="4" type="-+">V volck en haer aenslaghen, <rhyme label="a" type="m" corresp="#s5:l1">daert</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Na uwen wil ginck <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Maer alst verliet Heer uwen <rhyme label="a" type="m" corresp="#s5:l1">aert</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Hebt ghyt beswaert // en seer <rhyme label="a" type="m" corresp="#s5:l1">vervaert</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Alst dan berou van herten <rhyme label="a" type="m" corresp="#s5:l1">baert</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Om na u wille te <rhyme label="b" type="f" corresp="#s5:l4">streven</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Hebt ghy victori <rhyme label="b" type="f" corresp="#s5:l4">ghegheven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Iudith sprack oock met claer <rhyme label="a" type="m">bescheyt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Van lijdtsaemheyt // u toch <rhyme label="a" type="m" corresp="#s6:l1">bereyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Treet totten Heer weent ende <rhyme label="a" type="m" corresp="#s6:l1">schreyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wilt hem gheen tijt uyt <rhyme label="b" type="f">lesen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Want dat ons nu gheschiet dit <rhyme label="a" type="m" corresp="#s6:l1">leyt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Ons sonden breyt // t'zy u <rhyme label="a" type="m" corresp="#s6:l1">gheseyt</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Die hebben ons dit op <rhyme label="a" type="m" corresp="#s6:l1">gheleyt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Laet ons verduldich <rhyme label="b" type="f" corresp="#s6:l4">wesen</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">T'sal zijn tot betering <rhyme label="b" type="f" corresp="#s6:l4">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Isaack Iacob Moyses <rhyme label="a" type="m">naer</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En allegaer // die God lief <rhyme label="a" type="m" corresp="#s7:l1">waer</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Hebben veel droeffenisse <rhyme label="a" type="m" corresp="#s7:l1">swaer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Met lijdtsaemheyt <rhyme label="b" type="f">ghebroken</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Maer die de vreese Gods seer <rhyme label="a" type="m" corresp="#s7:l1">claer</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Verliet van haer // murmureerden daer, <rhyme label="a" type="m" corresp="#s7:l1">Exo.16,3</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Teghen God ons Hemelsche <rhyme label="a" type="m" corresp="#s7:l1">Vaer</rhyme></l>
        <l id="s7:l8" met="3" type="-+">En andere Goden <rhyme label="b" type="f" corresp="#s7:l4">roocken</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">Aen dien heeft hy hem <rhyme label="b" type="f" corresp="#s7:l4">ghewroocken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Oock leefde Iob in sware <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Naet dit bediet // wat hem <rhyme label="a" type="m" corresp="#s8:l1">gheschiet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">So lastert hy den Heere <rhyme label="a" type="m" corresp="#s8:l1">niet</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Maer is verduldich <rhyme label="b" type="f">bleven</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Tobyas hem daer niet aen <rhyme label="a" type="m" corresp="#s8:l1">stiet</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Niet af en liet // maer tot God <rhyme label="a" type="m" corresp="#s8:l1">vliet</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Dit ist dat hem die Heere <rhyme label="a" type="m" corresp="#s8:l1">onthiet</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Ick sal u gheringhe <rhyme label="b" type="f" corresp="#s8:l4">gheven</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">En sy zijn beyde <rhyme label="b" type="f" corresp="#s8:l4">verheven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Een ootmoedich mensch seer <rhyme label="a" type="m">sacht</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Den tijt verwacht // hoewel met <rhyme label="a" type="m" corresp="#s9:l1">cracht</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Sijn saeck verdruckt wort en <rhyme label="a" type="m" corresp="#s9:l1">veracht</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Sullent die vrome <rhyme label="b" type="f">prijsen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Oock een verduldich gheest <rhyme label="a" type="m" corresp="#s9:l1">bedacht</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Hier merck op slacht // is bet <rhyme label="a" type="m" corresp="#s9:l1">gheacht</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Dan hooghe gheesten sonder <rhyme label="a" type="m" corresp="#s9:l1">macht</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Laet lijdtsaemheyt nu <rhyme label="b" type="f" corresp="#s9:l4">rijsen</rhyme></l>
        <l id="s9:l9" met="3" type="+-">In uwen druck als <rhyme label="b" type="f" corresp="#s9:l4">wijsen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Want druck die brengt lijdtsaemheyt <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Luydt Paulus woort // vaert hier in <rhyme label="a" type="m" corresp="#s10:l1">voort</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Lijdtsaemheyt proeving <rhyme label="a" type="m" corresp="#s10:l1">onghestoort</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Proevinghe brengt ons <rhyme label="b" type="f">hope</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Die hope hout ons met goet <rhyme label="a" type="m" corresp="#s10:l1">accoort</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Dat niemant wort, beschaemt <rhyme label="a" type="m" corresp="#s10:l1">versmort</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Zijt vrolijck in der hope <rhyme label="a" type="m" corresp="#s10:l1">ghegort</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">En wilt doch niet <rhyme label="b" type="f" corresp="#s10:l4">verlope</rhyme></l>
        <l id="s10:l9" met="3" type="+-">Ghy soudet dier <rhyme label="b" type="f" corresp="#s10:l4">becoope</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Haddet o Heer u woort <rhyme label="a" type="m">ghedaen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Ick was vergaen // help my nu <rhyme label="a" type="m" corresp="#s11:l1">raen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Dat ick daer in mach blijven <rhyme label="a" type="m" corresp="#s11:l1">staen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">V woorden die zijn <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Die sonden lieve Heere <rhyme label="a" type="m" corresp="#s11:l1">saen</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Van my misdaen // wilt niet so <rhyme label="a" type="m" corresp="#s11:l1">slaen</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">IO Heer laet my niet <rhyme label="a" type="m" corresp="#s11:l1">vergaen</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Maer wilt een uytcomst <rhyme label="b" type="f" corresp="#s11:l4">gheven</rhyme>,</l>
        <l id="s11:l9" met="3" type="+-">En my weder <rhyme label="b" type="f" corresp="#s11:l4">verheven</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">O Heere wie is uws <rhyme label="a" type="m">ghelijck</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Want uwe rijck // duert <rhyme label="a" type="m" corresp="#s12:l1">eeuwelijck</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Ghy castijt ons en troost <rhyme label="a" type="m" corresp="#s12:l1">ghelijck</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Ghy condt ter Hellen <rhyme label="b" type="f">werpen</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">En oock verlossen van s'duyvels <rhyme label="a" type="m" corresp="#s12:l1">strijck</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Sonder beswijck // en neemt gheen <rhyme label="a" type="m" corresp="#s12:l1">wijck</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Ick ben doch niet dan aerd en <rhyme label="a" type="m" corresp="#s12:l1">slijck</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">O Heere wilt my <rhyme label="b" type="f" corresp="#s12:l4">stercken</rhyme></l>
        <l id="s12:l9" met="3" type="+-">Na u wil te <rhyme label="b" type="f" corresp="#s12:l4">wercken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">O Heer ghy wetet mijn <rhyme label="a" type="m">verlang</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">My is so bang // sterckt mijnen <rhyme label="a" type="m" corresp="#s13:l1">gang</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Hoe duert dees swaricheyt so <rhyme label="a" type="m" corresp="#s13:l1">lang</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Dat ick aldus moet <rhyme label="b" type="f">truere</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Helpt my uyt desen noot seer <rhyme label="a" type="m" corresp="#s13:l1">strang</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Wt dit bedwang // ick ben seer <rhyme label="a" type="m" corresp="#s13:l1">swang</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">Dat my die becoringhe niet <rhyme label="a" type="m" corresp="#s13:l1">bevang</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Helpt God dat ick <rhyme label="b" type="f" corresp="#s13:l4">uytvuere</rhyme></l>
        <l id="s13:l9" met="3" type="+-">Dat ick niet quaets <rhyme label="b" type="f" corresp="#s13:l4">aenruere</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Die dit sanck was in swaer <rhyme label="a" type="m">torment</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Almen bekent // tot God nu <rhyme label="a" type="m" corresp="#s14:l1">wendt</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">En bidt die Heer dat hy hem <rhyme label="a" type="m" corresp="#s14:l1">sendt</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Sijn gheest en hem <rhyme label="b" type="f">vermaecke</rhyme>,</l>
        <l id="s14:l5" met="4" type="-+">Hier mede heeft dit liedt een <rhyme label="a" type="m" corresp="#s14:l1">endt</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">O Heere jent // weest hem <rhyme label="a" type="m" corresp="#s14:l1">ontrent</rhyme>,</l>
        <l id="s14:l7" met="4" type="-+">Dat hem niet schade t'Helsche <rhyme label="a" type="m" corresp="#s14:l1">serpent</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Laet hem u goetheyt <rhyme label="b" type="f" corresp="#s14:l4">smaecke</rhyme></l>
        <l id="s14:l9" met="3" type="+-">Laet ons niet quaets <rhyme label="b" type="f" corresp="#s14:l4">ghenaecke</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
