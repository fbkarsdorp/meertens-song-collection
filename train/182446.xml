<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182446</id>
    <date>
      <year-start>1742</year-start>
      <year-end>1742</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatstmaal ging ik ter jacht</melody>
  <author>Tannetge Kornelis Blok</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als gy met u <rhyme label="a" type="m">Gedigt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">my gunstig quamt <rhyme label="b" type="f">vereeren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Doen vond in my <rhyme label="a" type="m" corresp="#s1:l1">verpligt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">U weder toe te <rhyme label="b" type="f" corresp="#s1:l2">keeren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Een teeken dat ik dankbaer <rhyme label="c" type="m">ben</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Schoon dank daer mijn <rhyme label="c" type="m" corresp="#s1:l5">pen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Niet toe bequaem en <rhyme label="c" type="m" corresp="#s1:l5">ken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer dog u goede <rhyme label="a" type="m">gunst</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Komt my vrymoedig <rhyme label="b" type="f">maken</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">U liefde tot de <rhyme label="a" type="m" corresp="#s2:l1">kunst</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Toond gy in dese <rhyme label="b" type="f" corresp="#s2:l2">saken</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">U soete en aengenaem <rhyme label="c" type="m">Gedigt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Heeft mijn zwakken geest <rhyme label="c" type="m" corresp="#s2:l5">verligt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">‘t Maekt aen u <rhyme label="c" type="m" corresp="#s2:l5">verpligt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gy kont my weerde <rhyme label="a" type="m">vrient</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Geen liever middel <rhyme label="b" type="f">plegen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met zank ben ik <rhyme label="a" type="m" corresp="#s3:l1">gedient</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tot rijm ben ik <rhyme label="b" type="f" corresp="#s3:l2">genegen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">In ‘t lesen van een goede <rhyme label="c" type="m">saek</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Of een Liedje tot <rhyme label="c" type="m" corresp="#s3:l5">vermaek</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Te zingen vind ik <rhyme label="c" type="m" corresp="#s3:l5">smaek</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">En blust den geest niet <rhyme label="a" type="m">uyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gaet ons Gods woord <rhyme label="b" type="f">getuyge</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gy kont geen teere <rhyme label="a" type="m" corresp="#s4:l1">Spruyt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Door sagter middel <rhyme label="b" type="f" corresp="#s4:l2">buygen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">My als zwakke Wijngaert <rhyme label="c" type="m">rank</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Leyt en queekt gy door <rhyme label="c" type="m" corresp="#s4:l5">gesank</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">dus vrient, weet ik u <rhyme label="c" type="m" corresp="#s4:l5">dank</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">‘t Is my een stemm’ <rhyme label="a" type="m">bequaem</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Der Tortelduyfjes <rhyme label="b" type="f">soete</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">die my dus <rhyme label="a" type="m" corresp="#s5:l1">aengenaem</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En gunstig komt <rhyme label="b" type="f" corresp="#s5:l2">begroete</rhyme>?</l>
        <l id="s5:l5" met="4" type="-+">In dese aengename <rhyme label="c" type="m">tijd</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Nu wy zijn tot ons <rhyme label="c" type="m" corresp="#s5:l5">jolijt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Den treurig Winter <rhyme label="c" type="m" corresp="#s5:l5">quyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nu na Salomons <rhyme label="a" type="m">woord</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">De Mey-tijd is voor <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En dat de Bloemtjes <rhyme label="a" type="m" corresp="#s6:l1">voort</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Komen in onse <rhyme label="b" type="f" corresp="#s6:l2">Landen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">De Vogeltjes en meenig <rhyme label="c" type="m">Dier</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Loven nu op haer <rhyme label="c" type="m" corresp="#s6:l5">manier</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Den Schepper <rhyme label="c" type="m" corresp="#s6:l5">goedertier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">God wil mijn dwase <rhyme label="a" type="m">jeugt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Voortaen ook so <rhyme label="b" type="f">geleyden</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Dat ik toeneem in <rhyme label="a" type="m" corresp="#s7:l1">deugt</rhyme>?</l>
        <l id="s7:l4" met="3" type="-+">en zijnen lof <rhyme label="b" type="f" corresp="#s7:l2">verbreyden</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Het sy met werken of <rhyme label="c" type="m">gesank</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Wat ik doe mijn leven <rhyme label="c" type="m" corresp="#s7:l5">lank</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Mag sijn tot Godes <rhyme label="c" type="m" corresp="#s7:l5">dank</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Als ik gedenk u <rhyme label="a" type="m">vreugt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Als gy my gink <rhyme label="b" type="f">verwagten</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Soo ben ik gantsch <rhyme label="a" type="m" corresp="#s8:l1">verheugt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">en seg in mijn <rhyme label="b" type="f" corresp="#s8:l2">gedagten</rhyme>?</l>
        <l id="s8:l5" met="4" type="-+">Gelijk hier in verwondere <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Met David, wat ben ik <rhyme label="c" type="m" corresp="#s8:l5">Heer</rhyme>!</l>
        <l id="s8:l7" met="3" type="-+">Dat my geschied dees <rhyme label="c" type="m" corresp="#s8:l5">eer</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ik heb wel in <rhyme label="a" type="m">Holland</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Geweest om te <rhyme label="b" type="f">playzieren</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Maer nooyt in <rhyme label="a" type="m" corresp="#s9:l1">Braband</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Of de Vlaamsche <rhyme label="b" type="f" corresp="#s9:l2">Quartieren</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Het was mijn een groot <rhyme label="c" type="m">verbien</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">De Landen ook eens te <rhyme label="c" type="m" corresp="#s9:l5">zien</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Maer ‘t kon niet wel <rhyme label="c" type="m" corresp="#s9:l5">geschien</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">En daer en leyd niet <rhyme label="a" type="m">an</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Schoon ik se niet <rhyme label="b" type="f">aenschouwen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">In ‘t Hemelse <rhyme label="a" type="m" corresp="#s10:l1">Canan</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Is mijn hoop en <rhyme label="b" type="f" corresp="#s10:l2">betrouwen</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Dat wy zullen met groot <rhyme label="c" type="m">verblien</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Heel heerlijker dingen <rhyme label="c" type="m" corresp="#s10:l5">zien</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Als eenige van <rhyme label="c" type="m" corresp="#s10:l5">dien</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die Stad van God <rhyme label="a" type="m">gevont</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">‘t Ierusalem daer <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Wiens straten zijn fijn <rhyme label="a" type="m" corresp="#s11:l1">gout</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Daer d’Engelen God <rhyme label="b" type="f" corresp="#s11:l2">loven</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">God die het heeft geschapen <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Wat heerlyk op ‘t aerdsche <rhyme label="c" type="m" corresp="#s11:l5">dal</rhyme>:</l>
        <l id="s11:l7" met="3" type="-+">Was, is of wesen <rhyme label="c" type="m" corresp="#s11:l5">zal</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Want schoon dat al de <rhyme label="a" type="m">pragt</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Van al des Werelds <rhyme label="b" type="f">ryken</rhyme>?</l>
        <l id="s12:l3" met="3" type="-+">Te samen was <rhyme label="a" type="m" corresp="#s12:l1">gebragt</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">‘t Moet voor den Hemel <rhyme label="b" type="f" corresp="#s12:l2">wyken</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Ia Thebis en <rhyme label="c" type="m">Alexandrien</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Roomen, Babel boven <rhyme label="c" type="m" corresp="#s12:l5">dien</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Nog wat hier is te <rhyme label="c" type="m" corresp="#s12:l5">zien</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Nu Vader zijt <rhyme label="a" type="m">gegroet</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Ik dank u meenig <rhyme label="b" type="f">werven</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">‘k Wensch u een vroom <rhyme label="a" type="m" corresp="#s13:l1">gemoet</rhyme>:</l>
        <l id="s13:l4" met="3" type="-+">En ook een zalig <rhyme label="b" type="f" corresp="#s13:l2">sterven</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">En dat wy namaels tot een <rhyme label="c" type="m">loon</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">En Gods genaed’ in ’s Hemels <rhyme label="c" type="m" corresp="#s13:l5">Throon</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Verkrijgen ’s levens <rhyme label="c" type="m" corresp="#s13:l5">kroon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
