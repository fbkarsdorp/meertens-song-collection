<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>17191</id>
    <date>
      <year-start>1903</year-start>
      <year-end>1908</year-end>
    </date>
    <tags/>
    <melody>Nicolai solemnia</melody>
  <author>Florimond van Duyse</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Jhesus Cristus, Marien soen,</l>
        <l id="s1:l2">verlient die leven wel te doen;</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Die syn versceiden van eertriic</l>
        <l id="s2:l2">kust vruechd bi u in hemmelriic,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">End vree die heilich kerc ghemeen</l>
        <l id="s3:l2">end al die kersten onder een,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1">End als ons tiit hier is gedaen,</l>
        <l id="s4:l2">die ewich croen van u tonfaen. Amen.</l>
      </lg>
    </lg>
  </text>
</song>
