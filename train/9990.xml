    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Les Feuillantines</melody>
  <author>Isaak A. van Vaerlen en Jan Jansz. van Asten</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">QUelt my niet meer velt <rhyme label="a" type="m">Godin</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Laet u <rhyme label="a" type="m" corresp="#s1:l1">sin</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Strecken tot de soete <rhyme label="a" type="m" corresp="#s1:l1">Min</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Soo sal ick u Schaepjes <rhyme label="b" type="f">drijven</rhyme>,</l>
        <l id="s1:l5" met="6" type="-+">En altijdt, en altijdt, u Dienaer <rhyme label="b" type="f" corresp="#s1:l4">blijven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schoon ben ick gheen Boere <rhyme label="a" type="m">Knecht</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Alsje <rhyme label="a" type="m" corresp="#s2:l1">seght</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daerom ben ick ook niet <rhyme label="a" type="m" corresp="#s2:l1">slecht</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Want ick weet al haer <rhyme label="b" type="f">manieren</rhyme></l>
        <l id="s2:l5" met="6" type="-+">Om het Vee, om het Vee, Seer wel 't <rhyme label="b" type="f" corresp="#s2:l4">hantieren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Voorts sel ick jou alle <rhyme label="a" type="m">daegh</rhyme>.</l>
        <l id="s3:l2" met="2" type="+-">Helpen <rhyme label="a" type="m" corresp="#s3:l1">graegh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Op dat ick jou noyt <rhyme label="a" type="m" corresp="#s3:l1">mishaegh</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En my neder voor u <rhyme label="b" type="f">buygen</rhyme>,</l>
        <l id="s3:l5" met="6" type="-+">Om mijn trou, om mijn trouheyt te <rhyme label="b" type="f" corresp="#s3:l4">getuygen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wel wat segje schoon <rhyme label="a" type="m">Jonckvrou</rhyme>?</l>
        <l id="s4:l2" met="2" type="+-">Wilje <rhyme label="a" type="m" corresp="#s4:l1">nou</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Siet soo krijgje strackx mijn <rhyme label="a" type="m" corresp="#s4:l1">Trouw</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En ick sel mijn voort <rhyme label="b" type="f">begheven</rhyme></l>
        <l id="s4:l5" met="6" type="-+">Om in vreught en in deught, By u te <rhyme label="b" type="f" corresp="#s4:l4">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
