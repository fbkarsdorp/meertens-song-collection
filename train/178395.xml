  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Onsen Meyboom staet in’t <rhyme label="a" type="f">groen</rhyme>:</l>
        <l id="s1:l2" met="3" type="+-">Tot een yeders <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Siet hy groeyt, siet hy <rhyme label="c" type="m">bloeyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Met Christus dierbaer <rhyme label="d" type="m">bloet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Is hy <rhyme label="d" type="m" corresp="#s1:l4">besproet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Laet ons dese vruchten <rhyme label="a" type="f">plucken</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Met ons herten <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Nieuwe jeucht, nieuwe <rhyme label="c" type="m">vreucht</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Sullen sy geven <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Naer ons <rhyme label="d" type="m" corresp="#s2:l4">ghemoet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Weest o sone Godts <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Deyr u cruys <rhyme label="b" type="m">ghegroet</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Iesu soet, Iesu <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ghy zijt ons <rhyme label="d" type="m">salicheydt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Ons <rhyme label="d" type="m" corresp="#s3:l4">soeticheydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ghy zijt die vreucht <rhyme label="a" type="f">uytghelesen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Die ons ‘tleven <rhyme label="b" type="m">gheeft</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Deur u leeft // al dat <rhyme label="c" type="m">heeft</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Gheleghen in doodts <rhyme label="d" type="m">noot</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Der sonden <rhyme label="d" type="m" corresp="#s4:l4">groot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Den sondaer hebt ghy <rhyme label="a" type="f">ghenesen</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Deur u wonden <rhyme label="b" type="m">swaer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">O minnaer // u <rhyme label="c" type="m">eenpaer</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Moet loven het <rhyme label="d" type="m">aerdtrijck</rhyme></l>
        <l id="s5:l5" met="2" type="-+">En’t <rhyme label="d" type="m" corresp="#s5:l4">Hemelrijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
