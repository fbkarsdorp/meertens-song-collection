  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">
          <rhyme label="a" type="m">Voedsteress'</rhyme>
        </l>
        <l id="s1:l2" met="3" type="-+">Der soetste <rhyme label="b" type="f">Minne-vlaeghjes</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daelt uyt uwen Hemel <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Stroyt Zege-bloemtjes <rhyme label="d" type="m">neer</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Ay! doet ons <rhyme label="d" type="m" corresp="#s1:l4">d'eer</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">O Liefde! komt heylight dit Paer met u <rhyme label="c" type="m" corresp="#s1:l3">staf</rhyme>:</l>
        <l id="s1:l7" met="2" type="+-">En met <rhyme label="e" type="m">een</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gunt eeuwigh blyde <rhyme label="b" type="f" corresp="#s1:l2">daeghjes</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Haer; en die haer Speelnoots <rhyme label="f" type="m">zijn</rhyme>.</l>
        <l id="s1:l10" met="3" type="-+">Nu werdt de <rhyme label="g" type="m">vreughde-tijdt</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Sacht <rhyme label="g" type="m" corresp="#s1:l10">ingewijdt</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">Met schroefjes daer 't suyckertje swemt in de <rhyme label="f" type="m" corresp="#s1:l9">Wijn</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Ay <rhyme label="h" type="f">Roose-kaeckjes</rhyme>!</l>
        <l id="s1:l14" met="3" type="+-">Weygert geen <rhyme label="i" type="m">bescheyt</rhyme>,</l>
        <l id="s1:l15" met="2" type="-+">Dronckjes noch <rhyme label="h" type="f" corresp="#s1:l13">smaeckjes</rhyme>,</l>
        <l id="s1:l16" met="3" type="+-">Op Bruyts <rhyme label="i" type="m" corresp="#s1:l14">gesontheyt</rhyme>:</l>
        <l id="s1:l17" met="1" type="-+">En <rhyme label="j" type="m">ziet</rhyme>,</l>
        <l id="s1:l18" met="1" type="-+">'t <rhyme label="j" type="m" corresp="#s1:l17">Geniet</rhyme>,</l>
        <l id="s1:l19" met="2" type="-+">Dat haer <rhyme label="k" type="m">genaeckt</rhyme>:</l>
        <l id="s1:l20" met="1" type="-+">En <rhyme label="j" type="m" corresp="#s1:l17">leert</rhyme>,</l>
        <l id="s1:l21" met="2" type="+-">Hoe <rhyme label="j" type="m" corresp="#s1:l17">ge-eert</rhyme>,</l>
        <l id="s1:l22" met="3" type="+-">D'Echt de Iuffertjes <rhyme label="k" type="m" corresp="#s1:l19">maeckt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
