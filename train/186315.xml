<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186315</id>
    <date>
      <year-start>1672</year-start>
      <year-end>1672</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La marine</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Heel Londen was in <rhyme label="a" type="f">roeren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den Coningh seer <rhyme label="b" type="m">ontstelt</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Van 't Land quamen de <rhyme label="a" type="f" corresp="#s1:l1">Boeren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Vluchten voor 't <rhyme label="b" type="m" corresp="#s1:l2">gewelt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Den Ruyter met sijn <rhyme label="c" type="m">Vloot</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van Gent daer mee by <rhyme label="c" type="m" corresp="#s1:l5">hoort</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Brakel <rhyme label="d" type="f">mede</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Hoort de <rhyme label="d" type="f" corresp="#s1:l7">rede</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Voeren <rhyme label="c" type="m" corresp="#s1:l5">voort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Haven <rhyme label="a" type="f">toe-geslooten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Al met een Kettingh <rhyme label="b" type="m">zwaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die Brakel <rhyme label="a" type="f" corresp="#s2:l1">onverdroten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Zeylden van <rhyme label="b" type="m" corresp="#s2:l2">malkaer</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">'t Casteel schoot met <rhyme label="c" type="m">geweld</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Maer ons Geschut <rhyme label="c" type="m" corresp="#s2:l5">gestelt</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Bonsde <rhyme label="d" type="f">mede</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Op die <rhyme label="d" type="f" corresp="#s2:l7">Rede</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Braef in 't <rhyme label="c" type="m" corresp="#s2:l5">Veld</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Na drie uren <rhyme label="a" type="f">verlopen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'t Casteel dat kregen <rhyme label="b" type="m">wy</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Daer viel braef wat te <rhyme label="a" type="f" corresp="#s3:l1">stroopen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Noch kregen wy daer <rhyme label="b" type="m" corresp="#s3:l2">by</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Het Coninckx <rhyme label="c" type="m">Magasijn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Groot, schoon, hoort doch na <rhyme label="c" type="m" corresp="#s3:l5">mijn</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Met veel <rhyme label="d" type="f">Schepen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Wy <rhyme label="d" type="f" corresp="#s3:l7">aen-grepen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Groot en <rhyme label="c" type="m" corresp="#s3:l5">kleyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men saghse daer <rhyme label="a" type="f">verbranden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Door onse Branders <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Van Gent die sagh men <rhyme label="a" type="f" corresp="#s4:l1">Landen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met sijn Soldaten <rhyme label="b" type="m" corresp="#s4:l2">hoort</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Oranje, blanje <rhyme label="c" type="m">eel</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Woey boven van 't <rhyme label="c" type="m" corresp="#s4:l5">Casteel</rhyme>:</l>
        <l id="s4:l7" met="2" type="+-">Hollants <rhyme label="d" type="f">glory</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Braef <rhyme label="d" type="f" corresp="#s4:l7">Victory</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Elck sijn <rhyme label="c" type="m" corresp="#s4:l5">deel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Coningh Karel <rhyme label="a" type="f">verslagen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Iorck en Prins Robbert <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Monck en meer andere <rhyme label="a" type="f" corresp="#s5:l1">Heeren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Begaven haer na <rhyme label="b" type="m" corresp="#s5:l2">Zee</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hebben haer Schepen <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Selfs in de grond <rhyme label="c" type="m" corresp="#s5:l5">geboort</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Veel <rhyme label="d" type="f">voetknechten</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Om te <rhyme label="d" type="f" corresp="#s5:l7">vechten</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Mosten <rhyme label="c" type="m" corresp="#s5:l5">voort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Seer sterck aen Strand <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Te voet en oock te <rhyme label="b" type="m">Paerd</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Als wilt en sonder <rhyme label="a" type="f" corresp="#s6:l1">schromen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Te maken ons <rhyme label="b" type="m" corresp="#s6:l2">vervaerd</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Maer ons Metael <rhyme label="c" type="m">Geweer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Met Schroot dat bralden <rhyme label="c" type="m" corresp="#s6:l5">seer</rhyme>,</l>
        <l id="s6:l7" met="2" type="+-">Dee haer <rhyme label="d" type="f">truren</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">Op die <rhyme label="d" type="f" corresp="#s6:l7">uren</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-">Aen Strand <rhyme label="c" type="m" corresp="#s6:l5">weer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Veel Ruyters en <rhyme label="a" type="f">Voetknechten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Verslagen daer aen <rhyme label="b" type="m">Strand</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Die ons quamen <rhyme label="a" type="f" corresp="#s7:l1">bevechten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wy hebbens' <rhyme label="b" type="m" corresp="#s7:l2">overmand</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wel duysent in 't <rhyme label="c" type="m">getal</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Verslagen aen de <rhyme label="c" type="m" corresp="#s7:l5">Wal</rhyme>,</l>
        <l id="s7:l7" met="2" type="+-">Wy met <rhyme label="d" type="f">vrede</rhyme>,</l>
        <l id="s7:l8" met="2" type="+-">Van die <rhyme label="d" type="f" corresp="#s7:l7">Rede</rhyme></l>
        <l id="s7:l9" met="2" type="+-">t'Samen <rhyme label="c" type="m" corresp="#s7:l5">al</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Door dit gewelt was <rhyme label="a" type="f">Londen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ses dagen in 't <rhyme label="b" type="m">Geweer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wie dat maer wierd <rhyme label="a" type="f" corresp="#s8:l1">gevonden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Most voort al op die <rhyme label="b" type="m" corresp="#s8:l2">keer</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Van sestien Iaren <rhyme label="c" type="m">out</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Tot sestigh sonder <rhyme label="c" type="m" corresp="#s8:l5">fout</rhyme>,</l>
        <l id="s8:l7" met="2" type="+-">Om te <rhyme label="d" type="f">waken</rhyme>,</l>
        <l id="s8:l8" met="2" type="+-">En te <rhyme label="d" type="f" corresp="#s8:l7">braken</rhyme>,</l>
        <l id="s8:l9" met="2" type="+-">Al soo <rhyme label="c" type="m" corresp="#s8:l5">stout</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Het volck was in <rhyme label="a" type="f">roeren</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">'t Huys van den <rhyme label="b" type="m">Cancelier</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">In dees groot <rhyme label="a" type="f" corresp="#s9:l1">rumoeren</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Wilden sy met <rhyme label="b" type="m" corresp="#s9:l2">getier</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Verdistrueren <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Scholden hem <rhyme label="c" type="m" corresp="#s9:l5">allegaer</rhyme>,</l>
        <l id="s9:l7" met="2" type="+-">O 's Lands <rhyme label="d" type="f">Vader</rhyme>,</l>
        <l id="s9:l8" met="2" type="+-">Ghy <rhyme label="d" type="f" corresp="#s9:l7">Verrader</rhyme>,</l>
        <l id="s9:l9" met="2" type="+-">Zijt <rhyme label="c" type="m" corresp="#s9:l5">onklaer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Den Coningh quam <rhyme label="a" type="f">gereden</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Den Cancelier teegh <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Noch op den dagh van <rhyme label="a" type="f" corresp="#s10:l1">heden</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Wert van hem niet <rhyme label="b" type="m" corresp="#s10:l2">gehoort</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">De Predicanten <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En den Coningh, 't is <rhyme label="c" type="m" corresp="#s10:l5">waer</rhyme>,</l>
        <l id="s10:l7" met="2" type="+-">Met goe <rhyme label="d" type="f">reden</rhyme>,</l>
        <l id="s10:l8" met="2" type="+-">Baden om <rhyme label="d" type="f" corresp="#s10:l7">Vrede</rhyme>,</l>
        <l id="s10:l9" met="2" type="+-">'t Volck <rhyme label="c" type="m" corresp="#s10:l5">allegaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De Gilden, wilt <rhyme label="a" type="f">noteren</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Riepen, och maeckt doch <rhyme label="b" type="m">Vree</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">Ghy siet hoe wy <rhyme label="a" type="f" corresp="#s11:l1">kleyneren</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Te Land en oock ter <rhyme label="b" type="m" corresp="#s11:l2">Zee</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Of wy sullen te <rhyme label="c" type="m">gaer</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Door oproer met <rhyme label="c" type="m" corresp="#s11:l5">malkaer</rhyme>,</l>
        <l id="s11:l7" met="2" type="+-">Door een <rhyme label="d" type="f">strijden</rhyme>,</l>
        <l id="s11:l8" met="2" type="+-">Noch meer <rhyme label="d" type="f" corresp="#s11:l7">lijden</rhyme>,</l>
        <l id="s11:l9" met="2" type="+-"><rhyme label="c" type="m" corresp="#s11:l5">Allegaer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Onse Vloot is <rhyme label="a" type="f">gesteecken</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Resoluyt in Zee <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Verwachten wilt het <rhyme label="a" type="f" corresp="#s12:l1">weten</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Ons Branders van hier <rhyme label="b" type="m" corresp="#s12:l2">daer</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">En onderwegen <rhyme label="c" type="m">hoort</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Wat Schepen komen <rhyme label="c" type="m" corresp="#s12:l5">voort</rhyme>,</l>
        <l id="s12:l7" met="2" type="+-">Nemen sy <rhyme label="d" type="f">mede</rhyme></l>
        <l id="s12:l8" met="2" type="+-">Voor haer <rhyme label="d" type="f" corresp="#s12:l7">Rede</rhyme>,</l>
        <l id="s12:l9" met="2" type="+-">'t Is buyt <rhyme label="c" type="m" corresp="#s12:l5">hoort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
