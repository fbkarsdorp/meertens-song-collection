      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOd sy ons altesaem <rhyme label="a" type="f">genadich</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En segen ons met <rhyme label="b" type="m">overvloet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hy geev' dat over ons <rhyme label="a" type="f" corresp="#s1:l1">gestadich</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Syn minlijck aenschijn lichten <rhyme label="b" type="m" corresp="#s1:l2">moet'</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Op dat, Heer, u <rhyme label="c" type="f">wegen</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Ons bekent ter <rhyme label="c" type="f" corresp="#s1:l5">degen</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Worden, en met <rhyme label="d" type="m">vlijt</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Oock de Heydens <rhyme label="e" type="f">leeren</rhyme></l>
        <l id="s1:l9" met="3" type="+-">V alleen te <rhyme label="e" type="f" corresp="#s1:l8">eeren</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Die haer Heylant <rhyme label="d" type="m" corresp="#s1:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De volcken, Heer, u sullen <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">De volcken sullen dancken <rhyme label="b" type="m">dy</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Sy sullen uwen lof <rhyme label="a" type="f" corresp="#s2:l1">voortbringen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En juychen uyter-maten <rhyme label="b" type="m" corresp="#s2:l2">bly</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Ghy sult alle <rhyme label="c" type="f">lieden</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Richten en <rhyme label="c" type="f" corresp="#s2:l5">gebieden</rhyme></l>
        <l id="s2:l7" met="3" type="+-">In <rhyme label="d" type="m">gerechticheyt</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Ghy sult alle <rhyme label="e" type="f">scharen</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Stieren, en <rhyme label="e" type="f" corresp="#s2:l8">bewaren</rhyme></l>
        <l id="s2:l10" met="3" type="+-">In u vry <rhyme label="d" type="m" corresp="#s2:l7">geleyd'</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De volcken, Heer, om te <rhyme label="a" type="f">gewagen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">V grooten roem, haer maken <rhyme label="b" type="m">ree</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Het aerdrijck sal syn vruchten <rhyme label="a" type="f" corresp="#s3:l1">dragen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">God deyl ons synen segen <rhyme label="b" type="m" corresp="#s3:l2">mee</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">God, ons God <rhyme label="c" type="f">alleene</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Gonstich ons <rhyme label="c" type="f" corresp="#s3:l5">verleene</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Segen ende <rhyme label="d" type="m">spoet</rhyme>:</l>
        <l id="s3:l8" met="3" type="+-">Dat hem alle <rhyme label="e" type="f">hoecken</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Des aerdbodems <rhyme label="e" type="f" corresp="#s3:l8">soecken</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Met een dweech <rhyme label="d" type="m" corresp="#s3:l7">gemoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
