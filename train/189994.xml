    <melody>Pots honderdduizend slapperment</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'k Zal u nimmermeer <rhyme label="a" type="f">verlaaten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Schoonste die 'er leeft op <rhyme label="b" type="m">Aard'</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Nooit zal ik myn liefde <rhyme label="a" type="f" corresp="#s1:l1">staaken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Schoon ik niet met u <rhyme label="b" type="m" corresp="#s1:l2">gepaard</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Door de Huwelyksband kan <rhyme label="c" type="f">weezen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En u krygen tot myn <rhyme label="d" type="m">Vrouw</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Gy hebt echter niet te <rhyme label="c" type="f" corresp="#s1:l5">vreezen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Dat ik u verlaaten <rhyme label="d" type="m" corresp="#s1:l6">zou</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Neen, ô leeven van myn <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Zulks zal nimmermeer <rhyme label="b" type="m">geschiên</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Nimmer zal ik u <rhyme label="a" type="f" corresp="#s2:l1">begeeven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Nimmer zal ik van u <rhyme label="b" type="m" corresp="#s2:l2">vliên</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Eerder zal ik voor u <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Eerder wil ik zyn in 't <rhyme label="d" type="m">graf</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Dan dat ik myn Schat zou <rhyme label="c" type="f" corresp="#s2:l5">derven</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Wien 'k myn trouw uit liefde <rhyme label="d" type="m" corresp="#s2:l6">gaf</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
