    <melody>O zalig heilig Bethlehem</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>anoniem</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WAer toeft mijn gouden <rhyme label="a" type="m">dagheraet</rhyme>?</l>
        <l id="s1:l2" met="4" type="-+">Waer mach Silvia nu <rhyme label="b" type="f">blijven</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Ach Nimphje komt eer ‘t wort te <rhyme label="a" type="m" corresp="#s1:l1">laet</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Ick sie de Son nae ‘t Westen <rhyme label="b" type="f" corresp="#s1:l2">drijven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">’T schijnt of mijn ruygh-ghewolde <rhyme label="a" type="m">Vee</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ghevoelt de smert die ick moet <rhyme label="b" type="f">lijden</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Want yder stil leyt op sijn <rhyme label="a" type="m" corresp="#s2:l1">stee</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En schijnt ghelijck als ick te <rhyme label="b" type="f" corresp="#s2:l2">beyden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Swijgt stil, mijn tongh hout u <rhyme label="a" type="m">gerust</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Verschuylt u Cloris inde <rhyme label="b" type="f">boomen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Nu heden wert u min <rhyme label="a" type="m" corresp="#s3:l1">gheblust</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Vermits ich Silvia sie <rhyme label="b" type="f" corresp="#s3:l2">komen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoe soet is ‘t minnen alsmen <rhyme label="a" type="m">mint</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Aen wederzijden even <rhyme label="b" type="f">krachtigh</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En als de liefde Weer-liefd’ <rhyme label="a" type="m" corresp="#s4:l1">vint</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Soo is de min door liefd’ <rhyme label="b" type="f" corresp="#s4:l2">eendrachtigh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wel hoe wat ‘s dit? ‘t is nu de <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Dat Cloris my hier had’ <rhyme label="b" type="f">bescheyden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Waer of mijn Herder nu <rhyme label="a" type="m" corresp="#s5:l1">verbijt</rhyme>?</l>
        <l id="s5:l4" met="4" type="-+">En laet Silvia oock <rhyme label="b" type="f" corresp="#s5:l2">verbeyden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wat ruyssen hoor ick in ’t <rhyme label="a" type="m">gheboomt</rhyme>?</l>
        <l id="s6:l2" met="4" type="-+">Ay my! o Goon! wat sal dit <rhyme label="b" type="f">wesen</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">Sacht waerde lief weest niet <rhyme label="a" type="m" corresp="#s6:l1">beschroomt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">‘T is Cloris dies verlaet u <rhyme label="b" type="f" corresp="#s6:l2">vresen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Zijt wellekom mijn waerde <rhyme label="a" type="m">Son</rhyme>.</l>
        <l id="s7:l2" met="4" type="-+">En ghy mijn Herderin <rhyme label="b" type="f">gepresen</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Kom rusten wy aen dese <rhyme label="a" type="m" corresp="#s7:l1">Bron</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Stelt van u hert ‘t onnodigh <rhyme label="b" type="f" corresp="#s7:l2">vreesen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ha Cloris roover van mijn <rhyme label="a" type="m">sin</rhyme>’.</l>
        <l id="s8:l2" met="4" type="-+">En ghy vooghdes van mijn <rhyme label="b" type="f">vryheden</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">En ghy Steel-diefje van mijn <rhyme label="a" type="m" corresp="#s8:l1">Min</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">En ghy Goddin in mijn <rhyme label="b" type="f" corresp="#s8:l2">gebeden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Waer isser soeter soet op <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Als Weerliefd, liefde komt <rhyme label="b" type="f">begroeten</rhyme>?</l>
        <l id="s9:l3" met="4" type="-+">‘T schijnt d’ Hemel ons te samen <rhyme label="a" type="m" corresp="#s9:l1">paert</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">‘T schijnt ick door dwangh u lust moet <rhyme label="b" type="f" corresp="#s9:l2">boeten</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ontfanght Silvia van mijn <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">De zeghel van mijn <rhyme label="b" type="f">trouwigheden</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">‘K aenvaert, en reyck u toe mijn <rhyme label="a" type="m" corresp="#s10:l1">pant</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Tot tuygh van onbreeckbare <rhyme label="b" type="f" corresp="#s10:l2">erden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Tuygt bloem, en blos, en blaetjes, <rhyme label="a" type="m">tuygt</rhyme></l>
        <l id="s11:l2" met="4" type="-+">De trouwe liefd’ by ons <rhyme label="b" type="f">beslooten</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Queelt beesjes, fluyt, van vreughde <rhyme label="a" type="m" corresp="#s11:l1">juycht</rhyme>:</l>
        <l id="s11:l4" met="4" type="-+">Mits liefde Weerliefd’ heeft <rhyme label="b" type="f" corresp="#s11:l2">ghenoten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Kom waerde Lief laet ons in ‘t <rhyme label="a" type="m">groen</rhyme>;</l>
        <l id="s12:l2" met="4" type="-+">Venus Altaren op gaen <rhyme label="b" type="f">stoocken</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">En blusschen min met soen op <rhyme label="a" type="m" corresp="#s12:l1">soen</rhyme>:</l>
        <l id="s12:l4" met="4" type="-+">Want noyt ons min en wert <rhyme label="b" type="f" corresp="#s12:l2">ghebroocken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
