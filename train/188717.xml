<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188717</id>
    <date>
      <year-start>1690</year-start>
      <year-end>1690</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nerea schoonste van uw geburen</melody>
  <author>Barent Pietersz. Kompas</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Ziel in ootmoet <rhyme label="a" type="f">neergebogen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Stelt u, o Heer! haer selven <rhyme label="b" type="m">voor</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Siet af met Vaderlijcke <rhyme label="a" type="f" corresp="#s1:l1">oogen</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">Beschouwtse door en weder <rhyme label="b" type="m" corresp="#s1:l2">door</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Siet hoe mijn Ziel en Hert van <rhyme label="a" type="f">binne</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ontsteecken is met uwe <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sy weet schier niet wat te <rhyme label="a" type="f" corresp="#s2:l1">beginne</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Soo heeft uw' Liefd' het herte <rhyme label="b" type="m" corresp="#s2:l2">in</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dan suchtse met betraende <rhyme label="a" type="f">wange</rhyme>:</l>
        <l id="s3:l2" met="4" type="-+">Dan singhtse weder in den <rhyme label="b" type="m">geest</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Dat sucht en hijghtse door <rhyme label="a" type="f" corresp="#s3:l1">verlange</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Dan isse als een Kindt dat <rhyme label="b" type="m" corresp="#s3:l2">vreest</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dan roeptse Abba lieve <rhyme label="a" type="f">Vader</rhyme>:</l>
        <l id="s4:l2" met="4" type="-+">Versoeckt u hulp en klaeght haer <rhyme label="b" type="m">noot</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Dan looft en prijst sy u, o <rhyme label="a" type="f" corresp="#s4:l1">Ader</rhyme>!</l>
        <l id="s4:l4" met="4" type="-+">Fonteyn des Levens <rhyme label="b" type="m" corresp="#s4:l2">overgroot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dan isse sterck gelijck een <rhyme label="a" type="f">Reuse</rhyme>:</l>
        <l id="s5:l2" met="4" type="-+">En dan weer swack gelijck een <rhyme label="b" type="m">Riedt</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Dan wilse staen, dan wilse <rhyme label="a" type="f" corresp="#s5:l1">kneuse</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dan is het blijdtschap, dan <rhyme label="b" type="m" corresp="#s5:l2">verdriet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dan isse boven: dan weer <rhyme label="a" type="f">onder</rhyme>.</l>
        <l id="s6:l2" met="4" type="-+">En dan is 't voor of in <rhyme label="b" type="m">windt</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Of licht, of duyster. O groot <rhyme label="a" type="f" corresp="#s6:l1">wonder</rhyme>!</l>
        <l id="s6:l4" met="4" type="-+">Wat dat de Ziel niet al <rhyme label="b" type="m" corresp="#s6:l2">bevindt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dit doet, o Heer! haer ooge <rhyme label="a" type="f">wecke</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Naer u en naer 't bestendigh <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dit doet haer hert en sinne <rhyme label="a" type="f" corresp="#s7:l1">trecke</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En haer opvaren van <rhyme label="b" type="m" corresp="#s7:l2">beneen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dit doet haer achten 't Aertsch <rhyme label="a" type="f">gewemel</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Voor enckel niet, en niet met <rhyme label="b" type="m">al</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Dit doet haer rijck-halsen ten <rhyme label="a" type="f" corresp="#s8:l1">Hemel</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Na 't beste, en 't beste van <rhyme label="b" type="m" corresp="#s8:l2">al</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Het stuck-werck kan haer niet <rhyme label="a" type="f">vernoegen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Om dat sy van 't volmaeckte <rhyme label="b" type="m">weet</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Dus doetse niet als hygen, <rhyme label="a" type="f" corresp="#s9:l1">swoegen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Het langh vertoeven is haer <rhyme label="b" type="m" corresp="#s9:l2">leet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ia, sy singht met betraende <rhyme label="a" type="f">wange</rhyme>:</l>
        <l id="s10:l2" met="4" type="-+">Wanneer, o Heere! is de <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Dat eens ophouden sal 't <rhyme label="a" type="f" corresp="#s10:l1">verlange</rhyme>?</l>
        <l id="s10:l4" met="4" type="-+">En dat ick met u ben <rhyme label="b" type="m" corresp="#s10:l2">verblijdt</rhyme>?</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wanneer sult ghy my <rhyme label="a" type="f">vergenoegen</rhyme>?</l>
        <l id="s11:l2" met="4" type="-+">Mijn Hemels Heer en <rhyme label="b" type="m">Bruydegom</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">U ende my te samen <rhyme label="a" type="f" corresp="#s11:l1">voegen</rhyme>?</l>
        <l id="s11:l4" met="4" type="-+">Kom Heer! mijn lieve Trooster <rhyme label="b" type="m" corresp="#s11:l2">kom</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ick tel byna de dagh en <rhyme label="a" type="f">uure</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Wanneer uw' komste sal <rhyme label="b" type="m">geschien</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Ick hoop het sal niet lange <rhyme label="a" type="f" corresp="#s12:l1">duuren</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Dat ick eens tot u op sal <rhyme label="b" type="m" corresp="#s12:l2">vlien</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Vertreck niet, maer verkort de <rhyme label="a" type="f">dage</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Breeck af, en maeck met my een <rhyme label="b" type="m">endt</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Wilt Heer! mijn Ziele <rhyme label="a" type="f" corresp="#s13:l1">overdrage</rhyme></l>
        <l id="s13:l4" met="4" type="-+">By u, in uwe Hemels <rhyme label="b" type="m" corresp="#s13:l2">Tent</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Neem doch in acht mijn groot <rhyme label="a" type="f">verlangen</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">En mijn ontsteecken hart vol <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s14:l3" met="4" type="-+">Snelt toch en spoeyt Heer uwe <rhyme label="a" type="f" corresp="#s14:l1">gangen</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">En treedt tot mijne Tempel <rhyme label="b" type="m" corresp="#s14:l2">in</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Wilt u toch tot u Duyfje <rhyme label="a" type="f">voegen</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">En u vereenigen met <rhyme label="b" type="m">haer</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Dan sal sy haer wel <rhyme label="a" type="f" corresp="#s15:l1">vergenoegen</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Al duurt het vry noch menigh <rhyme label="b" type="m" corresp="#s15:l2">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
