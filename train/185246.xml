      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 023 Datheen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Laet ons <rhyme label="a" type="m">eenpaer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">De eenigheyt der <rhyme label="b" type="f">zielen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Wel nemen <rhyme label="a" type="m" corresp="#s1:l1">waer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En al in ons <rhyme label="b" type="f" corresp="#s1:l2">vernielen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Wat Liefd’ en <rhyme label="c" type="m">Vreed</rhyme>’</l>
        <l id="s1:l6" met="3" type="-+">In ons sou mogen <rhyme label="d" type="f">krencken</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">’s Herten <rhyme label="c" type="m" corresp="#s1:l5">ghebeed</rhyme>’</l>
        <l id="s1:l8" met="3" type="-+">Den Heer gedurigh <rhyme label="d" type="f" corresp="#s1:l6">schencken</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Met reyn <rhyme label="e" type="m">ootmoedt</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Wandelen voor Godts <rhyme label="f" type="f">oogen</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">In liefde <rhyme label="e" type="m" corresp="#s1:l9">soet</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Tot malcander <rhyme label="f" type="f" corresp="#s1:l10">gheboghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Na dat ‘t <rhyme label="a" type="m">gemoed</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Des menschen is <rhyme label="b" type="f">ghelegen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Godts wesen <rhyme label="a" type="m" corresp="#s2:l1">soet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot hemwaerts is <rhyme label="b" type="f" corresp="#s2:l2">genegen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Is hy vol <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Liefd’ sal hem <rhyme label="d" type="f">wedervaren</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Heeft hy strijdt <rhyme label="c" type="m" corresp="#s2:l5">in</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Strijdt sal hem oock <rhyme label="d" type="f" corresp="#s2:l6">beswaren</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Na den <rhyme label="e" type="m">magneet</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Daer na bloeyen de <rhyme label="f" type="f">crachten</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Soo wie dit <rhyme label="e" type="m" corresp="#s2:l9">weet</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Roept uyt sijn qua <rhyme label="f" type="f" corresp="#s2:l10">gedachten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Verlaet u <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Op u verstant of <rhyme label="b" type="f">weten</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Al schynet <rhyme label="a" type="m" corresp="#s3:l1">yet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">‘t Is maer stuck-werck <rhyme label="b" type="f" corresp="#s3:l2">geheeten</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Buyght uwen <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Onder ghemeyne <rhyme label="d" type="f">stichtingh</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Vna boven <rhyme label="c" type="m" corresp="#s3:l5">in</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Vloeyt wijsheyts <rhyme label="d" type="f" corresp="#s3:l6">onderrichtingh</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Niet woort, maer <rhyme label="e" type="m">Geest</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Can ‘t hert alleen <rhyme label="f" type="f">genesen</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Niet in <rhyme label="e" type="m" corresp="#s3:l9">tempeest</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Maer stilheyt wil Godt <rhyme label="f" type="f" corresp="#s3:l10">wesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Den noorden <rhyme label="a" type="m">wint</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Verderft veel soete <rhyme label="b" type="f">vruchten</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Dit elck <rhyme label="a" type="m" corresp="#s4:l1">bevint</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Als hy liefde voelt <rhyme label="b" type="f" corresp="#s4:l2">vluchten</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Van Hermon <rhyme label="c" type="m">comt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Den Dau <rhyme label="d" type="f">nederghesteghen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Als <rhyme label="c" type="m" corresp="#s4:l5">onghesont</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Den vrede geeft den <rhyme label="d" type="f" corresp="#s4:l6">zeghen</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Als met <rhyme label="e" type="m">eendracht</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Ghebroeders herten ‘t <rhyme label="f" type="f">samen</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Sijn in <rhyme label="e" type="m" corresp="#s4:l9">aendacht</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Haer woort is. Iae en <rhyme label="f" type="f" corresp="#s4:l10">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
