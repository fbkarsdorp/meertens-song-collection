<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178378</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Wilhelmus</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Prins Willem is weer <rhyme label="a" type="f">Jarig</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zingt vrolyk uit de <rhyme label="b" type="m">Borst</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Geen agste Maart valt <rhyme label="a" type="f" corresp="#s1:l1">kaarig</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">'t Is voor Oranjes <rhyme label="b" type="m" corresp="#s1:l2">Vorst</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Wie kan zich zelf <rhyme label="c" type="f">bedwingen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van op zoo blij een <rhyme label="d" type="m">tijd</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Voor onzen Prins te <rhyme label="c" type="f" corresp="#s1:l5">zingen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Al wie hem haat ten <rhyme label="d" type="m" corresp="#s1:l6">spijt</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wij draagen vrije <rhyme label="a" type="f">nekken</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Zoo zingt al 't Hollands <rhyme label="b" type="m">bloed</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Wie zou zijn vreugd <rhyme label="a" type="f" corresp="#s2:l1">bedekken</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Elk Zwaai den vrijen <rhyme label="b" type="m" corresp="#s2:l2">hoedt</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Wen wij deez' dag zien <rhyme label="c" type="f">lichten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Verbannen wij den <rhyme label="d" type="m">druk</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">De bitse nijd moet <rhyme label="c" type="f" corresp="#s2:l5">zwichten</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dees dag brengt ons <rhyme label="d" type="m" corresp="#s2:l6">geluk</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Prins wil voor ons <rhyme label="a" type="f">waaken</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">Hij is aan 't <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Altijd in alle <rhyme label="a" type="f" corresp="#s3:l1">zaaken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Getrouw aan alle <rhyme label="b" type="m" corresp="#s3:l2">kant</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Nooit is zijn min <rhyme label="c" type="f">bezweeken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Voor dit <rhyme label="d" type="m">Gemeenebest</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Des juichen deeze <rhyme label="c" type="f" corresp="#s3:l5">Streeken</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Daar Vrijheid is <rhyme label="d" type="m" corresp="#s3:l6">gevest</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hij blijf op God <rhyme label="a" type="f">betrouwen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hoe zeer men hem ook <rhyme label="b" type="m">smaadt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die op die Rots kan <rhyme label="a" type="f" corresp="#s4:l1">bouwen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Vreest nimmermeer den <rhyme label="b" type="m" corresp="#s4:l2">haat</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Zijn schuldeloos <rhyme label="c" type="f">geweeten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Nooit met verraad <rhyme label="d" type="m">bevlekt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Schond nimmer <rhyme label="c" type="f" corresp="#s4:l5">eervergeeten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">'t Geen tot een pligt hem <rhyme label="d" type="m" corresp="#s4:l6">strekt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nooit hield hij 't met de <rhyme label="a" type="f">Britten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Nooit heeft hij 't Land <rhyme label="b" type="m">verraÃªn</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Ach zag men dat nooit <rhyme label="a" type="f" corresp="#s5:l1">zitten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Bij die hem 't meest <rhyme label="b" type="m" corresp="#s5:l2">versmaan</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">Dankt vrije <rhyme label="c" type="f">Batavieren</rhyme>.</l>
        <l id="s5:l6" met="3" type="-+">Dankt Willem voor zijn <rhyme label="d" type="m">trouw</rhyme>!</l>
        <l id="s5:l7" met="3" type="-+">Lang blyve Hij <rhyme label="c" type="f" corresp="#s5:l5">bestieren</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ons vrije <rhyme label="d" type="m" corresp="#s5:l6">Staatsgebouw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wy zyn Oranjes <rhyme label="a" type="f">Vrienden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En 't is ons tot veel <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat wy ons nooit <rhyme label="a" type="f" corresp="#s6:l1">bedienden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Van Leugentaal, door 't <rhyme label="b" type="m" corresp="#s6:l2">hof</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Des duivels <rhyme label="c" type="f">uitgevonden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Op dat door tweedragt <rhyme label="d" type="m">eens</rhyme></l>
        <l id="s6:l7" met="3" type="-+">De Vryheid word <rhyme label="c" type="f" corresp="#s6:l5">verslonden</rhyme>;</l>
        <l id="s6:l8" met="3" type="-+">Want dat's tans iets <rhyme label="d" type="m" corresp="#s6:l6">gemeens</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Sta op Heer! toont voor de <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat allersnoodst <rhyme label="b" type="m">gebroed</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Verbryzel zyn <rhyme label="a" type="f" corresp="#s7:l1">vermoogen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Bescherm den vryen <rhyme label="b" type="m" corresp="#s7:l2">hoed</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Laat elk Oranje <rhyme label="c" type="f">roemen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En nimmermeer dat <rhyme label="d" type="m">Huis</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dan met diepste eerbied <rhyme label="c" type="f" corresp="#s7:l5">eoemen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Al raast het boos <rhyme label="d" type="m" corresp="#s7:l6">gespuis</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Laat elk met lof <rhyme label="a" type="f">vermelden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Oranjes Vorsten <rhyme label="b" type="m">Stam</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Uit wien een reeks van <rhyme label="a" type="f" corresp="#s8:l1">Helden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Voor ons ten voorschyn <rhyme label="b" type="m" corresp="#s8:l2">kwam</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Hy kan geen <rhyme label="c" type="f">Nederlander</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Geen dankbaar mensch zelfs <rhyme label="d" type="m">zyn</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Die Willems <rhyme label="c" type="f" corresp="#s8:l5">eerestander</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Bezwaddert met <rhyme label="d" type="m" corresp="#s8:l6">fenyn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Zoo dat onzinnig <rhyme label="a" type="f">tieren</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Niet vond een <rhyme label="b" type="m">tegenstand</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Waar ras vol <rhyme label="a" type="f" corresp="#s9:l1">monsterdieren</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Dit vrye <rhyme label="b" type="m" corresp="#s9:l2">Nederland</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Te pryzen zyn die <rhyme label="c" type="f">geenen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die 't met Prins Willems <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">En zyn Persoon wel <rhyme label="c" type="f" corresp="#s9:l5">meenen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Al vloekt de nyd hem <rhyme label="d" type="m" corresp="#s9:l6">zeer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Daar zyn nog braave <rhyme label="a" type="f">mannen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Die stoutelyk 't <rhyme label="b" type="m">lasterrot</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Te keer gaan en 't <rhyme label="a" type="f" corresp="#s10:l1">verbannen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Hoe zeer 't met alles <rhyme label="b" type="m" corresp="#s10:l2">spot</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Die zullen nu ook <rhyme label="c" type="f">zingen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Vol edle vreugd, en <rhyme label="d" type="m">tans</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Het Vorstlyk hoofd <rhyme label="c" type="f" corresp="#s10:l5">omringen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Met een <rhyme label="d" type="m" corresp="#s10:l6">geboortekrans</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die durven uit te <rhyme label="a" type="f">koomen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Van 's Prinsen goed <rhyme label="b" type="m">bestuur</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">En weeten van geen <rhyme label="a" type="f" corresp="#s11:l1">schroomen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">De waarheid is hun <rhyme label="b" type="m" corresp="#s11:l2">muur</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Die zyn getrouw den <rhyme label="c" type="f">Staaten</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Prins Willem als 's Lands <rhyme label="d" type="m">Hooft</rhyme>;</l>
        <l id="s11:l7" met="3" type="-+">En al hun <rhyme label="c" type="f" corresp="#s11:l5">Magistraaten</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Van eer nog deugd <rhyme label="d" type="m" corresp="#s11:l6">beroofd</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Die laaten honden <rhyme label="a" type="f">bassen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En zwynen zig in <rhyme label="b" type="m">drek</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Van modderpoelen <rhyme label="a" type="f" corresp="#s12:l1">wassen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Den slaafsgekromden <rhyme label="b" type="m" corresp="#s12:l2">nek</rhyme>.</l>
        <l id="s12:l5" met="3" type="-+">Maar als ze op 't laast hun <rhyme label="c" type="f">pylen</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Afzenden, moet het <rhyme label="d" type="m">dier</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Naar broertje Cerbrus <rhyme label="c" type="f" corresp="#s12:l5">ylen</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">En het zwyn moet weg van <rhyme label="d" type="m" corresp="#s12:l6">hier</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Zoo zal het al die <rhyme label="a" type="f">geesten</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Die leugenspreekers <rhyme label="b" type="m">gaan</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Die, wreeder dan de <rhyme label="a" type="f" corresp="#s13:l1">beesten</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Als slangen aan de <rhyme label="b" type="m" corresp="#s13:l2">paan</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Prins Willems daan <rhyme label="c" type="f">beloeren</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">En syflend met hun <rhyme label="d" type="m">spog</rhyme>.</l>
        <l id="s13:l7" met="3" type="-+">Het gantsche Land <rhyme label="c" type="f" corresp="#s13:l5">beloeren</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Door allersnoodst <rhyme label="d" type="m" corresp="#s13:l6">bedrog</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Maar onze Prins zal <rhyme label="a" type="f">klimmen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">In Jaaren en in <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Dan zal geen beer meer <rhyme label="a" type="f" corresp="#s14:l1">grimmen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Dan blaast geen ringslang <rhyme label="b" type="m" corresp="#s14:l2">meer</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">De ontzaggelijke <rhyme label="c" type="f">Koning</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">De grootste Regter <rhyme label="d" type="m">komt</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">En vonnist uit zyn <rhyme label="c" type="f" corresp="#s14:l5">Wooning</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Het Leugenrot <rhyme label="d" type="m" corresp="#s14:l6">verstomt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Prins Willems Jaaren <rhyme label="a" type="f">rollen</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">In Vrede en Welvaart <rhyme label="b" type="m">voort</rhyme>;</l>
        <l id="s15:l3" met="3" type="-+">Daar is een einde aan 't <rhyme label="a" type="f" corresp="#s15:l1">hollen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Het Vloekrot is <rhyme label="b" type="m" corresp="#s15:l2">gestoord</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Zyn smertelyke <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Verwisselen in <rhyme label="d" type="m">geluk</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">En vreedzaam <rhyme label="c" type="f" corresp="#s15:l5">welbehaagen</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Verre af van hoon en <rhyme label="d" type="m" corresp="#s15:l6">druk</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Hij ziet aan zijn <rhyme label="a" type="f">Vorstinne</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Al wat hem vreugd <rhyme label="b" type="m">verwekt</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">Zulk eene <rhyme label="a" type="f" corresp="#s16:l1">Gemalinne</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Die elk ten wonder <rhyme label="b" type="m" corresp="#s16:l2">strekt</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Hij ziet aan zijn drie <rhyme label="c" type="f">Spruiten</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Al wat hij bidden <rhyme label="d" type="m">kon</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Niets kan dien zegen <rhyme label="c" type="f" corresp="#s16:l5">stuiten</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Zoo klimt d'Oranje <rhyme label="d" type="m" corresp="#s16:l6">Zon</rhyme>!</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Zoo zal zijn roem ook <rhyme label="a" type="f">rijzen</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">En alle nazaat <rhyme label="b" type="m">Hem</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Als grootsten Willem <rhyme label="a" type="f" corresp="#s17:l1">prijzen</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Naar dees <rhyme label="b" type="m" corresp="#s17:l2">Voorzeggingstem</rhyme>.</l>
        <l id="s17:l5" met="3" type="-+">Bedrieg ik my?. . . wat <rhyme label="c" type="f">oogen</rhyme></l>
        <l id="s17:l6" met="3" type="-+">Slaat George Frederik <rhyme label="d" type="m">op</rhyme>!</l>
        <l id="s17:l7" met="3" type="-+">Word hem de Ziel <rhyme label="c" type="f" corresp="#s17:l5">bewoogen</rhyme>?</l>
        <l id="s17:l8" met="3" type="-+">Ryst dan zyn vreugd in <rhyme label="d" type="m" corresp="#s17:l6">top</rhyme>?</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Ja Prinsezoon 't zal <rhyme label="a" type="f">koomen</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Daar nadert blyder <rhyme label="b" type="m">tyd</rhyme>!</l>
        <l id="s18:l3" met="3" type="-+">Dan word U zonder <rhyme label="a" type="f" corresp="#s18:l1">schroomen</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Ook een gezang <rhyme label="b" type="m" corresp="#s18:l2">gewyd</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Wanneer gy zult <rhyme label="c" type="f">verjaaren</rhyme>.</l>
        <l id="s18:l6" met="3" type="-+">Ons treft uw <rhyme label="d" type="m">schranderheid</rhyme>!</l>
        <l id="s18:l7" met="3" type="-+">U moog veel heils <rhyme label="c" type="f" corresp="#s18:l5">weervaren</rhyme>,</l>
        <l id="s18:l8" met="3" type="-+">Jong Prinsje vol <rhyme label="d" type="m" corresp="#s18:l6">beleid</rhyme>!</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Dat uw jong <rhyme label="a" type="f">medelyden</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">Met de armen op den <rhyme label="b" type="m">duur</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">In barre <rhyme label="a" type="f" corresp="#s19:l1">wintertyden</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">In zoet verkeere 't <rhyme label="b" type="m" corresp="#s19:l2">zuur</rhyme>.</l>
        <l id="s19:l5" met="3" type="-+">Dan klimt gy op <rhyme label="c" type="f">gebeden</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Der schaamlen opwaards <rhyme label="d" type="m">aan</rhyme>.</l>
        <l id="s19:l7" met="3" type="-+">Dan zal by Christus <rhyme label="c" type="f" corresp="#s19:l5">Leden</rhyme>,</l>
        <l id="s19:l8" met="3" type="-+">Uw Naam in zeegening <rhyme label="d" type="m" corresp="#s19:l6">staan</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Prins Willem sta <rhyme label="a" type="f">Voorspoedig</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">Nog lang aan 't roer van <rhyme label="b" type="m">Staat</rhyme>.</l>
        <l id="s20:l3" met="3" type="-+">Hij zie zijn vijand <rhyme label="a" type="f" corresp="#s20:l1">moedig</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Afleggen alle <rhyme label="b" type="m" corresp="#s20:l2">haat</rhyme>!</l>
        <l id="s20:l5" met="3" type="-+">Dan zullen blijde <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Verheugen dit vrij <rhyme label="d" type="m">Land</rhyme>,</l>
        <l id="s20:l7" met="3" type="-+">En ons geen twist <rhyme label="c" type="f" corresp="#s20:l5">belagen</rhyme>,</l>
        <l id="s20:l8" met="3" type="-+">Die allen zegen <rhyme label="d" type="m" corresp="#s20:l6">band</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
