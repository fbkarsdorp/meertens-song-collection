<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184581</id>
    <date>
      <year-start>1755</year-start>
      <year-end>1755</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het viel een hemelse dauw</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Goeden dag Maagd <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik wensch u goeden <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoe staat het met u <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Geen schoonder ik oyt en <rhyme label="b" type="m" corresp="#s1:l2">zag</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Ik hebbe met <rhyme label="c" type="f">behagen</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">Wel overschoone <rhyme label="d" type="m">Meyd</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Mijn zin op u <rhyme label="c" type="f" corresp="#s1:l5">geslagen</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Alzo u diend <rhyme label="d" type="m" corresp="#s1:l6">gezeyd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hebt gy u gantsche <rhyme label="a" type="f">zinne</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Ionkman op my <rhyme label="b" type="m">geleyd</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Gaat zoekt, gy zullet <rhyme label="a" type="f" corresp="#s2:l1">vinde</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Een vry wat beter <rhyme label="b" type="m" corresp="#s2:l2">Meyd</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Want Geld en al mijn <rhyme label="c" type="f">dingen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die zijn zo gy wel <rhyme label="d" type="m">weet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Voor u veel te <rhyme label="c" type="f" corresp="#s2:l5">geringe</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die een rijk Ionkman <rhyme label="d" type="m" corresp="#s2:l6">heet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Heb ik Ionk-vrouw wat <rhyme label="a" type="f">meere</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">Geld ofte goed als <rhyme label="b" type="m">gy</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">U deugd en uw’ <rhyme label="a" type="f" corresp="#s3:l1">eere</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als die daar maar is <rhyme label="b" type="m" corresp="#s3:l2">by</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Die kan het al <rhyme label="c" type="f">verzoete</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dus Ionkvrouw <rhyme label="d" type="m">rezolveerd</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En komt mijn Min te <rhyme label="c" type="f" corresp="#s3:l5">moete</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Met weder-liefd’ my <rhyme label="d" type="m" corresp="#s3:l6">eerd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Denkt eens Ionkman <rhyme label="a" type="f">eerbaaren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hoe dat het veel <rhyme label="b" type="m">gebeurt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die ongelijk gaat <rhyme label="a" type="f" corresp="#s4:l1">paaren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Het dikmaals wel <rhyme label="b" type="m" corresp="#s4:l2">betreurt</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Hoe datze zonder <rhyme label="c" type="f">schromen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wel zeggen tot haar <rhyme label="d" type="m">Vrouw</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Het is van my <rhyme label="c" type="f" corresp="#s4:l5">gekomen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dan leeft dat Wijf in <rhyme label="d" type="m" corresp="#s4:l6">rouw</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ik wil u wel <rhyme label="a" type="f">beloove</rhyme>:</l>
        <l id="s5:l2" met="3" type="-+">Dat ik u niet en <rhyme label="b" type="m">zal</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Brengen in zwaar <rhyme label="a" type="f" corresp="#s5:l1">bedrove</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Want mijn Goederen <rhyme label="b" type="m" corresp="#s5:l2">al</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Zo lange als ik zal <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Zijn u zo wel als <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dus wil mijn ’t Ia-woord <rhyme label="c" type="f" corresp="#s5:l5">geven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Indien het maar mag <rhyme label="d" type="m" corresp="#s5:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gy kund my niet <rhyme label="a" type="f">verblyden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Gy staat my gantsch niet <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Een aar zal aan mijn <rhyme label="a" type="f" corresp="#s6:l1">zyden</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">By my ter slapen <rhyme label="b" type="m" corresp="#s6:l2">gaan</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Dus Ionkman gaat uw’ <rhyme label="c" type="f">wegen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ziet na een ander <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die tot u is <rhyme label="c" type="f" corresp="#s6:l5">genegen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Die u werd tot een <rhyme label="d" type="m" corresp="#s6:l6">Bruyd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Met mijn Schapen en <rhyme label="a" type="f">Koeyen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ionkvrouw diend u <rhyme label="b" type="m">gezeyd</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zult gy u niet <rhyme label="a" type="f" corresp="#s7:l1">bemoeyen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Maar ik zal u een <rhyme label="b" type="m" corresp="#s7:l2">Meyd</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Houden tot u <rhyme label="c" type="f">geryven</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die mijne Koeyen <rhyme label="d" type="m">zal</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">In, ende uytwaards <rhyme label="c" type="f" corresp="#s7:l5">dryven</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En Melken in de <rhyme label="d" type="m" corresp="#s7:l6">Stal</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Den Visscher wild het <rhyme label="a" type="f">weeten</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die was alhier <rhyme label="b" type="m">ontrent</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">By deze twee <rhyme label="a" type="f" corresp="#s8:l1">gezeten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Maar z’hebben hem niet <rhyme label="b" type="m" corresp="#s8:l2">bekend</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Want het was donker <rhyme label="c" type="f">Nagte</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Zy konde hem niet <rhyme label="d" type="m">zien</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Nu ging den tijd uyt <rhyme label="c" type="f" corresp="#s8:l5">wagten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Dat den Land-man ging <rhyme label="d" type="m" corresp="#s8:l6">hien</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Hy ginck deze <rhyme label="a" type="f">Ionkvrouwe</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Den aldernaasten <rhyme label="b" type="m">Nagt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ook bieden zijne <rhyme label="a" type="f" corresp="#s9:l1">Trouwe</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Met Minnelijke <rhyme label="b" type="m" corresp="#s9:l2">klagt</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Zy scheen ’t te willen <rhyme label="c" type="f">wagen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Maar sprak met Woorden <rhyme label="d" type="m">zaan</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">‘k Moet eerst mijn Ouders <rhyme label="c" type="f" corresp="#s9:l5">vragen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Hoe dat het haar staat <rhyme label="d" type="m" corresp="#s9:l6">aan</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Zie daar mijn Ouders <rhyme label="a" type="f">goedig</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Aanhoord u lieve <rhyme label="b" type="m">Kind</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Komt uw spreeken <rhyme label="a" type="f" corresp="#s10:l1">ootmoedig</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Hoe dat ik werd’ <rhyme label="b" type="m" corresp="#s10:l2">bemind</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Van twee Ionkmans <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Zo werd ik nu <rhyme label="d" type="m">gevrijd</rhyme>;</l>
        <l id="s10:l7" met="3" type="-+">Dien ik wel dien te <rhyme label="c" type="f" corresp="#s10:l5">geven</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Antwoord in korten <rhyme label="d" type="m" corresp="#s10:l6">tijd</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Het een dat is een <rhyme label="a" type="f">Visser</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Het ander een <rhyme label="b" type="m">Land-man</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">Maar den Zee-man veel <rhyme label="a" type="f" corresp="#s11:l1">frisser</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">Die staat my ’t best wel <rhyme label="b" type="m" corresp="#s11:l2">an</rhyme>:</l>
        <l id="s11:l5" met="3" type="-+">Woud gy het maar toe <rhyme label="c" type="f">laten</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Gewis ik deed het <rhyme label="d" type="m">strak</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">‘k Zou met den Visscher <rhyme label="c" type="f" corresp="#s11:l5">praten</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">En geven den Boer de <rhyme label="d" type="m" corresp="#s11:l6">zak</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Een Boer tot uwer <rhyme label="a" type="f">eere</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ey slaat die liever <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En laat hem <rhyme label="a" type="f" corresp="#s12:l1">conferseeren</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En wild zijn Trouw <rhyme label="b" type="m" corresp="#s12:l2">ontfaan</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Wild onze les <rhyme label="c" type="f">onthouwen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">O! lieve Dogter <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">En wild den Boer-man <rhyme label="c" type="f" corresp="#s12:l5">trouwen</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Zo weet gy wat gy <rhyme label="d" type="m" corresp="#s12:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Den Huys-mans Vrouw wild <rhyme label="a" type="f">merken</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Doet nimmermeer <rhyme label="b" type="m">genoeg</rhyme>;</l>
        <l id="s13:l3" met="3" type="-+">Zy moeten altijd <rhyme label="a" type="f" corresp="#s13:l1">werken</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Als Paarden in de <rhyme label="b" type="m" corresp="#s13:l2">Ploeg</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Zy moeten Kaaze <rhyme label="c" type="f">wringen</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">En Boter maaken <rhyme label="d" type="m">mee</rhyme>:</l>
        <l id="s13:l7" met="3" type="-+">En nog meer and’re <rhyme label="c" type="f" corresp="#s13:l5">dingen</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Waar van mijn Hert doet <rhyme label="d" type="m" corresp="#s13:l6">wee</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Weet mijn Dogter <rhyme label="a" type="f">verkooren</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Wat ik u bidden <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Geen Blad en meugd gy <rhyme label="a" type="f" corresp="#s14:l1">hooren</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Of gy denkt om u <rhyme label="b" type="m" corresp="#s14:l2">Man</rhyme>;</l>
        <l id="s14:l5" met="3" type="-+">Gy denkt de felle <rhyme label="c" type="f">Winden</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Die ik geduurig <rhyme label="d" type="m">hoor</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Zullen mijn Man <rhyme label="c" type="f" corresp="#s14:l5">verslinden</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Dat komt u altijd <rhyme label="d" type="m" corresp="#s14:l6">voor</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Al blaast Apollos <rhyme label="a" type="f">Winden</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Nogtans de Heer zeer <rhyme label="b" type="m">ree</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Kan mijn Man zo wel <rhyme label="a" type="f" corresp="#s15:l1">vinden</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Op ’t Land als in de <rhyme label="b" type="m" corresp="#s15:l2">Zee</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Dus is mijn gantsche <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Tot den Visscher <rhyme label="d" type="m">gesteld</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Den Boer heb ik <rhyme label="c" type="f" corresp="#s15:l5">begeven</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Al had hy meerder <rhyme label="d" type="m" corresp="#s15:l6">Geld</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Ontfangt dit Lied ter <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Van den Visscher <rhyme label="b" type="m">bekwaam</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Al lijd hy veel <rhyme label="a" type="f" corresp="#s16:l1">verzeeren</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Nogtans hoog staat zijn <rhyme label="b" type="m" corresp="#s16:l2">Naam</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Die ons dit Liedje <rhyme label="c" type="f">digte</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Het was een Ionkman <rhyme label="d" type="m">stout</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Die hem wel laat <rhyme label="c" type="f" corresp="#s16:l5">berigte</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Waar in dat hy heeft <rhyme label="d" type="m" corresp="#s16:l6">fout</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
