<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185857</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Es ist das Heil uns kommen her</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Gheluckich en salich zijnse <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die in v huys mogen <rhyme label="b" type="f">woonen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">V soete presentie verheucht haer <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Reyn liefde sy v <rhyme label="b" type="f" corresp="#s1:l2">betoonen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Zy loven v inder <rhyme label="c" type="m">eewicheyt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Dat ghy so schone stadt hebt <rhyme label="c" type="m" corresp="#s1:l5">bereyt</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Om v dienaers daer in te <rhyme label="b" type="f" corresp="#s1:l2">croonen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dese stadt is wtgenomen <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sy en sal oock niet <rhyme label="b" type="f">verwoesten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want het is uwen heyligen <rhyme label="a" type="m" corresp="#s2:l1">troon</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Wie en sou daer niet na <rhyme label="b" type="f" corresp="#s2:l2">lusten</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Daer altijt genuechte is sonder <rhyme label="c" type="m">verdrach</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Daer duysent Iaer is als eenen <rhyme label="c" type="m" corresp="#s2:l5">dach</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Wel haer die daer in <rhyme label="b" type="f" corresp="#s2:l2">rusten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Soeckt dese stadt doch <rhyme label="a" type="m">allegaer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En wilt die werelt <rhyme label="b" type="f">laten</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Haer mueren zijn als Iaspis <rhyme label="a" type="m" corresp="#s3:l1">claer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Als fijn gout zijn haer <rhyme label="b" type="f" corresp="#s3:l2">straten</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Van gesteenten en van peerlen <rhyme label="c" type="m">net</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Zo zijn haer poorten fray <rhyme label="c" type="m" corresp="#s3:l5">beset</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Zy is schoon boven <rhyme label="b" type="f" corresp="#s3:l2">maten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die inwoonders van dese stadt Gods <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Zijn verciert met blinckende <rhyme label="b" type="f">cleeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Sy hebben hier in dit aertsche <rhyme label="a" type="m" corresp="#s4:l1">pleyn</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Gedient den Heer der <rhyme label="b" type="f" corresp="#s4:l2">Heeren</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Haer Lampen vercieret met olie <rhyme label="c" type="m">saen</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Tot den Bruydegom zijn zy <rhyme label="c" type="m" corresp="#s4:l5">ingegaen</rhyme></l>
        <l id="s4:l7" met="4" type="-+">In die vreuchde die niet sal <rhyme label="b" type="f" corresp="#s4:l2">keeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dit is haer meeste vreucht <rhyme label="a" type="m">gewis</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die zy hebben <rhyme label="b" type="f">warachtich</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Godt claer te aenschouwen gelijck hy <rhyme label="a" type="m" corresp="#s5:l1">is</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">In zijn Goddelijck wesen <rhyme label="b" type="f" corresp="#s5:l2">crachtich</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Van zijn claer blinckende aenschijn <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s5:l6" met="4" type="-+">V comt alle ghenuechte ende vruechden <rhyme label="c" type="m" corresp="#s5:l5">soet</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dus loven sy hem <rhyme label="b" type="f" corresp="#s5:l2">eendrachtich</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sy hebben oock vreucht en <rhyme label="a" type="m">melodije</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Te aenschouwen die schoon <rhyme label="b" type="f">vriendinne</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maria Godts moeder en maghet <rhyme label="a" type="m" corresp="#s6:l1">blije</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Die hemelsche <rhyme label="b" type="f" corresp="#s6:l2">Coninginne</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Die neghen chooren der Engelen <rhyme label="c" type="m">jent</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Die schoonheyt der Hemelen <rhyme label="c" type="m" corresp="#s6:l5">excellent</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Dit verhuecht haer hert en <rhyme label="b" type="f" corresp="#s6:l2">sinne</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Haer vreucht die is <rhyme label="a" type="m">onspreeckelijck</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die haer nu is <rhyme label="b" type="f">gegheven</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Om dat sy weten <rhyme label="a" type="m" corresp="#s7:l1">sekerlijck</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Dat haer namen zijn <rhyme label="b" type="f" corresp="#s7:l2">gheschreven</rhyme></l>
        <l id="s7:l5" met="4" type="-+">In het boeck des levens reyn <rhyme label="c" type="m">onbevleckt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Dit maeckt haer vreuchden gheheel <rhyme label="c" type="m" corresp="#s7:l5">perfeckt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Dat zy eeuwich daer sullen <rhyme label="b" type="f" corresp="#s7:l2">leven</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer en is gheen droefheyt oft <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gheen lyden en mach haer <rhyme label="b" type="f">crencken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Maer wat daer is en weetmen <rhyme label="a" type="m" corresp="#s8:l1">niet</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Gheen herte en kant <rhyme label="b" type="f" corresp="#s8:l2">bedencken</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Die schoonheyt van dese nieuwe <rhyme label="c" type="m">Stadt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Met al haer glorie, vreucht en <rhyme label="c" type="m" corresp="#s8:l5">schat</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Sal Godt zijn dienaeren <rhyme label="b" type="f" corresp="#s8:l2">schencken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
