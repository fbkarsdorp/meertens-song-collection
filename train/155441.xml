<song>
  <meta>
    <id>155441</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DOe Godts Volck den Heer <rhyme label="a" type="m">vergat</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Nam 't sijn <rhyme label="a" type="m" corresp="#s1:l1">schat</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Goudt, en and're <rhyme label="b" type="f">kost'lijckheden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daer Aaron 't Kalf van <rhyme label="c" type="m">goot</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Dat seer <rhyme label="c" type="m" corresp="#s1:l4">snoot</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Worde van't volck <rhyme label="b" type="f" corresp="#s1:l3">aengebeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Soud' light seggen: 't en is <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Meer <rhyme label="a" type="m" corresp="#s2:l1">geschiedt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'t Geen dat volck vol <rhyme label="b" type="f">onverstande</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Soo Godlooslijck heeft <rhyme label="c" type="m">ghedaen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">En badt <rhyme label="c" type="m" corresp="#s2:l4">aen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">'t Maecksel van haer eygen <rhyme label="b" type="f" corresp="#s2:l3">handen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer wanneer als ick ter <rhyme label="a" type="m">deegh</rhyme></l>
        <l id="s3:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s3:l1">Over-weegh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wat dat meenigh in het <rhyme label="b" type="f">midden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Van geluck, en voorspoet <rhyme label="c" type="m">dee</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Sie ick <rhyme label="c" type="m" corresp="#s3:l4">mee</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Datse den Goudt-klomp <rhyme label="b" type="f" corresp="#s3:l3">aen-bidden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Gelt is menighs <rhyme label="a" type="m">toe-verlaet</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Daer op <rhyme label="a" type="m" corresp="#s4:l1">staet</rhyme></l>
        <l id="s4:l3" met="4" type="+-">'t Fondament van haer <rhyme label="b" type="f">vertrouwen</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Al haer moeylijck <rhyme label="c" type="m">handen-werck</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Is een <rhyme label="c" type="m" corresp="#s4:l4">Kerck</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Die sy 't Gelt ter eere <rhyme label="b" type="f" corresp="#s4:l3">bouwen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
