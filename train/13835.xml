<song>
  <meta>
    <id>13835</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Een eeuwige vreugde die niet en vergaat</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">‘TIs Hollandts pronck, Haerlems <rhyme label="a" type="m">cieraet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ons wijdt beroemde <rhyme label="b" type="f">Kercke</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Die noch in vollen wesen <rhyme label="a" type="m" corresp="#s1:l1">staet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Soo yeder mach <rhyme label="b" type="f" corresp="#s1:l2">aenmercken</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Met stercke mueren dick en <rhyme label="c" type="m">hoogh</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Verciert met menich <rhyme label="c" type="m" corresp="#s1:l5">Kerrick-boogh</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die het Glas-werck <rhyme label="d" type="f">besluyten</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Nooyt ick in <rhyme label="e" type="m">Hollandt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Schoonder Ghebouw en <rhyme label="e" type="m" corresp="#s1:l8">vandt</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Als dees Kerck, van binnen en <rhyme label="d" type="f" corresp="#s1:l7">buyten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Indien men wil dit groot schoon <rhyme label="a" type="m">vat</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van binnen gaen <rhyme label="b" type="f">bekijcken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Men vindt daer in een kleyne <rhyme label="a" type="m" corresp="#s2:l1">Stadt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Want sy doet in haer <rhyme label="b" type="f" corresp="#s2:l2">blijcken</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Drie wandel-plaetsen voor de <rhyme label="c" type="m">gheen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Om in zedigheydt te gaen <rhyme label="c" type="m" corresp="#s2:l5">treen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En rondom met <rhyme label="d" type="f">Pylaren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Vol Lof-werck, sterck en <rhyme label="e" type="m">slecht</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Waer teghen staet <rhyme label="e" type="m" corresp="#s2:l8">gehecht</rhyme></l>
        <l id="s2:l10" met="4" type="-+">De plaets, om ‘t Woordt Godts te <rhyme label="d" type="f" corresp="#s2:l7">verklaren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Wulleften van steen en <rhyme label="a" type="m">hout</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met Schinckels net <rhyme label="b" type="f">doorvlochten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Vol Kruys-werck, soo cierlijck <rhyme label="a" type="m" corresp="#s3:l1">ghebouwt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Als yemandt maecken <rhyme label="b" type="f" corresp="#s3:l2">mochte</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Drie Orghels, die des Heeren <rhyme label="c" type="m">lof</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Vyt brommen, konstigh, fijn en <rhyme label="c" type="m" corresp="#s3:l5">grof</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En een hoogh Koor <rhyme label="d" type="f">verheven</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Waer boven ‘s Heeren <rhyme label="e" type="m">Wet</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Tot aendacht is <rhyme label="e" type="m" corresp="#s3:l8">gheset</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Met duytsche Letters, promt <rhyme label="d" type="f" corresp="#s3:l7">geschreven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick zwijge noch van al ‘t <rhyme label="a" type="m">cieraet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat in haer is <rhyme label="b" type="f">begrepen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">‘t Geen daer tot pronck noch hangt en <rhyme label="a" type="m" corresp="#s4:l1">staet</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">De Damiaatsche <rhyme label="b" type="f" corresp="#s4:l2">Schepen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Hanghen daer tot <rhyme label="c" type="m">ghedachtenis</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Hoe Damiaat’ gewonnen <rhyme label="c" type="m" corresp="#s4:l5">is</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Al van de trouwe <rhyme label="d" type="f">Christen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Door Haerlems kloeck <rhyme label="e" type="m">beleydt</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En wiens <rhyme label="e" type="m" corresp="#s4:l8">voorsichtigheydt</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Recht anders als de Heydens <rhyme label="d" type="f" corresp="#s4:l7">giften</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een Hemel-hooge Spits daer <rhyme label="a" type="m">op</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daer de Tijdt-klocken <rhyme label="b" type="f">hangen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Boven ‘t kruys van des Kercken <rhyme label="a" type="m" corresp="#s5:l1">top</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Drie schoone <rhyme label="b" type="f" corresp="#s5:l2">Ommegangen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En twee Klockjens van <rhyme label="c" type="m">Damiaat-stof</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Die staegh uyt brommen Haerlems <rhyme label="c" type="m" corresp="#s5:l5">lof</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Tot eeuwighe <rhyme label="d" type="f">memory</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Als men Tryumph brandt <rhyme label="e" type="m">hier</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Tot roem ‘s Landts <rhyme label="e" type="m" corresp="#s5:l8">Batavier</rhyme>;</l>
        <l id="s5:l10" met="4" type="-+">Dan toont men oock op ‘t Spits <rhyme label="d" type="f" corresp="#s5:l7">victory</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Een vrome Ziel mach vry en <rhyme label="a" type="m">vranck</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hier in sijn adem <rhyme label="b" type="f">halen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Lust hem Rust, hy sit op een <rhyme label="a" type="m" corresp="#s6:l1">bancck</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Sonder yets te <rhyme label="b" type="f" corresp="#s6:l2">betalen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">En speculeert in sijn <rhyme label="c" type="m">ghedacht</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Van d’Heyl’ge Goddelijcke <rhyme label="c" type="m" corresp="#s6:l5">kracht</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Ghelijck de <rhyme label="d" type="f">Iongelinghen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Die spraecken onder <rhyme label="e" type="m">een</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Heyl-stichtelijcke <rhyme label="e" type="m" corresp="#s6:l8">re’en</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Doenst’ op den wegh nae Emaus <rhyme label="d" type="f" corresp="#s6:l7">gingen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
