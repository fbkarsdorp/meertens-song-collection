    <melody>Windje daar het bos af drilt</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Michiel Vlack</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Minne God dom eerghierich,, <rhyme label="a" type="m">wicht</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Dwinght ghy met u vyerigh,, <rhyme label="a" type="m" corresp="#s1:l1">schicht</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Phoebi sinnen,, tot het <rhyme label="b" type="f">minnen</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Hoe sal u den crancken,, <rhyme label="c" type="m">mensch</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Dienen dan niet na danck,, end' <rhyme label="c" type="m" corresp="#s1:l4">wensch</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">t' Lijckt wel of brant geen brandt,, en <rhyme label="a" type="m">bluscht</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Als t' oogh nut moet vanden <rhyme label="a" type="m" corresp="#s2:l1">lust</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Dienaer wesen,, want of <rhyme label="b" type="f">desen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Godt niet dan een slaef en ,, <rhyme label="c" type="m">waer</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Saghmen hem Daphne draven,, <rhyme label="c" type="m" corresp="#s2:l4">naer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Roemt nu vergheefs o ghy Son <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Dat ghy hebt den Python,, <rhyme label="a" type="m" corresp="#s3:l1">doot</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Met u pijlen,, daer ghy <rhyme label="b" type="f">t'wylen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Niet dan swaer verdriet,, en <rhyme label="c" type="m">erft</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Stervende dich doch niet,, en <rhyme label="c" type="m" corresp="#s3:l4">sterft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Vinder der Medecijnen,, <rhyme label="a" type="m">ghy</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Zijnde niet van pijnen,, <rhyme label="a" type="m" corresp="#s4:l1">vry</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Wat magh baten,, doen of <rhyme label="b" type="f">laten</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Nerghens cruyt ghevonden,, <rhyme label="c" type="m">wert</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Goet tot der liefden wond' end',, <rhyme label="c" type="m" corresp="#s4:l4">smert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ghy die die dichters leert,, <rhyme label="a" type="m">t'ghedicht</rhyme></l>
        <l id="s5:l2" met="4" type="+-">V niet toe en keert,, <rhyme label="a" type="m" corresp="#s5:l1">t'ghesicht</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Door schoon reden,, noch <rhyme label="b" type="f">ghebeden</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Die schoon wel gheschapen,, <rhyme label="c" type="m">maeght</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Die ghy om te beslapen,, <rhyme label="c" type="m" corresp="#s5:l4">jaeght</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Eersaem zy noode,, spreken <rhyme label="a" type="m">hoort</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Van Veneris treecken,, <rhyme label="a" type="m" corresp="#s6:l1">woort</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Vroech en spa na, by Dia <rhyme label="b" type="f">na</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Plach te loopen, in der <rhyme label="c" type="m">jacht</rhyme></l>
        <l id="s6:l5" met="4" type="+-">'t Vryen zy nu veel minder <rhyme label="c" type="m" corresp="#s6:l4">acht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Tempe du schoon landouw' en,, <rhyme label="a" type="m">dal</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Langhe men onthouwen,, <rhyme label="a" type="m" corresp="#s7:l1">sal</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Hoe dat Daphne,, als haer <rhyme label="b" type="f">grafste</rhyme></l>
        <l id="s7:l4" met="4" type="+-">V int vluchten bald,, <rhyme label="c" type="m">verkoos</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Die sy lijf end' ghestalt,, <rhyme label="c" type="m" corresp="#s7:l4">verloos</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Comende by haers Vaders,, <rhyme label="a" type="m">vloet</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Zaghmen haer met bladers,, <rhyme label="a" type="m" corresp="#s8:l1">zoet</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Die groen blyven,, Boom <rhyme label="b" type="f">beclyven</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Phoebus desen warmen,, <rhyme label="c" type="m">stam</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Droevelick in zijn armen,, <rhyme label="c" type="m" corresp="#s8:l4">nam</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Boom ghy sult nu beghinnen,, <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Teecken van verwinnen,, <rhyme label="a" type="m" corresp="#s9:l1">mijn</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Harpen Lieren,, hooft oock <rhyme label="b" type="f">cieren</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Op dees reden doe,, <rhyme label="c" type="m">gheschict</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Heeft hem den Boom oock toe,, <rhyme label="c" type="m" corresp="#s9:l4">gheknict</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Dochters die maer het paren,, <rhyme label="a" type="m">recht</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Toestaet in eerbaren,, <rhyme label="a" type="m" corresp="#s10:l1">echt</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Om in vreughden,, te <rhyme label="b" type="f">verjeughden</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Zal uytstorten soete <rhyme label="c" type="m">lucht</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Voor u een eeuwigh goet,, <rhyme label="c" type="m" corresp="#s10:l4">gherucht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Sulck een maeght zal becroonen, <rhyme label="a" type="m">loff</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Als roos' inden schoonen,, <rhyme label="a" type="m" corresp="#s11:l1">hoff</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Js haer groeyen,, end' haer <rhyme label="b" type="f">bloeyen</rhyme></l>
        <l id="s11:l4" met="4" type="+-">t'Wylen sy is <rhyme label="c" type="m">ongheploct</rhyme></l>
        <l id="s11:l5" met="4" type="+-">Jonstigh haer yeders tonghe,, <rhyme label="c" type="m" corresp="#s11:l4">loct</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Even ghelijck den Lauwer,, <rhyme label="a" type="m">groen</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Sal zijn der Jongvrauwer,, <rhyme label="a" type="m" corresp="#s12:l1">doen</rhyme></l>
        <l id="s12:l3" met="4" type="+-">Wederstryen,, t'schandigh <rhyme label="b" type="f">vryen</rhyme></l>
        <l id="s12:l4" met="4" type="+-">End' d' oncuyssche vlammen,, <rhyme label="c" type="m">quaet</rhyme></l>
        <l id="s12:l5" met="4" type="+-">Vlieden met eenen grammen,, <rhyme label="c" type="m" corresp="#s12:l4">haet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
