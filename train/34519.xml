    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Paarlemontje zat en <rhyme label="a" type="f">lachte</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dat 'et Visschers oogjen <rhyme label="b" type="m">zach</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die, door deeze lieve <rhyme label="b" type="m" corresp="#s1:l2">lach</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Paareltjens te visschen <rhyme label="a" type="f" corresp="#s1:l1">trachte</rhyme></l>
        <l id="s1:l5" met="4" type="+-">In het beekjen van haar <rhyme label="c" type="m">mondt</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Maar eer hy 't gevaar <rhyme label="a" type="f" corresp="#s1:l1">bedachte</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Ging zijn zieltjen daar te <rhyme label="c" type="m" corresp="#s1:l5">grondt</rhyme>.</l>
      </lg>
    </lg>
