  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">MYn Lief <rhyme label="a" type="m">Klorimeen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">gy kunt my <rhyme label="b" type="f">behaagen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Maar wilt m' als <rhyme label="a" type="m" corresp="#s1:l1">voorheen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">zoo geweldig niet <rhyme label="b" type="f" corresp="#s1:l2">plaagen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Want zoo d'oude <rhyme label="c" type="f">kuuren</rhyme></l>
        <l id="s1:l6" met="3" type="+-">noch langer dus <rhyme label="c" type="f" corresp="#s1:l5">duuren</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Kunt gy myn voorwerp onmooglyk meer <rhyme label="d" type="m">zyn</rhyme>;</l>
        <l id="s1:l8" met="7" type="+-">Want Bachus, Want Bachus, want Bachus belooft my vol op van zyn <rhyme label="d" type="m" corresp="#s1:l7">Wyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
