    <melody>Psalm 105 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DEn Heere loff. sijn eere <rhyme label="a" type="f">klincke</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">De luyster sijner daden <rhyme label="a" type="f" corresp="#s1:l1">blincke</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Den volcken. Hier, met liedt en <rhyme label="b" type="m">luyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En spreeckt all’ sijne wond’ren <rhyme label="b" type="m" corresp="#s1:l3">uyt</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Dat sijnen name roem-rijck <rhyme label="c" type="m">zy</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Der Godt-gesinden hert <rhyme label="c" type="m" corresp="#s1:l5">verbly</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Heer, en sijn’ soo hooge <rhyme label="a" type="f">krachten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Zy ’t opper-wit van uw’ <rhyme label="a" type="f" corresp="#s2:l1">gedachten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Om durentlijck beglanst te <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Van sijnes Godlijckheydts <rhyme label="b" type="m" corresp="#s2:l3">aenschijn</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Behoudt in trouwe <rhyme label="c" type="m">herte-blaen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">De wercken oyt van hem <rhyme label="c" type="m" corresp="#s2:l5">gedaen</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De seldtsaemheden sijner <rhyme label="a" type="f">wercken</rhyme>:</l>
        <l id="s3:l2" met="4" type="-+">Sijn wetten, vaste <rhyme label="a" type="f" corresp="#s3:l1">wille-percken</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">V, zaedt van Abram sijnen <rhyme label="b" type="m">knecht</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">V, Iacobs kind’ren, is’t <rhyme label="b" type="m" corresp="#s3:l3">geseght</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">De Heer ons’ Godt. (daer houdt hem <rhyme label="c" type="m">voor</rhyme>)</l>
        <l id="s3:l6" met="4" type="-+">Sijn oordeel all’ de werelt <rhyme label="c" type="m" corresp="#s3:l5">door</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sijns woordts onsterflijck <rhyme label="a" type="f">na-geheugen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Vrijdt eeuwlijck sijnen mondt van <rhyme label="a" type="f" corresp="#s4:l1">leugen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">En knoopt door duysendt stammen <rhyme label="b" type="m">heen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Sijn eens-gemaeckte bondt aen <rhyme label="b" type="m" corresp="#s4:l3">een</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Daer van hy eerst met Abram <rhyme label="c" type="m">sprack</rhyme></l>
        <l id="s4:l6" met="4" type="-+">En na beswoer aen <rhyme label="c" type="m" corresp="#s4:l5">Isaac</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat hy met Iacob laetst <rhyme label="a" type="f">oprichte</rhyme></l>
        <l id="s5:l2" met="4" type="-+">En met sijn zaedt voor eeuwigh <rhyme label="a" type="f" corresp="#s5:l1">stichte</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Het landt van Canaan (sprack <rhyme label="b" type="m">Godt</rhyme>)</l>
        <l id="s5:l4" met="4" type="-+">Vw’ eeuwigh erff en vaste <rhyme label="b" type="m" corresp="#s5:l3">lot</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Te dier tijdt was haer aensien <rhyme label="c" type="m">kleen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">En haer getal ten naesten <rhyme label="c" type="m" corresp="#s5:l5">geen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Haer staet was als der <rhyme label="a" type="f">vreemdelingen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die vast van’t een tot ’tander <rhyme label="a" type="f" corresp="#s6:l1">gingen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Van volck tot volck, van landt tot <rhyme label="b" type="m">landt</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">En’ noch, in soo verschoven <rhyme label="b" type="m" corresp="#s6:l3">standt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">En leed Godt niet dat van den <rhyme label="c" type="m">lien</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Den sijnen hinder sou’ <rhyme label="c" type="m" corresp="#s6:l5">geschien</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Liet eenigh Konings hert sich <rhyme label="a" type="f">neygen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Ten leet; strax hoord’ hy Godes <rhyme label="a" type="f" corresp="#s7:l1">dreygen</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">En raeckt mijn uytverkoren <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Doet mijn Propheten geen <rhyme label="b" type="m" corresp="#s7:l3">verdriet</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">(Wel hun die als een waerden <rhyme label="c" type="m">schat</rhyme></l>
        <l id="s7:l6" met="4" type="-+">In Godes sorge zijn <rhyme label="c" type="m" corresp="#s7:l5">bevat</rhyme>.)</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Den honger quam, op Godts <rhyme label="a" type="f">gebieden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Al ’taerdtrijck door. ’tgebrack den <rhyme label="a" type="f" corresp="#s8:l1">lieden</rhyme></l>
        <l id="s8:l3" met="4" type="-+">(Mits hy natures krachten <rhyme label="b" type="m">sloot</rhyme>)</l>
        <l id="s8:l4" met="4" type="-+">Alom aen’t noodelijcke <rhyme label="b" type="m" corresp="#s8:l3">broodt</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">De Heer (o diepe <rhyme label="c" type="m">goediche’en</rhyme>!)</l>
        <l id="s8:l6" met="4" type="-+">Sandt, haer ten nut, een man <rhyme label="c" type="m" corresp="#s8:l5">voorheen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">’tWas Ioseph dieder wierd <rhyme label="a" type="f">gesonden</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Als slaeff verkocht, als schelm <rhyme label="a" type="f" corresp="#s9:l1">gebonden</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">De kluyster heeft sijn voet <rhyme label="b" type="m">geprangt</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Het yser sijne ziel <rhyme label="b" type="m" corresp="#s9:l3">beangt</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Tot dat sijn rede <rhyme label="c" type="m">(na-vertelt</rhyme>)</l>
        <l id="s9:l6" met="4" type="-+">Godts in-spraeck in hem heeft <rhyme label="c" type="m" corresp="#s9:l5">gemeldt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De volcker-heer sendt yligh <rhyme label="a" type="f">henen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">De Koning selfs ontboeyt sijn <rhyme label="a" type="f" corresp="#s10:l1">schenen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Stelt hem (onlangs een snooden <rhyme label="b" type="m">slaeff</rhyme>)</l>
        <l id="s10:l4" met="4" type="-+">Ten vollen Heer van huys en <rhyme label="b" type="m" corresp="#s10:l3">haeff</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Ten hoofden hoofdt, ten Raden <rhyme label="c" type="m">Raedt</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Ten wijsheydts mondt in doen en <rhyme label="c" type="m" corresp="#s10:l5">laet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Als Israël dit had <rhyme label="a" type="f">vernomen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Verplaetst’ hy sich, en is <rhyme label="a" type="f" corresp="#s11:l1">gekomen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Int rijck’ Egypten, ’tlandt van <rhyme label="b" type="m">Cham</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Daer hy als vreemdling wooning <rhyme label="b" type="m" corresp="#s11:l3">nam</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">En d’haters (door Godts <rhyme label="c" type="m">segening</rhyme>)</l>
        <l id="s11:l6" met="4" type="-+">In tal en kracht te boven <rhyme label="c" type="m" corresp="#s11:l5">ging</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Geluck volght nijdt. De Heer <rhyme label="a" type="f">gehengde</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Dat niet alleen de gunst sich <rhyme label="a" type="f" corresp="#s12:l1">engde</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Maer oock het hert gants om ging <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Om, haets- en quaedts-gesint, <rhyme label="b" type="m" corresp="#s12:l3">voortaen</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Sijn volck, sijn’ knechten, in den <rhyme label="c" type="m">grondt</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Te helpen door een loosen <rhyme label="c" type="m" corresp="#s12:l5">vondt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Noch mist de list. Godt, haer <rhyme label="a" type="f">behoeder</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Sendt sijn knecht Mose, met sijn <rhyme label="a" type="f" corresp="#s13:l1">broeder</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Aaron, een verkoren <rhyme label="b" type="m">paer</rhyme>.</l>
        <l id="s13:l4" met="4" type="-+">Die, door veel wond’ren, naeckt en <rhyme label="b" type="m" corresp="#s13:l3">klaer</rhyme></l>
        <l id="s13:l5" met="4" type="-+">In’t landt van Cham den harden <rhyme label="c" type="m">lien</rhyme></l>
        <l id="s13:l6" met="4" type="-+">Haers aen-segs wisheydt deden <rhyme label="c" type="m" corresp="#s13:l5">sien</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Met duysternis quam hyse <rhyme label="a" type="f">plagen</rhyme>;</l>
        <l id="s14:l2" met="4" type="-+">Tot nachten wierden haer de <rhyme label="a" type="f" corresp="#s14:l1">dagen</rhyme>.</l>
        <l id="s14:l3" met="4" type="-+">Sijn’ knechten hebben op sijn <rhyme label="b" type="m">last</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Met onvermoeyden vlijt <rhyme label="b" type="m" corresp="#s14:l3">gepast</rhyme>.</l>
        <l id="s14:l5" met="4" type="-+">Haer water maeckt’ hy enckel <rhyme label="c" type="m">bloedt</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">Haer visschen krengen in den <rhyme label="c" type="m" corresp="#s14:l5">vloedt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Haer landt gaff vorschen, die met <rhyme label="a" type="f">hoopen</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Des Konings slaep-vertreck <rhyme label="a" type="f" corresp="#s15:l1">bekropen</rhyme>.</l>
        <l id="s15:l3" met="4" type="-+">Hy sprack: en op sijn machtigh <rhyme label="b" type="m">woordt</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Quam boos gewormt te grimmel <rhyme label="b" type="m" corresp="#s15:l3">voort</rhyme>.</l>
        <l id="s15:l5" met="4" type="-+">De luys, het wal-gelijck <rhyme label="c" type="m">gediert</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Heeft alle hoeck en plaets <rhyme label="c" type="m" corresp="#s15:l5">door-swiert</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Voor regen sandt hy hagel <rhyme label="a" type="f">neder</rhyme>.</l>
        <l id="s16:l2" met="4" type="-+">Vuyr-vlammen straelden heen en <rhyme label="a" type="f" corresp="#s16:l1">weder</rhyme></l>
        <l id="s16:l3" met="4" type="-+">Door’t goddeloos verharde <rhyme label="b" type="m">landt</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Den aengename <rhyme label="b" type="m" corresp="#s16:l3">druyven-plant</rhyme>,</l>
        <l id="s16:l5" met="4" type="-+">Den vijgeboom smeet hy ter <rhyme label="c" type="m">ne’er</rhyme></l>
        <l id="s16:l6" met="4" type="-+">En alle hout in’t woudt om <rhyme label="c" type="m" corresp="#s16:l5">veer</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Noch eenmael sprack de <rhyme label="a" type="f">plagen-gever</rhyme>:</l>
        <l id="s17:l2" met="4" type="-+">Strax quam de sprinckhaen met de <rhyme label="a" type="f" corresp="#s17:l1">kever</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">En vrat, ontellijck in <rhyme label="b" type="m">getal</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Des landes vruchten all’ end’ <rhyme label="b" type="m" corresp="#s17:l3">all</rhyme>’.</l>
        <l id="s17:l5" met="4" type="-+">In’t endt, met algemeenen <rhyme label="c" type="m">moordt</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">Quam hy op alle <rhyme label="c" type="m" corresp="#s17:l5">eerst’-geboort</rhyme>’.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Dit was het slot der <rhyme label="a" type="f">wonderdaden</rhyme>.</l>
        <l id="s18:l2" met="4" type="-+">Sijn volck met rijcken buyt <rhyme label="a" type="f" corresp="#s18:l1">geladen</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Heeft hy ten laetsten <rhyme label="b" type="m">uyt-gebracht</rhyme>.</l>
        <l id="s18:l4" met="4" type="-+">En geen van dit bemint <rhyme label="b" type="m" corresp="#s18:l3">geslacht</rhyme></l>
        <l id="s18:l5" met="4" type="-+">(Terwijl Godts straff den boosen <rhyme label="c" type="m">sloegh</rhyme>)</l>
        <l id="s18:l6" met="4" type="-+">Die mede-last der plagen <rhyme label="c" type="m" corresp="#s18:l5">droegh</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Uyt waren sy; en bly, <rhyme label="a" type="f">Egipten</rhyme>.</l>
        <l id="s19:l2" met="4" type="-+">Geen liever ding dan datse <rhyme label="a" type="f" corresp="#s19:l1">glipten</rhyme></l>
        <l id="s19:l3" met="4" type="-+">Wiens bywoonst’ haer een <rhyme label="b" type="m">plagen-tas</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">Een eeuwigh stoff van grouw’len <rhyme label="b" type="m" corresp="#s19:l3">was</rhyme>.</l>
        <l id="s19:l5" met="4" type="-+">Sijn wolck heeft haer des daeghs <rhyme label="c" type="m">bedeckt</rhyme>,</l>
        <l id="s19:l6" met="4" type="-+">Sijn vuyr een nacht-geley <rhyme label="c" type="m" corresp="#s19:l5">verstreckt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Hy schafte Quackels’ does’ hem <rhyme label="a" type="f">baden</rhyme>,</l>
        <l id="s20:l2" met="4" type="-+">En Hemel-broodt tot haer <rhyme label="a" type="f" corresp="#s20:l1">versaden</rhyme>.</l>
        <l id="s20:l3" met="4" type="-+">Hy opend’ haer den harden <rhyme label="b" type="m">steen</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">En ’twater vloot met stroomen <rhyme label="b" type="m" corresp="#s20:l3">heen</rhyme></l>
        <l id="s20:l5" met="4" type="-+">Door’t dor en dorstigh <rhyme label="c" type="m">Heyden-zandt</rhyme></l>
        <l id="s20:l6" met="4" type="-+">In’t ongebouwt Arabisch <rhyme label="c" type="m" corresp="#s20:l5">landt</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Want hy gedacht aen’t <rhyme label="a" type="f">ver-voorleden</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">Aen’t bindt-woordt sijner <rhyme label="a" type="f" corresp="#s21:l1">heylicheden</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">Aen Abram sijn’ getrouwen <rhyme label="b" type="m">knecht</rhyme></l>
        <l id="s21:l4" met="4" type="-+">En’t bondt met hem eens <rhyme label="b" type="m" corresp="#s21:l3">op-gerecht</rhyme>.</l>
        <l id="s21:l5" met="4" type="-+">In triumphante <rhyme label="c" type="m">vrolijckheydt</rhyme></l>
        <l id="s21:l6" met="4" type="-+">Heeft hy sijn vrunden <rhyme label="c" type="m" corresp="#s21:l5">uyt-geleydt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Hy gaff haer, by ’tbesit der <rhyme label="a" type="f">landen</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">Den arbeydt van der volcken <rhyme label="a" type="f" corresp="#s22:l1">handen</rhyme>:</l>
        <l id="s22:l3" met="4" type="-+">Dat haerder elck in alle <rhyme label="b" type="m">trou</rhyme></l>
        <l id="s22:l4" met="4" type="-+">’tGeset sijns Wets bewaren <rhyme label="b" type="m" corresp="#s22:l3">sou</rhyme>’</l>
        <l id="s22:l5" met="4" type="-+">Na ’tvoorschrift van des waerheydts <rhyme label="c" type="m">leer</rhyme>.</l>
        <l id="s22:l6" met="4" type="-+">Loff zy den alderhooghsten <rhyme label="c" type="m" corresp="#s22:l5">Heer</rhyme>.</l>
      </lg>
    </lg>
