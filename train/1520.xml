    <melody>Het reed een ruitertje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIt gae ick nu in danck <rhyme label="a" type="f">ontfangen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">DIt gae ick nu in danck <rhyme label="a" type="f" corresp="#s1:l1">ontfangen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Om minlijck voort te gaen <rhyme label="a" type="f" corresp="#s1:l1">langen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Een Dochter hier, seer <rhyme label="b" type="m">fier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick hoop sy sal het van my <rhyme label="a" type="f">wachten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ick hoop sy sal het van my <rhyme label="a" type="f" corresp="#s2:l1">wachten</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hoe wel tot my hare <rhyme label="a" type="f" corresp="#s2:l1">ghedachten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Op dit termijn, niet <rhyme label="b" type="m">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick gae u schoon liefken eens <rhyme label="a" type="f">brengen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ick gae u schoon liefken eens <rhyme label="a" type="f" corresp="#s3:l1">brengen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ick hoop ghy sultet <rhyme label="a" type="f" corresp="#s3:l1">ghehengen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Op dese tijdt, met <rhyme label="b" type="m">vlijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hout daer o schoonste der <rhyme label="a" type="f">Godinnen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hout daer o schoonste der <rhyme label="a" type="f" corresp="#s4:l1">Godinnen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ontfanghter dit Kroesken uyt <rhyme label="a" type="f" corresp="#s4:l1">minnen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Van u dienaer // <rhyme label="b" type="m">eerbaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Princesse laet nu liefde <rhyme label="a" type="f">groeyen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Princesse laet nu liefde <rhyme label="a" type="f" corresp="#s5:l1">groeyen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Op dat ick in liefde mach <rhyme label="a" type="f" corresp="#s5:l1">bloeyen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Want liefd' baert konst // door <rhyme label="b" type="m">gonst</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
