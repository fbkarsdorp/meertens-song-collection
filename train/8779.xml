    <melody>Wo Gott der Herr nicht bei uns h√§lt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ACh HEEr wat syn der Vyand' <rhyme label="a" type="m">veel</rhyme></l>
        <l id="s1:l2" met="3" type="-+">End' veel my teghen <rhyme label="b" type="f">streven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Veel segghen nu van mijner <rhyme label="a" type="m" corresp="#s1:l1">Siel</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Godt heeft hem al <rhyme label="b" type="f" corresp="#s1:l2">begheven</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Maer ghy Heer zijt voor my den <rhyme label="c" type="m">Schilt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die my ter eeren helpen <rhyme label="c" type="m" corresp="#s1:l5">wilt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En doet mijn hooft op <rhyme label="d" type="f">rechten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mijn stem den Heer aenroepen <rhyme label="a" type="m">sal</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">End' hy sal my <rhyme label="b" type="f">verhooren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ick slaep gherust, ontwaeck oock <rhyme label="a" type="m" corresp="#s2:l1">wel</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gheen Vyandt mach my <rhyme label="b" type="f" corresp="#s2:l2">stooren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Want Godt de Heer my selfs <rhyme label="c" type="m">onthout</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Al haten my veel duysent <rhyme label="c" type="m" corresp="#s2:l5">stout</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Noch vreest hem niet mijn <rhyme label="d" type="f">herte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Helpt Heer mijn Godt, aenvaert die <rhyme label="a" type="m">saeck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Redd' my van mijn <rhyme label="b" type="f">Vyanden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ghy slaet den Vyant op syn <rhyme label="a" type="m" corresp="#s3:l1">kaeck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ghy breekt der Godloos' <rhyme label="b" type="f" corresp="#s3:l2">tanden</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dijn hulp is wel te vinden <rhyme label="c" type="m">HEEr</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Dijn Segen gaet noch ymmer <rhyme label="c" type="m" corresp="#s3:l5">meer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dijn Volck machs' al <rhyme label="d" type="f">ghenieten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
