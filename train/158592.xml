      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GY Prince van <rhyme label="a" type="f">Oranje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gesproten uit het <rhyme label="b" type="m">Bloed</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van hun, die ’t Iuk van <rhyme label="a" type="f" corresp="#s1:l1">Spanje</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Af wierpen, door hun <rhyme label="b" type="m" corresp="#s1:l2">Moed</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Gy zult ons nu <rhyme label="a" type="f" corresp="#s1:l1">bevryden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van ’t uiterste <rhyme label="b" type="m" corresp="#s1:l2">gevaar</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat ons van alle <rhyme label="a" type="f" corresp="#s1:l1">zyden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Omringt en dreigt ons <rhyme label="b" type="m" corresp="#s1:l2">zwaar</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Uw Hand zal ons <rhyme label="a" type="f">Behoeden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gesterkt met ‘s Hemels <rhyme label="b" type="m">magt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En Stryden met de <rhyme label="a" type="f" corresp="#s2:l1">Goeden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Voor ’t Vaderland met <rhyme label="b" type="m" corresp="#s2:l2">kragt</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Den Koning, uw Heer <rhyme label="a" type="f" corresp="#s2:l1">Vader</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Uw Broeder sterk te <rhyme label="b" type="m" corresp="#s2:l2">Veld</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Zal zyn uw Hulp en <rhyme label="a" type="f" corresp="#s2:l1">Rader</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Tot afbreuk van ‘t <rhyme label="b" type="m" corresp="#s2:l2">Geweld</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Des Vyands, die <rhyme label="a" type="f">Hoogmoedig</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Nu stoft, dat hy ons <rhyme label="b" type="m">zal</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Verdelgen wreed en <rhyme label="a" type="f" corresp="#s3:l1">spoedig</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ons brengen tot den <rhyme label="b" type="m" corresp="#s3:l2">Val</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Dat hy haast uit zal <rhyme label="a" type="f" corresp="#s3:l1">Roeijen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">De Vryheid van het <rhyme label="b" type="m" corresp="#s3:l2">Land</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">De Godsdienst zo <rhyme label="a" type="f" corresp="#s3:l1">Besnoeijen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ia leggen aan een <rhyme label="b" type="m" corresp="#s3:l2">Band</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Van zulken, die graag <rhyme label="a" type="f">Plagen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gods uitverkooren <rhyme label="b" type="m">Zaat</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Maar God zal ’t niet <rhyme label="a" type="f" corresp="#s4:l1">verdragen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat men zyn Volk zo <rhyme label="b" type="m" corresp="#s4:l2">Slaat</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">De Zeeuwsche <rhyme label="a" type="f" corresp="#s4:l1">Batavieren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Zyn ons eerst voor <rhyme label="b" type="m" corresp="#s4:l2">gegaan</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">d’Orange Wimpels <rhyme label="a" type="f" corresp="#s4:l1">zwieren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En op ons Toorens <rhyme label="b" type="m" corresp="#s4:l2">staan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Heer der <rhyme label="a" type="f">Heyerscharen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Zal door dees <rhyme label="b" type="m">Zonneschyn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Nevels op doen <rhyme label="a" type="f" corresp="#s5:l1">klaren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En elk eendragtig <rhyme label="b" type="m" corresp="#s5:l2">zyn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Om Goed en Bloed te <rhyme label="a" type="f" corresp="#s5:l1">wagen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Voor ’t lieve <rhyme label="b" type="m" corresp="#s5:l2">Vaderland</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Orange zal ons <rhyme label="a" type="f" corresp="#s5:l1">dragen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met Liefd’ en met <rhyme label="b" type="m" corresp="#s5:l2">Verstand</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dan zal de Godsdienst <rhyme label="a" type="f">Bloeijen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">De Vryheid en de <rhyme label="b" type="m">Vree</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">De Welvaart daaglyks <rhyme label="a" type="f" corresp="#s6:l1">groeijen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ter Land en ook ter <rhyme label="b" type="m" corresp="#s6:l2">Zee</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Den Vyand zal weer <rhyme label="a" type="f" corresp="#s6:l1">Vlugten</rhyme></l>
        <l id="s6:l6" met="3" type="-+">En heenen gaan met <rhyme label="b" type="m" corresp="#s6:l2">schand</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Wy hebben niet te <rhyme label="a" type="f" corresp="#s6:l1">Dugten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Nu God den Standaard <rhyme label="b" type="m" corresp="#s6:l2">plant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy schenkt aan ons zyn <rhyme label="a" type="f">Zegen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Tot Troost van Kerk en <rhyme label="b" type="m">Staat</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Terwyl dat wy <rhyme label="a" type="f" corresp="#s7:l1">verkregen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ons hoop en <rhyme label="b" type="m" corresp="#s7:l2">toeverlaat</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Den Prince van <rhyme label="a" type="f" corresp="#s7:l1">Oranje</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Kom, Burgers, schep nu <rhyme label="b" type="m" corresp="#s7:l2">Moed</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Gedenk aan ’t Iuk van <rhyme label="a" type="f" corresp="#s7:l1">Spanje</rhyme>;</l>
        <l id="s7:l8" met="3" type="-+">God altyd Wond’ren <rhyme label="b" type="m" corresp="#s7:l2">doed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
