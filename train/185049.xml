<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185049</id>
    <date>
      <year-start>1703</year-start>
      <year-end>1703</year-end>
    </date>
    <tags/>
    <melody>Het is te Gent nu al verkeerd</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE kans is nu geheel <rhyme label="a" type="m">verkeerd</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">De kans is nu geheel <rhyme label="a" type="m" corresp="#s1:l1">verkeerd</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Van Heussen is <rhyme label="a" type="m" corresp="#s1:l1">gesuspendeerd</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s1:l5" met="6" type="-+">Van Heussen is gesuspendeerd, is dat niet <rhyme label="b" type="m" corresp="#s1:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Groenhout is ook <rhyme label="a" type="m">gesuspendeerd</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Groenhout is ook <rhyme label="a" type="m" corresp="#s2:l1">gesuspendeerd</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Men hem voor geen Vicaris <rhyme label="a" type="m" corresp="#s2:l1">eerd</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s2:l5" met="6" type="-+">Men hem voor geen Vicaris eerd, is dat niet <rhyme label="b" type="m" corresp="#s2:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Swaan, in plaats van zingen, <rhyme label="a" type="m">kryt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">De Swaan, in plaats van zingen, <rhyme label="a" type="m" corresp="#s3:l1">kryt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En Kats die is zyn klauwen <rhyme label="a" type="m" corresp="#s3:l1">quyt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s3:l5" met="6" type="-+">En Kats die is zijn klauwen quyt, is dat niet <rhyme label="b" type="m" corresp="#s3:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Van Erkel is zyn geldje <rhyme label="a" type="m">quyt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Van Erkel is zyn geldje <rhyme label="a" type="m" corresp="#s4:l1">quyt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Daar by zijn eer, dat hem wel <rhyme label="a" type="m" corresp="#s4:l1">spyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s4:l5" met="6" type="-+">Daar by zyn eer, dat hem wel spyt, is dat niet <rhyme label="b" type="m" corresp="#s4:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Het gasten op het <rhyme label="a" type="m">Toorenvliet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Het gasten op het <rhyme label="a" type="m" corresp="#s5:l1">Toorenvliet</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Sal nu wel haast heel gaan te <rhyme label="a" type="m" corresp="#s5:l1">niet</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s5:l5" met="6" type="-+">Sal nu wel haast heel gaan te niet, is dat niet <rhyme label="b" type="m" corresp="#s5:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Staate bokkings smaaken <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">De Staate bokkings smaaken <rhyme label="a" type="m" corresp="#s6:l1">wel</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Janseenis bend' is in de <rhyme label="a" type="m" corresp="#s6:l1">knel</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s6:l5" met="6" type="-+">Janseenis bend' is in de knel, is dat niet <rhyme label="b" type="m" corresp="#s6:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Al maakt van Heussen groot <rhyme label="a" type="m">gehuyl</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Al maakt van Heussen groot <rhyme label="a" type="m" corresp="#s7:l1">gehuyl</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">'t Is om dat al zyn kraam is <rhyme label="a" type="m" corresp="#s7:l1">vuyl</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s7:l5" met="6" type="-+">'t Is om dat al zyn kraam is vuyl, is dat niet <rhyme label="b" type="m" corresp="#s7:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Al is hy nog zo <rhyme label="a" type="m">Jansenist</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Al is hy nog zo <rhyme label="a" type="m" corresp="#s8:l1">Jansenist</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Men ziet zijn opgang is <rhyme label="a" type="m" corresp="#s8:l1">gemist</rhyme>,&gt;</l>
        <l id="s8:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s8:l5" met="6" type="-+">Men ziet zijn opgang is gemist, is dat niet <rhyme label="b" type="m" corresp="#s8:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Al kreeg Heer Kok geen kist met <rhyme label="a" type="m">geld</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Al kreeg Heer Kok geen kist met <rhyme label="a" type="m" corresp="#s9:l1">geld</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Toen hem 's Paus Bulle wierd <rhyme label="a" type="m" corresp="#s9:l1">besteld</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s9:l5" met="6" type="-+">Toen hem 's Paus Bulle is besteld, 't is egter <rhyme label="b" type="m" corresp="#s9:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hy is gesteld in Coddens <rhyme label="a" type="m">ampt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Hy is gesteld in Coddens <rhyme label="a" type="m" corresp="#s10:l1">ampt</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Van Heussen had daar op <rhyme label="a" type="m" corresp="#s10:l1">gevlamd</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s10:l5" met="6" type="-+">Van Heussen had daar op gevlamd, is dat niet <rhyme label="b" type="m" corresp="#s10:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Koks Son die ryst nog meer en <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Koks Son die ryst nog meer en <rhyme label="a" type="m" corresp="#s11:l1">meer</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Klaar blinkt de Roomse leer nu <rhyme label="a" type="m" corresp="#s11:l1">weer</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s11:l5" met="6" type="-+">Klaar blinkt de Roomse leer nu weer, is dat niet <rhyme label="b" type="m" corresp="#s11:l4">fray</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Nu zal ons lieve <rhyme label="a" type="m">Vaderland</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Nu zal ons lieve <rhyme label="a" type="m" corresp="#s12:l1">Vaderland</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Wel haast zyn in geruste <rhyme label="a" type="m" corresp="#s12:l1">stand</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Sobbedomdyne van <rhyme label="b" type="m">Falalai</rhyme>,</l>
        <l id="s12:l5" met="6" type="-+">Wel haast zyn in geruste stand, is dat niet <rhyme label="b" type="m" corresp="#s12:l4">fray</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
