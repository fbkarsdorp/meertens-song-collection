    <tags/>
    <melody>Hoor de toon der nachtegalen</melody>
  <author>Antoine Mahaut, Kornelis Elzevier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Hoor dentoonder <rhyme label="a" type="f">Nachtegalen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Nu eens ryzen dan eens <rhyme label="a" type="f" corresp="#s1:l1">dalen</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Niets verwint de schelle <rhyme label="b" type="m">keel</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Van de kleine <rhyme label="b" type="m" corresp="#s1:l3">Filomeel</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Al de Boomen krygen <rhyme label="c" type="f">Ooren</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Als zy haer gezang doet <rhyme label="c" type="f" corresp="#s1:l5">hooren</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Al de Bloemenal de <rhyme label="d" type="m">Blaên</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Ziet men zelf verwonderd <rhyme label="d" type="m" corresp="#s1:l7">staen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ô hoe lieflyk zwelt haer <rhyme label="a" type="f">kropje</rhyme>!</l>
        <l id="s2:l2" met="4" type="+-">Hoe verheft ze 't moedig <rhyme label="a" type="f" corresp="#s2:l1">kopje</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als zy zingt op eenen <rhyme label="b" type="m">tak</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Overdekt door 't blaedrig <rhyme label="b" type="m" corresp="#s2:l3">dak</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Als de galm komt luchtwaerts <rhyme label="c" type="f">stygen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Doet zy al de Vogels <rhyme label="c" type="f" corresp="#s2:l5">zwygen</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Zy verwint al 't pluim <rhyme label="d" type="m">gediert</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Dat door dichte telgjes <rhyme label="d" type="m" corresp="#s2:l7">zwiert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doch schoon zy met hare <rhyme label="a" type="f">toonen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Yder kan naer buiten <rhyme label="a" type="f" corresp="#s3:l1">troonen</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Myn beminde <rhyme label="b" type="m">Klorimeen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Kan met haer in't Strydperk <rhyme label="b" type="m" corresp="#s3:l3">treên</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Ia zy gaet haer ver te <rhyme label="c" type="f">boven</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Zy kan haren luister <rhyme label="c" type="f" corresp="#s3:l5">dooven</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Sints zy sprak met blyden <rhyme label="d" type="m">zin</rhyme>:</l>
        <l id="s3:l8" met="4" type="+-">Damon is het dien ik <rhyme label="d" type="m" corresp="#s3:l7">min</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
