<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>23562</id>
    <date>
      <year-start>1572</year-start>
      <year-end>1572</year-end>
    </date>
    <location>
      <orginal-name>antwerpen [=alkmaar]</orginal-name>
      <standard-name>Alkmaar, The Netherlands</standard-name>
      <coordinates>
        <lon>52.6312592</lon>
        <lat>4.7503798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Gelobt seist du Jesu Christ</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Gelovet zijtstu Iesu <rhyme label="a" type="m">Christ</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat du mensche gheboren <rhyme label="a" type="m" corresp="#s1:l1">bist</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Van der Jonc-vrou dat is <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Des vrouwet sich aller <rhyme label="b" type="m" corresp="#s1:l3">Enghelenschaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+"><rhyme label="b" type="m" corresp="#s1:l3">Kyrieleyson</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Des eewiges Vaders eenige <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Nu men in der Cribben <rhyme label="a" type="m" corresp="#s2:l1">vint</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In onse arme vleesch ende <rhyme label="b" type="m">bloet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vercledet sich dat eeuwighe <rhyme label="b" type="m" corresp="#s2:l3">goet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+"><rhyme label="b" type="m" corresp="#s2:l3">Kyrieleyson</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den aller weerelt Kreysz niet <rhyme label="a" type="m">besloot</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die licht in Marien <rhyme label="a" type="m" corresp="#s3:l1">schoot</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">hy is een kindeken geworden <rhyme label="b" type="m">cleyn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die alle dinck schout <rhyme label="b" type="m" corresp="#s3:l3">alleyn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+"><rhyme label="b" type="m" corresp="#s3:l3">Kyrieleyson</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat eeuwige licht gaet daer her <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">geeft der werelt een nijen <rhyme label="a" type="m" corresp="#s4:l1">schijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">het luchtet wel midden in der <rhyme label="b" type="m">nacht</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ende ons des lichtes kinderen <rhyme label="b" type="m" corresp="#s4:l3">maect</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+"><rhyme label="b" type="m" corresp="#s4:l3">Kyrieleyson</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die Sone des Vaders goet van <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Een gast in de werelt <rhyme label="a" type="m" corresp="#s5:l1">weert</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ende voerde ons uyt den <rhyme label="b" type="m">jammerdal</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">hy maket ons erven in zijnen <rhyme label="b" type="m" corresp="#s5:l3">sael</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+"><rhyme label="b" type="m" corresp="#s5:l3">Kyrieleyson</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy is op aerden gecomen <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat hy onser sich <rhyme label="a" type="m" corresp="#s6:l1">erbarm</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ende in den hemel makende <rhyme label="b" type="m">rijc</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Ende zijnen lieven Engelen <rhyme label="b" type="m" corresp="#s6:l3">ghelijck</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+"><rhyme label="b" type="m" corresp="#s6:l3">Kyrieleyson</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dz heeft hy ons alle <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Sijn groote liefde te <rhyme label="a" type="m" corresp="#s7:l1">toonen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">des vrouwet sich alle <rhyme label="b" type="m">Christenheyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Ende dancke hem des in <rhyme label="b" type="m" corresp="#s7:l3">eeuwigheyt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+"><rhyme label="b" type="m" corresp="#s7:l3">Kyrieleyson</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
