  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SOete Nimphje, lief <rhyme label="a" type="f">Lerinde</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Waer heen met u wolligh <rhyme label="b" type="m">Vee</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Schoon Dorastus (Mijn <rhyme label="a" type="f" corresp="#s1:l1">beminde</rhyme>)</l>
        <l id="s1:l4" met="4" type="+-">Na de Weyden, aen de <rhyme label="b" type="m" corresp="#s1:l2">Zee</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Om mijn soete Geytjes, <rhyme label="c" type="m">elck</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Daer te loosen van haer <rhyme label="c" type="m" corresp="#s1:l5">Melck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Neen, mijn Pronck der <rhyme label="a" type="f">Herderinnen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Ghy wilt na u Cloris <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Die ghy liever wilt <rhyme label="a" type="f" corresp="#s2:l1">beminnen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Soo ick lestmael heb <rhyme label="b" type="m" corresp="#s2:l2">verstaen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Wt een Brief die ghy hem <rhyme label="c" type="m">sant</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Doen ick doolden langhs de <rhyme label="c" type="m" corresp="#s2:l5">Strant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ach! Dorastus (waerde <rhyme label="a" type="f">leven</rhyme>,)</l>
        <l id="s3:l2" met="4" type="+-">Hemel! Watter al <rhyme label="b" type="m">gheschiet</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Noyt en heb ick Brief <rhyme label="a" type="f" corresp="#s3:l1">geschreven</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Noch aen hem of niemandt <rhyme label="b" type="m" corresp="#s3:l2">niet</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Maer ick zweer! dat hert, en <rhyme label="c" type="m">ziel</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Ick alleen voor u <rhyme label="c" type="m" corresp="#s3:l5">behiel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wel dat laet ick daer by <rhyme label="a" type="f">blyven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Rust wat neder by 't <rhyme label="b" type="m">Geboomt</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Onder 't schaduw der <rhyme label="a" type="f" corresp="#s4:l1">Olijven</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Waer dat een Reviertje <rhyme label="b" type="m" corresp="#s4:l2">stroomt</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Met sijn golfjens in het <rhyme label="c" type="m">Dal</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Daer 'k u Vlockje melcken <rhyme label="c" type="m" corresp="#s4:l5">sal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Neen ick weet een plaets <rhyme label="a" type="f">bequamer</rhyme></l>
        <l id="s5:l2" met="4" type="+-">By een water-Borne <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Dat ist Vee veel <rhyme label="a" type="f" corresp="#s5:l1">aengenamer</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Want sy drincken uyt de <rhyme label="b" type="m" corresp="#s5:l2">Vloet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">En dit water schoon en <rhyme label="c" type="m">reyn</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Spruyt uyt eene klaer <rhyme label="c" type="m" corresp="#s5:l5">Fonteyn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daer ist koel want d' hooge <rhyme label="a" type="f">Eycken</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Die met haer bebladert <rhyme label="b" type="m">lof</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Doen haer top ten Hemel <rhyme label="a" type="f" corresp="#s6:l1">reycken</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Snyt de Son haer stralen <rhyme label="b" type="m" corresp="#s6:l2">of</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Als ick dan in 't schaduw <rhyme label="c" type="m">sta</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Voel 'k den soeten windt <rhyme label="c" type="m" corresp="#s6:l5">Aura</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wel (mijn Lief) ick sal u <rhyme label="a" type="f">leyden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Drijft mijn Schaepjens neffen d' <rhyme label="b" type="m">uw</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Wilt u Vee alleenigh <rhyme label="a" type="f" corresp="#s7:l1">Weyden</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Want mijn Vee dat is te <rhyme label="b" type="m" corresp="#s7:l2">schuw</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Neen Lerinde, 't is als 't <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">'t Wil wel by malkanderen <rhyme label="c" type="m" corresp="#s7:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Stil Dorastus 'k hoor <rhyme label="a" type="f">geruchten</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Vliet van my, 'k moet henen <rhyme label="b" type="m">gaen</rhyme>.</l>
        <l id="s8:l3" met="4" type="+-">Ach! mijn Lief, waer wilt ghy <rhyme label="a" type="f" corresp="#s8:l1">vluchten</rhyme>?</l>
        <l id="s8:l4" met="4" type="+-">'t Is maer ruysig' van de <rhyme label="b" type="m" corresp="#s8:l2">blaen</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Of het vlugge <rhyme label="c" type="m">Veer-gebroet</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Dat sijn jonge Beesjes <rhyme label="c" type="m" corresp="#s8:l5">voet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Siet hoe kan Dorastus <rhyme label="a" type="f">vleyen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">En sijn lusjes weet te <rhyme label="b" type="m">voen</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Dat een Maeghden hert sou <rhyme label="a" type="f" corresp="#s9:l1">schreyen</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Die hem eenigh leet sagh <rhyme label="b" type="m" corresp="#s9:l2">doen</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Ey! Lerinde, ey! een <rhyme label="c" type="m">kus</rhyme>;</l>
        <l id="s9:l6" met="4" type="+-">Gunt dat ick mijn vlammen <rhyme label="c" type="m" corresp="#s9:l5">blus</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Wel Dorastus, mijn <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Sweer my dat u trouwe <rhyme label="b" type="m">Trouw</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Ach! mijn Lief, mijn Lief <rhyme label="a" type="f" corresp="#s10:l1">Lerinde</rhyme></l>
        <l id="s10:l4" met="4" type="+-">'k Sweer u: eer den Hemel <rhyme label="b" type="m" corresp="#s10:l2">sou</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Van sijn gulde Polen <rhyme label="c" type="m">vlien</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Datmen 't blaeuw Gewelf sou <rhyme label="c" type="m" corresp="#s10:l5">sien</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Of, al quam my daer een <rhyme label="a" type="f">bieden</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Voor dees Staf, een Scepter <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Dat ick Lant, en u souw <rhyme label="a" type="f" corresp="#s11:l1">vlieden</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">En gaf my een Koninghs <rhyme label="b" type="m" corresp="#s11:l2">Croon</rhyme></l>
        <l id="s11:l5" met="4" type="+-">Voor dees stroyen Hoet, soo <rhyme label="c" type="m">souw</rhyme></l>
        <l id="s11:l6" met="4" type="+-">Ick 't af-slaen, om u mijn <rhyme label="c" type="m" corresp="#s11:l5">Vrouw</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">En terwijl ons herten <rhyme label="a" type="f">branden</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Laet ons na den Tempel <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">En doen Venus <rhyme label="a" type="f" corresp="#s12:l1">Offerhanden</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Knielen oock voor Son en <rhyme label="b" type="m" corresp="#s12:l2">Maen</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Bidden voor den <rhyme label="c" type="m">Echten-staet</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Dats' ons komen bey te <rhyme label="c" type="m" corresp="#s12:l5">baet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
