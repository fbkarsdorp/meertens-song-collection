    <melody>Psalm 068 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk danck van gantscher Herten <rhyme label="a" type="m">u</rhyme>:</l>
        <l id="s1:l2" met="4" type="-+">Mijn Godt, mijn Heer, en wil oock <rhyme label="a" type="m" corresp="#s1:l1">nu</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dijn Wonder veel <rhyme label="b" type="f">vertellen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ghy Alderhoochst' u Naem ick <rhyme label="c" type="m">bly</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wil Loven en verheughen <rhyme label="c" type="m" corresp="#s1:l4">my</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Want ghy te Rug doet <rhyme label="b" type="f" corresp="#s1:l3">vellen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Mijn Vyanden, voor t' Aensicht <rhyme label="d" type="m">dijn</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Ghevallen end' omcomen <rhyme label="d" type="m" corresp="#s1:l7">syn</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Recht cont ghy haer <rhyme label="e" type="f">vergelden</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Want ghy (die al mijn saecken <rhyme label="f" type="m">slecht</rhyme>)</l>
        <l id="s1:l11" met="4" type="+-">Sitt op den Soel een Rechter <rhyme label="f" type="m" corresp="#s1:l10">recht</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Die Heyden doet ghy <rhyme label="e" type="f" corresp="#s1:l9">schelden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die God'loos hebt ghy doen <rhyme label="a" type="m">vergaen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Met haer, heur Namen wt <rhyme label="a" type="m" corresp="#s2:l1">ghedaen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Datmens niet meer sal <rhyme label="b" type="f">dincken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Het sweert des Vyandts heeft een <rhyme label="c" type="m">eynt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Haer steden hebt ghy om <rhyme label="c" type="m" corresp="#s2:l4">gheweynt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">End' eeuwich doen <rhyme label="b" type="f" corresp="#s2:l3">versincken</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Der Godloosen <rhyme label="d" type="m">ghedachtenis</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Met haer t'eenmael verganghen <rhyme label="d" type="m" corresp="#s2:l7">is</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Maer eeuwich blijft die <rhyme label="e" type="f">HEERE</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Syn Stoel hy t' Oord'len heeft <rhyme label="f" type="m">bereyt</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Hy recht d'Aerd met <rhyme label="f" type="m" corresp="#s2:l10">gherechticheyt</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">T'volck hy oprecht <rhyme label="e" type="f" corresp="#s2:l9">regieret</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De HEER die is des Armen <rhyme label="a" type="m">schut</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Een Schilt ter noodt, in angst seer <rhyme label="a" type="m" corresp="#s3:l1">nut</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dies sullen op u <rhyme label="b" type="f">hopen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die uwen Naem (Heer) kennen <rhyme label="c" type="m">recht</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Want gheensins ghy verlaet den <rhyme label="c" type="m" corresp="#s3:l4">Knecht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die u heeft na <rhyme label="b" type="f" corresp="#s3:l3">gheloopen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Nu looft den HEER die t'Zion <rhyme label="d" type="m">woont</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Vercond' den volck, en haer <rhyme label="d" type="m" corresp="#s3:l7">verthoont</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Syn doen en syn <rhyme label="e" type="f">ontfermen</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Want hy ghedenckt en vraecht na <rhyme label="f" type="m">t'bloet</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Der Vromen, en vergheten <rhyme label="f" type="m" corresp="#s3:l10">doet</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Des schreyens niet der <rhyme label="e" type="f" corresp="#s3:l9">Armen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zijt mijns ghenadich, siet en <rhyme label="a" type="m">kent</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Onder den Vyandt mijn <rhyme label="a" type="m" corresp="#s4:l1">ellent</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghy die my hebt <rhyme label="b" type="f">verheven</rhyme></l>
        <l id="s4:l4" met="4" type="-+">(Heer) wt die poorten van den <rhyme label="c" type="m">Doot</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Op dat mijn stem v lof maeck <rhyme label="c" type="m" corresp="#s4:l4">groot</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">(Soo langh als ick sal <rhyme label="b" type="f" corresp="#s4:l3">leuen</rhyme>)</l>
        <l id="s4:l7" met="4" type="-+">In Zions Dochters dueren <rhyme label="d" type="m">vry</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Dat ick my uwer hulp <rhyme label="d" type="m" corresp="#s4:l7">verbly</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">T'syn in den Cuyl <rhyme label="e" type="f">ghevallen</rhyme></l>
        <l id="s4:l10" met="4" type="-+">(Die Heyden) die sy graefden <rhyme label="f" type="m">mijn</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">En selfs int Net ghevallen <rhyme label="f" type="m" corresp="#s4:l10">zijn</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Dat sy my spanden <rhyme label="e" type="f" corresp="#s4:l9">allen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Soo kent men dat de Heer recht <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Verstrickt is des Godloosen <rhyme label="a" type="m" corresp="#s5:l1">voet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Door t'Woort int werck syns <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die Godloos moeten al ter <rhyme label="c" type="m">Hel</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Die Heyden die hem zijn <rhyme label="c" type="m" corresp="#s5:l4">rebel</rhyme>.</l>
        <l id="s5:l6" met="3" type="-+">Al in de Helsche <rhyme label="b" type="f" corresp="#s5:l3">branden</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">En die vergheten Godt den <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Want hy hem tot den Armen <rhyme label="d" type="m" corresp="#s5:l7">keer</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Wil die niet gantsch <rhyme label="e" type="f">vergheten</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">En der Elenden Hope <rhyme label="f" type="m">syn</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Moet Eewich niet verloren <rhyme label="f" type="m" corresp="#s5:l10">zijn</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Maer t'wort haer wel toe <rhyme label="e" type="f" corresp="#s5:l9">meten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dat d' Overhandt (O Heer <rhyme label="a" type="m">opsiet</rhyme>)</l>
        <l id="s6:l2" met="4" type="-+">Die Menschen bidd' ick) crijgen <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En laet veroordeelt <rhyme label="b" type="f">werden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Die Heyden voor dijn <rhyme label="c" type="m">Aengesicht</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Dat sy bekennen dijn <rhyme label="c" type="m" corresp="#s6:l4">Ghericht</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die woonen hier op <rhyme label="b" type="f" corresp="#s6:l3">Eerden</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">O HEER Haer eenen Leeraer <rhyme label="d" type="m">sett</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Dat sy recht kennen uwe <rhyme label="d" type="m" corresp="#s6:l7">Wet</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En loven uwen <rhyme label="e" type="f">Name</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Dat al die Heyden int <rhyme label="f" type="m">ghemeyn</rhyme></l>
        <l id="s6:l11" met="4" type="+-">Bekennen dat sy Menschen <rhyme label="f" type="m" corresp="#s6:l10">syn</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">En ghy alleen Godt, <rhyme label="e" type="f" corresp="#s6:l9">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
