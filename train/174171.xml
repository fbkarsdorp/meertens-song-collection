<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>174171</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags>
      <tag standard-name="vaderlands lied" translation="patriotic song">vaderlands lied</tag>
      <tag standard-name="politiek lied" translation="political song">politiek lied</tag>
      <tag standard-name="spotlied" translation="mocking song">spotlied</tag>
    </tags>
    <melody>De wereld is in rep en roer</melody>
    <categories>
      <category translation="amusement">vermaak</category>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">In Landen daar 't Geweld <rhyme label="a" type="m">Regeerd</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Word Vryheid, Recht noch deugd <rhyme label="a" type="m" corresp="#s1:l1">geëerd</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dit doet 'er veelen <rhyme label="b" type="f">klaagen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dit doet 'er veelen <rhyme label="b" type="f" corresp="#s1:l3">klaagen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Men kies de vlugt, voor <rhyme label="c" type="m">Slaverny</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ontrukt zig aan de <rhyme label="c" type="m" corresp="#s1:l5">Dwinglandy</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En hoopt op beeter <rhyme label="b" type="f" corresp="#s1:l3">daagen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En hoopt op beeter <rhyme label="b" type="f" corresp="#s1:l3">daagen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hy die zyn Vaderland <rhyme label="a" type="m">ontvlugt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Om dat het onder schelmen <rhyme label="a" type="m" corresp="#s2:l1">zugt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Heeft zeker stof tot <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Heeft zeker stof tot <rhyme label="b" type="f" corresp="#s2:l3">klagen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Maar zo by egter in die <rhyme label="c" type="m">vlugt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Om geene wanbedryven <rhyme label="c" type="m" corresp="#s2:l5">zugt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Slyt hy verg'noegd zyn <rhyme label="b" type="f" corresp="#s2:l3">dagen</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Slyt hy verg'noegd zyn <rhyme label="b" type="f" corresp="#s2:l3">dagen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Vrouw van Willem speelt <rhyme label="a" type="m">helaas</rhyme>!</l>
        <l id="s3:l2" met="4" type="-+">In Neêrland al te veel den <rhyme label="a" type="m" corresp="#s3:l1">Baas</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dit geeft veel stof tot <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dit geeft veel stof tot <rhyme label="b" type="f" corresp="#s3:l3">klagen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Haar goede Egaê zwygt maar <rhyme label="c" type="m">stil</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Hy drinkt zo veel toch als hy <rhyme label="c" type="m" corresp="#s3:l5">wil</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En slyt in 't vocht zyn <rhyme label="b" type="f" corresp="#s3:l3">dagen</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">En slyt in 't vocht zyn <rhyme label="b" type="f" corresp="#s3:l3">dagen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Men noemt dat Vrouwtje <rhyme label="a" type="m">Debora</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En Gideon noemt men haar <rhyme label="a" type="m" corresp="#s4:l1">Gaê</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">O smert in dese <rhyme label="b" type="f">dagen</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">O smert in dese <rhyme label="b" type="f" corresp="#s4:l3">dagen</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Ik ken dat Vorstlyk Paar, te <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Geen Achab, nog geen <rhyme label="c" type="m" corresp="#s4:l5">Isabel</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Gaf meerder stof tot <rhyme label="b" type="f" corresp="#s4:l3">klagen</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Gaf meerder stof tot <rhyme label="b" type="f" corresp="#s4:l3">klagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Geweld en list heeft ons <rhyme label="a" type="m">beroofd</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Van onzen Burgervader <rhyme label="a" type="m" corresp="#s5:l1">Hoofd</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dit geeft veel stof tot <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dit geeft veel stof tot <rhyme label="b" type="f" corresp="#s5:l3">klagen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Maar als de Vryheid <rhyme label="c" type="m">Triomfeerd</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Wy Hem, in eer zien <rhyme label="c" type="m" corresp="#s5:l5">weergekeerd</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Zal men van vreugd <rhyme label="b" type="f" corresp="#s5:l3">gewagen</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Zal men van vreugd <rhyme label="b" type="f" corresp="#s5:l3">gewagen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
