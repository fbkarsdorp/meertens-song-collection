  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlt met hoopen, dit liet <rhyme label="a" type="f">koopen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In deez' Somer is 't <rhyme label="b" type="m">geschied</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Wilt met hoopen, dit liet <rhyme label="a" type="f" corresp="#s1:l1">koopen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">In deez' Somer is 't <rhyme label="b" type="m" corresp="#s1:l2">geschied</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Een Soldaetje, die moest <rhyme label="a" type="f" corresp="#s1:l1">loopen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dog zyn Lief begeerdent <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Een Soldaetje, die moest <rhyme label="a" type="f" corresp="#s1:l1">loopen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Dog zyn Lief begeerdent <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hy kreeg Ruzie, door <rhyme label="a" type="f">Confuzie</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Met een zyner <rhyme label="b" type="m">Kammerraeds</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Hy kreeg Ruzie, door <rhyme label="a" type="f" corresp="#s2:l1">Confuzie</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Met een zyner <rhyme label="b" type="m" corresp="#s2:l2">Kammerraeds</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Byde dronken, de <rhyme label="a" type="f" corresp="#s2:l1">Concluzie</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Was op de Parade <rhyme label="b" type="m" corresp="#s2:l2">Plaets</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Byde dronken, de <rhyme label="a" type="f" corresp="#s2:l1">Concluzie</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Was op de Parade <rhyme label="b" type="m" corresp="#s2:l2">Plaets</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer zyn Meisje, ging een <rhyme label="a" type="f">Reisje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wagen by zyn <rhyme label="b" type="m">Capiteyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Maer zyn Meisje, ging een <rhyme label="a" type="f" corresp="#s3:l1">Reisje</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Wagen by zyn <rhyme label="b" type="m" corresp="#s3:l2">Capiteyn</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Capiteyn, myn lief zyn <rhyme label="a" type="f" corresp="#s3:l1">Vleizie</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Sprak zy, is te Eel en <rhyme label="b" type="m" corresp="#s3:l2">Fyn</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Capiteyn, myn lief zyn <rhyme label="a" type="f" corresp="#s3:l1">Vleizie</rhyme>;</l>
        <l id="s3:l8" met="4" type="+-">Sprak zy, is te Eel en <rhyme label="b" type="m" corresp="#s3:l2">Fyn</rhyme>;</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zulke slagen, dat zyn <rhyme label="a" type="f">plagen</rhyme>:</l>
        <l id="s4:l2" met="4" type="+-">Voor een snaek van <rhyme label="b" type="m">Kwaliteit</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Zulke slagen, dat zyn <rhyme label="a" type="f" corresp="#s4:l1">plagen</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Voor een snaek van <rhyme label="b" type="m" corresp="#s4:l2">Kwaliteit</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Want hy is, ik moet 't nu <rhyme label="a" type="f" corresp="#s4:l1">klagen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Advocaet, al drie Iaer <rhyme label="b" type="m" corresp="#s4:l2">tyd</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Want hy is, ik moet 't nu <rhyme label="a" type="f" corresp="#s4:l1">klagen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Advocaet, al drie Iaer <rhyme label="b" type="m" corresp="#s4:l2">tyd</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Wil niet hoopen, dat hy <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Capiteyn verhoord een <rhyme label="b" type="m">Maegd</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">'k Wil niet hoopen, dat hy <rhyme label="a" type="f" corresp="#s5:l1">loopen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Capiteyn verhoord een <rhyme label="b" type="m" corresp="#s5:l2">Maegd</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Zegt my eens, kan ik 't af <rhyme label="a" type="f" corresp="#s5:l1">koopen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Met ernst is dit uw <rhyme label="b" type="m" corresp="#s5:l2">gevraegt</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Zegt my eens, kan ik 't af <rhyme label="a" type="f" corresp="#s5:l1">koopen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Met ernst is dit uw <rhyme label="b" type="m" corresp="#s5:l2">gevraegt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Want wy hoeven, niet te <rhyme label="a" type="f">toeven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Met te toonen wie wy <rhyme label="b" type="m">zyn</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Want wy hoeven, niet te <rhyme label="a" type="f" corresp="#s6:l1">toeven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met te toonen wie wy <rhyme label="b" type="m" corresp="#s6:l2">zyn</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Onze zaek, die staet op <rhyme label="a" type="f" corresp="#s6:l1">Schroeven</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Onthoud dit, myn Heer <rhyme label="b" type="m" corresp="#s6:l2">Captyn</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Onze zaek, die staet op <rhyme label="a" type="f" corresp="#s6:l1">Schroeven</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Onthoud dit, myn Heer <rhyme label="b" type="m" corresp="#s6:l2">Captyn</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hoeren Meisje, zoo'n <rhyme label="a" type="f">spreekwyzie</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Heb ik also lang <rhyme label="b" type="m">gehoord</rhyme>;</l>
        <l id="s7:l3" met="4" type="+-">Hoeren Meisje, zoo'n <rhyme label="a" type="f" corresp="#s7:l1">spreekwyzie</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Heb ik also lang <rhyme label="b" type="m" corresp="#s7:l2">gehoord</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Voor uw Liegen, sal uw <rhyme label="a" type="f" corresp="#s7:l1">Gyzie</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Straffer loopen als 't <rhyme label="b" type="m" corresp="#s7:l2">behoord</rhyme>.</l>
        <l id="s7:l7" met="4" type="+-">Voor uw Liegen, sal uw <rhyme label="a" type="f" corresp="#s7:l1">Gyzie</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Straffer loopen als 't <rhyme label="b" type="m" corresp="#s7:l2">behoord</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">O Canalje, Vee <rhyme label="a" type="f">Rapalje</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Sprak het Meisje, heel <rhyme label="b" type="m">verwoed</rhyme>;</l>
        <l id="s8:l3" met="4" type="+-">O Canalje, Vee <rhyme label="a" type="f" corresp="#s8:l1">Rapalje</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Sprak het Meisje, heel <rhyme label="b" type="m" corresp="#s8:l2">verwoed</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">'k Zal uw knellen, want ik <rhyme label="a" type="f" corresp="#s8:l1">kanje</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Buyge voor myn teere <rhyme label="b" type="m" corresp="#s8:l2">Voet</rhyme>.</l>
        <l id="s8:l7" met="4" type="+-">'k Zal uw knellen, want ik <rhyme label="a" type="f" corresp="#s8:l1">kanje</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Buyge voor myn teere <rhyme label="b" type="m" corresp="#s8:l2">Voet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">'k Ben een Nigte, wilt nu <rhyme label="a" type="f">zwigte</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Van myn Heer de <rhyme label="b" type="m">Commandant</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">'k Ben een Nigte, wilt nu <rhyme label="a" type="f" corresp="#s9:l1">zwigte</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Van myn Heer de <rhyme label="b" type="m" corresp="#s9:l2">Commandant</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Die weet raed; als ik <rhyme label="a" type="f" corresp="#s9:l1">berigte</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Wat Captyn gy syt voor 't <rhyme label="b" type="m" corresp="#s9:l2">Land</rhyme>.</l>
        <l id="s9:l7" met="4" type="+-">Die weet raed; als ik <rhyme label="a" type="f" corresp="#s9:l1">berigte</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Wat Captyn gy syt voor 't <rhyme label="b" type="m" corresp="#s9:l2">Land</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">O Iuffrouwe, 'k heb <rhyme label="a" type="f">berouwe</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Ik herroep myn woorden <rhyme label="b" type="m">in</rhyme>;</l>
        <l id="s10:l3" met="4" type="+-">O Iuffrouwe, 'k heb <rhyme label="a" type="f" corresp="#s10:l1">berouwe</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Ik herroep myn woorden <rhyme label="b" type="m" corresp="#s10:l2">in</rhyme>;</l>
        <l id="s10:l5" met="4" type="+-">U gedrag, deed my <rhyme label="a" type="f" corresp="#s10:l1">vertrouwe</rhyme>;</l>
        <l id="s10:l6" met="4" type="+-">Dat gy hiel, van yders <rhyme label="b" type="m" corresp="#s10:l2">Min</rhyme>.</l>
        <l id="s10:l7" met="4" type="+-">U gedrag, deed my <rhyme label="a" type="f" corresp="#s10:l1">vertrouwe</rhyme>;</l>
        <l id="s10:l8" met="4" type="+-">Dat gy hiel, van yders <rhyme label="b" type="m" corresp="#s10:l2">Min</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Uw Beminde, kunt gy <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Deesen avond hier by <rhyme label="b" type="m">my</rhyme>;</l>
        <l id="s11:l3" met="4" type="+-">Uw Beminde, kunt gy <rhyme label="a" type="f" corresp="#s11:l1">vinden</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Deesen avond hier by <rhyme label="b" type="m" corresp="#s11:l2">my</rhyme>;</l>
        <l id="s11:l5" met="4" type="+-">'k Ga hem haelen, wy syn <rhyme label="a" type="f" corresp="#s11:l1">Vrinden</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Zet dees saek maer aen een <rhyme label="b" type="m" corresp="#s11:l2">sy</rhyme>.</l>
        <l id="s11:l7" met="4" type="+-">'k Ga hem haelen, wy syn <rhyme label="a" type="f" corresp="#s11:l1">Vrinden</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Zet dees saek maer aen een <rhyme label="b" type="m" corresp="#s11:l2">sy</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Mag ik weeten in <rhyme label="a" type="f">Secreeten</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Om wat reën hy Soldaet <rhyme label="b" type="m">is</rhyme>;</l>
        <l id="s12:l3" met="4" type="+-">Mag ik weeten in <rhyme label="a" type="f" corresp="#s12:l1">Secreeten</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Om wat reën hy Soldaet <rhyme label="b" type="m" corresp="#s12:l2">is</rhyme>;</l>
        <l id="s12:l5" met="4" type="+-">Neen sy 't Meisje het syn <rhyme label="a" type="f" corresp="#s12:l1">Vleeten</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Die niet zwygt 't <rhyme label="b" type="m" corresp="#s12:l2">Geheimenis</rhyme></l>
        <l id="s12:l7" met="4" type="+-">Neen sy 't Meisje het syn <rhyme label="a" type="f" corresp="#s12:l1">Vleeten</rhyme>,</l>
        <l id="s12:l8" met="4" type="+-">Die niet zwygt 't <rhyme label="b" type="m" corresp="#s12:l2">Geheimenis</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Deez' Historie is tot <rhyme label="a" type="f">Glorie</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Van imand, in Rym <rhyme label="b" type="m">gestigt</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Deez' Historie is tot <rhyme label="a" type="f" corresp="#s13:l1">Glorie</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Van imand, in Rym <rhyme label="b" type="m" corresp="#s13:l2">gestigt</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Die het selfs als Per <rhyme label="a" type="f" corresp="#s13:l1">Memorie</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Door d' eyge mond is <rhyme label="b" type="m" corresp="#s13:l2">berigt</rhyme>,</l>
        <l id="s13:l7" met="4" type="+-">Die het selfs als Per <rhyme label="a" type="f" corresp="#s13:l1">Memorie</rhyme>,</l>
        <l id="s13:l8" met="4" type="+-">Door d' eyge mond is <rhyme label="b" type="m" corresp="#s13:l2">berigt</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Lieve Meisjes, leerd nouw <rhyme label="a" type="f">reisjes</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Wat de zuivre Liefde <rhyme label="b" type="m">doed</rhyme>:</l>
        <l id="s14:l3" met="4" type="+-">Lieve Meisjes, leerd nouw <rhyme label="a" type="f" corresp="#s14:l1">reisjes</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Wat de zuivre Liefde <rhyme label="b" type="m" corresp="#s14:l2">doed</rhyme>:</l>
        <l id="s14:l5" met="4" type="+-">Mind na 't voorbeeld, gy trekt <rhyme label="a" type="f" corresp="#s14:l1">prysjes</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Een huys Man uw pyn <rhyme label="b" type="m" corresp="#s14:l2">verzoet</rhyme>.</l>
        <l id="s14:l7" met="4" type="+-">Mind na 't voorbeeld, gy trekt <rhyme label="a" type="f" corresp="#s14:l1">prysjes</rhyme>,</l>
        <l id="s14:l8" met="4" type="+-">Een huys Man uw pyn <rhyme label="b" type="m" corresp="#s14:l2">verzoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
