      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Rozemondje die mijn oog</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LUci ick voor neder <rhyme label="a" type="m">buck</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En klaegh mijn <rhyme label="a" type="m" corresp="#s1:l1">ongeluck</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En stort voor <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Mijn gantsche <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Met sughjes, traentjes <rhyme label="b" type="m" corresp="#s1:l3">teer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'k Offer mijn verslagen <rhyme label="d" type="m">Geest</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Op d'Altaer van u <rhyme label="e" type="m">Zin</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Noch martelt ghy my <rhyme label="d" type="m" corresp="#s1:l6">meest</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">In pijnlijk vuyr der <rhyme label="e" type="m" corresp="#s1:l7">Min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Door een Kusje van u <rhyme label="a" type="m">Mont</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wert eerst mijn Ziel <rhyme label="a" type="m" corresp="#s2:l1">gewont</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">En den Nectar <rhyme label="b" type="m">Dou</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Wiens soete <rhyme label="c" type="m">vocht</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ick noyt verlaten <rhyme label="b" type="m" corresp="#s2:l3">wou</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Kleefde stadig aen't <rhyme label="d" type="m">Evoor</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Van u net Elphen <rhyme label="e" type="m">Been</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Daer ick mijn Geest <rhyme label="d" type="m" corresp="#s2:l6">verloor</rhyme></l>
        <l id="s2:l9" met="3" type="-+">En bleef een dorre <rhyme label="e" type="m" corresp="#s2:l7">steen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Komt dan schoone <rhyme label="a" type="m">Roselijn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ghy sult weer Heelster <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Ghy sult door u <rhyme label="b" type="m">gonst</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Mijn weer <rhyme label="c" type="m">ontslaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Van zugjes en <rhyme label="b" type="m" corresp="#s3:l3">getraen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Als eens lang gewenschte <rhyme label="d" type="m">woort</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Vloeyt van u Boesem <rhyme label="e" type="m">neer</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ia Ia, Ia Ia, Ia <rhyme label="d" type="m" corresp="#s3:l6">Ia</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Mijn Liefste heelt my <rhyme label="e" type="m" corresp="#s3:l7">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
