<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185886</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1790</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Aangename dagen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Aengename <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die men in Alkmaer <rhyme label="b" type="m">slyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Gy kunt een ieder <rhyme label="a" type="f" corresp="#s1:l1">behagen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Door uwe <rhyme label="b" type="m" corresp="#s1:l2">vrolykheid</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Daer 't Roozie by de <rhyme label="c" type="f">doorne</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Een ieder zyn liefje <rhyme label="d" type="m">ziet</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Maer gy kunt myn niet <rhyme label="c" type="f" corresp="#s1:l5">bekooren</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Want myn Fielis is hier <rhyme label="d" type="m" corresp="#s1:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vermakelyk avond <rhyme label="a" type="f">uur</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Daar de Koffie een <rhyme label="b" type="m">Thee-kan</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Banket en <rhyme label="a" type="f" corresp="#s2:l1">Confituur</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Werd gebruykt van menig <rhyme label="b" type="m" corresp="#s2:l2">man</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Daer 't Roozie by de <rhyme label="c" type="f">doorne</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Een ieder een zyn liefje <rhyme label="d" type="m">ziet</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Maer gy kunt myn niet <rhyme label="c" type="f" corresp="#s2:l5">bekooren</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Want myn Fielis is hier <rhyme label="d" type="m" corresp="#s2:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Stygertjes op 't <rhyme label="a" type="f">Water</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Langs de Veste van de <rhyme label="b" type="m">stad</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daer de Vissies met <rhyme label="a" type="f" corresp="#s3:l1">geschaeter</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">Langs de Golve <rhyme label="b" type="m" corresp="#s3:l2">trat</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Niets kan de vreugde <rhyme label="c" type="f">stooren</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">In 't vermakelyk <rhyme label="d" type="m">gebiet</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">Maer gy kunt myn niet <rhyme label="c" type="f" corresp="#s3:l5">bekooren</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Want myn Fielis is hier <rhyme label="d" type="m" corresp="#s3:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De teedere witte <rhyme label="a" type="f">schaepen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die zomtijds uit Weiden <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En droog staen te <rhyme label="a" type="f" corresp="#s4:l1">gaepen</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Terwyl men uw ziet <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Daer 't Lammetje zig laet <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Met een droeve <rhyme label="d" type="m">stem</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Maer gy kunt myn niet <rhyme label="c" type="f" corresp="#s4:l5">bekooren</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Want myn Fielis is hier <rhyme label="d" type="m" corresp="#s4:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Helaes nu moet ik <rhyme label="a" type="f">scheide</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van de schoone <rhyme label="b" type="m">stad</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Daer ik in korte <rhyme label="a" type="f" corresp="#s5:l1">tyden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Veel vreugd in heb <rhyme label="b" type="m" corresp="#s5:l2">gehad</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Daer 't Roozie by de <rhyme label="c" type="f">doorne</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Een ieder zyn liefje <rhyme label="d" type="m">ziet</rhyme>;</l>
        <l id="s5:l7" met="3" type="+-">Daerom zoo moet ik <rhyme label="c" type="f" corresp="#s5:l5">schijden</rhyme>;</l>
        <l id="s5:l8" met="3" type="+-">Want myn Fielis is hier <rhyme label="d" type="m" corresp="#s5:l6">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
