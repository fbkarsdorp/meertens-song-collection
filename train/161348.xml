  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Oven <rhyme label="a" type="m">op-gestoockt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die als den Af-grondt <rhyme label="a" type="m" corresp="#s1:l1">roockt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ontfingh in 't heetsts van haer <rhyme label="b" type="m">gloet</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Drie Jongelingen, vroom en <rhyme label="b" type="m" corresp="#s1:l3">goedt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Met banden // haer <rhyme label="c" type="f">handen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Wel vast <rhyme label="d" type="m">gemaeckt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Maer doe de vlam <rhyme label="d" type="m" corresp="#s1:l6">genaeckt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Verbranden // de <rhyme label="e" type="f">banden</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">Maer 't Lijf word niet van 't sengend vuur <rhyme label="d" type="m" corresp="#s1:l6">geraeckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dees wonderlijcke <rhyme label="a" type="m">gloet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Beeldt af de <rhyme label="a" type="m" corresp="#s2:l1">tegenspoedt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Die Godes kind'ren oock <rhyme label="b" type="m">ontslaet</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Van al de Ket'nen, daerme 't <rhyme label="b" type="m" corresp="#s2:l3">quaedt</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Der sonde // <rhyme label="c" type="f">gebonden</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Haer voortijdts <rhyme label="d" type="m">hiel</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Maer hare ed'le <rhyme label="d" type="m" corresp="#s2:l6">Ziel</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Gaet vrylijck // en <rhyme label="e" type="f">blijlijck</rhyme>,</l>
        <l id="s2:l9" met="5" type="+-">Als of voor haer niets pijnelijck en <rhyme label="d" type="m" corresp="#s2:l6">viel</rhyme>.</l>
      </lg>
    </lg>
  </text>
