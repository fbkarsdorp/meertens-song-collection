<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159095</id>
    <date>
      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De tijd is hier</melody>
  <author>Augustinus van Teylingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Comt en laet ons <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den groenen Mey in <rhyme label="b" type="f">draghen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Daer den <rhyme label="a" type="m" corresp="#s1:l1">Pellicaen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Iesus op heeft <rhyme label="c" type="m">gherust</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En voor ons <rhyme label="a" type="m" corresp="#s1:l1">voldaen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Nae zijn goet wel <rhyme label="b" type="f" corresp="#s1:l2">behaghen</rhyme></l>
        <l id="s1:l7" met="3" type="+-">onse sonden <rhyme label="a" type="m" corresp="#s1:l1">saen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Door ijn bloet uyt <rhyme label="c" type="m" corresp="#s1:l4">gheblust</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Na desen <rhyme label="a" type="m" corresp="#s1:l1">Meyboom</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Had Christus grooten <rhyme label="c" type="m" corresp="#s1:l4">lust</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Om dat hy brenghen sou te <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Die Evam door den boom <rhyme label="d" type="m" corresp="#s1:l11">verriet</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Triumphe en <rhyme label="e" type="m">accoort</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Brenght desen Meyboom <rhyme label="e" type="m" corresp="#s1:l13">voort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ghy traghe wilt u <rhyme label="a" type="m">spoen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Desen Mey in te <rhyme label="b" type="f">halen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Want hy kan ons <rhyme label="a" type="m" corresp="#s2:l1">voen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met alle weelden <rhyme label="c" type="m">fijn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ons schult en <rhyme label="a" type="m" corresp="#s2:l1">rantsoen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Wou Christus oock <rhyme label="b" type="f" corresp="#s2:l2">betalen</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Aen desen boom <rhyme label="a" type="m" corresp="#s2:l1">groen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Dat was den wille <rhyme label="c" type="m" corresp="#s2:l4">zijn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Desen groenen <rhyme label="a" type="m" corresp="#s2:l1">Meye</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Is ons een <rhyme label="c" type="m" corresp="#s2:l4">Medecijn</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Sijn costelijcke vruchten <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s2:l12" met="4" type="-+">Des vyants machte vluchten <rhyme label="d" type="m" corresp="#s2:l11">doet</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Triumphe en <rhyme label="e" type="m">accoort</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Brenght desen Meyboom <rhyme label="e" type="m" corresp="#s2:l13">voort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Een wort niet <rhyme label="a" type="m">vermoeyt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van desen Mey <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Want uyt hem soo <rhyme label="a" type="m" corresp="#s3:l1">vloeyt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Alle gheneuchte <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Lieffelijck hy <rhyme label="a" type="m" corresp="#s3:l1">bloeyt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van bloemkens <rhyme label="b" type="f" corresp="#s3:l2">uytghelesen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Want hy is <rhyme label="a" type="m" corresp="#s3:l1">bespoet</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Met Christus dierbaer <rhyme label="c" type="m" corresp="#s3:l4">bloet</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Laet ons <rhyme label="a" type="m" corresp="#s3:l1">glorieren</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">In desen Meyboom <rhyme label="c" type="m" corresp="#s3:l4">goet</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Die voor ons is <rhyme label="d" type="m">victorieus</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">Van krachten seer <rhyme label="d" type="m" corresp="#s3:l11">glorieus</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Triumphe en <rhyme label="e" type="m">accoort</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">Brenght desen Meyboom <rhyme label="e" type="m" corresp="#s3:l13">voort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Desen Mey <rhyme label="a" type="m">playsant</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als heden is <rhyme label="b" type="f">gevonden</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Met eeren <rhyme label="a" type="m" corresp="#s4:l1">triumphant</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Binnen Romen <rhyme label="c" type="m">ghebracht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hy is een seecker <rhyme label="a" type="m" corresp="#s4:l1">pant</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van vergiffenis der <rhyme label="b" type="f" corresp="#s4:l2">sonden</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Die Ioden tot een <rhyme label="a" type="m" corresp="#s4:l1">schant</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die hem hebben <rhyme label="c" type="m" corresp="#s4:l4">veracht</rhyme>:</l>
        <l id="s4:l9" met="3" type="-+">Veel verscheyde <rhyme label="a" type="m" corresp="#s4:l1">deuchden</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Heeft dese Mey <rhyme label="c" type="m" corresp="#s4:l4">ghebracht</rhyme></l>
        <l id="s4:l11" met="4" type="+-">Die desen Mey wel draghen <rhyme label="d" type="m">wil</rhyme></l>
        <l id="s4:l12" met="4" type="-+">Moet wesen sonder claghen <rhyme label="d" type="m" corresp="#s4:l11">stl</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Triumphe en <rhyme label="e" type="m">accoort</rhyme>,</l>
        <l id="s4:l14" met="3" type="-+">Brenght desen Mey-boom <rhyme label="e" type="m" corresp="#s4:l13">voort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Princen <rhyme label="a" type="m">alghemeyn</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laet ons nu <rhyme label="b" type="f">contempleren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">In desen Meyboom <rhyme label="a" type="m" corresp="#s5:l1">reyn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daer is ons <rhyme label="c" type="m">salicheyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hy is den wech <rhyme label="a" type="m" corresp="#s5:l1">verteyn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Daermen door moet <rhyme label="b" type="f" corresp="#s5:l2">passeren</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">In het Hemels <rhyme label="a" type="m" corresp="#s5:l1">pleyn</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Daer Christus ons heeft <rhyme label="c" type="m" corresp="#s5:l4">bereyt</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Tot die deure des <rhyme label="a" type="m" corresp="#s5:l1">levens</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Anders geen padt en <rhyme label="c" type="m" corresp="#s5:l4">leydt</rhyme></l>
        <l id="s5:l11" met="4" type="+-">dan door het Cruys ons Heeren <rhyme label="d" type="m">saen</rhyme></l>
        <l id="s5:l12" met="4" type="-+">Daer hy ons heeft door leeren <rhyme label="d" type="m" corresp="#s5:l11">gaen</rhyme>,</l>
        <l id="s5:l13" met="3" type="-+">Triumphe en <rhyme label="e" type="m">accoort</rhyme>,</l>
        <l id="s5:l14" met="3" type="-+">Brenght desen Mey-boom <rhyme label="e" type="m" corresp="#s5:l13">voort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
