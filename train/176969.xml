    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Myn uitgelezen <rhyme label="a" type="m">honigdief</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Myn uitgelezen <rhyme label="a" type="m" corresp="#s1:l1">honigdief</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Is my gedurig even <rhyme label="a" type="m" corresp="#s1:l1">lief</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Ik zweer, haar steeds te <rhyme label="b" type="f">minnen</rhyme>;</l>
        <l id="s1:l5" met="1" type="-+">Ik <rhyme label="c" type="m">zweer</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">'k Bemin haar <rhyme label="c" type="m" corresp="#s1:l5">teêr</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mogt ik haar ook zoo waardig <rhyme label="a" type="m">zyn</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Mogt ik haar ook zoo waardig <rhyme label="a" type="m" corresp="#s2:l1">zyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ik zag wel haast myn <rhyme label="a" type="m" corresp="#s2:l1">minnepyn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe groot, door haar <rhyme label="b" type="f">verwinnen</rhyme>;</l>
        <l id="s2:l5" met="1" type="-+">Hoe <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Door haar <rhyme label="c" type="m" corresp="#s2:l5">gedood</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ik haar lieve lipjes <rhyme label="a" type="m">druk</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Als ik haar lieve lipjes <rhyme label="a" type="m" corresp="#s3:l1">druk</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En roosjes van haar kaakjes <rhyme label="a" type="m" corresp="#s3:l1">pluk</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dan brand myn hart van <rhyme label="b" type="f">binnen</rhyme>;</l>
        <l id="s3:l5" met="1" type="-+">Dan <rhyme label="c" type="m">brand</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Myn <rhyme label="c" type="m" corresp="#s3:l5">ingewand</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laat ik het oog dan lager <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Laat ik het oog dan lager <rhyme label="a" type="m" corresp="#s4:l1">gaan</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En zie ik 't reizend dalen <rhyme label="a" type="m" corresp="#s4:l1">aan</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dier twee albaste <rhyme label="b" type="f">klipjes</rhyme>,</l>
        <l id="s4:l5" met="1" type="-+">Dier <rhyme label="c" type="m">twee</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">By Paphos <rhyme label="c" type="m" corresp="#s4:l5">reê</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dan roep ik, ach! kan niet <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dan roep ik, ach! kan niet <rhyme label="a" type="m" corresp="#s5:l1">meer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wyl ik geheel en al <rhyme label="a" type="m" corresp="#s5:l1">verteer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Van min, myn <rhyme label="b" type="f">zielsvoogdesje</rhyme>,</l>
        <l id="s5:l5" met="1" type="-+">Van <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Myn <rhyme label="c" type="m" corresp="#s5:l5">Engelin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Slaat zy dan slechts haar oog op <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Slaat zy dan slechts haar oog op <rhyme label="a" type="m" corresp="#s6:l1">my</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En lonkt zy my eens van ter <rhyme label="a" type="m" corresp="#s6:l1">zy'</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wat vreugd doet my dit <rhyme label="b" type="f">hopen</rhyme>,</l>
        <l id="s6:l5" met="1" type="-+">Wat <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Hoopt dan myn <rhyme label="c" type="m" corresp="#s6:l5">jeugd</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ach! mogt ik eens het waard <rhyme label="a" type="m">genot</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Ach! mogt ik eens het waard <rhyme label="a" type="m" corresp="#s7:l1">genot</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dier schone tot myn <rhyme label="a" type="m" corresp="#s7:l1">huwlykslot</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">ô Goôn! van u <rhyme label="b" type="f">genieten</rhyme>:</l>
        <l id="s7:l5" met="1" type="-+">ô <rhyme label="c" type="m">Goôn</rhyme>!</l>
        <l id="s7:l6" met="2" type="-+">Schenk my dit <rhyme label="c" type="m" corresp="#s7:l5">loon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
