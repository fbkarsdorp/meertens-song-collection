<song>
  <meta>
    <id>17076</id>
    <date>
      <year-start>1903</year-start>
      <year-end>1908</year-end>
    </date>
    <tags/>
    <melody>In de hemel is een dans</melody>
  <author>Florimond van Duyse</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">In den hemel is eenen dans;</l>
        <l id="s1:l2">alleluia!</l>
        <l id="s1:l3">Daer dansen all' de maegdekens.</l>
        <l id="s1:l4">Benedicamus Domino,</l>
        <l id="s1:l5">alleluia, alleluia!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">'t Is voor Amelia;</l>
        <l id="s2:l2">alleluia!</l>
        <l id="s2:l3">Wy dansen naer de maegdekens.</l>
        <l id="s2:l4">Benedicamus Domino,</l>
        <l id="s2:l5">alleluia, alleluia!</l>
      </lg>
    </lg>
  </text>
</song>
