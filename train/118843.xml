    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WEg met de <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Weg met de <rhyme label="a" type="m" corresp="#s1:l1">Min</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Daar steekt veel rampe, en sinerten <rhyme label="a" type="m" corresp="#s1:l1">in</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Weg met de <rhyme label="a" type="m" corresp="#s1:l1">Min</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Weg met de <rhyme label="a" type="m" corresp="#s1:l1">Min</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">En band die kuuren uit uwe <rhyme label="a" type="m" corresp="#s1:l1">zin</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Eerd Bagchus, <rhyme label="b" type="f">Silenus</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En lach wat met <rhyme label="b" type="f" corresp="#s1:l7">Venus</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Zyt altyd <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">By drinken, en <rhyme label="d" type="f">klinken</rhyme>,</l>
        <l id="s1:l11" met="2" type="-+">En braaf <rhyme label="d" type="f" corresp="#s1:l10">rinkinken</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Gy weze <rhyme label="c" type="m" corresp="#s1:l9">moet</rhyme>.</l>
      </lg>
