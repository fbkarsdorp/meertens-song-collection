<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180755</id>
    <date>
      <year-start>1688</year-start>
      <year-end>1688</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mon confesseur est rude</melody>
  <author>Philippus van Sorgen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Raar voorwerp van mijn <rhyme label="a" type="f">Liefde</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn waarde <rhyme label="b" type="m">Sulaamith</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Mijn Bloet-schat, mijn <rhyme label="a" type="f" corresp="#s1:l1">Geliefde</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die mijne Ziel <rhyme label="b" type="m" corresp="#s1:l2">bezit</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Hoe, wilt gy voor mijn <rhyme label="c" type="f">zuchten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En smeken altijd <rhyme label="c" type="f" corresp="#s1:l5">vluchten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ach Ziel-lief, hoe is <rhyme label="b" type="m" corresp="#s1:l2">dit</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe, wierd gy noit <rhyme label="a" type="f">bewogen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Door ‘t geen ik heb <rhyme label="b" type="m">gele’en</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Tot Liefd’, en <rhyme label="a" type="f" corresp="#s2:l1">mededogen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Daar ‘k leed om u <rhyme label="b" type="m" corresp="#s2:l2">alleen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Zint my mijn Moeder <rhyme label="c" type="f">baarde</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tot dat ik van der <rhyme label="c" type="f" corresp="#s2:l5">Aarde</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Moest scheuden, met <rhyme label="b" type="m" corresp="#s2:l2">gesteen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Kom zie dog eens te <rhyme label="a" type="f">rugge</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hoe ik het vinnig <rhyme label="b" type="m">slaan</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Der Beuls, op hoofd en <rhyme label="a" type="f" corresp="#s3:l1">rugge</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Om u heb, <rhyme label="b" type="m" corresp="#s3:l2">uitgestaan</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">En hoe de <rhyme label="c" type="f">qualster-kladden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Mijn droevig aanzicht <rhyme label="c" type="f" corresp="#s3:l5">hadden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Bedekt en <rhyme label="b" type="m" corresp="#s3:l2">overla’en</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gy waart als doen vol <rhyme label="a" type="f">schulden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die ‘k voor u heb <rhyme label="b" type="m">geboet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nog heb ik moeten <rhyme label="a" type="f" corresp="#s4:l1">dulden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat my mijn dierbaar <rhyme label="b" type="m" corresp="#s4:l2">bloet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wierd afgetapt; mijn <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Heb ik voor u <rhyme label="c" type="f" corresp="#s4:l5">gegeven</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Raakt zulx niet uw <rhyme label="b" type="m" corresp="#s4:l2">gemoed</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Kom gaan wy met ons <rhyme label="a" type="f">beyen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Op ‘t kneeklig <rhyme label="b" type="m">Galgen-veld</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Laat daar uw’ oogen <rhyme label="a" type="f" corresp="#s5:l1">weyen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ziet daar een Kruis <rhyme label="b" type="m" corresp="#s5:l2">gestelt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Waar aan der Beulen <rhyme label="c" type="f">vuisten</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">My, uwenthalven, <rhyme label="c" type="f" corresp="#s5:l5">kruisten</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Met een verwoed <rhyme label="b" type="m" corresp="#s5:l2">geweld</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Daar wierd ik op <rhyme label="a" type="f">gebonden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Genagelt en <rhyme label="b" type="m">gehecht</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">(Ik yz’ nog voor die <rhyme label="a" type="f" corresp="#s6:l1">wonden</rhyme>!)</l>
        <l id="s6:l4" met="3" type="-+">En zoo om hoog <rhyme label="b" type="m" corresp="#s6:l2">gerecht</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">In ‘t oog van groote <rhyme label="c" type="f">Scharen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van Volk’ren die daar <rhyme label="c" type="f" corresp="#s6:l5">waren</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Beschouwers van dat <rhyme label="b" type="m" corresp="#s6:l2">Recht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dit heb ik moeten <rhyme label="a" type="f">lijden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Eer ik voor u <rhyme label="b" type="m">voldeed</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">En eer ik u <rhyme label="a" type="f" corresp="#s7:l1">bevrijden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Kon van ‘t aanstaande <rhyme label="b" type="m" corresp="#s7:l2">leet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dat gy ten eeuw’ge <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Zoud hebben moeten <rhyme label="c" type="f" corresp="#s7:l5">dragen</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">Toont dat gy zullx wel <rhyme label="b" type="m" corresp="#s7:l2">weet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ey wilt u Lief <rhyme label="a" type="f">bedaren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gun’ my uw’ liefde, <rhyme label="b" type="m">daar</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Ik u om smeeke; <rhyme label="a" type="f" corresp="#s8:l1">waren</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">U Zinnen mywaarts. <rhyme label="b" type="m" corresp="#s8:l2">Maar</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Wilt gy my nog <rhyme label="c" type="f">versmaden</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Ziet dat mijn <rhyme label="c" type="f" corresp="#s8:l5">ongenaden</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Uw ook niet <rhyme label="b" type="m" corresp="#s8:l2">wedervaar</rhyme>’.</l>
      </lg>
    </lg>
  </text>
</song>
