<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181906</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hoe zoet is het daar de vriendschap woont</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GEluk waerde Prins en <rhyme label="a" type="m">Princes</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Op d’ aengename <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Met het aenstaende <rhyme label="a" type="m" corresp="#s1:l1">Huwelyk</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Een ieder is <rhyme label="b" type="m" corresp="#s1:l2">verbleit</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Om dat die Prins dat schoon <rhyme label="c" type="m">Juweel</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Krygt Carolina tot syn <rhyme label="c" type="m" corresp="#s1:l5">deel</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Vol deugt, vol deugt, vol <rhyme label="d" type="m">deugt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Eerst waer de droefheid over <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En nu weer volle <rhyme label="b" type="m">vreugt</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Al wat dat men nu seggen <rhyme label="a" type="m" corresp="#s2:l1">hoord</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die Prins soo vol van <rhyme label="b" type="m" corresp="#s2:l2">deugt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die neemt tot syn <rhyme label="c" type="m">Engelin</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die schoon voorwerp van de <rhyme label="c" type="m" corresp="#s2:l5">min</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Vol vreugt, vol vreugt, vol <rhyme label="d" type="m">vreugt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Heel ’s Gravenhage <rhyme label="a" type="m">triompheert</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Over die Jonge <rhyme label="b" type="m">Princes</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Om dat sy na geboode <rhyme label="a" type="m" corresp="#s3:l1">leeft</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Van de <rhyme label="b" type="m" corresp="#s3:l2">Stadhouders</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Haer Moeder by de Koning <rhyme label="c" type="m">leeft</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Want die het haer bevoolen <rhyme label="c" type="m" corresp="#s3:l5">heeft</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Soo vast, soo vast, soo <rhyme label="d" type="m">vast</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Godt geeft syn zeegen over <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Aan dees vereende <rhyme label="b" type="m">twee</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Een ieder haar zyn gunst <rhyme label="a" type="m" corresp="#s4:l1">aanbood</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Vol liefde en vol <rhyme label="b" type="m" corresp="#s4:l2">vree</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Een ieder roept lang leef de <rhyme label="c" type="m">Prins</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Soo hoord men hier en <rhyme label="c" type="m" corresp="#s4:l5">allezins</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van stad, van stad, tot <rhyme label="d" type="m">stad</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Komt heldere Son scheind met u <rhyme label="a" type="m">glans</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Over ’t Oranje <rhyme label="b" type="m">Bloed</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Komt Maen en Sterre <rhyme label="a" type="m" corresp="#s5:l1">altemael</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">U ligt dat is soo <rhyme label="b" type="m" corresp="#s5:l2">soet</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Verheugt Prins en Princes haer <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Dat nu door een gestrengelt <rhyme label="c" type="m" corresp="#s5:l5">werd</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">In kort, in kort, in <rhyme label="d" type="m">kort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Een Tulp, een opgaende <rhyme label="a" type="m">Roos</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die twee werde tot <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Die Prins die een Princes <rhyme label="a" type="m" corresp="#s6:l1">verkoos</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Al tot syn Egte <rhyme label="b" type="m" corresp="#s6:l2">deel</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Een Maegt al van een Edel <rhyme label="c" type="m">bloed</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">Gereet de Prins van ’t soetste <rhyme label="c" type="m" corresp="#s6:l5">soet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Vol glas, vol glans, vol <rhyme label="d" type="m">glans</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De jonge Prins Orange <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die toont een bly <rhyme label="b" type="m">gelaat</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Om dat zyn lieve Zuster <rhyme label="a" type="m" corresp="#s7:l1">zoet</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Treed in de Huwelyk <rhyme label="b" type="m" corresp="#s7:l2">staat</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Met Weilburg die Jonge <rhyme label="c" type="m">Prins</rhyme>;</l>
        <l id="s7:l6" met="4" type="-+">Daarom is in vreugde <rhyme label="c" type="m" corresp="#s7:l5">allezins</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Den Haag, den Haag, den <rhyme label="d" type="m">Haag</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ik wensch een Jonge Prins te <rhyme label="a" type="m">seer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En dat in korte <rhyme label="b" type="m">tyd</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Als het des Heeren wil soo <rhyme label="a" type="m" corresp="#s8:l1">is</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Dat u te saam <rhyme label="b" type="m" corresp="#s8:l2">verblyd</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Als dat vergunt die goede <rhyme label="c" type="m">Godt</rhyme>:</l>
        <l id="s8:l6" met="4" type="-+">Hier mede neem ik nu myn <rhyme label="c" type="m" corresp="#s8:l5">slot</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Met vreugt, met vreugd, met <rhyme label="d" type="m">vreugd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
