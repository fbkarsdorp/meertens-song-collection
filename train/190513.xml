<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190513</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>La danse n'est pas ce que j'aime</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De Prinsgezinde <rhyme label="a" type="f">Triumpheeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">De Keeze zitten in de <rhyme label="b" type="m">Muit</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Zy moeten 'er nog lang niet <rhyme label="b" type="m" corresp="#s1:l2">uit</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Tot al die verkeerde <rhyme label="a" type="f" corresp="#s1:l1">Veeren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ruijen van hun <rhyme label="a" type="f" corresp="#s1:l1">Exerceeren</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Of anders komt het oude <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Komt weer, komt weer, komt weer, komt <rhyme label="c" type="m" corresp="#s1:l6">weer</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Met het Geweer, met het <rhyme label="d" type="m">Geweer</rhyme>,</l>
        <l id="s1:l9" met="6" type="-+">En wie weet wat meer, en wie weet wat <rhyme label="d" type="m" corresp="#s1:l8">meer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat zal het Gekkehuis <rhyme label="a" type="f">floreeren</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Want die Keeze worden <rhyme label="b" type="m">dol</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Of zy zuipen zig zomtyds <rhyme label="b" type="m" corresp="#s2:l2">vol</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Of zou 't haar aan 't Gelt <rhyme label="a" type="f" corresp="#s2:l1">mankeeren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Of om als Baron <rhyme label="a" type="f" corresp="#s2:l1">t'Ageeren</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Nu roept men uit was 't nooit <rhyme label="c" type="m">gebeurt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Gebeurd, gebeurd, gebeurd, <rhyme label="c" type="m" corresp="#s2:l6">gebeurd</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Dat ik was Kees, dat ik was <rhyme label="d" type="m">Kees</rhyme>,</l>
        <l id="s2:l9" met="6" type="-+">'k Had nooit Gek geweest, 'k had nooit Gek <rhyme label="d" type="m" corresp="#s2:l8">geweest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'t Is niets, gy moet beter <rhyme label="a" type="f">leeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Tot dat gy alle zyt <rhyme label="b" type="m">bedaart</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Gy zy nog niet genoeg <rhyme label="b" type="m" corresp="#s3:l2">vervaart</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Of moet gy by Pruissen <rhyme label="a" type="f" corresp="#s3:l1">weezen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Om uw kwaalen te <rhyme label="a" type="f" corresp="#s3:l1">geneezen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Gaat weg uit 't Land na dien <rhyme label="c" type="m">Doctoor</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Doctoor, Doctoor, Doctoor, <rhyme label="c" type="m" corresp="#s3:l6">Doctoor</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Daar is 't goed, daar is 't <rhyme label="d" type="m">goed</rhyme>,</l>
        <l id="s3:l9" met="6" type="-+">Voor oproerig gebroed, voor oproerig <rhyme label="d" type="m" corresp="#s3:l8">gebroed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ach had dat spel maar nooit <rhyme label="a" type="f">begonnen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Te haaten het Oranje <rhyme label="b" type="m">Huis</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Want nu krygt gy u loon en <rhyme label="b" type="m" corresp="#s4:l2">kruis</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Had u by u zaak <rhyme label="a" type="f" corresp="#s4:l1">gehouwen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En geen haat mee helpen <rhyme label="a" type="f" corresp="#s4:l1">brouwen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Het helpt niet, gelyk gy <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Gy ziet, gy ziet, gy ziet, gy <rhyme label="c" type="m" corresp="#s4:l6">ziet</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Oranje leeft, Oranje <rhyme label="d" type="m">leeft</rhyme>,</l>
        <l id="s4:l9" met="6" type="-+">Zo lang men adem heeft, zo lang men adem <rhyme label="d" type="m" corresp="#s4:l8">heeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
