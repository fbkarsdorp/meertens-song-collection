<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190505</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Opgeworpen heerschzuchts <rhyme label="a" type="f">Benden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Monsters, die de Vryheid <rhyme label="a" type="f" corresp="#s1:l1">schenden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Volkverdrukkend <rhyme label="b" type="m">Dondersgoed</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Satams muitziek roof <rhyme label="b" type="m" corresp="#s1:l3">gebroed</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Al wat onheil baart <rhyme label="a" type="f">bewerkers</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Al wat ondeugt is <rhyme label="a" type="f" corresp="#s2:l1">versterkers</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Tirannische <rhyme label="b" type="m">Vloekcabaal</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Neêrlands Plutoos <rhyme label="b" type="m" corresp="#s2:l3">altemaal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Pligtverzaakers, <rhyme label="a" type="f">Rustverstoorders</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wetverbreekers, <rhyme label="a" type="f" corresp="#s3:l1">Burgermoorders</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Landverdervend <rhyme label="b" type="m">Eedgespan</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Zyt vervloekt van man tot <rhyme label="b" type="m" corresp="#s3:l3">man</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dat de blixem u <rhyme label="a" type="f">verslinde</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Wenschen alle <rhyme label="a" type="f" corresp="#s4:l1">Prinsgezinde</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Hemeltergend <rhyme label="b" type="m">Tweedrachts-Rot</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Wagt een strop van Neêrlands <rhyme label="b" type="m" corresp="#s4:l3">God</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Nassauws Vorst door u <rhyme label="a" type="f">geschonden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Gehoont, gedoemt, ô Keezen <rhyme label="a" type="f" corresp="#s5:l1">Honden</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Lydt al onrecht met <rhyme label="b" type="m">gedult</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Tot dat de maat zy <rhyme label="b" type="m" corresp="#s5:l3">vervult</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dan zal Willem zich <rhyme label="a" type="f">herstellen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dwang en Heerschzucht <rhyme label="a" type="f" corresp="#s6:l1">nedervellen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Vryheids Troon en Batoos <rhyme label="b" type="m">recht</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Door hem worden <rhyme label="b" type="m" corresp="#s6:l3">opgerecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
