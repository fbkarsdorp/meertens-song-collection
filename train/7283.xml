      <category translation="narratives">verhalen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ick wil te Lande <rhyme label="a" type="f">rijden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sprack Meester <rhyme label="b" type="m">Hillebrandt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die my den wegh wil <rhyme label="c" type="f">wijsen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Te Barnen al in dat <rhyme label="b" type="m" corresp="#s1:l2">Landt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zy zijn my onbekendt <rhyme label="d" type="m">gheweest</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Soo meenighen langhen <rhyme label="e" type="m">dagh</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">In drie-en-dertigh <rhyme label="f" type="f">jaren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Vrou Goedle ick niet en <rhyme label="e" type="m" corresp="#s1:l6">sagh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wilt ghy te lande <rhyme label="a" type="f">rijden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sprack Meester <rhyme label="b" type="m">Abeloen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ghy vindt daer op der <rhyme label="c" type="f">Marckten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Den Ionghen heldt is <rhyme label="b" type="m" corresp="#s2:l2">koen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ghy vindt daer op den <rhyme label="d" type="m">Marckten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Den Ionghen <rhyme label="e" type="m">Hillebrandt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Al quaemt ghy onder u <rhyme label="f" type="f">twaelven</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Van hem wordt ghy <rhyme label="e" type="m" corresp="#s2:l6">aengerant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soude hy my <rhyme label="a" type="f">aenrannen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met eenen euvelen <rhyme label="b" type="m">moet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ick doorhou hem zijn <rhyme label="c" type="f">schilde</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">’t En doe hem nimmer <rhyme label="b" type="m" corresp="#s3:l2">goedt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ick door-hou hem zijn <rhyme label="d" type="m">schilde</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Met eenen schermer <rhyme label="e" type="m">slagh</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat hy het zijn vrou <rhyme label="f" type="f">Moeder</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Een Iaer wel klaghen <rhyme label="e" type="m" corresp="#s3:l6">magh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dat en suldy doen <rhyme label="a" type="f">minnier</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sprack Ioncker <rhyme label="b" type="m">Diderick</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">’t Heb Hillebrandt den <rhyme label="c" type="f">Ionghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met goeder harten <rhyme label="b" type="m" corresp="#s4:l2">lief</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy sult hem seere <rhyme label="d" type="m">groeten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Al door den wille <rhyme label="e" type="m">mijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En dat hy u laet <rhyme label="f" type="f">rijden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Soo lief als ick hem mach <rhyme label="e" type="m" corresp="#s4:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Al met de selve <rhyme label="a" type="f">woorden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Hy die groene gaerde op <rhyme label="b" type="m">ran</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Tot in den Marckts <rhyme label="c" type="f">pleyne</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hillebrandt den Ouden <rhyme label="b" type="m" corresp="#s5:l2">Man</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Tot in der Marckts <rhyme label="d" type="m">pleyne</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Daer hy den Ionghen <rhyme label="e" type="m">vandt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wat doet dees Ouden <rhyme label="f" type="f">grijsaert</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Tot in mijns Vaders <rhyme label="e" type="m" corresp="#s5:l6">landt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Een harnas voert ghy <rhyme label="a" type="f">louter</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Al waerdy een Coninghs <rhyme label="b" type="m">kindt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ghy maeckter mijn Iongher <rhyme label="c" type="f">herte</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met siende Ooghen <rhyme label="b" type="m" corresp="#s6:l2">blindt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ghy soud’ tot Heyme <rhyme label="d" type="m">blijven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">En houden u <rhyme label="e" type="m">ghemack</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Met eenen huys <rhyme label="f" type="f">gheluyde</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Die Oude loegh en <rhyme label="e" type="m" corresp="#s6:l6">sprack</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Soud’ ick tot Heyme <rhyme label="a" type="f">blijven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En hou den mijn <rhyme label="b" type="m">ghemack</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Van strijden en van <rhyme label="c" type="f">vechten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Daer is my af <rhyme label="b" type="m" corresp="#s7:l2">gheseght</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Van strijden en van <rhyme label="d" type="m">vechten</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Al van mijn heene vaert <rhyme label="e" type="m">vaert</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat segh ick u heldt <rhyme label="f" type="f">Ionghe</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Daer af wert grijs mijn <rhyme label="e" type="m" corresp="#s7:l6">Baert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">’k Sal u den Baert af <rhyme label="a" type="f">rucken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En daer toe seere <rhyme label="b" type="m">slaen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Soo dat in ’t rooden <rhyme label="c" type="f">bloede</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Over u Wanghen sal <rhyme label="b" type="m" corresp="#s8:l2">gaen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">V harnas en u <rhyme label="d" type="m">Schilde</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Moet ghy my ghevende <rhyme label="e" type="m">zijn</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">En blijven mijn <rhyme label="f" type="f">ghevanghen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Behoed Godt ‘t <rhyme label="e" type="m" corresp="#s8:l6">Leven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Mijn harnas ende <rhyme label="a" type="f">Schilde</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Daer heb ick my mede <rhyme label="b" type="m">gheneert</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ick en was nooyt mijn <rhyme label="c" type="f">daghen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Van eenen Man <rhyme label="b" type="m" corresp="#s9:l2">verveert</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Sy lieten daer haer <rhyme label="d" type="m">woorden</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Zy ginghen met swaerden <rhyme label="e" type="m">slaen</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Wat zy daer <rhyme label="f" type="f">bedreven</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Dat sult ghy wel <rhyme label="e" type="m" corresp="#s9:l6">verstaen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Den Ionghen brocht den <rhyme label="a" type="f">Ouden</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Een alsoo swaren <rhyme label="b" type="m">slagh</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Maer dat hy dan al sijn <rhyme label="c" type="f">daghen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Nooyt soo vervaert en <rhyme label="b" type="m" corresp="#s10:l2">was</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Zijn Paert dat sprongh te <rhyme label="d" type="m">rugghe</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Wel twintigh vademen <rhyme label="e" type="m">wijt</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Die slagh die ghy daer <rhyme label="f" type="f">sloeghet</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Heeft u gheleert een <rhyme label="e" type="m" corresp="#s10:l6">Wijf</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Soud’ ick van vrouwen <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Dat waer mijn groote <rhyme label="b" type="m">schandt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Ick heb noch Ridders en <rhyme label="c" type="f">Heeren</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Binnen mijn vaders <rhyme label="b" type="m" corresp="#s11:l2">landt</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Ick heb noch Ridders en <rhyme label="d" type="m">Knechten</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Binnen mijne vaders <rhyme label="e" type="m">hof</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Dat ick niet gheleert <rhyme label="f" type="f">hebbe</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Daer over leer ick <rhyme label="e" type="m" corresp="#s11:l6">noch</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Hier quam soo dat den <rhyme label="a" type="f">Ouden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Liet in der Rijcken zijn <rhyme label="b" type="m">Schildt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Soo dat den Ionghen <rhyme label="c" type="f">Hillebrandt</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Zijn sweert t’onder <rhyme label="b" type="m" corresp="#s12:l2">ginck</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Hy nam hem in zijn <rhyme label="d" type="m">middele</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Aldaer hy smalste <rhyme label="e" type="m">was</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">En wierp hem neder te <rhyme label="f" type="f">rugghe</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Al in dat groene <rhyme label="e" type="m" corresp="#s12:l6">gras</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Wie aen de Ketel <rhyme label="a" type="f">wrijvet</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Heeft geerne van den <rhyme label="b" type="m">Roet</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Soo heb ghy ghedaen jongher <rhyme label="c" type="f">geldt</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Hier teghen u <rhyme label="b" type="m" corresp="#s13:l2">wederspoet</rhyme>:</l>
        <l id="s13:l5" met="3" type="-+">Spreecket nu uwe <rhyme label="d" type="m">biechte</rhyme></l>
        <l id="s13:l6" met="3" type="-+">V Biechtvaer wil ick <rhyme label="e" type="m">zijn</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Dat zydy van den <rhyme label="f" type="f">wolven</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Ghenesen mocht ghy <rhyme label="e" type="m" corresp="#s13:l6">zijn</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wolven dat zijne <rhyme label="a" type="f">wolven</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Zy loopen door dat <rhyme label="b" type="m">wout</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Ick ben een jonghen <rhyme label="c" type="f">Deghen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Ghebooren uyt Griecken <rhyme label="b" type="m" corresp="#s14:l2">stout</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Mijn Moeder hiet vrouw <rhyme label="d" type="m">Goed’le</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Een Hertoginne <rhyme label="e" type="m">fijn</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">En Hillebrandt den <rhyme label="f" type="f">Ouden</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Dat is de vader <rhyme label="e" type="m" corresp="#s14:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Hiet u Moeder vrouw <rhyme label="a" type="f">Goed’le</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Een Hertoginne <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Was Hillebrandt u <rhyme label="c" type="f">vader</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Soo bistu de Soone <rhyme label="b" type="m" corresp="#s15:l2">mijn</rhyme>:</l>
        <l id="s15:l5" met="3" type="-+">Hy schoot op zijnen <rhyme label="d" type="m">helme</rhyme></l>
        <l id="s15:l6" met="3" type="-+">En kusten hem aen zijn <rhyme label="e" type="m">mondt</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Nu danck ick Godt den <rhyme label="f" type="f">Heere</rhyme></l>
        <l id="s15:l8" met="3" type="-+">Dat ick u sie <rhyme label="e" type="m" corresp="#s15:l6">ghesondt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Och Vader lieve <rhyme label="a" type="f">Vader</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Die wonde die ‘k u heb <rhyme label="b" type="m">gheslaghen</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Wil ick soo langh ick <rhyme label="c" type="f">leve</rhyme></l>
        <l id="s16:l4" met="3" type="-+">In mijnder herten <rhyme label="b" type="m" corresp="#s16:l2">draghen</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">En swijghet Soone <rhyme label="d" type="m">stille</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Der wonden weet ick wel <rhyme label="e" type="m">raedt</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Wy willen dan hier <rhyme label="f" type="f">scheyden</rhyme></l>
        <l id="s16:l8" met="3" type="-+">Godt sterck ons op de <rhyme label="e" type="m" corresp="#s16:l6">vaert</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Nu nemet my <rhyme label="a" type="f">ghevanghen</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Als men een ghevanghen <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Vraghen u dan de <rhyme label="c" type="f">lieden</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Wat Man dat ghy daer <rhyme label="b" type="m" corresp="#s17:l2">voert</rhyme>:</l>
        <l id="s17:l5" met="3" type="-+">Soo suldy hun dan <rhyme label="d" type="m">segghen</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">’t Is een de quaetste <rhyme label="e" type="m">Man</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Die oyt op dese <rhyme label="f" type="f">wereldt</rhyme></l>
        <l id="s17:l8" met="3" type="-+">Van ’s Moeders lyve <rhyme label="e" type="m" corresp="#s17:l6">quam</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">’t Gheviel op eenen <rhyme label="a" type="f">Saterdagh</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Ontrent de vesper <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Dat Hillebrandt den <rhyme label="c" type="f">Ionghen</rhyme></l>
        <l id="s18:l4" met="3" type="-+">De groene gaerde op <rhyme label="b" type="m" corresp="#s18:l2">reedt</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Hy voerde op zijnen <rhyme label="d" type="m">helme</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Van goude een <rhyme label="e" type="m">Kranselijn</rhyme>,</l>
        <l id="s18:l7" met="3" type="-+">En nevens zijner <rhyme label="f" type="f">zijde</rhyme></l>
        <l id="s18:l8" met="3" type="-+">Den liefsten vader <rhyme label="e" type="m" corresp="#s18:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Hy voerde hem <rhyme label="a" type="f">ghevanghen</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Al sonder <rhyme label="b" type="m">arghelist</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Hy setten hem by zijnder <rhyme label="c" type="f">Moeder</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Boven haer aen den <rhyme label="b" type="m" corresp="#s19:l2">disch</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Soone wel lieve <rhyme label="d" type="m">soone</rhyme></l>
        <l id="s19:l6" met="3" type="-+">Dat gheeft my al te <rhyme label="e" type="m">vry</rhyme>,</l>
        <l id="s19:l7" met="3" type="-+">Waerom ghy dees <rhyme label="f" type="f">ghevanghen</rhyme></l>
        <l id="s19:l8" met="3" type="-+">Hier settet boven <rhyme label="e" type="m" corresp="#s19:l6">my</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Moeder seyde hy <rhyme label="a" type="f">Moeder</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Die waerheydt wil ick u <rhyme label="b" type="m">saghen</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Aen gheener groenen <rhyme label="c" type="f">heyden</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Had hy my by nae <rhyme label="b" type="m" corresp="#s20:l2">verslaghen</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">‘t Is Hillebrandt de <rhyme label="d" type="m">Oude</rhyme></l>
        <l id="s20:l6" met="3" type="-+">De liefste Vader <rhyme label="e" type="m">mijn</rhyme>,</l>
        <l id="s20:l7" met="3" type="-+">Nu neemt hem in u <rhyme label="f" type="f">armen</rhyme></l>
        <l id="s20:l8" met="3" type="-+">En heet hem wellekom <rhyme label="e" type="m" corresp="#s20:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Zy nam hem in haer <rhyme label="a" type="f">armen</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Zy kuste hem aen zijn <rhyme label="b" type="m">mondt</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">Nu danck ick Godt den <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Dat ick u sie <rhyme label="b" type="m" corresp="#s21:l2">ghesondt</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Wy willen van hier <rhyme label="d" type="m">scheyden</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">En varen in ons <rhyme label="e" type="m">Landt</rhyme>,</l>
        <l id="s21:l7" met="3" type="-+">Te Barnen binnen der <rhyme label="f" type="f">stede</rhyme></l>
        <l id="s21:l8" met="3" type="-+">Daer zijn wy wel <rhyme label="e" type="m" corresp="#s21:l6">bekandt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
