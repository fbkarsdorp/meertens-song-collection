<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164921</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Een kindje is ons geboren</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SInght Lof ghy Hemelse <rhyme label="a" type="f">Scharen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Singt Lof met soete <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verheught u ghy Gods <rhyme label="a" type="f" corresp="#s1:l1">dienaren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met Hert en Ziels <rhyme label="b" type="m" corresp="#s1:l2">geneught</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hier is een <rhyme label="c" type="f">melody</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Een oversoet <rhyme label="d" type="m">accoort</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Een Hemels <rhyme label="c" type="f" corresp="#s1:l5">Hermonye</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Wert over al <rhyme label="d" type="m" corresp="#s1:l6">gehoort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Herdertjes op den <rhyme label="a" type="f">Velde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Verscheen dees <rhyme label="b" type="m">heerlijckheyt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Haer hertjes haer seer <rhyme label="a" type="f" corresp="#s2:l1">ontstelde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Van vrees en <rhyme label="b" type="m" corresp="#s2:l2">anghstigheyt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Een helder licht dat <rhyme label="c" type="f">scheen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Hier in de <rhyme label="d" type="m">duysternis</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Een Engel vol van <rhyme label="c" type="f" corresp="#s2:l5">klaerheyt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Die geeft <rhyme label="d" type="m" corresp="#s2:l6">getuychenis</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En vreest niet gy kleyn <rhyme label="a" type="f">vermaerde</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghy Herders <rhyme label="b" type="m">ongeacht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Want siet, u wert hier op <rhyme label="a" type="f" corresp="#s3:l1">aerden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Een blijde maer <rhyme label="b" type="m" corresp="#s3:l2">gebracht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat heden is <rhyme label="c" type="f">geboren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">d' Heer der <rhyme label="d" type="m">heerlijckheyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In doeckjes is <rhyme label="c" type="f" corresp="#s3:l5">gewonden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En in de kribbe <rhyme label="d" type="m" corresp="#s3:l6">leyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dit sal u een teken <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gelijck u is <rhyme label="b" type="m">geseyt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Gods Lof moet zijn <rhyme label="a" type="f" corresp="#s4:l1">gepresen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot in der <rhyme label="b" type="m" corresp="#s4:l2">eeuwigheyt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Hier mee verdwijnt het <rhyme label="c" type="f">licht</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Met al het soet <rhyme label="d" type="m">Mysijck</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">De Hemelsche <rhyme label="c" type="f" corresp="#s4:l5">Heyrscharen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Die gaen na 't <rhyme label="d" type="m" corresp="#s4:l6">Hemelrijck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Herdertjes die <rhyme label="a" type="f">vervoegen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Haer stracx na <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Verwondert en vol <rhyme label="a" type="f" corresp="#s5:l1">vernoegen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van dese soete <rhyme label="b" type="m" corresp="#s5:l2">stem</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Sy vinden 't kleyne <rhyme label="c" type="f">kint</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Gelijck haer was <rhyme label="d" type="m">geseyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Sy loven en sy <rhyme label="c" type="f" corresp="#s5:l5">prijsen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Godts groote <rhyme label="d" type="m" corresp="#s5:l6">mogentheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Komt nu ghy <rhyme label="a" type="f">uytverkoren</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Komt nu ghy soete <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">U Bruydegom is <rhyme label="a" type="f" corresp="#s6:l1">geboren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Singht nu met vreughden <rhyme label="b" type="m" corresp="#s6:l2">uyt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">U Salighmaker <rhyme label="c" type="f">reyn</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die siet ghy hier <rhyme label="d" type="m">voorwaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Wilt hem uyt Liefd' <rhyme label="c" type="f" corresp="#s6:l5">omhelsen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Hier in dit <rhyme label="d" type="m" corresp="#s6:l6">Nieuwe-Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
