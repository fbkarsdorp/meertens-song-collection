      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Stokkendans</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GAlathea soud' ik <rhyme label="a" type="f">uyten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wat al soete <rhyme label="b" type="m">waerdigheyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Vwe oogjes in sich <rhyme label="a" type="f" corresp="#s1:l1">sluyten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wen gy die min-geerig <rhyme label="b" type="m" corresp="#s1:l2">spreyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Op mijn oogen, om te <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Of mijn oogen op u <rhyme label="b" type="m">sien</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Soo diend' ik wel <rhyme label="a" type="f" corresp="#s2:l1">opgetogen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">By de blijde <rhyme label="b" type="m" corresp="#s2:l2">Hemel-lie'n</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Om bevrijt van aertse <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Met een Hemelsche <rhyme label="b" type="m">Basuyn</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Vyt te blaesen, gy meugt <rhyme label="a" type="f" corresp="#s3:l1">winnen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Mirth, en Laurier op u <rhyme label="b" type="m" corresp="#s3:l2">Kruyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">En tot diens beduydingh <rhyme label="a" type="f">melde</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Wat voor reden mijn <rhyme label="b" type="m">geboon</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Dat ik uwe waerde <rhyme label="a" type="f" corresp="#s4:l1">stelde</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">In het Koor der Aertse <rhyme label="b" type="m" corresp="#s4:l2">Goon</rhyme>;</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Want ik vindt soo hooge <rhyme label="a" type="f">dingen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">In de kas van u <rhyme label="b" type="m">gesicht</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">Wat ik geensints kan <rhyme label="a" type="f" corresp="#s5:l1">volbringen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">'t Wit van mijn verbonde <rhyme label="b" type="m" corresp="#s5:l2">plicht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Lieve sterren, <rhyme label="a" type="f">stooke-brantjes</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Van een ongewende <rhyme label="b" type="m">vlam</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Overwelft met bruyne <rhyme label="a" type="f" corresp="#s6:l1">rantjes</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Dat ik dese stoutheyt <rhyme label="b" type="m" corresp="#s6:l2">nam</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Om van u waerdy te <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">'t Schiede door des sellefs <rhyme label="b" type="m">kracht</rhyme>;</l>
        <l id="s7:l3" met="4" type="+-">Want sy mijn gemoet <rhyme label="a" type="f" corresp="#s7:l1">bedwingen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En ontmachten al mijn <rhyme label="b" type="m" corresp="#s7:l2">macht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
