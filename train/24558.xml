<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24558</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Nobis est natus hodie</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ONs is gebaren nv ter <rhyme label="a" type="m">tys</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Jhesus christus <rhyme label="a" type="m" corresp="#s1:l1">gebenedyt</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Hi is god ende mensche des seker <rhyme label="a" type="m" corresp="#s1:l1">syt</rhyme></l>
        <l id="s1:l4" met="4" type="-+">die ons van sonden heuet <rhyme label="a" type="m" corresp="#s1:l1">gevryet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den hemel ende eerde niet en conde <rhyme label="a" type="m">beuaen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">den heuet een teder maget <rhyme label="a" type="m" corresp="#s2:l1">ontfaen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">in hoeren weerden lichaem heeft sy hem <rhyme label="a" type="m" corresp="#s2:l1">beuaen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">mit enen woerde heeft sy hem <rhyme label="a" type="m" corresp="#s2:l1">ontfaen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die maget waert moeder ende baerden een <rhyme label="a" type="m">kynt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">oer maegdom bleef al <rhyme label="a" type="m" corresp="#s3:l1">ongeschent</rhyme></l>
        <l id="s3:l3" met="4" type="-+">beulecket soe en waert sy nye ter <rhyme label="a" type="m" corresp="#s3:l1">tyt</rhyme></l>
        <l id="s3:l4" met="4" type="-+">mer sy bleef maget van gade <rhyme label="a" type="m" corresp="#s3:l1">gemynt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat kynt wiesche op ende waert een <rhyme label="a" type="m">man</rhyme></l>
        <l id="s4:l2" met="4" type="-+">den heelschen drakjen hi <rhyme label="a" type="m" corresp="#s4:l1">verwan</rhyme></l>
        <l id="s4:l3" met="4" type="-+">daer alle onse salicheit leget <rhyme label="a" type="m" corresp="#s4:l1">aen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">daer ewige salicheit leget <rhyme label="a" type="m" corresp="#s4:l1">aen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Voer ons heeft hi <rhyme label="a" type="m">genoechgedaen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">hi heeft van mynnen den doet <rhyme label="a" type="m" corresp="#s5:l1">ontfaen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">dat hemelryke heeft hi <rhyme label="a" type="m" corresp="#s5:l1">opgedaen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">dat lange geslaten hadde <rhyme label="a" type="m" corresp="#s5:l1">gestaen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Desen kynde soe bidden <rhyme label="a" type="m">wy</rhyme></l>
        <l id="s6:l2" met="4" type="-+">dat vander reynre maget <rhyme label="a" type="m" corresp="#s6:l1">vry</rhyme></l>
        <l id="s6:l3" met="4" type="-+">gebaren wolde <rhyme label="a" type="m" corresp="#s6:l1">weerden</rhyme></l>
        <l id="s6:l4" met="4" type="-+">dat hy ons arme sundaren genadich <rhyme label="a" type="m" corresp="#s6:l1">sy</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wy bidden der moeder van desen <rhyme label="a" type="m">kynde</rhyme></l>
        <l id="s7:l2" met="4" type="-+">dat sy in onse hulpe <rhyme label="a" type="m" corresp="#s7:l1">sende</rhyme></l>
        <l id="s7:l3" met="4" type="-+">oer kynt tot desen lesten <rhyme label="a" type="m" corresp="#s7:l1">eynde</rhyme></l>
        <l id="s7:l4" met="4" type="-+">dat ons die viant niet en <rhyme label="a" type="m" corresp="#s7:l1">schende</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maria soe heyt die maget  <rhyme label="a" type="m">reyne</rhyme></l>
        <l id="s8:l2" met="4" type="-+">dat kynt heit Jhesus van <rhyme label="a" type="m" corresp="#s8:l1">naserene</rhyme></l>
        <l id="s8:l3" met="4" type="-+">wi bidden hem dat hi ons <rhyme label="a" type="m" corresp="#s8:l1">verleen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">syne gracie want onse doget is <rhyme label="a" type="m" corresp="#s8:l1">cleyn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Lof glorie eer ende <rhyme label="a" type="m">weerdicheit</rhyme></l>
        <l id="s9:l2" met="4" type="-+">die moet hem syn in <rhyme label="a" type="m" corresp="#s9:l1">ewicheit</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Want hi ons allen heuet <rhyme label="a" type="m" corresp="#s9:l1">bereyt</rhyme></l>
        <l id="s9:l4" met="4" type="-+">die wech totter ewiger <rhyme label="a" type="m" corresp="#s9:l1">salicheit</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
