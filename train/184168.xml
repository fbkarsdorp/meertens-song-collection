  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">‘Klein kleu-ter--ke, klein <rhyme label="a" type="m">‘kleu-ter-ke</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Wat doet gij in mijn <rhyme label="b" type="m">hof</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Gij plukt er al de bloempjes <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En maakt het veel te <rhyme label="b" type="m" corresp="#s1:l2">grof</rhyme>!’</l>
      </lg>
    </lg>
