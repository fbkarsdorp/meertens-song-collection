<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186900</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Nieuw Amsterdam</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LIef en waardig <rhyme label="a" type="m">Vaderland</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wellust van myn hart en <rhyme label="b" type="f">Oogen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ach! word gy dus <rhyme label="a" type="m" corresp="#s1:l1">aangerand</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Van een snoode <rhyme label="a" type="m" corresp="#s1:l1">Dwingeland</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Die u schelms naer 't Harte <rhyme label="c" type="m">steekt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Trouwloos de Verbonde <rhyme label="c" type="m" corresp="#s1:l5">breekt</rhyme>!</l>
        <l id="s1:l7" met="4" type="-+">Wel eer door 's Voorzaets wys <rhyme label="d" type="m">beleid</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Tot beider heil <rhyme label="d" type="m" corresp="#s1:l7">bereid</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maer Ã´ Heilloos basterd <rhyme label="a" type="m">zaedt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ge hebt u schande'yk <rhyme label="b" type="f">misgerekend</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Doe gy d'Eendragt onzer <rhyme label="a" type="m" corresp="#s2:l1">Staet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Zogt te breeke door <rhyme label="a" type="m" corresp="#s2:l1">verraedt</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Neen dien Broederlyke <rhyme label="c" type="m">band</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">Is te sterk voor uwe <rhyme label="c" type="m" corresp="#s2:l5">Hand</rhyme>;</l>
        <l id="s2:l7" met="4" type="-+">Al waert ge een Duivel in Engels <rhyme label="d" type="m">schyn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">'t Zou uw onmooglyk <rhyme label="d" type="m" corresp="#s2:l7">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Oproermakers, snood <rhyme label="a" type="m">gebroed</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Schenders onzer braave <rhyme label="b" type="f">Vaders</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Niet alleen 't vergankelyk <rhyme label="a" type="m" corresp="#s3:l1">Goed</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Maer ons laeste druppel <rhyme label="a" type="m" corresp="#s3:l1">bloed</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Geven wy zeer graeg te <rhyme label="c" type="m">Pand</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Voor het lieve <rhyme label="c" type="m" corresp="#s3:l5">Vaderland</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Ten trots van u vervloekte <rhyme label="d" type="m">nydt</rhyme>!</l>
        <l id="s3:l8" met="3" type="-+">Die haast u val <rhyme label="d" type="m" corresp="#s3:l7">bereidt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Beef! nu Hollands Leeuw <rhyme label="a" type="m">ontwaakt</rhyme>!</l>
        <l id="s4:l2" met="4" type="+-">Beeft Trouweloose <rhyme label="b" type="f">Bondgenooten</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Want als hy aen 't woede <rhyme label="a" type="m" corresp="#s4:l1">raekt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En in toorne hevig <rhyme label="a" type="m" corresp="#s4:l1">blaekt</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Kan geen Dog hem weerstand <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Maar moet schandelyk voor hem <rhyme label="c" type="m" corresp="#s4:l5">vlien</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Triomph! myn Vaderland gy <rhyme label="d" type="m">zult</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Verwinnen, hebt <rhyme label="d" type="m" corresp="#s4:l7">gedult</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
