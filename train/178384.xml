<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178384</id>
    <date>
      <year-start>1623</year-start>
      <year-end>1623</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Maagd rein eerbaar van zeden</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Maghet schoon en jent van <rhyme label="a" type="f">seden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wilt ghy nu hooren naer mijn <rhyme label="a" type="f" corresp="#s1:l1">reden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Het sal u saligh zijn <rhyme label="b" type="m">veurwaer</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als ghy sult zijn <rhyme label="a" type="f" corresp="#s1:l1">overleden</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En begraven hier <rhyme label="a" type="f" corresp="#s1:l1">beneden</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Daerom neemt den tijdt nu <rhyme label="b" type="m" corresp="#s1:l3">waer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick weet alreed’ wat ghy wilt <rhyme label="a" type="f">praten</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Dat ick de weirelt sou <rhyme label="a" type="f" corresp="#s2:l1">verlaten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En my begheven metter <rhyme label="b" type="m">vliet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Tot de Gheestelijcke <rhyme label="a" type="f" corresp="#s2:l1">staten</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dien raer wil ick niet <rhyme label="a" type="f" corresp="#s2:l1">vaten</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Neen, neen, neen men doeter <rhyme label="b" type="m" corresp="#s2:l3">niet</rhyme>/</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy moest my emmers eerst hooren <rhyme label="a" type="f">spreken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Want vanden wech zijt ghy <rhyme label="a" type="f" corresp="#s3:l1">gheweken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wilt ghty de weirelt dus volghen <rhyme label="b" type="m">naer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Sy sal u met loose <rhyme label="a" type="f" corresp="#s3:l1">treken</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Leyden tot soo veel <rhyme label="a" type="f" corresp="#s3:l1">ghebreken</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Daerom neempty den tijt nu <rhyme label="b" type="m" corresp="#s3:l3">waer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Kan ick naer de weirelt niet <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Sonder de sonden aen te <rhyme label="a" type="f" corresp="#s4:l1">cleven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En mijn siel brenghen in’t <rhyme label="b" type="m">verdriet</rhyme>?</l>
        <l id="s4:l4" met="4" type="+-">Maer staet dit doch nu <rhyme label="a" type="f" corresp="#s4:l1">gheschreven</rhyme>?</l>
        <l id="s4:l5" met="4" type="+-">Spreeckt dat Godt de Heer <rhyme label="a" type="f" corresp="#s4:l1">verheven</rhyme>?</l>
        <l id="s4:l6" met="4" type="+-">Neen, neen, neen, men doeter <rhyme label="b" type="m" corresp="#s4:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De weirelt volghen en Godt den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En can niet wesen (naer Christus <rhyme label="a" type="f" corresp="#s5:l1">leere</rhyme>)</l>
        <l id="s5:l3" met="4" type="-+">Den wech des levens is engh dats <rhyme label="b" type="m">claer</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Die ons leydt tot ‘shemels <rhyme label="a" type="f" corresp="#s5:l1">eere</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Dus al zijdy jonck en <rhyme label="a" type="f" corresp="#s5:l1">teere</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Wilt den tijt wel nemen <rhyme label="b" type="m" corresp="#s5:l3">waer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Soud’ ick de weirelt <rhyme label="a" type="f">abandonneren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ick ben ghewoon te <rhyme label="a" type="f" corresp="#s6:l1">domineren</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Oock dansen en springhen als een <rhyme label="b" type="m">riet</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met de gilden vreught <rhyme label="a" type="f" corresp="#s6:l1">hanteren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Zoo laet ick den tijt <rhyme label="a" type="f" corresp="#s6:l1">passeren</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Neen, neen, neen, men doeter <rhyme label="b" type="m" corresp="#s6:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Och peyst doch op de leste <rhyme label="a" type="f">daghen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Als Godt nae rekeningh sal <rhyme label="a" type="f" corresp="#s7:l1">vraghen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Daer’t al sal worden <rhyme label="b" type="m">openbaer</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">O dan suldy wel <rhyme label="a" type="f" corresp="#s7:l1">beclaghen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Dat ghy dit hebt derren <rhyme label="a" type="f" corresp="#s7:l1">waghen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Daerom neempt den tijt nu <rhyme label="b" type="m" corresp="#s7:l3">waer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dat ick hier op veel sou <rhyme label="a" type="f">beraden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Het mocht aen mijn ghesontheydt <rhyme label="a" type="f" corresp="#s8:l1">schaden</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Dus triumpheer ick alsoo ghy <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Ick betrou op Godts <rhyme label="a" type="f" corresp="#s8:l1">ghenaden</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Die my comen sal ter <rhyme label="a" type="f" corresp="#s8:l1">staden</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Neen, neen, neen, men doeter <rhyme label="b" type="m" corresp="#s8:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Siet hoe de jonckheyt wordt <rhyme label="a" type="f">bedroghen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Als sy ‘sweirels melck heeft <rhyme label="a" type="f" corresp="#s9:l1">ghesoghen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En meynt te leven sonder <rhyme label="b" type="m">beswaer</rhyme></l>
        <l id="s9:l4" met="4" type="+-">De weirelt dient sy naer <rhyme label="a" type="f" corresp="#s9:l1">vermoghen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">En inder hellen wordt <rhyme label="a" type="f" corresp="#s9:l1">ghetoghen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Daerom neemt den tijt nu <rhyme label="b" type="m" corresp="#s9:l3">waer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
