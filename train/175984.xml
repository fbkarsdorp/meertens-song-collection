      <year-start>1640</year-start>
      <year-end>1640</year-end>
    </date>
    <location>
      <orginal-name>deventer</orginal-name>
      <standard-name>Deventer, The Netherlands</standard-name>
      <coordinates>
        <lon>52.2532272</lon>
        <lat>6.1584725</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 026 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Heere, doet my <rhyme label="a" type="m">recht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want als een willich <rhyme label="a" type="m" corresp="#s1:l1">knecht</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Betracht ich u geboden <rhyme label="b" type="m">al</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Ick heb op u <rhyme label="c" type="f">vertrouwet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daerom my niet en <rhyme label="c" type="f" corresp="#s1:l4">grouwet</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Dat ick sal comen tot den <rhyme label="b" type="m" corresp="#s1:l3">val</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Doorgrondt, Heer, mijn <rhyme label="a" type="m">gemoet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Doorsoeckt my in den <rhyme label="a" type="m" corresp="#s2:l1">gloet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Of 't binnen is als 't buyten <rhyme label="b" type="m">toont</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Op mijn gedachten <rhyme label="c" type="f">lettet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En op de proeve <rhyme label="c" type="f" corresp="#s2:l4">settet</rhyme></l>
        <l id="s2:l6" met="4" type="-+">'t Verborgenste dat in my <rhyme label="b" type="m" corresp="#s2:l3">woont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Want, Heer, de oogen <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Seer vast geslagen <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Op uwe goetheyt en <rhyme label="b" type="m">genae</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ick andel inde <rhyme label="c" type="f">claerheyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van uwe trouwe <rhyme label="c" type="f" corresp="#s3:l4">waerheyt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Die my geleydet waer ick <rhyme label="b" type="m" corresp="#s3:l3">gae</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De schalcken welcker <rhyme label="a" type="m">mont</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Is anders als haer <rhyme label="a" type="m" corresp="#s4:l1">gront</rhyme></l>
        <l id="s4:l3" met="4" type="-+">In haer geselschap ick niet <rhyme label="b" type="m">com</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Van de bedeckte <rhyme label="c" type="f">lieden</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wil ick gants verre <rhyme label="c" type="f" corresp="#s4:l4">vlieden</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">En met haer nimmermeer gaen <rhyme label="b" type="m" corresp="#s4:l3">om</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De <rhyme label="a" type="m">tsamen-rottery</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Der valschen ick <rhyme label="a" type="m" corresp="#s5:l1">vermy</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ia hatese uyt al mijn <rhyme label="b" type="m">hert</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Men sach my noyt <rhyme label="c" type="f">verkeeren</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Met die die God <rhyme label="c" type="f" corresp="#s5:l4">onteeren</rhyme></l>
        <l id="s5:l6" met="4" type="-+">En in de boosheyt zijn <rhyme label="b" type="m" corresp="#s5:l3">verwert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick wasch mijn handen <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">So ick 't van herten <rhyme label="a" type="m" corresp="#s6:l1">meyn</rhyme></l>
        <l id="s6:l3" met="4" type="-+">In onschult suyver ende <rhyme label="b" type="m">clear</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Met offer ick u <rhyme label="c" type="f">(Heere</rhyme>)</l>
        <l id="s6:l5" met="3" type="-+">En met gebeden <rhyme label="c" type="f" corresp="#s6:l4">eere</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Aen uwen heyligen <rhyme label="b" type="m" corresp="#s6:l3">altaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daer seg' ick met <rhyme label="a" type="m">gesanck</rhyme></l>
        <l id="s7:l2" met="3" type="-+">V, Heere, lof en <rhyme label="a" type="m" corresp="#s7:l1">danck</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En roeme met een bly <rhyme label="b" type="m">geschal</rhyme></l>
        <l id="s7:l4" met="3" type="-+">V wonderlijcke <rhyme label="c" type="f">wercken</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Die weerdich syn <rhyme label="c" type="f" corresp="#s7:l4">t'aenmercken</rhyme></l>
        <l id="s7:l6" met="4" type="-+">En te doen hooren over <rhyme label="b" type="m" corresp="#s7:l3">al</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick hebbe seer <rhyme label="a" type="m">bemint</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En innerlijck <rhyme label="a" type="m" corresp="#s8:l1">besint</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Het huys daer ghy, o Heere, <rhyme label="b" type="m">woont</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">De hutte hier <rhyme label="c" type="f">beneden</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Daer boven alle <rhyme label="c" type="f" corresp="#s8:l4">steden</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Ghy uwe heerlijckheyt in <rhyme label="b" type="m" corresp="#s8:l3">toont</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Laet mijne ziele <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Geraken int <rhyme label="a" type="m" corresp="#s9:l1">verdriet</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Met hen die drijven <rhyme label="b" type="m">overmoet</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">En raept niet wech mijn <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Met dien die haer <rhyme label="c" type="f" corresp="#s9:l4">begeven</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Te storten het onnosel <rhyme label="b" type="m" corresp="#s9:l3">bloet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">In hare handen <rhyme label="a" type="m">wreet</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Is allerhande <rhyme label="a" type="m" corresp="#s10:l1">leet</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Dat haer gemoet bedencken <rhyme label="b" type="m">can</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Na giften en <rhyme label="c" type="f">gaven</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Sy seer begeerich <rhyme label="c" type="f" corresp="#s10:l4">draven</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Tot schade van den vromen <rhyme label="b" type="m" corresp="#s10:l3">man</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Maer ick wil, Heere, <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Op uwe rechte <rhyme label="a" type="m" corresp="#s11:l1">baen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">In eenvout met een suyver <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Bewijst my u <rhyme label="c" type="f">genade</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Hoed't my voor allen <rhyme label="c" type="f" corresp="#s11:l4">quade</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Verlosset my uyt druck en <rhyme label="b" type="m" corresp="#s11:l3">smert</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Mijn voet, o Heere, <rhyme label="a" type="m">vast</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Op uwe wegen <rhyme label="a" type="m" corresp="#s12:l1">past</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Dies wil ick met een rechten <rhyme label="b" type="m">vlijt</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Ter plaetse daer de <rhyme label="c" type="f">vroomen</rhyme></l>
        <l id="s12:l5" met="3" type="-+">By-een met hoopen <rhyme label="c" type="f" corresp="#s12:l4">comen</rhyme></l>
        <l id="s12:l6" met="4" type="-+">V lof verconden t'aller <rhyme label="b" type="m" corresp="#s12:l3">tijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
