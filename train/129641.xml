  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gun my, mijn <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Uw mondje zoet te <rhyme label="b" type="f">drukken</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Tot koeling van mijn <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Laat ik die vrugten <rhyme label="b" type="f" corresp="#s1:l2">Plukken</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daar ik so lang een <rhyme label="c" type="m">tyd</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met zugten en <rhyme label="d" type="m">geklag</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Om smeekend' heb <rhyme label="c" type="m" corresp="#s1:l5">gevryt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En nog vry al den <rhyme label="d" type="m" corresp="#s1:l6">dag</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Of zo dit brand <rhyme label="a" type="m">ontsteekt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In plaats van die te <rhyme label="b" type="f">blussen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En meer begeerte <rhyme label="a" type="m" corresp="#s2:l1">kweekt</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Laat ik u dan vry <rhyme label="b" type="f" corresp="#s2:l2">kussen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Tot gy eens <rhyme label="c" type="m">mêgevoelt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Hoe veel ik lyden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En hoe mijn hart nu <rhyme label="c" type="m" corresp="#s2:l5">doelt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Op ‘t allerzoetste <rhyme label="d" type="m" corresp="#s2:l6">zoet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Of meent gy dat die <rhyme label="a" type="m">vlam</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die my soo kan <rhyme label="b" type="f">ontvonken</rhyme></l>
        <l id="s3:l3" met="3" type="-+">En die zyn voetsel <rhyme label="a" type="m" corresp="#s3:l1">nam</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Door uwe lieve <rhyme label="b" type="f" corresp="#s3:l2">lonke</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Nu veel te hoog mogt <rhyme label="c" type="m">gaan</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Of dat hy an ons <rhyme label="d" type="m">hart</rhyme></l>
        <l id="s3:l7" met="3" type="-+">En zinnen ooit zou <rhyme label="c" type="m" corresp="#s3:l5">schaân</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En wekken pijn en <rhyme label="d" type="m" corresp="#s3:l6">smart</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wij konden dese <rhyme label="a" type="m">pijn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Indien gy woud, ligt <rhyme label="b" type="f">heelen</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Moest gy haar oorsprong <rhyme label="a" type="m" corresp="#s4:l1">zijn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Laat ons die smart dan <rhyme label="b" type="f" corresp="#s4:l2">deelen</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Zo zullen wy, in ‘t <rhyme label="c" type="m">kort</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Geneesen zyn, so <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat min ons t’elkens <rhyme label="c" type="m" corresp="#s4:l5">port</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat men die   weder <rhyme label="d" type="m" corresp="#s4:l6">voed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
