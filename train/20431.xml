<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20431</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Phoebus is lang over de zee</melody>
  <author>B. Targier, Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HErders kint het geeft mijn <rhyme label="a" type="m">vreemt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Door 't verloopen van u <rhyme label="b" type="f">jaren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat ghy niet een Vryer <rhyme label="a" type="m" corresp="#s1:l1">neemt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Om u schaepjens te <rhyme label="b" type="f" corresp="#s1:l2">bewaren</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Och of ick u krijgen <rhyme label="c" type="m">kon</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'kWedd' mijn overschoone <rhyme label="c" type="m" corresp="#s1:l5">Son</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Geen trouwer Dienaer <rhyme label="c" type="m" corresp="#s1:l5">von</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Smorgens soud' ick met de <rhyme label="a" type="m">schop</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Vullis van u stal op <rhyme label="b" type="f">rapen</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">'Savonts soud' ick passen <rhyme label="a" type="m" corresp="#s2:l1">op</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Water pompen voor u <rhyme label="b" type="f" corresp="#s2:l2">Schapen</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">'k Soud' mijn vlijt en yver <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Om u schaepjes teer te <rhyme label="c" type="m" corresp="#s2:l5">voen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Meer als ghy soud' <rhyme label="c" type="m" corresp="#s2:l5">vermoen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Souse voeren dat u <rhyme label="a" type="m">lust</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Met de beste koeck en <rhyme label="b" type="f">boonen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Smorgens als ghy laecht tot <rhyme label="a" type="m" corresp="#s3:l1">rust</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Soud' ick die met stroo <rhyme label="b" type="f" corresp="#s3:l2">verschoonen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Passen op dat ick de <rhyme label="c" type="m">Koy</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Suyver maeck van stinckent <rhyme label="c" type="m" corresp="#s3:l5">hoy</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En weer met stroo <rhyme label="c" type="m" corresp="#s3:l5">bestroy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Voorders wat het huys <rhyme label="a" type="m">belanght</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sal ick vloer en solder <rhyme label="b" type="f">veegen</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Maecken dat ghy my <rhyme label="a" type="m" corresp="#s4:l1">bedanckt</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Stoocken 't vyer, en d'as uyt <rhyme label="b" type="f" corresp="#s4:l2">dreeghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">'kSouw mijn winst wel nemen <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Niet een pintjen hier of <rhyme label="c" type="m" corresp="#s4:l5">daer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Verteeren in een <rhyme label="c" type="m" corresp="#s4:l5">jaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sooje sieck, of suchtigh <rhyme label="a" type="m">wert</rhyme>:</l>
        <l id="s5:l2" met="4" type="+-">Sal ick om een Doctor <rhyme label="b" type="f">loopen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Tot verlichtingh van u <rhyme label="a" type="m" corresp="#s5:l1">smert</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Sal ick wijn en suycker <rhyme label="b" type="f" corresp="#s5:l2">koopen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Krijgh j'een kint, dat sel ick <rhyme label="c" type="m">mee</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Draghen, als wy buyten <rhyme label="c" type="m" corresp="#s5:l5">Stee</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Wandelen met ons <rhyme label="c" type="m" corresp="#s5:l5">twee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Heb mijn Vryster datje 't <rhyme label="a" type="m">wist</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Noch veel raer en schoon <rhyme label="b" type="f">Juweelen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Met een pot-stick in mijn <rhyme label="a" type="m" corresp="#s6:l1">kist</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">'k Heb noch van dat ick liep <rhyme label="b" type="f" corresp="#s6:l2">spelen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Twintigh pont, of daer <rhyme label="c" type="m">ontrent</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Alle jaer tot eene <rhyme label="c" type="m" corresp="#s6:l5">rent</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Van Besjes <rhyme label="c" type="m" corresp="#s6:l5">Testament</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'k Hebje nu ghenoegh <rhyme label="a" type="m">gheseyt</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Wilje nu soo moet je <rhyme label="b" type="f">spreecken</rhyme>?</l>
        <l id="s7:l3" met="4" type="+-">Seghme nu in 't kort <rhyme label="a" type="m" corresp="#s7:l1">bescheyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Of ick laet het vryen <rhyme label="b" type="f" corresp="#s7:l2">steken</rhyme>?</l>
        <l id="s7:l5" met="4" type="+-">Sooje nou niet kort <rhyme label="c" type="m">beraet</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Datje my de koop toe <rhyme label="c" type="m" corresp="#s7:l5">slaet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Denckt vry dat ickje <rhyme label="c" type="m" corresp="#s7:l5">laet</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
