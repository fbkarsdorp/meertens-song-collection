<song>
  <meta>
    <id>12026</id>
    <date>
      <year-start>1617</year-start>
      <year-end>1617</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Adieu nymphe du bois</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LOf Maria seer <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Schoon suyuer claer <rhyme label="a" type="m" corresp="#s1:l1">fonteyn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Godts Tabernakel <rhyme label="b" type="f">schoone</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Wy menschen groot en <rhyme label="a" type="m" corresp="#s1:l1">kleyn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Louen v al <rhyme label="a" type="m" corresp="#s1:l1">ghemeyn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met sanghen soet van <rhyme label="b" type="f" corresp="#s1:l3">thoone</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon als den <rhyme label="a" type="m">dagheraet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met Goddelick <rhyme label="a" type="m" corresp="#s2:l1">cieraet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Is u licht claer in’t <rhyme label="b" type="f">blincken</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Want ghy altijts <rhyme label="a" type="m" corresp="#s2:l1">bystaet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Met troost vroech ende <rhyme label="a" type="m" corresp="#s2:l1">laet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die om v louen <rhyme label="b" type="f" corresp="#s2:l3">dincken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy zijn ons leuens <rhyme label="a" type="m">vreucht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Daer Godt in heeft <rhyme label="a" type="m" corresp="#s3:l1">verheucht</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Want bouen alle <rhyme label="b" type="f">vrouwen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sijt ghy vol aller <rhyme label="a" type="m" corresp="#s3:l1">deucht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Naest Godt ghy veel <rhyme label="a" type="m" corresp="#s3:l1">vermeucht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ghy helpt ons uyt <rhyme label="b" type="f" corresp="#s3:l3">benauwen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vol van ghenaden <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghy, oock <rhyme label="a" type="m" corresp="#s4:l1">ghebenedijt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Maria hoogh <rhyme label="b" type="f">verheuen</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Dus seggh’ ick <rhyme label="a" type="m" corresp="#s4:l1">onuermijt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Den ketteren tot <rhyme label="a" type="m" corresp="#s4:l1">spijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Iont ons het eeuwich <rhyme label="b" type="f" corresp="#s4:l3">leuen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Princersse Maghet <rhyme label="a" type="m">wijs</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Aarons bloeyende <rhyme label="a" type="m" corresp="#s5:l1">rijs</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Lof moet v altijts <rhyme label="b" type="f">wesen</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">V zy eer’ ende <rhyme label="a" type="m" corresp="#s5:l1">prijs</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Met lofsanghen <rhyme label="a" type="m" corresp="#s5:l1">propijs</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Reyn’ Maghet <rhyme label="b" type="f" corresp="#s5:l3">uytghelesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
