<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190479</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Figaro</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Keziaanen en <rhyme label="a" type="f">Salmieten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Lecht je stompe waapens <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Al dat vegten, al dat <rhyme label="a" type="f" corresp="#s1:l1">schieten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Is maar drygen en niets <rhyme label="b" type="m" corresp="#s1:l2">meer</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat gesnork moet elk <rhyme label="a" type="f" corresp="#s1:l1">verdrieten</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Vlugt maar vaardig uit het <rhyme label="c" type="m">Lant</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Of je raakt voor vast van <rhyme label="c" type="m" corresp="#s1:l6">kant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zints dat Brunswyk kwam <rhyme label="a" type="f">verscheinen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Op zyns grooten Vorst <rhyme label="b" type="m">bevel</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Moet je als de rook <rhyme label="a" type="f" corresp="#s2:l1">verdwynen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Of licht raaken in de <rhyme label="b" type="m" corresp="#s2:l2">knel</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Je kunt ons niet <rhyme label="a" type="f" corresp="#s2:l1">ondermynen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Met je raazen en <rhyme label="c" type="m">getier</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Pak je biezen maar van <rhyme label="c" type="m" corresp="#s2:l6">hier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wilt ons langer niet <rhyme label="a" type="f">braveeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Steunt niet op je zwakke <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Gaat te saamen <rhyme label="a" type="f" corresp="#s3:l1">Exerceeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">In des Afgronds duistre <rhyme label="b" type="m" corresp="#s3:l2">nacht</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Daar zal Heintje Pik je <rhyme label="a" type="f" corresp="#s3:l1">leeren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Hoe de boosheid werdt <rhyme label="c" type="m">gestraft</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Schoon je daar ook tegen <rhyme label="c" type="m" corresp="#s3:l6">blaft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Al de Nickers zullen <rhyme label="a" type="f">schreeuwen</rhyme>.</l>
        <l id="s4:l2" met="4" type="+-">Op Vorst Plutoos streng <rhyme label="b" type="m">bevel</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Haal by ons die barsche <rhyme label="a" type="f" corresp="#s4:l1">Leeuwen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Uit de stal hier in de <rhyme label="b" type="m" corresp="#s4:l2">Hel</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dan zullen die naakte <rhyme label="a" type="f" corresp="#s4:l1">Spreeuwen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Allen beeven als een <rhyme label="c" type="m">riet</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">In het Onderaards <rhyme label="c" type="m" corresp="#s4:l6">gebiet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maar dit Lied wierdt niet <rhyme label="a" type="f">gepreezen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Kwam Kees Salm niet by dien <rhyme label="b" type="m">stoet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als het hooft van al de <rhyme label="a" type="f" corresp="#s5:l1">Keezen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Voegt hy mee by 't Helsch <rhyme label="b" type="m" corresp="#s5:l2">Gebroet</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Waar Prins Willem niet <rhyme label="a" type="f" corresp="#s5:l1">verreezen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Om te keeren het <rhyme label="c" type="m">gewelt</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Was ons Land needer <rhyme label="c" type="m" corresp="#s5:l6">gevelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laaten wy dien Held dan <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Om dees vroolyke <rhyme label="b" type="m">ommekeer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Door zyn moedig <rhyme label="a" type="f" corresp="#s6:l1">Triompheeren</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Bloeit de gulde Vryheid <rhyme label="b" type="m" corresp="#s6:l2">weer</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Wy genieten ons <rhyme label="a" type="f" corresp="#s6:l1">begeeren</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En het treuren heeft nu <rhyme label="c" type="m">uit</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Wyl het woeden is <rhyme label="c" type="m" corresp="#s6:l6">gestuit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
