<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159072</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Iets moet ik u Laura vragen</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KOude, lange, nare <rhyme label="a" type="f">nachten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Dringen ons verstant en <rhyme label="a" type="f" corresp="#s1:l1">krachten</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Tot haer rechte <rhyme label="b" type="m">weder-deel</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Wie soud' niet het Paren <rhyme label="c" type="f">wenschen</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Want een Lichaem, en twee <rhyme label="c" type="f" corresp="#s1:l4">menschen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Warmen eens en eens soo <rhyme label="b" type="m" corresp="#s1:l3">veel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maer hoe hoogh ons' sinnen <rhyme label="a" type="f">malen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Boven alle Hemels <rhyme label="a" type="f" corresp="#s2:l1">stralen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Is'er een, die twee <rhyme label="b" type="m">ver-eent</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Is'er een, die haer by <rhyme label="c" type="f">namen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Roept, en voeght voor af te <rhyme label="c" type="f" corresp="#s2:l4">samen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Eer de een de ander <rhyme label="b" type="m" corresp="#s2:l3">meent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Blijckt hier op den dagh van <rhyme label="a" type="f">huyden</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Aen dees twee getrouwde <rhyme label="a" type="f" corresp="#s3:l1">Luyden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Onse Bruydegom en <rhyme label="b" type="m">Bruyt</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Als de Speel-noots komen <rhyme label="c" type="f">t'samen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En een-drachtigh seggen <rhyme label="c" type="f" corresp="#s3:l4">Amen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Daer mee is het praetjen <rhyme label="b" type="m" corresp="#s3:l3">uyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wel dan nieu-getrouwde <rhyme label="a" type="f">Lieden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Zegen, kan ick niet <rhyme label="a" type="f" corresp="#s4:l1">gebieden</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Niemandt doet dit als de <rhyme label="b" type="m">Heer</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Daerom sal ick u toe <rhyme label="c" type="f">wenschen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Saligheyt by Godt en <rhyme label="c" type="f" corresp="#s4:l4">Menschen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dan behoefje doch niet <rhyme label="b" type="m" corresp="#s4:l3">meer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Noch soo wensch ick u te <rhyme label="a" type="f">samen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Datmen noemt u beyder <rhyme label="a" type="f" corresp="#s5:l1">namen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Taet en Mem wat <rhyme label="b" type="m">om-gekromt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Als de Maen in 't <rhyme label="c" type="f">Maeghde-teken</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Preuts en prachtigh <rhyme label="c" type="f" corresp="#s5:l4">uyt-gestreken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Even oudt te woonen <rhyme label="b" type="m" corresp="#s5:l3">komt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Oock soo raed ick de <rhyme label="a" type="f">Speel-nootjes</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Eer verstrammen hare <rhyme label="a" type="f" corresp="#s6:l1">kootjes</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Eer de Grijsert komt <rhyme label="b" type="m">aen-boort</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Nae een goet Partuur te <rhyme label="c" type="f">ramen</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Want een Man en Wijf te <rhyme label="c" type="f" corresp="#s6:l4">samen</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Maken verr' het best <rhyme label="b" type="m" corresp="#s6:l3">accoort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
