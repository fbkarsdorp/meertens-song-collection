  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">BEreidt u te <rhyme label="a" type="m">zaam</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">En maakt u <rhyme label="a" type="m" corresp="#s1:l1">bequaam</rhyme>:</l>
        <l id="s1:l3" met="6" type="+-">Om deez' feest te vieren, en kransjes te <rhyme label="b" type="f">sieren</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Met den zoeten <rhyme label="a" type="m" corresp="#s1:l1">naam</rhyme></l>
        <l id="s1:l5" met="5" type="+-">Van twee nieu verbonden hun trouw te <rhyme label="c" type="f">verkonden</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Blaast nu uyt, ô <rhyme label="a" type="m" corresp="#s1:l1">Faam</rhyme>,</l>
        <l id="s1:l7" met="6" type="-+">Koridons Triomph, die zyn <rhyme label="d" type="f">welbeminde</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Vol liefde komt <rhyme label="d" type="f" corresp="#s1:l7">vinden</rhyme>;</l>
        <l id="s1:l9" met="3" type="+-">Biedt zyn <rhyme label="e" type="m">rechterhand</rhyme>,</l>
        <l id="s1:l10" met="5" type="-+">Om in den trouw, zyn vrindin tot <rhyme label="f" type="m">Vrouw</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Voor hem te <rhyme label="g" type="f">erlangen</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">Haar gunst te <rhyme label="g" type="f" corresp="#s1:l11">ontfangen</rhyme>,</l>
        <l id="s1:l13" met="2" type="-+">Waar door zyn <rhyme label="h" type="m">smert</rhyme>,</l>
        <l id="s1:l14" met="6" type="+-">Naar veele verlangen, eens genezen <rhyme label="h" type="m" corresp="#s1:l13">werd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wie kan dit <rhyme label="a" type="m">aanzien</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">En dan nog <rhyme label="a" type="m" corresp="#s2:l1">ontvlien</rhyme>,</l>
        <l id="s2:l3" met="6" type="+-">Om niet meê te tragten, te mogen <rhyme label="b" type="f">vernagten</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En zyn dienst te <rhyme label="a" type="m" corresp="#s2:l1">bien</rhyme></l>
        <l id="s2:l5" met="5" type="+-">Aan zyn Godinne, om 't hert te <rhyme label="c" type="f">gewinnen</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Ach mogt het <rhyme label="a" type="m" corresp="#s2:l1">geschiên</rhyme>,</l>
        <l id="s2:l7" met="6" type="-+">Dat men van haar mondt de nectar mogt <rhyme label="d" type="f">smaaken</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En dan meê <rhyme label="d" type="f" corresp="#s2:l7">genaaken</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Aan dat groot <rhyme label="e" type="m">geluk</rhyme>,</l>
        <l id="s2:l10" met="5" type="-+">Waar door de gloet van 't <rhyme label="f" type="m">minnesoet</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Ons hert komt te <rhyme label="g" type="f">streelen</rhyme>,</l>
        <l id="s2:l12" met="3" type="+-">De zugtjens kan <rhyme label="g" type="f" corresp="#s2:l11">heelen</rhyme></l>
        <l id="s2:l13" met="2" type="-+">Door liefde <rhyme label="h" type="m">kragt</rhyme>,</l>
        <l id="s2:l14" met="6" type="+-">Op 't praalbedt van Hymen alles werd <rhyme label="h" type="m" corresp="#s2:l13">volbragt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wie werdt niet <rhyme label="a" type="m">verrukt</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Wanneer men eens <rhyme label="a" type="m" corresp="#s3:l1">drukt</rhyme></l>
        <l id="s3:l3" met="6" type="+-">De korale lipjes, en denkt om de <rhyme label="b" type="f">tipjes</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Ach wat zoet <rhyme label="a" type="m" corresp="#s3:l1">geluk</rhyme>!</l>
        <l id="s3:l5" met="5" type="+-">ô God der minne! wie voelt niet van <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat uw pyltje <rhyme label="a" type="m" corresp="#s3:l1">drukt</rhyme>!</l>
        <l id="s3:l7" met="6" type="-+">Speelnoodjes neemt moet, het guytjen is <rhyme label="d" type="f">aardig</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">En somtyds heel <rhyme label="d" type="f" corresp="#s3:l7">vaardig</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Hy zyn boogje <rhyme label="e" type="m">spandt</rhyme>;</l>
        <l id="s3:l10" met="5" type="-+">Om dan uw keur, door zyn <rhyme label="f" type="m">faveur</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Zo wonder te <rhyme label="g" type="f">stieren</rhyme>,</l>
        <l id="s3:l12" met="3" type="+-">De hertjens doen <rhyme label="g" type="f" corresp="#s3:l11">vieren</rhyme></l>
        <l id="s3:l13" met="2" type="-+">In ligten <rhyme label="h" type="m">brandt</rhyme>,</l>
        <l id="s3:l14" met="6" type="+-">Waar door uw beminde willig zig <rhyme label="h" type="m" corresp="#s3:l13">verpandt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
