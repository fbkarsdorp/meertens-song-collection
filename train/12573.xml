      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk wil mijn gaen <rhyme label="a" type="f">verheugen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En wesen wel <rhyme label="b" type="m">gemoedt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want dit en is gheen <rhyme label="a" type="f" corresp="#s1:l1">leugen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al die Gods wille <rhyme label="b" type="m" corresp="#s1:l2">doet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En sal hy niet <rhyme label="c" type="f">verlaten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En noch ten geender <rhyme label="d" type="m">tijt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Maer haer komen te <rhyme label="c" type="f" corresp="#s1:l5">baten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Soo ghy volstandigh <rhyme label="d" type="m" corresp="#s1:l6">zijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dit heeftmen sien <rhyme label="a" type="f">ghebeuren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Al aen die jonghen <rhyme label="b" type="m">Helt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Doen hy daer moste <rhyme label="a" type="f" corresp="#s2:l1">treuren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Voor sijn Broers in het <rhyme label="b" type="m" corresp="#s2:l2">velt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Als sy hem woude <rhyme label="c" type="f">moorden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Soo was hy seer <rhyme label="d" type="m">belaen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maer Godt hem noch <rhyme label="c" type="f" corresp="#s2:l5">verhoorden</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat sy hem lieten <rhyme label="d" type="m" corresp="#s2:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daer door is hy <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gekomen tot grooten <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dit is ons voor <rhyme label="a" type="f" corresp="#s3:l1">gheschreven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Datmen niet nae en <rhyme label="b" type="m" corresp="#s3:l2">laet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Om hier voor alle <rhyme label="c" type="f">dinghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Te leven vroom <rhyme label="d" type="m">oprecht</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dan sal sijn Geest <rhyme label="c" type="f" corresp="#s3:l5">ontspringhen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En blijven vast <rhyme label="d" type="m" corresp="#s3:l6">ghehecht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Alsoo hy ginck <rhyme label="a" type="f">betoonen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Aen David jonck en <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Als Samuel hem ginck <rhyme label="a" type="f" corresp="#s4:l1">kroonen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En hem gaf tot een <rhyme label="b" type="m" corresp="#s4:l2">loon</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hoe seer hy waer <rhyme label="c" type="f">verschoven</rhyme></l>
        <l id="s4:l6" met="3" type="-+">By sijn Schaepjens int <rhyme label="d" type="m">gras</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Geraeckte hy noch <rhyme label="c" type="f" corresp="#s4:l5">boven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dat hy een Koningh <rhyme label="d" type="m" corresp="#s4:l6">was</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Vaert wel ghy <rhyme label="a" type="f">Akoleyen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Al aende Zeeuse <rhyme label="b" type="m">strant</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Den Lauwer sal u <rhyme label="a" type="f" corresp="#s5:l1">verspreyen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Al uyt het vaste <rhyme label="b" type="m" corresp="#s5:l2">Landt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Dat ghy moet jeugdigh <rhyme label="c" type="f">bloeyen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In Godes zeghen <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dan sult ghy altijdt <rhyme label="c" type="f" corresp="#s5:l5">groeyen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Gheen dingh u hinder <rhyme label="d" type="m" corresp="#s5:l6">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
