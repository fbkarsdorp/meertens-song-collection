  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MA Maitresse l'autre <rhyme label="a" type="m">jour</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Soupirant pour mais <rhyme label="b" type="f">figure</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Mais dit d'un ton plain <rhyme label="a" type="m" corresp="#s1:l1">d'Amour</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Ture <rhyme label="b" type="f" corresp="#s1:l2">lure</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Chante je vous en <rhyme label="b" type="f" corresp="#s1:l2">consure</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Robin, ture, lure, <rhyme label="b" type="f" corresp="#s1:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Mais voyent dans ces beaux <rhyme label="a" type="m">jeux</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Certain feu qui mais <rhyme label="b" type="f">raisure</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Je le dit qu'aime vous <rhyme label="a" type="m" corresp="#s2:l1">mieux</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Ture <rhyme label="b" type="f" corresp="#s2:l2">lure</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">B Mol, B Kar ou <rhyme label="b" type="f" corresp="#s2:l2">nature</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Robin, ture, lure, <rhyme label="b" type="f" corresp="#s2:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">B Mol dit el &amp; trop <rhyme label="a" type="m">doux</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Le voix de B Kar &amp; plus <rhyme label="b" type="f">dure</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Mais dans un tendre rendez <rhyme label="a" type="m" corresp="#s3:l1">vous</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Ture <rhyme label="b" type="f" corresp="#s3:l2">lure</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Il faut chanter par <rhyme label="b" type="f" corresp="#s3:l2">nature</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Robin, ture, lure, <rhyme label="b" type="f" corresp="#s3:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">A ses mots tout plain <rhyme label="a" type="m">d'appas</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Je chante le voix plus <rhyme label="b" type="f">d'ure</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Mais je vous en dira <rhyme label="a" type="m" corresp="#s4:l1">pas</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Ture <rhyme label="b" type="f" corresp="#s4:l2">lure</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">La rest de mon <rhyme label="b" type="f" corresp="#s4:l2">avonture</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Robin, ture, lure, <rhyme label="b" type="f" corresp="#s4:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gy hebt myn door u <rhyme label="a" type="m">gezigt</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Gestelt tuszen vier <rhyme label="b" type="f">muuren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als ik miszen moet dat <rhyme label="a" type="m" corresp="#s5:l1">ligt</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Ture <rhyme label="b" type="f" corresp="#s5:l2">lure</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Zit ik als een Uil te <rhyme label="b" type="f" corresp="#s5:l2">truuren</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Robin, ture, lure, <rhyme label="b" type="f" corresp="#s5:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zoete Mietjen, lieve <rhyme label="a" type="m">Meid</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Wat heb jy raare <rhyme label="b" type="f">kuure</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Mogt ik eens, je weet wel <rhyme label="a" type="m" corresp="#s6:l1">wat</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Ture <rhyme label="b" type="f" corresp="#s6:l2">lure</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Mogt ik eens myn Zondags <rhyme label="b" type="f" corresp="#s6:l2">geld</rhyme></l>
        <l id="s6:l6" met="4" type="+-">By u vertuure <rhyme label="b" type="f" corresp="#s6:l2">lure</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
