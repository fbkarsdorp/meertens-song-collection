  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Reik ons, Broeder, uwe <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In deez' tempel zijt gij <rhyme label="b" type="f">veilig</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Deze stonde zij u <rhyme label="b" type="f" corresp="#s1:l2">heilig</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Welkom in den <rhyme label="a" type="m" corresp="#s1:l1">broederband</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Eer den Bouwheer van 't <rhyme label="a" type="m">heel√§l</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Blijf getrouw in vreugde en <rhyme label="b" type="f">smarte</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Toon een moedig edel <rhyme label="b" type="f" corresp="#s2:l2">harte</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">In al wat u treffen <rhyme label="a" type="m" corresp="#s2:l1">zal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wees bescheiden, en <rhyme label="a" type="m">verricht</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Wat u de Orde zal <rhyme label="b" type="f">bevelen</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Blijf getrouw in alle <rhyme label="b" type="f" corresp="#s3:l2">deelen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Aan uw woord en dienst en <rhyme label="a" type="m" corresp="#s3:l1">plicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zie elk biedt u hart en <rhyme label="a" type="m">hand</rhyme>!</l>
        <l id="s4:l2" met="4" type="+-">Laat u door de Waarheid <rhyme label="b" type="f">leiden</rhyme></l>
        <l id="s4:l3" met="4" type="+-">En door Deugd u <rhyme label="b" type="f" corresp="#s4:l2">voorbereiden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Tot der Schoonheid <rhyme label="a" type="m" corresp="#s4:l1">Vaderland</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
