<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190471</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wat Vreugd is 'er door 't gantsche <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wy danssen, springen, hand aan <rhyme label="a" type="m" corresp="#s1:l1">hand</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wat Vreugd is 'er door 't gantsche <rhyme label="a" type="m" corresp="#s1:l1">Land</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">De Keesen zyn van <rhyme label="a" type="m" corresp="#s1:l1">kant</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Een ieder vlugtte zo gaauw hy <rhyme label="b" type="m">kon</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Op 't schynen van d'Oranje <rhyme label="b" type="m" corresp="#s1:l5">Zon</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Een ieder vlugtte zo gaauw hy <rhyme label="b" type="m" corresp="#s1:l5">kon</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Op 't schynen van die <rhyme label="b" type="m" corresp="#s1:l5">Zon</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al was ons Prinsie nog zo <rhyme label="a" type="m">rein</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En zuiver, als een Mensch kon <rhyme label="a" type="m" corresp="#s2:l1">zyn</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Al was ons Prinsje nog zo <rhyme label="a" type="m" corresp="#s2:l1">rein</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Zy spoogen hun <rhyme label="a" type="m" corresp="#s2:l1">venyn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Op hem, als Captein <rhyme label="b" type="m">Generaal</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die moest maar weg dat was heur <rhyme label="b" type="m" corresp="#s2:l5">taal</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Op hem, als Captein <rhyme label="b" type="m" corresp="#s2:l5">Generaal</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die moest weg, was heur <rhyme label="b" type="m" corresp="#s2:l5">taal</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Voort Kees, voort Kees, de Hal weêr <rhyme label="a" type="m">uit</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Wat doe je hier met je schurfde <rhyme label="a" type="m" corresp="#s3:l1">huid</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Voort Kees, voort Kees, de Hal weêr <rhyme label="a" type="m" corresp="#s3:l1">uit</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Je hoogmoed is <rhyme label="a" type="m" corresp="#s3:l1">gestuit</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">De schrik voor Pruissisch <rhyme label="b" type="m">Veldbanier</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Die jaagd je vuile Ziel van <rhyme label="b" type="m" corresp="#s3:l5">hier</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">De schrik voor Pruissisch <rhyme label="b" type="m" corresp="#s3:l5">Veldbanier</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Die jaagd je alleen van <rhyme label="b" type="m" corresp="#s3:l5">hier</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat waart ge, Vryheidskraaijers <rhyme label="a" type="m">schouw</rhyme></l>
        <l id="s4:l2" met="4" type="-+">De duivel removeerd je ook <rhyme label="a" type="m" corresp="#s4:l1">nou</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wat waard ge Vryheidskraaijers, <rhyme label="a" type="m" corresp="#s4:l1">schouw</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Voor Nassouws Deugd en <rhyme label="a" type="m" corresp="#s4:l1">Trouw</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Gy lag eer Vryheid aan den <rhyme label="b" type="m">band</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">In dit ons vrye <rhyme label="b" type="m" corresp="#s4:l5">Nederland</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Gy lag eer vryheid aan den <rhyme label="b" type="m" corresp="#s4:l5">band</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">In dit ons vrye <rhyme label="b" type="m" corresp="#s4:l5">Land</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Zyt welkom Vorst met uw <rhyme label="a" type="m">Vorstin</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Wy haalen u met vreugd weer <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Zyt welkom Vorst met uw <rhyme label="a" type="m" corresp="#s5:l1">Vorstin</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Nu hebben wy 't naar ons <rhyme label="a" type="m" corresp="#s5:l1">zin</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Vivat, leef met uw dierbaar <rhyme label="b" type="m">Kroost</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Tot Neêrlands lust en Haagjes <rhyme label="b" type="m" corresp="#s5:l5">troost</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Vivat, leef met uw dierbaar <rhyme label="b" type="m" corresp="#s5:l5">Kroost</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Tot vreugd' en onze <rhyme label="b" type="m" corresp="#s5:l5">troost</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
