<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157272</id>
    <date>
      <year-start>1712</year-start>
      <year-end>1712</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hansje sneed het koren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">‘Heer,   Aeltje <rhyme label="a" type="m">buur</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Wat <rhyme label="a" type="m" corresp="#s1:l1">avontuur</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Denk,   Teun die heeft een <rhyme label="b" type="f">vryertje</rhyme>!</l>
        <l id="s1:l4" met="2" type="-+">Is 't niet <rhyme label="c" type="m">ontdieft</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Zy is <rhyme label="c" type="m" corresp="#s1:l4">verlieft</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Op een wildweyig   <rhyme label="b" type="f" corresp="#s1:l3">Snyertje</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Een <rhyme label="a" type="m">Huppel-klink</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Een lichte <rhyme label="a" type="m" corresp="#s2:l1">vink</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer niet en is ten <rhyme label="b" type="f">beste</rhyme>.</l>
        <l id="s2:l4" met="2" type="-+">En weetje 't <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Hoe dat hy <rhyme label="c" type="m" corresp="#s2:l4">hiet</rhyme>?</l>
        <l id="s2:l6" met="4" type="-+">'t Is puikien aen de <rhyme label="b" type="f" corresp="#s2:l3">Veste</rhyme>.’</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">‘Wat magse <rhyme label="a" type="m">doen</rhyme>?</l>
        <l id="s3:l2" met="2" type="-+">'t Is geen <rhyme label="a" type="m" corresp="#s3:l1">fatsoen</rhyme>!’</l>
        <l id="s3:l3" met="4" type="-+">‘Ia wel!   Ik zegt jou, <rhyme label="b" type="f">Aeltje</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Dat teere <rhyme label="c" type="m">wigt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Valt veel te <rhyme label="c" type="m" corresp="#s3:l4">ligt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Voor zulk een aerdig <rhyme label="b" type="f" corresp="#s3:l3">staeltje</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">'t Is wel een <rhyme label="a" type="m">lanst</rhyme>!</l>
        <l id="s4:l2" met="2" type="-+">Hy speuld en <rhyme label="a" type="m" corresp="#s4:l1">danst</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ja alle nieuwe <rhyme label="b" type="f">wijsjes</rhyme>;</l>
        <l id="s4:l4" met="2" type="-+">Oft niet een <rhyme label="c" type="m">quak</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Hy weetse <rhyme label="c" type="m" corresp="#s4:l4">strak</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Dat hoord' ik laetst tot <rhyme label="b" type="f" corresp="#s4:l3">Tijsjes</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Waer op dat <rhyme label="a" type="m">pas</rhyme></l>
        <l id="s5:l2" met="2" type="-+">'t Geselschap <rhyme label="a" type="m" corresp="#s5:l1">was</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Van al de jonge <rhyme label="b" type="f">Maetjes</rhyme>.</l>
        <l id="s5:l4" met="2" type="-+">Daer hy <rhyme label="c" type="m">alleen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Het gisje   <rhyme label="c" type="m" corresp="#s5:l4">scheen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">In klugjes en in <rhyme label="b" type="f" corresp="#s5:l3">praetjes</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">De lange <rhyme label="a" type="m">Teun</rhyme></l>
        <l id="s6:l2" met="2" type="-+">En Jaep en <rhyme label="a" type="m" corresp="#s6:l1">Zeun</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Nog zeiden: 'Dat 's een <rhyme label="b" type="f">giesje</rhyme>!'</l>
        <l id="s6:l4" met="2" type="-+">'Ja zoo <rhyme label="c" type="m">ondieft</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Dat ik <rhyme label="c" type="m" corresp="#s6:l4">verlieft</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Hem minne moet,' zeid <rhyme label="b" type="f" corresp="#s6:l3">Niesje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Die zoete <rhyme label="a" type="m">sloof</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Heeft goed <rhyme label="a" type="m" corresp="#s7:l1">geloof</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Dat zy hem zal <rhyme label="b" type="f">bekooren</rhyme>;</l>
        <l id="s7:l4" met="2" type="-+">O, lieven <rhyme label="c" type="m">heer</rhyme>!</l>
        <l id="s7:l5" met="2" type="-+">Komt morgen <rhyme label="c" type="m" corresp="#s7:l4">weêr</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Gy sult wat anders <rhyme label="b" type="f" corresp="#s7:l3">hooren</rhyme>.’</l>
      </lg>
    </lg>
  </text>
</song>
