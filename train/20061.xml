  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">JUpyn gheen Nectar <rhyme label="a" type="m">smaakt</rhyme></l>
        <l id="s1:l2" met="6" type="-+">Die meerder vermaakt, als daar mijn hert na <rhyme label="a" type="m" corresp="#s1:l1">haakt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Te weten, ’t <rhyme label="b" type="m">zoet</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Wiens lieve <rhyme label="b" type="m" corresp="#s1:l3">vloet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Sy van haar kaakjes stromen <rhyme label="b" type="m" corresp="#s1:l3">doet</rhyme>,</l>
        <l id="s1:l6" met="1" type="-+">En <rhyme label="c" type="f">wekten</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s1:l6">treckten</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">Mijn <rhyme label="d" type="m">zinn</rhyme>’,</l>
        <l id="s1:l9" met="1" type="-+">Door <rhyme label="d" type="m" corresp="#s1:l8">Min</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Tot mijn <rhyme label="d" type="m" corresp="#s1:l8">Godin</rhyme>:</l>
        <l id="s1:l11" met="2" type="-+">Zoo dra ik <rhyme label="e" type="m">hadt</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Het <rhyme label="e" type="m" corresp="#s1:l11">honigh-nat</rhyme></l>
        <l id="s1:l13" met="4" type="-+">Van hare lipjes maar <rhyme label="e" type="m" corresp="#s1:l11">gevat</rhyme>.</l>
      </lg>
    </lg>
  </text>
