  <author>Jan Steven van Esveldt Holtrop, Willem Holtrop</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wij bouwen, deugd! uw' <rhyme label="a" type="f">tempel</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">De trouw bewaakt zijn' <rhyme label="a" type="f" corresp="#s1:l1">drempel</rhyme>;</l>
        <l id="s1:l3" met="2" type="-+">Geen <rhyme label="b" type="m">dartelheid</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mag 't heilig altaar <rhyme label="c" type="f">naadren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Schoon blijdschap in onze <rhyme label="c" type="f" corresp="#s1:l4">aadren</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Haar' glans <rhyme label="b" type="m" corresp="#s1:l3">verspreid'</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon domheid ons moog' <rhyme label="a" type="f">lastren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ons hart zal noojt <rhyme label="a" type="f" corresp="#s2:l1">verbastren</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Van <rhyme label="b" type="m">Broedertrouw</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Vergeeven en <rhyme label="c" type="f">vergeeten</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Gebredt hier elks <rhyme label="c" type="f" corresp="#s2:l4">geweten</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Bevrijd van <rhyme label="b" type="m" corresp="#s2:l3">rouw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wij strenglen vriendschaps - <rhyme label="a" type="f">banden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wij bieden <rhyme label="a" type="f" corresp="#s3:l1">offerhanden</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Aan reine <rhyme label="b" type="m">deugd</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">In hulprijk <rhyme label="c" type="f">mededoogen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Een Broedertraan te <rhyme label="c" type="f" corresp="#s3:l4">droogen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Verschaft ons <rhyme label="b" type="m" corresp="#s3:l3">vreugd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Verdiensten, gul <rhyme label="a" type="f">bekroonen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Der braven trouw <rhyme label="a" type="f" corresp="#s4:l1">beloonen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Is steeds ons <rhyme label="b" type="m">doel</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Rampspoedigen <rhyme label="c" type="f">verzachten</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Is eedlen wellust te <rhyme label="c" type="f" corresp="#s4:l4">achten</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Voor 't zacht <rhyme label="b" type="m" corresp="#s4:l3">gevoel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wel√§an! dat het <rhyme label="a" type="f">verrichten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van Broederlijke <rhyme label="a" type="f" corresp="#s5:l1">pligten</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Ons heil dan <rhyme label="b" type="m">zij</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Zoo drukt, in deezen <rhyme label="c" type="f">tempel</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Der deugd, zij-zelf haar' <rhyme label="c" type="f" corresp="#s5:l4">stempel</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Op <rhyme label="b" type="m" corresp="#s5:l3">Metslarij</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
