  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Nu laet ghy Heer <rhyme label="a" type="m">oprecht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gaen in vreed’ uwen <rhyme label="a" type="m" corresp="#s1:l1">Knecht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Nae u beloft <rhyme label="b" type="f">ghestadigh</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Nae dat mijn ooghen <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hebben ghesien <rhyme label="c" type="m" corresp="#s1:l4">voorwaer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Uwen Heylant <rhyme label="b" type="f" corresp="#s1:l3">ghenadich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Salichmaker <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Aller olcken <rhyme label="a" type="m" corresp="#s2:l1">ghemeyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die aen nemen sijn <rhyme label="b" type="f">Leere</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Een licht schijnende <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Den Heydenen <rhyme label="c" type="m" corresp="#s2:l4">eenpaer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">End’ oock Israels <rhyme label="b" type="f" corresp="#s2:l3">eere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
