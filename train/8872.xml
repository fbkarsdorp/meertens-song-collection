  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE HEEr is Coninck, end' <rhyme label="a" type="m">Verciert</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met Heerlijckheyt <rhyme label="b" type="f">Almachtich</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Syn Rijck daer in hy selffs <rhyme label="a" type="m" corresp="#s1:l1">Regiert</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Sal blijven altoos <rhyme label="b" type="f" corresp="#s1:l2">Crachtich</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Van Aenbegin dijn Stoel <rhyme label="c" type="m">bereydt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Blijft vast in grooter <rhyme label="c" type="m" corresp="#s1:l5">Heerlijckheyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ghy zijt Eewich <rhyme label="d" type="f">verheven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Waterstroomen Bruysschen <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">T'verheffen haer die <rhyme label="b" type="f">Stroomen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die Waterbaren in den <rhyme label="a" type="m" corresp="#s2:l1">Meer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Seer grouwelijck voort <rhyme label="b" type="f" corresp="#s2:l2">comen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Dan noch is Machtigher de <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dijn Woort is eene Rechte <rhyme label="c" type="m" corresp="#s2:l5">Leer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dijns Huys Cieraet is <rhyme label="d" type="f">Heylich</rhyme>.</l>
      </lg>
    </lg>
