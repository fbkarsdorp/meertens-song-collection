    <melody>Hoe geestig danst mijn Roosje</melody>
  <author>Hermanus van den Burg</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe geestig danst myn <rhyme label="a" type="f">Roosje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">'t Meisje krygt een <rhyme label="a" type="f" corresp="#s1:l1">bloosje</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">by de <rhyme label="b" type="m">vreugd</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">daar ze yder cen <rhyme label="b" type="m" corresp="#s1:l3">verheugt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Waarom of die schoone <rhyme label="c" type="m">Maagd</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zoo veel' Jonge Minnaars <rhyme label="c" type="m" corresp="#s1:l5">plaagt</rhyme>?</l>
      </lg>
    </lg>
  </text>
