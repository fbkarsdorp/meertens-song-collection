  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'k VOelden mijn <rhyme label="a" type="f">gevoelen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Ontsteken, <rhyme label="a" type="f" corresp="#s1:l1">doelen</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Om te <rhyme label="b" type="f">glippen</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">In de <rhyme label="b" type="f" corresp="#s1:l3">knippen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Van haar purp're <rhyme label="b" type="f" corresp="#s1:l3">lippen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Als ik op de <rhyme label="b" type="f" corresp="#s1:l3">tippen</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">Mijn <rhyme label="b" type="f" corresp="#s1:l3">lippen</rhyme></l>
        <l id="s1:l8" met="1" type="-+">Liet <rhyme label="b" type="f" corresp="#s1:l3">slippen</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Scheen d'aarde een lente van <rhyme label="c" type="m">kaneel</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Braakten uit ontsloten <rhyme label="c" type="m" corresp="#s1:l9">keel</rhyme>.</l>
      </lg>
    </lg>
