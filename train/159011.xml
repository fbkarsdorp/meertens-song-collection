<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159011</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nadien uw goddelijkheid</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe vreemdt en <rhyme label="a" type="m">ongesien</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">'t Kan evenwel <rhyme label="a" type="m" corresp="#s1:l1">geschien</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">'t Heeft voor ons tijdt <rhyme label="b" type="f">gebleken</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">En die'er acht op <rhyme label="c" type="m">slaet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Met recht daer van kan <rhyme label="b" type="f" corresp="#s1:l3">spreken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat het aldus noch <rhyme label="c" type="m" corresp="#s1:l4">gaet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Egypten tuyght dit <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Want over meenich <rhyme label="a" type="m" corresp="#s2:l1">Iaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Is 't openbaer <rhyme label="b" type="f">bewesen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe dat een Ionge <rhyme label="c" type="m">Soon</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Verworpen en <rhyme label="b" type="f" corresp="#s2:l3">verwesen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Stijght naest des Koninghs <rhyme label="c" type="m" corresp="#s2:l4">Kroon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als Pharo de <rhyme label="a" type="m">Tyran</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een wreedt bloedt-dorstigh <rhyme label="a" type="m" corresp="#s3:l1">Man</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">De Vroe-moers heeft <rhyme label="b" type="f">geboden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">De Mannelijcke <rhyme label="c" type="m">stam</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van 't Ioodsche Volck te <rhyme label="b" type="f" corresp="#s3:l3">dooden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Als die ter Werelt <rhyme label="c" type="m" corresp="#s3:l4">quam</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wist Godt door sijne <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Het hert en kleyn <rhyme label="a" type="m" corresp="#s4:l1">verstant</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Der Vroe-moers soo te <rhyme label="b" type="f">buygen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat Pharo in sijn <rhyme label="c" type="m">list</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Door haer doen en <rhyme label="b" type="f" corresp="#s4:l3">betuygen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sijn gantsche oogh-wit <rhyme label="c" type="m" corresp="#s4:l4">mist</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">In dese droeve <rhyme label="a" type="m">dagh</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Godt noch een middel <rhyme label="a" type="m" corresp="#s5:l1">sach</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Want Moses wordt <rhyme label="b" type="f">gebooren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die naemaels als een <rhyme label="c" type="m">Helt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Het IsraÃ«l <rhyme label="b" type="f" corresp="#s5:l3">verkooren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In hare vryheyt <rhyme label="c" type="m" corresp="#s5:l4">stelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy hadd' een droef <rhyme label="a" type="m">begin</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Men leyd hem neder <rhyme label="a" type="m" corresp="#s6:l1">in</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Een Krib gemaeckt van <rhyme label="b" type="f">Biesen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Daer mede drijft hy <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Om 't leven te <rhyme label="b" type="f" corresp="#s6:l3">verliesen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">met suchten en <rhyme label="c" type="m" corresp="#s6:l4">geween</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Doch was 't al-siende <rhyme label="a" type="m">oogh</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Bewogen tot <rhyme label="a" type="m" corresp="#s7:l1">mee-doogh</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Om 't Ionge Kindt te <rhyme label="b" type="f">sparen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Want als sijn Suster <rhyme label="c" type="m">maer</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Een weynigh stont te <rhyme label="b" type="f" corresp="#s7:l3">staren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Quam Pharoos Dochter <rhyme label="c" type="m" corresp="#s7:l4">daer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dees preutse Maeght die <rhyme label="a" type="m">siet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dit Kribjen in het <rhyme label="a" type="m" corresp="#s8:l1">Riet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En doet het vaerdigh <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Daer vondt sy 't Ionge <rhyme label="c" type="m">wicht</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Met tranen-vloet <rhyme label="b" type="f" corresp="#s8:l3">bedropen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En een bedroeft <rhyme label="c" type="m" corresp="#s8:l4">gesicht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dit is van Iacobs <rhyme label="a" type="m">zaet</rhyme></l>
        <l id="s9:l2" met="3" type="-+">(Seyd' sy) dies haestigh <rhyme label="a" type="m" corresp="#s9:l1">gaet</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En laet een Voester <rhyme label="b" type="f">halen</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Sijn Suster krijght het <rhyme label="c" type="m">woort</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Die oock haest sonder <rhyme label="b" type="f" corresp="#s9:l3">dralen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Quam met sijn Moeder <rhyme label="c" type="m" corresp="#s9:l4">voort</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">O wonderlijck <rhyme label="a" type="m">bestier</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Van Godt! want Moses <rhyme label="a" type="m" corresp="#s10:l1">hier</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Krijght tot een Min sijn <rhyme label="b" type="f">Moeder</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Soo brenght de <rhyme label="c" type="m">Opper-heer</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Als een getrouw <rhyme label="b" type="f" corresp="#s10:l3">behoeder</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Om hoogh die lagh ter <rhyme label="c" type="m" corresp="#s10:l4">eer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Dus stelt u hert <rhyme label="a" type="m">gerust</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">En scheyt in Godt u <rhyme label="a" type="m" corresp="#s11:l1">lust</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Hy is een troost in't <rhyme label="b" type="f">lijden</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">Die slechts nae hem <rhyme label="c" type="m">toe-gaen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Sal hy noch eens <rhyme label="b" type="f" corresp="#s11:l3">verblijden</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Hoe droef haer saken <rhyme label="c" type="m" corresp="#s11:l4">staen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
