<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185135</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Van Helena</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O heylich suyver <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Fonteyne alles <rhyme label="b" type="m">goets</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Daer ghy bewoont <rhyme label="a" type="m" corresp="#s1:l1">alleen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Den Tempel des <rhyme label="b" type="m" corresp="#s1:l2">gemoets</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wat isser <rhyme label="b" type="m" corresp="#s1:l2">overvloets</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van Goddelijcke <rhyme label="c" type="f">weelden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Daer blijft in soo veel <rhyme label="b" type="m" corresp="#s1:l2">soets</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Geen plaetse voor de <rhyme label="c" type="f" corresp="#s1:l6">beelden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hemelschen <rhyme label="a" type="m">Bruydegom</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">V lieffelick <rhyme label="b" type="m">aenschijn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat neyghet hem al <rhyme label="a" type="m" corresp="#s2:l1">om</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot die de uwe <rhyme label="b" type="m" corresp="#s2:l2">zijn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ghy trecktse al tot <rhyme label="b" type="m" corresp="#s2:l2">dijn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Vyt kracht van uwer <rhyme label="c" type="f">minnen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Want uwen soeten <rhyme label="b" type="m" corresp="#s2:l2">wijn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Berooft verstant en <rhyme label="c" type="f" corresp="#s2:l6">sinnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">V edel suyver <rhyme label="a" type="m">licht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">V Goddelicke <rhyme label="b" type="m">aert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">V vriendelick <rhyme label="a" type="m" corresp="#s3:l1">ghesicht</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">V liefde tot <rhyme label="b" type="m" corresp="#s3:l2">hunwaert</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Een weder liefde <rhyme label="b" type="m" corresp="#s3:l2">baert</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Sy worden so <rhyme label="c" type="f">ontsteken</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Als ghy in haer <rhyme label="b" type="m" corresp="#s3:l2">verklaert</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Haer herte schijnt te <rhyme label="c" type="f" corresp="#s3:l6">breken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Want als de ziel <rhyme label="a" type="m">aensiet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">V heylich wesen <rhyme label="b" type="m">puur</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Soo heeftse groot <rhyme label="a" type="m" corresp="#s4:l1">verdriet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hier inde <rhyme label="b" type="m" corresp="#s4:l2">creatuur</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Stondet in haren <rhyme label="b" type="m" corresp="#s4:l2">kuur</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Sy liep uyt hare <rhyme label="c" type="f">krachten</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Doch haer bestelde <rhyme label="b" type="m" corresp="#s4:l2">uur</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Wil sy van u <rhyme label="c" type="f" corresp="#s4:l6">verwachten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Om uwent wil sy <rhyme label="a" type="m">dient</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De ydelheyt O <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ghy zijt haer sulcken <rhyme label="a" type="m" corresp="#s5:l1">vrient</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Sentse vry heen en <rhyme label="b" type="m" corresp="#s5:l2">weer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En drucktse vry ter <rhyme label="b" type="m" corresp="#s5:l2">neer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ist u Godlick <rhyme label="c" type="f">behaghen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ghewilligh u <rhyme label="b" type="m" corresp="#s5:l2">begeer</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Sal sy uyt liefde <rhyme label="c" type="f" corresp="#s5:l6">draghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">V liefde is soo <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Int binnenste <rhyme label="b" type="m">gheprent</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo dat sy door geen <rhyme label="a" type="m" corresp="#s6:l1">last</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Druck lyden noch <rhyme label="b" type="m" corresp="#s6:l2">torment</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Can werden af <rhyme label="b" type="m" corresp="#s6:l2">ghewent</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van uwe soete <rhyme label="c" type="f">minne</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Al wat ghy haer <rhyme label="b" type="m" corresp="#s6:l2">toesent</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat treckt haer tot u <rhyme label="c" type="f" corresp="#s6:l6">inne</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy zijt in haer, en <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Is oock in u <rhyme label="b" type="m">altijt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wat sy doet dat doet <rhyme label="a" type="m" corresp="#s7:l1">ghy</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Haer lyden ghy oock <rhyme label="b" type="m" corresp="#s7:l2">lijdt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Haer strijdt is uwen <rhyme label="b" type="m" corresp="#s7:l2">strijdt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Haer vreucht is u <rhyme label="c" type="f">vermaken</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Met haer ghy eenich <rhyme label="b" type="m" corresp="#s7:l2">sijt</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Daerom zijn ‘t uwe <rhyme label="c" type="f" corresp="#s7:l6">saken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O wonderlicke <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die hem met u <rhyme label="b" type="m">verbint</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Al wort hy hier <rhyme label="a" type="m" corresp="#s8:l1">bespot</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hy weet wat hy <rhyme label="b" type="m" corresp="#s8:l2">bevint</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Hy weet wat hy <rhyme label="b" type="m" corresp="#s8:l2">bemint</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">‘t Is hem te sien <rhyme label="c" type="f">ghegeven</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Maer die daer syn <rhyme label="b" type="m" corresp="#s8:l2">verblint</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Oock na haer blintheyt <rhyme label="c" type="f" corresp="#s8:l6">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
