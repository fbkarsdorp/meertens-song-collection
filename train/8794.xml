<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8794</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Nun freut euch liebe Christen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WEl hertlijck lieff heb ick dy <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn Sterck, mijn Borch, <rhyme label="b" type="f">Steenrotze</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Mijn Heyl, mijn Troost, mijn Schilt, <rhyme label="a" type="m" corresp="#s1:l1">geweer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mijn Godt die my <rhyme label="b" type="f" corresp="#s1:l2">beschutse</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ick looff den Heer' en roep hem <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Soo mach ick mijn Vyand <rhyme label="c" type="m" corresp="#s1:l5">ontgaen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Want Godt doet my <rhyme label="d" type="f">bevrijden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die Strick' des Doots omvingen <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Des Belials Beeck daer <rhyme label="b" type="f">neven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Der Hellen Ancxt waer oock daer <rhyme label="a" type="m" corresp="#s2:l1">by</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Doots Bandt had my om <rhyme label="b" type="f" corresp="#s2:l2">gheven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Doen riep ick aen mijn Godt end <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">End' hy van syn Tempel vol <rhyme label="c" type="m" corresp="#s2:l5">Eer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Mijn Stem wel rasch <rhyme label="d" type="f">verhoorde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">So haest voor hem mijn roepen <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Bewagen wert die <rhyme label="b" type="f">Aerde</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die Berghen haer verruerden <rhyme label="a" type="m" corresp="#s3:l1">tsaem</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Doen hy so thoornich <rhyme label="b" type="f" corresp="#s3:l2">werde</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Jae Damp wt synen Neus <rhyme label="c" type="m">ontstont</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Verteerend' Vier wt synen <rhyme label="c" type="m" corresp="#s3:l5">Mont</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Affgrijslijck was den <rhyme label="d" type="f">Blicxem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy booch den Hemel, daelde <rhyme label="a" type="m">neer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">T'was doncker om syn <rhyme label="b" type="f">Voeten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ops Windes Vleugel sweed' de <rhyme label="a" type="m" corresp="#s4:l1">Heer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hem Cherub dienen <rhyme label="b" type="f" corresp="#s4:l2">moeten</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Met Duysternis hy was <rhyme label="c" type="m">becleede</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Die Waterwolcken dick en <rhyme label="c" type="m" corresp="#s4:l5">breede</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat was die Tent des <rhyme label="d" type="f">HEEren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een Heerlijck glants omleuchted' <rhyme label="a" type="m">hem</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daer deysden haer die <rhyme label="b" type="f">Wolcken</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Met Donder ginck des HEEren <rhyme label="a" type="m" corresp="#s5:l1">stem</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Verschrickt also die <rhyme label="b" type="f" corresp="#s5:l2">Volcken</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Met Hagel end' met Blixem <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">De HEEr syn Vierig' Stralen <rhyme label="c" type="m" corresp="#s5:l5">schoot</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Den Vyant te <rhyme label="d" type="f">verstroyen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Des Waters was daer <rhyme label="a" type="m">Overvloet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Der Aerden gront wert <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">So thoornich was des HEEren <rhyme label="a" type="m" corresp="#s6:l1">Moet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">T moest al voor hem <rhyme label="b" type="f" corresp="#s6:l2">verloopen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Doen thoond' hy my syn Hulpe <rhyme label="c" type="m">hooch</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">End' my wt groote Watren <rhyme label="c" type="m" corresp="#s6:l5">tooch</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Holp my van die my <rhyme label="d" type="f">haten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Mijns Vyandts macht was my te <rhyme label="a" type="m">swaer</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Maer Godt quam my te <rhyme label="b" type="f">baten</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Mijns Onvals Tijdt was comen <rhyme label="a" type="m" corresp="#s7:l1">daer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Had my die HEEr <rhyme label="b" type="f" corresp="#s7:l2">verlaten</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Maer recht wert hy mijn <rhyme label="c" type="m">Toeversicht</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Verliet my in Benautheyt <rhyme label="c" type="m" corresp="#s7:l5">nicht</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ruckt my her wt int <rhyme label="d" type="f">Ruyme</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De HEEr heeft Lust end' <rhyme label="a" type="m">Welbehaeg</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Aen my syn Dienaer <rhyme label="b" type="f">trouwe</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Mijn Cruys end' Noot als ic hem <rhyme label="a" type="m" corresp="#s8:l1">claeg</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ten mach my niet <rhyme label="b" type="f" corresp="#s8:l2">berouwen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Hy helpt my nae mijn <rhyme label="c" type="m">Vromicheyt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Betaelt my nae die <rhyme label="c" type="m" corresp="#s8:l5">Reynicheyt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">End' Onschult mijner <rhyme label="d" type="f">Hande</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Want ick des HEEren weeg' <rhyme label="a" type="m">bewaer</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Teghens hem niet <rhyme label="b" type="f">hanteere</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Sijn Rechten houd' ick <rhyme label="a" type="m" corresp="#s9:l1">allegaer</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Werp syn Ghebodt niet <rhyme label="b" type="f" corresp="#s9:l2">veere</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Ben Onbevleckt voor zijnen <rhyme label="c" type="m">Oogh'</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Aen syn Faveur hebb' ick <rhyme label="c" type="m" corresp="#s9:l5">ghenoech</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">End' wachte my voor <rhyme label="d" type="f">Sonden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">By Heyl'gen ghy oock Heylich <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">By Vromen vroom ter <rhyme label="b" type="f">Eeren</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">End' by den Reynen reyn <rhyme label="a" type="m" corresp="#s10:l1">altijdt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Verkeert by den <rhyme label="b" type="f" corresp="#s10:l2">Verkeerden</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Ghy helpt dijn Elend' Volck so <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Maer hooge Ooghen condt ghy <rhyme label="c" type="m" corresp="#s10:l5">snel</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Vercleynen end' <rhyme label="d" type="f">vernielen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">HEEr ghy v'licht mijn <rhyme label="a" type="m">Aengesicht</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Mijn Droeff'nis mach niet <rhyme label="b" type="f">blijven</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Des Vyants Macht en acht ick <rhyme label="a" type="m" corresp="#s11:l1">nicht</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Met Godt can icks' <rhyme label="b" type="f" corresp="#s11:l2">verdrijven</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">GOds Weghen die syn <rhyme label="c" type="m">Onbevlecht</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Sijn Woort is Louter en <rhyme label="c" type="m" corresp="#s11:l5">Perfect</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Een Schilt die hem <rhyme label="d" type="f">betrouwen</rhyme>..</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Waer is een Godt, dan onse <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Den Troost is hy <rhyme label="b" type="f">alleene</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Gheen sterck Confoort en isser <rhyme label="a" type="m" corresp="#s12:l1">meer</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Cracht doet hy my <rhyme label="b" type="f" corresp="#s12:l2">verleenen</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Mijn Weghen maeckt hy <rhyme label="c" type="m">Onbesmet</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Mijn Voeten Rasch end' <rhyme label="c" type="m" corresp="#s12:l5">Onbelet</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Ghelijck die Harten <rhyme label="d" type="f">springhen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Hy stelt my op mijn Hoochte <rhyme label="a" type="m">snel</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">En leert mijn Handen <rhyme label="b" type="f">strijden</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Mijn Arms den Boghen spannen <rhyme label="a" type="m" corresp="#s13:l1">wel</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Om schieten tallen <rhyme label="b" type="f" corresp="#s13:l2">zijden</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Dijn Handt my sterckt al in der <rhyme label="c" type="m">Noot</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">In Demoet dan maeckt ghy my <rhyme label="c" type="m" corresp="#s13:l5">groot</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Den Schilt dijn Heyls my <rhyme label="d" type="f">gheve</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Ghy maeckt my Ruym daer heen ick <rhyme label="a" type="m">gae</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Dat mijn Voet niet <rhyme label="b" type="f">faelgieren</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Mijn Vyand' wil ick iaghen <rhyme label="a" type="m" corresp="#s14:l1">nae</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">En sal my niet <rhyme label="b" type="f" corresp="#s14:l2">omkeeren</rhyme>,</l>
        <l id="s14:l5" met="4" type="-+">Maer t'eenmael sal icks' al <rhyme label="c" type="m">verslaen</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">Sy moghen my niet <rhyme label="c" type="m" corresp="#s14:l5">wederstaen</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Ten Voeten sy my <rhyme label="d" type="f">vallen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Ghy Rust my machtich totten <rhyme label="a" type="m">strijt</rhyme></l>
        <l id="s15:l2" met="3" type="-+">End' werpt den Vyant <rhyme label="b" type="f">neder</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Ghy maeckt dat sy gaen vluchten <rhyme label="a" type="m" corresp="#s15:l1">wijt</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Haest breken haer <rhyme label="b" type="f" corresp="#s15:l2">Geleder</rhyme>.</l>
        <l id="s15:l5" met="4" type="-+">Haer Roepen coemt ten Dooven <rhyme label="c" type="m">Oor</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">De HEEr geeft haer geen <rhyme label="c" type="m" corresp="#s15:l5">gehoor</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">En niemant machse <rhyme label="d" type="f">helpen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Verwaeyen sal icks' als den <rhyme label="a" type="m">Stoff</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Verpletten als den <rhyme label="b" type="f">Slijcke</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Daer van sal comen GOdes <rhyme label="a" type="m" corresp="#s16:l1">Loff</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Verbreyden sal syn <rhyme label="b" type="f" corresp="#s16:l2">Rijcke</rhyme>,</l>
        <l id="s16:l5" met="4" type="-+">T'kijffachtich Volc hem van my <rhyme label="c" type="m">went</rhyme>,</l>
        <l id="s16:l6" met="4" type="-+">Den Heyden maeckt ghy my <rhyme label="c" type="m" corresp="#s16:l5">bekent</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Haer Hooft ben ick <rhyme label="d" type="f">gheworden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Een Volck dat ick bekende <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Dient my nu met <rhyme label="b" type="f">begeere</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Die valschen Kinder (T is <rhyme label="a" type="m" corresp="#s17:l1">verdriet</rhyme>)</l>
        <l id="s17:l4" met="3" type="-+">Verlooch'nen my, haer <rhyme label="b" type="f" corresp="#s17:l2">HEERE</rhyme>,</l>
        <l id="s17:l5" met="4" type="-+">Gehoorsaem is mijn Nieuw' <rhyme label="c" type="m">Gemeen</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">Die Vreemden Kinder al met <rhyme label="c" type="m" corresp="#s17:l5">een</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Versmachten in haer <rhyme label="d" type="f">Banden</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Die HEEre leeft, is mijn <rhyme label="a" type="m">Conftoor</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Hy moet ghelovet <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Die Godt des Heyls mijn beste <rhyme label="a" type="m" corresp="#s18:l1">tzort</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">sy stadich hooch <rhyme label="b" type="f" corresp="#s18:l2">ghepresen</rhyme>,</l>
        <l id="s18:l5" met="4" type="-+">Die Godt die my Victory <rhyme label="c" type="m">bringt</rhyme>,</l>
        <l id="s18:l6" met="4" type="-+">End' onder my die Volcken <rhyme label="c" type="m" corresp="#s18:l5">dwingt</rhyme>,</l>
        <l id="s18:l7" met="3" type="-+">Helpt my van all' mijn <rhyme label="d" type="f">Haters</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Dies wil ick dancken dy mijn <rhyme label="a" type="m">HEer</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Mijn GOdt al by die <rhyme label="b" type="f">Heyden</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Dijn Name stadich Loven <rhyme label="a" type="m" corresp="#s19:l1">meer</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">En dijn Goetheyt <rhyme label="b" type="f" corresp="#s19:l2">verbreyden</rhyme>,</l>
        <l id="s19:l5" met="4" type="-+">Want dijn Gesalffden ghy wel <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s19:l6" met="4" type="-+">End' dijnen Coninck Weldaet <rhyme label="c" type="m" corresp="#s19:l5">groot</rhyme></l>
        <l id="s19:l7" met="3" type="-+">Den Zade Davids <rhyme label="d" type="f">Eewich</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
