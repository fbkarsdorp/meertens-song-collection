<song>
  <meta>
    <id>181884</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Neeltje heeft met lobberigs Wijntje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Philida lag neer <rhyme label="a" type="f">gezegen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In de Schaduw van de <rhyme label="b" type="m">Zon</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Met haer keurslyf half <rhyme label="a" type="f" corresp="#s1:l1">ontregen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Digt by een klaer <rhyme label="b" type="m" corresp="#s1:l2">Waterbron</rhyme></l>
        <l id="s1:l5" met="4" type="+-">En zy droomden <rhyme label="c" type="m">overluid</rhyme>,</l>
        <l id="s1:l6" met="5" type="+-">Dat zy met den Herder Damon was de <rhyme label="c" type="m" corresp="#s1:l5">Bruid</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Damon Kust: Naer u <rhyme label="d" type="m">Lust</rhyme></l>
        <l id="s1:l8" met="5" type="+-">Want zyn Minne-brand moet zyn <rhyme label="d" type="m" corresp="#s1:l7">geblust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Damon die dat soete <rhyme label="a" type="f">dromen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Had verstaen van woord tot <rhyme label="b" type="m">woord</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Is terstond by haer <rhyme label="a" type="f" corresp="#s2:l1">gekomen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dat heeft zyn jonghert <rhyme label="b" type="m" corresp="#s2:l2">bekoord</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En zyn jong hertje dat lilt en <rhyme label="c" type="m">beeft</rhyme>,</l>
        <l id="s2:l6" met="5" type="+-">Om dat hy de Assurantie niet en <rhyme label="c" type="m" corresp="#s2:l5">heeft</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">En hy zugt, dat de <rhyme label="d" type="m">lugt</rhyme></l>
        <l id="s2:l8" met="5" type="+-">Wedergalmt van zyn <rhyme label="d" type="m" corresp="#s2:l7">ongenugt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wel sprak hy wie heeft zyn <rhyme label="a" type="f">Leven</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Zulk een blooyheid meer <rhyme label="b" type="m">gezien</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Waerom in 't Uerdriet <rhyme label="a" type="f" corresp="#s3:l1">gebleven</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Blooyheid wilt dog van myn <rhyme label="b" type="m" corresp="#s3:l2">vlien</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Hier op bekeek hy zyn <rhyme label="c" type="m">Philida</rhyme>,</l>
        <l id="s3:l6" met="5" type="+-">Adieu Schoonste zei hy zonder <rhyme label="c" type="m" corresp="#s3:l5">weerga</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Mogt myn lust, zyn <rhyme label="d" type="m">geblust</rhyme>:</l>
        <l id="s3:l8" met="5" type="+-">Dan was Damon zyn jong hert <rhyme label="d" type="m" corresp="#s3:l7">gerust</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hier op ging hy haer lief <rhyme label="a" type="f">koosen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Zy ontwaekte met angst <rhyme label="b" type="m">belaen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Schaemte pronkt met <rhyme label="a" type="f" corresp="#s4:l1">rosen</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Als zy Damon daer zag <rhyme label="b" type="m" corresp="#s4:l2">staen</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Ach! zei zy Herder Damon <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s4:l6" met="5" type="+-">Ik ben tegenwoordig watje wil <rhyme label="c" type="m" corresp="#s4:l5">gebied</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Net bezweek, z' aen de <rhyme label="d" type="m">beek</rhyme>,</l>
        <l id="s4:l8" met="5" type="+-">En haer roode Kaekjes wierden <rhyme label="d" type="m" corresp="#s4:l7">bleek</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Straks met honderd duyzend <rhyme label="a" type="f">Kusjes</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Heeft hy zynen brand <rhyme label="b" type="m">geblust</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">En voldoed zyn <rhyme label="a" type="f" corresp="#s5:l1">Minnelusjes</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Doe was zyn Ionghert <rhyme label="b" type="m" corresp="#s5:l2">gerust</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Zy drukte beid' haer Borsjes <rhyme label="c" type="m">bloot</rhyme>:</l>
        <l id="s5:l6" met="5" type="+-">En haer rodermond hem weer een Kusje <rhyme label="c" type="m" corresp="#s5:l5">bood</rhyme>;</l>
        <l id="s5:l7" met="4" type="-+">Toen de zon, <rhyme label="d" type="m">ondergong</rhyme>,</l>
        <l id="s5:l8" met="5" type="+-">Scheiden dese beide van de <rhyme label="d" type="m" corresp="#s5:l7">Bron</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Philida begon te <rhyme label="a" type="f">zugten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ag! ag wat is my <rhyme label="b" type="m">geschied</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Damon sprak Lief wilt niet <rhyme label="a" type="f" corresp="#s6:l1">dugten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Gy zyt die myn Ziel <rhyme label="b" type="m" corresp="#s6:l2">gebied</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Ik zal u eeuwig zyn <rhyme label="c" type="m">getrouw</rhyme>,</l>
        <l id="s6:l6" met="5" type="+-">Daer en boven Eeren, als een Man zyn <rhyme label="c" type="m" corresp="#s6:l5">Urouw</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Zoo dat zy, alle <rhyme label="d" type="m">by</rhyme>,</l>
        <l id="s6:l8" met="5" type="+-">Stelden de zwarigheid aen een <rhyme label="d" type="m" corresp="#s6:l7">zy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
