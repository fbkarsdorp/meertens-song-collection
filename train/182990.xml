<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182990</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'t Is Vreugde door ons <rhyme label="a" type="m">Nederland</rhyme></l>
        <l id="s1:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">De Oranje Vlag die staat <rhyme label="a" type="m" corresp="#s1:l1">geplant</rhyme>,</l>
        <l id="s1:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s1:l2">Hoezé</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">Laat ons Eenparig zyn <rhyme label="c" type="m">verheugd</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">'t Is nut voor Ouderdom en <rhyme label="c" type="m" corresp="#s1:l5">Jeugd</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s1:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Oranje-Boom staat in zyn <rhyme label="a" type="m">fleur</rhyme>,</l>
        <l id="s2:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">De Bloeisem is een mooye <rhyme label="a" type="m" corresp="#s2:l1">kleur</rhyme>,</l>
        <l id="s2:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s2:l2">Hoezé</rhyme>!</l>
        <l id="s2:l5" met="4" type="-+">Oranje is de Oude <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Waar door ons Land tot Vryheid <rhyme label="c" type="m" corresp="#s2:l5">kwam</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s2:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy stelde voor ons <rhyme label="a" type="m">Nederland</rhyme>,</l>
        <l id="s3:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">Voor ons zyn vlees en Bloed te <rhyme label="a" type="m" corresp="#s3:l1">Pand</rhyme></l>
        <l id="s3:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s3:l2">Hoezé</rhyme>!</l>
        <l id="s3:l5" met="4" type="-+">De Vyfde Willem vol van <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Zal waake voor ons met speer en <rhyme label="c" type="m" corresp="#s3:l5">hoet</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s3:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Als zyn voor Ouders hebben <rhyme label="a" type="m">gedaan</rhyme></l>
        <l id="s4:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Zal hy nog in de Waapens <rhyme label="a" type="m" corresp="#s4:l1">gaan</rhyme>,</l>
        <l id="s4:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s4:l2">Hoezé</rhyme>!</l>
        <l id="s4:l5" met="4" type="-+">De Oranje Boom die nimmer <rhyme label="c" type="m">beeft</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Zoo lang haar God het leeven <rhyme label="c" type="m" corresp="#s4:l5">geeft</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s4:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gy Nederlanders vol van <rhyme label="a" type="m">moed</rhyme></l>
        <l id="s5:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">Draagt nu Oranje op den <rhyme label="a" type="m" corresp="#s5:l1">Hoed</rhyme></l>
        <l id="s5:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s5:l2">Hoezé</rhyme>!</l>
        <l id="s5:l5" met="4" type="-+">Laat ons te saamen zyn <rhyme label="c" type="m">verblyd</rhyme></l>
        <l id="s5:l6" met="4" type="-+">God geeft ons weer een goede <rhyme label="c" type="m" corresp="#s5:l5">tyd</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s5:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Laat ons in deeze lente <rhyme label="a" type="m">tyd</rhyme></l>
        <l id="s6:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s6:l3" met="4" type="-+">Eendragtig leeven saam <rhyme label="a" type="m" corresp="#s6:l1">verblyd</rhyme>,</l>
        <l id="s6:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s6:l2">Hoezé</rhyme>!</l>
        <l id="s6:l5" met="4" type="-+">Ter eere van 't Nassauwse <rhyme label="c" type="m">bloed</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Dat ieder een verheugen <rhyme label="c" type="m" corresp="#s6:l5">doet</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s6:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">God geeft dat wy in Dorp en <rhyme label="a" type="m">Stêe</rhyme></l>
        <l id="s7:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s7:l3" met="4" type="-+">Myn Hertewensch is na de <rhyme label="a" type="m" corresp="#s7:l1">Vree</rhyme></l>
        <l id="s7:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s7:l2">Hoezé</rhyme>!</l>
        <l id="s7:l5" met="4" type="-+">De Vlaggen waaijen de Toorens <rhyme label="c" type="m">of</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Beschoud maar eens het Haagse <rhyme label="c" type="m" corresp="#s7:l5">hof</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s7:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Hy geeft de Staten van ons <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s8:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s8:l3" met="4" type="-+">Dat zy met de Oranje <rhyme label="a" type="m" corresp="#s8:l1">plant</rhyme></l>
        <l id="s8:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s8:l2">Hoezé</rhyme>!</l>
        <l id="s8:l5" met="4" type="-+">De Oranje Boom die staat en <rhyme label="c" type="m">bloeid</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Zal nimmer worden <rhyme label="c" type="m" corresp="#s8:l5">uitgeroeid</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s8:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hy zegent onze <rhyme label="a" type="m">Magistraat</rhyme></l>
        <l id="s9:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s9:l3" met="4" type="-+">Ons Burgery die vroeg en <rhyme label="a" type="m" corresp="#s9:l1">laat</rhyme></l>
        <l id="s9:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s9:l2">Hoezé</rhyme>!</l>
        <l id="s9:l5" met="4" type="-+">Ons Burgery die vroeg en <rhyme label="c" type="m">laat</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Voor onheyl in de Waapens <rhyme label="c" type="m" corresp="#s9:l5">gaat</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s9:l2">Hoezé</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ik sing dat Vaersje tot <rhyme label="a" type="m">besluit</rhyme>,</l>
        <l id="s10:l2" met="1" type="-+"><rhyme label="b" type="m">Hoezé</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">Juig maar Eendragtig <rhyme label="a" type="m" corresp="#s10:l1">overluid</rhyme>,</l>
        <l id="s10:l4" met="1" type="-+"><rhyme label="b" type="m" corresp="#s10:l2">Hoezé</rhyme>!</l>
        <l id="s10:l5" met="4" type="-+">Set tog de Tweedragt aan een <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Dan krygt men Segen van Gods <rhyme label="c" type="m" corresp="#s10:l5">hand</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Viva Oranje <rhyme label="b" type="m" corresp="#s10:l2">Hoezé</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
