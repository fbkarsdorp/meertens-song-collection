<song>
  <meta>
    <id>190500</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Plukt 't Hoen, kaal en <rhyme label="a" type="m">naakt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Klopt de Klopper dat 't <rhyme label="a" type="m" corresp="#s1:l1">kraakt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Stroopt den huid de Paling <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Kapt Verlem zo kort als <rhyme label="b" type="m" corresp="#s1:l3">kaf</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Kruyd de Kruyer na de <rhyme label="c" type="m">Zee</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En verzuipt ook Schuurman <rhyme label="c" type="m" corresp="#s1:l5">mee</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Wybo Fynje niet <rhyme label="d" type="f">vergeeten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Moet worden in de Hel <rhyme label="d" type="f" corresp="#s1:l7">gesmeeten</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dat zyn zeeve <rhyme label="e" type="f">Patriotten</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Landverders, vuile <rhyme label="e" type="f" corresp="#s1:l9">zotten</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Oproermaakers, weg met <rhyme label="f" type="m">hen</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Weg met hun vervloekte <rhyme label="f" type="m" corresp="#s1:l11">pen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
