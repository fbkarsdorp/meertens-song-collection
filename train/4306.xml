<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4306</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Blaas oostenwind</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">NObel <rhyme label="a" type="m">divijn</rhyme> //</l>
        <l id="s1:l2" met="2" type="-+">Wesen <rhyme label="a" type="m" corresp="#s1:l1">sanguijn</rhyme> //</l>
        <l id="s1:l3" met="3" type="-+">Prijs naer ge- dienstich <rhyme label="b" type="f">poogen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Deurluchtich <rhyme label="a" type="m" corresp="#s1:l1">fijn</rhyme> //</l>
        <l id="s1:l5" met="2" type="-+">Als <rhyme label="a" type="m" corresp="#s1:l1">Christalijn</rhyme> //</l>
        <l id="s1:l6" met="3" type="-+">Rayen u Claer bruyn <rhyme label="b" type="f" corresp="#s1:l3">oogen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Lichten als <rhyme label="c" type="m">twee</rhyme> //</l>
        <l id="s1:l8" met="2" type="-+">Sterren ter <rhyme label="c" type="m" corresp="#s1:l7">zee</rhyme> //</l>
        <l id="s1:l9" met="3" type="-+">Die my mijn wee // <rhyme label="b" type="f" corresp="#s1:l3">verthoogen</rhyme> //</l>
        <l id="s1:l10" met="3" type="+-">Ondancx ick moets <rhyme label="b" type="f" corresp="#s1:l3">gedoogen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Edel <rhyme label="a" type="m">geset</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Staet lijf // en <rhyme label="a" type="m" corresp="#s2:l1">ledt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">T'mach sulck een herte <rhyme label="b" type="f">vromen</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Was Venus <rhyme label="a" type="m" corresp="#s2:l1">net</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ghy meught u <rhyme label="a" type="m" corresp="#s2:l1">met</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Veel meerder recht <rhyme label="b" type="f" corresp="#s2:l3">beromen</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Noyt en sach <rhyme label="c" type="m">man</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Volmaecter <rhyme label="c" type="m" corresp="#s2:l7">an</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Al watmen can <rhyme label="b" type="f" corresp="#s2:l3">genomen</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Ghy draeght in al <rhyme label="b" type="f" corresp="#s2:l3">t'volcomen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Eenigh <rhyme label="a" type="m">gewin</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Die van <rhyme label="a" type="m" corresp="#s3:l1">t'begin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Waert cause van <rhyme label="b" type="f">versijcke</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Maer niet te <rhyme label="a" type="m" corresp="#s3:l1">min</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Herte en <rhyme label="a" type="m" corresp="#s3:l1">sin</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Prent u in mijn <rhyme label="b" type="f" corresp="#s3:l3">Chronijcke</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Tot veel <rhyme label="c" type="m">gepijns</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Geen <rhyme label="c" type="m" corresp="#s3:l7">medecijns</rhyme></l>
        <l id="s3:l9" met="3" type="-+">En weet ick dijns // <rhyme label="b" type="f" corresp="#s3:l3">gelijcke</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Op geheel <rhyme label="b" type="f" corresp="#s3:l3">aerden-rijcke</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Leven, en <rhyme label="a" type="m">doot</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Vreught, <rhyme label="a" type="m" corresp="#s4:l1">wederstoot</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Staet in dijn <rhyme label="b" type="f">welberaden</rhyme>:</l>
        <l id="s4:l4" met="2" type="-+">Gelijck als <rhyme label="a" type="m" corresp="#s4:l1">broot</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Bluscht hongers <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Condy mijns drucx <rhyme label="b" type="f" corresp="#s4:l3">versaden</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Dus <rhyme label="c" type="m">welgemint</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Soethertich <rhyme label="c" type="m" corresp="#s4:l7">kint</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Weest my doch int // <rhyme label="b" type="f" corresp="#s4:l3">beschaden</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Een voorbeelt der <rhyme label="b" type="f" corresp="#s4:l3">genaden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Kies' ick niet <rhyme label="a" type="m">best</rhyme></l>
        <l id="s5:l2" met="2" type="-+">In dit <rhyme label="a" type="m" corresp="#s5:l1">arest</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Aflaet van mijn <rhyme label="b" type="f">doleren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Hier <rhyme label="a" type="m" corresp="#s5:l1">aengevest</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Volght een <rhyme label="a" type="m" corresp="#s5:l1">request</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Om u te <rhyme label="b" type="f" corresp="#s5:l3">remonstreren</rhyme>:</l>
        <l id="s5:l7" met="2" type="-+">Den <rhyme label="c" type="m">suppliant</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Versoect <rhyme label="c" type="m" corresp="#s5:l7">bystant</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Wilt metter hant // <rhyme label="b" type="f" corresp="#s5:l3">schriberen</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Fiat, naer <rhyme label="b" type="f" corresp="#s5:l3">t'supplieren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Een sterck <rhyme label="a" type="m">bewijs</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Van lof en <rhyme label="a" type="m" corresp="#s6:l1">prijs</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Sal u by dien <rhyme label="b" type="f">gewerden</rhyme>:</l>
        <l id="s6:l4" met="2" type="-+">Schoon ieughdich <rhyme label="a" type="m" corresp="#s6:l1">rijs</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Gestaen <rhyme label="a" type="m" corresp="#s6:l1">propijs</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ick moet u naerder <rhyme label="b" type="f" corresp="#s6:l3">terden</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Waer my <rhyme label="c" type="m">ontseyt</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Dijn <rhyme label="c" type="m" corresp="#s6:l7">mogentheyt</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En gaeft afscheyt ten <rhyme label="b" type="f" corresp="#s6:l3">derden</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Noch soud' ick t'stuck <rhyme label="b" type="f" corresp="#s6:l3">volherden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Niet el schoon <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Heyscht desen <rhyme label="a" type="m" corresp="#s7:l1">brief</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">By mijnder hant <rhyme label="b" type="f">geschreven</rhyme>:</l>
        <l id="s7:l4" met="2" type="-+">Doen ick <rhyme label="a" type="m" corresp="#s7:l1">besief</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Dat al mijn <rhyme label="a" type="m" corresp="#s7:l1">grief</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">By u mocht zijn <rhyme label="b" type="f" corresp="#s7:l3">verdreven</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Heb ick mijn <rhyme label="c" type="m">leet</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Voor u <rhyme label="c" type="m" corresp="#s7:l7">ontcleet</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Dus wilt gereet // doch <rhyme label="b" type="f" corresp="#s7:l3">geven</rhyme>,</l>
        <l id="s7:l10" met="3" type="+-">T'behouden van mijn <rhyme label="b" type="f" corresp="#s7:l3">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
