<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183001</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nadien uw goddelijkheid</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Roemwaardig <rhyme label="a" type="m">Nassauws-bloed</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Op wien de <rhyme label="a" type="m" corresp="#s1:l1">euvelmoed</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Mikt met vergifte <rhyme label="b" type="f">schigten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ik wil, op zuivre <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Uw toebrengen myn <rhyme label="b" type="f" corresp="#s1:l3">gedigten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">O dierbre Friso's <rhyme label="c" type="m" corresp="#s1:l4">Zoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Voor u, <rhyme label="a" type="m">Oranje-Spruit</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Voor u speelt nu myn <rhyme label="a" type="m" corresp="#s2:l1">luit</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Voor u wil ik <rhyme label="b" type="f">opzingen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Voor u wil ik myn <rhyme label="c" type="m">zang</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Voor u myn lof <rhyme label="b" type="f" corresp="#s2:l3">toebrengen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Voor u, edel in <rhyme label="c" type="m" corresp="#s2:l4">rang</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Leef lang, ô Neêrlands <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Leef lang, die Neêrland <rhyme label="a" type="m" corresp="#s3:l1">torst</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Leef lang, tot Neêrlands <rhyme label="b" type="f">Luister</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Leef lang, in Neêrlands <rhyme label="c" type="m">Raad</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Leef lang, breek Neêrlands <rhyme label="b" type="f" corresp="#s3:l3">kluister</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Leef lang, voor Neêrlands <rhyme label="c" type="m" corresp="#s3:l4">Zaad</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Op uw zy onze <rhyme label="a" type="m">hoop</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Stuit gy de <rhyme label="a" type="m" corresp="#s4:l1">Oproer-loop</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met u willen wy <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Voor uw is ons <rhyme label="c" type="m">gebed</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">By uw is Neêrlands <rhyme label="b" type="f" corresp="#s4:l3">zegen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">By u is Godes <rhyme label="c" type="m" corresp="#s4:l4">Wet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Men ziet de zoete <rhyme label="a" type="m">Jeugd</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Bekleed met zuivre <rhyme label="a" type="m" corresp="#s5:l1">deugd</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Waar in wy ons <rhyme label="b" type="f">beroemen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ik meen 't <rhyme label="c" type="m">Oranje-bloed</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Dat wy gezegend <rhyme label="b" type="f" corresp="#s5:l3">noemen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En zwaaijen met de <rhyme label="c" type="m" corresp="#s5:l4">hoed</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Uw lof word nog <rhyme label="a" type="m">vergroot</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Uw lof duurt na uw <rhyme label="a" type="m" corresp="#s6:l1">dood</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Uw lof zal lustig <rhyme label="b" type="f">bloeijen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Uw lof wast meer en <rhyme label="c" type="m">meer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Uw lof zal lustig <rhyme label="b" type="f" corresp="#s6:l3">groeijen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Uw hulp zy van den <rhyme label="c" type="m" corresp="#s6:l4">Heer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Myn tong vermeld uw <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Myn pen geeft <rhyme label="a" type="m" corresp="#s7:l1">juichensstof</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Myn lust is tot <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Myn hart wenst hem <rhyme label="c" type="m">voorspoed</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Daar wy vry zyn van <rhyme label="b" type="f" corresp="#s7:l3">Spanje</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Door zyn Voorouders <rhyme label="c" type="m" corresp="#s7:l4">Bloed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Zo lang de zon zal <rhyme label="a" type="m">staan</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">De maan zal schynen <rhyme label="a" type="m" corresp="#s8:l1">gaan</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Zal ik de lof <rhyme label="b" type="f">vermelden</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Daar zy zyn agting <rhyme label="c" type="m">waard</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Van de braave <rhyme label="b" type="f" corresp="#s8:l3">Oranje-helden</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">De gansche wereld door <rhyme label="c" type="m" corresp="#s8:l4">vermaard</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
