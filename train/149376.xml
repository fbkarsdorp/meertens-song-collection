    <melody>Psalm 031 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">U Heere heb ick <rhyme label="a" type="f">uytverkoren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In al mijns levens <rhyme label="b" type="m">loop</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Tot Grondtvest mijnder <rhyme label="b" type="m" corresp="#s1:l2">hoop</rhyme>’.</l>
        <l id="s1:l4" met="4" type="-+">Dies laetse nimmermeer <rhyme label="a" type="f" corresp="#s1:l1">verloren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat schandt-roodt door haer <rhyme label="c" type="m">sterven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn wangen noyt magh <rhyme label="c" type="m" corresp="#s1:l5">verven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Verlos, rechtveerdigh <rhyme label="a" type="f">opper-Heere</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Verlos van alle <rhyme label="b" type="m">quaedt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat my rontsomme <rhyme label="b" type="m" corresp="#s2:l2">staet</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Laet sich u heyligh oor niet <rhyme label="a" type="f" corresp="#s2:l1">keeren</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Van mijn demoedich <rhyme label="c" type="m">quellen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Maer wilt ter hulpe <rhyme label="c" type="m" corresp="#s2:l5">snellen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laet d’uytkomst niet mijn hoop’ <rhyme label="a" type="f">benemen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Maer toont u in <rhyme label="b" type="m">ellendt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">’tGeen daer u ’thert voor <rhyme label="b" type="m" corresp="#s3:l2">kent</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Een stercken rots-steen om my <rhyme label="a" type="f" corresp="#s3:l1">henen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Een burght en vaste <rhyme label="c" type="m">wallen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Een stutsel voor het <rhyme label="c" type="m" corresp="#s3:l5">vallen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En om u naem een klanck te <rhyme label="a" type="f">maken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In aller menschen <rhyme label="b" type="m">oor</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Soo weest my uyt en <rhyme label="b" type="m" corresp="#s4:l2">door</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Een stuer en richtmaet mijnder <rhyme label="a" type="f" corresp="#s4:l1">saken</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Op dat de loose <rhyme label="c" type="m">netten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Vergeefs sich voor my <rhyme label="c" type="m" corresp="#s4:l5">setten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy zijt mijn sterckt daer ick aen <rhyme label="a" type="f">houwe</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Mijn leven, ’twaerdtste <rhyme label="b" type="m">pandt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Beveel ick in uw’ <rhyme label="b" type="m" corresp="#s5:l2">handt</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Met recht. want ghy, mijn Godt vol <rhyme label="a" type="f" corresp="#s5:l1">trouwe</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hebt menighmael een <rhyme label="c" type="m">ende</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Gemaeckt van mijn <rhyme label="c" type="m" corresp="#s5:l5">ellende</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ick haetse die de leugen <rhyme label="a" type="f">lieven</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En nemen haren <rhyme label="b" type="m">keer</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ter ongesoutter <rhyme label="b" type="m" corresp="#s6:l2">leer</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Van’s waerheydts yd’le <rhyme label="a" type="f" corresp="#s6:l1">eeren-dieven</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Maer sie geen beter <rhyme label="c" type="m">Open</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dan op mijn Godt te <rhyme label="c" type="m" corresp="#s6:l5">hopen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">’tVervrolijckt my in mijn <rhyme label="a" type="f">inwendigh</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Nu ’thert, in noodt, <rhyme label="b" type="m">bevindt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dat ghy my meent en <rhyme label="b" type="m" corresp="#s7:l2">mint</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En my, hoewel op’t hooghst <rhyme label="a" type="f" corresp="#s7:l1">ellendigh</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Den gunst-strael uwer <rhyme label="c" type="m">oogen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Niet eeuw’lijck hebt <rhyme label="c" type="m" corresp="#s7:l5">ontogen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy hebt my, Heer, niet <rhyme label="a" type="f">overgeven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ter wreeder <rhyme label="b" type="m">slaverny</rhyme></l>
        <l id="s8:l3" met="3" type="-+">In handen van <rhyme label="b" type="m" corresp="#s8:l2">party</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Maer hebt my ’taengename <rhyme label="a" type="f" corresp="#s8:l1">leven</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Geredt, dat ick my <rhyme label="c" type="m">reppen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">En ruyme lucht magh <rhyme label="c" type="m" corresp="#s8:l5">scheppen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O weest my nu als wel voor <rhyme label="a" type="f">desen</rhyme>.</l>
        <l id="s9:l2" met="3" type="-+">En daelt, in plaets van <rhyme label="b" type="m">straff</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Genadigh op my <rhyme label="b" type="m" corresp="#s9:l2">aff</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Want my is bang. Mijn heele <rhyme label="a" type="f" corresp="#s9:l1">wesen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Door swaer getreur <rhyme label="c" type="m">vervallen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Is schier op <rhyme label="c" type="m" corresp="#s9:l5">niemendallen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De ooge traent, en is <rhyme label="a" type="f">ontluystert</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Der zielen kracht <rhyme label="b" type="m">verdwijnt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Het swacke lichaem <rhyme label="b" type="m" corresp="#s10:l2">quijnt</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">De Reden taent en is <rhyme label="a" type="f" corresp="#s10:l1">verduystert</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">De leden en <rhyme label="c" type="m">gewrichten</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Vertragen in haer <rhyme label="c" type="m" corresp="#s10:l5">plichten</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">In smert verteer ick mijne <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Door d’opgehoopten <rhyme label="b" type="m">druck</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Van’t droeve <rhyme label="b" type="m" corresp="#s11:l2">tegen-luck</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Mijn leven slijt in angstigh <rhyme label="a" type="f" corresp="#s11:l1">klagen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">In overvloedigh <rhyme label="c" type="m">weenen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">En diep-gehaelde <rhyme label="c" type="m" corresp="#s11:l5">steenen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ach! door mijn <rhyme label="a" type="f">ongerechticheden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">(Fonteynen van <rhyme label="b" type="m">’tverdriet</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Dat om my henen <rhyme label="b" type="m" corresp="#s12:l2">vliet</rhyme>)</l>
        <l id="s12:l4" met="4" type="-+">Ontmannen sich de stercke <rhyme label="a" type="f" corresp="#s12:l1">leden</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Ontspannen mijne <rhyme label="c" type="m">zenen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Verkanck’ren mijne <rhyme label="c" type="m" corresp="#s12:l5">beenen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ick sta mijn felle <rhyme label="a" type="f">we’er-partijen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Ten doel-wit van <rhyme label="b" type="m">gepraet</rhyme></l>
        <l id="s13:l3" met="3" type="-+">En dichten <rhyme label="b" type="m" corresp="#s13:l2">laster-smaet</rhyme>.</l>
        <l id="s13:l4" met="4" type="-+">Mijn buren streckt mijn hooge <rhyme label="a" type="f" corresp="#s13:l1">lij’en</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Niet dan een schimpigh <rhyme label="c" type="m">kluchjen</rhyme>.</l>
        <l id="s13:l6" met="3" type="-+">En dertel <rhyme label="c" type="m" corresp="#s13:l5">oor-geruchjen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Hoe traegh laet sich d’Ellende <rhyme label="a" type="f">stuyten</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Wanneers’ eens haren <rhyme label="b" type="m">tredt</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Ten onheyl heeft <rhyme label="b" type="m" corresp="#s14:l2">geset</rhyme>!</l>
        <l id="s14:l4" met="4" type="-+">Off blijv’ ick in, off koom ick <rhyme label="a" type="f" corresp="#s14:l1">buyten</rhyme>:</l>
        <l id="s14:l5" met="3" type="-+">’kSie vrunden en <rhyme label="c" type="m">bekenden</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Het aensicht van my <rhyme label="c" type="m" corresp="#s14:l5">wenden</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Ick ben een schrick in haren <rhyme label="a" type="f">oogen</rhyme>.</l>
        <l id="s15:l2" met="3" type="-+">Soo haest sy my maer <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Een yeder is aen’t <rhyme label="b" type="m" corresp="#s15:l2">vlien</rhyme>:</l>
        <l id="s15:l4" met="4" type="-+">Recht off Ick, nu ’tluck is <rhyme label="a" type="f" corresp="#s15:l1">vervlogen</rhyme></l>
        <l id="s15:l5" met="3" type="-+">En ellendt my <rhyme label="c" type="m">om-wandert</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">In ellendt waer <rhyme label="c" type="m" corresp="#s15:l5">verandert</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Ick ben al doodt in haer <rhyme label="a" type="f">gedachten</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Gelijck een vat dat <rhyme label="b" type="m">breeckt</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">Daer niemandt meer van <rhyme label="b" type="m" corresp="#s16:l2">spreeckt</rhyme>.</l>
        <l id="s16:l4" met="4" type="-+">Mijn tranen, suchten, weenen, <rhyme label="a" type="f" corresp="#s16:l1">klachten</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Zijn losselijck <rhyme label="c" type="m">vergeten</rhyme></l>
        <l id="s16:l6" met="3" type="-+">En ver uyt ’thert <rhyme label="c" type="m" corresp="#s16:l5">versmeten</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Als sy met lasterlijck <rhyme label="a" type="f">verwijten</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">’tWelck mijn verladen <rhyme label="b" type="m">oor</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Gestadigh komt te <rhyme label="b" type="m" corresp="#s17:l2">voor</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">My van veel tongen sien <rhyme label="a" type="f" corresp="#s17:l1">besmijten</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Begint elck een te <rhyme label="c" type="m">grouwen</rhyme></l>
        <l id="s17:l6" met="3" type="-+">En my te meer te <rhyme label="c" type="m" corresp="#s17:l5">schouwen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">’tEn blijft, ’ten blijft noch niet by <rhyme label="a" type="f">woorden</rhyme>:</l>
        <l id="s18:l2" met="3" type="-+">Sy, yv’righ in het <rhyme label="b" type="m">quaedt</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Slaen onderlingen <rhyme label="b" type="m" corresp="#s18:l2">raedt</rhyme>.</l>
        <l id="s18:l4" met="4" type="-+">In hare herten, graegh tot <rhyme label="a" type="f" corresp="#s18:l1">moorden</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Is’t eynde van mijn <rhyme label="c" type="m">leven</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Al vastelijck <rhyme label="c" type="m" corresp="#s18:l5">geschreven</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Dus staet mijn staet. en <rhyme label="a" type="f">midd’lerwijlen</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">In’t dickste van de <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Leeft noch de hoop’ in’t <rhyme label="b" type="m" corresp="#s19:l2">hert</rhyme>:</l>
        <l id="s19:l4" met="4" type="-+">De hoop’, die, met onwanck’le <rhyme label="a" type="f" corresp="#s19:l1">stijlen</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Op u gegrondt, dees <rhyme label="c" type="m">tale</rhyme></l>
        <l id="s19:l6" met="3" type="-+">uyt dieper ziel komt <rhyme label="c" type="m" corresp="#s19:l5">halen</rhyme>:</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Ghy zijt mijn Godt. Al dreyght het <rhyme label="a" type="f">Ende</rhyme>:</l>
        <l id="s20:l2" met="3" type="-+">Ghy nauwt en ruymt den <rhyme label="b" type="m">draedt</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Van mijnes levens <rhyme label="b" type="m" corresp="#s20:l2">maet</rhyme>;</l>
        <l id="s20:l4" met="4" type="-+">Al smoortmen schier in sijn <rhyme label="a" type="f" corresp="#s20:l1">ellende</rhyme>:</l>
        <l id="s20:l5" met="3" type="-+">Ghy paelt en perckt de <rhyme label="c" type="m">uren</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Van goedts en quaedts <rhyme label="c" type="m" corresp="#s20:l5">geduren</rhyme></l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Verlos my uyt de wreede <rhyme label="a" type="f">klauwen</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Van die my vyandt <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">En, op gesochten <rhyme label="b" type="m" corresp="#s21:l2">schijn</rhyme>,</l>
        <l id="s21:l4" met="4" type="-+">My hard belagen en <rhyme label="a" type="f" corresp="#s21:l1">benauwen</rhyme>.</l>
        <l id="s21:l5" met="3" type="-+">Ach! laet uws goedtheydts <rhyme label="c" type="m">stralen</rhyme></l>
        <l id="s21:l6" met="3" type="-+">Op my, uw’, dienaer <rhyme label="c" type="m" corresp="#s21:l5">dalen</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Laet door’t te trage <rhyme label="a" type="f">hulp-verwerven</rhyme></l>
        <l id="s22:l2" met="3" type="-+">(Want u, Heer, roep ick <rhyme label="b" type="m">aen</rhyme>.)</l>
        <l id="s22:l3" met="3" type="-+">Mijn klacht ten spot niet <rhyme label="b" type="m" corresp="#s22:l2">staen</rhyme></l>
        <l id="s22:l4" met="4" type="-+">Noch bloode schaemt mijn wangen <rhyme label="a" type="f" corresp="#s22:l1">verven</rhyme>:</l>
        <l id="s22:l5" met="3" type="-+">Maer laet de <rhyme label="c" type="m">Godt-vergeten</rhyme></l>
        <l id="s22:l6" met="3" type="-+">Met schande zijn <rhyme label="c" type="m" corresp="#s22:l5">door-smeten</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Laet sich de Doodt tot haerwaerts <rhyme label="a" type="f">snellen</rhyme>,</l>
        <l id="s23:l2" met="3" type="-+">En ’tzielle-loos <rhyme label="b" type="m">gekil</rhyme></l>
        <l id="s23:l3" met="3" type="-+">Van’t eeuwigh-eeuwigh <rhyme label="b" type="m" corresp="#s23:l2">Stil</rhyme></l>
        <l id="s23:l4" met="4" type="-+">Haer tonge binden in der <rhyme label="a" type="f" corresp="#s23:l1">Hellen</rhyme></l>
        <l id="s23:l5" met="3" type="-+">(Tot loon der booser <rhyme label="c" type="m">domheydt</rhyme>)</l>
        <l id="s23:l6" met="3" type="-+">Met eyndeloose <rhyme label="c" type="m" corresp="#s23:l5">stomheydt</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Verstommen moeten alle <rhyme label="a" type="f">monden</rhyme></l>
        <l id="s24:l2" met="3" type="-+">Die oyt, ter <rhyme label="b" type="m">leugen-spraeck</rhyme></l>
        <l id="s24:l3" met="3" type="-+">Gëeygent, haer <rhyme label="b" type="m" corresp="#s24:l2">vermaeck</rhyme></l>
        <l id="s24:l4" met="4" type="-+">In’t lasteren der vromen <rhyme label="a" type="f" corresp="#s24:l1">vonden</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">En met verglimpte <rhyme label="c" type="m">streken</rhyme></l>
        <l id="s24:l6" met="3" type="-+">Verwaent en schimpigh <rhyme label="c" type="m" corresp="#s24:l5">spreken</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Hoe groot zijn, Heer, uws goedtheydts <rhyme label="a" type="f">stroomen</rhyme></l>
        <l id="s25:l2" met="3" type="-+">Die gh’over hen laet <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s25:l3" met="3" type="-+">Die in uw’ vreese <rhyme label="b" type="m" corresp="#s25:l2">staen</rhyme>!</l>
        <l id="s25:l4" met="4" type="-+">Wat schat bewaert ghy voor den <rhyme label="a" type="f" corresp="#s25:l1">vromen</rhyme>!</l>
        <l id="s25:l5" met="3" type="-+">Hoe klaer toont ghy, voor <rhyme label="c" type="m">allen</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">Uw’godlijck <rhyme label="c" type="m" corresp="#s25:l5">wel-gevallen</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
