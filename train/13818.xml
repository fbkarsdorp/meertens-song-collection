      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>anoniem</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL heeft ons nu <rhyme label="a" type="f">begheven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den Somer met haer <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Men siet hier weder <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Van nieuws de groene <rhyme label="b" type="m" corresp="#s1:l2">jeught</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Aen dese twee, die heden <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Het paren nu <rhyme label="d" type="f">beginnen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Wy wenschen haer, dat sy voor ‘t <rhyme label="e" type="m">jaer</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De derde moghen <rhyme label="d" type="f" corresp="#s1:l6">winnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al is het Wout <rhyme label="a" type="f">verstorven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En d’ ouden stam <rhyme label="b" type="m">vergaen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Nochtans zijn niet <rhyme label="a" type="f" corresp="#s2:l1">bedorven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">De spruytjes met haer <rhyme label="b" type="m" corresp="#s2:l2">bla’en</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Maer komen weer, al even <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Door warmt’ en vochts <rhyme label="d" type="f">mee-deelen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Van tijt tot tijt, op ‘t aertrijck <rhyme label="e" type="m">wijt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Meer jonghe Woutjes <rhyme label="d" type="f" corresp="#s2:l6">teelen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Het gheeft de <rhyme label="a" type="f">Bosch-Goddinnen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En al de Nimphjes <rhyme label="b" type="m">vreemt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dit onverwachte <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat haer ghespeel wech <rhyme label="b" type="m" corresp="#s3:l2">neemt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Die nu verlaet, de Maeghden <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En is gheworden <rhyme label="d" type="f">verder</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Door d’ echte trouw, de rechte <rhyme label="e" type="m">Vrouw</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Van haer beminde <rhyme label="d" type="f" corresp="#s3:l6">herder</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Laet u geen wonder <rhyme label="a" type="f">gheven</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat u dees’ Maeght <rhyme label="b" type="m">ontviel</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Sy wil nu haest <rhyme label="a" type="f" corresp="#s4:l1">aenkleven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Haer Lief, haer tweede <rhyme label="b" type="m" corresp="#s4:l2">ziel</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Sy acht doch meer, haer Hooft en <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Haer blijdtschap, haer <rhyme label="d" type="f">vermaken</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Dan Maeghde praet, of vryen <rhyme label="e" type="m">staet</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die sy nu gaet <rhyme label="d" type="f" corresp="#s4:l6">versaken</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Winters koude <rhyme label="a" type="f">nachten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hoeft sy te vreesen <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die ghy noch moet <rhyme label="a" type="f" corresp="#s5:l1">verwachten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">‘t Welcke menich Maeght <rhyme label="b" type="m" corresp="#s5:l2">verdriet</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Maer dese twee, die moghen <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Malkanders voetjes <rhyme label="d" type="f">warmen</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En als ‘t haer lust, nemen de <rhyme label="e" type="m">rust</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Elck in sijn Liefjes <rhyme label="d" type="f" corresp="#s5:l6">armen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Truert dan niet meer o <rhyme label="a" type="f">Maeghden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ghy hebt gheen truerens <rhyme label="b" type="m">noot</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Maer even seer sy <rhyme label="a" type="f" corresp="#s6:l1">klaeghden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">‘t Verlies was al te <rhyme label="b" type="m" corresp="#s6:l2">groot</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ick sey sus, sus, hoe hebt ghy <rhyme label="c" type="m">dus</rhyme></l>
        <l id="s6:l6" met="3" type="-+">De droefheyt <rhyme label="d" type="f">ingheswolghen</rhyme>?</l>
        <l id="s6:l7" met="4" type="-+">Ey weent niet veel, om u <rhyme label="e" type="m">Ghespeel</rhyme>:</l>
        <l id="s6:l8" met="3" type="-+">Maer wilt haer liever <rhyme label="d" type="f" corresp="#s6:l6">volghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Soo salmen u <rhyme label="a" type="f">bekroonen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Met Palm en Laurier <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Iae vreught en blijdschap <rhyme label="a" type="f" corresp="#s7:l1">toonen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ghelijck wy nou hier <rhyme label="b" type="m" corresp="#s7:l2">doen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En yder mensch, sal met een <rhyme label="c" type="m">wensch</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">V veel gheluck <rhyme label="d" type="f">aenbieden</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Op ‘t aertsche dal, ‘t welck wy voor <rhyme label="e" type="m">al</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wenschen dees echte <rhyme label="d" type="f" corresp="#s7:l6">Lieden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
