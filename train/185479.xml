<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185479</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie Jezus uw Heer</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">By 't jong ontloken <rhyme label="a" type="m">groen</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">Jn 't jeugdig <rhyme label="a" type="m" corresp="#s1:l1">lent-saizoen</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">Wil Philis aan Thirza <rhyme label="b" type="f">denken</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Myn hart door min <rhyme label="c" type="m">ontvonkt</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Word als het waar <rhyme label="c" type="m" corresp="#s1:l4">verjonkt</rhyme> /</l>
        <l id="s1:l6" met="4" type="-+">Ik wil de liefde een deuntje <rhyme label="b" type="f" corresp="#s1:l3">schenken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een plant van <rhyme label="a" type="m">violier</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">Een keurige <rhyme label="a" type="m" corresp="#s2:l1">angelier</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">Een roosje met ontloke <rhyme label="b" type="f">bladen</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Werd door myn hand <rhyme label="c" type="m">gevlecht</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Zy Thirza <rhyme label="c" type="m" corresp="#s2:l4">toegezecht</rhyme> /</l>
        <l id="s2:l6" met="4" type="-+">Het zal myn teder hart <rhyme label="b" type="f" corresp="#s2:l3">verraden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dog welk een <rhyme label="a" type="m">ziels-verdriet</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">Myn Thirza mint mij <rhyme label="a" type="m" corresp="#s3:l1">niet</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Wat helpt een krans van <rhyme label="b" type="f">bloemen</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Als ik haar slegts <rhyme label="c" type="m">ontmoet</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Met al de <rhyme label="c" type="m" corresp="#s3:l4">herdersstoet</rhyme> /</l>
        <l id="s3:l6" met="4" type="-+">Dan zal ik stil haar naamtje <rhyme label="b" type="f" corresp="#s3:l3">noemen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'k Zal fluks haar tegen <rhyme label="a" type="m">gaan</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">'k Zal zeggen ei blyvt <rhyme label="a" type="m" corresp="#s4:l1">staan</rhyme> /</l>
        <l id="s4:l3" met="4" type="-+">'k Ben Philis / Ã´ myn teer <rhyme label="b" type="f">beminden</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">'k Vlieg ylings om haar <rhyme label="c" type="m">hals</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Zoo teder kuisch en <rhyme label="c" type="m" corresp="#s4:l4">mals</rhyme> /</l>
        <l id="s4:l6" met="4" type="-+">Een enk'le kus zal my <rhyme label="b" type="f" corresp="#s4:l3">verbinden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dog zoo zy <rhyme label="a" type="m">onverwagt</rhyme> /</l>
        <l id="s5:l2" met="3" type="-+">Om al myn reeden <rhyme label="a" type="m" corresp="#s5:l1">lagt</rhyme> /</l>
        <l id="s5:l3" met="4" type="-+">Dan wil ik zelve voor haar <rhyme label="b" type="f">knielen</rhyme> /</l>
        <l id="s5:l4" met="3" type="-+">De min heeft altoos <rhyme label="c" type="m">dans</rhyme> /</l>
        <l id="s5:l5" met="3" type="-+">Dit voorregt voegt de <rhyme label="c" type="m" corresp="#s5:l4">mans</rhyme> /</l>
        <l id="s5:l6" met="4" type="-+">De min doet steen en was <rhyme label="b" type="f" corresp="#s5:l3">bezielen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maar blyft zy stuurs en <rhyme label="a" type="m">straf</rhyme> /</l>
        <l id="s6:l2" met="3" type="-+">'k Zie daad'lyk van haar <rhyme label="a" type="m" corresp="#s6:l1">af</rhyme> /</l>
        <l id="s6:l3" met="4" type="-+">Regtschaapen lievde kan my <rhyme label="b" type="f">streelen</rhyme> /</l>
        <l id="s6:l4" met="3" type="-+">Geen stuurze <rhyme label="c" type="m">herderin</rhyme> /</l>
        <l id="s6:l5" met="3" type="-+">Ontvonkt myn hart en <rhyme label="c" type="m" corresp="#s6:l4">min</rhyme> /</l>
        <l id="s6:l6" met="4" type="-+">Of zal het doel eens Ionkmans <rhyme label="b" type="f" corresp="#s6:l3">streelen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dog zoo zy teer van <rhyme label="a" type="m">aart</rhyme> /</l>
        <l id="s7:l2" met="3" type="-+">Verstand met liefde <rhyme label="a" type="m" corresp="#s7:l1">paart</rhyme> /</l>
        <l id="s7:l3" met="4" type="-+">Zy zal altoos myn schoonste <rhyme label="b" type="f">weezen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Dan leef ik vry van <rhyme label="c" type="m">smart</rhyme> /</l>
        <l id="s7:l5" met="3" type="-+">'t Schenk Haar een Mami'lyk <rhyme label="c" type="m" corresp="#s7:l4">hart</rhyme> /</l>
        <l id="s7:l6" met="4" type="-+">En eeuwig zy dien stond <rhyme label="b" type="f" corresp="#s7:l3">gepreezen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
