<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160837</id>
    <date>
      <year-start>1829</year-start>
      <year-end>1829</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Kon ik een meisje vinden</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een Nachtegaal die <rhyme label="a" type="f">kweelde</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zeer lieflyk in het <rhyme label="b" type="m">Veld</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Een Lied dat Damon <rhyme label="a" type="f" corresp="#s1:l1">streelde</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wyl Philis hem <rhyme label="b" type="m" corresp="#s1:l2">verzeld</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ach! sprak hy: die strelende <rhyme label="c" type="f">tonen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die zullen onz' Liefde <rhyme label="c" type="f" corresp="#s1:l5">bekronen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Ach Philis! geef my maar een <rhyme label="d" type="m">kusch</rhyme>;</l>
        <l id="s1:l8" met="4" type="-+">Opdat ik uw minnegloed <rhyme label="d" type="m" corresp="#s1:l7">blusch</rhyme>;</l>
        <l id="s1:l9" met="4" type="-+">Ach Philis! geef my maar een <rhyme label="d" type="m" corresp="#s1:l7">kusch</rhyme>;</l>
        <l id="s1:l10" met="4" type="-+">Opdat ik uw minnegloed <rhyme label="d" type="m" corresp="#s1:l7">blusch</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Uw minnegloed <rhyme label="d" type="m" corresp="#s1:l7">blusch</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zy aarzelde die <rhyme label="a" type="f">schoonen</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Voldeed niet zyn <rhyme label="b" type="m">begeer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hy kuschte hare <rhyme label="a" type="f" corresp="#s2:l1">konen</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Zy zegen beide <rhyme label="b" type="m" corresp="#s2:l2">neer</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Ach! sprak zy: gy hebt my <rhyme label="c" type="f">veroverd</rhyme>;</l>
        <l id="s2:l6" met="4" type="-+">Ik ben door de min reeds <rhyme label="c" type="f" corresp="#s2:l5">betoverd</rhyme>;</l>
        <l id="s2:l7" met="4" type="-+">Nu kuschte haar Damon nog <rhyme label="d" type="m">meer</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">En Philis omhelsden hun <rhyme label="d" type="m" corresp="#s2:l7">weer</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Nu kuschte haar Damon nog <rhyme label="d" type="m" corresp="#s2:l7">meer</rhyme>;</l>
        <l id="s2:l10" met="4" type="-+">En Philis omhelsden hun <rhyme label="d" type="m" corresp="#s2:l7">weer</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Omhelsden hun <rhyme label="d" type="m" corresp="#s2:l7">weer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy zyn toen neer <rhyme label="a" type="f">gezeten</rhyme>;</l>
        <l id="s3:l2" met="3" type="-+">Door 't schieten van de <rhyme label="b" type="m">min</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Kon Damon niet <rhyme label="a" type="f" corresp="#s3:l1">vergeten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat Philis deed zyn <rhyme label="b" type="m" corresp="#s3:l2">zin</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Ach! sprak hy: het licht uit myn <rhyme label="c" type="f">oogen</rhyme>;</l>
        <l id="s3:l6" met="4" type="-+">Heb ik uw dan eind'lyk <rhyme label="c" type="f" corresp="#s3:l5">bewogen</rhyme>;</l>
        <l id="s3:l7" met="4" type="-+">Gy koesterd de min in myn <rhyme label="d" type="m">hart</rhyme>;</l>
        <l id="s3:l8" met="4" type="-+">Ik ben door de Liefde <rhyme label="d" type="m" corresp="#s3:l7">verward</rhyme>;</l>
        <l id="s3:l9" met="4" type="-+">Gy koesterd de min in myn <rhyme label="d" type="m" corresp="#s3:l7">hart</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Ik ben door de Liefde <rhyme label="d" type="m" corresp="#s3:l7">verward</rhyme>:</l>
        <l id="s3:l11" met="2" type="+-">De Liefde <rhyme label="d" type="m" corresp="#s3:l7">verward</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Als Cupido zyn <rhyme label="a" type="f">pylen</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">Nu kwyt was per <rhyme label="b" type="m">hazard</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Wou Philis niet <rhyme label="a" type="f" corresp="#s4:l1">verwylen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maar deernis trof haar <rhyme label="b" type="m" corresp="#s4:l2">hart</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Wyl Damon haar niet meer kon <rhyme label="c" type="f">raken</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">En haar door geen wellust <rhyme label="c" type="f" corresp="#s4:l5">vermaken</rhyme>;</l>
        <l id="s4:l7" met="4" type="-+">Ach! sprak zy: wat ben ik <rhyme label="d" type="m">misleid</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Ik ben door de min <rhyme label="d" type="m" corresp="#s4:l7">gekastyd</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Ach! sprak zy: wat ben ik <rhyme label="d" type="m" corresp="#s4:l7">misleid</rhyme>;</l>
        <l id="s4:l10" met="4" type="-+">Ik ben door de min <rhyme label="d" type="m" corresp="#s4:l7">gekastyd</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Ja, zwaar <rhyme label="d" type="m" corresp="#s4:l7">gekastyd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gaat gy zomtyds uit <rhyme label="a" type="f">vryen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wanneer de <rhyme label="b" type="m">Nachtegaal</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zyn toonen uw <rhyme label="a" type="f" corresp="#s5:l1">misleijen</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Schiet dan geen <rhyme label="b" type="m" corresp="#s5:l2">minnestraal</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Wilt nimmer een Meisje <rhyme label="c" type="f">bedriegen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Of door uw geveinsdheid <rhyme label="c" type="f" corresp="#s5:l5">voorliegen</rhyme>;</l>
        <l id="s5:l7" met="4" type="-+">Den Hemel die uw steeds <rhyme label="d" type="m">verzeld</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Bevestigd uw Eed in 't <rhyme label="d" type="m" corresp="#s5:l7">Veld</rhyme>;</l>
        <l id="s5:l9" met="4" type="-+">Den Hemel die u steeds <rhyme label="d" type="m" corresp="#s5:l7">verzeld</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Bevestigd uw Eed in 't <rhyme label="d" type="m" corresp="#s5:l7">veld</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">Uw Eed in 't <rhyme label="d" type="m" corresp="#s5:l7">Veld</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gy Heders wilt gy <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Volgt dan de les van <rhyme label="b" type="m">myn</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Doet aan geen <rhyme label="a" type="f" corresp="#s6:l1">Herderinnen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Verdriet door <rhyme label="b" type="m" corresp="#s6:l2">minnepyn</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Wilt haar door geveinsdheid niet <rhyme label="c" type="f">kwellen</rhyme>;</l>
        <l id="s6:l6" met="4" type="-+">Dan zal uw geluk steeds <rhyme label="c" type="f" corresp="#s6:l5">verzellen</rhyme>;</l>
        <l id="s6:l7" met="4" type="-+">Als gy gevoeld min voor een <rhyme label="d" type="m">Vrouw</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Blyf haar dan standvastig <rhyme label="d" type="m" corresp="#s6:l7">getrouw</rhyme>;</l>
        <l id="s6:l9" met="4" type="-+">Als gy gevoeld min voor een <rhyme label="d" type="m" corresp="#s6:l7">Vrouw</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Blyf haar dan standvastig <rhyme label="d" type="m" corresp="#s6:l7">getrouw</rhyme>,</l>
        <l id="s6:l11" met="2" type="+-">Standvastig <rhyme label="d" type="m" corresp="#s6:l7">getrouw</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Tot slot: gy <rhyme label="a" type="f">Herderinnen</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Steld niet uw eer ten <rhyme label="b" type="m">toon</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wilt niet zoo ras <rhyme label="a" type="f" corresp="#s7:l1">beminnen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Al komt een Herder <rhyme label="b" type="m" corresp="#s7:l2">schoon</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Die uw door gevlei wil <rhyme label="c" type="f">vermaken</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ontzegt hem uw min, wilt niet <rhyme label="c" type="f" corresp="#s7:l5">blaken</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Want Liefde gebluscht, word soms <rhyme label="d" type="m">guur</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">En blyft steeds bedrog op den <rhyme label="d" type="m" corresp="#s7:l7">duur</rhyme>,</l>
        <l id="s7:l9" met="4" type="-+">Want Liefde gebluscht, word soms <rhyme label="d" type="m" corresp="#s7:l7">guur</rhyme>,</l>
        <l id="s7:l10" met="4" type="-+">En blyft steeds bedrog op den <rhyme label="d" type="m" corresp="#s7:l7">duur</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">Bedrog op den <rhyme label="d" type="m" corresp="#s7:l7">duur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
