    <melody>Tien geboden</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Volkerus van Oosterwyck, Volckerus van Oosterwyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VVAer of u Lief, gegaen mag <rhyme label="a" type="f">wesen</rhyme>?</l>
        <l id="s1:l2" met="4" type="-+">Segt, schoonste vande Wyven, <rhyme label="b" type="m">segt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wy sullen, sonder yet te <rhyme label="a" type="f" corresp="#s1:l1">vresen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Hem soecken, waer hy sit, of <rhyme label="b" type="m" corresp="#s1:l2">legt</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Na dien ghy't vraegt, ick sal 't u <rhyme label="a" type="f">seggen</rhyme>;</l>
        <l id="s2:l2" met="4" type="-+">Myn Lief is henen inden <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In zyn Gemeent', daer Bedden <rhyme label="a" type="f" corresp="#s2:l1">leggen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Van Speceri, en and're <rhyme label="b" type="m" corresp="#s2:l2">stoff'</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy is gaen Weyden inde <rhyme label="a" type="f">Hoven</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Daer alderhande Bloemen <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maer Bloemen, die sijn Name <rhyme label="a" type="f" corresp="#s3:l1">loven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En hier in trecken eenen <rhyme label="b" type="m" corresp="#s3:l2">Lijn</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Of, soo ghy 't nader wilt? hy <rhyme label="a" type="f">weydet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Aen gene kant, en aenden <rhyme label="b" type="m">Rhijn</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Der Lelien, daer hy <rhyme label="a" type="f" corresp="#s4:l1">vermeydet</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ick ben de Sijn', en hy de <rhyme label="b" type="m" corresp="#s4:l2">Mijn'</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Jerusalem met hare <rhyme label="a" type="f">Wallen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Sijn uytermaten <rhyme label="b" type="m">liefelijck</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En ghy ô Bruyt! ghy sijt in <rhyme label="a" type="f" corresp="#s5:l1">allen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Ierusalem oock seer <rhyme label="b" type="m" corresp="#s5:l2">gelijck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vriendin', ghy sijt ons in't <rhyme label="a" type="f">aenschouwen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Als Thirz', eer dat Samari <rhyme label="b" type="m">was</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">U Wapenen, sijn in <rhyme label="a" type="f" corresp="#s6:l1">benouwen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Wanneermen roept, Hela's! <rhyme label="b" type="m" corresp="#s6:l2">Hela's</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Soo schrickelijck, als de <rhyme label="a" type="f">Banieren</rhyme>;</l>
        <l id="s7:l2" met="4" type="-+">Van Hey-er-legers, als den <rhyme label="b" type="m">Trom</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Alarm, arm, slaet, na <rhyme label="a" type="f" corresp="#s7:l1">Krijgs-manieren</rhyme>;</l>
        <l id="s7:l4" met="4" type="-+">En de Trompette blaest, kom! <rhyme label="b" type="m" corresp="#s7:l2">kom</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ey! wilt u Oogen van my <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Se doen me kracht, se doen <rhyme label="b" type="m">gewelt</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Het schijnt ghy my wilt <rhyme label="a" type="f" corresp="#s8:l1">Over-heeren</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Dan, 't lustme soo te zyn <rhyme label="b" type="m" corresp="#s8:l2">gevelt</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">U Tanden, heb ick <rhyme label="a" type="f">vergeleken</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En Roode Wangen, en u <rhyme label="b" type="m">Haar</rhyme></l>
        <l id="s9:l3" met="4" type="-+">By Geyten, en by sulcke <rhyme label="a" type="f" corresp="#s9:l1">streken</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Alwaer men Weyt, de <rhyme label="b" type="m" corresp="#s9:l2">Wolle-Schaar</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ick meen die Schapen, die haer <rhyme label="a" type="f">Aasen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Rontom de Bergen <rhyme label="b" type="m">Gilead</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Met al die groene sijne <rhyme label="a" type="f" corresp="#s10:l1">Graasen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Heel wit bedouwt door 's Hemels <rhyme label="b" type="m" corresp="#s10:l2">nat</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Die Schapen, die ten allen <rhyme label="a" type="f">tyen</rhyme>;</l>
        <l id="s11:l2" met="4" type="-+">Den Huysman, en sijn <rhyme label="b" type="m">Kammeraet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Veel meer, als 't and're Vé, <rhyme label="a" type="f" corresp="#s11:l1">verblyen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Om dat hy daer soo wel mé <rhyme label="b" type="m" corresp="#s11:l2">slaet</rhyme>;</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Want, daer sijn ander Vé, en <rhyme label="a" type="f">Dieren</rhyme>;</l>
        <l id="s12:l2" met="4" type="-+">Soo nu soo dan, gaen sonder <rhyme label="b" type="m">Vrucht</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Daer ist dat dese soo wel <rhyme label="a" type="f" corresp="#s12:l1">tieren</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Dat over haer geen Huysman <rhyme label="b" type="m" corresp="#s12:l2">sucht</rhyme>;</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Want, vruchteloos sy noyt <rhyme label="a" type="f">vergaren</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Maer hebben doorgaens dees' of <rhyme label="b" type="m">geen'</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Ja, werpen alle-tijt met <rhyme label="a" type="f" corresp="#s13:l1">paren</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Dat is, sy geven twee voor <rhyme label="b" type="m" corresp="#s13:l2">een</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Schoon datmen 't sestich <rhyme label="a" type="f">Koninginnen</rhyme>;</l>
        <l id="s14:l2" met="4" type="-+">By-Wyven tachtich in <rhyme label="b" type="m">getal</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">En meerder Maegden kon <rhyme label="a" type="f" corresp="#s14:l1">versinnen</rhyme></l>
        <l id="s14:l4" met="4" type="-+">(Daer sijnder laes! veel over <rhyme label="b" type="m" corresp="#s14:l2">al</rhyme>.]</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Soo is mijn Kercke, myn <rhyme label="a" type="f">Vriendinne</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">En myn volmaeckte Duyve <rhyme label="b" type="m">Een'</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">(By mynen Geest vernieuwt van <rhyme label="a" type="f" corresp="#s15:l1">binnẽ</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Door 't waer' geloof,) en anders <rhyme label="b" type="m" corresp="#s15:l2">geen'</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Sy is by my een suyv're <rhyme label="a" type="f">Maget</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Die, als de Dochters sullen <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">(Om datse my soo seer <rhyme label="a" type="f" corresp="#s16:l1">behaget</rhyme>,)</l>
        <l id="s16:l4" met="4" type="-+">Dan seggen sullen met de <rhyme label="b" type="m" corresp="#s16:l2">Lien</rhyme>,</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Och! och! hoe Zalich is die <rhyme label="a" type="f">Dochter</rhyme>!</l>
        <l id="s17:l2" met="4" type="-+">Die soete, Liev', en schoone <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Haer Zeegens sijn gelijck een <rhyme label="a" type="f" corresp="#s17:l1">Lochter</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Wiens Licht breeckt allen kanten <rhyme label="b" type="m" corresp="#s17:l2">uyt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Wie is die Dochter, wiens <rhyme label="a" type="f">geblicker</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Is even als de Silv're <rhyme label="b" type="m">Maen</rhyme>?</l>
        <l id="s18:l3" met="4" type="-+">En welckers glans, en sterck <rhyme label="a" type="f" corresp="#s18:l1">geflicker</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">De Sterren doet haer licht <rhyme label="b" type="m" corresp="#s18:l2">vergaen</rhyme>?</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Wie sy? wiens Kleet is als de <rhyme label="a" type="f">Sonne</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">En schricklick als een machtich <rhyme label="b" type="m">Hëer</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Dat selfs, der Reusen Bloet doet <rhyme label="a" type="f" corresp="#s19:l1">ronnen</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">Wanneer men 't maer en siet van <rhyme label="b" type="m" corresp="#s19:l2">veer</rhyme>?</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Ick was gaen wand'len inde <rhyme label="a" type="f">Hoven</rhyme>,</l>
        <l id="s20:l2" met="4" type="-+">Alwaer de Note-boomen <rhyme label="b" type="m">staen</rhyme>:</l>
        <l id="s20:l3" met="4" type="-+">Daer sagh ick, nu beneen, dan <rhyme label="a" type="f" corresp="#s20:l1">boven</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">Hoe dat het met de Vrucht mocht <rhyme label="b" type="m" corresp="#s20:l2">gaen</rhyme>:</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Of oock den Stock des Wijns al <rhyme label="a" type="f">bloeyde</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">En, of den Appel van <rhyme label="b" type="m">Granaad</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">En, of de teere Telgen <rhyme label="a" type="f" corresp="#s21:l1">groeyden</rhyme>,</l>
        <l id="s21:l4" met="4" type="-+">En vruchten gaven vroegh en <rhyme label="b" type="m" corresp="#s21:l2">spaad'</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Dan, eer ick 't wist', soo was mijn <rhyme label="a" type="f">Ziele</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">In aller haest, soo seer <rhyme label="b" type="m">ontroert</rhyme></l>
        <l id="s22:l3" met="4" type="-+">Dat ick me vond, als door de <rhyme label="a" type="f" corresp="#s22:l1">Wielen</rhyme></l>
        <l id="s22:l4" met="4" type="-+">Van eenen Wagen, <rhyme label="b" type="m" corresp="#s22:l2">weg-gevoert</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">In aller Yl, in korten <rhyme label="a" type="f">stonden</rhyme>,</l>
        <l id="s23:l2" met="4" type="-+">Als of ick waer een vlugge <rhyme label="b" type="m">Wolck</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">Heb ick ter Post my doen <rhyme label="a" type="f" corresp="#s23:l1">gevonden</rhyme>,</l>
        <l id="s23:l4" met="4" type="-+">By mijn Vry-willich, danckbaer <rhyme label="b" type="m" corresp="#s23:l2">Volck</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Keert weder! Sulamith, keert <rhyme label="a" type="f">weder</rhyme>.</l>
        <l id="s24:l2" met="4" type="-+">(Dus riep ick wat ick roepen <rhyme label="b" type="m">mocht</rhyme>,)</l>
        <l id="s24:l3" met="4" type="-+">ô Vreden-Dochter! keert doch <rhyme label="a" type="f" corresp="#s24:l1">weder</rhyme>;</l>
        <l id="s24:l4" met="4" type="-+">Want, siet, ick u met tranen <rhyme label="b" type="m" corresp="#s24:l2">socht</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Keert weder! laet my sien u <rhyme label="a" type="f">schreyen</rhyme>,</l>
        <l id="s25:l2" met="4" type="-+">Dat soete nat, der Eng'len <rhyme label="b" type="m">Wijn</rhyme>;</l>
        <l id="s25:l3" met="4" type="-+">Soo sal ick u terstont <rhyme label="a" type="f" corresp="#s25:l1">geleyen</rhyme>,</l>
        <l id="s25:l4" met="4" type="-+">Alwaer ghy sult behouden <rhyme label="b" type="m" corresp="#s25:l2">sijn</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Ick sie, op Sulamith u <rhyme label="a" type="f">Oogen</rhyme>,</l>
        <l id="s26:l2" met="4" type="-+">En strack, en onbeweeglick <rhyme label="b" type="m">staen</rhyme>;</l>
        <l id="s26:l3" met="4" type="-+">Ick sal u seggen sonder <rhyme label="a" type="f" corresp="#s26:l1">loogen</rhyme>,</l>
        <l id="s26:l4" met="4" type="-+">Wie dat sy is, en henen <rhyme label="b" type="m" corresp="#s26:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Sy is, gelijck de twee <rhyme label="a" type="f">Banieren</rhyme>,</l>
        <l id="s27:l2" met="4" type="-+">Van Hey-er-legers, als den <rhyme label="b" type="m">Trom'</rhyme></l>
        <l id="s27:l3" met="4" type="-+">Alarm, arm, slaet, na <rhyme label="a" type="f" corresp="#s27:l1">Krijgs-manieren</rhyme>;</l>
        <l id="s27:l4" met="4" type="-+">En de Trompette blaest, kom! <rhyme label="b" type="m" corresp="#s27:l2">kom</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
