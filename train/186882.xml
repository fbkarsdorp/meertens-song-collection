<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186882</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Daar was laatst een meisje loos</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Scheveningen draag uw <rhyme label="a" type="m">roem</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Op uw wakkre <rhyme label="b" type="f">Bootsgesellen</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Scheveningen draag uw <rhyme label="a" type="m" corresp="#s1:l1">roem</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Op die, die 'k met eerbied <rhyme label="a" type="m" corresp="#s1:l1">noem</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Op Dykhuyzen, Pronk, en <rhyme label="c" type="m">Spaan</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Ieder moet met lof <rhyme label="b" type="f" corresp="#s1:l2">vertellen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Hoe Dykhuizen, Pronk en <rhyme label="c" type="m" corresp="#s1:l5">Spaan</rhyme></l>
        <l id="s1:l8" met="4" type="+-">'t Grootste Heldenstuk <rhyme label="c" type="m" corresp="#s1:l5">bestaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zy bekneld op 't Britsche <rhyme label="a" type="m">Strand</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Schaers voorzien van Brood en <rhyme label="b" type="f">Water</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Zy bekneld op 't Britsche <rhyme label="a" type="m" corresp="#s2:l1">Strand</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Steken vol van moed van <rhyme label="a" type="m" corresp="#s2:l1">kand</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">In een boot, slegt <rhyme label="c" type="m">toegerust</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Vreezende geen <rhyme label="b" type="f" corresp="#s2:l2">Zeegeklater</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">In een Boot, slegt <rhyme label="c" type="m" corresp="#s2:l5">toegerust</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Roeijen zy naer onze <rhyme label="c" type="m" corresp="#s2:l5">Kust</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vryheid die by Helden <rhyme label="a" type="m">woont</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sleept het Bootje langs de <rhyme label="b" type="f">baeren</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Vryheid, die by Helden <rhyme label="a" type="m" corresp="#s3:l1">woont</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Heeft hun moet met heil <rhyme label="a" type="m" corresp="#s3:l1">bekroond</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Zy verhaelen, bly te <rhyme label="c" type="m">moê</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Pas geland, hun <rhyme label="b" type="f" corresp="#s3:l2">doodsgevaeren</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Zy verhaelen, bly te <rhyme label="c" type="m" corresp="#s3:l5">moë</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Ieder hoort, en juicht hun <rhyme label="c" type="m" corresp="#s3:l5">toe</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Vrees, ô Brit! voor zulk een <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Daer m'om Vryheid t'al durf <rhyme label="b" type="f">wagen</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Vrees, ô Brit! voor zulk een <rhyme label="a" type="m" corresp="#s4:l1">Land</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Daar het vuur der Vryheid <rhyme label="a" type="m" corresp="#s4:l1">brandt</rhyme>!</l>
        <l id="s4:l5" met="4" type="+-">Hier is zelfs een kind een <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Komt ge hier, ge krygt wis <rhyme label="b" type="f" corresp="#s4:l2">slagen</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Hier is zelfs een kind een <rhyme label="c" type="m" corresp="#s4:l5">Man</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die vermeetlen kloppen <rhyme label="c" type="m" corresp="#s4:l5">kan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
