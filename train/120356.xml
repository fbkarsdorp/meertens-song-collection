  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Als Annette haar <rhyme label="a" type="f">Minneklachten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Laastmaal aan haar Moeder <rhyme label="b" type="m">dee</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Hoe zy met Colyn de <rhyme label="a" type="f" corresp="#s1:l1">nachten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Doorbrogt in de <rhyme label="b" type="m" corresp="#s1:l2">minnevree</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Dan kwam haar de Moeder <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Annette hoe hebje zoo’n <rhyme label="d" type="m">bestaan</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Jy bent in het bloeijen van u <rhyme label="c" type="f" corresp="#s1:l5">Jaaren</rhyme>,</l>
        <l id="s1:l8" met="5" type="+-">Kindje, kindje lief wat heb jy <rhyme label="d" type="m" corresp="#s1:l6">gedaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Moeder lief ik was wel te <rhyme label="a" type="f">vreeden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Als ik by myn Minnaar <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Hy wou myn zyn trouwtje <rhyme label="a" type="f" corresp="#s2:l1">geeven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En toen was ik wel te <rhyme label="b" type="m" corresp="#s2:l2">pas</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dan kwam haar de Moeder <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Annette heeft hy u niets <rhyme label="d" type="m">misdaan</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ik zal het wel zien in volgende <rhyme label="c" type="f" corresp="#s2:l5">dagen</rhyme>,</l>
        <l id="s2:l8" met="5" type="+-">Kindje, kindje lief, wat heb jy <rhyme label="d" type="m" corresp="#s2:l6">gedaan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Moeder lief al in zyn <rhyme label="a" type="f">armen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Rusten ik den gantsche <rhyme label="b" type="m">nacht</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hy kwam myn jonkhert <rhyme label="a" type="f" corresp="#s3:l1">verwarmen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ik voelde vreugde <rhyme label="b" type="m" corresp="#s3:l2">onverwacht</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dan kwam haar de Moeder <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Annette hoe is u de nacht <rhyme label="d" type="m">vergaan</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Ik zal het wel zien in volgende <rhyme label="c" type="f" corresp="#s3:l5">dagen</rhyme>,</l>
        <l id="s3:l8" met="5" type="+-">Kindje, kindje lief, al wat hy heeft <rhyme label="d" type="m" corresp="#s3:l6">gedaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Voor ’t lest Moeder laat ons <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Want onze herten zyn <rhyme label="b" type="m">verknogt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Hy kwam myn zyn min te <rhyme label="a" type="f" corresp="#s4:l1">geven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En toen heeft hy my iets <rhyme label="b" type="m" corresp="#s4:l2">verzogt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dan kwam haar de Moeder <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Annette heeft hy u lust <rhyme label="d" type="m">voldaan</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Leeft dan in vreede in deeze <rhyme label="c" type="f" corresp="#s4:l5">dagen</rhyme>,</l>
        <l id="s4:l8" met="5" type="+-">Kindje, kindje lief, ik heb het ook <rhyme label="d" type="m" corresp="#s4:l6">gedaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
