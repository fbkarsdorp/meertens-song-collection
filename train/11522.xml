    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Jacob Janszoon Colevelt, A. Pietersz. Craen, Matthijs van Velden, Jan Robbertsz</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">T Meest genoegen dat de <rhyme label="a" type="m">Mensch</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Heeft na <rhyme label="a" type="m" corresp="#s1:l1">wensch</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Is gelegen in de <rhyme label="b" type="f">ooghjes</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Die sich wribb'len heen en <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Op en <rhyme label="c" type="m" corresp="#s1:l4">neer</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Nu wat laeghjes, dan weer <rhyme label="b" type="f" corresp="#s1:l3">hooghjes</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sedert mijn scheem'rend' <rhyme label="a" type="m">gesicht</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">O mijn <rhyme label="a" type="m" corresp="#s2:l1">licht</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Over-moghen preutse <rhyme label="b" type="f">ziele</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Eerstmael u tintelen <rhyme label="c" type="m">sach</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Heb ick, <rhyme label="c" type="m" corresp="#s2:l4">ach</rhyme>!</l>
        <l id="s2:l6" met="4" type="+-">Voor u grootheyt moeten <rhyme label="b" type="f" corresp="#s2:l3">knielen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Minne, segh ick, Minne <rhyme label="a" type="m">wreet</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Die mijn <rhyme label="a" type="m" corresp="#s3:l1">leet</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Steets vergroot van dagh tot <rhyme label="b" type="f">dage</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Sal mijn over-swaere <rhyme label="c" type="m">druck</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">En <rhyme label="c" type="m" corresp="#s3:l4">on-luck</rhyme></l>
        <l id="s3:l6" met="4" type="+-">V ten laetsten niet <rhyme label="b" type="f" corresp="#s3:l3">mishagen</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sal u schoonheyt, brave <rhyme label="a" type="m">vrouw</rhyme>!</l>
        <l id="s4:l2" met="2" type="+-">Myne <rhyme label="a" type="m" corresp="#s4:l1">rouw</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Dan vergrooten <rhyme label="b" type="f">ongenadigh</rhyme>?</l>
        <l id="s4:l4" met="4" type="+-">Of is 't, blinckende <rhyme label="c" type="m">Goddin</rhyme>!</l>
        <l id="s4:l5" met="2" type="+-">Dan u <rhyme label="c" type="m" corresp="#s4:l4">sin</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Dat ick kerm en klaegh <rhyme label="b" type="f" corresp="#s4:l3">ghestadigh</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Ben gelijck 't gewonde <rhyme label="a" type="m">Hart</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Dat sijn <rhyme label="a" type="m" corresp="#s5:l1">smart</rhyme></l>
        <l id="s5:l3" met="4" type="+-">En sijn doot door eygen <rhyme label="b" type="f">wroeten</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Selfs verhaest met springen <rhyme label="c" type="m">wilt</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s5:l4">Ongestilt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Door des jagers snel <rhyme label="b" type="f" corresp="#s5:l3">ontmoeten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Soeck ick, laes! der <rhyme label="a" type="m">minne-brant</rhyme></l>
        <l id="s6:l2" met="2" type="+-">Aen d'een <rhyme label="a" type="m" corresp="#s6:l1">kant</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Uyt te dempen en te <rhyme label="b" type="f">decken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Soo sie ick vlammige <rhyme label="c" type="m">roock</rhyme></l>
        <l id="s6:l5" met="2" type="+-">En <rhyme label="c" type="m" corresp="#s6:l4">gesmoock</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Weer ter ander sy op <rhyme label="b" type="f" corresp="#s6:l3">trecken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Al mijn klagen was al <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Soo ghy <rhyme label="a" type="m" corresp="#s7:l1">liet</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Slechts een deeltje, 't welck my <rhyme label="b" type="f">quellet</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat's dat ghy u steets soo <rhyme label="c" type="m">dreuts</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Snar en <rhyme label="c" type="m" corresp="#s7:l4">preuts</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Inde plaets van lief-tal <rhyme label="b" type="f" corresp="#s7:l3">stellet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dan rechts als het onweer <rhyme label="a" type="m">suur</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Kout en <rhyme label="a" type="m" corresp="#s8:l1">stuur</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Sich vergaert tot mottich <rhyme label="b" type="f">Zuyen</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Even is der vrouwen <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Inde <rhyme label="c" type="m" corresp="#s8:l4">Min</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En verkeert als <rhyme label="b" type="f" corresp="#s8:l3">regen-buyen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
