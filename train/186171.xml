  <meta>
    <id>186171</id>
    <date>
      <year-start>1881</year-start>
      <year-end>1891</year-end>
    </date>
    <location>
      <orginal-name>arnhem-nijmegen</orginal-name>
      <standard-name>Nijmegen, The Netherlands</standard-name>
