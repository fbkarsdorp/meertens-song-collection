  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SAngheresjes leent u <rhyme label="a" type="f">keelen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Aen ons Sparens Bron en <rhyme label="b" type="m">vliet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">‘t Gheen van ‘t bracke Ty <rhyme label="b" type="m" corresp="#s1:l2">inschiet</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Wilt een Sparens Deuntje <rhyme label="a" type="f" corresp="#s1:l1">queelen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Schallickwijck en <rhyme label="c" type="m">Schoolenaer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">T’ Heemstee, op het endt van ‘t <rhyme label="c" type="m" corresp="#s1:l5">Spaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
