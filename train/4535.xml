  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DIanier, mijn tweede <rhyme label="a" type="m">Ziel</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Open toch u boesems <rhyme label="b" type="f">koffer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wijlick voor u neder <rhyme label="a" type="m" corresp="#s1:l1">kniel</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wijlick u als mijn <rhyme label="c" type="m">Godin</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Smeeck met een verliefde <rhyme label="c" type="m" corresp="#s1:l4">zin</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En een vloed van tranen <rhyme label="b" type="f" corresp="#s1:l2">offer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Door de wreetheyt van de <rhyme label="c" type="m" corresp="#s1:l4">min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sal dan al myn droef <rhyme label="a" type="m">gesucht</rhyme></l>
        <l id="s2:l2" met="4" type="+-">U noyt buygen tot <rhyme label="b" type="f">medoogen</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Sal ick staeg met <rhyme label="a" type="m" corresp="#s2:l1">ongenucht</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van de Liefde zyn <rhyme label="c" type="m">geknelt</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">Soo stuurt myn, ter neÃªr <rhyme label="c" type="m" corresp="#s2:l4">gevelt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Door den blixem uwer <rhyme label="b" type="f" corresp="#s2:l2">oogen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Liever strax nae 't Minnaers <rhyme label="c" type="m" corresp="#s2:l4">velt</rhyme>.</l>
      </lg>
    </lg>
