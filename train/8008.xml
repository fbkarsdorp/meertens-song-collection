    <melody>Psalm 118 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SInght den Heer inden gantschen <rhyme label="a" type="f">lande</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Met ghesangh looft nu sijnen <rhyme label="b" type="m">naem</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Prijst hem met mondt en met <rhyme label="a" type="f" corresp="#s1:l1">verstande</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Roemt sijn goetheyt alle te <rhyme label="b" type="m" corresp="#s1:l2">saem</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Spreeckt hoe wonderlijck zijt ghy <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">In al u wercken groot en <rhyme label="d" type="m">cleyn</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Uwe vyanden beschaemt <rhyme label="c" type="f" corresp="#s1:l5">seere</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Bidden om vreed' alle <rhyme label="d" type="m" corresp="#s1:l6">ghemeyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat u dan, o mijn Godt <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">De werelt roeme met <rhyme label="b" type="m">ootmoet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">V lof moet oock gesonghen <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Alsins met stemmen claer en <rhyme label="b" type="m" corresp="#s2:l2">soet</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Comt hier en wilt doch wel <rhyme label="c" type="f">aenmercken</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">De daden Gods des Heeren <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Hoe wonderlijck dat oock sijn <rhyme label="c" type="f" corresp="#s2:l5">wercken</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">By der menschen kinderen <rhyme label="d" type="m" corresp="#s2:l6">sijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy verdrooght dat groote Meyr <rhyme label="a" type="f">crachtigh</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Datmen drooghs voets can gaen daer <rhyme label="b" type="m">door</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Dies wy sijn volck in liefd' <rhyme label="a" type="f" corresp="#s3:l1">eendrachtigh</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Hem seer vrolijck dancken daer <rhyme label="b" type="m" corresp="#s3:l2">voor</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Sijn heerschappy sal eeuwigh <rhyme label="c" type="f">blijven</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Sijn ooghe de volcken <rhyme label="d" type="m">aensiet</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Wie van hem wijckt sal niet <rhyme label="c" type="f" corresp="#s3:l5">beclijven</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Maer vernedert werden tot <rhyme label="d" type="m" corresp="#s3:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy volckeren wilt u <rhyme label="a" type="f">begheven</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Om God te prijsen boven <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat sijn naem seer hoogh zy <rhyme label="a" type="f" corresp="#s4:l1">verheven</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Van allen in dit Aertsche <rhyme label="b" type="m" corresp="#s4:l2">dal</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Hy ist die ons bewaert ons <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Die voor ons sorght tot ons <rhyme label="d" type="m">behoet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Op dat wy niet vallen noch <rhyme label="c" type="f" corresp="#s4:l5">beven</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Ia dat niet slibbert onsen <rhyme label="d" type="m" corresp="#s4:l6">voet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy hebt ons doorsocht, Heer <rhyme label="a" type="f">ghenadigh</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ende beproeft alle <rhyme label="b" type="m">ghelijck</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Alsoomen door dat vyer <rhyme label="a" type="f" corresp="#s5:l1">ghestadigh</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dat silver loutert van den <rhyme label="b" type="m" corresp="#s5:l2">slijck</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Ghy hebt ons van den <rhyme label="c" type="f">onbekenden</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Vyanden laten zijn <rhyme label="d" type="m">gevaen</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">End' hebt ons, Heer, op onsen <rhyme label="c" type="f" corresp="#s5:l5">lenden</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Met een groot last swaerlijck <rhyme label="d" type="m" corresp="#s5:l6">belaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Men heeft op ons hoofden <rhyme label="a" type="f">gheklommen</rhyme>.</l>
        <l id="s6:l2" met="4" type="-+">Soo men beklimt een Kemel <rhyme label="b" type="m">dier</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Als beesten werden wy <rhyme label="a" type="f" corresp="#s6:l1">alommen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ghedreven door water end' <rhyme label="b" type="m" corresp="#s6:l2">vyer</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Daer na hebt ghy ons, Heere, <rhyme label="c" type="f">goedigh</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Vertroost, dies ick tot u huys <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Wil brenghen mij offranden <rhyme label="c" type="f" corresp="#s6:l5">bloedigh</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">End' mijn beloften groot en <rhyme label="d" type="m" corresp="#s6:l6">kleyn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Mijn beloften sal ick <rhyme label="a" type="f">betalen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die mijn lippen hebben <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die in den noot end' in mijn <rhyme label="a" type="f" corresp="#s7:l1">qualen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Wt mijnen mondt zijn <rhyme label="b" type="m" corresp="#s7:l2">gegaen</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Ick wil u Heer, veel vett' <rhyme label="c" type="f">off'randen</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Der rammen op uwen <rhyme label="d" type="m">Altaer</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">End' der bocken met vyer <rhyme label="c" type="f" corresp="#s7:l5">verbranden</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Daer toe oock veel runderen <rhyme label="d" type="m" corresp="#s7:l6">swaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy al die Godt vreest weest doch <rhyme label="a" type="f">stille</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Comt tot my, hoort en wilt <rhyme label="b" type="m">verstaen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Want te verhalen heb ick <rhyme label="a" type="f" corresp="#s8:l1">wille</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Dat goedt dat my Godt heeft <rhyme label="b" type="m" corresp="#s8:l2">gedaen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Als ick hem heb gebeden <rhyme label="c" type="f">krachtigh</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Hy heeft my haest verhoort <rhyme label="d" type="m">voorwaer</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Dies heeft mijn tongh' oorsaeck <rhyme label="c" type="f" corresp="#s8:l5">waerachtigh</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Hem te loven vry <rhyme label="d" type="m" corresp="#s8:l6">openbaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Waert dat ick hadd' ghenomen <rhyme label="a" type="f">voren</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">In mijn ghemoet eenigh <rhyme label="b" type="m">onrecht</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Soo hadde Godt niet willen <rhyme label="a" type="f" corresp="#s9:l1">hooren</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Dat ghebedt van my sijnen <rhyme label="b" type="m" corresp="#s9:l2">knecht</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Maer ick magh met rechte wel <rhyme label="c" type="f">spreken</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Dat my Godt altijdt verhoort <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Mijn smeecken heeft hy noyt <rhyme label="c" type="f" corresp="#s9:l5">versteken</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Soo langh' als ick hebbe <rhyme label="d" type="m" corresp="#s9:l6">gheleeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
