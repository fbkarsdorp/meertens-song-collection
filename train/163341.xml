<song>
  <meta>
    <id>163341</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Baise-moi ma Jeanneton</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WAerom soete <rhyme label="a" type="m">Velt-godin</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Moet ick eensaem <rhyme label="b" type="f">dwalen</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">En geduerigh in de <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Hopeloos gaen <rhyme label="b" type="f" corresp="#s1:l2">malen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Waerom treft my <rhyme label="c" type="m">tegenspoet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Meerder als een ander <rhyme label="c" type="m" corresp="#s1:l5">doet</rhyme>?</l>
        <l id="s1:l7" met="3" type="+-">Dat ick in het <rhyme label="d" type="f">vryen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Niet en kan <rhyme label="d" type="f" corresp="#s1:l7">bedyen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sal ick nimmermeer de <rhyme label="a" type="m">naem</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Van een Bruygom <rhyme label="b" type="f">dragen</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Ben ick dan soo <rhyme label="a" type="m" corresp="#s2:l1">onbequaem</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">In mijn jonge <rhyme label="b" type="f" corresp="#s2:l2">dagen</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">Soete Meysje tot <rhyme label="c" type="m">besluyt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">'k Wenste dat ghy waert de <rhyme label="c" type="m" corresp="#s2:l5">Bruyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">En ick neffens <rhyme label="d" type="f">desen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Mocht de Bruygom <rhyme label="d" type="f" corresp="#s2:l7">wesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dat ghy niet bedyen <rhyme label="a" type="m">kunt</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Vryer, dit 's de <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat ghy soeckt u eygen <rhyme label="a" type="m" corresp="#s3:l1">munt</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Hooger te <rhyme label="b" type="f" corresp="#s3:l2">besteden</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Aen een Maeght als 't waerdigh <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Daerom slaet ghy altijt <rhyme label="c" type="m" corresp="#s3:l5">mis</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Ende kunt in 't <rhyme label="d" type="f">minnen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Niemant <rhyme label="d" type="f" corresp="#s3:l7">overwinnen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Neen Godin, dit raedje <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Stelt dit vry ter <rhyme label="b" type="f">syen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Menigh Vryster als sy <rhyme label="a" type="m" corresp="#s4:l1">siet</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Dat men haer wil <rhyme label="b" type="f" corresp="#s4:l2">vryen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Toont haer nors en al te <rhyme label="c" type="m">stuer</rhyme></l>
        <l id="s4:l6" met="4" type="+-">En siet altijt even <rhyme label="c" type="m" corresp="#s4:l5">suer</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Sulcke vijse <rhyme label="d" type="f">streken</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Doet de Liefde <rhyme label="d" type="f" corresp="#s4:l7">breken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Schort de Vryers lieve <rhyme label="a" type="m">vrient</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Aen haer eygen <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">'t Wort met veynsery <rhyme label="a" type="m" corresp="#s5:l1">gemient</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Vals zijn zy in 't <rhyme label="b" type="f" corresp="#s5:l2">minnen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Yder Maget heeft een <rhyme label="c" type="m">dat</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Wie het is daer scheelt oock <rhyme label="c" type="m" corresp="#s5:l5">wat</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Die haer stout <rhyme label="d" type="f">vermeten</rhyme></l>
        <l id="s5:l8" met="3" type="+-">'t Geen zy niet en <rhyme label="d" type="f" corresp="#s5:l7">weten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Valsheyt schuylter by een <rhyme label="a" type="m">Maegt</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Dit kan klaerder <rhyme label="b" type="f">blijcken</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Schoon of haer de min <rhyme label="a" type="m" corresp="#s6:l1">behaeght</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Noch gaet zy <rhyme label="b" type="f" corresp="#s6:l2">beswijcken</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Al 't gepronck en al 't <rhyme label="c" type="m">cieraet</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Dat getuyght van d' <rhyme label="c" type="m" corresp="#s6:l5">Echte-staet</rhyme>;</l>
        <l id="s6:l7" met="3" type="+-">Noch kan sy haer <rhyme label="d" type="f">houwen</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Of sy noyt wil <rhyme label="d" type="f" corresp="#s6:l7">trouwen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Vryer in het kort <rhyme label="a" type="m">geseyt</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Wilt u wel <rhyme label="b" type="f">versinnen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Soeckt een schoon en rijcke <rhyme label="a" type="m" corresp="#s7:l1">Meyt</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">En wilt die <rhyme label="b" type="f" corresp="#s7:l2">beminnen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">'t Is doch al de Vryers <rhyme label="c" type="m">aert</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Om met meerder zijn <rhyme label="c" type="m" corresp="#s7:l5">gepaert</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Gelt en goet te <rhyme label="d" type="f">winnen</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Dat 's de brugh van <rhyme label="d" type="f" corresp="#s7:l7">minnen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Zijn de Vryers daer aen <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Wel mijn <rhyme label="b" type="f">Herderinne</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Dit oock op de Vrijsters <rhyme label="a" type="m" corresp="#s8:l1">past</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Wie kan <rhyme label="b" type="f" corresp="#s8:l2">overwinnen</rhyme>?</l>
        <l id="s8:l5" met="4" type="+-">Heeft de Vryster meerder <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Wech soo is het met Ian <rhyme label="c" type="m" corresp="#s8:l5">Soet</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Om de ronde <rhyme label="d" type="f">schijven</rhyme></l>
        <l id="s8:l8" met="3" type="+-">Kan hy niet <rhyme label="d" type="f" corresp="#s8:l7">bedrijven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">'t Is voor my de beste <rhyme label="a" type="m">raedt</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Noch een reys te <rhyme label="b" type="f">wagen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Met een Vryster na mijn <rhyme label="a" type="m" corresp="#s9:l1">Staet</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">'t Staet aen ons te <rhyme label="b" type="f" corresp="#s9:l2">vragen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Is den eenen Iuffer <rhyme label="c" type="m">traegh</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">d' Ander die is weder <rhyme label="c" type="m" corresp="#s9:l5">graegh</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">'k Hoop ick sal na <rhyme label="d" type="f">desen</rhyme></l>
        <l id="s9:l8" met="3" type="+-">Haest de Bruygom <rhyme label="d" type="f" corresp="#s9:l7">wesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
