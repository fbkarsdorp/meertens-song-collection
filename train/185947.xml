    <melody>Ik weet wel wat gij zoekt</melody>
  <author>Hermanus van den Burg</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IK weet wel wat gy <rhyme label="a" type="m">zoekt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">maar 'k zal het geenszins <rhyme label="b" type="f">waagen</rhyme>,</l>
        <l id="s1:l3" met="1" type="+-"><rhyme label="c" type="m">geenszins</rhyme>,</l>
        <l id="s1:l4" met="1" type="+-"><rhyme label="c" type="m" corresp="#s1:l3">geenszins</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">geenszins <rhyme label="b" type="f" corresp="#s1:l2">waagen</rhyme>:</l>
        <l id="s1:l6" met="4" type="-+">Uw' loosheit zouw ik my <rhyme label="d" type="m">gewis</rhyme></l>
        <l id="s1:l7" met="3" type="-+">in 't kort <rhyme label="e" type="f">beklaagen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">gewis, gewis, gewis, <rhyme label="d" type="m" corresp="#s1:l6">gewis</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">in 't kort gewis <rhyme label="e" type="f" corresp="#s1:l7">beklaagen</rhyme>.</l>
      </lg>
    </lg>
