<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188341</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La marine</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GY eed'le jonge <rhyme label="a" type="f">Geeste</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die hier te saamen <rhyme label="b" type="m">zyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Om op dit heerlyk <rhyme label="a" type="f" corresp="#s1:l1">Feeste</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Te zyn van herten <rhyme label="b" type="m" corresp="#s1:l2">blydt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">En om dees zoete <rhyme label="c" type="m">vreugd</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">In alle eer, en <rhyme label="c" type="m" corresp="#s1:l5">deugd</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Te vemeeren, ‘En te <rhyme label="d" type="f">eeren</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">In uw <rhyme label="c" type="m" corresp="#s1:l5">jeugd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gy kond U hier <rhyme label="a" type="f">vermaaken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met aangenaame <rhyme label="b" type="m">Wyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om aan de gang te <rhyme label="a" type="f" corresp="#s2:l1">raaken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Het is een <rhyme label="b" type="m" corresp="#s2:l2">Medicyn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Die ons het hert <rhyme label="c" type="m">bereyd</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tot alle <rhyme label="c" type="m" corresp="#s2:l5">vrolykheyd</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">En de Zinnen, ‘Sterkt van <rhyme label="d" type="f">binnen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Met <rhyme label="c" type="m" corresp="#s2:l5">bescheyd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gy jonge frisse <rhyme label="a" type="f">Helden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Neemt nu uw zaken <rhyme label="b" type="m">waar</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Wy komen zeker <rhyme label="a" type="f" corresp="#s3:l1">zelden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zo lustig by <rhyme label="b" type="m" corresp="#s3:l2">malkaar</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Neemt nu de tydt in <rhyme label="c" type="m">agt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Om 't Jufferlyk <rhyme label="c" type="m" corresp="#s3:l5">geslagt</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Wel te voegen, ‘Met <rhyme label="d" type="f">vernoegen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Zoet en <rhyme label="c" type="m" corresp="#s3:l5">zagt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Betoond nu met <rhyme label="a" type="f">manieren</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">U wetenschap en <rhyme label="b" type="m">konst</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Om by die zoete <rhyme label="a" type="f" corresp="#s4:l1">dieren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Te raken in de <rhyme label="b" type="m" corresp="#s4:l2">gonst</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Dat zal U wonder <rhyme label="c" type="m">zeer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Verstrekken tot uw <rhyme label="c" type="m" corresp="#s4:l5">eer</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">En haar Zinnen, ‘Zult gy <rhyme label="d" type="f">winnen</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Meer, en <rhyme label="c" type="m" corresp="#s4:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy Juffers <rhyme label="a" type="f">uytgelesen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">In suyv're eer en <rhyme label="b" type="m">deugd</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die met uw minn'lyk <rhyme label="a" type="f" corresp="#s5:l1">wezen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">In uwe zoete <rhyme label="b" type="m" corresp="#s5:l2">jeugd</rhyme></l>
        <l id="s5:l5" met="3" type="-+">De herten <rhyme label="c" type="m">overwind</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En tot uw Liefde <rhyme label="c" type="m" corresp="#s5:l5">bind</rhyme>;</l>
        <l id="s5:l7" met="4" type="+-">Door't vermoogen, ‘Van uw <rhyme label="d" type="f">Oogen</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Zeer <rhyme label="c" type="m" corresp="#s5:l5">geswind</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Weest dog niet al te <rhyme label="a" type="f">moedig</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Maar draagt U zeer <rhyme label="b" type="m">geschikt</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Op dat gy zoo <rhyme label="a" type="f" corresp="#s6:l1">voorspoedig</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">De Jongmans 't hert <rhyme label="b" type="m" corresp="#s6:l2">verquikt</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Zy zullen met <rhyme label="c" type="m">bescheyd</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Tot trouwe <rhyme label="c" type="m" corresp="#s6:l5">dankbaarheyd</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Om uw eere, ,, <rhyme label="d" type="f">Tevermeere</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">Zyn <rhyme label="c" type="m" corresp="#s6:l5">bereyd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Voor't leste, eer wy <rhyme label="a" type="f">scheyden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Daar moet een Glaasjen <rhyme label="b" type="m">Wyn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Tot welvaart van die <rhyme label="a" type="f" corresp="#s7:l1">beyden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Der nieuw-getrouwde <rhyme label="b" type="m" corresp="#s7:l2">zyn</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">En dan nog eens <rhyme label="c" type="m">bedagt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Het Jufferlyk <rhyme label="c" type="m" corresp="#s7:l5">geslagt</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Datze leven, ,, Vry van <rhyme label="d" type="f">sneven</rhyme>,</l>
        <l id="s7:l8" met="2" type="+-">Dag en <rhyme label="c" type="m" corresp="#s7:l5">Nagt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
