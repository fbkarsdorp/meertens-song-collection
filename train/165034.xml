<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>165034</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatst als de nachtgodin Diana</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SOo langh ick, soete <rhyme label="a" type="m">Rosenmondt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">U Lief was, en my in uw Hertje <rhyme label="a" type="m" corresp="#s1:l1">vondt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En u geen Minnaer sachter <rhyme label="b" type="f">streelde</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Heb ick een Coninghs <rhyme label="c" type="m">macht</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En al sijn groote <rhyme label="b" type="f" corresp="#s1:l3">weelde</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">By de mijn <rhyme label="c" type="m" corresp="#s1:l4">veracht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Soo langh als Rosemont, u <rhyme label="a" type="m">Lief</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Uw hoop, uw troost was, en uw ziels <rhyme label="a" type="m" corresp="#s2:l1">gerief</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">En Lydia haer poesl'e <rhyme label="b" type="f">vleysje</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van u geen gunst en <rhyme label="c" type="m">hadt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Scheen ick het soetste <rhyme label="b" type="f" corresp="#s2:l3">Meysje</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Van de gantsche <rhyme label="c" type="m" corresp="#s2:l4">Stadt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn lust, mijn rust, vermaeck en <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Is Lydia, mijn aerdtsche <rhyme label="a" type="m" corresp="#s3:l1">Coningin</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Voor wien ick willigh soude <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Soo sy maer door mijn <rhyme label="c" type="m">doodt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Kond' dubbelt leven <rhyme label="b" type="f" corresp="#s3:l3">erven</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Ick my haest <rhyme label="c" type="m" corresp="#s3:l4">aen-boodt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick brandt van we'er-min, en mijn <rhyme label="a" type="m">lust</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Is Coridon, waer op mijn hertje <rhyme label="a" type="m" corresp="#s4:l1">rust</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Voor wien ick willigh wilde <rhyme label="b" type="f">baren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met Charon <rhyme label="c" type="m">onbedocht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Indien hy voor mijn <rhyme label="b" type="f" corresp="#s4:l3">Iaeren</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Duysent leven <rhyme label="c" type="m" corresp="#s4:l4">mocht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wel, of die oude <rhyme label="a" type="m">Venus-brandt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ons weder bondt met een Metale <rhyme label="a" type="m" corresp="#s5:l1">Bandt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En nieuwe voncken in ons <rhyme label="b" type="f">blaeckte</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En dat voor <rhyme label="c" type="m">Lydia</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Mijn Rosemondt <rhyme label="b" type="f" corresp="#s5:l3">geraeckte</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Weêr in mijn <rhyme label="c" type="m" corresp="#s5:l4">genâ</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Al ist dat mijn lief <rhyme label="a" type="m">Coridon</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Is schoon, en dat sijn hayr blinckt als de <rhyme label="a" type="m" corresp="#s6:l1">zon</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En ghy veel woester als de <rhyme label="b" type="f">baren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En lichter als de <rhyme label="c" type="m">wint</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Noch wil ick met u <rhyme label="b" type="f" corresp="#s6:l3">paeren</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Als ghy my weêr <rhyme label="c" type="m" corresp="#s6:l4">mint</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
