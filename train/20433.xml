  <author>B. Targier, Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Jongh-mans <rhyme label="a" type="m">hedens-daeghs</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Leeren wat Hoofs of <rhyme label="a" type="m" corresp="#s1:l1">Haeghs</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En Franse <rhyme label="b" type="f">Complementen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Te Leyen gaet het <rhyme label="c" type="m">groff</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Daer studeren de <rhyme label="b" type="f" corresp="#s1:l3">Studenten</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Niet als in dese <rhyme label="c" type="m" corresp="#s1:l4">stof</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Met alle Boon <rhyme label="a" type="m">precijs</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Krijghen zy van <rhyme label="a" type="m" corresp="#s2:l1">Parijs</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">De nieuwe <rhyme label="b" type="f">Complementen</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Ie suis vostre <rhyme label="c" type="m">Serviteur</rhyme></l>
        <l id="s2:l5" met="4" type="+-">'t Is te oudt voor dese <rhyme label="b" type="f" corresp="#s2:l3">venten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Sy hebben wat anders <rhyme label="c" type="m" corresp="#s2:l4">veur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ie suis vostre <rhyme label="a" type="m">valet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Geen ghebruyck meer <rhyme label="a" type="m" corresp="#s3:l1">het</rhyme></l>
        <l id="s3:l3" met="3" type="-+">By dese <rhyme label="b" type="f">Cortisanen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Alst op een spreecken <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Krayen sy als Fransche <rhyme label="b" type="f" corresp="#s3:l3">hanen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Meest by die 't minst <rhyme label="c" type="m" corresp="#s3:l4">verstaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Het nieuwste <rhyme label="a" type="m">Complement</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Daer men mee komt <rhyme label="a" type="m" corresp="#s4:l1">gerent</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Is dit je vous <rhyme label="b" type="f">presente</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Le bonsoir ou <rhyme label="c" type="m">bonjour</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Ou ce que vous <rhyme label="b" type="f" corresp="#s4:l3">contente</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Mon Caeur &amp; mon <rhyme label="c" type="m" corresp="#s4:l4">amour</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sy zijn op goeden <rhyme label="a" type="m">voet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dit Complemente <rhyme label="a" type="m" corresp="#s5:l1">gebroet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Voor Leugenaers uyt <rhyme label="b" type="f">luyen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Complementeur <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Is in duyts te <rhyme label="b" type="f" corresp="#s5:l3">beduyen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Volkomen <rhyme label="c" type="m" corresp="#s5:l4">Leugenaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
