<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161081</id>
    <date>
      <year-start>1730</year-start>
      <year-end>1730</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nerea schoonste van uw geburen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">D. NErea schoonste van u <rhyme label="a" type="f">gebueren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die woondt in 't woudt, of daer <rhyme label="b" type="m">omtrent</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Hoe langh sal noch uw' wreetheyd <rhyme label="a" type="f" corresp="#s1:l1">dueren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Hoe lang sal dueren mijn <rhyme label="b" type="m" corresp="#s1:l2">ellent</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daphile sielt vry uyt u <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Den heeten brandt die in u <rhyme label="b" type="m">woont</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Hoe komt gy soo een Vrouw <rhyme label="a" type="f" corresp="#s2:l1">beminnen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die u met ondanckbaerheyt <rhyme label="b" type="m" corresp="#s2:l2">loont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ik  hoop u hert nog eens sal <rhyme label="a" type="f">buygen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Om mijn mismaeckte wangen <rhyme label="b" type="m">bloot</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">U selven sult gy <rhyme label="a" type="f" corresp="#s3:l1">overtuygen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat gy zijt d'oorsaeck van mijn <rhyme label="b" type="m" corresp="#s3:l2">doodt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Neen, al sag ick u alsoo <rhyme label="a" type="f">sterven</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Een enckel kusjen voor uw' <rhyme label="b" type="m">mond</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En soud gy naulijcks van my <rhyme label="a" type="f" corresp="#s4:l1">verwerven</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Al kond ick u maken <rhyme label="b" type="m" corresp="#s4:l2">gesond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">So heb ick dan mijn trouwe <rhyme label="a" type="f">minne</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Op een quaed fondament <rhyme label="b" type="m">geleyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Nochtans so sal ick u <rhyme label="a" type="f" corresp="#s5:l1">beminne</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Spijt u en die 't sal wesen <rhyme label="b" type="m" corresp="#s5:l2">leyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wel wilt ghy dan mijn minnaer <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">En vraeg u niet na mijn <rhyme label="b" type="m">begeer</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Ick heb een ander <rhyme label="a" type="f" corresp="#s6:l1">uytgelesen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Die my bemint en ick hem <rhyme label="b" type="m" corresp="#s6:l2">weer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nochtans sal ick u Lief niet <rhyme label="a" type="f">laten</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Al soud gy eeuwig van my <rhyme label="b" type="m">vlien</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Gy kont my by u selfs wel <rhyme label="a" type="f" corresp="#s7:l1">haten</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Maer 't minnen kont gy my niet <rhyme label="b" type="m" corresp="#s7:l2">verbien</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Besadigt wat u woeste <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Een weynig doch ter neder <rhyme label="b" type="m">slaet</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Vermindert wat u herte <rhyme label="a" type="f" corresp="#s8:l1">minne</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ick sal verminderen mijnen <rhyme label="b" type="m" corresp="#s8:l2">haet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ick sal u even gedienstig <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Al is 't dat gy my loot met <rhyme label="b" type="m">quaed</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Van waer komt doch den haet <rhyme label="a" type="f" corresp="#s9:l1">geresen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Die u soo diep <rhyme label="b" type="m" corresp="#s9:l2">slaet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Den haet die ik dus droeg van <rhyme label="a" type="f">binnen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Daerom dat gy doet dese <rhyme label="b" type="m">klacht</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Die spruyt alleen uyt uwe <rhyme label="a" type="f" corresp="#s10:l1">minne</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Die gy te veel loon waerdig <rhyme label="b" type="m" corresp="#s10:l2">acht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wy hebben hier genoeg staen <rhyme label="a" type="f">praten</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Ons kans mocht haest nemen een <rhyme label="b" type="m">keer</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">Siet of gy my soudt konnen <rhyme label="a" type="f" corresp="#s11:l1">haten</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Moog'lijck soudt ick u minnen <rhyme label="b" type="m" corresp="#s11:l2">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
