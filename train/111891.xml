<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>111891</id>
    <date>
      <year-start>1745</year-start>
      <year-end>1745</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kinderlied" translation="children's song">kinderlied</tag>
    </tags>
    <melody>Ik voer al over zee</melody>
    <categories>
      <category translation="children">kinderen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL over langen <rhyme label="a" type="m">tyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Heeft Tryn die bolle <rhyme label="a" type="m" corresp="#s1:l1">Meyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Myn gezeyt, Myn <rhyme label="a" type="m" corresp="#s1:l1">gezeyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat ik haer Hart alleen <rhyme label="b" type="m">bezat</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m" corresp="#s1:l5">bien</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Maer Tryn die fopt my <rhyme label="b" type="m" corresp="#s1:l4">wat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Is al maer valse <rhyme label="a" type="m">praet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Want gister Avond <rhyme label="a" type="m" corresp="#s2:l1">laet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Op de straet, Op de <rhyme label="a" type="m" corresp="#s2:l1">straet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vond ik haer Juist by Krelis <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m" corresp="#s2:l5">bien</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Daer ging het Spel toen <rhyme label="b" type="m" corresp="#s2:l4">aen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gants bloed ik wierd zoo <rhyme label="a" type="m">vals</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hy greep haer om haer <rhyme label="a" type="m" corresp="#s3:l1">hals</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Blank en mals, Blank en <rhyme label="a" type="m" corresp="#s3:l1">mals</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Zy scheen begut op hem zoo <rhyme label="b" type="m">Groen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m" corresp="#s3:l5">bien</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En hy gafze Zoen op <rhyme label="b" type="m" corresp="#s3:l4">Zoen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">My dogt dat was niet <rhyme label="a" type="m">ryn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik geef den bras van <rhyme label="a" type="m" corresp="#s4:l1">Tryn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'t Vuyle Zwyn, 't Vuyle <rhyme label="a" type="m" corresp="#s4:l1">Zwyn</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Wyl zy haer Min om gelt <rhyme label="b" type="m">Verhuurt</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m" corresp="#s4:l5">bien</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat had ik mooy <rhyme label="b" type="m" corresp="#s4:l4">begluurt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu ben ik weer eens <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van Minne <rhyme label="a" type="m" corresp="#s5:l1">Slaverny</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">In 't gevrey, In 't <rhyme label="a" type="m" corresp="#s5:l1">gevrey</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Is 't altyt weer van voor af <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m" corresp="#s5:l5">bien</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Men heeft daer nooyt <rhyme label="b" type="m" corresp="#s5:l4">gedaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De Seyne, en den <rhyme label="a" type="m">Ryn</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Verschaft my <rhyme label="a" type="m" corresp="#s6:l1">Medicyn</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Voor myn pyn, Voor myn <rhyme label="a" type="m" corresp="#s6:l1">pyn</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ik rook met rust een Pyp <rhyme label="b" type="m">Tabak</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Vous m'entendez <rhyme label="c" type="m" corresp="#s6:l5">bien</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dan vind ik meer <rhyme label="b" type="m" corresp="#s6:l4">gemak</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
