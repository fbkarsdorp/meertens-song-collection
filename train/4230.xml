<song>
  <meta>
    <id>4230</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 079 Datheen</melody>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HElpt Heer ons <rhyme label="a" type="m">Voogd</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En 's Lands <rhyme label="b" type="f">trouwe-Behoeder</rhyme>:</l>
        <l id="s1:l3" met="2" type="-+">Die vutig <rhyme label="a" type="m" corresp="#s1:l1">poogd</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Te volgen Vader, <rhyme label="b" type="f" corresp="#s1:l2">Bröeder</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">ô! God <rhyme label="a" type="m" corresp="#s1:l1">gedoogd</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat hy verstrek een <rhyme label="b" type="f" corresp="#s1:l2">Moeder</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Die ons <rhyme label="a" type="m" corresp="#s1:l1">be-oogd</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En geeft ons <rhyme label="b" type="f" corresp="#s1:l2">vryheyts-voeder</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Helpt Heer dien vromen <rhyme label="c" type="m">Held</rhyme>:</l>
        <l id="s1:l10" met="3" type="-+">Verstoord het <rhyme label="c" type="m" corresp="#s1:l9">Spaarts-geweld</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Door u <rhyme label="d" type="f">Goddelijk-wesen</rhyme>:</l>
        <l id="s1:l12" met="3" type="-+">U Vader bidden <rhyme label="e" type="m">wy</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Helpt Heer ons Prins so <rhyme label="e" type="m" corresp="#s1:l12">gy</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Hielpt Iosua voor <rhyme label="d" type="f" corresp="#s1:l11">desen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">U anschijns <rhyme label="a" type="m">blik</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En Weer-licht, zy sijn <rhyme label="b" type="f">Sabel</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">U wraak <rhyme label="a" type="m" corresp="#s2:l1">verschrik</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En kneus het <rhyme label="b" type="f" corresp="#s2:l2">twede-Babel</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Beneveld, <rhyme label="a" type="m" corresp="#s2:l1">dik</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met ménig <rhyme label="b" type="f" corresp="#s2:l2">valsche-fabel</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Breekt Heer de <rhyme label="a" type="m" corresp="#s2:l1">strik</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Haar's sterke-streng, en <rhyme label="b" type="f" corresp="#s2:l2">Kabel</rhyme>:</l>
        <l id="s2:l9" met="3" type="-+">Want buyten u is <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Daar ons hulp van <rhyme label="c" type="m" corresp="#s2:l9">geschied</rhyme>:</l>
        <l id="s2:l11" met="3" type="+-">Gy zijt alleen <rhyme label="d" type="f">gepresen</rhyme>.</l>
        <l id="s2:l12" met="3" type="-+">Daarom staat ons doch <rhyme label="e" type="m">by</rhyme>.</l>
        <l id="s2:l13" met="3" type="-+">Helpt Heer ons Prins so <rhyme label="e" type="m" corresp="#s2:l12">gy</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Hielpt Iosua, voor <rhyme label="d" type="f" corresp="#s2:l11">desen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Geeft dat hy 't <rhyme label="a" type="m">wit</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sijn's yver mach <rhyme label="b" type="f">bejagen</rhyme>:</l>
        <l id="s3:l3" met="2" type="-+">Die Heerlijk <rhyme label="a" type="m" corresp="#s3:l1">sit</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Op sijnen <rhyme label="b" type="f" corresp="#s3:l2">Zegen-Wagen</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Voor wien men <rhyme label="a" type="m" corresp="#s3:l1">bid</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">(Heer) is het u <rhyme label="b" type="f" corresp="#s3:l2">behagen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Het <rhyme label="a" type="m" corresp="#s3:l1">grootste-lid</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Sijn jok te helpen <rhyme label="b" type="f" corresp="#s3:l2">dragen</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Op dat hy u <rhyme label="c" type="m">gemeent</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Beschermen mach. <rhyme label="c" type="m" corresp="#s3:l9">(Ver-eend</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">In rust, en hoog <rhyme label="d" type="f">geresen</rhyme>)</l>
        <l id="s3:l12" met="3" type="-+">Van Dwingelanden <rhyme label="e" type="m">vry</rhyme>.</l>
        <l id="s3:l13" met="3" type="-+">Helpt Heer ons Prins so <rhyme label="e" type="m" corresp="#s3:l12">gy</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Hielpt Iosua, voor <rhyme label="d" type="f" corresp="#s3:l11">desen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Iosua <rhyme label="a" type="m">won</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In 't strijden (seer <rhyme label="b" type="f">kloek-hartig</rhyme>)</l>
        <l id="s4:l3" met="2" type="-+">Sijn Swaard <rhyme label="a" type="m" corresp="#s4:l1">verslon</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Der Vorsten <rhyme label="b" type="f" corresp="#s4:l2">een-en-dartig</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Doen Maan, en <rhyme label="a" type="m" corresp="#s4:l1">Son</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sich stelden <rhyme label="b" type="f" corresp="#s4:l2">even-partig</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">'t Geen haar <rhyme label="a" type="m" corresp="#s4:l1">begon</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In 't hart te prik'len, <rhyme label="b" type="f" corresp="#s4:l2">smartig</rhyme>.</l>
        <l id="s4:l9" met="3" type="-+">Maar hy heeft (straf) <rhyme label="c" type="m">vernield</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Verbrand, verworgd, <rhyme label="c" type="m" corresp="#s4:l9">ontzield</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">De Heydenen: vol <rhyme label="d" type="f">vresen</rhyme>.</l>
        <l id="s4:l12" met="3" type="-+">Daar u den roem van <rhyme label="e" type="m">zy</rhyme>.</l>
        <l id="s4:l13" met="3" type="-+">Helpt Heer ons Prins so <rhyme label="e" type="m" corresp="#s4:l12">gy</rhyme></l>
        <l id="s4:l14" met="3" type="-+">Hielpt Iosua, voor <rhyme label="d" type="f" corresp="#s4:l11">desen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Helpt Heer ons <rhyme label="a" type="m">Prins</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Den Draak sijn wieken <rhyme label="b" type="f">snoeyen</rhyme>:</l>
        <l id="s5:l3" met="2" type="-+">Als gy hem <rhyme label="a" type="m" corresp="#s5:l1">gins</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hielpt af-doen, en <rhyme label="b" type="f" corresp="#s5:l2">uyt-roeyen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Des Duyvels <rhyme label="a" type="m" corresp="#s5:l1">grins</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Der beelden-dienst: in 't <rhyme label="b" type="f" corresp="#s5:l2">groeyen</rhyme>.</l>
        <l id="s5:l7" met="2" type="-+">Op dat <rhyme label="a" type="m" corresp="#s5:l1">alsins</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">U Kerk in Liefd mach <rhyme label="b" type="f" corresp="#s5:l2">Bloeyen</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Gelijk een Lely, <rhyme label="c" type="m">vast</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Onder de Doornen <rhyme label="c" type="m" corresp="#s5:l9">wast</rhyme>.</l>
        <l id="s5:l11" met="3" type="+-">Breekt doch ons vyands <rhyme label="d" type="f">pésen</rhyme>:</l>
        <l id="s5:l12" met="3" type="-+">Na dit gebed van <rhyme label="e" type="m">my</rhyme>.</l>
        <l id="s5:l13" met="3" type="-+">Helpt Heer ons Prins so <rhyme label="e" type="m" corresp="#s5:l12">gy</rhyme></l>
        <l id="s5:l14" met="3" type="-+">Hielpt Iosua, voor <rhyme label="d" type="f" corresp="#s5:l11">desen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
