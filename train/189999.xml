<song>
  <meta>
    <id>189999</id>
    <date>
      <year-start>1792</year-start>
      <year-end>1792</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ohne Lieb und ohne Wein</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'k Zit hier op de stoep, en <rhyme label="a" type="m">wagt</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">zou de Meid niet <rhyme label="b" type="f">hooren</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Ja, al legt zy warm en <rhyme label="a" type="m" corresp="#s1:l1">zagt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">'k Moet haar slaap tog <rhyme label="b" type="f" corresp="#s1:l2">stooren</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">'k Schel nog eens, het word ook <rhyme label="c" type="m">tyd</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">'t zou my haast <rhyme label="d" type="f">verveelen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Als ik hier de tyd <rhyme label="c" type="m" corresp="#s1:l5">verslyt</rhyme></l>
        <l id="s1:l8" met="3" type="+-">zou 'k het d√¢gloon <rhyme label="d" type="f" corresp="#s1:l6">steelen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Heb ook nog zo veel te <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Eerst de trap en 't <rhyme label="b" type="f">zaaltje</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daar ik alles schoontjes <rhyme label="a" type="m" corresp="#s2:l1">boen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En dan aan 't <rhyme label="b" type="f" corresp="#s2:l2">portaaltje</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">'k Moet nog schuuren nat en <rhyme label="c" type="m">droog</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Keuken zelfs en <rhyme label="d" type="f">Kelder</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En niet slegts zo wat voor 't <rhyme label="c" type="m" corresp="#s2:l5">oog</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Maar ter deeg en <rhyme label="d" type="f" corresp="#s2:l6">helder</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Juffrouw gaat my wel niet <rhyme label="a" type="m">na</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Maar zou ik <rhyme label="b" type="f">verdraagen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat zy, als ik heenen <rhyme label="a" type="m" corresp="#s3:l1">gaa</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Reden vind tot <rhyme label="b" type="f" corresp="#s3:l2">klaagen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">'k Word van haar zo wel <rhyme label="c" type="m">beloond</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">En kryg kostelyk <rhyme label="d" type="f">eeten</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">'t Is dan billyk dat men <rhyme label="c" type="m" corresp="#s3:l5">toont</rhyme></l>
        <l id="s3:l8" met="3" type="+-">zelfs zyn pligt te <rhyme label="d" type="f" corresp="#s3:l6">weeten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Denk wel eens, het valt tog <rhyme label="a" type="m">zwaar</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Dag aan dag te <rhyme label="b" type="f">slooven</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Had ik werk voor 't gansche <rhyme label="a" type="m" corresp="#s4:l1">jaar</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Ik kwam 't nog eens te <rhyme label="b" type="f" corresp="#s4:l2">boven</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Maar van 't geen ik zomers <rhyme label="c" type="m">win</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Moet ik 's winters <rhyme label="d" type="f">leeven</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">En dan heeft het moeite <rhyme label="c" type="m" corresp="#s4:l5">in</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Elk het zyn te <rhyme label="d" type="f" corresp="#s4:l6">geeven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maar dit krenkt geenzins myn <rhyme label="a" type="m">moed</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">'k Ben tog wel te <rhyme label="b" type="f">vreeden</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Heb ik dan geen <rhyme label="a" type="m" corresp="#s5:l1">overvloed</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">'k Heb gezonde <rhyme label="b" type="f" corresp="#s5:l2">leden</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">zyn myn kleeren eens wat <rhyme label="c" type="m">slegt</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Oud, en vol met <rhyme label="d" type="f">lapjes</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">'t Schaad niet, als slegts ieder <rhyme label="c" type="m" corresp="#s5:l5">zegt</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">ze is tog schoon en <rhyme label="d" type="f" corresp="#s5:l6">knapjes</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Ben gezond en vlug en <rhyme label="a" type="m">sterk</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">'k Win myn brood met <rhyme label="b" type="f">eeren</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Als ik trouw ben in myn <rhyme label="a" type="m" corresp="#s6:l1">werk</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Kan my niemand <rhyme label="b" type="f" corresp="#s6:l2">deeren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Daar geen mensch myn lot <rhyme label="c" type="m">benyd</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Heb ik niets te <rhyme label="d" type="f">vreezen</rhyme>;</l>
        <l id="s6:l7" met="4" type="+-">'k Schel nog eens, waar blyft de <rhyme label="c" type="m" corresp="#s6:l5">Meid</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">zagt,... daar zal ze <rhyme label="d" type="f" corresp="#s6:l6">weezen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
