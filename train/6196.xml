      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat mag Goossen toch gebreken</melody>
  <author>C.P. van Wesbusch</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LOnckjes van mijn <rhyme label="a" type="f">Harderinne</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Lonckjes van mijn <rhyme label="b" type="m">Galathee</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Leeren my de rechte <rhyme label="a" type="f" corresp="#s1:l1">Minne</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wysen my gheneucht en <rhyme label="b" type="m" corresp="#s1:l2">vree</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Duyden my hoe 'k met een <rhyme label="b" type="m" corresp="#s1:l2">bee</rhyme>:</l>
        <l id="s1:l6" met="3" type="+-">Kusjes sal <rhyme label="c" type="f">verwerven</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Van Me-vrou// die ick <rhyme label="d" type="m">nou</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Langher niet sal <rhyme label="c" type="f" corresp="#s1:l6">derven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Noyt en wist ick van de <rhyme label="a" type="f">Lonckjes</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Noyt en kend' ick dit <rhyme label="b" type="m">playsier</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Noyt en wiest ick van de <rhyme label="a" type="f" corresp="#s2:l1">vonckjes</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Die soo haest een dapper <rhyme label="b" type="m" corresp="#s2:l2">vyer</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">In mijn hart ontsteecken <rhyme label="b" type="m" corresp="#s2:l2">(schier</rhyme>)</l>
        <l id="s2:l6" met="3" type="+-">Dat ick sou <rhyme label="c" type="f">versmachten</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">Maer sy zijn// <rhyme label="d" type="m">medicijn</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Waerdigh hoog te <rhyme label="c" type="f" corresp="#s2:l6">achten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Lonckjes konnen my <rhyme label="a" type="f">vermaecken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Lonckjes zijn mijn levens <rhyme label="b" type="m">lust</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Lonkjes strecken my een <rhyme label="a" type="f" corresp="#s3:l1">Baecken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Op een langh-ghewenschte <rhyme label="b" type="m" corresp="#s3:l2">Kust</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Daer ick dan mijn Min <rhyme label="b" type="m" corresp="#s3:l2">(gherust</rhyme>)</l>
        <l id="s3:l6" met="3" type="+-">Heenen laet <rhyme label="c" type="f">laveren</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Lonkjes bly// van ter <rhyme label="d" type="m">sy'</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Konnen ons wat <rhyme label="c" type="f" corresp="#s3:l6">leeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Lonckjes konnen wel wat <rhyme label="a" type="f">segghen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Schoon de mondt ghesloten <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Lonckjes konnen oock <rhyme label="a" type="f" corresp="#s4:l1">uyt-legghen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Of een Maeght u draghet <rhyme label="b" type="m" corresp="#s4:l2">haet</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Die met Lonckjes omme <rhyme label="b" type="m" corresp="#s4:l2">gaet</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Kan gheen min <rhyme label="c" type="f">verberghen</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">Venus Wicht// haer <rhyme label="d" type="m">ghesicht</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Tot dees min gaet <rhyme label="c" type="f" corresp="#s4:l6">verghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Princelijcke <rhyme label="a" type="f">Lonck-Goddinnen</rhyme>:</l>
        <l id="s5:l2" met="4" type="+-">Lonckjes hebben my <rhyme label="b" type="m">gheleert</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Waer ick vry soud' moghen <rhyme label="a" type="f" corresp="#s5:l1">minnen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En wie sich tot minnen <rhyme label="b" type="m" corresp="#s5:l2">keert</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Die my Lonckjes heeft <rhyme label="b" type="m" corresp="#s5:l2">vereert</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Sal ick weer <rhyme label="c" type="f">vereeren</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Een Iuweel// soet en <rhyme label="d" type="m">eel</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Tot haers lofs <rhyme label="c" type="f" corresp="#s5:l6">vermeeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
