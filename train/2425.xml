    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Hendrik van Boxtel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">COemt Scholieren, Venus <rhyme label="a" type="f">dieren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die van mijn ghenoten <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vreucht hanteren, <rhyme label="a" type="f" corresp="#s1:l1">domineren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Wilt met my in desen <rhyme label="b" type="m" corresp="#s1:l2">tijt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Helpt ongheneucht neder <rhyme label="c" type="f">staken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En wilt met my vreuchden <rhyme label="c" type="f" corresp="#s1:l5">maken</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Laet alle solaes <rhyme label="d" type="m">opstaen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Troost van lieve heb ick <rhyme label="d" type="m" corresp="#s1:l7">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Speelt op luyten, en op <rhyme label="a" type="f">fluyten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En verheucht u al <rhyme label="b" type="m">ghelijck</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wilt wtspruyten, ende <rhyme label="a" type="f" corresp="#s2:l1">wten</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Sterckt gheneucht en vreuchden <rhyme label="b" type="m" corresp="#s2:l2">rijck</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Coemt ghy lustighe <rhyme label="c" type="f">fiolen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ick roep u tot mijnder <rhyme label="c" type="f" corresp="#s2:l5">scholen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Doet doch al na mijn <rhyme label="d" type="m">vermaen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Troost van lieve heb ick <rhyme label="d" type="m" corresp="#s2:l7">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Coemt Musijckers, <rhyme label="a" type="f">Rethorijckers</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Singt ons wat vreuchden <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ghy practijckers, <rhyme label="a" type="f" corresp="#s3:l1">fantasijckers</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Coemt en hebt al goeden <rhyme label="b" type="m" corresp="#s3:l2">moet</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Wilt ons al gheneucht <rhyme label="c" type="f">voorbringhen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Speelt oft wilt lustelick <rhyme label="c" type="f" corresp="#s3:l5">singhen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Houdt nv alle met my <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Troost van lief heb ick <rhyme label="d" type="m" corresp="#s3:l7">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Int ghemeene, groot en <rhyme label="a" type="f">cleene</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Maeckt u al by my <rhyme label="b" type="m">ghepaert</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Coemt by eene, sterckt <rhyme label="a" type="f" corresp="#s4:l1">certeene</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Vreucht gheneucht na uwen <rhyme label="b" type="m" corresp="#s4:l2">aert</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Coemt hier by ghy jong <rhyme label="c" type="f">ghesellen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En wilt u voortaen niet <rhyme label="c" type="f" corresp="#s4:l5">quellen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Doet also ick heb <rhyme label="d" type="m">ghedaen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Troost van lief heb ick <rhyme label="d" type="m" corresp="#s4:l7">ontfaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
