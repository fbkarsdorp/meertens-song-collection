<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186659</id>
    <date>
      <year-start>1794</year-start>
      <year-end>1794</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pots honderdduizend slapperment</melody>
  <author>Gerrit Manheer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'t Was Feest, de Goden kwamen <rhyme label="a" type="m">saam</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Op eenen grooten <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Toen Chitere van <rhyme label="c" type="m">Cupido</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In 't blyde Kraambed <rhyme label="b" type="m" corresp="#s1:l2">lag</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Geen wonder, dit <rhyme label="d" type="m">geboortefeest</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Waar van te veel <rhyme label="e" type="m">gewigt</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Dan dat niet op dien blyden <rhyme label="f" type="m">stond</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Den beker wierd <rhyme label="e" type="m" corresp="#s1:l6">geligt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De groote zaal was <rhyme label="a" type="m">opgeruimd</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Aan Venus prachtig <rhyme label="b" type="m">Hof</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Alleen 't beschryven van die <rhyme label="c" type="m">zaal</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Schenkt my een ruime <rhyme label="b" type="m" corresp="#s2:l2">stof</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Deez' zaal is ryk <rhyme label="d" type="m">gestucadoord</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Het lystwerk zwaar <rhyme label="e" type="m">verguld</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">En nis by nis, met <rhyme label="f" type="m">Bloemfestoens</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En beeldwerk <rhyme label="e" type="m" corresp="#s2:l6">opgevuld</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De zolder is <rhyme label="a" type="m">geblaffonneert</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met paarlen en <rhyme label="b" type="m">robyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En diamanten, dia zoo <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als eendenÃ«yren <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Dit praalgewelf rust op een' <rhyme label="d" type="m">ry</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Kolommen van <rhyme label="e" type="m">agaat</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Daar elke zuil op eenen <rhyme label="f" type="m">voet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van louter zilver <rhyme label="e" type="m" corresp="#s3:l6">staat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De vloer is van gesleepen <rhyme label="a" type="m">staal</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De trappen van <rhyme label="b" type="m">ivoor</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">De deuren zyn van <rhyme label="c" type="m">ebbenhout</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Nog zwarter dan den <rhyme label="b" type="m" corresp="#s4:l2">moor</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Ook treed men op een <rhyme label="d" type="m">vloer-tapyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van purper <rhyme label="e" type="m">zydamast</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">De stoelen zyn van <rhyme label="f" type="m">elpenbeen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">In blozend goud <rhyme label="e" type="m" corresp="#s4:l6">gekast</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'t Bouffet, waar in de glazen <rhyme label="a" type="m">staan</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En flessen met <rhyme label="b" type="m">liqueur</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Is enkel uit kristal <rhyme label="c" type="m">gemaakt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van onderscheide <rhyme label="b" type="m" corresp="#s5:l2">kleur</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">De Tafel, waar aan 't <rhyme label="d" type="m">Godendom</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Moest zitten, was <rhyme label="e" type="m">ovaal</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En stond op eenen gouden <rhyme label="f" type="m">voet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">In 't midden van de <rhyme label="e" type="m" corresp="#s5:l6">zaal</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Op Negen was de komst <rhyme label="a" type="m">bepaald</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Doch 't wierd wel ruim <rhyme label="b" type="m">halftien</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Geen wonder, elk die binnen <rhyme label="c" type="m">trad</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Moest eerst het kindje <rhyme label="b" type="m" corresp="#s6:l2">zien</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Vrouw Moeder lag in 't <rhyme label="d" type="m">kinderbed</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Gelyk een frisse <rhyme label="e" type="m">roos</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Niet bleek bestorven om den <rhyme label="f" type="m">mond</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Maar, met een malschen <rhyme label="e" type="m" corresp="#s6:l6">bloos</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hier pronkte Vulkaan als <rhyme label="a" type="m">Papa</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Aan 't hoofd van 't <rhyme label="b" type="m">ledikant</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Gebakerd in een sitse <rhyme label="c" type="m">pon</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Met eenen zyden <rhyme label="b" type="m" corresp="#s7:l2">rand</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Hier zat hy met een <rhyme label="d" type="m">Majesteit</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die aan een Kraamheer <rhyme label="e" type="m">past</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">En hield met zijn berookte <rhyme label="f" type="m">vuist</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Vrouw Moeders handje <rhyme label="e" type="m" corresp="#s7:l6">vast</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De wieg waar in het wichtje <rhyme label="a" type="m">lag</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">o! Die was wonder <rhyme label="b" type="m">fyn</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Het wiegkleed, deken, zelf het <rhyme label="c" type="m">bed</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Was altemaal <rhyme label="b" type="m" corresp="#s8:l2">satyn</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Hoe lief betoovrend, stil en <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Lag hier dien kleenen <rhyme label="e" type="m">guit</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">En lachten reeds, hoe jong hy <rhyme label="f" type="m">was</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Den kring der Goden <rhyme label="e" type="m" corresp="#s8:l6">uit</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Elk kust en streelt het lieve <rhyme label="a" type="m">wicht</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Onkundig van zyn <rhyme label="b" type="m">aart</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Doch voeld iets wonders in zyn <rhyme label="c" type="m">bloed</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Zoo iets, dat prikkling <rhyme label="b" type="m" corresp="#s9:l2">baart</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Voor al het schoon <rhyme label="d" type="m">Godinneudom</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Kreeg bloosjes op 't <rhyme label="e" type="m">gelaat</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">En elk gevoelde een streelend <rhyme label="f" type="m">vuur</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Waar van den boezem <rhyme label="e" type="m" corresp="#s9:l6">slaat</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De Kraamvrouw hield zich doodlyk <rhyme label="a" type="m">stil</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En sprak geen enkel <rhyme label="b" type="m">woord</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Wyl heer Papa niet dulden <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dat men haar rust <rhyme label="b" type="m" corresp="#s10:l2">verstoord</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Doch zoo draa Mavors binnen <rhyme label="d" type="m">trad</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Rees Venus <rhyme label="e" type="m">overend</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Zagt lieve Vrouwtje! sprak <rhyme label="f" type="m">Vulkaan</rhyme>:</l>
        <l id="s10:l8" met="3" type="-+">Wat raakt u toch die <rhyme label="e" type="m" corresp="#s10:l6">vent</rhyme>! -</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ach poesje! zei hy: zyt toch <rhyme label="a" type="m">stil</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Uw staat eischt immers <rhyme label="b" type="m">rust</rhyme>?</l>
        <l id="s11:l3" met="4" type="-+">Doch, Mars trad toe en Venus <rhyme label="c" type="m">wierd</rhyme></l>
        <l id="s11:l4" met="3" type="-+">In wederwil <rhyme label="b" type="m" corresp="#s11:l2">gekust</rhyme>!</l>
        <l id="s11:l5" met="4" type="-+">Toen lag zy 't hoofdje weÃªr in 't <rhyme label="d" type="m">dons</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">De lieve Vrouw scheen <rhyme label="e" type="m">moÃª</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">De schalke Goden lachten <rhyme label="f" type="m">eens</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">En knikten Mavors <rhyme label="e" type="m" corresp="#s11:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Dan, eindelyk trad de gansche <rhyme label="a" type="m">Ry</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Ter zaal in, Bachus <rhyme label="b" type="m">koos</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Zyn zitplaats, aan de hooge <rhyme label="c" type="m">zy</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Het duurde een ruime <rhyme label="b" type="m" corresp="#s12:l2">poos</rhyme>.</l>
        <l id="s12:l5" met="4" type="-+">Eer men nog iets te drinken <rhyme label="d" type="m">bragt</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Dan, Bachus, droog van <rhyme label="e" type="m">borst</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Riep waar, waar blyft den <rhyme label="f" type="m">brandewyn</rhyme>!</l>
        <l id="s12:l8" met="3" type="-+">De Goden hebben <rhyme label="e" type="m" corresp="#s12:l6">dorst</rhyme>! -</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Doch, eindelyk bragt <rhyme label="a" type="m">Ganimeed</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Den gouden Beker <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Waar in me een lange pyp <rhyme label="c" type="m">Kaneel</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Zag overeinde <rhyme label="b" type="m" corresp="#s13:l2">staan</rhyme>.</l>
        <l id="s13:l5" met="4" type="-+">Als ook een schotel met <rhyme label="d" type="m">Banket</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">En andere <rhyme label="e" type="m">snoepery</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">Terwyl vast Bachus raasd en <rhyme label="f" type="m">roept</rhyme>:</l>
        <l id="s13:l8" met="3" type="-+">Breng eerst den drank by <rhyme label="e" type="m" corresp="#s13:l6">my</rhyme>! -</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Jupyn nam nochtans d'eersten <rhyme label="a" type="m">dronk</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Als oudste van de <rhyme label="b" type="m">gild</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Dat's kost sprak hy, die 'k gare <rhyme label="c" type="m">meug</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Welaan nog eens <rhyme label="b" type="m" corresp="#s14:l2">getilt</rhyme>.</l>
        <l id="s14:l5" met="4" type="-+">Al zagt! riep Bachus, Broer hou <rhyme label="d" type="m">op</rhyme>! -</l>
        <l id="s14:l6" met="3" type="-+">Zend, zend den beker <rhyme label="e" type="m">hier</rhyme>;</l>
        <l id="s14:l7" met="4" type="-+">Doch eer Jupyn hem <rhyme label="f" type="m">overgaf</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Dronk hy een halve <rhyme label="e" type="m" corresp="#s14:l6">slier</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Toen ging den gouden Beker <rhyme label="a" type="m">rond</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Tot hy by Bachus <rhyme label="b" type="m">kwam</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Maar of den Beker wierd <rhyme label="c" type="m">gestraft</rhyme>!</l>
        <l id="s15:l4" met="3" type="-+">'t Waar of hy met een <rhyme label="b" type="m" corresp="#s15:l2">kram</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Aan Bachus lippen was <rhyme label="d" type="m">gehegt</rhyme>.</l>
        <l id="s15:l6" met="3" type="-+">Pan riep, o dronken <rhyme label="e" type="m">guit</rhyme>!</l>
        <l id="s15:l7" met="4" type="-+">Geef hier den brandenwynl wel <rhyme label="f" type="m">ja</rhyme>:</l>
        <l id="s15:l8" met="3" type="-+">Zei Bachus, 't is 'er <rhyme label="e" type="m" corresp="#s15:l6">uit</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Doch, Ganimedes die wel <rhyme label="a" type="m">weet</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Wat snaaken dat het <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Schonk daadlyk weÃªr den Beker <rhyme label="c" type="m">vol</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Met besten <rhyme label="b" type="m" corresp="#s16:l2">brandewyn</rhyme>.</l>
        <l id="s16:l5" met="4" type="-+">Vulkaan zat mede in den <rhyme label="d" type="m">kring</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Gelyk 't een Kraamheer <rhyme label="e" type="m">voegt</rhyme>,</l>
        <l id="s16:l7" met="4" type="-+">En wierd van elk geluk <rhyme label="f" type="m">gewenscht</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">De man scheen <rhyme label="e" type="m" corresp="#s16:l6">vergenoegt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Mars sloop, intusschen, stil van <rhyme label="a" type="m">daar</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Naar 't bed van <rhyme label="b" type="m">Chitere</rhyme>.</l>
        <l id="s17:l3" met="4" type="-+">Dan, Vulkaan kreeg het in den <rhyme label="c" type="m">neus</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">En sprong fluks van zyn <rhyme label="b" type="m" corresp="#s17:l2">stee</rhyme>.</l>
        <l id="s17:l5" met="4" type="-+">Waar, waar is Mars! riep hy <rhyme label="d" type="m">verbaast</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Waar zit dien stouten <rhyme label="e" type="m">kwant</rhyme>!</l>
        <l id="s17:l7" met="4" type="-+">Bedaar Papa, zei <rhyme label="f" type="m">Ganimeed</rhyme>,</l>
        <l id="s17:l8" met="3" type="-+">Hy zit by 't <rhyme label="e" type="m" corresp="#s17:l6">Ledikant</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Toen huppelde den manken <rhyme label="a" type="m">smit</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Naar 't Kraamsalet, en <rhyme label="b" type="m">vond</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Mars by zyn schoone <rhyme label="c" type="m">Gemaalin</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Vast kussend mond aan <rhyme label="b" type="m" corresp="#s18:l2">mond</rhyme>.</l>
        <l id="s18:l5" met="4" type="-+">Hy tierd, hy raast, by scheld en <rhyme label="d" type="m">vloekt</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Doch, hoe hy vloeken <rhyme label="e" type="m">mag</rhyme>,</l>
        <l id="s18:l7" met="4" type="-+">Wat werkt hy uit? niet anders, <rhyme label="f" type="m">dan</rhyme></l>
        <l id="s18:l8" met="3" type="-+">Een algemeen <rhyme label="e" type="m" corresp="#s18:l6">gelach</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Intusschen nam elk een de <rhyme label="a" type="m">laag</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">De boel raakte in de <rhyme label="b" type="m">war</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Dees was zoo dronken als een <rhyme label="c" type="m">zwyn</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Die had een halve <rhyme label="b" type="m" corresp="#s19:l2">star</rhyme>.</l>
        <l id="s19:l5" met="4" type="-+">Elk zogt, zoo goed by kon, de <rhyme label="d" type="m">deur</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Deez' gaat, die rold 'er <rhyme label="e" type="m">uit</rhyme>.</l>
        <l id="s19:l7" met="4" type="-+">Dus einde het <rhyme label="f" type="m">Geboortefeest</rhyme></l>
        <l id="s19:l8" met="3" type="-+">Van Venus lieve <rhyme label="e" type="m" corresp="#s19:l6">Spruit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
