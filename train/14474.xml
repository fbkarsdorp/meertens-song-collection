<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14474</id>
    <date>
      <year-start>1638</year-start>
      <year-end>1638</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Nu wees gegroet Maria zoet</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Aegidius Haeffacker</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Menschen haest u metter <rhyme label="a" type="m">spoet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wilt met Lof-sanghen <rhyme label="b" type="f">prijsen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Maria, die reyn Maget <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt haer toch eer <rhyme label="b" type="f" corresp="#s1:l2">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt haer altijdt, na mijn <rhyme label="a" type="m">bedien</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Als Maeght en Moeder <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die van 't beginsel was <rhyme label="a" type="m" corresp="#s2:l1">voorsien</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Tot een Moeder des <rhyme label="b" type="f" corresp="#s2:l2">Heeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy is die Maghet sonder <rhyme label="a" type="m">smet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een Vrouw van groote <rhyme label="b" type="f">krachten</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">'t Hooft des Serpents heeft sy <rhyme label="a" type="m" corresp="#s3:l1">verplet</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Eert haer alle <rhyme label="b" type="f" corresp="#s3:l2">gheslachten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Esaias die heeft van <rhyme label="a" type="m">haer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghepropheteert <rhyme label="b" type="f">waerachtigh</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat sy ons baeren sou <rhyme label="a" type="m" corresp="#s4:l1">voorwaer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den Soone Godts <rhyme label="b" type="f" corresp="#s4:l2">almachtigh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy was verciert, die suyver <rhyme label="a" type="m">Maegt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met deughden <rhyme label="b" type="f">overvloedigh</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Maer s' heeft Godt sonderling <rhyme label="a" type="m" corresp="#s5:l1">behaegt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Om dat sy was <rhyme label="b" type="f" corresp="#s5:l2">ootmoedigh</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Geen tonghe kan uyt-spreken, <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Haer deughd en heyligh <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Een herte kan 't bedencken <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hoe sy nu is <rhyme label="b" type="f" corresp="#s6:l2">verheven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dus prijst haer alle menschen <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Haer deughden wilt <rhyme label="b" type="f">aenmercken</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Wilt ghy met haer in vreughden <rhyme label="a" type="m" corresp="#s7:l1">zijn</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Volght na haer goede <rhyme label="b" type="f" corresp="#s7:l2">wercken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
