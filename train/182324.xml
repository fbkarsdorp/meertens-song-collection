<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182324</id>
    <date>
      <year-start>1786</year-start>
      <year-end>1786</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat is ons al vreugde gegeven</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wat word ons al vreugd <rhyme label="a" type="f">gegeeven</rhyme></l>
        <l id="s1:l2" met="4" type="+-">In 't vermaak'lyk <rhyme label="a" type="f" corresp="#s1:l1">Burger-Leven</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">'t Ademt vryheid;... Bato's <rhyme label="b" type="m">moed</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Alles wat Wyk dertien <rhyme label="b" type="m" corresp="#s1:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vry van Heerschzugt, dwang, of <rhyme label="a" type="f">kommer</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Smaken wy in vryheid's <rhyme label="a" type="f" corresp="#s2:l1">lommer</rhyme></l>
        <l id="s2:l3" met="4" type="+-">'t Nut der Wapen - <rhyme label="b" type="m">öeffening</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Waar aan Neêrlands glorie <rhyme label="b" type="m" corresp="#s2:l3">hing</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Held van Son! door uwe <rhyme label="a" type="f">straalen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Licht ge ons voor in 't <rhyme label="a" type="f" corresp="#s3:l1">roembehaalen</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">'t Strekt ons tot uitsteekende <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">U te volgen in 't <rhyme label="b" type="m" corresp="#s3:l3">geweer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Jonkheid! die in 't prilst der <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Vryheid's loflyk doel wilt <rhyme label="a" type="f" corresp="#s4:l1">schraagen</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Voeg U by de <rhyme label="b" type="m">Krygsbanier</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Van deez' braaven <rhyme label="b" type="m" corresp="#s4:l3">Batavier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Burgers! hoort de vryheid <rhyme label="a" type="f">spreeken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">(Daar ze U groet uit hooger <rhyme label="a" type="f" corresp="#s5:l1">streeken</rhyme>:)</l>
        <l id="s5:l3" met="4" type="+-">‘Waakt (roept ze uit) voor speer, en <rhyme label="b" type="m">Hoed</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">‘Duur gekogt voor <rhyme label="b" type="m" corresp="#s5:l3">Heldenbloed</rhyme>;</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">‘Vegt voor Haardsteên en <rhyme label="a" type="f">Altaaren</rhyme>;</l>
        <l id="s6:l2" met="4" type="+-">‘Laat geen Meroz U <rhyme label="a" type="f" corresp="#s6:l1">vervaaren</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">‘Die zig aan uw kring <rhyme label="b" type="m">onttrekt</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">‘Lafheid word met schand' <rhyme label="b" type="m" corresp="#s6:l3">bedekt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">‘Zyn 'er die van muitzugt <rhyme label="a" type="f">vlammen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">‘Wilt hunn' vuist terstond <rhyme label="a" type="f" corresp="#s7:l1">verlammen</rhyme>!</l>
        <l id="s7:l3" met="4" type="+-">‘Doodt; .. verdelgt dat Korah's <rhyme label="b" type="m">Rot</rhyme>!</l>
        <l id="s7:l4" met="4" type="+-">‘'t Is de wil van Neêrlands <rhyme label="b" type="m" corresp="#s7:l3">God</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">‘Dat uw halsen nimmer <rhyme label="a" type="f">bukken</rhyme></l>
        <l id="s8:l2" met="4" type="+-">‘Onder Monarchaaale <rhyme label="a" type="f" corresp="#s8:l1">Jukken</rhyme>!</l>
        <l id="s8:l3" met="4" type="+-">‘Noch 't Aristocraat's <rhyme label="b" type="m">gezag</rhyme></l>
        <l id="s8:l4" met="4" type="+-">‘Immer U beheerschen <rhyme label="b" type="m" corresp="#s8:l3">mag</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">‘Dreigt ooit Dwingland U te <rhyme label="a" type="f">schenden</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">‘Trapt Hem fier op Borst en <rhyme label="a" type="f" corresp="#s9:l1">Lenden</rhyme>;</l>
        <l id="s9:l3" met="4" type="+-">‘Toont Hem uw ontgyzeld <rhyme label="b" type="m">staal</rhyme></l>
        <l id="s9:l4" met="4" type="+-">‘Tot zyn' val!... uw <rhyme label="b" type="m" corresp="#s9:l3">zegepraal</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">‘Amstel's Burgers! trouwe <rhyme label="a" type="f">Wagters</rhyme>!</l>
        <l id="s10:l2" met="4" type="+-">‘Duldt geen Wet- geen <rhyme label="a" type="f" corresp="#s10:l1">Rechtverkragters</rhyme>;</l>
        <l id="s10:l3" met="4" type="+-">‘Schuwt in uwe vrye <rhyme label="b" type="m">vest</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">‘Zulke Monsters als de <rhyme label="b" type="m" corresp="#s10:l3">pest</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">‘Schat, schat hoog uwe <rhyme label="a" type="f">Overheden</rhyme>;</l>
        <l id="s11:l2" met="4" type="+-">‘Daar ze in 't spoor der Wetten <rhyme label="a" type="f" corresp="#s11:l1">treden</rhyme>;</l>
        <l id="s11:l3" met="4" type="+-">‘Pleng, (is 't nood!) voor hun uw <rhyme label="b" type="m">bloed</rhyme>:</l>
        <l id="s11:l4" met="4" type="+-">‘Zo behoudt ge uw Speer en <rhyme label="b" type="m" corresp="#s11:l3">Hoed</rhyme>.’</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">God der Vryheid! Eeuwig <rhyme label="a" type="f">Wezen</rhyme>!</l>
        <l id="s12:l2" met="4" type="+-">Zon, en Schild voor die U <rhyme label="a" type="f" corresp="#s12:l1">vreezen</rhyme>!</l>
        <l id="s12:l3" met="4" type="+-">Vader van ons <rhyme label="b" type="m">Vaderland</rhyme>!</l>
        <l id="s12:l4" met="4" type="+-">Houd by ons uw haard in <rhyme label="b" type="m" corresp="#s12:l3">brand</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Heersch, heersch in der Burgren <rhyme label="a" type="f">harten</rhyme>!</l>
        <l id="s13:l2" met="4" type="+-">Doe hen list en moedwil <rhyme label="a" type="f" corresp="#s13:l1">tarten</rhyme>!</l>
        <l id="s13:l3" met="4" type="+-">Zegen, <rhyme label="b" type="m">Oppermajesteid</rhyme>!</l>
        <l id="s13:l4" met="4" type="+-">Burgery en <rhyme label="b" type="m" corresp="#s13:l3">Overheid</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Eendragt snoer, de <rhyme label="a" type="f">stadgenooten</rhyme>!</l>
        <l id="s14:l2" met="4" type="+-">Eendragt woone op 't Veld! - op <rhyme label="a" type="f" corresp="#s14:l1">vlooten</rhyme>!</l>
        <l id="s14:l3" met="4" type="+-">Eendragt heersche in Kerk en <rhyme label="b" type="m">staat</rhyme>!</l>
        <l id="s14:l4" met="4" type="+-">Eendragt blyve.... als 't all' <rhyme label="b" type="m" corresp="#s14:l3">vergaat</rhyme>!</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Komt nu Burgers! zaam <rhyme label="a" type="f">gedronken</rhyme>;</l>
        <l id="s15:l2" met="4" type="+-">En den Beeker vol <rhyme label="a" type="f" corresp="#s15:l1">geschonken</rhyme>!</l>
        <l id="s15:l3" met="4" type="+-">Deeze teug zy de agting <rhyme label="b" type="m">blyk</rhyme></l>
        <l id="s15:l4" met="4" type="+-">Voor van Son; en gansch zyn <rhyme label="b" type="m" corresp="#s15:l3">wyk</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Zweeren we onzen Hopman <rhyme label="a" type="f">trouwe</rhyme>!</l>
        <l id="s16:l2" met="4" type="+-">‘Hy mooge op Wyk dertien <rhyme label="a" type="f" corresp="#s16:l1">bouwen</rhyme>!</l>
        <l id="s16:l3" met="4" type="+-">‘Op ons Sulfer, staal en <rhyme label="b" type="m">Lood</rhyme>!</l>
        <l id="s16:l4" met="4" type="+-">‘t' Zy ten leven;.... 't zy ten <rhyme label="b" type="m" corresp="#s16:l3">dood</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
