    <tags/>
    <melody>La marine</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat vreugd ziet men <rhyme label="a" type="f">bedryven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Al in het Engels <rhyme label="b" type="m">Ryk</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Dan Mannen ende <rhyme label="a" type="f" corresp="#s1:l1">Wyven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En Kinders <rhyme label="b" type="m" corresp="#s1:l2">algelyk</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Om datter voor <rhyme label="c" type="m">gewis</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Nu vaste tyding <rhyme label="c" type="m" corresp="#s1:l5">is</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Daer gekomen, Sonder <rhyme label="d" type="f">schromen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">’t Kan niet <rhyme label="c" type="m" corresp="#s1:l5">mis</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe dat Haer <rhyme label="a" type="f">Admirale</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En grooten Held <rhyme label="b" type="m">Vernon</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met zes Schepen <rhyme label="a" type="f" corresp="#s2:l1">eengaale</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Kruyt, kogels en <rhyme label="b" type="m" corresp="#s2:l2">kanon</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En zeven duizend <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">By Carthagena <rhyme label="c" type="m" corresp="#s2:l5">dan</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Quam te Lande, Met <rhyme label="d" type="f">verstande</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Hoort dit <rhyme label="c" type="m" corresp="#s2:l5">an</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Spanjaerts op die <rhyme label="a" type="f">tyen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die schoten wel <rhyme label="b" type="m">verwoet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Uyt zeve <rhyme label="a" type="f" corresp="#s3:l1">Batterijen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En twee kasteelen <rhyme label="b" type="m" corresp="#s3:l2">goet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Als mede uyt een <rhyme label="c" type="m">Fort</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dog kwamen veels te <rhyme label="c" type="m" corresp="#s3:l5">kort</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Wilt aanhooren, Sonder <rhyme label="d" type="f">stooren</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Waer ’t aen <rhyme label="c" type="m" corresp="#s3:l5">schort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Engelse zeer <rhyme label="a" type="f">vaerdig</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die gaven te fel <rhyme label="b" type="m">vuur</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En vogten heel <rhyme label="a" type="f" corresp="#s4:l1">pryswaardig</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ja stonden als een <rhyme label="b" type="m" corresp="#s4:l2">muur</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Waar door dat meenig <rhyme label="c" type="m">held</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ter neder wiert <rhyme label="c" type="m" corresp="#s4:l5">geveld</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">In het stryde, Aen <rhyme label="d" type="f">weerszyde</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Zoo men <rhyme label="c" type="m" corresp="#s4:l5">meld</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Spanjaerts deen hun <rhyme label="a" type="f">beste</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Maer verlooren de <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Vlugten ten lange <rhyme label="a" type="f" corresp="#s5:l1">lesten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Uyt haeren Sterktens met <rhyme label="b" type="m" corresp="#s5:l2">spoed</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En lieten daer  <rhyme label="c" type="m">masooy</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daer kanonnen ten <rhyme label="c" type="m" corresp="#s5:l5">prooy</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">In de handen, Der <rhyme label="d" type="f">Vyanden</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Is ’t niet <rhyme label="c" type="m" corresp="#s5:l5">mooy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ja wierden zoo <rhyme label="a" type="f">benepen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat zy ter zelver <rhyme label="b" type="m">stond</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Vier van haer <rhyme label="a" type="f" corresp="#s6:l1">Oorlogschepen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Gingen boren in de <rhyme label="b" type="m" corresp="#s6:l2">grond</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Met zes Galjoenen <rhyme label="c" type="m">trant</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Om datze den <rhyme label="c" type="m" corresp="#s6:l5">Vyand</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Sonder semen, Diet zou <rhyme label="d" type="f">nemen</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-"><rhyme label="c" type="m" corresp="#s6:l5">Triomphant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Engelse <rhyme label="a" type="f">kloekmoedig</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die hebben met <rhyme label="b" type="m">malkaer</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Nog veel Vaertuygen <rhyme label="a" type="f" corresp="#s7:l1">spoedig</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In brand gaen steken <rhyme label="b" type="m" corresp="#s7:l2">daer</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En maken zig nu <rhyme label="c" type="m">rad</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Voort meester van de <rhyme label="c" type="m" corresp="#s7:l5">Stad</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Om te buiten, Gouwe <rhyme label="d" type="f">duiten</rhyme>,</l>
        <l id="s7:l8" met="2" type="+-">En veel <rhyme label="c" type="m" corresp="#s7:l5">Schat</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sa toond uw nu als <rhyme label="a" type="f">bazen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gy Britten met <rhyme label="b" type="m">geluyt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wil: uw Victory <rhyme label="a" type="f" corresp="#s8:l1">blazen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Aan alle kanten <rhyme label="b" type="m" corresp="#s8:l2">uyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Die gy met weynig <rhyme label="c" type="m">scha</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Weer tot <rhyme label="c" type="m" corresp="#s8:l5">Carthagena</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Hebt gekregen, Door Gods <rhyme label="d" type="f">Zegen</rhyme>,</l>
        <l id="s8:l8" met="2" type="+-">Vlug en <rhyme label="c" type="m" corresp="#s8:l5">dra</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
