      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DArt'le Zieltjes, moedighe <rhyme label="a" type="f">Schaapjes</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Zijt <rhyme label="b" type="m">gegroet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Lonkende, vonkende, wulpsche <rhyme label="a" type="f" corresp="#s1:l1">Knaapjes</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Volvan <rhyme label="b" type="m" corresp="#s1:l2">gloet</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Drink eens om, op een <rhyme label="c" type="m">som</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">Gheef Soentjes van <rhyme label="d" type="m">gheluyt</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Vergheet gheen <rhyme label="d" type="m" corresp="#s1:l6">Bruyt</rhyme>.</l>
