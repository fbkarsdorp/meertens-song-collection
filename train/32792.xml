<song>
  <meta>
    <id>32792</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>O hoge waarde vrouw</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Hooghe waerde <rhyme label="a" type="m">vrouw</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Maria <rhyme label="b" type="f">Coninghinne</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">O Hemels gulden <rhyme label="a" type="m" corresp="#s1:l1">douw</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">Ghy weet dat ick u <rhyme label="b" type="f" corresp="#s1:l2">minne</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Mijn hert en all' mijn <rhyme label="b" type="f" corresp="#s1:l2">sinne</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En soecken gheen <rhyme label="c" type="m">bystant</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Als u schoonen <rhyme label="c" type="m" corresp="#s1:l6">Diament</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Natuer u milt <rhyme label="a" type="m">begiet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met haer schoonheyt <rhyme label="b" type="f">juweelen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Godt Almachtich u <rhyme label="a" type="m" corresp="#s2:l1">ghebiet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Des hemels schat te <rhyme label="b" type="f" corresp="#s2:l2">deelen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Met mijn Vader te <rhyme label="b" type="f" corresp="#s2:l2">spelen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Iesus u liefste <rhyme label="c" type="m">Soon</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Och isser hoogher <rhyme label="c" type="m" corresp="#s2:l6">croon</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">V deucht heb ick <rhyme label="a" type="m">gheleert</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vans 's Enghels wijse <rhyme label="b" type="f">tonghen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die u hebben <rhyme label="a" type="m" corresp="#s3:l1">vereert</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En heylich lof <rhyme label="b" type="f" corresp="#s3:l2">ghesonghen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ick segh dat <rhyme label="b" type="f" corresp="#s3:l2">onbedwonghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">O schoone suyver <rhyme label="c" type="m">jeught</rhyme>!</l>
        <l id="s3:l7" met="3" type="-+">Veel minder als u <rhyme label="c" type="m" corresp="#s3:l6">deught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
