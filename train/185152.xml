<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185152</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Als alle akers rijpen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Die goede Heer der <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die is altijt om <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hy weet al mijn <rhyme label="a" type="f" corresp="#s1:l1">begeeren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hy weet mijn gaen en <rhyme label="a" type="f" corresp="#s1:l1">keeren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">‘t Sy waer ick wende <rhyme label="b" type="m" corresp="#s1:l2">my</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy weet al mijn <rhyme label="a" type="f">ghedachten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En op een yder <rhyme label="b" type="m">woort</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wil hy seer neerstigh <rhyme label="a" type="f" corresp="#s2:l1">achten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">O wonderlijcke <rhyme label="a" type="f" corresp="#s2:l1">crachten</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Die ‘t alles siet en <rhyme label="b" type="m" corresp="#s2:l2">hoort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn innerlijcke <rhyme label="a" type="f">Nieren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die staen in sijnder <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ick ben door sijn <rhyme label="a" type="f" corresp="#s3:l1">bestieren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En wonderlijck <rhyme label="a" type="f" corresp="#s3:l1">regieren</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ter werelt voort <rhyme label="b" type="m" corresp="#s3:l2">ghebracht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sijn ooghen my <rhyme label="a" type="f">aensaghen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Doen ick noch niet en <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En alle mijne <rhyme label="a" type="f" corresp="#s4:l1">daghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die noch verborghen <rhyme label="a" type="f" corresp="#s4:l1">laghen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">In sijnen boeck hy <rhyme label="b" type="m" corresp="#s4:l2">las</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wanneer ick ben <rhyme label="a" type="f">verleghen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Doet hy my <rhyme label="b" type="m">onderstant</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ionstigh tot my <rhyme label="a" type="f" corresp="#s5:l1">gheneghen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Beschickt hy al mijn <rhyme label="a" type="f" corresp="#s5:l1">weghen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En leyt my deur sijn <rhyme label="b" type="m" corresp="#s5:l2">hant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sorch-vuldelijck <rhyme label="a" type="f">ghedreven</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En vreest mijn herte <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wat my in desen <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Van nood’ is sal hy <rhyme label="a" type="f" corresp="#s6:l1">geven</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">De sorgh hy my <rhyme label="b" type="m" corresp="#s6:l2">verbiet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Want hy als eenen <rhyme label="a" type="f">wachter</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Rontom my leghert <rhyme label="b" type="m">stil</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Bewaert my voor en <rhyme label="a" type="f" corresp="#s7:l1">achter</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">My hindert geen <rhyme label="a" type="f" corresp="#s7:l1">verachter</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ten sy met sijnen <rhyme label="b" type="m" corresp="#s7:l2">wil</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dat hy my dus <rhyme label="a" type="f">bewaerden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat schijnt een <rhyme label="b" type="m">wonderdaet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Nochtans en valt op <rhyme label="a" type="f" corresp="#s8:l1">aerden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Gheen musken cleyn van <rhyme label="a" type="f" corresp="#s8:l1">waerden</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ten sy dat hy ‘t <rhyme label="b" type="m" corresp="#s8:l2">toelaet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">By hem is niet <rhyme label="a" type="f">vergeten</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Soo cleynen <rhyme label="b" type="m">creatuur</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Al is hy hoogh <rhyme label="a" type="f" corresp="#s9:l1">gheseten</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">‘t Staet al in sijn <rhyme label="a" type="f" corresp="#s9:l1">gheweten</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Persoone, tijt, en <rhyme label="b" type="m" corresp="#s9:l2">uur</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Al woud’ ick hem <rhyme label="a" type="f">ontvlieden</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">In Hemel of in <rhyme label="b" type="m">Hel</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">‘t En can geensins <rhyme label="a" type="f" corresp="#s10:l1">gheschieden</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Hy soude my <rhyme label="a" type="f" corresp="#s10:l1">bespieden</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Met sijn vier’ge oogen <rhyme label="b" type="m" corresp="#s10:l2">snel</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Hy siet op alle <rhyme label="a" type="f">paden</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Der menschen quate en <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Hy wederstaet den <rhyme label="a" type="f" corresp="#s11:l1">quaden</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Maer hy neemt in <rhyme label="a" type="f" corresp="#s11:l1">ghenaden</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Die wandelt in <rhyme label="b" type="m" corresp="#s11:l2">ootmoet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Mijn liefde wil ick <rhyme label="a" type="f">vesten</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Op hem, vry <rhyme label="b" type="m">onvertsaeght</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Want die hem liefst, ten <rhyme label="a" type="f" corresp="#s12:l1">lesten</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Dient alle dinck ten <rhyme label="a" type="f" corresp="#s12:l1">besten</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">‘t Sy hoe ‘t hem oock <rhyme label="b" type="m" corresp="#s12:l2">toedraeght</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
