      <year-start>1755</year-start>
      <year-end>1755</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Engelse fortuin</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Maas-sluysche <rhyme label="a" type="f">Menschen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Lieden <rhyme label="b" type="m">allegaar</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Ik moet u <rhyme label="a" type="f" corresp="#s1:l1">wenschen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Hier een <rhyme label="b" type="m" corresp="#s1:l2">Nieuwe-Iaar</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dat gy, Dat <rhyme label="c" type="m">gy</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In voorspoed leven <rhyme label="d" type="m">meugt</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">En dat elk <rhyme label="c" type="m" corresp="#s1:l5">zy</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In zijn beroep <rhyme label="d" type="m" corresp="#s1:l6">verheugt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">’t Zy <rhyme label="a" type="f">Polityke</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Heeren van <rhyme label="b" type="m">Maas-Sluys</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">Of <rhyme label="a" type="f" corresp="#s2:l1">Kerkelyke</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Dienaars van <rhyme label="b" type="m" corresp="#s2:l2">Gods-Huys</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ik wensch, Ik <rhyme label="c" type="m">wensch</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Haar veel geluk <rhyme label="d" type="m">eenpaar</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Een yder <rhyme label="c" type="m" corresp="#s2:l5">Mensch</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Een zalig <rhyme label="d" type="m" corresp="#s2:l6">Nieuwe-Iaar</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ik wensch de <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Burgemeesters <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Die hier <rhyme label="a" type="f" corresp="#s3:l1">regeeren</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">In ’t <rhyme label="b" type="m" corresp="#s3:l2">Maas-Sluysche-dal</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Dat zy met <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Regeeren voor en <rhyme label="d" type="m">naar</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Met vreed’ en <rhyme label="c" type="m" corresp="#s3:l5">rust</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In ’t zalig <rhyme label="d" type="m" corresp="#s3:l6">Nieuwe-Iaar</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">De Schepens <rhyme label="a" type="f">t’zame</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Gy die d’Regten <rhyme label="b" type="m">wijst</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">‘k Wensch dat gy <rhyme label="a" type="f" corresp="#s4:l1">t’zame</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Tot den Hemel <rhyme label="b" type="m" corresp="#s4:l2">rijst</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Dat rust en <rhyme label="c" type="m">vree</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">By u mag blinken <rhyme label="d" type="m">klaar</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Ik wensch u <rhyme label="c" type="m" corresp="#s4:l5">mee</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Een zalig <rhyme label="d" type="m" corresp="#s4:l6">Nieuwe-Iaar</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">De zeer <rhyme label="a" type="f">Lof-weerde</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Heeren over <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">De <rhyme label="a" type="f" corresp="#s5:l1">Gecommitteerde</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Van de <rhyme label="b" type="m" corresp="#s5:l2">Visschers</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">‘k Wensch u <rhyme label="c" type="m">voorspoed</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En vreedzaamheyd te <rhyme label="d" type="m">gaar</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Dat u <rhyme label="c" type="m" corresp="#s5:l5">ontmoet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Een zalig <rhyme label="d" type="m" corresp="#s5:l6">Nieuwe-Iaar</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Gy Burgers <rhyme label="a" type="f">alle</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">‘k Wensch dat Godt den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Op u laat <rhyme label="a" type="f" corresp="#s6:l1">valle</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Zegen meer en <rhyme label="b" type="m" corresp="#s6:l2">meer</rhyme>!</l>
        <l id="s6:l5" met="2" type="-+">Dat yder <rhyme label="c" type="m">Mensch</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Zijn Neering en <rhyme label="d" type="m">Affaar</rhyme>???,</l>
        <l id="s6:l7" met="2" type="-+">Doet na zijn <rhyme label="c" type="m" corresp="#s6:l5">wensch</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">In ’t zalig <rhyme label="d" type="m" corresp="#s6:l6">Nieuwe-Iaar</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Visschers <rhyme label="a" type="f">gewezen</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">En zeer wijd <rhyme label="b" type="m">vermaard</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Wiens Lot moet <rhyme label="a" type="f" corresp="#s7:l1">wezen</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Ook van mijn <rhyme label="b" type="m" corresp="#s7:l2">verklaard</rhyme>:</l>
        <l id="s7:l5" met="2" type="-+">Godt geeft dat <rhyme label="c" type="m">gy</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Meugt vaaren <rhyme label="d" type="m">allegaar</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Behouden <rhyme label="c" type="m" corresp="#s7:l5">vry</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">In ’t zalig <rhyme label="d" type="m" corresp="#s7:l6">Nieuwe-Iaar</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Oorlof voor ’t <rhyme label="a" type="f">leste</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Het word <rhyme label="b" type="m">scheydens-tijd</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Houd ’t my ten <rhyme label="a" type="f" corresp="#s8:l1">besten</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Ga ik hier niet <rhyme label="b" type="m" corresp="#s8:l2">wijd</rhyme>:</l>
        <l id="s8:l5" met="2" type="-+">Wie gy zijn <rhyme label="c" type="m">meugt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dit is mijn wensch <rhyme label="d" type="m">eenpaar</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Word nieuw in <rhyme label="c" type="m" corresp="#s8:l5">deugd</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">In ’t zalig <rhyme label="d" type="m" corresp="#s8:l6">Nieuwe-Iaar</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
