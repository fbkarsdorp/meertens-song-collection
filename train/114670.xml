    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Bien que je sache que Cloris</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">PHilippe Neri, die 't <rhyme label="a" type="m">begin</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Gheluckigh hebt <rhyme label="b" type="f">ghegheven</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Aen 't Oratorie, in den <rhyme label="a" type="m" corresp="#s1:l1">sin</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Print my u heylich <rhyme label="b" type="f" corresp="#s1:l2">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die dit nieuw liedt singh tot u <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O spieghel aller <rhyme label="b" type="f">deughden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In welcker glans sich wonder <rhyme label="a" type="m" corresp="#s2:l1">seer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Godt en sijn Hof <rhyme label="b" type="f" corresp="#s2:l2">verheughden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Diep is gheweest u <rhyme label="a" type="m">nedricheydt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Waer door ghy hooghe <rhyme label="b" type="f">staten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">U aengheboden, hebt <rhyme label="a" type="m" corresp="#s3:l1">verleyt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Groot erf-deel oock <rhyme label="b" type="f" corresp="#s3:l2">ghelaten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat liefde heeft in u <rhyme label="a" type="m">ghebrandt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die stadich u de <rhyme label="b" type="f">quelen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die uyt des booser Vyandts <rhyme label="a" type="m" corresp="#s4:l1">handt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De zielen u de <rhyme label="b" type="f" corresp="#s4:l2">stelen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Door 't vier der minnen swol u <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Meer plaets Godt dat moest <rhyme label="b" type="f">gheven</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Door 't breken van twee ribben <rhyme label="a" type="m" corresp="#s5:l1">wert</rhyme></l>
        <l id="s5:l4" met="3" type="-+">U borst van hem <rhyme label="b" type="f" corresp="#s5:l2">verheven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Tot d' arme menschen oock <rhyme label="a" type="m">waerdy</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Met liefde heel <rhyme label="b" type="f">bevanghen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Waerom een almoes van u <rhyme label="a" type="m" corresp="#s6:l1">bly</rhyme></l>
        <l id="s6:l4" met="3" type="-+">'Nen Enghel heeft <rhyme label="b" type="f" corresp="#s6:l2">ontfanghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wel seltsaem was u <rhyme label="a" type="m">suyverheyt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat, alsm'u quam <rhyme label="b" type="f">ghenaken</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ghy door den reuck wist <rhyme label="a" type="m" corresp="#s7:l1">onderscheydt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Van ieders staet te <rhyme label="b" type="f" corresp="#s7:l2">maecken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">D' onsuyver mensch stoenck als een <rhyme label="a" type="m">pry</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Aes van de helsche <rhyme label="b" type="f">raven</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">De suyvere u maeckten <rhyme label="a" type="m" corresp="#s8:l1">bly</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">'Nen soeten geur die <rhyme label="b" type="f" corresp="#s8:l2">gaven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Sint Ian Baptist heeft u <rhyme label="a" type="m">gheleert</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Te laten 't eenich <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En u, van iever Godts <rhyme label="a" type="m" corresp="#s9:l1">verheert</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Tot Zielen winst te <rhyme label="b" type="f" corresp="#s9:l2">gheven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Tot dien eynd' gh' oock Priester <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">(Ghelijck u was <rhyme label="b" type="f">bevolen</rhyme>)</l>
        <l id="s10:l3" met="4" type="-+">Door de ghehoorsaemheydt <rhyme label="a" type="m" corresp="#s10:l1">ghewijdt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Ghevolght om niet te <rhyme label="b" type="f" corresp="#s10:l2">dolen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">De Moeder Godts in een claer <rhyme label="a" type="m">licht</rhyme></l>
        <l id="s11:l2" met="3" type="-+">In een swaer sieckt' is <rhyme label="b" type="f">comen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">U troosten met haer soet <rhyme label="a" type="m" corresp="#s11:l1">ghesicht</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">U quael 's heeft wegh <rhyme label="b" type="f" corresp="#s11:l2">ghenomen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Veel sieckten zijn door u <rhyme label="a" type="m">ghebedt</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Oock wonderlijck <rhyme label="b" type="f">ghenesen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">En, teghen den natures <rhyme label="a" type="m" corresp="#s12:l1">wet</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">'Nen dooden is <rhyme label="b" type="f" corresp="#s12:l2">verresen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Recht ons oock op tot Godt den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Die onghereghelt <rhyme label="b" type="f">dalen</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Tot 's wereldts goedt, tot wellust, <rhyme label="a" type="m" corresp="#s13:l1">eer</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Brenght ons in 'shemels <rhyme label="b" type="f" corresp="#s13:l2">palen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
