<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186895</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Broeder Michel</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IK heb Dienst genoomen voor den <rhyme label="a" type="m">Staat</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En wel met goed <rhyme label="a" type="m" corresp="#s1:l1">beraed</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Men hoedt de Vryheid nooit te <rhyme label="a" type="m" corresp="#s1:l1">laat</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als 't aen de keuze <rhyme label="a" type="m" corresp="#s1:l1">staet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Van braeve Borsten in ons <rhyme label="b" type="m">Land</rhyme>;</l>
        <l id="s1:l6" met="4" type="-+">Zo biet men haer het best de <rhyme label="b" type="m" corresp="#s1:l5">hand</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Dit deedt altoos de Oranje <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De Staat en <rhyme label="c" type="m" corresp="#s1:l7">Amsterdam</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">De Britsche magt op 't <rhyme label="a" type="m">Pekelveld</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Moog oefenen haer <rhyme label="a" type="m" corresp="#s2:l1">Geweld</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Zo Trouw en Eendragt ons <rhyme label="a" type="m" corresp="#s2:l1">verzelt</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Blyft alles <rhyme label="a" type="m" corresp="#s2:l1">welgesteld</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Ja, dan hecht de Eer zig aen den <rhyme label="b" type="m">moed</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">Daer men 's Lands Vryheid hulde <rhyme label="b" type="m" corresp="#s2:l5">doet</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Dit deet altoos de Oranje <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">De Staet en <rhyme label="c" type="m" corresp="#s2:l7">Amsterdam</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">De Vryheid, dat zo dierhaer <rhyme label="a" type="m">Pand</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Voor 't zaemverbonden <rhyme label="a" type="m" corresp="#s3:l1">Land</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Eischt thans van aller Burgren <rhyme label="a" type="m" corresp="#s3:l1">hand</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Getrouwe <rhyme label="a" type="m" corresp="#s3:l1">onderstand</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Zy kreeg dien steeds in vroeger <rhyme label="b" type="m">Tyd</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Toen elk voor haer zig bood ten <rhyme label="b" type="m" corresp="#s3:l5">stryd</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Zo deed altoos de Oranje <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">De Staet en <rhyme label="c" type="m" corresp="#s3:l7">Amsterdam</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Haer Roem, zo hoog, zo wyd <rhyme label="a" type="m">vermaard</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als Neerland is op <rhyme label="a" type="m" corresp="#s4:l1">Aard</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Acht ik myn bloed, myn leven <rhyme label="a" type="m" corresp="#s4:l1">waerd</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">En vlieg door Vuur en <rhyme label="a" type="m" corresp="#s4:l1">Zwaerd</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Op 't Britsch Geweld grootmoedig <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ach! had een ieder myn <rhyme label="b" type="m" corresp="#s4:l5">bestaen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Op 't voorbeeld van d'Oranje <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Den Staet en <rhyme label="c" type="m" corresp="#s4:l7">Amsterdam</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">De uitwerking van den Britschen <rhyme label="a" type="m">hoon</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Verstrekk'zig-zelv'ten <rhyme label="a" type="m" corresp="#s5:l1">loon</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Verdrukte Deugd behael de <rhyme label="a" type="m" corresp="#s5:l1">Kroon</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">En stell' zig bly ten <rhyme label="a" type="m" corresp="#s5:l1">toon</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Voor 't oog van waere <rhyme label="b" type="m">Onzydigheid</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Door 't beste Staets- en <rhyme label="b" type="m" corresp="#s5:l5">Krygsbeleid</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Van Nassaus en Oranjes <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Den Staet en <rhyme label="c" type="m" corresp="#s5:l7">Amsterdam</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ik wacht dit groot geluk reeds <rhyme label="a" type="m">af</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Tot aller Britten <rhyme label="a" type="m" corresp="#s6:l1">straf</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Ik zal, wat my het Lot ook <rhyme label="a" type="m" corresp="#s6:l1">gav'</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Niet weezen laeg noch <rhyme label="a" type="m" corresp="#s6:l1">laf</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Kom volg my, Nederlandsche <rhyme label="b" type="m">Jeugd</rhyme>!</l>
        <l id="s6:l6" met="4" type="-+">En stry voor Vryheid en voor <rhyme label="b" type="m" corresp="#s6:l5">Deugd</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Dan eert gy den Oranje <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Den Staet en <rhyme label="c" type="m" corresp="#s6:l7">Amsterdam</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
