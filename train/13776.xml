        <l id="s1:l4" met="4" type="-+">Wt mijnen sin noyt <rhyme label="c" type="m">Herderin</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En wijcken <rhyme label="b" type="f" corresp="#s1:l3">deden</rhyme>.</l>
