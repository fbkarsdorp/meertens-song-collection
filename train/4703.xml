<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4703</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O Heer God van boven</melody>
  <author>Joannes Stalpaert van der Wiele</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Goddelijke <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En schiepen ons de <rhyme label="b" type="m">dood</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Noch vlammen om te <rhyme label="a" type="f" corresp="#s1:l1">branden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Noch zonde kleyn of <rhyme label="b" type="m" corresp="#s1:l2">groot</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Neygt handen en <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Na Goods <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Want waerder geen <rhyme label="c" type="m" corresp="#s1:l5">zond</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Daer waer geen <rhyme label="d" type="m" corresp="#s1:l6">hel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">God schiep ons nae zijn <rhyme label="a" type="f">wapen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met wesmen weet, of <rhyme label="b" type="m">ziet</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">En zonder hem <rhyme label="a" type="f" corresp="#s2:l1">geschapen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Niet anders is als <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Neygt handen en <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Na Goods <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Want waerder geen <rhyme label="c" type="m" corresp="#s2:l5">zond</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Daer waer geen <rhyme label="d" type="m" corresp="#s2:l6">hel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Het niet zeg ik der <rhyme label="a" type="f">zonden</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Want daer en werd geen <rhyme label="b" type="m">naem</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Voort yd’le spook <rhyme label="a" type="f" corresp="#s3:l1">gevonden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Zoo eigen en <rhyme label="b" type="m" corresp="#s3:l2">bequaem</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Neygt handen en <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Na Goods <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Want waerder geen <rhyme label="c" type="m" corresp="#s3:l5">zond</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Daer waer geen <rhyme label="d" type="m" corresp="#s3:l6">hel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Almagtig is den <rhyme label="a" type="f">Vader</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">Zijn wil is ’t hoogste <rhyme label="b" type="m">goed</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Maer ’t is de sno’e mis <rhyme label="a" type="f" corresp="#s4:l1">dader</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die ’t niet der zonden <rhyme label="b" type="m" corresp="#s4:l2">doed</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Neygt handen en <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Na Goods <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Want waerder geen <rhyme label="c" type="m" corresp="#s4:l5">zond</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Daer waer geen <rhyme label="d" type="m" corresp="#s4:l6">hel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">God is de zaek der <rhyme label="a" type="f">zaken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">End’ altijd even <rhyme label="b" type="m">sterk</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Mids dien en kon hy <rhyme label="a" type="f" corresp="#s5:l1">maken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ook noyt mis dadig <rhyme label="b" type="m" corresp="#s5:l2">werk</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Neygt handen en <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Na Goods <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Want waerder geen <rhyme label="c" type="m" corresp="#s5:l5">zond</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Daer waer geen <rhyme label="d" type="m" corresp="#s5:l6">hel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Laet u dan niet <rhyme label="a" type="f">versteken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hier van, door ’t nieu <rhyme label="b" type="m">gesprek</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Want die noit kon <rhyme label="a" type="f" corresp="#s6:l1">gebreken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En dee ook noit <rhyme label="b" type="m" corresp="#s6:l2">gebrek</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Neygt handen en <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Na Goods <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Want waerder geen <rhyme label="c" type="m" corresp="#s6:l5">zond</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Daer waer geen <rhyme label="d" type="m" corresp="#s6:l6">hel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy haet o Heer der <rhyme label="a" type="f">Heren</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Den Boos wigt, en ‘tmis <rhyme label="b" type="m">daed</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Dies ik, die <rhyme label="a" type="f" corresp="#s7:l1">blasphemeren</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Wil, blasphemeren <rhyme label="b" type="m" corresp="#s7:l2">laet</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">Neygt handen en <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Na Goods <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Want waerder geen <rhyme label="c" type="m" corresp="#s7:l5">zond</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Daer waer geen <rhyme label="d" type="m" corresp="#s7:l6">hel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
