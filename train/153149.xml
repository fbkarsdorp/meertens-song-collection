<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>153149</id>
    <date>
      <year-start>1612</year-start>
      <year-end>1612</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Aanhoor toch mijn geklaag gij ruiters</melody>
  <author>J. Jacobsz. Harlingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL ist <rhyme label="a" type="m">ghepropheteert</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat God zijn volck sal <rhyme label="b" type="f">wreken</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Al wortet seer <rhyme label="a" type="m" corresp="#s1:l1">begeert</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van zijn Heylighen <rhyme label="a" type="m" corresp="#s1:l1">weert</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ondert altaer <rhyme label="a" type="m" corresp="#s1:l1">gheeert</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Nochtans wort ons <rhyme label="a" type="m" corresp="#s1:l1">gheleert</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Bidt voor haer die u <rhyme label="b" type="f" corresp="#s1:l2">steken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Bedroeft ist herte <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Mijn siele moet nu <rhyme label="b" type="f">schreyen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Want Gods toornich <rhyme label="a" type="m" corresp="#s2:l1">aenschijn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Blijckt claer op dit <rhyme label="a" type="m" corresp="#s2:l1">termijn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ghy blijft al even <rhyme label="a" type="m" corresp="#s2:l1">blijn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Quaet nydich , vol <rhyme label="a" type="m" corresp="#s2:l1">fenijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Zijn straffe wil niet <rhyme label="b" type="f" corresp="#s2:l2">beyen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Cont ghy dit niet <rhyme label="a" type="m">verstaen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat zijn bedroefde <rhyme label="b" type="f">saken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dit brenghen en dit <rhyme label="a" type="m" corresp="#s3:l1">slaen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer soo veel door <rhyme label="a" type="m" corresp="#s3:l1">vergaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Waerom dattet wort <rhyme label="a" type="m" corresp="#s3:l1">ghedaen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wilt ghy noch dus <rhyme label="a" type="m" corresp="#s3:l1">voortgaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Gods plaghe wil u <rhyme label="b" type="f" corresp="#s3:l2">raken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Denckt wat gheschreven <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wie dat Gods volck wil <rhyme label="b" type="f">krencken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">God selven hy <rhyme label="a" type="m" corresp="#s4:l1">versmaet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En in zijn ooch hy <rhyme label="a" type="m" corresp="#s4:l1">slaet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ghy boose Cains <rhyme label="a" type="m" corresp="#s4:l1">zaet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Keert weer eert wort te <rhyme label="a" type="m" corresp="#s4:l1">laet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Gods oordeel wilt <rhyme label="b" type="f" corresp="#s4:l2">bedencken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Esekiel <rhyme label="a" type="m">diespreekt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Teghen veel Heydenen <rhyme label="b" type="f">koene</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die eene die <rhyme label="a" type="m" corresp="#s5:l1">begeckt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Gods heylichdom <rhyme label="a" type="m" corresp="#s5:l1">perfeckt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die ander wreeckt en <rhyme label="a" type="m" corresp="#s5:l1">steekt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die derde dit <rhyme label="a" type="m" corresp="#s5:l1">ontdeckt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Sy zijn van onsen <rhyme label="b" type="f" corresp="#s5:l2">doene</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Foortaen een yeder <rhyme label="a" type="m">boet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Zijn oude haet met <rhyme label="b" type="f">lusten</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Zy rasen als <rhyme label="a" type="m" corresp="#s6:l1">verwoet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En treden onder de <rhyme label="a" type="m" corresp="#s6:l1">voet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Die haer doch niet <rhyme label="a" type="m" corresp="#s6:l1">misdoet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Met quaet loonense <rhyme label="a" type="m" corresp="#s6:l1">goet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Haer boosheyt wil niet  <rhyme label="b" type="f" corresp="#s6:l2">rusten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghelijck alle met <rhyme label="a" type="m">een</rhyme></l>
        <l id="s7:l2" met="3" type="-+">.Edom en <rhyme label="b" type="f">d'Ismaliten</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Moab die <rhyme label="a" type="m" corresp="#s7:l1">Hagareen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ammon de <rhyme label="a" type="m" corresp="#s7:l1">Gabaleen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Amaleck <rhyme label="a" type="m" corresp="#s7:l1">Philisteen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Tirus Assar <rhyme label="a" type="m" corresp="#s7:l1">onreen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Teghen <rhyme label="b" type="f" corresp="#s7:l2">d'Israeliten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Haer laghen listich <rhyme label="a" type="m">fel</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Maken sy int <rhyme label="b" type="f">verborghen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wy willen met <rhyme label="a" type="m" corresp="#s8:l1">ghequel</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Dat gantsche <rhyme label="a" type="m" corresp="#s8:l1">Israel</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Wtroeyen alsoo <rhyme label="a" type="m" corresp="#s8:l1">snel</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">T'sal ons ghelucken <rhyme label="a" type="m" corresp="#s8:l1">wel</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Wy willense <rhyme label="b" type="f" corresp="#s8:l2">verworghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">In uwen toorne <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Achtse van gheender <rhyme label="b" type="f">waerden</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Werptse badt David <rhyme label="a" type="m" corresp="#s9:l1">seer</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Als Midiam ter <rhyme label="a" type="m" corresp="#s9:l1">neer</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Als Sissera vol <rhyme label="a" type="m" corresp="#s9:l1">eer</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Met Iabin zynen <rhyme label="a" type="m" corresp="#s9:l1">Heer</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Maecktse tot dreck op <rhyme label="b" type="f" corresp="#s9:l2">aerden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Kort af haer Vorsten <rhyme label="a" type="m">al</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ghelijck Seb ende <rhyme label="b" type="f">Oreb</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Haer Oversten <rhyme label="a" type="m" corresp="#s10:l1">ghetal</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Als Salmuna en <rhyme label="a" type="m" corresp="#s10:l1">Sebal</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Soo brengtse Heer ten <rhyme label="a" type="m" corresp="#s10:l1">val</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Die haer roemen doch <rhyme label="a" type="m" corresp="#s10:l1">mal</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Gods huys hier te <rhyme label="b" type="f" corresp="#s10:l2">verstooren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Laet uwe <rhyme label="a" type="m">rechterhand</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Noch uytghestrecket <rhyme label="b" type="f">blyven</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Verdoetse aen elcken <rhyme label="a" type="m" corresp="#s11:l1">kant</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Wt u heylighe <rhyme label="a" type="m" corresp="#s11:l1">Lant</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Ghelijck de Wint het <rhyme label="a" type="m" corresp="#s11:l1">Zant</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Als een Wout dat <rhyme label="a" type="m" corresp="#s11:l1">verbrant</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Soo wiltse Heer <rhyme label="b" type="f" corresp="#s11:l2">verdryven</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Maer wilt ghy, spreeckt de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Nae mynen naem niet <rhyme label="b" type="f">vraghen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En doen na myne <rhyme label="a" type="m" corresp="#s12:l1">Leer</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Sevenmael wil ick <rhyme label="a" type="m" corresp="#s12:l1">meer</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Mijn plaghen senden <rhyme label="a" type="m" corresp="#s12:l1">neer</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Tot dat ick u <rhyme label="a" type="m" corresp="#s12:l1">omkeer</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Want ick doe mijn <rhyme label="b" type="f" corresp="#s12:l2">behaghen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Neemt waer, 't wort u <rhyme label="a" type="m">ghesecht</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Al wat God heeft <rhyme label="b" type="f">ghesproken</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Ghy zijt den Heer of <rhyme label="a" type="m" corresp="#s13:l1">knecht</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Verheven ofte <rhyme label="a" type="m" corresp="#s13:l1">slecht</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Het blijft vast en <rhyme label="a" type="m" corresp="#s13:l1">oprecht</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Deuchde wort wel <rhyme label="a" type="m" corresp="#s13:l1">belecht</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Maar boosheyt wort <rhyme label="b" type="f" corresp="#s13:l2">ghewroken</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Och denckt hoe 't toe sal <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Als Iesus hooch <rhyme label="b" type="f">verheven</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Ten gherechte sal <rhyme label="a" type="m" corresp="#s14:l1">staen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Met Enghelen <rhyme label="a" type="m" corresp="#s14:l1">ombevaen</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Met crachten <rhyme label="a" type="m" corresp="#s14:l1">aenghedaen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">En wy sullen <rhyme label="a" type="m" corresp="#s14:l1">ontfaen</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Een eyghelijck nae zijn <rhyme label="b" type="f" corresp="#s14:l2">leven</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Plat isset woort hoor <rhyme label="a" type="m">reen</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Dat God wrake sal <rhyme label="b" type="f">gheven</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Druck die u druck <rhyme label="a" type="m" corresp="#s15:l1">aendeen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">En haer breken van <rhyme label="a" type="m" corresp="#s15:l1">een</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Maer ghy die met ons <rhyme label="a" type="m" corresp="#s15:l1">leen</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Rustet met ons in <rhyme label="a" type="m" corresp="#s15:l1">vreen</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Hier nae int eeuwighe <rhyme label="b" type="f" corresp="#s15:l2">leven</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Quellinghen <rhyme label="a" type="m">onsoet</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Sullen sy moeten <rhyme label="b" type="f">lyden</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Die hem nu niet en <rhyme label="a" type="m" corresp="#s16:l1">spoet</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Te doen oprecht <rhyme label="a" type="m" corresp="#s16:l1">boet</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Maer wech stoot metter <rhyme label="a" type="m" corresp="#s16:l1">voet</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Gods Euangelium <rhyme label="a" type="m" corresp="#s16:l1">goet</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">En nimmermeer <rhyme label="b" type="f" corresp="#s16:l2">verblyden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Roepen sullen sy <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Berghen wilt ons <rhyme label="b" type="f">bedecken</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Ghy heuvelen valt <rhyme label="a" type="m" corresp="#s17:l1">neer</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Gods toorne van ons <rhyme label="a" type="m" corresp="#s17:l1">keer</rhyme></l>
        <l id="s17:l5" met="3" type="-+">Die wy nu sien van <rhyme label="a" type="m" corresp="#s17:l1">veer</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Wat helpt ons nu ons <rhyme label="a" type="m" corresp="#s17:l1">eer</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Wy ontsinnighe <rhyme label="b" type="f" corresp="#s17:l2">gecken</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Sonder een ware <rhyme label="a" type="m">boet</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Machmen dit niet <rhyme label="b" type="f">ontcomen</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Hier ghelt gheen tijtlijck <rhyme label="a" type="m" corresp="#s18:l1">goet</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Mis metten Lofsanck <rhyme label="a" type="m" corresp="#s18:l1">soet</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Noch water, sweert of <rhyme label="a" type="m" corresp="#s18:l1">gloet</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Noch Christus dierbaer <rhyme label="a" type="m" corresp="#s18:l1">bloet</rhyme></l>
        <l id="s18:l7" met="3" type="-+">Hoe hooch datment oock <rhyme label="b" type="f" corresp="#s18:l2">roemen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Tot u sonder <rhyme label="a" type="m">ghetal</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">Huychelaers, <rhyme label="b" type="f">ypocryten</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Propheten van <rhyme label="a" type="m" corresp="#s19:l1">Baal</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Priesters van <rhyme label="a" type="m" corresp="#s19:l1">Belial</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Dienaers van den <rhyme label="a" type="m" corresp="#s19:l1">afval</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Diet verwoest over <rhyme label="a" type="m" corresp="#s19:l1">al</rhyme>,</l>
        <l id="s19:l7" met="3" type="-+">Tot u is onse <rhyme label="b" type="f" corresp="#s19:l2">cryten</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">V selven eens <rhyme label="a" type="m">kent</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">Ghy moet doch eenmael <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Dan wort u vreucht <rhyme label="a" type="m" corresp="#s20:l1">ghewent</rhyme></l>
        <l id="s20:l4" met="3" type="-+">In droeffelijck <rhyme label="a" type="m" corresp="#s20:l1">ellent</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">V eere wort <rhyme label="a" type="m" corresp="#s20:l1">gheschent</rhyme>,,</l>
        <l id="s20:l6" met="3" type="-+">V hoochmoet heeft een <rhyme label="a" type="m" corresp="#s20:l1">ent</rhyme>,</l>
        <l id="s20:l7" met="3" type="-+">Worm en Mot is u <rhyme label="b" type="f" corresp="#s20:l2">erven</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Xerxes daet <rhyme label="a" type="m">aensiet</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Door Hamans list <rhyme label="b" type="f">ghevanghen</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">Soo deed' hy een <rhyme label="a" type="m" corresp="#s21:l1">onthiet</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Maer doent hem was <rhyme label="a" type="m" corresp="#s21:l1">bediet</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Dit volck hindert u <rhyme label="a" type="m" corresp="#s21:l1">niet</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">Het doet niemant <rhyme label="a" type="m" corresp="#s21:l1">verdriet</rhyme>,</l>
        <l id="s21:l7" met="3" type="-+">Doe moest haer vyandt <rhyme label="b" type="f" corresp="#s21:l2">hanghen</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Yvert doch eens nae 't <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">God wilt alleen <rhyme label="b" type="f">aenbeden</rhyme>,</l>
        <l id="s22:l3" met="3" type="-+">En zijn Heylighen <rhyme label="a" type="m" corresp="#s22:l1">vroet</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Wilt dienen metter <rhyme label="a" type="m" corresp="#s22:l1">spoet</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Den hongherighen <rhyme label="a" type="m" corresp="#s22:l1">voet</rhyme>,</l>
        <l id="s22:l6" met="3" type="-+">En uwen naesten <rhyme label="a" type="m" corresp="#s22:l1">doet</rhyme></l>
        <l id="s22:l7" met="3" type="-+">Soo ghy wout datmen u <rhyme label="b" type="f" corresp="#s22:l2">deden</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Zee, Landt, Hemel en <rhyme label="a" type="m">Eerd</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Heeft God alleen <rhyme label="b" type="f">gheschapen</rhyme>,</l>
        <l id="s23:l3" met="3" type="-+">Van hem ghenaed <rhyme label="a" type="m" corresp="#s23:l1">begeert</rhyme>,</l>
        <l id="s23:l4" met="3" type="-+">Hem looft, danckt ende <rhyme label="a" type="m" corresp="#s23:l1">eert</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Zijn Woort laet zijn u <rhyme label="a" type="m" corresp="#s23:l1">sweert</rhyme>,</l>
        <l id="s23:l6" met="3" type="-+">Wort eens te recht <rhyme label="a" type="m" corresp="#s23:l1">formeert</rhyme>,</l>
        <l id="s23:l7" met="3" type="-+">Op dat ghy zijt zijn <rhyme label="b" type="f" corresp="#s23:l2">knapen</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Ellijck heeft eenen <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s24:l2" met="3" type="-+">Die sal niet connen <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s24:l3" met="3" type="-+">Wie nu s'Hemels <rhyme label="a" type="m" corresp="#s24:l1">pryiel</rhyme></l>
        <l id="s24:l4" met="3" type="-+">Van hem stoot metter <rhyme label="a" type="m" corresp="#s24:l1">hiel</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Praelt met Sijd en <rhyme label="a" type="m" corresp="#s24:l1">Fluwiel</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">Oft dat yemant <rhyme label="a" type="m" corresp="#s24:l1">gheviel</rhyme>,</l>
        <l id="s24:l7" met="3" type="-+">Die poel sal zijn zijn <rhyme label="b" type="f" corresp="#s24:l2">erven</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Contrary salt <rhyme label="a" type="m">gheschien</rhyme></l>
        <l id="s25:l2" met="3" type="-+">Met haer die hier nu <rhyme label="b" type="f">lyden</rhyme></l>
        <l id="s25:l3" met="3" type="-+">Om Godes Woort <rhyme label="a" type="m" corresp="#s25:l1">allien</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">d'Afgoderie <rhyme label="a" type="m" corresp="#s25:l1">vlien</rhyme></l>
        <l id="s25:l5" met="3" type="-+">Consenterender <rhyme label="a" type="m" corresp="#s25:l1">gien</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">Want sy sullen <rhyme label="a" type="m" corresp="#s25:l1">mitsdien</rhyme></l>
        <l id="s25:l7" met="3" type="-+">In eeuwicheyt <rhyme label="b" type="f" corresp="#s25:l2">verblyden</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Est dat yemant <rhyme label="a" type="m">bekent</rhyme></l>
        <l id="s26:l2" met="3" type="-+">Dat wy hier nu <rhyme label="b" type="f">verclaren</rhyme>,</l>
        <l id="s26:l3" met="3" type="-+">En hem van boosheyt <rhyme label="a" type="m" corresp="#s26:l1">went</rhyme>,</l>
        <l id="s26:l4" met="3" type="-+">V Gheest Heer in hem <rhyme label="a" type="m" corresp="#s26:l1">sent</rhyme>,</l>
        <l id="s26:l5" met="3" type="-+">Blijft hem altijt <rhyme label="a" type="m" corresp="#s26:l1">ontrent</rhyme>,</l>
        <l id="s26:l6" met="3" type="-+">Nu heeft dit Liedt een <rhyme label="a" type="m" corresp="#s26:l1">ent</rhyme>,</l>
        <l id="s26:l7" met="3" type="-+">O Heer wilt ons <rhyme label="b" type="f" corresp="#s26:l2">bewaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
