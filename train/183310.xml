<song>
  <meta>
    <id>183310</id>
    <date>
      <year-start>1752</year-start>
      <year-end>1752</year-end>
    </date>
    <location>
      <orginal-name>gorinchem</orginal-name>
      <standard-name>Gorinchem, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8296133</lon>
        <lat>4.9738743</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Gavotte à la fronte</melody>
  <author>Jacob Groenewegen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Vader Magtig Wijs en <rhyme label="a" type="m">Goed</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die in kruys en <rhyme label="a" type="m" corresp="#s1:l1">tegenspoet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Hebt en houd u liefde <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Op u Volk in hun <rhyme label="c" type="m">elend</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En die met hun smert <rhyme label="b" type="f" corresp="#s1:l3">bewoogen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">U in gunste tot haar <rhyme label="c" type="m" corresp="#s1:l4">wend</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die met medelyden <rhyme label="a" type="m">ziet</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Al de moeyte en <rhyme label="a" type="m" corresp="#s2:l1">verdriet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die zy hebben in dit <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Al haar zugten en <rhyme label="c" type="m">getraan</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Tot zy ’t in zijn handen <rhyme label="b" type="f" corresp="#s2:l3">geeven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En geloovig tot hem <rhyme label="c" type="m" corresp="#s2:l4">gaan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Og ik voele tot mijn <rhyme label="a" type="m">smert</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Mijn verkeerd en zondig <rhyme label="a" type="m" corresp="#s3:l1">hert</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Onverloochend in u <rhyme label="b" type="f">weegen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En tot twisten slechs <rhyme label="c" type="m">bereyd</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Heere maak mijn hert <rhyme label="b" type="f" corresp="#s3:l3">geneegen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Tot den weg die gy my <rhyme label="c" type="m" corresp="#s3:l4">leyd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">’K wil al is ’t my nut en <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Aan geen weg van <rhyme label="a" type="m" corresp="#s4:l1">tegenspoet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En ik schrik en vrees voor <rhyme label="b" type="f">banden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Schoon ’t my nog maar word <rhyme label="c" type="m">gedreygd</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Zalig die zig in u <rhyme label="b" type="f" corresp="#s4:l3">handen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Geeft, en altijd Gode <rhyme label="c" type="m" corresp="#s4:l4">zwijgt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">O mijn eygen wil en <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Wil geen andere weegen <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als daar ’t vlees in heeft <rhyme label="b" type="f">behagen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Eer genoegen vryheid <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Al mijn tijd en <rhyme label="b" type="f" corresp="#s5:l3">levensdagen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Leeven enkel in <rhyme label="c" type="m" corresp="#s5:l4">geneugt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Souvereyne Hemel <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Og mogt ik het gantsch <rhyme label="a" type="m" corresp="#s6:l1">regeer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En besinne van alle <rhyme label="b" type="f">zaaken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Stellen aan u wyzen <rhyme label="c" type="m">raad</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">En geloove God zal ’t <rhyme label="b" type="f" corresp="#s6:l3">maken</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Schoon my treften alle <rhyme label="c" type="m" corresp="#s6:l4">kwaad</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Strydig was het met u <rhyme label="a" type="m">trouw</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Dat gy my verlaten <rhyme label="a" type="m" corresp="#s7:l1">zouw</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Want gy geeft den moeden <rhyme label="b" type="f">kragten</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En verlost op uwen <rhyme label="c" type="m">tijd</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Dikwils als wy ’t minste <rhyme label="b" type="f" corresp="#s7:l3">verwagten</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Tot u roem en <rhyme label="c" type="m" corresp="#s7:l4">heerlijkheid</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Veylig mag u lieve <rhyme label="a" type="m">Volk</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Schoon ’er is een donkere <rhyme label="a" type="m" corresp="#s8:l1">wolk</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Haar belangens op u <rhyme label="b" type="f">wagen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Goede Meester leer het <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Want gy hebt dog alle <rhyme label="b" type="f" corresp="#s8:l3">dagen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Toegezeyd by mij te <rhyme label="c" type="m" corresp="#s8:l4">zijn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Ia mijn Volk dat is u <rhyme label="a" type="m">woord</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">’K heb u eer gy roept <rhyme label="a" type="m" corresp="#s9:l1">gehoord</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En in u benaauwde <rhyme label="b" type="f">tyden</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Is mijn ingewant <rhyme label="c" type="m">ontroert</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Hoe zou ik dan hunne <rhyme label="b" type="f" corresp="#s9:l3">lyden</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dat gy moede <rhyme label="c" type="m" corresp="#s9:l4">weggevoerd</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">En verlaten leeven <rhyme label="a" type="m">zou</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Neen ik zweer u myne <rhyme label="a" type="m" corresp="#s10:l1">trouw</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Dat ik u gewis zal <rhyme label="b" type="f">leyden</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Na mijn raad in alle <rhyme label="c" type="m">kruys</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Tot dat gy van hier zult <rhyme label="b" type="f" corresp="#s10:l3">scheyden</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">En ik u zal halen <rhyme label="c" type="m" corresp="#s10:l4">t’huys</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Nu mijn ziel onthoud dat <rhyme label="a" type="m">Woord</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Uyt des Heeren mond <rhyme label="a" type="m" corresp="#s11:l1">gehoord</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">En zoek door geloof en <rhyme label="b" type="f">hopen</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Moedig en met <rhyme label="c" type="m">lydzaamheid</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Regt op Canaan aan te <rhyme label="b" type="f" corresp="#s11:l3">loopen</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Tot het eynde van u <rhyme label="c" type="m" corresp="#s11:l4">strijd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
