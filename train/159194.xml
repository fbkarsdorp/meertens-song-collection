<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159194</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon lief wil mij troost geven</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOort Ian-Neef, macht u <rhyme label="a" type="f">beuren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Spreeck vry, en wilt niet <rhyme label="a" type="f" corresp="#s1:l1">treuren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Om stracks te varen <rhyme label="b" type="m">uyt</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Wy sullen t'samen <rhyme label="c" type="f">Vissen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">De Sood kan ons niet <rhyme label="c" type="f" corresp="#s1:l4">missen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Een Yeder in sijn <rhyme label="b" type="m" corresp="#s1:l3">Schuyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dit dienje noch te <rhyme label="a" type="f">weten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Maeck, sonder te <rhyme label="a" type="f" corresp="#s2:l1">vergeten</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">De wijde Schakel <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ick sal de nauwe <rhyme label="c" type="f">keuren</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En Visschen sonder <rhyme label="c" type="f" corresp="#s2:l4">steuren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Gedurich aen <rhyme label="b" type="m" corresp="#s2:l3">malkaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Kom laet ons buyten <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wy sullen t'saem wel <rhyme label="a" type="f" corresp="#s3:l1">klaren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En zeylen om de <rhyme label="b" type="m">West</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dan 't nachjen over <rhyme label="c" type="f">loeren</rhyme></l>
        <l id="s3:l5" met="3" type="-+">In 't Hooy al by de <rhyme label="c" type="f" corresp="#s3:l4">Boeren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Of in des Ooy-vaers <rhyme label="b" type="m" corresp="#s3:l3">Nest</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De dagh komt aen te <rhyme label="a" type="f">breken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De duyster is <rhyme label="a" type="f" corresp="#s4:l1">geweken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De Son komt voor den <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ons Schakeltjes te <rhyme label="c" type="f">water</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Stoot aen met groot <rhyme label="c" type="f" corresp="#s4:l4">geklater</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">So stijf als yeder <rhyme label="b" type="m" corresp="#s4:l3">mach</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sie daer, sie daer, 't is <rhyme label="a" type="f">wonder</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hoe gaen de Bollen <rhyme label="a" type="f" corresp="#s5:l1">onder</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Bedraey, het is een <rhyme label="b" type="m">Snoeck</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Bedraey, ey wilje <rhyme label="c" type="f">haesten</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Of hy ontsnapt ten <rhyme label="c" type="f" corresp="#s5:l4">quaetsten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In d'een of d'ander <rhyme label="b" type="m" corresp="#s5:l3">hoeck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hael op, daer komt hy <rhyme label="a" type="f">stuyven</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hier valt wat aen te <rhyme label="a" type="f" corresp="#s6:l1">kluyven</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hy is wel drie voet <rhyme label="b" type="m">langh</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Ses Baersen met een <rhyme label="c" type="f">Seeltjen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En noch een groot <rhyme label="c" type="f" corresp="#s6:l4">gedeeltjen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Van Voorens in dees <rhyme label="b" type="m" corresp="#s6:l3">gangh</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hier moeten wy wat <rhyme label="a" type="f">blijven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Siet ghy het Kroos wel <rhyme label="a" type="f" corresp="#s7:l1">drijven</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Daer Katte-steerten <rhyme label="b" type="m">by</rhyme>?</l>
        <l id="s7:l4" met="3" type="-+">Dat moeten wy <rhyme label="c" type="f">besetten</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Met onser beyder <rhyme label="c" type="f" corresp="#s7:l4">Netten</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">'t Is goede <rhyme label="b" type="m" corresp="#s7:l3">Schaecklery</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hoe staet ghy dus te <rhyme label="a" type="f">dromen</rhyme>?</l>
        <l id="s8:l2" met="3" type="-+">Sal juyst de Pachter <rhyme label="a" type="f" corresp="#s8:l1">komen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">'t Zy Pieter of een <rhyme label="b" type="m">aer</rhyme>?</l>
        <l id="s8:l4" met="3" type="-+">Ey wilt daerom niet <rhyme label="c" type="f">dencken</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">De Wijn zal hy ons <rhyme label="c" type="f" corresp="#s8:l4">schencken</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dan sijn ons saken <rhyme label="b" type="m" corresp="#s8:l3">klaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Laet ons de Schakels <rhyme label="a" type="f">spoelen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">De Wind begint te <rhyme label="a" type="f" corresp="#s9:l1">koelen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En zeylen nae <rhyme label="b" type="m">Enchuys</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ons Vis mocht anders <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">En al-te-mael <rhyme label="c" type="f" corresp="#s9:l4">verderven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Al eer wy komen 't <rhyme label="b" type="m" corresp="#s9:l3">Huys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
