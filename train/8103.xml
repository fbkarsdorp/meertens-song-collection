    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">Geestelijk lied</tag>
    </tags>
    <melody>Psalm 006 Datheen</melody>
    <categories/>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LEert my o Heer <rhyme label="a" type="f">bedincken</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hooghmoet te laten <rhyme label="a" type="f" corresp="#s1:l1">sincken</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ick moet sterven <rhyme label="b" type="m">haest</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">De doodt komt my <rhyme label="c" type="f">bestrijden</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Haer wreetheyt moet ick <rhyme label="c" type="f" corresp="#s1:l4">lijden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Verschrickt, en seer <rhyme label="b" type="m" corresp="#s1:l3">verbaest</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Kort is des menschen <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Een handtbreet hier <rhyme label="a" type="f" corresp="#s2:l1">beneven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Vol moeyt, en jammer <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Altijdt strijdt sonder <rhyme label="c" type="f">ende</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Waer ick my keer oft <rhyme label="c" type="f" corresp="#s2:l4">wende</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ontmoet my veel <rhyme label="b" type="m" corresp="#s2:l3">aenstoot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wie sal konnen <rhyme label="a" type="f">vertellen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Het menighvuldigh <rhyme label="a" type="f" corresp="#s3:l1">quellen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat hier den mensch <rhyme label="b" type="m">aenkleeft</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">Gantsch onvolmaeckt <rhyme label="c" type="f">bevonden</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Is hy geneyght tot <rhyme label="c" type="f" corresp="#s3:l4">sonden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Van selfs niet goets en <rhyme label="b" type="m" corresp="#s3:l3">heeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ia niet een goet <rhyme label="a" type="f">ghedachte</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Den mensch uyt eyghen <rhyme label="a" type="f" corresp="#s4:l1">krachte</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En is bequame <rhyme label="b" type="m">niet</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Hoe veel meer <rhyme label="c" type="f">onbequame</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Goet te doen, naer <rhyme label="c" type="f" corresp="#s4:l4">betame</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Sonder Godts hulpe, <rhyme label="b" type="m" corresp="#s4:l3">siet</rhyme>;</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Godt alleen // goet // <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In 't ghemeen // moet // by <rhyme label="a" type="f" corresp="#s5:l1">desen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Alles in allen <rhyme label="b" type="m">zijn</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Maer wat is doch den <rhyme label="c" type="f">mensche</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">Niet bysonders ten <rhyme label="c" type="f" corresp="#s5:l4">wensche</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Aerde voor Godts <rhyme label="b" type="m" corresp="#s5:l3">aenschijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Als een gras bloeme <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Verciert staende ten <rhyme label="a" type="f" corresp="#s6:l1">toone</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Afvalt, haest moet <rhyme label="b" type="m">vergaen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Sulcx is des mensche <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ghelijck, maer soo wy <rhyme label="c" type="f" corresp="#s6:l4">lesen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Godts woort sal blijven <rhyme label="b" type="m" corresp="#s6:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Godts woordt beleven <rhyme label="a" type="f">goedigh</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Barmhertigh, en <rhyme label="a" type="f" corresp="#s7:l1">ootmoedigh</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Christum volghende <rhyme label="b" type="m">naer</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Moeten wy al te <rhyme label="c" type="f">samen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En schouwen 't quaedt <rhyme label="c" type="f" corresp="#s7:l4">bynamen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Gierigheydt, hooghmoedt <rhyme label="b" type="m" corresp="#s7:l3">swaer</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wortel van alle <rhyme label="a" type="f">quaden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gierigh niet om <rhyme label="a" type="f" corresp="#s8:l1">versaden</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En hooghmoedt, drijft <rhyme label="b" type="m">altijdt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Tot sonden <rhyme label="c" type="f">overvloedigh</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Daerom moetmen seer <rhyme label="c" type="f" corresp="#s8:l4">spoedigh</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Hem daer van maken <rhyme label="b" type="m" corresp="#s8:l3">quijt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Alles als boven <rhyme label="a" type="f">schreven</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Door Godts hulpe <rhyme label="a" type="f" corresp="#s9:l1">verheven</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die in ons werckt met <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Den wil en dat <rhyme label="c" type="f">volbringhen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Nae 't ghemoedts goeder <rhyme label="c" type="f" corresp="#s9:l4">dingen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Voornemen dagh en <rhyme label="b" type="m" corresp="#s9:l3">nacht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ghy welbedachte <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ick schenck uyt goeder <rhyme label="a" type="f" corresp="#s10:l1">minnen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">V dit Liedt, kort <rhyme label="b" type="m">ghesanck</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Van gierigheydt <rhyme label="c" type="f">verlaten</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Hooghmoedt te leeren <rhyme label="c" type="f" corresp="#s10:l4">haten</rhyme>.</l>
        <l id="s10:l6" met="3" type="-+">Oorlof neemt dit in <rhyme label="b" type="m" corresp="#s10:l3">danck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
