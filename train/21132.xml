    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Naar dat ik heb gelezen</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEer Bruydegom <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met u Lief ende <rhyme label="b" type="m">Bruydt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Godt wil u zegen <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om te breyden wijdt <rhyme label="b" type="m" corresp="#s1:l2">uyt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ter eeren Gods en <rhyme label="c" type="m">Heerlijckheydt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Al vande Echte-staet <rhyme label="c" type="m" corresp="#s1:l5">verbreydt</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Ghy twee zijt <rhyme label="d" type="m">een-gepaert</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s1:l7">baerdt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Godt is de Liefd' <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Door liefd' in 't hert <rhyme label="b" type="m">geraet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Zijt ghy twee, een in 't <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In d'Echtelijcke <rhyme label="b" type="m" corresp="#s2:l2">staet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Door liefde in malkander <rhyme label="c" type="m">welt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Als 't Yser door 't vyer t'samen <rhyme label="c" type="m" corresp="#s2:l5">smelt</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Ghy twee zijt een <rhyme label="d" type="m">gepaert</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s2:l7">baert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De liefde kan 't oock <rhyme label="a" type="f">maecken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat twee eensinnigh <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Twee willen, en twee <rhyme label="a" type="f" corresp="#s3:l1">saecken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Haest trecken eenen <rhyme label="b" type="m" corresp="#s3:l2">lijn</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Twee sijnde, een vleysch ende <rhyme label="c" type="m">been</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Wandelen in liefd' ende <rhyme label="c" type="m" corresp="#s3:l5">vreen</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Ghy twee zijt een <rhyme label="d" type="m">gepaert</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s3:l7">baert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De liefd', by 't vyer <rhyme label="a" type="f">geleecken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Verwarmt 't koude <rhyme label="b" type="m">gemoedt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Heelt en geneest <rhyme label="a" type="f" corresp="#s4:l1">gebreecken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Leet en smaet sy <rhyme label="b" type="m" corresp="#s4:l2">versoet</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Door liefd' kanmen verwinnen <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Tegenspoet ende <rhyme label="c" type="m" corresp="#s4:l5">ongeval</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Ghy twee zijt een <rhyme label="d" type="m">gepaert</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s4:l7">baert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Liefde kan 't al <rhyme label="a" type="f">verdragen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sy vergaet <rhyme label="b" type="m">nimmermeer</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Lieffelijck sijn de <rhyme label="a" type="f" corresp="#s5:l1">dagen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Daer liefde is in <rhyme label="b" type="m" corresp="#s5:l2">eer</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Laet vry varen de geyle <rhyme label="c" type="m">Min</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Plant vaste liefde in u <rhyme label="c" type="m" corresp="#s5:l5">sin</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Ghy twee zijt een <rhyme label="d" type="m">gepaert</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s5:l7">baert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die met ware liefde <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En elck-ander <rhyme label="b" type="m">genoeght</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met twee handen en <rhyme label="a" type="f" corresp="#s6:l1">sinnen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">In een t'samen <rhyme label="b" type="m" corresp="#s6:l2">gevoeght</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Dat is die rechte <rhyme label="c" type="m">Echte-bandt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Van Godt in 't Paradijs <rhyme label="c" type="m" corresp="#s6:l5">geplant</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Ghy twee zijt <rhyme label="d" type="m">een-gepaert</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s6:l7">baert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wenschet in dese <rhyme label="a" type="f">Feeste</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Den Bruydegom en <rhyme label="b" type="m">Bruydt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Geluck, minst ende <rhyme label="a" type="f" corresp="#s7:l1">meeste</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat zy ten eynde <rhyme label="b" type="m" corresp="#s7:l2">uyt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Wel levend' liefd' en <rhyme label="c" type="m">waerdt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Elck na ander best <rhyme label="c" type="m" corresp="#s7:l5">veraerdt</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">Ghy twee zijt <rhyme label="d" type="m">een-gepaert</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s7:l7">baert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wilt Godt loven met <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Voor 't lieve jonge <rhyme label="b" type="m">Paer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat haer en ons <rhyme label="a" type="f" corresp="#s8:l1">gelingen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">d'Eeuwige vreught hier <rhyme label="b" type="m" corresp="#s8:l2">naer</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">De Heer bewaer u recht en <rhyme label="c" type="m">slecht</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Tot welstandt vande reyne <rhyme label="c" type="m" corresp="#s8:l5">Echt</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">Ghy twee zijt <rhyme label="d" type="m">een-gepaert</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Siet dat ghy liefde <rhyme label="d" type="m" corresp="#s8:l7">baert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
