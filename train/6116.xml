      <year-start>1632</year-start>
      <year-end>1632</year-end>
    </date>
    <location>
      <orginal-name>emmerik</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Verjubileer u gij Venus' jongens</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt reder een Ridder wt <rhyme label="a" type="f">jaghen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wt jaghen, en vant een Maegden <rhyme label="b" type="m">croon</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Hy dacht hoe hy haer mocht <rhyme label="a" type="f" corresp="#s1:l1">behagen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Hy gaf haer een krans van eere <rhyme label="b" type="m" corresp="#s1:l2">schoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hy sprack aenhoort ghy Maegden <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Reyne, Jesus heeft u <rhyme label="b" type="m">gesint</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dies wil hy oock voorwaer <rhyme label="a" type="f" corresp="#s2:l1">alleyne</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van u suyverlijck wesen <rhyme label="b" type="m" corresp="#s2:l2">bemint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gedenckt hoe ghy verwint met <rhyme label="a" type="f">strijden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Met strijden, die kranckheyt van die <rhyme label="b" type="m">natuer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat ghy moocht namaels u <rhyme label="a" type="f" corresp="#s3:l1">verblijden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Met die jonckvroulijcke croone <rhyme label="b" type="m" corresp="#s3:l2">puer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">In seden wilt niet sijn <rhyme label="a" type="f">hoochmoedich</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hoochmoedich, verheft u <rhyme label="b" type="m">nimmermeer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Hoe wel ghy sijt van Jaren <rhyme label="a" type="f" corresp="#s4:l1">bloedich</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Van lichaem en leden schoon en <rhyme label="b" type="m" corresp="#s4:l2">teer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want wat sijn toch die claer <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Claer oogen, in u ghesicht <rhyme label="b" type="m">geprent</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En al dat gratieus <rhyme label="a" type="f" corresp="#s5:l1">vertogen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Van u geheel lichaem <rhyme label="b" type="m" corresp="#s5:l2">excellent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Och anders niet dan stof der <rhyme label="a" type="f">aerden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Der aerden, dus trout daer op <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">V lichaem dat is cleyn van <rhyme label="a" type="f" corresp="#s6:l1">weerden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">'T welck haest verdrogen sal als een <rhyme label="b" type="m" corresp="#s6:l2">riet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Spiegelt u toch eens aen een <rhyme label="a" type="f">rose</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Een rose, als sy open <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Sy bloeyt voorwaer een cleyne <rhyme label="a" type="f" corresp="#s7:l1">pose</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Want haer schoonheyt alte haest <rhyme label="b" type="m" corresp="#s7:l2">vergaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Soo sal in u oock haest <rhyme label="a" type="f">verdwijnen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Verdwijnen, 't aensicht door den <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Dat ghy nu hebt met vele <rhyme label="a" type="f" corresp="#s8:l1">pijnen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Willen behouden claer en <rhyme label="b" type="m" corresp="#s8:l2">root</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer soo ghy u siel maeckt van <rhyme label="a" type="f">binnen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Van binnen, door deugt hupsch en <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dan sal u den Heere Christus <rhyme label="a" type="f" corresp="#s9:l1">minnen</rhyme></l>
        <l id="s9:l4" met="4" type="-+">En cieren met die hemelsche <rhyme label="b" type="m" corresp="#s9:l2">croon</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Laet dan des werelts vreugde <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Al varen, sy brengt droefheyt en <rhyme label="b" type="m">pijn</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Wilt dencken op der Maeghden <rhyme label="a" type="f" corresp="#s10:l1">scharen</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Die in die hemelsche glory <rhyme label="b" type="m" corresp="#s10:l2">sijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Een niew liet voor den Heer <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Verheven, singen die Maeghden <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En haer wort oock alleen <rhyme label="a" type="f" corresp="#s11:l1">gegeven</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Te volgen altijt dat Lammeken <rhyme label="b" type="m" corresp="#s11:l2">klaer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Wilt ghy met haer dit niew liet <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Liet singen, d'ydelheyt <rhyme label="b" type="m">versmaet</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">En wilt oock u lichaem <rhyme label="a" type="f" corresp="#s12:l1">bedwingen</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Van ontucht en alle sonden <rhyme label="b" type="m" corresp="#s12:l2">quaet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Adieu ghy Maeghden al te <rhyme label="a" type="f">samen</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Te samen, brengt den tijt soo <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Maeckt u met deugden soo <rhyme label="a" type="f" corresp="#s13:l1">bequame</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Om Jesu te sijn een lieve <rhyme label="b" type="m" corresp="#s13:l2">bruyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
