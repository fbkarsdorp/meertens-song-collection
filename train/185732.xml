<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185732</id>
    <date>
      <year-start>1655</year-start>
      <year-end>1655</year-end>
    </date>
    <location>
      <orginal-name>bergen op zoom</orginal-name>
      <standard-name>Bergen op Zoom, The Netherlands</standard-name>
      <coordinates>
        <lon>51.4958265</lon>
        <lat>4.2871293</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De vogeltjes in de muiten</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een nieuw Liedt tot vreughts <rhyme label="a" type="f">verstercken</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wil ick ons singen <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Van des Heere wonder <rhyme label="a" type="f" corresp="#s1:l1">wercken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die Christus heeft <rhyme label="b" type="m" corresp="#s1:l2">gedaen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hoe dat hy heeft <rhyme label="c" type="f">bewesen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sijn liefde <rhyme label="d" type="m">diligent</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Om de sondaers te <rhyme label="c" type="f" corresp="#s1:l5">genesen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die in sonden leven <rhyme label="d" type="m" corresp="#s1:l6">verblent</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Christus den Heer der <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">By de sondaers heeft <rhyme label="b" type="m">verkeert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat sy haer souden <rhyme label="a" type="f" corresp="#s2:l1">bekeeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Altijdt ootmoedigh <rhyme label="b" type="m" corresp="#s2:l2">geleert</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Want om die te <rhyme label="c" type="f">ontfangen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">In genade was hy <rhyme label="d" type="m">bereyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Het bleeck doen hy quam <rhyme label="c" type="f" corresp="#s2:l5">gegangen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Tot Iericho soo Lucas <rhyme label="d" type="m" corresp="#s2:l6">seyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Siet eenen Man naer Schrifts <rhyme label="a" type="f">vermanen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die Zacheus <rhyme label="b" type="m">hiet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Een Prince des <rhyme label="a" type="f" corresp="#s3:l1">Publicanen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Seer rijck van goede <rhyme label="b" type="m" corresp="#s3:l2">siet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maer was kleyn van <rhyme label="c" type="f">persoone</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En begeerde den Heere te <rhyme label="d" type="m">sien</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maer om die groote schare <rhyme label="c" type="f" corresp="#s3:l5">idoone</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En kost het hem niet <rhyme label="d" type="m" corresp="#s3:l6">geschien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Toen liep hy met haesten <rhyme label="a" type="f">voren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Vyt de schaer wilt het <rhyme label="b" type="m">verstaen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Aldaer Christo <rhyme label="a" type="f" corresp="#s4:l1">verkoren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Voorby moest passeren <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Is hy aldaer <rhyme label="c" type="f">geklommen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Op eenen wilden Vijgenboom <rhyme label="d" type="m">ras</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Om dat hy niet sou zijn <rhyme label="c" type="f" corresp="#s4:l5">verdrommen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Want daer een groote schaere <rhyme label="d" type="m" corresp="#s4:l6">was</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Want hy hadde groot <rhyme label="a" type="f">verlangen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Om Christo te sien <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Toen quam den Heere <rhyme label="a" type="f" corresp="#s5:l1">gegangen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En sprack met woorden <rhyme label="b" type="m" corresp="#s5:l2">klaer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zacheus komt <rhyme label="c" type="f">beneden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">V naeckt blijschap <rhyme label="d" type="m">propijs</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Want ick wille als <rhyme label="c" type="f" corresp="#s5:l5">heden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">By u nemen <rhyme label="d" type="m" corresp="#s5:l6">logijs</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dies Zacheus seer <rhyme label="a" type="f">spoedigh</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Quam haestelijck af <rhyme label="b" type="m">gegaen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En heeft Christum <rhyme label="a" type="f" corresp="#s6:l1">ootmoedigh</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Seer blijdelijck <rhyme label="b" type="m" corresp="#s6:l2">ontfaen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dies de <rhyme label="c" type="f">Phariseen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Spraken op â€˜t selve <rhyme label="d" type="m">pas</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat den Heer rechtveerdigh by <rhyme label="c" type="f" corresp="#s6:l5">een</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Sondigh mensch geherberght <rhyme label="d" type="m" corresp="#s6:l6">was</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Zacheus met <rhyme label="a" type="f">ootmoedt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Sprack doen tot Christus <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Siet Heer de helft van mijn <rhyme label="a" type="f" corresp="#s7:l1">goedt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Geve ick den armen <rhyme label="b" type="m" corresp="#s7:l2">vroet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Hebbe ick jet <rhyme label="c" type="f">onrechtveerdigh</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Iemants huysen of <rhyme label="d" type="m">steen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dat wil ick seer <rhyme label="c" type="f" corresp="#s7:l5">volherdigh</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Vier dobbel weder <rhyme label="d" type="m" corresp="#s7:l6">geen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Toen heeft Christus <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Tot Zacheus troostelijck <rhyme label="b" type="m">geseyt</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Heden is den huyse van <rhyme label="a" type="f" corresp="#s8:l1">desen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Nu geschiet <rhyme label="b" type="m" corresp="#s8:l2">saligheyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Om dat dit is van den <rhyme label="c" type="f">sade</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Een Sone van <rhyme label="d" type="m">Abraham</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Om die te soecken vroegh en <rhyme label="c" type="f" corresp="#s8:l5">spade</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Den Sone des menschen <rhyme label="d" type="m" corresp="#s8:l6">quam</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Den Sone Godts <rhyme label="a" type="f">uytverkoren</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Quam in de wereldt <rhyme label="b" type="m">blent</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Om te soecken dat was <rhyme label="a" type="f" corresp="#s9:l1">verloren</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En saligh te maken <rhyme label="b" type="m" corresp="#s9:l2">bekent</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Dit heeft Christus altijdt <rhyme label="c" type="f">beleden</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Soo daer geschreven <rhyme label="d" type="m">staet</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Wilt zijn voet-paden <rhyme label="c" type="f" corresp="#s9:l5">treden</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Een schoon licht voor u <rhyme label="d" type="m" corresp="#s9:l6">gaet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">O Broeders <rhyme label="a" type="f">alle-gare</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Hoort naer des Heeren <rhyme label="b" type="m">vermaen</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Loopt uyt de boose <rhyme label="a" type="f" corresp="#s10:l1">schare</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Soo Zacheus heeft <rhyme label="b" type="m" corresp="#s10:l2">gedaen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Klimt op den boom vol <rhyme label="c" type="f">trouwen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Ontfanght een blijde <rhyme label="d" type="m">maer</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Soo sult gy den Heere <rhyme label="c" type="f" corresp="#s10:l5">aenschouwen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Als hy passeert <rhyme label="d" type="m" corresp="#s10:l6">aldaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Schout het sondigh <rhyme label="a" type="f">aenkleven</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Soo Zacheus heeft <rhyme label="b" type="m">gedaen</rhyme>:</l>
        <l id="s11:l3" met="3" type="-+">Laet varen uw boos <rhyme label="a" type="f" corresp="#s11:l1">leven</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Doet goede wercken <rhyme label="b" type="m" corresp="#s11:l2">saen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Deelt nu uwâ€™ goedt den <rhyme label="c" type="f">armen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Als Zacheus in <rhyme label="d" type="m">persoon</rhyme>:</l>
        <l id="s11:l7" met="3" type="-+">Godt sal u <rhyme label="c" type="f" corresp="#s11:l5">ontfermen</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Seer groot weet uwen <rhyme label="d" type="m" corresp="#s11:l6">loon</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Oorlof Broeders <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Die hier zijt in â€˜t <rhyme label="b" type="m">getal</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Siet hier den tijdt <rhyme label="a" type="f" corresp="#s12:l1">uytgelesen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Christus roept over <rhyme label="b" type="m" corresp="#s12:l2">al</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Komt tot my van wat <rhyme label="c" type="f">natie</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Mijn gratie open <rhyme label="d" type="m">staet</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Eerwijlen gy hebt <rhyme label="c" type="f" corresp="#s12:l5">statie</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Eer dat het wordt te <rhyme label="d" type="m" corresp="#s12:l6">laet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
