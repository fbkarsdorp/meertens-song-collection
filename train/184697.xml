<song>
  <meta>
    <id>184697</id>
    <date>
      <year-start>1634</year-start>
      <year-end>1634</year-end>
    </date>
    <location>
      <orginal-name>den haag</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach moorderes</melody>
  <author>Pieter Nootmans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Titer, U <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">En u soet <rhyme label="b" type="m">onthael</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van u lieve <rhyme label="c" type="m">minne-treeck</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">'tHart soo <rhyme label="a" type="f" corresp="#s1:l1">bekoorde</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Dat ick <rhyme label="b" type="m" corresp="#s1:l2">menigmael</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Aen de koele <rhyme label="c" type="m" corresp="#s1:l3">water-beeck</rhyme>,</l>
        <l id="s1:l7" met="8" type="+-">Daer het weeligh kruyt, langs hare kant, men de steen-klip, op siet <rhyme label="d" type="f">palmen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Om de traentjes van u <rhyme label="e" type="m">vee</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dat mijn Ziel bewegen <rhyme label="e" type="m" corresp="#s1:l8">dee</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Weder-klagjes moest <rhyme label="d" type="f" corresp="#s1:l7">uytgalmen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ach mijn <rhyme label="a" type="f">gepeynsen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Stuyrde <rhyme label="b" type="m">Amaril</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Tot een <rhyme label="c" type="m">Weder-liefdens-trouw</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Wat wil ick <rhyme label="a" type="f" corresp="#s2:l1">veynsen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Titer ist u <rhyme label="b" type="m" corresp="#s2:l2">wil</rhyme>!</l>
        <l id="s2:l6" met="4" type="+-">T'sa wel an, maeck my u <rhyme label="c" type="m" corresp="#s2:l3">vrouw</rhyme>,</l>
        <l id="s2:l7" met="8" type="+-">Die uwe Ziel vast is in-gezielt, en u nimmer sal <rhyme label="d" type="f">begeven</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Tweede Ziele van mijn <rhyme label="e" type="m">Ziel</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Die mijn kieskeur vaeck <rhyme label="e" type="m" corresp="#s2:l8">beviel</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Hartens-af-god in mijn <rhyme label="d" type="f" corresp="#s2:l7">leven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">V droeve <rhyme label="a" type="f">nachtjes</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Die ghy in het <rhyme label="b" type="m">Veldt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Met mijn Vee hebt <rhyme label="c" type="m">uytgestaen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">V <rhyme label="a" type="f" corresp="#s3:l1">minne-klachjes</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Die ghy met <rhyme label="b" type="m" corresp="#s3:l2">geweldt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Laes! uyt-parste door <rhyme label="c" type="m" corresp="#s3:l3">'tgetraen</rhyme>,</l>
        <l id="s3:l7" met="8" type="+-">Heeft Philomelaes soete stem op de tackjes vaeck nae doen <rhyme label="d" type="f">botsen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Soo dat Echoos naer <rhyme label="e" type="m">gegil</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Vwe klacht aen <rhyme label="e" type="m" corresp="#s3:l8">Amaril</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Wt-gallemde uyt de <rhyme label="d" type="f" corresp="#s3:l7">rotsen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Dies ick van <rhyme label="a" type="f">bloemen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Vlechten sal een <rhyme label="b" type="m">krans</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om te drucken Titers <rhyme label="c" type="m">hooft</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Niet dan tot <rhyme label="a" type="f" corresp="#s4:l1">roemen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Aen de <rhyme label="b" type="m" corresp="#s4:l2">Nymphjes-dans</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat hy heeft voor sich <rhyme label="c" type="m" corresp="#s4:l3">gerooft</rhyme></l>
        <l id="s4:l7" met="8" type="+-">Wt het getal der Maegden Rey een de trouste <rhyme label="d" type="f">Velt-Godinne</rhyme>;</l>
        <l id="s4:l8" met="4" type="+-">Dies u trouwe <rhyme label="e" type="m">Amaril</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Nae haer Titers eygen <rhyme label="e" type="m" corresp="#s4:l8">wil</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Hem gestadigh sal <rhyme label="d" type="f" corresp="#s4:l7">beminnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
