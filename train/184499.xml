    <tags/>
    <melody>Kom Orpheus kom Amphion</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ik lag laatst op een <rhyme label="a" type="m">morgenstond</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Te droomen op myn <rhyme label="b" type="m">bed</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dat naast myn zy lag <rhyme label="a" type="m" corresp="#s1:l1">Rosemond</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gantsch poesel, blank en <rhyme label="b" type="m" corresp="#s1:l2">vet</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Haar oogjes waren als <rhyme label="c" type="m">vergit</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Haar kaakjes als een <rhyme label="d" type="m">roos</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Geplant op zilver <rhyme label="c" type="m" corresp="#s1:l5">lelywit</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Geplant op zilver <rhyme label="c" type="m" corresp="#s1:l5">lelywit</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Die ik voor 't beste <rhyme label="d" type="m" corresp="#s1:l6">koo...s</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Die ik voor 't beste <rhyme label="d" type="m" corresp="#s1:l6">koos</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Haar tandjes als een <rhyme label="a" type="m">paerlemoer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Een klofje in haar <rhyme label="b" type="m">kin</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Verdubbelt met een <rhyme label="a" type="m" corresp="#s2:l1">paerelsnoer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van venus <rhyme label="b" type="m" corresp="#s2:l2">mingodin</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Haar borsjes waren <rhyme label="c" type="m">appelrond</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Gelyk als melk en <rhyme label="d" type="m">room</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Op ieder een robyntje <rhyme label="c" type="m" corresp="#s2:l5">stond</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Op ieder een robyntje <rhyme label="c" type="m" corresp="#s2:l5">stond</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Die kuste ik zonder <rhyme label="d" type="m" corresp="#s2:l6">schroo...m</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Die kuste ik zonder <rhyme label="d" type="m" corresp="#s2:l6">schroom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het schoone lighaam <rhyme label="a" type="m">nederwaards</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Was net als een <rhyme label="b" type="m">albas</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Niet laager als twee spaanen <rhyme label="a" type="m" corresp="#s3:l1">ras</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Een aardig kloofje <rhyme label="b" type="m" corresp="#s3:l2">was</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Haar buikje my een kruidberg <rhyme label="c" type="m">scheen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Voor Venus tempel <rhyme label="d" type="m">staan</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Op elpe liever <rhyme label="c" type="m" corresp="#s3:l5">tempelbeen</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Op elpe liever <rhyme label="c" type="m" corresp="#s3:l5">tempelbeen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Een plant van <rhyme label="d" type="m" corresp="#s3:l6">lelybaa...n</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Een plant van <rhyme label="d" type="m" corresp="#s3:l6">lelyblaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De tempel daar men 't offer <rhyme label="a" type="m">doet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van 't beste dat men <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En dat men van zyn vleesch en <rhyme label="a" type="m" corresp="#s4:l1">bloed</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ja al ten beste <rhyme label="b" type="m" corresp="#s4:l2">geeft</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Daar vroeg ik of ik in mogt <rhyme label="c" type="m">gaan</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En doen myn <rhyme label="d" type="m">offerhand</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Zy lonkte my zo vriendelyk <rhyme label="c" type="m" corresp="#s4:l5">aan</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Zy lonkte my zo vriendelyk <rhyme label="c" type="m" corresp="#s4:l5">aan</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En drukte my myn <rhyme label="d" type="m" corresp="#s4:l6">han...d</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">En drukte my myn <rhyme label="d" type="m" corresp="#s4:l6">hand</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gestrengeld kusten wy <rhyme label="a" type="m">elkaar</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met zugies vol van <rhyme label="b" type="m">min</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Maar toen myn offer was <rhyme label="a" type="m" corresp="#s5:l1">volbragt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Was 't vuurig in 't <rhyme label="b" type="m" corresp="#s5:l2">begin</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Maar in het einde <rhyme label="c" type="m">Cupido</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Zyn beste schigt <rhyme label="d" type="m">verloor</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Ontwaakten ik heel <rhyme label="c" type="m" corresp="#s5:l5">onverwagt</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Ontwaakten ik heel <rhyme label="c" type="m" corresp="#s5:l5">onverwagt</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Myn schoonste ik <rhyme label="d" type="m" corresp="#s5:l6">verloo...r</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Myn schoonste ik <rhyme label="d" type="m" corresp="#s5:l6">verloor</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
