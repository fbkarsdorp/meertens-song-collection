  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">J. Aardsche <rhyme label="a" type="f">Godinne</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Aangename brave <rhyme label="b" type="m">Maagd</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Doel van mijn verliefde <rhyme label="a" type="f" corresp="#s1:l1">zinnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die mijn teêre ziel <rhyme label="b" type="m" corresp="#s1:l2">behaagt</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Zeg eens, wie bekoort gij <rhyme label="c" type="m">niet</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">Die uw schoonheid maar <rhyme label="c" type="m" corresp="#s1:l5">aanziet</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Zeker 'k moet u eeuwig <rhyme label="d" type="f">minnen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Schoon uw strafheid 't mij <rhyme label="c" type="m" corresp="#s1:l5">verbiedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ei, ei, loop <rhyme label="a" type="f">henen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'k Heb in 't minste geen <rhyme label="b" type="m">vermaak</rhyme></l>
        <l id="s2:l3" met="4" type="+-">In uw woorden zonder <rhyme label="a" type="f" corresp="#s2:l1">meenen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En uw praatjes voor de <rhyme label="b" type="m" corresp="#s2:l2">vaak</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Vreijers, vreijers <rhyme label="c" type="m">inderdaad</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Want hun uiterlijk <rhyme label="c" type="m" corresp="#s2:l5">gelaat</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Zuchten, steenen, klagen, <rhyme label="d" type="f">weenen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Hun niet eens ter harte <rhyme label="c" type="m" corresp="#s2:l5">gaat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Zoudt gij <rhyme label="a" type="f">gelooven</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dat ik door ontrouwe <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zoek uw teeder hart te <rhyme label="a" type="f" corresp="#s3:l1">rooven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Neen, mijn schoonste <rhyme label="b" type="m" corresp="#s3:l2">Engelin</rhyme>!</l>
        <l id="s3:l5" met="4" type="+-">Proef, ach Lief! ach proef mij <rhyme label="c" type="m">maar</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Schoon dat liep mijn lijf <rhyme label="c" type="m" corresp="#s3:l5">gevaar</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Nimmer zal dat vuur <rhyme label="d" type="f">verdooven</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Al was lijden nog zoo <rhyme label="c" type="m" corresp="#s3:l5">zwaar</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Tusschen het <rhyme label="a" type="f">zeggen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">En het doen is <rhyme label="b" type="m">onderscheid</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Even als de bergen <rhyme label="a" type="f" corresp="#s4:l1">leggen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Tusschen 't Zuid en Noord <rhyme label="b" type="m" corresp="#s4:l2">verspreid</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Als de vogelvanger <rhyme label="c" type="m">zet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Het arglistig <rhyme label="c" type="m" corresp="#s4:l5">garennet</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Schuilt hij achter struik en <rhyme label="d" type="f">heggen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Tot het beestje is in 't <rhyme label="c" type="m" corresp="#s4:l5">net</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Eer zal den <rhyme label="a" type="f">hemel</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Plett'ren zaad en land en <rhyme label="b" type="m">volk</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Eer zal 't sierlijk <rhyme label="a" type="f" corresp="#s5:l1">pluimgewemel</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Nestelen in een dorre <rhyme label="b" type="m" corresp="#s5:l2">kolk</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Eer een logge walvisch <rhyme label="c" type="m">vloog</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Eer ik u mijn Lief <rhyme label="c" type="m" corresp="#s5:l5">bedroog</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Zal den allergrootsten <rhyme label="d" type="f">kemel</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Gaan door 't kleinste <rhyme label="c" type="m" corresp="#s5:l5">naalden-oog</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Hoe zwaarder <rhyme label="a" type="f">eden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Hoeveel ligter van <rhyme label="b" type="m">waardij</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Zet die leuren en <rhyme label="a" type="f" corresp="#s6:l1">gebeden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met dit zweeren aan een <rhyme label="b" type="m" corresp="#s6:l2">zij</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Vreijers! met uw eden <rhyme label="c" type="m">loop</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Eden die zijn heel <rhyme label="c" type="m" corresp="#s6:l5">goedkoop</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Wilt die aan geen Maagd <rhyme label="d" type="f">besteden</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Duizend heb je er op een <rhyme label="c" type="m" corresp="#s6:l5">hoop</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Moet ik dan <rhyme label="a" type="f">dragen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Smaadheid, straf of and're <rhyme label="b" type="m">schuld</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Of hebt gij alleen <rhyme label="a" type="f" corresp="#s7:l1">behagen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">In den toets van mijn <rhyme label="b" type="m" corresp="#s7:l2">geduld</rhyme>?</l>
        <l id="s7:l5" met="4" type="+-">Wel ik zal langmoedig <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">'k Hoop, dat nog uw <rhyme label="c" type="m" corresp="#s7:l5">zonneschijn</rhyme></l>
        <l id="s7:l7" met="4" type="+-">Eind'lijk eens op mij zal <rhyme label="d" type="f">dagen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Hoop van gunst en voelt geen <rhyme label="c" type="m" corresp="#s7:l5">pijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Gij draagt geen <rhyme label="a" type="f">lijden</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Smaadheid, schuld of and're <rhyme label="b" type="m">straf</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Ben ik vuur, zoo wilt mij <rhyme label="a" type="f" corresp="#s8:l1">mijden</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Brand ik u, gaat van mij <rhyme label="b" type="m" corresp="#s8:l2">af</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Want of ik u scheen <rhyme label="c" type="m">getrouw</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Ik u eind'lijk dan nog <rhyme label="c" type="m" corresp="#s8:l5">zou</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Als een gladde draad <rhyme label="d" type="f">afsnijden</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Want ik heb geen zin in <rhyme label="c" type="m" corresp="#s8:l5">jou</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">J. 'k Kan 't niet <rhyme label="a" type="f">verkroppen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Hart en ziel lijdt mij en <rhyme label="b" type="m">beeft</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Voelt eens hoe mijn ad'ren <rhyme label="a" type="f" corresp="#s9:l1">kloppen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Ach, mijn adem mij <rhyme label="b" type="m" corresp="#s9:l2">begeeft</rhyme>;</l>
        <l id="s9:l5" met="4" type="+-">Ach, ach, ach, dat zijn geen <rhyme label="c" type="m">reên</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dat gij altijd roept van <rhyme label="c" type="m" corresp="#s9:l5">neen</rhyme>!</l>
        <l id="s9:l7" met="4" type="+-">Zelfs een kei beweegt door 't <rhyme label="d" type="f">droppen</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Zijt gij harder dan een <rhyme label="c" type="m" corresp="#s9:l5">steen</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
