    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Amarilli mia bella</melody>
  <author>Jacob Cats</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">TOrtelduyfje mijn <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die van alle kant <rhyme label="b" type="m">gejaeght</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Vlucht als een <rhyme label="a" type="f" corresp="#s1:l1">hinde</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Doch, soo ick dickmael <rhyme label="a" type="f" corresp="#s1:l1">vinde</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">V herte <rhyme label="b" type="m" corresp="#s1:l2">knaeght</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Oock wel geheele <rhyme label="c" type="f">nachten</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Ey matight uwe <rhyme label="c" type="f" corresp="#s1:l6">klachten</rhyme>,</l>
        <l id="s1:l8" met="5" type="-+">Dat u geen druck of wan-hoop en <rhyme label="a" type="f" corresp="#s1:l1">verslinde</rhyme>,</l>
        <l id="s1:l9" met="8" type="+-">Tortelduyfje, Tortelduyfje, Tortelduyfje mijn <rhyme label="a" type="f" corresp="#s1:l1">beminde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Eenigh trooster aller <rhyme label="a" type="f">zielen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Doot en hel met al haer <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Zijn op mijn <rhyme label="a" type="f" corresp="#s2:l1">hielen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En willen my <rhyme label="a" type="f" corresp="#s2:l1">vernielen</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Hebt doch eens <rhyme label="b" type="m" corresp="#s2:l2">acht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Op al mijn drouvigh <rhyme label="c" type="f">kermen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En wilt my doch <rhyme label="c" type="f" corresp="#s2:l6">beschermen</rhyme></l>
        <l id="s2:l8" met="5" type="-+">Voor al die om mijn swacke leden <rhyme label="a" type="f" corresp="#s2:l1">krielen</rhyme>,</l>
        <l id="s2:l9" met="8" type="+-">Eenigh trooster, eenigh trooster, eenigh trooster aller <rhyme label="a" type="f" corresp="#s2:l1">zielen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dochter Zions mijn <rhyme label="a" type="f">verkoren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Druck, en pijn, en swarte <rhyme label="b" type="m">nijt</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Die u <rhyme label="a" type="f" corresp="#s3:l1">verstoren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sijn voor u nutte <rhyme label="a" type="f" corresp="#s3:l1">sporen</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Hier inder <rhyme label="b" type="m" corresp="#s3:l2">tijt</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Maer wacht hier namaels <rhyme label="c" type="f">segen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Want die is u <rhyme label="c" type="f" corresp="#s3:l6">verkregen</rhyme>.</l>
        <l id="s3:l8" met="5" type="-+">Die ick oyt koos en gingen noyt <rhyme label="a" type="f" corresp="#s3:l1">verloren</rhyme>,</l>
        <l id="s3:l9" met="8" type="+-">Dochter Zions, dochter Zions, dochter Zions myn verko <rhyme label="a" type="f" corresp="#s3:l1">ren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Opper-hoeder u wer <rhyme label="a" type="f">Kercken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ach! het vleys is al te <rhyme label="b" type="m">blint</rhyme>;</l>
        <l id="s4:l3" met="2" type="-+">'Ten kan niet <rhyme label="a" type="f" corresp="#s4:l1">mercken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">V wonderbare <rhyme label="a" type="f" corresp="#s4:l1">wercken</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">'Tis <rhyme label="b" type="m" corresp="#s4:l2">onbesint</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">En doet my dickmael <rhyme label="c" type="f">dwalen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Gansch buyten uwe <rhyme label="c" type="f" corresp="#s4:l6">palen</rhyme>.</l>
        <l id="s4:l8" met="5" type="-+">Ghy daerom wilt mijn swacke ziel <rhyme label="a" type="f" corresp="#s4:l1">verstrecken</rhyme>,</l>
        <l id="s4:l9" met="8" type="+-">Opper-hoeder, Opper-hoeder, Opper-hoeder uwer <rhyme label="a" type="f" corresp="#s4:l1">Kercken</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sunamite mijn <rhyme label="a" type="f">vriendinne</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Al maeckt u het lijden <rhyme label="b" type="m">drouf</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Weest koel van <rhyme label="a" type="f" corresp="#s5:l1">sinne</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">'Tis uyt oprechte <rhyme label="a" type="f" corresp="#s5:l1">minne</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Dat ick u <rhyme label="b" type="m" corresp="#s5:l2">prouf</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Geen krachten vander <rhyme label="c" type="f">hellen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Geen doot en kan u <rhyme label="c" type="f" corresp="#s5:l6">quellen</rhyme>.</l>
        <l id="s5:l8" met="5" type="-+">Ghy zyt mijn erf oock van den <rhyme label="a" type="f" corresp="#s5:l1">aenbeginne</rhyme>,</l>
        <l id="s5:l9" met="8" type="+-">Tortel-duyfje, dochter Zions, Sunamite mijn <rhyme label="a" type="f" corresp="#s5:l1">vriendinne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ziel-verlosser mijn <rhyme label="a" type="f">verlangen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ick ben Iangh des werelts <rhyme label="b" type="m">sat</rhyme>.</l>
        <l id="s6:l3" met="2" type="-+">De tranen <rhyme label="a" type="f" corresp="#s6:l1">hangen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Staegh op mijn drouve <rhyme label="a" type="f" corresp="#s6:l1">wangen</rhyme>;</l>
        <l id="s6:l5" met="2" type="-+">Ach, siltigh <rhyme label="b" type="m" corresp="#s6:l2">nat</rhyme>!</l>
        <l id="s6:l6" met="3" type="-+">Wanneer sal't doch eens <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dat ick van druck <rhyme label="c" type="f" corresp="#s6:l6">genesen</rhyme></l>
        <l id="s6:l8" met="5" type="-+">Mijn God, mijn heyl, mijn Schepper sal <rhyme label="a" type="f" corresp="#s6:l1">omvangen</rhyme>,</l>
        <l id="s6:l9" met="8" type="+-">Eenig trooster, Opper-hoeder, Ziel-verlosser mijn <rhyme label="a" type="f" corresp="#s6:l1">verlangen</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
