  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Verscheenen in het vol <rhyme label="a" type="m">Geweer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zo treeden wy met <rhyme label="a" type="m" corresp="#s1:l1">eer'</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Als Burgers, <rhyme label="b" type="m">bly</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Vernoegd en <rhyme label="b" type="m" corresp="#s1:l3">vry</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Naar 't Veld, dat ons ter oefning <rhyme label="b" type="m" corresp="#s1:l3">zy</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">En onze Hopman gaat ons <rhyme label="c" type="m">voor</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Gaat voor, gaat <rhyme label="c" type="m" corresp="#s1:l6">voor</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wy volgen op zyn <rhyme label="c" type="m" corresp="#s1:l6">spoor</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Zyn spoor, zyn <rhyme label="c" type="m" corresp="#s1:l6">spoor</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">En geeven zyn bevel <rhyme label="c" type="m" corresp="#s1:l6">gehoor</rhyme>.</l>
      </lg>
    </lg>
