<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186994</id>
    <date>
      <year-start>1708</year-start>
      <year-end>1708</year-end>
    </date>
    <location>
      <orginal-name>gouda</orginal-name>
      <standard-name>Gouda, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114922</lon>
        <lat>4.7106094</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  <author>Jan van Hoogstraten</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wie beneden 't <rhyme label="a" type="m">stargewelf</rhyme>.</l>
        <l id="s1:l2" met="2" type="+-">Uit zig <rhyme label="a" type="m" corresp="#s1:l1">zelf</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Te bestaan hem wil <rhyme label="b" type="f">vermeten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zal, bedrogen door zijn <rhyme label="c" type="m">waan</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Haast <rhyme label="c" type="m" corresp="#s1:l4">vergaan</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Daar hy Gode heeft <rhyme label="b" type="f" corresp="#s1:l3">vergeten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zoo de Bouwheer van 't <rhyme label="a" type="m">heelal</rhyme></l>
        <l id="s2:l2" met="2" type="+-">'T aardse <rhyme label="a" type="m" corresp="#s2:l1">dal</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Niet bezigtigde in het <rhyme label="b" type="f">ronde</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En op yders los <rhyme label="c" type="m">bestaan</rhyme></l>
        <l id="s2:l5" met="2" type="+-">'T oog liet <rhyme label="c" type="m" corresp="#s2:l4">gaan</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Alles storte voort te <rhyme label="b" type="f" corresp="#s2:l3">gronde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Geen paleys, al stond het <rhyme label="a" type="m">trots</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Op een <rhyme label="a" type="m" corresp="#s3:l1">rots</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Sterker dan oit mens <rhyme label="b" type="f">aanschouwde</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Of het storte plots ter <rhyme label="c" type="m">neer</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Zoo de <rhyme label="c" type="m" corresp="#s3:l4">Heer</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Na zijn wil dat zelf niet <rhyme label="b" type="f" corresp="#s3:l3">bouwde</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maar wie in des Heeren <rhyme label="a" type="m">naam</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Zijne <rhyme label="a" type="m" corresp="#s4:l1">faam</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Huis of aards geluk ziet <rhyme label="b" type="f">groejen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Zal, door zijne gunst <rhyme label="c" type="m">gesterkt</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Wat hy <rhyme label="c" type="m" corresp="#s4:l4">werkt</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Zien in kragt, en luister <rhyme label="b" type="f" corresp="#s4:l3">bloejen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'T is door zijne wil <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Dat <rhyme label="a" type="m" corresp="#s5:l1">beneen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">De aardse koningen <rhyme label="b" type="f">regeeren</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Wie op hem zijn hope <rhyme label="c" type="m">stelt</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Zal <rhyme label="c" type="m" corresp="#s5:l4">geweld</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Ramp nog werrelts onheil <rhyme label="b" type="f" corresp="#s5:l3">deeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'K heb daarom mijn <rhyme label="a" type="m">levenslot</rhyme></l>
        <l id="s6:l2" met="2" type="+-">U mijn <rhyme label="a" type="m" corresp="#s6:l1">God</rhyme>!</l>
        <l id="s6:l3" met="4" type="+-">Heel en al gestelt in <rhyme label="b" type="f">handen</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Op dat nimmer los <rhyme label="c" type="m">bestaan</rhyme></l>
        <l id="s6:l5" met="2" type="+-">My <rhyme label="c" type="m" corresp="#s6:l4">voortaan</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Brenge in dootsgevaar of <rhyme label="b" type="f" corresp="#s6:l3">schanden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Gy alleen hebt mijn <rhyme label="a" type="m">gemoed</rhyme></l>
        <l id="s7:l2" met="2" type="+-">
          <rhyme label="a" type="m" corresp="#s7:l1">Tegenspoed</rhyme>
        </l>
        <l id="s7:l3" met="4" type="+-">Haat, en onheil leeren <rhyme label="b" type="f">dragen</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">'K zing des uwen naam ten <rhyme label="c" type="m">prijz</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Eene <rhyme label="c" type="m" corresp="#s7:l4">wijz</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Die al 't aardrijk zal <rhyme label="b" type="f" corresp="#s7:l3">behagen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Want gy geeft mijn zwakke <rhyme label="a" type="m">stem</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Kragt, en <rhyme label="a" type="m" corresp="#s8:l1">klem</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Om na 't hoog paleis hier <rhyme label="b" type="f">boven</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Heen te zweven uit elx <rhyme label="c" type="m">oog</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Daar om <rhyme label="c" type="m" corresp="#s8:l4">hoog</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Uwe magt alle Englen <rhyme label="b" type="f" corresp="#s8:l3">loven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Daar is reeds mijn wens en <rhyme label="a" type="m">zugt</rhyme></l>
        <l id="s9:l2" met="2" type="+-">En ik <rhyme label="a" type="m" corresp="#s9:l1">vlugt</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Wars van alles hier <rhyme label="b" type="f">beneden</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Als ik zing uw naam ten <rhyme label="c" type="m">prijz</rhyme></l>
        <l id="s9:l5" met="2" type="+-">Op die <rhyme label="c" type="m" corresp="#s9:l4">wijz</rhyme></l>
        <l id="s9:l6" met="4" type="+-">'T schijnvermaak der <rhyme label="b" type="f" corresp="#s9:l3">ydelheden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Want berooft van u, is 't <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s10:l2" met="2" type="+-">Als <rhyme label="a" type="m" corresp="#s10:l1">verdriet</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Wat ons komt alhier te <rhyme label="b" type="f">voren</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Niet als quelling voor den <rhyme label="c" type="m">geest</rhyme>.</l>
        <l id="s10:l5" met="2" type="+-">Wie u <rhyme label="c" type="m" corresp="#s10:l4">vreest</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Heeft zijn eewig heyl <rhyme label="b" type="f" corresp="#s10:l3">verkoren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
