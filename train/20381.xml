<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20381</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hei hoe helder schijnt het maantje</melody>
  <author>B. Targier, Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIs ghy hadd'et spel <rhyme label="a" type="f">verloren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hippomenus aerdigh <rhyme label="b" type="m">gast</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maer ghy kond' de Maeght <rhyme label="a" type="f" corresp="#s1:l1">bekoren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daerse wel me was <rhyme label="b" type="m" corresp="#s1:l2">ghepast</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Met Goudt, 't gheen <rhyme label="c" type="m">bouwt</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Liefd' en soete <rhyme label="d" type="m">Min</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">'t Goudt heeft groote krachten <rhyme label="d" type="m" corresp="#s1:l6">in</rhyme>;</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Konsten, wijsheydt, goede <rhyme label="a" type="f">Zeden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Deught, gheleertheydt, kloeck <rhyme label="b" type="m">verstant</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Tucht en welghekamde <rhyme label="a" type="f" corresp="#s2:l1">Reden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Raecken in een hoeck en <rhyme label="b" type="m" corresp="#s2:l2">kant</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Maer Goudt,, dat <rhyme label="c" type="m">houdt</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Ende plant de <rhyme label="d" type="m">Min</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">'t Goudt heeft groote krachten <rhyme label="d" type="m" corresp="#s2:l6">in</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die een Vrijster thans wil <rhyme label="a" type="f">vrij'en</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Soo hy niet veel Goudt en <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wis soo sal hy niet <rhyme label="a" type="f" corresp="#s3:l1">bedy'en</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Schoon sijn doen nae deughde <rhyme label="b" type="m" corresp="#s3:l2">streeft</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Maer Goudt,, dat <rhyme label="c" type="m">trouwt</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">En dat bouwt de <rhyme label="d" type="m">Min</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">'t Goudt heeft groote krachten <rhyme label="d" type="m" corresp="#s3:l6">in</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Is een Meysje scheef, of <rhyme label="a" type="f">scheeltjes</rhyme>;</l>
        <l id="s4:l2" met="4" type="+-">Isse bruyn ghelijck een <rhyme label="b" type="m">kaeuw</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Isse swart, of isse <rhyme label="a" type="f" corresp="#s4:l1">geeltjes</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Isse grijs, of isse <rhyme label="b" type="m" corresp="#s4:l2">graeuw</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Soo 't Goudt,, <rhyme label="c" type="m">bedouwt</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Heure slimme <rhyme label="d" type="m">le'en</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">'t Knechjen isser me te <rhyme label="d" type="m" corresp="#s4:l6">vre'en</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gheen soo groven, botten <rhyme label="a" type="f">Lompert</rhyme>:</l>
        <l id="s5:l2" met="4" type="+-">Gheen soo overdommen <rhyme label="b" type="m">Loer</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Gheen soo dobbel slechten <rhyme label="a" type="f" corresp="#s5:l1">Plompert</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Tart de Kinckel of sen <rhyme label="b" type="m" corresp="#s5:l2">Moer</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Soo 't Goudt,, maer <rhyme label="c" type="m">smout</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Sijne <rhyme label="d" type="m">bottigheydt</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Soo ist knoopjen haest <rhyme label="d" type="m" corresp="#s5:l6">gheleydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Weet niet of ick nu wil <rhyme label="a" type="f">suchten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Met den droeven <rhyme label="b" type="m">Heraclijt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Of dat ick met veel <rhyme label="a" type="f" corresp="#s6:l1">ghenuchten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Lacchen wil met <rhyme label="b" type="m" corresp="#s6:l2">Democrijt</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Om 't Goudt,, 't gunt <rhyme label="c" type="m">stout</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Ider maeckt in <rhyme label="d" type="m">Min</rhyme>:</l>
        <l id="s6:l7" met="4" type="+-">'t Goudt heeft groote krachten <rhyme label="d" type="m" corresp="#s6:l6">in</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
