<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157744</id>
    <date>
      <year-start>1624</year-start>
      <year-end>1624</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het reed een ruitertje</melody>
  <author>Jacob Claesz</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe gaet het nu ten <rhyme label="a" type="f">tijden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">De menschen jaghen en <rhyme label="a" type="f" corresp="#s1:l1">rijden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Met hoop // na haren <rhyme label="b" type="m">loop</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want vintmen al Iaghers op <rhyme label="a" type="f">aerden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die Gods Rijcke door <rhyme label="a" type="f" corresp="#s2:l1">onwaerden</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Niet acht // ofte <rhyme label="b" type="m">verwacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Deen jaecht na s'werelts <rhyme label="a" type="f">eeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En krijchtse <rhyme label="a" type="f" corresp="#s3:l1">nimmermeeren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met smaet wort hy <rhyme label="b" type="m">ghehaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De sommighe al met <rhyme label="a" type="f">praten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wel spreecken t'mach haer niet <rhyme label="a" type="f" corresp="#s4:l1">baten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Het is // ter werelt <rhyme label="b" type="m">mis</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wat vintmen oock Iagers met <rhyme label="a" type="f">hoopen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die na aertsche schatten <rhyme label="a" type="f" corresp="#s5:l1">loopen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En gaen // op des doots <rhyme label="b" type="m">baen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Een deel met proncken en <rhyme label="a" type="f">pralen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Souden gheeren wat eer <rhyme label="a" type="f" corresp="#s6:l1">behalen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maer tis // al <rhyme label="b" type="m">duysternis</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Sommighe met <rhyme label="a" type="f">maeltijden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Om een weynich te <rhyme label="a" type="f" corresp="#s7:l1">verblijden</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dees staet // oock haest <rhyme label="b" type="m">vergaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sommich met <rhyme label="a" type="f">tenticheden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dees hebben oock sullicke <rhyme label="a" type="f" corresp="#s8:l1">seden</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Met cracht // te sijn <rhyme label="b" type="m">gheacht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Deen wil wijs sijn <rhyme label="a" type="f">gheheeten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">En spreeckt van Godts <rhyme label="a" type="f" corresp="#s9:l1">secreten</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Men vint // dees menschen <rhyme label="b" type="m">blint</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dus jaechtmen al naet <rhyme label="a" type="f">verderven</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Soo datmen niet mach <rhyme label="a" type="f" corresp="#s10:l1">beerven</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Het loon // des levens <rhyme label="b" type="m">schoon</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Deen soeckt eens anders <rhyme label="a" type="f">ghebreken</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Dat teghen sijn vrient te <rhyme label="a" type="f" corresp="#s11:l1">spreken</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Dees aert // is seer <rhyme label="b" type="m">vermaert</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">O dwaling seer <rhyme label="a" type="f">ongehoordich</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Datmen siet oyt <rhyme label="a" type="f" corresp="#s12:l1">teghenwoordich</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En jaecht // dat hem <rhyme label="b" type="m">behaecht</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Dus staen die menschen <rhyme label="a" type="f">boosachtich</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Al na den doot seer <rhyme label="a" type="f" corresp="#s13:l1">crachtich</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Dit mijdt // tis meer dat <rhyme label="b" type="m">tijdt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wilt jaghen na Godts <rhyme label="a" type="f">behaghen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Hier in wilt niet <rhyme label="a" type="f" corresp="#s14:l1">vertraghen</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Met macht // hier wel op <rhyme label="b" type="m">acht</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">En wilt de werelt niet <rhyme label="a" type="f">achten</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Versaecktse met al u <rhyme label="a" type="f" corresp="#s15:l1">crachten</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Hoocheyt // doch onder <rhyme label="b" type="m">leyt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Rijckdom mach niet <rhyme label="a" type="f">verschoonen</rhyme></l>
        <l id="s16:l2" met="3" type="-+">De doot is veel haer <rhyme label="a" type="f" corresp="#s16:l1">loonen</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Die wint // gout dit <rhyme label="b" type="m">versint</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Dus Iaecht omt prijckel te <rhyme label="a" type="f">schouwen</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Dat ghy mocht blijven <rhyme label="a" type="f" corresp="#s17:l1">behouwen</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Hier // in Godts Rijck <rhyme label="b" type="m">ga</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Soeckt met Davidt Godt <rhyme label="a" type="f">reene</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Te hebben, na d'Hemel <rhyme label="a" type="f" corresp="#s18:l1">ghemeene</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Aerdt siet // en vraech dy <rhyme label="b" type="m">niet</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Dus vrienden al int <rhyme label="a" type="f">leste</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Schickt u te Iaghen naet <rhyme label="a" type="f" corresp="#s19:l1">beste</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Christi // <rhyme label="b" type="m">ghebenedy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
