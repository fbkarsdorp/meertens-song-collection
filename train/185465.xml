<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185465</id>
    <date>
      <year-start>1824</year-start>
      <year-end>1824</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schep vreugde in het leven</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Kom laat ons <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">Ter eer van Bruidegom en <rhyme label="b" type="m">Bruid</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Bij 't afscheid <rhyme label="c" type="f">nemen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="b" type="m" corresp="#s1:l2">uit</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Zie, hoe het lieve Bruidje <rhyme label="d" type="m">lacht</rhyme>!</l>
        <l id="s1:l6" met="4" type="-+">Zij denkt, het wordt reeds <rhyme label="d" type="m" corresp="#s1:l5">middernacht</rhyme>....</l>
        <l id="s1:l7" met="4" type="-+">En dat's de tijd, dat..... elke <rhyme label="e" type="m">gast</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Verheugd tot zijnent <rhyme label="f" type="m">keert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Kom laat ons <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s2:l2" met="4" type="-+">Ter eer van Bruidegom en <rhyme label="b" type="m">Bruid</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Bij 't afscheid <rhyme label="c" type="f">nemen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="b" type="m" corresp="#s2:l2">uit</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">De Bruigom telt elk <rhyme label="d" type="m">klokgetik</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Rijkhalzend' naar het <rhyme label="d" type="m" corresp="#s2:l5">oogenblik</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Waarin hij ..... ziet hoe elke <rhyme label="e" type="m">gast</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Verheugd tot zijnent <rhyme label="f" type="m">keert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Kom laat ons <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s3:l2" met="4" type="-+">Ter eer van Bruidegom en <rhyme label="b" type="m">Bruid</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Bij 't afscheid <rhyme label="c" type="f">nemen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="b" type="m" corresp="#s3:l2">uit</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Wij wenschen 't Bruidje in den <rhyme label="d" type="m">strijd</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">De diepste <rhyme label="d" type="m" corresp="#s3:l5">onderdanigheid</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">En veel genoegen in deez' <rhyme label="e" type="m">nacht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Door Amor <rhyme label="f" type="m">uitgedacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Kom laat ons <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s4:l2" met="4" type="-+">Ter eer van Bruidegom en <rhyme label="b" type="m">Bruid</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Bij 't afscheid <rhyme label="c" type="f">nemen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="b" type="m" corresp="#s4:l2">uit</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Den Bruigom goeden frisschen <rhyme label="d" type="m">moed</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dat hij voor 't struik'len blijv' <rhyme label="d" type="m" corresp="#s4:l5">behoed</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Maar als 't een koenen ridder <rhyme label="e" type="m">past</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">In venus strijdperk <rhyme label="f" type="m">treedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Kom laat ons <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s5:l2" met="4" type="-+">Ter eer van Bruidegom en <rhyme label="b" type="m">Bruid</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Bij 't afscheid <rhyme label="c" type="f">nemen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="b" type="m" corresp="#s5:l2">uit</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Volg voorts deez' raad, o lieve <rhyme label="d" type="m">Bruid</rhyme>!</l>
        <l id="s5:l6" met="4" type="-+">Doet toch terstond het kaarsje <rhyme label="d" type="m" corresp="#s5:l5">uit</rhyme>;</l>
        <l id="s5:l7" met="4" type="-+">Want, in het donker, zoete <rhyme label="e" type="m">meid</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Wordt best den strijd <rhyme label="f" type="m">volbragt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Kom laat ons <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s6:l2" met="4" type="-+">Ter eer van Bruidegom en <rhyme label="b" type="m">Bruid</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Bij 't afscheid <rhyme label="c" type="f">nemen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="b" type="m" corresp="#s6:l2">uit</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Elk neemt zijn meisje in den <rhyme label="d" type="m">arm</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Het jeugdig hartje dat word <rhyme label="d" type="m" corresp="#s6:l5">warm</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">En spoed zich vrolijk naar zijn <rhyme label="e" type="m">huis</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En dan naar ..... <rhyme label="f" type="m">vederveld</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Kom laat ons <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s7:l2" met="4" type="-+">Ter eer van Bruidegom en <rhyme label="b" type="m">Bruid</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Bij 't afscheid <rhyme label="c" type="f">nemen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="b" type="m" corresp="#s7:l2">uit</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Slaap zacht in <rhyme label="d" type="m">vrengde</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Bruidegom en schoone <rhyme label="d" type="m" corresp="#s7:l5">Bruid</rhyme>!</l>
        <l id="s7:l7" met="4" type="-+">Bij 't vrolijk <rhyme label="e" type="m">scheiden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Drinkt elk zijn glaasje <rhyme label="f" type="m">uit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
