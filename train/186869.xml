<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186869</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Het kuipertje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Neêrlands Volk gaat na de <rhyme label="a" type="m">Vloot</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Volgt haar dappere <rhyme label="b" type="f">Officieren</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">'k Vrees geen kruyt, 'k Vrees geen <rhyme label="a" type="m" corresp="#s1:l1">loot</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Is de Tael der <rhyme label="b" type="f" corresp="#s1:l2">Bataviezen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">' k Vrees geen blaffen van een <rhyme label="c" type="m">Dog</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Hollands ouwe Leeuw roept <rhyme label="c" type="m" corresp="#s1:l5">nog</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Kam maer, kom maer onder myn <rhyme label="d" type="m">poot</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Trouwelooze <rhyme label="d" type="m" corresp="#s1:l7">Bontgenoot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Is wel slegter tyd <rhyme label="a" type="m">geweest</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ja ruym hondert Jaer <rhyme label="b" type="f">geleden</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Wie maar 's Lands Historie <rhyme label="a" type="m" corresp="#s2:l1">leest</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Laet geen Brit nu meer in <rhyme label="b" type="f" corresp="#s2:l2">Vreede</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Die het heiligste <rhyme label="c" type="m">Verbond</rhyme>!</l>
        <l id="s2:l6" met="4" type="+-">Voor de gantsche Werelt <rhyme label="c" type="m" corresp="#s2:l5">schont</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Hoonden, hoonden en smaaden ons <rhyme label="d" type="m">Land</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Weg die Broederlyke <rhyme label="d" type="m" corresp="#s2:l7">Band</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Tot een nieuw <rhyme label="a" type="m">gedagtenis</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ziet men weer zyn oude <rhyme label="b" type="f">nukken</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Het schijnt hy Kajans nakroost <rhyme label="a" type="m" corresp="#s3:l1">is</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dog zyn aanslag zal niet <rhyme label="b" type="f" corresp="#s3:l2">lukken</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Hy zingt een vry hooge <rhyme label="c" type="m">toon</rhyme>!</l>
        <l id="s3:l6" met="4" type="+-">Zulke Liedjes kunnen wy <rhyme label="c" type="m" corresp="#s3:l5">schoon</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Zingen, Zingen, op onze <rhyme label="d" type="m">Vloot</rhyme>;</l>
        <l id="s3:l8" met="4" type="+-">Maar ik meen met kruit en <rhyme label="d" type="m" corresp="#s3:l7">loot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zo zal men de hoogmoed <rhyme label="a" type="m">eens</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Eeuwig aan de keeten <rhyme label="b" type="f">leggen</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">En bezoeken weer de <rhyme label="a" type="m" corresp="#s4:l1">Theems</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Kyken wat hy heeft te <rhyme label="b" type="f" corresp="#s4:l2">zeggen</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Want ons <rhyme label="c" type="m">Opper-Admiraal</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Vreest geen vuur, geen loot nog <rhyme label="c" type="m" corresp="#s4:l5">stael</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Maar zal toone, toone <rhyme label="d" type="m">gewis</rhyme>;</l>
        <l id="s4:l8" met="4" type="+-">Dat hij 't bloed van Grootvaer <rhyme label="d" type="m" corresp="#s4:l7">is</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Adieu dan Helden houw maar <rhyme label="a" type="m">woord</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Kom begeef u op uw <rhyme label="b" type="f">Kielen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Gy hebt Trompen nog aan <rhyme label="a" type="m" corresp="#s5:l1">boord</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ruyters en veel braavè <rhyme label="b" type="f" corresp="#s5:l2">ziele</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Agt geen blaffen van een <rhyme label="c" type="m">Dog</rhyme>.</l>
        <l id="s5:l6" met="4" type="+-">Hollandsch Oude Leeuw, roept <rhyme label="c" type="m" corresp="#s5:l5">nog</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Kom maer, kom maer, onder myn <rhyme label="d" type="m">poot</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Trouwe ooze <rhyme label="d" type="m" corresp="#s5:l7">Bondgenoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
