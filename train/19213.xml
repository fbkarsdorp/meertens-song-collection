    </tags>
    <melody>Het kwam een man van Schelde</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CHristus is <rhyme label="a" type="f">verresen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghedoyt heeft hy de <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En ons verdient mits <rhyme label="a" type="f" corresp="#s1:l1">desen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sijns vaders rijcke <rhyme label="b" type="m" corresp="#s1:l2">groot</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Hy heeft die <rhyme label="c" type="m">hel//fel//snel</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Den viant verwonnen en niemant <rhyme label="c" type="m" corresp="#s1:l5">el</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat Eua hadde <rhyme label="a" type="f">bedoruen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En Adam heeft <rhyme label="b" type="m">verbuert</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Heeft Christus peys <rhyme label="a" type="f" corresp="#s2:l1">verworuen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Tuerbont heeft hy <rhyme label="b" type="m" corresp="#s2:l2">gheschuert</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Tviants <rhyme label="c" type="m">schacht//cracht//macht</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Is heel triumphelijck tonder <rhyme label="c" type="m" corresp="#s2:l5">bracht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Tserpent dat is <rhyme label="a" type="f">vertreden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat deerste lueghen <rhyme label="b" type="m">loech</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sijn hooft mit allen den <rhyme label="a" type="f" corresp="#s3:l1">leden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ueracht dat ons <rhyme label="b" type="m" corresp="#s3:l2">bedroech</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Tviants <rhyme label="c" type="m">list//mist//quist</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Hy blijft verwonnen die maecten <rhyme label="c" type="m" corresp="#s3:l5">twist</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Tviants septer is <rhyme label="a" type="f">ghebroken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sijn croon die is <rhyme label="b" type="m">ontwee</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Als Sathan wonder wou <rhyme label="a" type="f" corresp="#s4:l1">coken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Brack hem zijn mes int <rhyme label="b" type="m" corresp="#s4:l2">schee</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Hy wert <rhyme label="c" type="m">bespot//bot//sot</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Dat hy hem stelden teghen <rhyme label="c" type="m" corresp="#s4:l5">Godt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dat vlees wou Sathan <rhyme label="a" type="f">hulpen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">De weerelt wou wonder <rhyme label="b" type="m">doen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">De sonde dat leelijck <rhyme label="a" type="f" corresp="#s5:l1">wulpen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Beriet dat <rhyme label="b" type="m" corresp="#s5:l2">venisoen</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Een hielp int <rhyme label="c" type="m">crijt//spijt/nijt</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Christus verrysen want den <rhyme label="c" type="m" corresp="#s5:l5">strijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nv laet ons Christum <rhyme label="a" type="f">louen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Onsen verlosser, <rhyme label="b" type="m">want</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Hi ons verwerft hier <rhyme label="a" type="f" corresp="#s6:l1">bouen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Sijns vaders rijck en <rhyme label="b" type="m" corresp="#s6:l2">lant</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Dus weest met <rhyme label="c" type="m">my//bly//ghy</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Cristus triumphe maect ons <rhyme label="c" type="m" corresp="#s6:l5">vry</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
