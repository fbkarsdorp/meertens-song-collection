  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AY set u <rhyme label="a" type="m">Philida</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wat onder dese <rhyme label="b" type="f">Boompjes</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Neen Coridon, ick <rhyme label="a" type="m" corresp="#s1:l1">gae</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Na gints waert, daer de <rhyme label="b" type="f" corresp="#s1:l2">stroompjes</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van 't gulle ruysent <rhyme label="c" type="f">Beeckje</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Welt door de Duyntjes <rhyme label="d" type="m">heen</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Dat 's weer u oude <rhyme label="c" type="f" corresp="#s1:l5">treckje</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Neen Philida, o <rhyme label="d" type="m" corresp="#s1:l6">neen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer set u hier in 't <rhyme label="a" type="m">groen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">By dese hoogge <rhyme label="b" type="f">Liende</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Dat wil ick willigh <rhyme label="a" type="m" corresp="#s2:l1">doen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Coridon, mijn <rhyme label="b" type="f" corresp="#s2:l2">beminde</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Voldoet mijn lieve <rhyme label="c" type="f">lusjes</rhyme></l>
        <l id="s2:l6" met="3" type="-+">(Mijn Waerde <rhyme label="d" type="m">Paragon</rhyme>)</l>
        <l id="s2:l7" met="3" type="-+">Met uwe soete <rhyme label="c" type="f" corresp="#s2:l5">kusjes</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">By dese koele <rhyme label="d" type="m" corresp="#s2:l6">Bron'</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Kusjes Minnaer <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En sal ick niet <rhyme label="b" type="f">weyg'ren</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Mijn lusjens suycker <rhyme label="a" type="m" corresp="#s3:l1">soet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Nu heel ten Hemel <rhyme label="b" type="f" corresp="#s3:l2">steyg'ren</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Druckt u bedoude <rhyme label="c" type="f">Lipjes</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat aen mijn Montje <rhyme label="d" type="m">raeckt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Op dat het aen de <rhyme label="c" type="f" corresp="#s3:l5">tipjes</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van mijn jonck Hartje <rhyme label="d" type="m" corresp="#s3:l6">smaeckt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Voor desen stroyen <rhyme label="a" type="m">Hoet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Kies ick geen gulde <rhyme label="b" type="f">Croonen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Noch voor u lonckjes <rhyme label="a" type="m" corresp="#s4:l1">soet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Koos ick geen Koninghs <rhyme label="b" type="f" corresp="#s4:l2">Throonen</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Wilt my maer niet <rhyme label="c" type="f">af-wijcken</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">Mijn Lief mijn <rhyme label="d" type="m">Coridon</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">'k Sal u noch trou doen <rhyme label="c" type="f" corresp="#s4:l5">blijcken</rhyme>:</l>
        <l id="s4:l8" met="3" type="-+">En zweer u by de <rhyme label="d" type="m" corresp="#s4:l6">Son</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dat eer den Walvisch <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Sal op het Landt <rhyme label="b" type="f">geneeren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Eer sal des <rhyme label="a" type="m" corresp="#s5:l1">Werelts-kloot</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hem 't onderst' boven <rhyme label="b" type="f" corresp="#s5:l2">keeren</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Eer dat ick sou <rhyme label="c" type="f">begeven</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Mijn over trouwe <rhyme label="d" type="m">Min</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Mijn hart, mijn ziel, mijn <rhyme label="c" type="f" corresp="#s5:l5">leven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Mijn <rhyme label="d" type="m" corresp="#s5:l6">liefdens-afgoddin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wel Coridon ick <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mijn vee na Huys toe <rhyme label="b" type="f">drijven</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Neen Philidaetje <rhyme label="a" type="m" corresp="#s6:l1">soet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Wilt noch wat by my <rhyme label="b" type="f" corresp="#s6:l2">blijven</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Neen 't Vee dat gaet <rhyme label="c" type="f">verstroyen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Den eenen verr' van d' <rhyme label="d" type="m">aer</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Neen al u Ram, en <rhyme label="c" type="f" corresp="#s6:l5">Oyen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Die zijn noch by <rhyme label="d" type="m" corresp="#s6:l6">malkaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maer ick moet na mijn <rhyme label="a" type="m">rust</rhyme></l>
        <l id="s7:l2" met="3" type="-+">De Son beghint te <rhyme label="b" type="f">dalen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hoe sout ghy <rhyme label="a" type="m" corresp="#s7:l1">ongekust</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dan met u Schaepjes <rhyme label="b" type="f" corresp="#s7:l2">dwalen</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">O neen ick sal u <rhyme label="c" type="f">leyden</rhyme></l>
        <l id="s7:l6" met="3" type="-+">(Naer uwe lieve <rhyme label="d" type="m">lust</rhyme>)</l>
        <l id="s7:l7" met="3" type="-+">Door Velden, en door <rhyme label="c" type="f" corresp="#s7:l5">Heyen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Maer eerst mijn brandt <rhyme label="d" type="m" corresp="#s7:l6">geblust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
