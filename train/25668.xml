    <melody>Ik heb gedragen wel zeven jaar</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Leenaert Clock</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">My doch bystaet, O lieve <rhyme label="a" type="m">Heerm</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ghy weet ick ben u schepsel <rhyme label="a" type="m" corresp="#s1:l1">teer</rhyme></l>
        <l id="s1:l3" met="2" type="-+">En u <rhyme label="b" type="m">handtwerck</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Draeght sorgh voor my, dats mijn <rhyme label="a" type="m" corresp="#s1:l1">begeer</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En maeckt my <rhyme label="b" type="m" corresp="#s1:l3">sterck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Alsoo ghy my het leven <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Licht ghegheven op dit <rhyme label="a" type="m" corresp="#s2:l1">termijn</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">En my <rhyme label="b" type="m">bereyt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Soo wilt oock mijn behoeder <rhyme label="a" type="m" corresp="#s2:l1">zijn</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Door u <rhyme label="b" type="m" corresp="#s2:l3">goetheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En bewaert my onder u <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Soo langh ick leef int Aerdtsche <rhyme label="a" type="m" corresp="#s3:l1">landt</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">En gheeft doch <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">O lieve Heer, een recht <rhyme label="a" type="m" corresp="#s3:l1">verstandt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Met moede <rhyme label="b" type="m" corresp="#s3:l3">vry</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ylende soo nae Schrifts <rhyme label="a" type="m">vermaen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Om in u vreese voort te <rhyme label="a" type="m" corresp="#s4:l1">gaen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">En uwen <rhyme label="b" type="m">wil</rhyme></l>
        <l id="s4:l4" met="4" type="-+">In mijn beste recht te <rhyme label="a" type="m" corresp="#s4:l1">verslaen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Sonder <rhyme label="b" type="m" corresp="#s4:l3">gheschil</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Kent dat ick ben dickmaels <rhyme label="a" type="m">bevreest</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Sendt my doch uwen goeden <rhyme label="a" type="m" corresp="#s5:l1">Gheest</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Die my <rhyme label="b" type="m">bewaer</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En my versterck in dit <rhyme label="a" type="m" corresp="#s5:l1">tempeest</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">O lieve <rhyme label="b" type="m" corresp="#s5:l3">Vaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">En laet my doch beswaren <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Door Satans list die nu <rhyme label="a" type="m" corresp="#s6:l1">gheschiet</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Seer <rhyme label="b" type="m">menichfout</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Met uwer macht, die ghy <rhyme label="a" type="m" corresp="#s6:l1">aenbiet</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">My <rhyme label="b" type="m" corresp="#s6:l3">onderhout</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nimmermeer Heer my en <rhyme label="a" type="m">verlaet</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Maer verlost my van alle <rhyme label="a" type="m" corresp="#s7:l1">quaet</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Weest ghy mijn <rhyme label="b" type="m">cracht</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Ghy kent doch mijnen swacken <rhyme label="a" type="m" corresp="#s7:l1">staet</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Mijn cleyne <rhyme label="b" type="m" corresp="#s7:l3">macht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Christi Iesu Gods Sone <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Om my te helpen u doch <rhyme label="a" type="m" corresp="#s8:l1">spoet</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Toeft niet te <rhyme label="b" type="m">langh</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En gheeft my eenen goeden <rhyme label="a" type="m" corresp="#s8:l1">moet</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Als my is <rhyme label="b" type="m" corresp="#s8:l3">bangh</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Lopen sal ick u dan <rhyme label="a" type="m">subijt</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Als ghy lief Heere my <rhyme label="a" type="m" corresp="#s9:l1">bevrijt</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Aen ziel en <rhyme label="b" type="m">lijf</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En gheeft victory in den <rhyme label="a" type="m" corresp="#s9:l1">strijt</rhyme></l>
        <l id="s9:l5" met="2" type="-+">In mijn <rhyme label="b" type="m" corresp="#s9:l3">bedrijf</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Alle crachten in my <rhyme label="a" type="m">ghemeyn</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Sullen u loven groot en <rhyme label="a" type="m" corresp="#s10:l1">cleyn</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Mijn leven <rhyme label="b" type="m">lanck</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">En u gheven den prijs <rhyme label="a" type="m" corresp="#s10:l1">alleyn</rhyme></l>
        <l id="s10:l5" met="2" type="-+">Met grooten <rhyme label="b" type="m" corresp="#s10:l3">danck</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">En daer op wil ick hopen <rhyme label="a" type="m">vast</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Dat ghy sult draghen mijnen <rhyme label="a" type="m" corresp="#s11:l1">last</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">My helpen <rhyme label="b" type="m">voort</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Tot mijn gheloove groeyt en <rhyme label="a" type="m" corresp="#s11:l1">wast</rhyme></l>
        <l id="s11:l5" met="2" type="-+">Alsoo’t <rhyme label="b" type="m" corresp="#s11:l3">behoort</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Staet by, lief Heer, ick nu <rhyme label="a" type="m">beviel</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Mijnen gheest, t’lichaem en mijn <rhyme label="a" type="m" corresp="#s12:l1">ziel</rhyme></l>
        <l id="s12:l3" met="2" type="-+">In u <rhyme label="b" type="m">behoet</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Mijn leven, sterven al <rhyme label="a" type="m" corresp="#s12:l1">gheheel</rhyme></l>
        <l id="s12:l5" met="2" type="-+">Stiert doch int <rhyme label="b" type="m" corresp="#s12:l3">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
