<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13930</id>
    <date>
      <year-start>1670</year-start>
      <year-end>1670</year-end>
    </date>
    <location>
      <orginal-name>middelburg</orginal-name>
      <standard-name>Middelburg, The Netherlands</standard-name>
      <coordinates>
        <lon>51.4997302</lon>
        <lat>3.6136962</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>M. van de Speybroek</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DEn Mey-boom <rhyme label="a" type="m">triumphant</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tot Syon in den <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Heeft den Vader <rhyme label="a" type="m" corresp="#s1:l1">geplant</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dies zy hem eeuwig <rhyme label="b" type="m" corresp="#s1:l2">lof</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Hy staet wat <rhyme label="c" type="m">uyt-gebreydt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met bladers schoon <rhyme label="d" type="m">omdaen</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Komt nu met <rhyme label="c" type="m" corresp="#s1:l5">neerstigheyt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En laet ons tot hem <rhyme label="d" type="m" corresp="#s1:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gaet nu om plucken <rhyme label="a" type="m">uyt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Al tot den Mey-boom <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met de Cantijcke <rhyme label="a" type="m" corresp="#s2:l1">Bruyt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In den Hof van <rhyme label="b" type="m" corresp="#s2:l2">Syon</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Sijn vruchten maken <rhyme label="c" type="m">sterck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat gy kloeck en <rhyme label="d" type="m">gesont</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Sult konnen doen sijn <rhyme label="c" type="m" corresp="#s2:l5">werck</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En houden sijn <rhyme label="d" type="m" corresp="#s2:l6">Verbondt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Seer lieffelijck is <rhyme label="a" type="m">hy</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En van kracht <rhyme label="b" type="m">wonderbaer</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Genesende daer <rhyme label="a" type="m" corresp="#s3:l1">by</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Den bedroefden <rhyme label="b" type="m" corresp="#s3:l2">Sondaer</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Hy roept; Komt al tot <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die zijt belast, <rhyme label="d" type="m">belaen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ick sal u maken <rhyme label="c" type="m" corresp="#s3:l5">vry</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En in genaed' <rhyme label="d" type="m" corresp="#s3:l6">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den boom om ons <rhyme label="a" type="m">misdaet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Was eens neder <rhyme label="b" type="m">gevelt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Sijn Vader <rhyme label="a" type="m" corresp="#s4:l1">delicaet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Heeft hem we'er <rhyme label="b" type="m" corresp="#s4:l2">op-gestelt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En verheven seer <rhyme label="c" type="m">hoogh</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Boven d' Hem'len <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat hy hem nu <rhyme label="c" type="m" corresp="#s4:l5">vertooght</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Tot in des Vaders <rhyme label="d" type="m" corresp="#s4:l6">schoot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Met verachtinge <rhyme label="a" type="m">snoot</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Moest hy uyt den <rhyme label="b" type="m">Wijngaert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Seer schandelijck ter <rhyme label="a" type="m" corresp="#s5:l1">doodt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Aen een Cruyce <rhyme label="b" type="m" corresp="#s5:l2">bezwaert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Gedaente en <rhyme label="c" type="m">schoonheyt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In hem doen gansch <rhyme label="d" type="m">verviel</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat men hem door dit <rhyme label="c" type="m" corresp="#s5:l5">leyt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Al voor d' onwaertste <rhyme label="d" type="m" corresp="#s5:l6">hiel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Eenen verhoogden <rhyme label="a" type="m">Naem</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hy doen verkregen <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die den Vader <rhyme label="a" type="m" corresp="#s6:l1">bequaem</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Boven ander hem <rhyme label="b" type="m" corresp="#s6:l2">geeft</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Dat alle knien (hoe <rhyme label="c" type="m">sterck</rhyme>)</l>
        <l id="s6:l6" met="3" type="-+">Haer moeten buygen <rhyme label="d" type="m">tem</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Boven in 's Hemels <rhyme label="c" type="m" corresp="#s6:l5">perck</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En in d' Aerde voor <rhyme label="d" type="m" corresp="#s6:l6">hem</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Een verspieder seer <rhyme label="a" type="m">loos</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Was med' in 't Hof <rhyme label="b" type="m">getelt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Door 't werck des Duyvels <rhyme label="a" type="m" corresp="#s7:l1">boos</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Verkocht den Mey om <rhyme label="b" type="m" corresp="#s7:l2">gelt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Seer haest berouw <rhyme label="c" type="m">ontfinck</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Als hy 't heeft <rhyme label="d" type="m">overdocht</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Na de Kooper hy <rhyme label="c" type="m" corresp="#s7:l5">ginck</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En 't gelt weer-omme <rhyme label="d" type="m" corresp="#s7:l6">brocht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Moedeloos <rhyme label="a" type="m">desperaet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Buyten den Hof hy <rhyme label="b" type="m">bleef</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Een schandelijck dinck <rhyme label="a" type="m" corresp="#s8:l1">quaet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Aen hem selven <rhyme label="b" type="m" corresp="#s8:l2">bedreef</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Verhingh hem aen een <rhyme label="c" type="m">strick</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Sijn lichaem borst <rhyme label="d" type="m">ontween</rhyme></l>
        <l id="s8:l7" met="3" type="-+">'t Ingewandt <rhyme label="c" type="m" corresp="#s8:l5">gruwelick</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Is oock van hem <rhyme label="d" type="m" corresp="#s8:l6">gescheen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Die hier den Mey-boom <rhyme label="a" type="m">schent</rhyme></l>
        <l id="s9:l2" met="3" type="-+">En sijn tacken <rhyme label="b" type="m">af-houwt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Nemen een schandigh <rhyme label="a" type="m" corresp="#s9:l1">endt</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Daerom en weest niet <rhyme label="b" type="m" corresp="#s9:l2">stout</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">Of uwen Boom hy <rhyme label="c" type="m">sal</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Uyt-wortelen <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">En 't eeuwigh <rhyme label="c" type="m" corresp="#s9:l5">ongeval</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Wert noch in 't lest haer <rhyme label="d" type="m" corresp="#s9:l6">deel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Prince Lief-hebbers <rhyme label="a" type="m">van</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Den Mey-boom schoon <rhyme label="b" type="m">verciert</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Draegt hem in 't herte <rhyme label="a" type="m" corresp="#s10:l1">dan</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Dat hy by u <rhyme label="b" type="m" corresp="#s10:l2">logiert</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En sijn rancken <rhyme label="c" type="m">bemint</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Als u selven <rhyme label="d" type="m">gelijck</rhyme>:</l>
        <l id="s10:l7" met="3" type="-+">Blijft ghy alsoo <rhyme label="c" type="m" corresp="#s10:l5">gesint</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Gy komt met hem in 't <rhyme label="d" type="m" corresp="#s10:l6">Rijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
