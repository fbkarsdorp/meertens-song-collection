  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik kwam laast by <rhyme label="a" type="m">Clemeen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zy kon myn hart <rhyme label="b" type="f">bekoren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik vond haar in ‘t <rhyme label="a" type="m" corresp="#s1:l1">gemeen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Zy had haar Poes <rhyme label="b" type="f" corresp="#s1:l2">verloren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Al wat ik had, Zy riep myn <rhyme label="c" type="m">schat</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Maar ik zei Poes, Poes, <rhyme label="d" type="m">Poes</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Maar ik zei Poes, Poes, <rhyme label="d" type="m" corresp="#s1:l6">Poes</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Ach ach myn <rhyme label="c" type="m" corresp="#s1:l5">Kat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">‘t Was een zo een aardig <rhyme label="a" type="m">Beest</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik hield hem veel en <rhyme label="b" type="f">waarden</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">‘t Is wel en Swart <rhyme label="a" type="m" corresp="#s2:l1">geweest</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met bonte knevel <rhyme label="b" type="f" corresp="#s2:l2">baarden</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">‘t Stont hem zo Eel, Als een <rhyme label="c" type="m">Iuweel</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Al van myn Poes, Poes, <rhyme label="d" type="m">Poes</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Al van myn Poes, Poes, <rhyme label="d" type="m" corresp="#s2:l6">Poes</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Hield ik zo <rhyme label="c" type="m" corresp="#s2:l5">veel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik zeid wel fiere <rhyme label="a" type="m">Maagt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Kan ik geen troost <rhyme label="b" type="f">verwerve</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zo moet hy die U <rhyme label="a" type="m" corresp="#s3:l1">klaagt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Uit zuiver liefde <rhyme label="b" type="f" corresp="#s3:l2">Sterven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Gunt my uw schoot, Uit liefde <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Ik zal uw Poes, Poes, <rhyme label="d" type="m">Poes</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ik zal uw Poes, Poes, <rhyme label="d" type="m" corresp="#s3:l6">Poes</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Zyn tot der <rhyme label="c" type="m" corresp="#s3:l5">Dood</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doe kreeg Climeen een <rhyme label="a" type="m">bloos</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Op haar snee witte <rhyme label="b" type="f">Kaken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En sprak zult gy <rhyme label="a" type="m" corresp="#s4:l1">altoos</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In liefde tot my <rhyme label="b" type="f" corresp="#s4:l2">blaken</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Weest dan niet flau, Kom kus my <rhyme label="c" type="m">gau</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Doe riep zy Poes, Poes, <rhyme label="d" type="m">Poes</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Doe riep zy Poes, Poes, <rhyme label="d" type="m" corresp="#s4:l6">Poes</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En ik zeid <rhyme label="c" type="m" corresp="#s4:l5">Mau</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
