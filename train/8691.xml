  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs Faustina in de <rhyme label="a" type="m">min</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van een Prince wiert <rhyme label="b" type="f">bestreden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Heeft de kuyse <rhyme label="a" type="m" corresp="#s1:l1">Herderin</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Lust bedwongen door de <rhyme label="b" type="f" corresp="#s1:l2">reden</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Sy (om minne aen <rhyme label="b" type="f" corresp="#s1:l2">gebeden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Op beloft van eer en <rhyme label="c" type="m">trou</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Om te zijn een Princen <rhyme label="c" type="m" corresp="#s1:l6">Vrou</rhyme>)</l>
        <l id="s1:l8" met="4" type="+-">Heeft door reyne kuysheyts <rhyme label="d" type="m">wet</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Princen minne af <rhyme label="d" type="m" corresp="#s1:l8">geset</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vw' o! maeghdelijck <rhyme label="a" type="m">geslacht</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Tot een voorbeeld om te <rhyme label="b" type="f">leeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat ghy door der deughden <rhyme label="a" type="m" corresp="#s2:l1">kracht</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Minne-lust moet tegen <rhyme label="b" type="f" corresp="#s2:l2">weeren</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Schoon dat minnaers Eden <rhyme label="b" type="f" corresp="#s2:l2">sweeren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Trou (na mins genot) te <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ionghe maeghden leer van <rhyme label="c" type="m" corresp="#s2:l6">mijn</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Minne Eden licht <rhyme label="d" type="m">gelooft</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Meenigh Maeghd haer kuysheyt <rhyme label="d" type="m" corresp="#s2:l8">rooft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Schoon dat u een Minnaer <rhyme label="a" type="m">vrijt</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Groot van rijckdom en <rhyme label="b" type="f">vermogen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ghy van lage afkomst <rhyme label="a" type="m" corresp="#s3:l1">zijt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En in kleynheyt op <rhyme label="b" type="f" corresp="#s3:l2">getoghen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Wilt hem efter niet <rhyme label="b" type="f" corresp="#s3:l2">gedogen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Dat hij in zijn minne <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Verder als het deughd <rhyme label="c" type="m" corresp="#s3:l6">toe-laet</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Leer hier dat ghy Ionghe <rhyme label="d" type="m">Maeght</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Sorge voor u kuysheyd <rhyme label="d" type="m" corresp="#s3:l8">draeght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Leer hier aen een <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Geyle lusten tegen <rhyme label="b" type="f">strijen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Schuwt de Minnaers die om <rhyme label="a" type="m" corresp="#s4:l1">min</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Meerder als uyt liefde <rhyme label="b" type="f" corresp="#s4:l2">vrijen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Wilt o Ionghe Maeghden <rhyme label="b" type="f" corresp="#s4:l2">mijen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Minne-stricken, Minnaers <rhyme label="c" type="m">reen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Minnaers smeecken en <rhyme label="c" type="m" corresp="#s4:l6">gebeen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Schuwt in't vrijen 'tminne <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Buyten suycker, binnen <rhyme label="d" type="m" corresp="#s4:l8">roet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
