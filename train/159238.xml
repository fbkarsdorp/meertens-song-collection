<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159238</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O welig Amsterdam</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOmt hier Huys-luyden <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met vreughden-rijck <rhyme label="a" type="m" corresp="#s1:l1">geschal</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Singht met my dit <rhyme label="b" type="m">Liet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Tot lof en eer van 't <rhyme label="c" type="m">Landt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer van ghy hebt <rhyme label="c" type="m" corresp="#s1:l4">verstandt</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Swicht voor niemandt <rhyme label="b" type="m" corresp="#s1:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick weet ghy zijt schier <rhyme label="a" type="m">slaeu</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Om dat de Stee-man <rhyme label="a" type="m" corresp="#s2:l1">raeu</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dickwijls u <rhyme label="b" type="m">bespot</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Maer ghy op haer niet <rhyme label="c" type="m">past</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Houdt doch in't klouwen <rhyme label="c" type="m" corresp="#s2:l4">vast</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Viert haer gantsch geen <rhyme label="b" type="m" corresp="#s2:l3">bot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Stae vast gelijck een <rhyme label="a" type="m">kluts</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En smijt haer op de <rhyme label="a" type="m" corresp="#s3:l1">muts</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Koe-dreck ende <rhyme label="b" type="m">slijck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die u verachten <rhyme label="c" type="m">hier</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van wegen haer <rhyme label="c" type="m" corresp="#s3:l4">playsier</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Doch met <rhyme label="b" type="m" corresp="#s3:l3">ongelijck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Komt tegen haer te <rhyme label="a" type="m">Velt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En toont u als een <rhyme label="a" type="m" corresp="#s4:l1">Helt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Treck u Wapen <rhyme label="b" type="m">an</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met al u Ploegh <rhyme label="c" type="m">geweer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En slaet haer plat ter <rhyme label="c" type="m" corresp="#s4:l4">neer</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Lustigh als een <rhyme label="b" type="m" corresp="#s4:l3">Man</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick wenste dat de <rhyme label="a" type="m">Stee</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Den Huys-man liet met <rhyme label="a" type="m" corresp="#s5:l1">vree</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Woonen in sijn <rhyme label="b" type="m">Stulp</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En dat een yder <rhyme label="c" type="m">dacht</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Van wien hy, nae 'k het <rhyme label="c" type="m" corresp="#s5:l4">acht</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Krijght de meeste <rhyme label="b" type="m" corresp="#s5:l3">hulp</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Was niet het groene <rhyme label="a" type="m">Landt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Het eerste datmen <rhyme label="a" type="m" corresp="#s6:l1">vandt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Vol playsierigh <rhyme label="b" type="m">groen</rhyme>?</l>
        <l id="s6:l4" met="3" type="-+">Daer menschen ende <rhyme label="c" type="m">Vee</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Haer stracks vermaeckten <rhyme label="c" type="m" corresp="#s6:l4">mee</rhyme>:</l>
        <l id="s6:l6" met="3" type="+-">Och waer waert gy <rhyme label="b" type="m" corresp="#s6:l3">doen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy seght, de Boer is <rhyme label="a" type="m">plomp</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En datter in sijn <rhyme label="a" type="m" corresp="#s7:l1">romp</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Gantsch geen wijsheyt <rhyme label="b" type="m">steeckt</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Maer mist ghy hem in 't <rhyme label="c" type="m">Velt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ghy waert genoegh <rhyme label="c" type="m" corresp="#s7:l4">gequelt</rhyme>;</l>
        <l id="s7:l6" met="3" type="+-">Dus niet schimpigh <rhyme label="b" type="m" corresp="#s7:l3">streeckt</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hy heeft soo wel <rhyme label="a" type="m">verstant</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Te trecken vrucht van 't <rhyme label="a" type="m" corresp="#s8:l1">Landt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Als ghy Stee-man <rhyme label="b" type="m">doet</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Hy Spit, hy Plant, hy <rhyme label="c" type="m">Saeyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En het volwassen <rhyme label="c" type="m" corresp="#s8:l4">Maeyt</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Hy in <rhyme label="b" type="m" corresp="#s8:l3">overvloet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Komt dan, en geeft hem <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Van wien het eerst komt <rhyme label="a" type="m" corresp="#s9:l1">of</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Wordet wijs, en <rhyme label="b" type="m">ist</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat ghy een Huys-man <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Veracht den selven <rhyme label="c" type="m" corresp="#s9:l4">niet</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Want hier dient geen <rhyme label="b" type="m" corresp="#s9:l3">twist</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
