    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">ICk voelde strack mijn hert van liefde <rhyme label="a" type="f">blaken</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Door d’aerdigheden en het soet <rhyme label="b" type="m">gesicht</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Van mijn <rhyme label="c" type="f">Beminde</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">t’Wijl Venus <rhyme label="b" type="m" corresp="#s1:l2">wicht</rhyme></l>
        <l id="s1:l5" met="2" type="-+">My scheen te <rhyme label="c" type="f" corresp="#s1:l3">binden</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Door sijn <rhyme label="b" type="m" corresp="#s1:l2">minne-schicht</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Aen sijn <rhyme label="b" type="m" corresp="#s1:l2">gericht</rhyme>.</l>
      </lg>
    </lg>
