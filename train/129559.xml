      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O soetste   <rhyme label="a" type="m">Vrindin</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">O Sijlsteen der <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme></l>
        <l id="s1:l3" met="2" type="-+">En trooster der <rhyme label="b" type="f">herten</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Verjaagt mijne <rhyme label="b" type="f" corresp="#s1:l3">smerten</rhyme></l>
        <l id="s1:l5" met="2" type="-+">En pijn uit mijn <rhyme label="a" type="m" corresp="#s1:l1">sin</rhyme>;</l>
        <l id="s1:l6" met="2" type="-+">Keert uwe <rhyme label="c" type="f">zinnen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Tot het <rhyme label="c" type="f" corresp="#s1:l6">wederminne</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Dan doen ik <rhyme label="a" type="m" corresp="#s1:l1">gewin</rhyme>.</l>
        <l id="s1:l9" met="3" type="+-">Stilt mijn droeve <rhyme label="d" type="m">pijn</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Wilt niet stuer   meer <rhyme label="e" type="f">wesen</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Maar mijn wont <rhyme label="e" type="f" corresp="#s1:l10">genesen</rhyme></l>
        <l id="s1:l12" met="3" type="+-">Eer dat ik <rhyme label="d" type="m" corresp="#s1:l9">verdwijn</rhyme>.</l>
        <l id="s1:l13" met="2" type="-+">Gy zijt mijn <rhyme label="f" type="m">vreugt</rhyme>,</l>
        <l id="s1:l14" met="2" type="-+">Den Troost van mijn <rhyme label="f" type="m" corresp="#s1:l13">Jeugt</rhyme>.</l>
        <l id="s1:l15" met="2" type="-+">U schoonheyt bekoort <rhyme label="g" type="f">my</rhyme>,</l>
        <l id="s1:l16" met="2" type="-+">U Liefde Doorboort <rhyme label="g" type="f" corresp="#s1:l15">my</rhyme>,</l>
        <l id="s1:l17" met="2" type="-+">Maakt my eens <rhyme label="f" type="m" corresp="#s1:l13">verheugt</rhyme>.</l>
        <l id="s1:l18" met="3" type="+-">Zijt niet meer soo <rhyme label="h" type="m">straf</rhyme></l>
        <l id="s1:l19" met="3" type="+-">Anders gy vermoort <rhyme label="g" type="f" corresp="#s1:l15">my</rhyme></l>
        <l id="s1:l20" met="3" type="+-">En stoot my in ‘t <rhyme label="h" type="m" corresp="#s1:l18">graf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ik heb nu soo <rhyme label="a" type="m">lank</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Gevoelt de Liefdes <rhyme label="a" type="m" corresp="#s2:l1">dwank</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Bekoort door uw <rhyme label="b" type="f">seeden</rhyme></l>
        <l id="s2:l4" met="2" type="-+">En u nette <rhyme label="b" type="f" corresp="#s2:l3">Leden</rhyme></l>
        <l id="s2:l5" met="2" type="-+">En uw soete <rhyme label="a" type="m" corresp="#s2:l1">sank</rhyme>.</l>
        <l id="s2:l6" met="2" type="-+">Ik heb u <rhyme label="c" type="f">gebeden</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Altijdt naar <rhyme label="c" type="f" corresp="#s2:l6">getreeden</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Uwen lugten <rhyme label="a" type="m" corresp="#s2:l1">gank</rhyme>:</l>
        <l id="s2:l9" met="3" type="+-">En wat dat ik <rhyme label="d" type="m">vraag</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Ten heeft geen <rhyme label="e" type="f">vermooge</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Gy wort niet <rhyme label="e" type="f" corresp="#s2:l10">bewoogen</rhyme>.</l>
        <l id="s2:l12" met="3" type="+-">Wat ik sugt of <rhyme label="d" type="m" corresp="#s2:l9">klaag</rhyme>,</l>
        <l id="s2:l13" met="2" type="-+">Gy zijt van <rhyme label="f" type="m">staal</rhyme></l>
        <l id="s2:l14" met="2" type="-+">Dat   geen soete <rhyme label="f" type="m" corresp="#s2:l13">taal</rhyme>,</l>
        <l id="s2:l15" met="2" type="-+">Geen sugten of <rhyme label="g" type="f">tranen</rhyme></l>
        <l id="s2:l16" met="2" type="-+">Een weg konnen <rhyme label="g" type="f" corresp="#s2:l15">Baanen</rhyme></l>
        <l id="s2:l17" met="2" type="-+">Tot hulp van mijn <rhyme label="f" type="m" corresp="#s2:l13">quaal</rhyme>;</l>
        <l id="s2:l18" met="3" type="+-">Dat   al mijn <rhyme label="h" type="m">gekrijt</rhyme></l>
        <l id="s2:l19" met="3" type="+-">U niet kan <rhyme label="g" type="f" corresp="#s2:l15">opmaanen</rhyme></l>
        <l id="s2:l20" met="3" type="+-">Tot <rhyme label="h" type="m" corresp="#s2:l18">medoogentheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Lank duerige <rhyme label="a" type="m">tijdt</rhyme> -</l>
        <l id="s3:l2" met="2" type="-+">Die alles <rhyme label="a" type="m" corresp="#s3:l1">verslijdt</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Die Bittere <rhyme label="b" type="f">saaken</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Kan soet doen <rhyme label="b" type="f" corresp="#s3:l3">smaaken</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Die ‘t yser door <rhyme label="a" type="m" corresp="#s3:l1">byt</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Wien niet staat <rhyme label="c" type="f">tegen</rhyme> -</l>
        <l id="s3:l7" met="2" type="-+">Kan nog niet <rhyme label="c" type="f" corresp="#s3:l6">bewegen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">U <rhyme label="a" type="m" corresp="#s3:l1">hertneckigheyt</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">Het nat, door den <rhyme label="d" type="m">val</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Doorboort harde <rhyme label="e" type="f">steenen</rhyme></l>
        <l id="s3:l11" met="3" type="+-">En mijn stadigh <rhyme label="e" type="f" corresp="#s3:l10">weenen</rhyme></l>
        <l id="s3:l12" met="3" type="+-">Helpt my niet <rhyme label="d" type="m" corresp="#s3:l9">mendal</rhyme>.</l>
        <l id="s3:l13" met="2" type="-+">Leeuwin, hoe kant <rhyme label="f" type="m">sijn</rhyme></l>
        <l id="s3:l14" met="2" type="-+">Dat gy al mijn <rhyme label="f" type="m" corresp="#s3:l13">pijn</rhyme></l>
        <l id="s3:l15" met="2" type="-+">Gerust kont <rhyme label="g" type="f">beoogen</rhyme></l>
        <l id="s3:l16" met="2" type="-+">En sonder <rhyme label="g" type="f" corresp="#s3:l15">meedoogen</rhyme></l>
        <l id="s3:l17" met="2" type="-+">Siet dat ik <rhyme label="f" type="m" corresp="#s3:l13">verdwijn</rhyme>!</l>
        <l id="s3:l18" met="3" type="+-">Ach, hoe kan u <rhyme label="h" type="m">hert</rhyme></l>
        <l id="s3:l19" met="3" type="+-">Nog niet zijn <rhyme label="g" type="f" corresp="#s3:l15">bewogen</rhyme></l>
        <l id="s3:l20" met="3" type="+-">Door soo groote <rhyme label="h" type="m" corresp="#s3:l18">smert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Cupido, wreed <rhyme label="a" type="m">wicht</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Waar blijft uwe <rhyme label="a" type="m" corresp="#s4:l1">schicht</rhyme>?</l>
        <l id="s4:l3" met="2" type="-+">‘t Schijnt gy op my Boos <rhyme label="b" type="f">seyt</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+">Waar is uwe <rhyme label="b" type="f" corresp="#s4:l3">Loosheydt</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Die my heeft <rhyme label="a" type="m" corresp="#s4:l1">verplicht</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Die my <rhyme label="c" type="f">doorgriefde</rhyme>?</l>
        <l id="s4:l7" met="2" type="-+">G’hebt den brand der <rhyme label="c" type="f" corresp="#s4:l6">liefde</rhyme></l>
        <l id="s4:l8" met="2" type="-+">In mijn hert <rhyme label="a" type="m" corresp="#s4:l1">gestigt</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Maar het is om <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s4:l10" met="3" type="+-">En ik gaan <rhyme label="e" type="f">verlooren</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Wilt gy niet <rhyme label="e" type="f" corresp="#s4:l10">bekooren</rhyme></l>
        <l id="s4:l12" met="3" type="+-">Daar mijn min op <rhyme label="d" type="m" corresp="#s4:l9">siet</rhyme>.</l>
        <l id="s4:l13" met="2" type="-+">Kont gy geene <rhyme label="f" type="m">maagt</rhyme></l>
        <l id="s4:l14" met="2" type="-+">Die Jeder <rhyme label="f" type="m" corresp="#s4:l13">behaagt</rhyme></l>
        <l id="s4:l15" met="2" type="-+">Tot liefde <rhyme label="g" type="f">verwinne</rhyme>,</l>
        <l id="s4:l16" met="2" type="-+">Soo blijf ik van <rhyme label="g" type="f" corresp="#s4:l15">binne</rhyme></l>
        <l id="s4:l17" met="2" type="-+">En altijdt <rhyme label="f" type="m" corresp="#s4:l13">geplaagt</rhyme>.</l>
        <l id="s4:l18" met="3" type="+-">Hoort mijn droeve <rhyme label="h" type="m">klacht</rhyme></l>
        <l id="s4:l19" met="3" type="+-">En bekoort haar <rhyme label="g" type="f" corresp="#s4:l15">sinne</rhyme></l>
        <l id="s4:l20" met="3" type="+-">Die mijn min <rhyme label="h" type="m" corresp="#s4:l18">veracht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Vrindin, is ‘t dan <rhyme label="a" type="m">gek</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Dat gy my den <rhyme label="a" type="m" corresp="#s5:l1">nek</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Tot nog toe blyft <rhyme label="b" type="f">keere</rhyme>:</l>
        <l id="s5:l4" met="2" type="-+">Wilt gy iets <rhyme label="b" type="f" corresp="#s5:l3">Leere</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Door soo valsen <rhyme label="a" type="m" corresp="#s5:l1">trek</rhyme>?</l>
        <l id="s5:l6" met="2" type="-+">Sijn u <rhyme label="c" type="f">gedachten</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Dat ik mijn <rhyme label="c" type="f" corresp="#s5:l6">klagten</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Met valsheit <rhyme label="a" type="m" corresp="#s5:l1">bedeck</rhyme>?</l>
        <l id="s5:l9" met="3" type="+-">Het is nu Lank <rhyme label="d" type="m">tijdt</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Van die af te <rhyme label="e" type="f">weere</rhyme>.</l>
        <l id="s5:l11" met="3" type="+-">Ik kan niet meer <rhyme label="e" type="f" corresp="#s5:l10">sweere</rhyme>,</l>
        <l id="s5:l12" met="3" type="+-">Ik ben al uyt <rhyme label="d" type="m" corresp="#s5:l9">geseyt</rhyme>.</l>
        <l id="s5:l13" met="2" type="-+">Gy kent mijn <rhyme label="f" type="m">hert</rhyme>,</l>
        <l id="s5:l14" met="2" type="-+">Gy weet mijne <rhyme label="f" type="m" corresp="#s5:l13">smert</rhyme>,</l>
        <l id="s5:l15" met="2" type="-+">Gy sent mijn de <rhyme label="g" type="f">plagen</rhyme></l>
        <l id="s5:l16" met="2" type="-+">Die ik moet <rhyme label="g" type="f" corresp="#s5:l15">verdraagen</rhyme>,</l>
        <l id="s5:l17" met="2" type="-+">In u min <rhyme label="f" type="m" corresp="#s5:l13">verwert</rhyme>.</l>
        <l id="s5:l18" met="3" type="+-">Keert dan uwe <rhyme label="h" type="m">zin</rhyme>,</l>
        <l id="s5:l19" met="3" type="+-">Laat my niet meer <rhyme label="g" type="f" corresp="#s5:l15">Klaage</rhyme>,</l>
        <l id="s5:l20" met="3" type="+-">Die u trouw <rhyme label="h" type="m" corresp="#s5:l18">Bemin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
