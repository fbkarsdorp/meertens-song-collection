<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178371</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Juich Batavieren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Juich, <rhyme label="a" type="f">Batavieren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat zich het Vrycorps <rhyme label="b" type="m">schaamdt</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Die <rhyme label="a" type="f" corresp="#s1:l1">Lasterdieren</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Al wat <rhyme label="b" type="m" corresp="#s1:l2">betaamd</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Word door hun snood <rhyme label="c" type="m">veracht</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">Zingd, daar gy hun <rhyme label="c" type="m" corresp="#s1:l5">belacht</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Vivat, <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Vivat de Prins Oranje, <rhyme label="d" type="m" corresp="#s1:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Zie nu <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">De schoone Agtste <rhyme label="b" type="m">Maart</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">De glippers <rhyme label="a" type="f" corresp="#s2:l1">schroomen</rhyme>;</l>
        <l id="s2:l4" met="2" type="-+">De Prins <rhyme label="b" type="m" corresp="#s2:l2">verjaardt</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Zy zoeken wel zyn <rhyme label="c" type="m">val</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Maar nu nu juicht het <rhyme label="c" type="m" corresp="#s2:l5">al</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Vivat, <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Vivat de Prins Oranje, <rhyme label="d" type="m" corresp="#s2:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Zou gy tans <rhyme label="a" type="f">zwygen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die God en Prins <rhyme label="b" type="m">bemind</rhyme>?</l>
        <l id="s3:l3" met="2" type="-+">Of, Rekels <rhyme label="a" type="f" corresp="#s3:l1">drygen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Aan 't Staats <rhyme label="b" type="m" corresp="#s3:l2">bewind</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Aan Prins, aan <rhyme label="c" type="m">Vaderland</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zyn wy, van ouds <rhyme label="c" type="m" corresp="#s3:l5">verpand</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Vivat <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s3:l8" met="4" type="-+">Vivat de Prins Oranje, <rhyme label="d" type="m" corresp="#s3:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Gy, <rhyme label="a" type="f">Burgerheeren</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">Gy die geen Vrycorps <rhyme label="b" type="m">schraagt</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">De Prins blyft <rhyme label="a" type="f" corresp="#s4:l1">eeren</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Word Gy <rhyme label="b" type="m" corresp="#s4:l2">belaagt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Vreest niet, wy staan, in <rhyme label="c" type="m">nood</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Voor U tot in de <rhyme label="c" type="m" corresp="#s4:l5">dood</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">Vivat, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s4:l8" met="4" type="-+">Vivat de Prins Oranje, <rhyme label="d" type="m" corresp="#s4:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">God spaar hem 't <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hy, Neêrlands Vorst en <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Wy altoos <rhyme label="a" type="f" corresp="#s5:l1">geeven</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">De Lof en <rhyme label="b" type="m" corresp="#s5:l2">Eer</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Aan hem aan 't <rhyme label="c" type="m">Vaderland</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Is goed en bloed <rhyme label="c" type="m" corresp="#s5:l5">verpand</rhyme>.</l>
        <l id="s5:l7" met="2" type="-+">Vivat <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s5:l8" met="4" type="-+">Vivat de Prins Oranje <rhyme label="d" type="m" corresp="#s5:l7">Hoezée</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
