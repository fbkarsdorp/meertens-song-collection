    <date>
      <year-start>1425</year-start>
      <year-end>1430</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="meilied" translation="May song">meilied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">So der meye zich lat <rhyme label="a" type="f">scouwen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">scone in dem ghebluinde <rhyme label="a" type="f" corresp="#s1:l1">ouwyn</rhyme></l>
        <l id="s1:l3" met="4" type="+-">onde der valt gheblumit <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s1:l4" met="4" type="+-">ond horet vogheliin <rhyme label="c" type="f">zinghen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">zo weys man by waren <rhyme label="c" type="f" corresp="#s1:l4">dinghen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">das der winter ende <rhyme label="b" type="m" corresp="#s1:l3">hat</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Zo moys ich mit zorghen <rhyme label="c" type="f" corresp="#s1:l4">ringhen</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">In have troest van <rhyme label="d" type="f">wibe</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">by dem ich tsijt <rhyme label="d" type="f" corresp="#s1:l8">verdrive</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">dy mir anders nicht <rhyme label="b" type="m" corresp="#s1:l3">vurvat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Weystu, minne, was ich <rhyme label="a" type="f">wolde</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">of ich ho wenschen <rhyme label="a" type="f" corresp="#s2:l1">wolde</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Zych, das ware eyn wiiflijch <rhyme label="b" type="m">wijf</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">zenfte van ziden, wijs van <rhyme label="c" type="f">zinnen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">strijst an eren, zuetz an <rhyme label="c" type="f" corresp="#s2:l4">mynnen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">scone, unde da by kÜnscher <rhyme label="b" type="m" corresp="#s2:l3">lyph</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Zol ich yummer ich <rhyme label="c" type="f" corresp="#s2:l4">ghewinnen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">by der werelde <rhyme label="d" type="f">doghenlijche</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">so ne stÜnt nye so wol die <rhyme label="d" type="f" corresp="#s2:l8">rijche</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">nie zaghe in mime zanghe <rhyme label="b" type="m" corresp="#s2:l3">blyph</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ich wil nemmer vyl <rhyme label="a" type="f">ghedriven</rhyme></l>
        <l id="s3:l2" met="4" type="+-">onghemach no zwachen <rhyme label="a" type="f" corresp="#s3:l1">wyven</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">mynne, das gheloÜe ich <rhyme label="b" type="m">dyr</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Du wilt, hoef ich, mit <rhyme label="c" type="f">dinghen</rhyme></l>
        <l id="s3:l5" met="4" type="+-">mich vyl werdelicher <rhyme label="c" type="f" corresp="#s3:l4">tswinghen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">die nach willen loenet <rhyme label="b" type="m" corresp="#s3:l3">myr</rhyme>.</l>
        <l id="s3:l7" met="4" type="+-">Du moyst aver mich da <rhyme label="c" type="f" corresp="#s3:l4">bringhen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">da miin dienst zuetz <rhyme label="d" type="f">erhelle</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Bystu danne can goet <rhyme label="d" type="f" corresp="#s3:l8">ghezelle</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">das yrvind ich wal an <rhyme label="b" type="m" corresp="#s3:l3">dyr</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
