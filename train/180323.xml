    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De Leidse klots</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HOe soet <rhyme label="a" type="m">geluyt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Van snaer en <rhyme label="a" type="m" corresp="#s1:l1">fluyt</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Tot Lof van Damons schoone <rhyme label="a" type="m" corresp="#s1:l1">Bruyt</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">Wiens byzyn niet dan vreugde <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Soo dat een ziel gelukkig <rhyme label="b" type="m" corresp="#s1:l4">leeft</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die van haer gunste <rhyme label="b" type="m" corresp="#s1:l4">heeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Het dartel <rhyme label="a" type="m">Wigt</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Lost schigt op <rhyme label="a" type="m" corresp="#s2:l1">schigt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Uyt haren mond ende <rhyme label="a" type="m" corresp="#s2:l1">Gesigt</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Ik kan in dit verliefde <rhyme label="b" type="m">Uur</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">De smerte van haer <rhyme label="b" type="m" corresp="#s2:l4">minnevuur</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Niet uytstaen door <rhyme label="b" type="m" corresp="#s2:l4">Natuur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">ô Aerds <rhyme label="a" type="m">Godin</rhyme>!</l>
        <l id="s3:l2" met="2" type="-+">U soete <rhyme label="a" type="m" corresp="#s3:l1">min</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Bekoort natuur zyn ziel en <rhyme label="a" type="m" corresp="#s3:l1">zin</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">U Lente is met uw held're <rhyme label="b" type="m">glans</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En 't Reukwerk van u <rhyme label="b" type="m" corresp="#s3:l4">Lauwer-Krans</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Verquikt mijn harte <rhyme label="b" type="m" corresp="#s3:l4">thans</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">ô Minne <rhyme label="a" type="m">vreugt</rhyme>!</l>
        <l id="s4:l2" met="2" type="-+">Die ons <rhyme label="a" type="m" corresp="#s4:l1">verheugt</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Uw eerd en dient de viere <rhyme label="a" type="m" corresp="#s4:l1">Ieugt</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Als gy u Harte open <rhyme label="b" type="m">sluyt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dan treed de Ieugt met lofzang <rhyme label="b" type="m" corresp="#s4:l4">uyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Tot lof van Damons <rhyme label="b" type="m" corresp="#s4:l4">Bruyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
