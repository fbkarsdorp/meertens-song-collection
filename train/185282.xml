<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185282</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De wereld is in rep en roer</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ik wensch u veels geluk <rhyme label="a" type="m">vrindin</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gy bent het die ik teer <rhyme label="a" type="m" corresp="#s1:l1">bemin</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Om u wil ik alles <rhyme label="b" type="f">lyden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Een lieve lonk van u <rhyme label="c" type="m">gezigt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Waar door gy my aan u <rhyme label="c" type="m" corresp="#s1:l4">verpligt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En ik my kan <rhyme label="b" type="f" corresp="#s1:l3">verblyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">U teedere mond die roept my <rhyme label="a" type="m">toe</rhyme>:</l>
        <l id="s2:l2" met="4" type="-+">Ach maatje lief maak u niet <rhyme label="a" type="m" corresp="#s2:l1">moe</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Wild dog u drift maar <rhyme label="b" type="f">staaken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Wy zyn hier veilig en ook <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">De tyd die legt alleen aan <rhyme label="c" type="m" corresp="#s2:l4">my</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat gy u kunt <rhyme label="b" type="f" corresp="#s2:l3">vermaaken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wanneer u boezem is <rhyme label="a" type="m">ontbloot</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En ik myn hand roer in u <rhyme label="a" type="m" corresp="#s3:l1">schoot</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">Dan kom ik tot <rhyme label="b" type="f">bedaaren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">U lieve hand dekt u <rhyme label="c" type="m">gezigt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Wanneer ik dit aan u <rhyme label="c" type="m" corresp="#s3:l4">verrigt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Myn aan u kom <rhyme label="b" type="f" corresp="#s3:l3">verklaaren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoord nu myn waarde en tedre <rhyme label="a" type="m">Bruid</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ik stort voor u myn klagte <rhyme label="a" type="m" corresp="#s4:l1">uit</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Het is om u hart te <rhyme label="b" type="f">winnen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Gy zyt alleen myn tweede <rhyme label="c" type="m">ziel</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Waarom ik voor u <rhyme label="c" type="m" corresp="#s4:l4">nederkniel</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Om eeuwig u te <rhyme label="b" type="f" corresp="#s4:l3">minnen</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
