  <author>Jan Steven van Esveldt Holtrop, Willem Holtrop</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Chantons la <rhyme label="a" type="f">Maçonnerie</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Qui seule noûs rend <rhyme label="b" type="m">heureux</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">D'une chaine si <rhyme label="a" type="f" corresp="#s1:l1">chérie</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Resserrons toujours les <rhyme label="b" type="m" corresp="#s1:l2">noeuds</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Aimons-nous avec <rhyme label="a" type="f">tendresse</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Et vivons en vrais <rhyme label="b" type="m">Maçons</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ne nous occupons sans <rhyme label="a" type="f" corresp="#s2:l1">cesse</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Que de leurs doctes <rhyme label="b" type="m" corresp="#s2:l2">leçons</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ordre illustre et <rhyme label="a" type="f">respectable</rhyme>!</l>
        <l id="s3:l2" met="4" type="+-">Tu fais goûter en <rhyme label="b" type="m">secret</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Le plaisir vif et <rhyme label="a" type="f" corresp="#s3:l1">durable</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Au Maçon sage et <rhyme label="b" type="m" corresp="#s3:l2">discret</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">En suivant le noble <rhyme label="a" type="f">exemple</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Des vertus et de <rhyme label="b" type="m">l'honneur</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Qu'on nous donne dans ton <rhyme label="a" type="f" corresp="#s4:l1">temple</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">L'on acquiert le vrai <rhyme label="b" type="m" corresp="#s4:l2">bonheur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
