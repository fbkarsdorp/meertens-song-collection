  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Jupyn, wy zyn <rhyme label="a" type="m">bereid</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Op 't allerminste <rhyme label="b" type="f">wénken</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van u-we <rhyme label="a" type="m" corresp="#s1:l1">Majesteit</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Wat vreugd men kan <rhyme label="b" type="f" corresp="#s1:l2">bedénken</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Aan 't Godendom te <rhyme label="b" type="f" corresp="#s1:l2">schénken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Tót blyk van <rhyme label="a" type="m" corresp="#s1:l1">dienstbaarheid</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Laat dan uit uw hooge <rhyme label="c" type="f">zaalen</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Uw gebód maar <rhyme label="c" type="f" corresp="#s1:l7">néderdaalen</rhyme>;</l>
        <l id="s1:l9" met="6" type="-+">Uw gebód, uw gebód elk <rhyme label="d" type="m">aangenaam</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Maakt onze geest <rhyme label="d" type="m" corresp="#s1:l9">bekwaam</rhyme>.</l>
        <l id="s1:l11" met="4" type="-+">Uw gebód elk <rhyme label="d" type="m" corresp="#s1:l9">aangenaam</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Maakt onze geest <rhyme label="d" type="m" corresp="#s1:l9">bekwaam</rhyme>.</l>
      </lg>
    </lg>
