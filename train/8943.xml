<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8943</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Vom Himmel hoch da komm ich her</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VAn HEmel hooch com ick <rhyme label="a" type="m">voorwaer</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ick brengh u goede Nieuwe <rhyme label="a" type="m" corresp="#s1:l1">Maer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Veel Nieuwe Maer brengh ick tot <rhyme label="b" type="m">dy</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Daer van ick segh end singhe <rhyme label="b" type="m" corresp="#s1:l3">bly</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">V is een Kindlijn huyd' <rhyme label="a" type="m">gheboorn</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Van eener Maghet <rhyme label="a" type="m" corresp="#s2:l1">Wtvercoorn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Een Kindelijn soo fraey end <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat sal u Vreucht end blyschap <rhyme label="b" type="m" corresp="#s2:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het is de HEEr Christ onse <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die wil u helpen wt den <rhyme label="a" type="m" corresp="#s3:l1">Noot</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hy wil u Heylandt syn <rhyme label="b" type="m">alleyn</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Van allen Sonden maecken <rhyme label="b" type="m" corresp="#s3:l3">Reyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy brengt u alle <rhyme label="a" type="m">Salicheyt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die Godt de Vader heeft <rhyme label="a" type="m" corresp="#s4:l1">bereydt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat ghy met ons int <rhyme label="b" type="m">HEmelrijck</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Sult Leven nu end <rhyme label="b" type="m" corresp="#s4:l3">Eewichlijck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">So mercket nu dat Teecken <rhyme label="a" type="m">recht</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">In een Kribb' end Windoecken <rhyme label="a" type="m" corresp="#s5:l1">slecht</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Daer in vint ghy dat Kindt <rhyme label="b" type="m">gheleydt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dat alle Werelt heeft <rhyme label="b" type="m" corresp="#s5:l3">bereydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dies laet ons alle Vrolick <rhyme label="a" type="m">syn</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">End met den Herders gaen her <rhyme label="a" type="m" corresp="#s6:l1">ein</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Te sien, wat Godt ons heeft <rhyme label="b" type="m">bescheert</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Met synen lieven Soon <rhyme label="b" type="m" corresp="#s6:l3">vereert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Merck op mijn Hert, end siet daer <rhyme label="a" type="m">in</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Wat leydt daer in die <rhyme label="a" type="m" corresp="#s7:l1">Kribbelijn</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Wiens is dat schoone <rhyme label="b" type="m">Kindelijn</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Het is dat lieve <rhyme label="b" type="m" corresp="#s7:l3">JEsulijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Weest ons Wilkom' ghy Eedle <rhyme label="a" type="m">gast</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Die de Sondaers niet laet in <rhyme label="a" type="m" corresp="#s8:l1">last</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">End coemt int Elend' her tot <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Hoe sal ick dies voldancken <rhyme label="b" type="m" corresp="#s8:l3">dy</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ach Heer ghy Schepper aller <rhyme label="a" type="m">Ding</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Hoe zijt ghy worden so <rhyme label="a" type="m" corresp="#s9:l1">ghering</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat ghy daer ligt op Dorre <rhyme label="b" type="m">Gras</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Dat voor een Os end Ezel <rhyme label="b" type="m" corresp="#s9:l3">was</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Al waer de Werelt noch so <rhyme label="a" type="m">wijt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Van Edelsteen end Gout <rhyme label="a" type="m" corresp="#s10:l1">bereydt</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">So waerse nochtans veel te <rhyme label="b" type="m">cleyn</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Te wesen dy een <rhyme label="b" type="m" corresp="#s10:l3">Wieghelijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Die Flueel end die Zijden <rhyme label="a" type="m">dijn</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Dat is groff Hoy end <rhyme label="a" type="m" corresp="#s11:l1">Windelijn</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Daer op ghy Coninck groot en <rhyme label="b" type="m">Rijck</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Braggeert, als waert dijn <rhyme label="b" type="m" corresp="#s11:l3">Hemelrijck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Dat heeft also behaghet <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Die Waerheyt te bethoonen <rhyme label="a" type="m" corresp="#s12:l1">my</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Hoe aller Werelt Macht en <rhyme label="b" type="m">Goet</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Voor dy niet gelt noch Hulpe <rhyme label="b" type="m" corresp="#s12:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ach mijn Hertlieve <rhyme label="a" type="m">JEsulijn</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Maeck dy een Reyn sacht <rhyme label="a" type="m" corresp="#s13:l1">Beddelijn</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Te Rusten in mijns Herten <rhyme label="b" type="m">schrijn</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Dat ick niet en vergeete <rhyme label="b" type="m" corresp="#s13:l3">dijn</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Daer van ick altijdt vrolick <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Te singhen, springhen altijt <rhyme label="a" type="m" corresp="#s14:l1">vry</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Dat rechte Susaninne <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Met Herten Lust den soeten <rhyme label="b" type="m" corresp="#s14:l3">Thoon</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Loff, Eer sy Godt in Hoochsten <rhyme label="a" type="m">Troon</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Die ons gaet schencken synen <rhyme label="a" type="m" corresp="#s15:l1">Soon</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Des syn verblijdt der Eng'len <rhyme label="b" type="m">schaer</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">End singent ons ten <rhyme label="b" type="m" corresp="#s15:l3">Nieuwen-Jaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
