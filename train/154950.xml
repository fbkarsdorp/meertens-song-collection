    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EEn schoonen frissen <rhyme label="a" type="m">Jongeling</rhyme></l>
        <l id="s1:l2" met="3" type="-+">hebt gy de ziel <rhyme label="b" type="m">gewont</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Terwyl hy met u wand'len <rhyme label="a" type="m" corresp="#s1:l1">ging</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat sterre bosje <rhyme label="b" type="m" corresp="#s1:l2">ront</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Gy mind hem en gy agt hem <rhyme label="c" type="m">hoog</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Zyn aankomst maakt u <rhyme label="d" type="m">bly</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Of al de vonkjes van uw <rhyme label="c" type="m" corresp="#s1:l5">oog</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Bedriegen meer als <rhyme label="d" type="m" corresp="#s1:l6">wy</rhyme>.</l>
      </lg>
    </lg>
