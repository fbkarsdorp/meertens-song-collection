<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>37022</id>
    <date>
      <year-start>1607</year-start>
      <year-end>1607</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Het haantje van de wijnen</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Michiel Vlack</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gheluck, salicheyt, <rhyme label="a" type="f">vrede</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wenschen wy in u <rhyme label="b" type="m">staat</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Bruydegom, Vrou' Bruyt <rhyme label="a" type="f" corresp="#s1:l1">mede</rhyme></l>
        <l id="s1:l4" met="3" type="-+">De Heer jont u <rhyme label="b" type="m" corresp="#s1:l2">ghenaad'</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Op dat t' mach zijnder <rhyme label="c" type="f">eere</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ghedyen t' gheen ghy <rhyme label="d" type="m">doet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Soo volght Tobyas <rhyme label="c" type="f" corresp="#s1:l5">leere</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gods zeghen u <rhyme label="d" type="m" corresp="#s1:l6">ontmoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want die den Heere <rhyme label="a" type="f">vreesen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sal hy verlaten <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ghezeghent sal 't werck <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat door zijn handt <rhyme label="b" type="m" corresp="#s2:l2">gheschiet</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Sijn ooghen sullen <rhyme label="c" type="f">schouwen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Jerusalems <rhyme label="d" type="m">gheluck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En Jsr'els vreed' vol <rhyme label="c" type="f" corresp="#s2:l5">trouwen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ontlast van Satans <rhyme label="d" type="m" corresp="#s2:l6">juck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De heyl'ghe Echt <rhyme label="a" type="f">hooghwaardigh</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Heeft God self <rhyme label="b" type="m">ghelaudeert</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Daar hy het water <rhyme label="a" type="f" corresp="#s3:l1">vaardigh</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Jn goed' Wijn heeft <rhyme label="b" type="m" corresp="#s3:l2">verkeert</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Sulcx doet hy noch ten <rhyme label="c" type="f">tyen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Jn 't midden hy hem <rhyme label="d" type="m">voeght</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Daar twee haar t'zamen <rhyme label="c" type="f" corresp="#s3:l5">vlyen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Jn Trouw' en Liefd' <rhyme label="d" type="m" corresp="#s3:l6">vernoeght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dees die ghy hebt <rhyme label="a" type="f">vercreghen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Door Gods schickingh' ten <rhyme label="b" type="m">Echt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Betreed' met haar Gods <rhyme label="a" type="f" corresp="#s4:l1">weghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Jn handelingh' <rhyme label="b" type="m" corresp="#s4:l2">op-recht</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Vreedsaem leeft met u <rhyme label="c" type="f">beyden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Alsoo ghy hebt <rhyme label="d" type="m">belooft</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Tot u de doot comt <rhyme label="c" type="f" corresp="#s4:l5">scheyden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En s' levens cracht <rhyme label="d" type="m" corresp="#s4:l6">ontrooft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">V Huys-vrouw' wilt <rhyme label="a" type="f">beminnen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Als u eyghen <rhyme label="b" type="m">Lichaam</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Met hert, ghemoet, en <rhyme label="a" type="f" corresp="#s5:l1">sinnen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Haar aandoen smaet noch <rhyme label="b" type="m" corresp="#s5:l2">blaam</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Want ghy zijt nu twee <rhyme label="c" type="f">herten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Jn een begrip <rhyme label="d" type="m">vereent</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Laat d' eens leet d' ander <rhyme label="c" type="f" corresp="#s5:l5">smerten</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Als vleysch van een <rhyme label="d" type="m" corresp="#s5:l6">ghebeent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy sult Vader en <rhyme label="a" type="f">Moeder</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Verlaten en af <rhyme label="b" type="m">gaan</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Als Assuerus <rhyme label="a" type="f" corresp="#s6:l1">behoeder</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van u Vrouw' zijn <rhyme label="b" type="m" corresp="#s6:l2">voortaan</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Haar swackheyt helpen <rhyme label="c" type="f">doghen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En troosten haar in <rhyme label="d" type="m">rouw'</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Na u s' herten <rhyme label="c" type="f" corresp="#s6:l5">vermoghen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En uytwysingh' van <rhyme label="d" type="m" corresp="#s6:l6">trouw'</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hier teghen suldy <rhyme label="a" type="f">goedigh</rhyme></l>
        <l id="s7:l2" met="3" type="-+">V Man ghehoorsaam <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Als Sara deed' <rhyme label="a" type="f" corresp="#s7:l1">ootmoedigh</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Abram t' allen <rhyme label="b" type="m" corresp="#s7:l2">termijn</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Een Heer gingh sy hem <rhyme label="c" type="f">nomen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Diende hem naar haar <rhyme label="d" type="m">macht</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Sulcx is de daad der <rhyme label="c" type="f" corresp="#s7:l5">vromen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Vrouw'-Bruyt neemt hier op <rhyme label="d" type="m" corresp="#s7:l6">acht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De Huys-zorgh' wilt <rhyme label="a" type="f">aanveerden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die u wert <rhyme label="b" type="m">opgheleyt</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Denckt s'Meesters oogh de <rhyme label="a" type="f" corresp="#s8:l1">Peerden</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Vet maken ( soo men <rhyme label="b" type="m" corresp="#s8:l2">seyt</rhyme>:)</l>
        <l id="s8:l5" met="3" type="-+">Daarom wilt met <rhyme label="c" type="f">op-mercken</rhyme></l>
        <l id="s8:l6" met="3" type="-+">T'gheen ghy aanvaart <rhyme label="d" type="m">bevroen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Sonder nochtans te <rhyme label="c" type="f" corresp="#s8:l5">wercken</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Suldy t' Huys veel nuts <rhyme label="d" type="m" corresp="#s8:l6">doen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De opper Prins <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Die al dat leeft <rhyme label="b" type="m">bestiert</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wil u zijn zeghen <rhyme label="a" type="f" corresp="#s9:l1">gheven</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat ghy 't wijslijck <rhyme label="b" type="m" corresp="#s9:l2">regiert</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Laet liefd in u <rhyme label="c" type="f">beclyven</rhyme></l>
        <l id="s9:l6" met="3" type="-+">En wilt God eere <rhyme label="d" type="m">bien</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Als planten van <rhyme label="c" type="f" corresp="#s9:l5">Olyven</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Suldy u Kindren <rhyme label="d" type="m" corresp="#s9:l6">sien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
