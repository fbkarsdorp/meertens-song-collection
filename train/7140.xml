  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Tvsschen twee hooge <rhyme label="a" type="f">bergen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En eenen groenen <rhyme label="b" type="m">gaert</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Daer bleef myn lief in <rhyme label="c" type="f">strijden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van hooger minnen <rhyme label="b" type="m" corresp="#s1:l2">aert</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daer bleef myn lief in <rhyme label="c" type="f" corresp="#s1:l3">strijden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van hooger minnen <rhyme label="b" type="m" corresp="#s1:l2">aert</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Och lief van hooger <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hoe sijdij aldus <rhyme label="b" type="m">mismaect</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Al om een arm <rhyme label="c" type="f">ziele</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Die luttel nae v <rhyme label="b" type="m" corresp="#s2:l2">haeckt</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Al om een arm <rhyme label="c" type="f" corresp="#s2:l3">ziele</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Die luttel nae v <rhyme label="b" type="m" corresp="#s2:l2">haeckt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O heere voor v <rhyme label="a" type="f">ghenaden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En door v minne <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hoe geerne soude ick <rhyme label="c" type="f">spreken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tot v een minnelijck <rhyme label="b" type="m" corresp="#s3:l2">woort</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Hoe geerne soude ick <rhyme label="c" type="f" corresp="#s3:l3">spreken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Tot v een minnelijck <rhyme label="b" type="m" corresp="#s3:l2">woort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Och edel <rhyme label="a" type="f">creatuere</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Vrij edel ziele <rhyme label="b" type="m">mijn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En wat is v <rhyme label="c" type="f">begheren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat ghij versueckt aen <rhyme label="b" type="m" corresp="#s4:l2">mij</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">En wat is v <rhyme label="c" type="f" corresp="#s4:l3">begheren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat ghij versueckt aen <rhyme label="b" type="m" corresp="#s4:l2">mij</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Och lief van hooger <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van hooger minnen <rhyme label="b" type="m">aert</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Hoe geerne soudic mij <rhyme label="c" type="f">vermeyden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In uwer minnen <rhyme label="b" type="m" corresp="#s5:l2">gaert</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Hoe geerne soudic mij <rhyme label="c" type="f" corresp="#s5:l3">vermeyden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In uwer minnen <rhyme label="b" type="m" corresp="#s5:l2">gaert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Och wildy v <rhyme label="a" type="f">vermeyden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In mijnder minnen <rhyme label="b" type="m">gaert</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Soo moet ghijs veel <rhyme label="c" type="f">begheuen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Daer uwen sin toe <rhyme label="b" type="m" corresp="#s6:l2">staet</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Soo moet ghijs veel <rhyme label="c" type="f" corresp="#s6:l3">begheuen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Daer uwen sin toe <rhyme label="b" type="m" corresp="#s6:l2">staet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hoe sal ick dat <rhyme label="a" type="f">ghemaeken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Mijn lief mijn <rhyme label="b" type="m">minnekijn</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Soo veel te <rhyme label="c" type="f">versaeken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat doet mij groote <rhyme label="b" type="m" corresp="#s7:l2">pijn</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Soo veel te <rhyme label="c" type="f" corresp="#s7:l3">versaeken</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dat doet mij groote <rhyme label="b" type="m" corresp="#s7:l2">pijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O edel <rhyme label="a" type="f">creatuere</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Vrij edel ziele <rhyme label="b" type="m">mijn</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Slaet doch eens op v <rhyme label="c" type="f">ooghen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Aensiet mijn bitter <rhyme label="b" type="m" corresp="#s8:l2">pijn</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Slaet doch eens op v <rhyme label="c" type="f" corresp="#s8:l3">ooghen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Aensiet mijn bitter <rhyme label="b" type="m" corresp="#s8:l2">pijn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Aent cruyce wilt <rhyme label="a" type="f">versinnen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">In eender perssen <rhyme label="b" type="m">swaer</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Stont ick om uwer <rhyme label="c" type="f">minnen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Om v tontfaen <rhyme label="b" type="m" corresp="#s9:l2">aldaer</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Stont ick om uwer <rhyme label="c" type="f" corresp="#s9:l3">minnen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Om v tontfaen <rhyme label="b" type="m" corresp="#s9:l2">aldaer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">O Iesus lieue <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ick bid v staghet mij <rhyme label="b" type="m">bij</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Ick wil die werelt <rhyme label="c" type="f">laeten</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Al om die minne van <rhyme label="b" type="m" corresp="#s10:l2">dij</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Ick wil die werelt <rhyme label="c" type="f" corresp="#s10:l3">laeten</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Al om die minne van <rhyme label="b" type="m" corresp="#s10:l2">dij</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ick wilt nv al <rhyme label="a" type="f">begheuen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En soecken daer diepe <rhyme label="b" type="m">ootmoet</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Dat ick hier naemael mach <rhyme label="c" type="f">leuen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">En vercrijghen dat eewich <rhyme label="b" type="m" corresp="#s11:l2">goet</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Dat ick hier naemael mach <rhyme label="c" type="f" corresp="#s11:l3">leuen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">En vercrijghen dat eewich <rhyme label="b" type="m" corresp="#s11:l2">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
