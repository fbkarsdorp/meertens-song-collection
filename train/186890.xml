<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186890</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Lison dormait dans un bocage</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ENgland, dol en <rhyme label="a" type="f">uigelaaten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Vloekt en dreigt ons <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Schend de Vlag der Heeren <rhyme label="a" type="f" corresp="#s1:l1">Staaten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Pleegt gevloekte moord en <rhyme label="b" type="m" corresp="#s1:l2">brand</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Enkel om dat de <rhyme label="c" type="f">Overheden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Tot de vaste <rhyme label="d" type="m">onzydigheid</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Naer <rhyme label="d" type="m" corresp="#s1:l6">onderscheid</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Van wys <rhyme label="d" type="m" corresp="#s1:l6">beleid</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Met de Russen zyn <rhyme label="c" type="f" corresp="#s1:l5">getreeden</rhyme>;</l>
        <l id="s1:l10" met="2" type="-+">Dat zyne <rhyme label="e" type="m">buit</rhyme> --,</l>
        <l id="s1:l11" met="2" type="+-">Vergiste <rhyme label="e" type="m" corresp="#s1:l10">muit</rhyme> --,</l>
        <l id="s1:l12" met="4" type="+-">En Barbaersche heerschzugt <rhyme label="e" type="m" corresp="#s1:l10">stuit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schoon nu onze Braave <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Hem naer hunnen vroomen <rhyme label="b" type="m">aerd</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Raeden tot zyn' pligt te <rhyme label="a" type="f" corresp="#s2:l1">keeren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Neemt zyn trotsheid hooger <rhyme label="b" type="m" corresp="#s2:l2">vaert</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Durft den Oorlog ons <rhyme label="c" type="f">verklaaren</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Neemt twee hondert Schepen <rhyme label="d" type="m">mÃªe</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Die op zyn <rhyme label="d" type="m" corresp="#s2:l6">Ree</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Of in de <rhyme label="d" type="m" corresp="#s2:l6">Zee</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Onbewust van alles <rhyme label="c" type="f" corresp="#s2:l5">waren</rhyme>:</l>
        <l id="s2:l10" met="2" type="-+">Hy slaat geen <rhyme label="e" type="m">acht</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Op Recht, nog <rhyme label="e" type="m" corresp="#s2:l10">Kracht</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Onzer vreedzaame <rhyme label="e" type="m" corresp="#s2:l10">Oppermagt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Is de tyd, <rhyme label="a" type="f">Hoogmogendheden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dat gy't Zwaard op zyde <rhyme label="b" type="m">gordt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Daar hy alles wil <rhyme label="a" type="f" corresp="#s3:l1">vertreeden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En uw hoog gezag <rhyme label="b" type="m" corresp="#s3:l2">verkort</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Wy u willige <rhyme label="c" type="f">Onderzaeten</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Walgen van zijn <rhyme label="d" type="m">euvelmoed</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ontzien ons <rhyme label="d" type="m" corresp="#s3:l6">goed</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Noch dierbaer <rhyme label="d" type="m" corresp="#s3:l6">bloed</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Geen minnut voor U te <rhyme label="c" type="f" corresp="#s3:l5">laeten</rhyme>;</l>
        <l id="s3:l10" met="2" type="-+">Maer gaen ten <rhyme label="e" type="m">stryd'</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Tot dat de <rhyme label="e" type="m" corresp="#s3:l10">tyd</rhyme></l>
        <l id="s3:l12" met="4" type="+-">Ons door lieve Vree <rhyme label="e" type="m" corresp="#s3:l10">verblyd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Roovers, hebt gy U <rhyme label="a" type="f">verschoolen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Wreest gy onzen <rhyme label="b" type="m">Heldendrom</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Geeft ren eersten al 't <rhyme label="a" type="f" corresp="#s4:l1">gestolen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Eerlyk Hollands good <rhyme label="b" type="m" corresp="#s4:l2">weerom</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Boet nu tot een duit de <rhyme label="c" type="f">schade</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die gy ons hebt <rhyme label="d" type="m">aangedaan</rhyme>;</l>
        <l id="s4:l7" met="2" type="-+">Brengt alles <rhyme label="d" type="m" corresp="#s4:l6">aan</rhyme>:</l>
        <l id="s4:l8" met="2" type="-+">Geen <rhyme label="d" type="m" corresp="#s4:l6">Vredevaan</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Komt in dezen u te <rhyme label="c" type="f" corresp="#s4:l5">stade</rhyme>.</l>
        <l id="s4:l10" met="2" type="-+">Neen <rhyme label="e" type="m">boevenrot</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Verwacht u <rhyme label="e" type="m" corresp="#s4:l10">lot</rhyme></l>
        <l id="s4:l12" met="4" type="+-">Van 't gedoemd', doch braaf, <rhyme label="e" type="m" corresp="#s4:l10">complot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Schenders van myn <rhyme label="a" type="f">Burgervaders</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dieven van elk <rhyme label="b" type="m">Onderdaen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Vloekverwanten <rhyme label="a" type="f" corresp="#s5:l1">landverraders</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Nooit zal uwe zaek <rhyme label="b" type="m" corresp="#s5:l2">bestaen</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Gaat uw schelmen dit <rhyme label="c" type="f">ontdekken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En verhaelt vooral dat <rhyme label="d" type="m">gy</rhyme></l>
        <l id="s5:l7" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s5:l6">Burgery</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Van 't Kielryke <rhyme label="d" type="m" corresp="#s5:l6">Y</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Tot geen oproer kunt <rhyme label="c" type="f" corresp="#s5:l5">verwekken</rhyme>:</l>
        <l id="s5:l10" met="2" type="-+">Maar dat zy <rhyme label="e" type="m">zeit</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">Tot elks <rhyme label="e" type="m" corresp="#s5:l10">bescheid</rhyme>:</l>
        <l id="s5:l12" met="4" type="+-">Gy hebt u Vorst <rhyme label="e" type="m" corresp="#s5:l10">misleid</rhyme>;</l>
      </lg>
    </lg>
  </text>
</song>
