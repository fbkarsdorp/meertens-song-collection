      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HAar Lippen zijn <rhyme label="a" type="m">bekwaam</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Om Huspot van te <rhyme label="b" type="f">kappen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Haar Lippen zijn <rhyme label="a" type="m" corresp="#s1:l1">bekwaam</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En staan heel <rhyme label="a" type="m" corresp="#s1:l1">aangenaam</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Een Moor moet daar voor <rhyme label="c" type="f">wijken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Een Wezoper <rhyme label="d" type="m">Student</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Is by haar niet te <rhyme label="c" type="f" corresp="#s1:l5">lijken</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">'k Wou, 'k waar by haar <rhyme label="d" type="m" corresp="#s1:l6">bekend</rhyme>.</l>
      </lg>
