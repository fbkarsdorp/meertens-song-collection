  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ORbis patrator <rhyme label="a" type="m">optime</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Qui quicquid est, <rhyme label="b" type="m">potentia</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Magna creasti, nec <rhyme label="c" type="m">regis</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Minore <rhyme label="d" type="m">providentia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Adesto <rhyme label="a" type="m">supplicantium</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Tibi reorum <rhyme label="b" type="m">coetui</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Lucisque sub <rhyme label="c" type="m">crepusculum</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Lucem novam da <rhyme label="d" type="m">mentibus</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Tuusque nobis <rhyme label="a" type="m">Angelus</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Signatus ad <rhyme label="b" type="m">custodiam</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hic adsit, Ã  <rhyme label="c" type="m">contagio</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Qui criminum nos <rhyme label="d" type="m">protegat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Serpentis nobis <rhyme label="a" type="m">aemuli</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Calumnias <rhyme label="b" type="m">exterminet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ne rete <rhyme label="c" type="m">fraudulentiae</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Incauta nectat <rhyme label="d" type="m">pectora</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Metum propellat <rhyme label="a" type="m">hostium</rhyme>.</l>
        <l id="s5:l2" met="4" type="-+">Nostris procul de <rhyme label="b" type="m">finibus</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Pacem procuret <rhyme label="c" type="m">civium</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Fugetque <rhyme label="d" type="m">pestilentiam</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Deo Patri sit <rhyme label="a" type="m">gloria</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Qui, quos redemit <rhyme label="b" type="m">Filius</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Et Sanctus unxit <rhyme label="c" type="m">Spiritus</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Per Angelos <rhyme label="d" type="m">custodiat</rhyme>.</l>
      </lg>
    </lg>
