      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHy Hoogh-verwaende <rhyme label="a" type="f">Princen</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Ey! hoort een Nimph een Schaeps-hoed'resse <rhyme label="b" type="f">spreken</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die na haer wil, en <rhyme label="a" type="f" corresp="#s1:l1">wenssen</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Het aen geen stof van reden sal <rhyme label="b" type="f" corresp="#s1:l2">ontbreken</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Ghy die <rhyme label="c" type="m">verweent</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Door dwaesheyt <rhyme label="c" type="m" corresp="#s1:l5">meent</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat eerbaer Maeghden <rhyme label="d" type="f">schennis</rhyme></l>
        <l id="s1:l8" met="2" type="-+">U glans doet <rhyme label="e" type="f">hoogen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Maer neen. ghy zijt <rhyme label="e" type="f" corresp="#s1:l8">bedroogen</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Buyten <rhyme label="d" type="f" corresp="#s1:l7">kennis</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Daer gaet nauw' by der <rhyme label="a" type="f">Straten</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Geen eerbaer Maeght, hoe jonck, of <rhyme label="b" type="f">uyt-gelesen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Hoe hoogh of laegh van <rhyme label="a" type="f" corresp="#s2:l1">staten</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Ghy beelt u in sy tot u wil moet <rhyme label="b" type="f" corresp="#s2:l2">wesen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ghy leght en <rhyme label="c" type="m">lelt</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Soo lang door <rhyme label="c" type="m" corresp="#s2:l5">Gelt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Door snoode <rhyme label="d" type="f">Copp'larijen</rhyme>:</l>
        <l id="s2:l8" met="2" type="-+">Die veel <rhyme label="e" type="f">versieren</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Tot dat d' onnosel <rhyme label="e" type="f" corresp="#s2:l8">dieren</rhyme></l>
        <l id="s2:l10" met="2" type="+-">Zijn in 't <rhyme label="d" type="f" corresp="#s2:l7">lijen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maer al quam Cresus <rhyme label="a" type="f">selven</rhyme></l>
        <l id="s3:l2" met="5" type="-+">En boogh my aen een Konings Croon <rhyme label="b" type="f">beperelt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En socht in 't Graf te <rhyme label="a" type="f" corresp="#s3:l1">delven</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Mijn Maegden-glans, mijn schat op dese <rhyme label="b" type="f" corresp="#s3:l2">weerelt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Soo sal geen <rhyme label="c" type="m">Graf</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Noch <rhyme label="c" type="m" corresp="#s3:l5">Princen-slaef</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Mijn kuysheyt komen <rhyme label="d" type="f">rucken</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Als mijn <rhyme label="e" type="f">beminde</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Wiens Throon is d' hooge <rhyme label="e" type="f" corresp="#s3:l8">Linde</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Die sal 't <rhyme label="d" type="f" corresp="#s3:l7">plucken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dees nevel mist en <rhyme label="a" type="f">dampen</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Bedwelmt den Haegh en is een poel <rhyme label="b" type="f">geworden</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">En levert ramp, op <rhyme label="a" type="f" corresp="#s4:l1">rampen</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Sint sy den bant van Venus <rhyme label="b" type="f" corresp="#s4:l2">omme-gorden</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">De oorsaeck <rhyme label="c" type="m">zy</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Door <rhyme label="c" type="m" corresp="#s4:l5">Mominery</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Het Gelt en <rhyme label="d" type="f">Sy-gewaden</rhyme>:</l>
        <l id="s4:l8" met="2" type="-+">'t Luylecker <rhyme label="e" type="f">leven</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Waer voor een roof sy <rhyme label="e" type="f" corresp="#s4:l8">geven</rhyme></l>
        <l id="s4:l10" met="2" type="+-">d' <rhyme label="d" type="f" corresp="#s4:l7">Eer-cieraden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maer neen Godt wil <rhyme label="a" type="f">behoeden</rhyme></l>
        <l id="s5:l2" met="5" type="-+">Een suyv're Ziel, voor sulcke <rhyme label="b" type="f">onweers-winden</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">'k Wou liever 't lichaem <rhyme label="a" type="f" corresp="#s5:l1">voeden</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Met kruyt, en loof, en 't Linde kleet <rhyme label="b" type="f" corresp="#s5:l2">op-binden</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Met Hoy of <rhyme label="c" type="m">Gras</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">En uyt de <rhyme label="c" type="m" corresp="#s5:l5">plas</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Van 't loopent Beeckje <rhyme label="d" type="f">drincken</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Eer mijn <rhyme label="e" type="f">ghedachten</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Mijn Zieltje door haer <rhyme label="e" type="f" corresp="#s5:l8">krachten</rhyme></l>
        <l id="s5:l10" met="2" type="+-">Sou <rhyme label="d" type="f" corresp="#s5:l7">versincken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Want als voor Dagh en <rhyme label="a" type="f">kriecken</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Mijn Herder op sijn Riet begint te <rhyme label="b" type="f">spelen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dan zweeft het Bos vol <rhyme label="a" type="f" corresp="#s6:l1">wiecken</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Ja 't schatert al wat men sijn beck kan <rhyme label="b" type="f" corresp="#s6:l2">quelen</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">De <rhyme label="c" type="m">Nachtegael</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Bralt met sijn <rhyme label="c" type="m" corresp="#s6:l5">tael</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En klatert door 't <rhyme label="d" type="f">Bosschagie</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">O werck des <rhyme label="e" type="f">Heeren</rhyme>!</l>
        <l id="s6:l9" met="3" type="-+">Wie sou daer voor <rhyme label="e" type="f" corresp="#s6:l8">begeren</rhyme></l>
        <l id="s6:l10" met="2" type="+-">Snoo <rhyme label="d" type="f" corresp="#s6:l7">boelagie</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Soo dra den lieven <rhyme label="a" type="f">Morgen</rhyme></l>
        <l id="s7:l2" met="5" type="-+">Den dagh door-breeckt, bevind ick my naer <rhyme label="b" type="f">lusten</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Van anghst, van vrees en <rhyme label="a" type="f" corresp="#s7:l1">sorgen</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">In mijn beminde Harders schoot te <rhyme label="b" type="f" corresp="#s7:l2">rusten</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Die my dan <rhyme label="c" type="m">streelt</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">En met my <rhyme label="c" type="m" corresp="#s7:l5">speelt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Soo vriend'lijck en <rhyme label="d" type="f">lieftaligh</rhyme>;</l>
        <l id="s7:l8" met="2" type="-+">Fy! vuyle <rhyme label="e" type="f">Hoven</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Ons staet is (wilt <rhyme label="e" type="f" corresp="#s7:l8">gelooven</rhyme>/)</l>
        <l id="s7:l10" met="2" type="+-">Wel soo <rhyme label="d" type="f" corresp="#s7:l7">saligh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
