    <melody>Ach Vater unser der du bist</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O God die onsen Vader <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Door Christi <rhyme label="a" type="m" corresp="#s1:l1">bloet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Geworden zijt, geeft ons den <rhyme label="b" type="m">Geest</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die ons ter waerheyt <rhyme label="c" type="f">leyde</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Verhooret ons in dese <rhyme label="d" type="m">stont</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Ontsluyt den <rhyme label="d" type="m" corresp="#s1:l5">mont</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Vws dienaers, dat aen minst' en <rhyme label="b" type="m" corresp="#s1:l3">meest'</rhyme></l>
        <l id="s1:l8" met="3" type="-+">V leering' hy <rhyme label="c" type="f" corresp="#s1:l4">uytbreyde</rhyme>.</l>
        <l id="s1:l9" met="4" type="-+">Daer toe, o Heer, aen onse <rhyme label="e" type="m">syd'</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Ontsluytet hert en <rhyme label="f" type="f">ooren</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Dat wy int hooren toonen <rhyme label="e" type="m" corresp="#s1:l9">vlijt</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">En trouwe int <rhyme label="g" type="f">bewaren</rhyme>.</l>
        <l id="s1:l13" met="4" type="-+">Op dat wy, al ons' levens <rhyme label="e" type="m" corresp="#s1:l9">tijt</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">V grooten lof <rhyme label="g" type="f" corresp="#s1:l12">verclaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
