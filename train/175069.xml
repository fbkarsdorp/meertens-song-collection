  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+">In <rhyme label="a" type="m">Mei</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dan leggen alle <rhyme label="b" type="f">vogeltjes</rhyme></l>
        <l id="s1:l3" met="1" type="-+">Een <rhyme label="a" type="m" corresp="#s1:l1">ei</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">En waar ze zitten <rhyme label="c" type="f">broeijen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daar zullen we niet <rhyme label="c" type="f" corresp="#s1:l4">stoeijen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+">In <rhyme label="a" type="m">Mei</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dan kruipt een heel klein <rhyme label="b" type="f">vogeltje</rhyme></l>
        <l id="s2:l3" met="1" type="-+">Door 't <rhyme label="a" type="m" corresp="#s2:l1">ei</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Wie zou het willen <rhyme label="c" type="f">deren</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Het heeft geen eens nog <rhyme label="c" type="f" corresp="#s2:l4">veÃªren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+">In <rhyme label="a" type="m">Mei</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dan leggen alle <rhyme label="b" type="f">vogeltjes</rhyme></l>
        <l id="s3:l3" met="1" type="-+">Een <rhyme label="a" type="m" corresp="#s3:l1">ei</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">En wie die beestjes <rhyme label="c" type="f">hindren</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat zijn wel booze <rhyme label="c" type="f" corresp="#s3:l4">kindren</rhyme>.</l>
      </lg>
    </lg>
