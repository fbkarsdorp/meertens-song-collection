<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184057</id>
    <date>
      <year-start>1806</year-start>
      <year-end>1806</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Dieu d'amour en ce jour</melody>
  <author>Jan Steven van Esveldt Holtrop, Willem Holtrop</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-"><rhyme label="a" type="m">Charité</rhyme>!</l>
        <l id="s1:l2" met="2" type="+-">Ta <rhyme label="a" type="m" corresp="#s1:l1">beauté</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Nous enlève et nous <rhyme label="b" type="f">enchante</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Et nêtre hommage à jamais t'est <rhyme label="a" type="m" corresp="#s1:l1">voué</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Tu remplis notre <rhyme label="b" type="f" corresp="#s1:l3">attente</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En agréant nos <rhyme label="c" type="m">voeux</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Que ta lumière est <rhyme label="b" type="f" corresp="#s1:l3">brillante</rhyme>!</l>
        <l id="s1:l8" met="4" type="-+">Elle éclaire et charme nos <rhyme label="c" type="m" corresp="#s1:l6">yeux</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Notre <rhyme label="a" type="m">coeur</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Plein <rhyme label="a" type="m" corresp="#s2:l1">d'ardeur</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Sent ta divine <rhyme label="b" type="f">insluence</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Et de ton règne exalte la <rhyme label="a" type="m" corresp="#s2:l1">douceur</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">De tes loix <rhyme label="b" type="f" corresp="#s2:l3">l'excellence</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Honnore la <rhyme label="c" type="m">vertu</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Et l'orgueil en ta <rhyme label="b" type="f" corresp="#s2:l3">presence</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Rougit, tombe et se voit <rhyme label="c" type="m" corresp="#s2:l6">vaincu</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">A ta <rhyme label="a" type="m">voix</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Par leurs <rhyme label="a" type="m" corresp="#s3:l1">choix</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Les Maçons, sous tes <rhyme label="b" type="f">auspices</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Remplis de zèle, sont soumis à tes <rhyme label="a" type="m" corresp="#s3:l1">loix</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Ils goûtent les <rhyme label="b" type="f" corresp="#s3:l3">délices</rhyme></l>
        <l id="s3:l6" met="3" type="-+">D'un tendre <rhyme label="c" type="m">attachement</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Et par tes regards <rhyme label="b" type="f" corresp="#s3:l3">propices</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Leur coeur s'ouvre au <rhyme label="c" type="m" corresp="#s3:l6">contentement</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">
          <rhyme label="a" type="m">Exaltons</rhyme>
        </l>
        <l id="s4:l2" met="2" type="+-">Et <rhyme label="a" type="m" corresp="#s4:l1">chantons</rhyme></l>
        <l id="s4:l3" met="4" type="+-">De ce temple la <rhyme label="b" type="f">Déesse</rhyme>!</l>
        <l id="s4:l4" met="5" type="-+">Sois, temple auguste, un rempart aux <rhyme label="a" type="m" corresp="#s4:l1">Maçons</rhyme>!</l>
        <l id="s4:l5" met="3" type="-+">Qu'ils y trouvent sans <rhyme label="b" type="f" corresp="#s4:l3">cesse</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Amitié et <rhyme label="c" type="m">candeur</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Beauté, force, <rhyme label="b" type="f" corresp="#s4:l3">sagesse</rhyme>;</l>
        <l id="s4:l8" met="4" type="-+">Harmonie, gloire et <rhyme label="c" type="m" corresp="#s4:l6">grandeur</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
