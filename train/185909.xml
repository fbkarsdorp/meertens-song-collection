<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185909</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1790</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Lison dormait dans un bocage</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Kom laet ons saem als helden <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Denk om geen ramp, denk om geen <rhyme label="b" type="m">Pyn</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Droefheid moet ons hier niet doen <rhyme label="a" type="f" corresp="#s1:l1">beven</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">De Jeugt wil dat wy vrolyk <rhyme label="b" type="m" corresp="#s1:l2">zyn</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">De Grysheid kan ons hier niet <rhyme label="c" type="f">behagen</rhyme>:</l>
        <l id="s1:l6" met="4" type="-+">Viva de Min! Viva de <rhyme label="d" type="m">Wyn</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Weg, weg, o <rhyme label="e" type="m">druk</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Verschoon onz' edele Lente <rhyme label="c" type="f" corresp="#s1:l5">dagen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Weg, weg, o <rhyme label="e" type="m" corresp="#s1:l7">druk</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Beroof ons nimmer van 't <rhyme label="e" type="m" corresp="#s1:l7">geluk</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat kan onz' harten toch <rhyme label="a" type="f">bekoren</rhyme>?</l>
        <l id="s2:l2" met="4" type="-+">'t Is zoete Wyn,'t is zoete <rhyme label="b" type="m">Min</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wy willen na geen veinzers <rhyme label="a" type="f" corresp="#s2:l1">hooren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Geen huichlary kend onze <rhyme label="b" type="m" corresp="#s2:l2">zin</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">De grysheid kan ons hier niet <rhyme label="c" type="f">behagen</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">Viva de Min, viva de <rhyme label="d" type="m">Wyn</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Weg, weg, o <rhyme label="e" type="m">smart</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Verschoon onz' edele <rhyme label="c" type="f" corresp="#s2:l5">Lentedagen</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Weg, weg, o <rhyme label="e" type="m" corresp="#s2:l7">smart</rhyme>:</l>
        <l id="s2:l10" met="4" type="-+">Komt nimmer in ons vrolyk <rhyme label="e" type="m" corresp="#s2:l7">hart</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy weten dat by 't jeugdig <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Een glaesje Wyn, een kusjes <rhyme label="b" type="m">voegt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Zo lang als die ons niet <rhyme label="a" type="f" corresp="#s3:l1">begeven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Zyn wy te vreen, zyn wy <rhyme label="b" type="m" corresp="#s3:l2">vernoegd</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Den gryzaerd kan ons niet <rhyme label="c" type="f">behaegen</rhyme>:</l>
        <l id="s3:l6" met="4" type="-+">Viva de Min, viva de <rhyme label="d" type="m">wyn</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Weg, weg, o <rhyme label="e" type="m">Pyn</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Verschoon onz' edele <rhyme label="c" type="f" corresp="#s3:l5">Lentedagen</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Weg, weg, o <rhyme label="e" type="m" corresp="#s3:l7">Pyn</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Beroof ons nimmer 't vrolyk <rhyme label="e" type="m" corresp="#s3:l7">zyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
