<song>
  <meta>
    <id>20447</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Psalm 118 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>C. J. Wits</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Hooghste Goedt, vol glans en <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">O Schoonste Schoonheydt, Soetste <rhyme label="b" type="m">Soet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">O Goedtste Goedtheyt, Heer der <rhyme label="a" type="f" corresp="#s1:l1">Heeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ick nietigh Mensch val u te <rhyme label="b" type="m" corresp="#s1:l2">voet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt, Heer, mijn laeuwen Geest <rhyme label="a" type="f">ontsteecken</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Op dat die steeds van Liefde <rhyme label="b" type="m">brandt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Verteert, vernietight mijn <rhyme label="a" type="f" corresp="#s2:l1">ghebreecken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Regeert, bestiert, mijn blindt <rhyme label="b" type="m" corresp="#s2:l2">verstandt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Op dat de Liefd' haer mach <rhyme label="a" type="f">uyt-spreyden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ver over 't gheen ick doe, of <rhyme label="b" type="m">laet</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Och ! Heer, wilt soo mijn hert <rhyme label="a" type="f" corresp="#s3:l1">gheleyden</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dats' u eer stelt in d'hooghsten <rhyme label="b" type="m" corresp="#s3:l2">graedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laet my gheen dinck soo hoogh noyt <rhyme label="a" type="f">achten</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Als u, mijn Vreucht, mijn Godt, mijn <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Laet my uyt Liefde altijdt <rhyme label="a" type="f" corresp="#s4:l1">trachten</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Om u te dienen tot u <rhyme label="b" type="m" corresp="#s4:l2">Eer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Och ! wilt de Liefde soo <rhyme label="a" type="f">ontsteecken</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dat ghy noyt gaet uyt mijnen <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En onversadigh u mach <rhyme label="a" type="f" corresp="#s5:l1">smeecken</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Me'er als een Minnaer sijn <rhyme label="b" type="m" corresp="#s5:l2">Vriendin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Op dat ick u soo mach <rhyme label="a" type="f">aenhanghen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Dat noyt u Dienst my en <rhyme label="b" type="m">verveelt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Soo wel als my 't verdriet komt <rhyme label="a" type="f" corresp="#s6:l1">pranghen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Dan als ghy vreuchd' my <rhyme label="b" type="m" corresp="#s6:l2">mede-deelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Heer, laet ick trouw'lijck sorghe <rhyme label="a" type="f">draghen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Dat oock wiens werck blijft <rhyme label="b" type="m">onghedaen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">U werck op 't hooghst' my mach <rhyme label="a" type="f" corresp="#s7:l1">behaghen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En neerstigh mach voor allen <rhyme label="b" type="m" corresp="#s7:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Op dat ick alles mach <rhyme label="a" type="f">versaecken</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Dat my van uwe Liefde <rhyme label="b" type="m">scheydt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Om meer, en meer, u te <rhyme label="a" type="f" corresp="#s8:l1">ghenaecken</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">O goede Godt ! in <rhyme label="b" type="m" corresp="#s8:l2">eenigheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O groote Godt ! ghy zijt de <rhyme label="a" type="f">Liefde</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Wie in u blijft, ghy blijft in <rhyme label="b" type="m">hem</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Och ! dat u gunst my altijdt <rhyme label="a" type="f" corresp="#s9:l1">stijfde</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Dan soud' mijn Liefd' eerst hebben <rhyme label="b" type="m" corresp="#s9:l2">klem</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">LAet my soo sterck steeds zijn <rhyme label="a" type="f">bevonden</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Dat ick in 't minste niet en <rhyme label="b" type="m">acht</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Al werd' ick van de spitse <rhyme label="a" type="f" corresp="#s10:l1">monden</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Om mijnen yver <rhyme label="b" type="m" corresp="#s10:l2">uyt-gelacht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Maer laet mijn Liefde meer <rhyme label="a" type="f">ontsteecken</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Als men in 't goedt my <rhyme label="b" type="m">teghen-staet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En draghen duldigh 't schimpigh <rhyme label="a" type="f" corresp="#s11:l1">spreken</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Om u Liefd', Heer, mijn <rhyme label="b" type="m" corresp="#s11:l2">toe-verlaet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Al waer 't schoon dat vast alle <rhyme label="a" type="f">luyden</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Door haet, of onverstandt <rhyme label="b" type="m">verblint</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Al mijn bedrijf ten quaedtsten <rhyme label="a" type="f" corresp="#s12:l1">duyden</rhyme>:</l>
        <l id="s12:l4" met="4" type="-+">Maeckt dat u Liefde 't al <rhyme label="b" type="m" corresp="#s12:l2">verwint</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Laet my mijn Naesten soo <rhyme label="a" type="f">beminnen</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Ghelijck my selven, tot u <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Om uwen 't wil met hert en <rhyme label="a" type="f" corresp="#s13:l1">sinnen</rhyme>:</l>
        <l id="s13:l4" met="4" type="-+">Och ! gheeft my kracht daer toe, o ! <rhyme label="b" type="m" corresp="#s13:l2">Heer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Ey ! laet de Liefde my <rhyme label="a" type="f">regeeren</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">En heyligh stieren na u <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Dat ick mijns Naestens schaed' koom <rhyme label="a" type="f" corresp="#s14:l1">weeren</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">En soecken sijn eer, en <rhyme label="b" type="m" corresp="#s14:l2">ghewin</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Laet my door gierigheydt niet <rhyme label="a" type="f">trachten</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Alleen te soecken mijn <rhyme label="b" type="m">proffijt</rhyme>:</l>
        <l id="s15:l3" met="4" type="-+">Maer oock mijn Naesten waerdigh <rhyme label="a" type="f" corresp="#s15:l1">achten</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Mijn gunst te toonen t'allen <rhyme label="b" type="m" corresp="#s15:l2">tijt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Op dat mijn woorden, en mijn <rhyme label="a" type="f">wercken</rhyme></l>
        <l id="s16:l2" met="4" type="-+">Hem moghen stichten tot u <rhyme label="b" type="m">eer</rhyme>.</l>
        <l id="s16:l3" met="4" type="-+">Wilt my hier in, o Godt ! <rhyme label="a" type="f" corresp="#s16:l1">verstercken</rhyme></l>
        <l id="s16:l4" met="4" type="-+">Dit te be-yv'ren meer en <rhyme label="b" type="m" corresp="#s16:l2">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
