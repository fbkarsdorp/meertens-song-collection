<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6121</id>
    <date>
      <year-start>1632</year-start>
      <year-end>1632</year-end>
    </date>
    <location>
      <orginal-name>emmerik</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Venus gij en uw kind</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CHristus, o Heer, <rhyme label="a" type="m">playsant</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghy staet int herte <rhyme label="a" type="m" corresp="#s1:l1">geplant</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Versterckt my al mijn <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mijn droevich hert van <rhyme label="b" type="f" corresp="#s1:l3">binnen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Och mocht ick u <rhyme label="c" type="f">gewinnen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die ick soo seer <rhyme label="c" type="f" corresp="#s1:l5">beminnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick bid u Heere <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Laet my u dienaer <rhyme label="a" type="m" corresp="#s2:l1">zijn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ick hoop 't sal my <rhyme label="b" type="f">gebeuren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Anders soo sal ick <rhyme label="b" type="f" corresp="#s2:l3">treuren</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Och mocht ick u <rhyme label="c" type="f">gewinnen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die ick soo seer <rhyme label="c" type="f" corresp="#s2:l5">beminnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy zijt al mijn <rhyme label="a" type="m">gerief</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ghy zijt, o Heer, mijn <rhyme label="a" type="m" corresp="#s3:l1">lief</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Na u staet mijn <rhyme label="b" type="f">verlangen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ghy hebt mijn hert <rhyme label="b" type="f" corresp="#s3:l3">gevangen</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Och mocht ick u <rhyme label="c" type="f">gewinnen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die ick soo seer <rhyme label="c" type="f" corresp="#s3:l5">beminnen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick heb my gantsch <rhyme label="a" type="m">gestelt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">O Godt in u <rhyme label="a" type="m" corresp="#s4:l1">gewelt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wilt mijn hert t'allen <rhyme label="b" type="f">tijden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met u aenschijn <rhyme label="b" type="f" corresp="#s4:l3">verblijden</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Och mocht ick u <rhyme label="c" type="f">gewinnen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Die ick soo seer <rhyme label="c" type="f" corresp="#s4:l5">beminnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Noyt en heeft mijn <rhyme label="a" type="m">natuer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Gesocht die <rhyme label="a" type="m" corresp="#s5:l1">creatuer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Maer u alleen, o <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tot uwen lof, en <rhyme label="b" type="f" corresp="#s5:l3">eere</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Och mocht ick u <rhyme label="c" type="f">gewinnen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die ick soo seer <rhyme label="c" type="f" corresp="#s5:l5">beminnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Comt, Heer, en beyt niet <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Mijn hert verlanght soo <rhyme label="a" type="m" corresp="#s6:l1">seer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Mijn Bruydegom <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Mijn troost, mijn hoop, mijn <rhyme label="b" type="f" corresp="#s6:l3">leven</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En sal ick u nu <rhyme label="c" type="f">derven</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Den doot die moet ick <rhyme label="c" type="f" corresp="#s6:l5">sterven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Boven in 's Hemels <rhyme label="a" type="m">throon</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Spant ghy alleen die <rhyme label="a" type="m" corresp="#s7:l1">Croon</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">V gracy comt van <rhyme label="b" type="f">baven</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">V lieffelicke <rhyme label="b" type="f" corresp="#s7:l3">gaven</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">En sal ick u nu <rhyme label="c" type="f">derven</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Den doot die moet ick <rhyme label="c" type="f" corresp="#s7:l5">sterven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Daerom die uyr en <rhyme label="a" type="m">stondt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Als ick u eerstmaels <rhyme label="a" type="m" corresp="#s8:l1">vondt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Deden my wel <rhyme label="b" type="f">behagen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Nu en tot allen <rhyme label="b" type="f" corresp="#s8:l3">dagen</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">En sal ick u nu <rhyme label="c" type="f">derven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Den doot die moet ick <rhyme label="c" type="f" corresp="#s8:l5">sterven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Welkom ghy edel <rhyme label="a" type="m">Prins</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Welkom daer na ick <rhyme label="a" type="m" corresp="#s9:l1">wins</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Welkom mijn Godt <rhyme label="b" type="f">verheven</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Welkom mijn vreucht mijn <rhyme label="b" type="f" corresp="#s9:l3">leven</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">V sal mijn ziel noyt <rhyme label="c" type="f">derven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Met u so moet ick <rhyme label="c" type="f" corresp="#s9:l5">sterven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
