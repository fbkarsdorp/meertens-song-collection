  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Ag, hoe <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Is het als twee <rhyme label="b" type="f">beminnen</rhyme>;</l>
        <l id="s1:l3" met="2" type="+-">Ag, hoe <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Is wedersijdse <rhyme label="a" type="m" corresp="#s1:l1">gloed</rhyme>!</l>
        <l id="s1:l5" met="2" type="-+">Voorwaar, geen <rhyme label="c" type="m">vreugd</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Streeld immer soo de <rhyme label="b" type="f" corresp="#s1:l2">sinnen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Voorwaar, geen <rhyme label="c" type="m" corresp="#s1:l5">vreugd</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Die soo volmaakt <rhyme label="c" type="m" corresp="#s1:l5">verheugd</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Als mijn <rhyme label="a" type="m">mond</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Kloris, u mag <rhyme label="b" type="f">kussen</rhyme>;</l>
        <l id="s2:l3" met="2" type="+-">Als mijn <rhyme label="a" type="m" corresp="#s2:l1">mond</rhyme></l>
        <l id="s2:l4" met="3" type="-+">(O oorsaak van mijn <rhyme label="a" type="m" corresp="#s2:l1">wond</rhyme>)</l>
        <l id="s2:l5" met="2" type="-+">Door dat <rhyme label="c" type="m">genot</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Iets   tragt mijn vlam te <rhyme label="b" type="f" corresp="#s2:l2">blussen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Is dat <rhyme label="c" type="m" corresp="#s2:l5">genot</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Voor my een heilsaam <rhyme label="c" type="m" corresp="#s2:l5">lot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Hoe vol <rhyme label="a" type="m">vreugd</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sou dan mijn hert <rhyme label="b" type="f">herleven</rhyme>;</l>
        <l id="s3:l3" met="2" type="+-">Hoe vol <rhyme label="a" type="m" corresp="#s3:l1">vreugd</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Herleefde dan mijn <rhyme label="a" type="m" corresp="#s3:l1">jeugd</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Geen Konings <rhyme label="c" type="m">staat</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Sou my meer blydschap <rhyme label="b" type="f" corresp="#s3:l2">geven</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Geen Konings <rhyme label="c" type="m" corresp="#s3:l5">staat</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Dat soet te boven <rhyme label="c" type="m" corresp="#s3:l5">gaat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
