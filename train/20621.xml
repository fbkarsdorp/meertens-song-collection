      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Joan Ysermans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LEstmael hadden Lethys <rhyme label="a" type="f">stroomen</rhyme></l>
        <l id="s1:l2" met="2" type="+-">My <rhyme label="b" type="m">ghevelt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Oock heeft my den Godt der <rhyme label="a" type="f" corresp="#s1:l1">droomen</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Doen <rhyme label="b" type="m" corresp="#s1:l2">ghequelt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">My docht dat my Iuno <rhyme label="c" type="m">sant</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Daer ick Mars by Venus <rhyme label="c" type="m" corresp="#s1:l5">vant</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Die met <rhyme label="d" type="f">lusten</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Haer vast <rhyme label="d" type="f" corresp="#s1:l7">custen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Ende <rhyme label="d" type="f" corresp="#s1:l7">blusten</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Haren <rhyme label="c" type="m" corresp="#s1:l5">brant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Phoebus diet met sijne <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Al <rhyme label="b" type="m">doorsiet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die en cost dit niet <rhyme label="a" type="f" corresp="#s2:l1">ghedooghen</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Des hy <rhyme label="b" type="m" corresp="#s2:l2">liet</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Sijnen troon, en vvees <rhyme label="c" type="m">Vulcan'</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Waer sy moeder nackt <rhyme label="c" type="m" corresp="#s2:l5">ontdaen</rhyme></l>
        <l id="s2:l7" met="2" type="+-">T'samen <rhyme label="d" type="f">laghen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Dies den <rhyme label="d" type="f" corresp="#s2:l7">traghen</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Doort <rhyme label="d" type="f" corresp="#s2:l7">mishaghen</rhyme></l>
        <l id="s2:l10" met="2" type="+-">Wert <rhyme label="c" type="m" corresp="#s2:l5">bestaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Als hier Mulciber op <rhyme label="a" type="f">letten</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Moest hy <rhyme label="b" type="m">sien</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Dat hem Mars vast horens <rhyme label="a" type="f" corresp="#s3:l1">setten</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Bouen <rhyme label="b" type="m" corresp="#s3:l2">dien</rhyme></l>
        <l id="s3:l5" met="4" type="+-">En vvas hy niet vvel te <rhyme label="c" type="m">vre</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Op d'oncuysche <rhyme label="c" type="m" corresp="#s3:l5">CytherÃ«</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Sijn seer <rhyme label="d" type="f">bouvve</rhyme></l>
        <l id="s3:l8" met="2" type="+-">Snoode <rhyme label="d" type="f" corresp="#s3:l7">vrouvve</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Om <rhyme label="d" type="f" corresp="#s3:l7">d'ontrouvve</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Die s'hem <rhyme label="c" type="m" corresp="#s3:l5">de</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy is naer sijn smis <rhyme label="a" type="f">ghetreden</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">En <rhyme label="b" type="m">beghonst</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Een doorluchtich net te <rhyme label="a" type="f" corresp="#s4:l1">smeden</rhyme></l>
        <l id="s4:l4" met="2" type="+-">Door sijn <rhyme label="b" type="m" corresp="#s4:l2">const</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">D'vvelck hy met <rhyme label="c" type="m">behendicheyt</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Ouer t'Bedde heeft <rhyme label="c" type="m" corresp="#s4:l5">ghespreyt</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">De Goden <rhyme label="d" type="f">quamen</rhyme></l>
        <l id="s4:l8" met="2" type="+-">Al te <rhyme label="d" type="f" corresp="#s4:l7">samen</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Hun <rhyme label="d" type="f" corresp="#s4:l7">beschamen</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">Op het <rhyme label="c" type="m" corresp="#s4:l5">feyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer al vvast dat sy'r me <rhyme label="a" type="f">schoren</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Haren <rhyme label="b" type="m">sodt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">VVie en soud'om sulcken <rhyme label="a" type="f" corresp="#s5:l1">voeren</rhyme></l>
        <l id="s5:l4" met="2" type="+-">Niet <rhyme label="b" type="m" corresp="#s5:l2">bespodt</rhyme></l>
        <l id="s5:l5" met="4" type="+-">VVillen vvesen? hier oft <rhyme label="c" type="m">el</rhyme>?</l>
        <l id="s5:l6" met="4" type="+-">En niet achten op <rhyme label="c" type="m" corresp="#s5:l5">t'ghe-quel</rhyme>?</l>
        <l id="s5:l7" met="2" type="+-">Oft <rhyme label="d" type="f">gheschieden</rhyme></l>
        <l id="s5:l8" met="2" type="+-">VVie soud' <rhyme label="d" type="f" corresp="#s5:l7">vlieden</rhyme></l>
        <l id="s5:l9" met="2" type="+-">Van u <rhyme label="d" type="f" corresp="#s5:l7">lieden</rhyme></l>
        <l id="s5:l10" met="2" type="+-">Sulcken <rhyme label="c" type="m" corresp="#s5:l5">spel</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
