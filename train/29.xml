  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">HOoge <rhyme label="a" type="m">Goon</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Kunt ghy 't verlies <rhyme label="b" type="f">gedogen</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Wat verdriet baart dese <rhyme label="c" type="m">rouw</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Of is het om een <rhyme label="d" type="m">Lam</rhyme>?</l>
        <l id="s1:l5" met="2" type="-+">Het sien <rhyme label="d" type="m" corresp="#s1:l4">benam</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">My, dat ik verloor, en echter noch <rhyme label="c" type="m" corresp="#s1:l3">behouw</rhyme>.</l>
        <l id="s1:l7" met="2" type="+-">'t Schijnt, tot <rhyme label="a" type="m" corresp="#s1:l1">loon</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dat zich mijn Ziel <rhyme label="b" type="f" corresp="#s1:l2">bewogen</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Met uw droeve klachten <rhyme label="e" type="m">toont</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Uw seggen met mijn <rhyme label="f" type="m">boert</rhyme>.</l>
        <l id="s1:l11" met="2" type="+-">De waerheyt <rhyme label="f" type="m" corresp="#s1:l10">voert</rhyme></l>
        <l id="s1:l12" met="5" type="-+">De droefheyt, en 't geen in mijn verhalen <rhyme label="e" type="m" corresp="#s1:l9">woont</rhyme>.</l>
        <l id="s1:l13" met="2" type="-+">Jaa dit <rhyme label="g" type="f">verloren</rhyme></l>
        <l id="s1:l14" met="3" type="+-">Baert mijn grootste <rhyme label="h" type="m">vreugt</rhyme>.</l>
        <l id="s1:l15" met="2" type="-+">Laat my doch <rhyme label="g" type="f" corresp="#s1:l13">horen</rhyme></l>
        <l id="s1:l16" met="3" type="+-">d'Oorsaak uws <rhyme label="h" type="m" corresp="#s1:l14">geneugt</rhyme>.</l>
        <l id="s1:l17" met="1" type="-+">Het <rhyme label="i" type="m">Koor</rhyme></l>
        <l id="s1:l18" met="1" type="-+">Mijns <rhyme label="i" type="m" corresp="#s1:l17">Oor</rhyme></l>
        <l id="s1:l19" met="2" type="-+">Wiert, door uw <rhyme label="j" type="m">Tongh</rhyme>,</l>
        <l id="s1:l20" met="1" type="-+">
          <rhyme label="k" type="m">Betreen</rhyme>
        </l>
        <l id="s1:l21" met="2" type="-+">Met die <rhyme label="k" type="m" corresp="#s1:l20">reen</rhyme></l>
        <l id="s1:l22" met="3" type="-+">Die mijn begrip niet <rhyme label="j" type="m" corresp="#s1:l19">vong</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-"><rhyme label="a" type="m">Afgodin</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Vw overhelder <rhyme label="b" type="f">zingen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat gelijck Amphions <rhyme label="c" type="m">stem</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Een Tebens prat <rhyme label="d" type="m">gebouw</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Bemuren <rhyme label="d" type="m" corresp="#s2:l4">zouw</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Sloot vryheyt, en hert in een gewenste <rhyme label="c" type="m" corresp="#s2:l3">klem</rhyme>.</l>
        <l id="s2:l7" met="2" type="+-">Heeft de <rhyme label="a" type="m" corresp="#s2:l1">zin</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Van dat ik deed <rhyme label="b" type="f" corresp="#s2:l2">ontspringen</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">V dan niet tot vreugd <rhyme label="e" type="m">bekoort</rhyme>?</l>
        <l id="s2:l10" met="3" type="-+">De zin van 't zinrijck <rhyme label="f" type="m">liet</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Verstond ik <rhyme label="f" type="m" corresp="#s2:l10">niet</rhyme>:</l>
        <l id="s2:l12" met="5" type="-+">Maer ben door 't gehoor met vele vreugd' <rhyme label="e" type="m" corresp="#s2:l9">gesthoort</rhyme>.</l>
        <l id="s2:l13" met="2" type="-+">Vw fluyt, en <rhyme label="g" type="f">reden</rhyme></l>
        <l id="s2:l14" met="3" type="+-">Zouw, de wreedste <rhyme label="h" type="m">ziel</rhyme>,</l>
        <l id="s2:l15" met="2" type="-+">Zoo kunnen <rhyme label="g" type="f" corresp="#s2:l13">kneden</rhyme></l>
        <l id="s2:l16" met="3" type="+-">Als uw lust <rhyme label="h" type="m" corresp="#s2:l14">beviel</rhyme>.</l>
        <l id="s2:l17" met="1" type="-+">
          <rhyme label="i" type="m">Waarom</rhyme>
        </l>
        <l id="s2:l18" met="1" type="-+">
          <rhyme label="i" type="m" corresp="#s2:l17">Bekom</rhyme>
        </l>
        <l id="s2:l19" met="2" type="-+">'k Dan niet door <rhyme label="j" type="m">reen</rhyme>,</l>
        <l id="s2:l20" met="1" type="-+">Vw <rhyme label="k" type="m">liefd'</rhyme>,</l>
        <l id="s2:l21" met="2" type="-+">Die my <rhyme label="k" type="m" corresp="#s2:l20">griefd'</rhyme></l>
        <l id="s2:l22" met="3" type="-+">Door boesem, Hert, en <rhyme label="j" type="m" corresp="#s2:l19">leen</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Ach! mijn <rhyme label="a" type="m">eer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Kan dese reen niet <rhyme label="b" type="f">dogen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Een op Echt gevest <rhyme label="c" type="m">verbond</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">My naakt, van schaemt, een <rhyme label="d" type="m">bloos</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Wiens root, de <rhyme label="d" type="m" corresp="#s3:l4">Roos</rhyme></l>
        <l id="s3:l6" met="5" type="-+">Om haar kleur gekeurt, in hare schoonheyt <rhyme label="c" type="m" corresp="#s3:l3">wond</rhyme>.</l>
        <l id="s3:l7" met="2" type="+-">Maar, <rhyme label="a" type="m" corresp="#s3:l1">wanneer</rhyme></l>
        <l id="s3:l8" met="3" type="-+">De Son, de dauw, komt <rhyme label="b" type="f" corresp="#s3:l2">drogen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">En haar Karmomzijne <rhyme label="e" type="m">blaan</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Op 't heerlijkst, <rhyme label="f" type="m">openset</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Haar glans <rhyme label="f" type="m" corresp="#s3:l10">verplet</rhyme>,</l>
        <l id="s3:l12" met="5" type="-+">En maeckt, van haer geur, een digt verwelckte <rhyme label="e" type="m" corresp="#s3:l9">laen</rhyme>.</l>
        <l id="s3:l13" met="2" type="-+">De Dadel <rhyme label="g" type="f">tronken</rhyme></l>
        <l id="s3:l14" met="3" type="+-">Zoo langh vrugtloos <rhyme label="h" type="m">staen</rhyme>,</l>
        <l id="s3:l15" met="2" type="-+">Tot <rhyme label="g" type="f" corresp="#s3:l13">Mannestronken</rhyme>,</l>
        <l id="s3:l16" met="3" type="+-">Naken hare <rhyme label="h" type="m" corresp="#s3:l14">blaan</rhyme>.</l>
        <l id="s3:l17" met="1" type="-+">
          <rhyme label="i" type="m">Mevrouw</rhyme>
        </l>
        <l id="s3:l18" met="1" type="-+">De <rhyme label="i" type="m" corresp="#s3:l17">trouw</rhyme></l>
        <l id="s3:l19" met="2" type="-+">Ontmaagt geen <rhyme label="j" type="m">Maagt</rhyme>,</l>
        <l id="s3:l20" met="1" type="-+">Zoo <rhyme label="k" type="m">zy</rhyme></l>
        <l id="s3:l21" met="2" type="-+">Zig, staag, <rhyme label="k" type="m" corresp="#s3:l20">by</rhyme></l>
        <l id="s3:l22" met="3" type="-+">Haar Ega eerlijck <rhyme label="j" type="m" corresp="#s3:l19">draagt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
