<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161239</id>
    <date>
      <year-start>1740</year-start>
      <year-end>1740</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Air de Joconde</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ONtwaeckt, loopt Herders desen <rhyme label="a" type="m">nacht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Verlaet all' uwe <rhyme label="b" type="f">Schaepen</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Uw' nieuwen Koninck lof <rhyme label="a" type="m" corresp="#s1:l1">verwacht</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En blyft niet langer <rhyme label="b" type="f" corresp="#s1:l2">slaepen</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Geeft soet gesanck, en wilt <rhyme label="c" type="m">Schalmey</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Noch Fluyte niet <rhyme label="d" type="f">vergeten</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Waer mé gy in de groene <rhyme label="c" type="m" corresp="#s1:l5">wey</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Soo dickwils syt <rhyme label="d" type="f" corresp="#s1:l6">geseten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat een ontydig' stem voel <rhyme label="a" type="m">ick</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soo vroegh myn ooren <rhyme label="b" type="f">raeken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En my op eenen <rhyme label="a" type="m" corresp="#s2:l1">oogenblick</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Uyt mynen slaep <rhyme label="b" type="f" corresp="#s2:l2">ontwaeken</rhyme>?</l>
        <l id="s2:l5" met="4" type="-+">Den nacht versoekt noch langer <rhyme label="c" type="m">rust</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Den Haen gaet noch niet <rhyme label="d" type="f">schreyen</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">Laet ieder slaepen naer syn <rhyme label="c" type="m" corresp="#s2:l5">lust</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Eer gy roept naer de <rhyme label="d" type="f" corresp="#s2:l6">weyen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Te lanck, ô Herder, duert uw' <rhyme label="a" type="m">rust</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Heft uw' vermoeyde <rhyme label="b" type="f">leden</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Het Hemels licht schynt op dees <rhyme label="a" type="m" corresp="#s3:l1">kust</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Verstaet gy wel myn <rhyme label="b" type="f" corresp="#s3:l2">reden</rhyme>?</l>
        <l id="s3:l5" met="4" type="-+">Geboren is den Heer, Godt <rhyme label="c" type="m">mensch</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Om breeder te <rhyme label="d" type="f">verklaeren</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Ick dael van boven om uw' <rhyme label="c" type="m" corresp="#s3:l5">wensch</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En vreught te <rhyme label="d" type="f" corresp="#s3:l6">openbaeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat klaerheydt sien ick <rhyme label="a" type="m">onverwacht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Voor myn gesicht <rhyme label="b" type="f">verschynen</rhyme>?</l>
        <l id="s4:l3" met="4" type="-+">Siet seffens op den <rhyme label="a" type="m" corresp="#s4:l1">midder-nacht</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De duysterheydt <rhyme label="b" type="f" corresp="#s4:l2">verdwynen</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">t'Is seker, dat den Heer van <rhyme label="c" type="m">Al</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Op d'aerde wordt <rhyme label="d" type="f">geboren</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Myn hert, en ziele branden <rhyme label="c" type="m" corresp="#s4:l5">sal</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Door't licht, soo lanck <rhyme label="d" type="f" corresp="#s4:l6">verloren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">t'En is dan, Herder, niet te <rhyme label="a" type="m">vroegh</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Gaet, geeft hem hert, en <rhyme label="b" type="f">sinnen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Men kan niet haestigh syn <rhyme label="a" type="m" corresp="#s5:l1">genoegh</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Om Godt te gaen <rhyme label="b" type="f" corresp="#s5:l2">beminnen</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Spoeyt u, roept uw' gebueren <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En gaet met vol <rhyme label="d" type="f">betrouwen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Dien Godt, vol teerheydt, in <rhyme label="c" type="m" corresp="#s5:l5">eenstal</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met teere liefd' <rhyme label="d" type="f" corresp="#s5:l6">aenschouwen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Kloeck Herders al, staet-op met <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Den Heer komt ons <rhyme label="b" type="f">ontmoeten</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Leydt, Engels Geesten, ons daer <rhyme label="a" type="m" corresp="#s6:l1">by</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Om't Hemels kindt te <rhyme label="b" type="f" corresp="#s6:l2">groeten</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Thoont ons de plaets, wy gaen tot <rhyme label="c" type="m">hem</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Den oorspronck van ons <rhyme label="d" type="f">leven</rhyme>:</l>
        <l id="s6:l7" met="4" type="-+">Wy volgen t'saemen op uw' <rhyme label="c" type="m" corresp="#s6:l5">stem</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Om danckbaerheydt te <rhyme label="d" type="f" corresp="#s6:l6">geven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
