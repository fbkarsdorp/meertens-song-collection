<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185866</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Solaas wil ik hanteren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ghy Mannen ende <rhyme label="a" type="f">Vrouwen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die op die werelt <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wildy v ziel <rhyme label="a" type="f" corresp="#s1:l1">behouwen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En beteren uwen <rhyme label="b" type="m" corresp="#s1:l2">tijt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wildy den vyant <rhyme label="c" type="f">mincken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die ons altijt volght <rhyme label="d" type="m">nae</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hebt altijt in v <rhyme label="c" type="f" corresp="#s1:l5">gedincken</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Den name van <rhyme label="d" type="m" corresp="#s1:l6">Maria</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maria is so <rhyme label="a" type="f">soeten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ende so wel <rhyme label="b" type="m">genaemt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Datse die Engelen <rhyme label="a" type="f" corresp="#s2:l1">groeten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Also haer wel <rhyme label="b" type="m" corresp="#s2:l2">betaemt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Het is die <rhyme label="c" type="f">Coninginne</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die Godt zijn boodtschap <rhyme label="d" type="m">sandt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Vol gratien ende vol <rhyme label="c" type="f" corresp="#s2:l5">minne</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Maria is zy <rhyme label="d" type="m" corresp="#s2:l6">ghenaemt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Des heyligen <rhyme label="a" type="f">Kersnacht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Woude hy gheboren <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Van haer die op <rhyme label="a" type="f" corresp="#s3:l1">bracht</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Des Hemelschen <rhyme label="b" type="m" corresp="#s3:l2">doctrijn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Sonder wee oft <rhyme label="c" type="f">smerte</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In Maechdelijcker <rhyme label="d" type="m">vreucht</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Nu loven wy met <rhyme label="c" type="f" corresp="#s3:l5">herte</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Maria die suyver <rhyme label="d" type="m" corresp="#s3:l6">jeucht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">By des Enghels <rhyme label="a" type="f">salute</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Datse Iesum <rhyme label="b" type="m">ontsinck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wert die vyant te <rhyme label="a" type="f" corresp="#s4:l1">buyte</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Van ons die <rhyme label="b" type="m" corresp="#s4:l2">bevinck</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Het landt en Godt die <rhyme label="c" type="f">Vader</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Al Marien <rhyme label="d" type="m">lijf</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Wy dancken alle <rhyme label="c" type="f" corresp="#s4:l5">gader</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Marien dat reyne <rhyme label="d" type="m" corresp="#s4:l6">wijf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die neghen Enghelen <rhyme label="a" type="f">chooren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die salighe <rhyme label="b" type="m">Seraphin</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Altoos sonder <rhyme label="a" type="f" corresp="#s5:l1">verhooren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Loven met herten <rhyme label="b" type="m" corresp="#s5:l2">fijn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die soete suyver <rhyme label="c" type="f">name</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die elck met herten <rhyme label="d" type="m">dient</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Die Godt wel is <rhyme label="c" type="f" corresp="#s5:l5">bequame</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Maria des menschen <rhyme label="d" type="m" corresp="#s5:l6">vrient</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maria is die <rhyme label="a" type="f">vlame</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die tâ€™Paradijs <rhyme label="b" type="m">verlicht</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maria sonder <rhyme label="a" type="f" corresp="#s6:l1">blame</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Vol gratien is <rhyme label="b" type="m" corresp="#s6:l2">ghesticht</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Sy is die <rhyme label="c" type="f">tresorie</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Van gratien, dat is <rhyme label="d" type="m">certeyn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Met love willen wy <rhyme label="c" type="f" corresp="#s6:l5">vercieren</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Maria al <rhyme label="d" type="m" corresp="#s6:l6">gemeyn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">So wie dat in dit <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Maria eere <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sy en sal hem niet <rhyme label="a" type="f" corresp="#s7:l1">begeven</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Al ist dat hy <rhyme label="b" type="m" corresp="#s7:l2">misdoet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Gratie sal hy <rhyme label="c" type="f">vercrijgen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Als dede <rhyme label="d" type="m">Theophilus</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Hy en wilde den lof niet <rhyme label="c" type="f" corresp="#s7:l5">swijgen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Mariam die Moeder <rhyme label="d" type="m" corresp="#s7:l6">Iesus</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Die can Maria <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ende dienen met herten <rhyme label="b" type="m">geheel</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sy sal hem <rhyme label="a" type="f" corresp="#s8:l1">inpetreren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Van gratien sulck <rhyme label="b" type="m" corresp="#s8:l2">ghedeel</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Dat hy niet en sal <rhyme label="c" type="f">mogen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Verdoemt zijn na zijner <rhyme label="d" type="m">doot</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Nu laet ons allen <rhyme label="c" type="f" corresp="#s8:l5">loven</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Mariam, wy hebbens <rhyme label="d" type="m" corresp="#s8:l6">noot</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maria draeght die <rhyme label="a" type="f">croone</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Naest Iesus haer lieve <rhyme label="b" type="m">kint</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Sy sal hem waerlijck <rhyme label="a" type="f" corresp="#s9:l1">loonen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Diese met herten <rhyme label="b" type="m" corresp="#s9:l2">mint</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Nu bidden wy alle <rhyme label="c" type="f">gemeyne</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Mariam de suyver <rhyme label="d" type="m">jeucht</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Dat sy ons <rhyme label="c" type="f" corresp="#s9:l5">verleene</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Haer gratie ende haer <rhyme label="d" type="m" corresp="#s9:l6">deucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
