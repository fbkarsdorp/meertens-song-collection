<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22378</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gezongen onze vader" translation="sung Lord's prayer">gezongen onze vader</tag>
    </tags>
    <melody>Onze vader in hemelrijk</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vader onse in <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ghy die ons alle hert <rhyme label="a" type="m" corresp="#s1:l1">ghelijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Broeders zijn, en u roepen <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Wilt dit bidden van ons <rhyme label="b" type="m" corresp="#s1:l3">ontfaen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Gheeft dat alleen niet bid den <rhyme label="c" type="m">mondt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Helpt dat het coemt van herten <rhyme label="c" type="m" corresp="#s1:l5">grondt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gheheylicht worde uwen name <rhyme label="a" type="m">alleyn</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">U woordt by ons helpt houden <rhyme label="a" type="m" corresp="#s2:l1">reyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat wy oock leuen <rhyme label="b" type="m">heylichlijck</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Nae uwen name <rhyme label="b" type="m" corresp="#s2:l3">weerdichlijck</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Behoedt ons Heer voor valsche <rhyme label="c" type="m">leer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dat arm verdoelde volck <rhyme label="c" type="m" corresp="#s2:l5">bekeer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Toecoem ons u rijck in deser <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En oock hier nae int eewich <rhyme label="a" type="m" corresp="#s3:l1">iolijt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Den heylghen Gheest die woon ons <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Met zijne ghauen <rhyme label="b" type="m" corresp="#s3:l3">menigherley</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Des Duyuels toorn en groot <rhyme label="c" type="m">ghewelt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Heer breeckt, voor hem u Kerck <rhyme label="c" type="m" corresp="#s3:l5">behelt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dijn will geschie Heer God <rhyme label="a" type="m">ghelijck</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Op Aerden als in <rhyme label="a" type="m" corresp="#s4:l1">Hemelrijck</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gheeft ons ghedult in slijdens <rhyme label="b" type="m">pijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ghehoorsaem in lief en leet te <rhyme label="b" type="m" corresp="#s4:l3">zijn</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Weert en keert alle vleesch en <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dat teghens uwen wille <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gheeft ons heden ons daglijcx <rhyme label="a" type="m">broodt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En watmen behoeft tot sleuens <rhyme label="a" type="m" corresp="#s5:l1">noodt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Behoedt ons Heer voor twist en <rhyme label="b" type="m">strijt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Voor sieckte en voor dieren <rhyme label="b" type="m" corresp="#s5:l3">tijt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dat wy in goeden vrede <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Doet sorg en giericheyt van ons <rhyme label="c" type="m" corresp="#s5:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Al onse schult vergheeft ons <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat sy ons niet bedroeuen <rhyme label="a" type="m" corresp="#s6:l1">meer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Als wy oock onsen <rhyme label="b" type="m">schuldenern</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Haer schult en fault vergheuen <rhyme label="b" type="m" corresp="#s6:l3">ghern</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Te dienen maeckt ons al <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">In rechte liefd en <rhyme label="c" type="m" corresp="#s6:l5">eenicheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">En leyt ons Heer in becoring <rhyme label="a" type="m">nicht</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Als ons den boosen gheest <rhyme label="a" type="m" corresp="#s7:l1">aenuicht</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ter slincker oft ter rechter <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Helpt ons doen stercken <rhyme label="b" type="m" corresp="#s7:l3">wederstandt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Vast int gheloof met goedt <rhyme label="c" type="m">propoost</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">En door des heylighen Gheestes <rhyme label="c" type="m" corresp="#s7:l5">troost</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Aan alle quaden ons <rhyme label="a" type="m">ontslaet</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Tzijn die tijden ende daghen <rhyme label="a" type="m" corresp="#s8:l1">quaet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Verlost ons van den eewighen <rhyme label="b" type="m">doot</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En troost ons inder lester <rhyme label="b" type="m" corresp="#s8:l3">noot</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Doet ons een salich <rhyme label="c" type="m">onderstandt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Neemt onse siel in uwe <rhyme label="c" type="m" corresp="#s8:l5">handt</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Amen, dat is het werde <rhyme label="a" type="m">waer</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Sterckt ons ghelooue ymmers <rhyme label="a" type="m" corresp="#s9:l1">daer</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Op dat wy daer niet en twijflen <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Dat wy hier mede gebeden <rhyme label="b" type="m" corresp="#s9:l3">haen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Op dijn woordt inden name <rhyme label="c" type="m">dijn</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Soo spreken wy dat Amen <rhyme label="c" type="m" corresp="#s9:l5">fijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
