<song>
  <meta>
    <id>188418</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Les Feuillantines</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlje wercken op goe <rhyme label="a" type="m">voedt</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Dan soo <rhyme label="a" type="m" corresp="#s1:l1">moet</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Ghy sien wat elck Diertje <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Ghy moet yder Beest <rhyme label="b" type="f">bestueren</rhyme>,</l>
        <l id="s1:l5" met="6" type="-+">Nae de drift, na de drift van sijn <rhyme label="b" type="f" corresp="#s1:l4">natuere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hebje eenen lustigh <rhyme label="a" type="m">Paerdt</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Schatten <rhyme label="a" type="m" corresp="#s2:l1">waert</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Laet het volgen sijnen <rhyme label="a" type="m" corresp="#s2:l1">aerdt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En laet het u selven <rhyme label="b" type="f">draghen</rhyme>,</l>
        <l id="s2:l5" met="6" type="-+">Ofte spant, ofte spant het voor de <rhyme label="b" type="f" corresp="#s2:l4">Waghen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Heb j' een Os slaet hem eens <rhyme label="a" type="m">vroegh</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Voor de <rhyme label="a" type="m" corresp="#s3:l1">Ploegh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat is voor hem spuls <rhyme label="a" type="m" corresp="#s3:l1">ghenoegh</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Maer kan hy dat niet <rhyme label="b" type="f">betrachten</rhyme>,</l>
        <l id="s3:l5" met="6" type="-+">Weyd hem vet, weyd hem vet om dan te <rhyme label="b" type="f" corresp="#s3:l4">slachten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hebje Verckens geeft haer <rhyme label="a" type="m">veel</rhyme></l>
        <l id="s4:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s4:l1">Garsten-Meel</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want dat is haer beste <rhyme label="a" type="m" corresp="#s4:l1">deel</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Geeft haer 't Wey van uwe <rhyme label="b" type="f">Koeyen</rhyme>,</l>
        <l id="s4:l5" met="6" type="-+">Dat helpt veel, dat helpt veel om vet te <rhyme label="b" type="f" corresp="#s4:l4">groeyen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer soo ghy een Ezel <rhyme label="a" type="m">hebt</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Daer van <rhyme label="a" type="m" corresp="#s5:l1">schept</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Ghy profijt, soo hy hem <rhyme label="a" type="m" corresp="#s5:l1">rept</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Geeft hem dan te met wat <rhyme label="b" type="f">slaghen</rhyme>,</l>
        <l id="s5:l5" met="6" type="-+">Laet hem sack, laet hem sack en packen <rhyme label="b" type="f" corresp="#s5:l4">draghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hebje Koeyen, geeft haer <rhyme label="a" type="m">Gras</rhyme></l>
        <l id="s6:l2" met="2" type="+-">Op haer <rhyme label="a" type="m" corresp="#s6:l1">pas</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Anders het noyt voordeel <rhyme label="a" type="m" corresp="#s6:l1">was</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">Want daer moeten zy van <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s6:l5" met="6" type="-+">En haer Melck, en haer Melck en plompjes <rhyme label="b" type="f" corresp="#s6:l4">geven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hebje dan noch oock een <rhyme label="a" type="m">Kat</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Geeft het <rhyme label="a" type="m" corresp="#s7:l1">wat</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Van dat witte <rhyme label="a" type="m" corresp="#s7:l1">Koeye-nat</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Laet het woonen in uw <rhyme label="b" type="f">Huysje</rhyme>,</l>
        <l id="s7:l5" met="6" type="-+">Dan soo vanght, dan soo vanght het menigh <rhyme label="b" type="f" corresp="#s7:l4">Muysje</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hebje noch een schrapend <rhyme label="a" type="m">Hoen</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Wilt het <rhyme label="a" type="m" corresp="#s8:l1">voÃªn</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Wat salt schaem'le diertje <rhyme label="a" type="m" corresp="#s8:l1">doen</rhyme>?</l>
        <l id="s8:l4" met="4" type="+-">Daer en valt niet op te <rhyme label="b" type="f">seggen</rhyme>.</l>
        <l id="s8:l5" met="6" type="-+">Want het wil, want het wil u Eyers <rhyme label="b" type="f" corresp="#s8:l4">leggen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">9.Hebje van sulck slagh noch <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Als een <rhyme label="a" type="m" corresp="#s9:l1">Heer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Houdt uw' Beestjes fray in <rhyme label="a" type="m" corresp="#s9:l1">eer</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">Want men moet na Reden <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s9:l5" met="6" type="-+">Yder Dier, yder Dier het sijne <rhyme label="b" type="f" corresp="#s9:l4">gheven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
