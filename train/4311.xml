<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4311</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Courageuse violette</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEl geboren // schoon <rhyme label="a" type="f">vriendinne</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Daer mijn geest, en siel naer <rhyme label="b" type="m">haect</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Wtvercoren // Van <rhyme label="a" type="f" corresp="#s1:l1">beginne</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Boven al meer dan <rhyme label="b" type="m" corresp="#s1:l2">volmaect</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Mijn ionck hert hebt ghy <rhyme label="b" type="m" corresp="#s1:l2">geraect</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Dat ick <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+"><rhyme label="d" type="m">Voortaen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Lijf, en <rhyme label="c" type="m" corresp="#s1:l6">goet</rhyme>,</l>
        <l id="s1:l9" met="1" type="+-"><rhyme label="d" type="m" corresp="#s1:l7">Afgaen</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Om u te <rhyme label="e" type="f">behagen</rhyme>:</l>
        <l id="s1:l11" met="1" type="-+">V <rhyme label="c" type="m" corresp="#s1:l6">soet</rhyme></l>
        <l id="s1:l12" met="1" type="-+"><rhyme label="d" type="m" corresp="#s1:l7">Bystaen</rhyme>,</l>
        <l id="s1:l13" met="1" type="-+">My <rhyme label="c" type="m" corresp="#s1:l6">doet</rhyme></l>
        <l id="s1:l14" met="1" type="-+"><rhyme label="d" type="m" corresp="#s1:l7">Ontslaen</rhyme>,</l>
        <l id="s1:l15" met="3" type="+-">Menich drouvich <rhyme label="e" type="f" corresp="#s1:l10">clagen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">In mijn herte // leyt <rhyme label="a" type="f">gesloten</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Een tresoor der liefden <rhyme label="b" type="m">groot</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Dat noyt smerte // heeft <rhyme label="a" type="f" corresp="#s2:l1">verdroten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">My, noch oock vreese der <rhyme label="b" type="m" corresp="#s2:l2">doot</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Ghy vriendinne zijt (in <rhyme label="b" type="m" corresp="#s2:l2">noot</rhyme>)</l>
        <l id="s2:l6" met="2" type="+-">Soo <rhyme label="c" type="m">troostbaer</rhyme>,</l>
        <l id="s2:l7" met="1" type="-+">Dat <rhyme label="d" type="m">ghy</rhyme></l>
        <l id="s2:l8" met="2" type="+-">T'lijden <rhyme label="c" type="m" corresp="#s2:l6">swaer</rhyme></l>
        <l id="s2:l9" met="1" type="+-">(In <rhyme label="d" type="m" corresp="#s2:l7">my</rhyme>)</l>
        <l id="s2:l10" met="3" type="+-">Cont in vreught <rhyme label="e" type="f">verkeeren</rhyme>:</l>
        <l id="s2:l11" met="1" type="-+"><rhyme label="c" type="m" corresp="#s2:l6">Voorwaer</rhyme>,</l>
        <l id="s2:l12" met="1" type="-+">Ten <rhyme label="d" type="m" corresp="#s2:l7">zy</rhyme>,</l>
        <l id="s2:l13" met="1" type="-+">Ghy <rhyme label="c" type="m" corresp="#s2:l6">daer</rhyme></l>
        <l id="s2:l14" met="1" type="-+">Blijft <rhyme label="d" type="m" corresp="#s2:l7">by</rhyme>,</l>
        <l id="s2:l15" met="3" type="+-">T'hert sal ick <rhyme label="e" type="f" corresp="#s2:l10">verteeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Lofbaer schoone // maeght vol <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ghy verciert een gulden <rhyme label="b" type="m">troon</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Met geen croone // <rhyme label="a" type="f" corresp="#s3:l1">coronneeren</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Can ick, dan uwen <rhyme label="b" type="m" corresp="#s3:l2">persoon</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Mach ick dienstbaer sonder <rhyme label="b" type="m" corresp="#s3:l2">loon</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Eewich <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s3:l7" met="1" type="-+">V <rhyme label="d" type="m">maeght</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Geen <rhyme label="c" type="m" corresp="#s3:l6">gepijn</rhyme></l>
        <l id="s3:l9" met="1" type="+-">
          <rhyme label="d" type="m" corresp="#s3:l7">Beclaeght</rhyme>
        </l>
        <l id="s3:l10" met="3" type="+-">Wert, tot geender <rhyme label="e" type="f">uren</rhyme></l>
        <l id="s3:l11" met="1" type="-+">
          <rhyme label="c" type="m" corresp="#s3:l6">Divijn</rhyme>
        </l>
        <l id="s3:l12" met="1" type="-+">Ghy <rhyme label="d" type="m" corresp="#s3:l7">draeght</rhyme></l>
        <l id="s3:l13" met="1" type="-+"><rhyme label="c" type="m" corresp="#s3:l6">T'aenschijn</rhyme>,</l>
        <l id="s3:l14" met="1" type="-+">En <rhyme label="d" type="m" corresp="#s3:l7">saeght</rhyme></l>
        <l id="s3:l15" met="3" type="+-">Mijn ionck herte <rhyme label="e" type="f" corresp="#s3:l10">trueren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Liever geene // op <rhyme label="a" type="f">aerdrijcke</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dan u, wtgelesen <rhyme label="b" type="m">saet</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">V alleene // <rhyme label="a" type="f" corresp="#s4:l1">vergelijcke</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Ick by Venus inden <rhyme label="b" type="m" corresp="#s4:l2">graet</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Soetste Jeught, die my <rhyme label="b" type="m" corresp="#s4:l2">bestaet</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Reyct u <rhyme label="c" type="m">hant</rhyme></l>
        <l id="s4:l7" met="1" type="-+"><rhyme label="d" type="m">Oprecht</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Doet <rhyme label="c" type="m" corresp="#s4:l6">bystant</rhyme></l>
        <l id="s4:l9" met="1" type="+-">Dijn <rhyme label="d" type="m" corresp="#s4:l7">knecht</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Die om u <rhyme label="e" type="f">volveerdich</rhyme></l>
        <l id="s4:l11" met="1" type="-+">
          <rhyme label="c" type="m" corresp="#s4:l6">Valiant</rhyme>
        </l>
        <l id="s4:l12" met="1" type="-+">Noch <rhyme label="d" type="m" corresp="#s4:l7">vecht</rhyme>,</l>
        <l id="s4:l13" met="1" type="-+"><rhyme label="c" type="m" corresp="#s4:l6">Promant</rhyme>,</l>
        <l id="s4:l14" met="1" type="-+">Hy <rhyme label="d" type="m" corresp="#s4:l7">secht</rhyme></l>
        <l id="s4:l15" met="3" type="+-">Lief! ghy zijt het <rhyme label="e" type="f" corresp="#s4:l10">weerdich</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Eerloos moeten // (qua <rhyme label="a" type="f">vileynen</rhyme>)</l>
        <l id="s5:l2" met="4" type="+-">Sterven, met een hert <rhyme label="b" type="m">beswaert</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Die soo soeten // kint <rhyme label="a" type="f" corresp="#s5:l1">vercleynen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Als ghy zijt, in u <rhyme label="b" type="m" corresp="#s5:l2">regaert</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Ghy die boven t'goudt <rhyme label="b" type="m" corresp="#s5:l2">vermaert</rhyme></l>
        <l id="s5:l6" met="2" type="+-">Waert, en <rhyme label="c" type="m">zijt</rhyme></l>
        <l id="s5:l7" met="1" type="-+"><rhyme label="d" type="m">Ge-eert</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Mijnen <rhyme label="c" type="m" corresp="#s5:l6">strijt</rhyme></l>
        <l id="s5:l9" met="1" type="+-"><rhyme label="d" type="m" corresp="#s5:l7">Fineert</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Bid' ick hondert <rhyme label="e" type="f">werven</rhyme>:</l>
        <l id="s5:l11" met="1" type="-+">Niet <rhyme label="c" type="m" corresp="#s5:l6">wijt</rhyme></l>
        <l id="s5:l12" met="1" type="-+"><rhyme label="d" type="m" corresp="#s5:l7">Afkeert</rhyme>,</l>
        <l id="s5:l13" met="1" type="-+">Geen <rhyme label="c" type="m" corresp="#s5:l6">tijt</rhyme></l>
        <l id="s5:l14" met="1" type="-+"><rhyme label="d" type="m" corresp="#s5:l7">T'ardeert</rhyme>,</l>
        <l id="s5:l15" met="3" type="+-">Of haest moet ick <rhyme label="e" type="f" corresp="#s5:l10">sterven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Mijn Princesse // <rhyme label="a" type="f">wtgesondert</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ghy zijt een reyn gulden <rhyme label="b" type="m">rijs</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">O Deesse // my <rhyme label="a" type="f" corresp="#s6:l1">verwondert</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Vwer schoonheyt, soo <rhyme label="b" type="m" corresp="#s6:l2">propijs</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Nu dan, beeldeken <rhyme label="b" type="m" corresp="#s6:l2">exquijs</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Tot my <rhyme label="c" type="m">sent</rhyme></l>
        <l id="s6:l7" met="1" type="-+"><rhyme label="d" type="m">Verdrach</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">Mijn <rhyme label="c" type="m" corresp="#s6:l6">torment</rhyme></l>
        <l id="s6:l9" met="1" type="+-">(O <rhyme label="d" type="m" corresp="#s6:l7">wach</rhyme>)</l>
        <l id="s6:l10" met="3" type="+-">Is t'point der <rhyme label="e" type="f">ellenden</rhyme>,</l>
        <l id="s6:l11" met="1" type="-+">Ick <rhyme label="c" type="m" corresp="#s6:l6">kent</rhyme></l>
        <l id="s6:l12" met="1" type="-+">Dien <rhyme label="d" type="m" corresp="#s6:l7">slach</rhyme></l>
        <l id="s6:l13" met="1" type="-+">My <rhyme label="c" type="m" corresp="#s6:l6">schent</rhyme>,</l>
        <l id="s6:l14" met="1" type="-+">Ten <rhyme label="d" type="m" corresp="#s6:l7">mach</rhyme></l>
        <l id="s6:l15" met="3" type="+-">Sonder u niet <rhyme label="e" type="f" corresp="#s6:l10">enden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
