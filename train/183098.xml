<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183098</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat is ons al vreugde gegeven</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Alle druk is nu <rhyme label="a" type="f">verdreeven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wy door Gods gunst 't heil <rhyme label="a" type="f" corresp="#s1:l1">beleven</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wy ontmoeten Willem <rhyme label="b" type="m">bly</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Weer in Hollands Tuin ten <rhyme label="b" type="m" corresp="#s1:l3">rei</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Niemant weet nu weer van <rhyme label="a" type="f">kommer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ider rust nu onder 't <rhyme label="a" type="f" corresp="#s2:l1">lommer</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van den schoone Oranje <rhyme label="b" type="m">Stam</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Waar door 't Land in Vryheid <rhyme label="b" type="m" corresp="#s2:l3">kwam</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Schenk een glaasje dat we eens <rhyme label="a" type="f">drinke</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ja dat wy eens vrolyk <rhyme label="a" type="f" corresp="#s3:l1">klinke</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">'t Gaatje voor, Hoezée, <rhyme label="b" type="m">Hoezée</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Lang leef Willem nog in <rhyme label="b" type="m" corresp="#s3:l3">vreê</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">ô Wie zou niet vrolyk <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">'t komt van zelfs de borst <rhyme label="a" type="f" corresp="#s4:l1">uitdringen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Willems lof en bly <rhyme label="b" type="m">Hoezée</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Roept nu elk in Land en <rhyme label="b" type="m" corresp="#s4:l3">Steê</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ieder draagt Oranje <rhyme label="a" type="f">Strikken</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">'t geen ons weder doet <rhyme label="a" type="f" corresp="#s5:l1">verkwikken</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Of ook een Oranje <rhyme label="b" type="m">Bloem</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">'t Geen wy eerst niet dorsten <rhyme label="b" type="m" corresp="#s5:l3">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Nu is weer de tyd <rhyme label="a" type="f">geschaapen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat wy mogen vreugde <rhyme label="a" type="f" corresp="#s6:l1">raapen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Vreugde past nu onder <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Voor Oranjes <rhyme label="b" type="m" corresp="#s6:l3">waardigheên</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Heden is de tyd <rhyme label="a" type="f">gekomen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Dat men vry, en zonder <rhyme label="a" type="f" corresp="#s7:l1">schroomen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Onze Braave Prins <rhyme label="b" type="m">verjaard</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">'k Wensch hy nog lang word <rhyme label="b" type="m" corresp="#s7:l3">gespaard</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Maar laat ons nog eerst eens <rhyme label="a" type="f">drinken</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Met volle Bookaale <rhyme label="a" type="f" corresp="#s8:l1">klinken</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Ook op welstant van den <rhyme label="b" type="m">Staat</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Die zig met Oranje <rhyme label="b" type="m" corresp="#s8:l3">paart</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Vlegt ter eere <rhyme label="a" type="f">Bloeme-Kranssen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Vol Oranje, nu aan 't <rhyme label="a" type="f" corresp="#s9:l1">dansfen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Maaktze aan de <rhyme label="b" type="m">Eereboog</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dog ter deegen vast om <rhyme label="b" type="m" corresp="#s9:l3">hoog</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Moet men niet verwondert <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">De Oranje kleur <rhyme label="a" type="f" corresp="#s10:l1">gepreezen</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Ziet aan 's Hemels trans zo <rhyme label="b" type="m">hoog</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Mede in den <rhyme label="b" type="m" corresp="#s10:l3">Regenboog</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Wilt nu vrolykheit <rhyme label="a" type="f">betoonen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">En het waarde hoofd <rhyme label="a" type="f" corresp="#s11:l1">bekroonen</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Toont hoe gy de Prins <rhyme label="b" type="m">bemind</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">En gy zyt Oranjes <rhyme label="b" type="m" corresp="#s11:l3">Vrind</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Door 't Goddelyk <rhyme label="a" type="f">Opperweezen</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Ziet men meerder als voor <rhyme label="a" type="f" corresp="#s12:l1">deezen</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">'t Schynt hoe meer men het <rhyme label="b" type="m">verbied</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Hoe meer eer den Vorst <rhyme label="b" type="m" corresp="#s12:l3">geschiedt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
