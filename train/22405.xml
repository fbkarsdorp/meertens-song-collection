    <melody>Vom Himmel hoch da komm ich her</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Jhesu die om al ons <rhyme label="a" type="m">profijt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In armoede gheboren <rhyme label="a" type="m" corresp="#s1:l1">zijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Als ghy int Cribbeken wert <rhyme label="b" type="m">gheleyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Gheen groote diensten hebt ghy <rhyme label="b" type="m" corresp="#s1:l3">verbeyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy hebt u nae die veertich <rhyme label="a" type="m">daghen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">In den Tempel wel laeten <rhyme label="a" type="m" corresp="#s2:l1">draghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer Symeon den ouden <rhyme label="b" type="m">man</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">U nam op syne armen <rhyme label="b" type="m" corresp="#s2:l3">dan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat hy voor langhe ghebeden <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ghy hem met uwen leden <rhyme label="a" type="m" corresp="#s3:l1">gheeft</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dies prijst hy Godt van herten <rhyme label="b" type="m">gront</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat hy leefde ter seluer <rhyme label="b" type="m" corresp="#s3:l3">stont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een Weduwe Anna <rhyme label="a" type="m">ghenaempt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die Prophetinne seer <rhyme label="a" type="m" corresp="#s4:l1">befaempt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die Godes woort vierich <rhyme label="b" type="m">aenhinck</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Quam oock en sprack wonderlijck <rhyme label="b" type="m" corresp="#s4:l3">dinck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Christe u volck wilt helpen <rhyme label="a" type="m">fyn</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dat wy dijns Gheestens kinderen <rhyme label="a" type="m" corresp="#s5:l1">syn</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ons herten ghenadich u <rhyme label="b" type="m">toekeert</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En uwen Gheest, die ons recht <rhyme label="b" type="m" corresp="#s5:l3">leert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Op dat u woort ons altijt <rhyme label="a" type="m">spijst</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Tot uwen Tempele ons hier <rhyme label="a" type="m" corresp="#s6:l1">wijst</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Daermen u prijst, ende lof <rhyme label="b" type="m">secht</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Met bidden, ende dancken <rhyme label="b" type="m" corresp="#s6:l3">recht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Al watmen daer begheert van <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Dat gheeft ghy gheeren, en meer daer <rhyme label="a" type="m" corresp="#s7:l1">by</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Salich is hy die in u <rhyme label="b" type="m">besluyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Eens compt, en niet en valt daer <rhyme label="b" type="m" corresp="#s7:l3">wt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Hier toe helpt ons Christe <rhyme label="a" type="m">altijt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Die voor ons mensche gheboren <rhyme label="a" type="m" corresp="#s8:l1">zijt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dat wy door uwen heylighen <rhyme label="b" type="m">naem</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Gheholpen worden, en syn <rhyme label="b" type="m" corresp="#s8:l3">bequaem</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
