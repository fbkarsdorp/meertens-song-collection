    <melody>O gij die Pallas eert</melody>
  <author>Cornelis Sweerts</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O gy die Pallas <rhyme label="a" type="m">eert</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Door <rhyme label="b" type="f">offerhanden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Laat Venus uw <rhyme label="a" type="m" corresp="#s1:l1">begeert</rhyme></l>
        <l id="s1:l4" met="2" type="-+">In min doen <rhyme label="b" type="f" corresp="#s1:l2">branden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wyl gy de Konst met Schoonheid <rhyme label="c" type="m">paart</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Bekoorlyke Iris, zoet van <rhyme label="c" type="m" corresp="#s1:l5">aart</rhyme>,</l>
      </lg>
    </lg>
  </text>
