    <tags>
      <tag standard-name="soldatenlied" translation="soldier's song">soldatenlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="work">werk</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Rom bom bom zoo slaat de <rhyme label="a" type="m">trom</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Tomboer houw je Stokken <rhyme label="a" type="m" corresp="#s1:l1">krom</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Tamboer houw je Armen <rhyme label="b" type="m">styf</rhyme>,</l>
        <l id="s1:l4" met="6" type="+-">Houd ze zindelyk aan uw lyf. rom bom <rhyme label="a" type="m" corresp="#s1:l1">bom</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Alles zit in 't <rhyme label="c" type="m">Kalfsvel</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Alles hoort naar zyn <rhyme label="c" type="m" corresp="#s1:l5">bevel</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Ieder buigt zich voor zyn <rhyme label="d" type="m">wil</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">En zwygt voor het Kalfsvel stil. rom bom <rhyme label="d" type="m" corresp="#s1:l7">bom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ieder Vader knort en <rhyme label="a" type="m">kyft</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Op zyn Zoon, die koppig <rhyme label="a" type="m" corresp="#s2:l1">blyft</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Maar hoort Jan het <rhyme label="b" type="m">Tromgeluid</rhyme>.</l>
        <l id="s2:l4" met="6" type="+-">Vlug is hy de Herberg uit. rom bom <rhyme label="a" type="m" corresp="#s2:l1">bom</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Rom bom bom zoo slaat de <rhyme label="c" type="m">trom</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Tamboer houw je Stokken <rhyme label="c" type="m" corresp="#s2:l5">krom</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Tamboer houw je Armen <rhyme label="d" type="m">styf</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Houd ze zindelyk aan uw lyf. rom bom <rhyme label="d" type="m" corresp="#s2:l7">bom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Domineé Preekt van de <rhyme label="a" type="m">Stoel</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Jantje blyft toch koud en <rhyme label="a" type="m" corresp="#s3:l1">koel</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Maar hoor Jan de Storm-Marsch <rhyme label="b" type="m">slaan</rhyme>,</l>
        <l id="s3:l4" met="6" type="+-">Dwars door 't Vuur zou Jantje gaan. rom bom <rhyme label="a" type="m" corresp="#s3:l1">bom</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">'T Meisje keek eens door een <rhyme label="c" type="m">Glas</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Keek waar of haar Minnaar <rhyme label="c" type="m" corresp="#s3:l5">was</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Zogt een uit, drie vier op <rhyme label="d" type="m">Straat</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">'S Avonds als de Taptoe slaat. rom bom <rhyme label="d" type="m" corresp="#s3:l7">bom</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Oude Stuurman stok en <rhyme label="a" type="m">styf</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Klopt het Hart nog in het <rhyme label="a" type="m" corresp="#s4:l1">lyf</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En hy Huppelt op de <rhyme label="b" type="m">maat</rhyme>,</l>
        <l id="s4:l4" met="6" type="+-">'S Avonds als de Taptoe slaat, rom bom <rhyme label="a" type="m" corresp="#s4:l1">bom</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Napje riep zyn Garde <rhyme label="c" type="m">laast</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En vond Neêrland van zyn <rhyme label="c" type="m" corresp="#s4:l5">plaats</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">Voor Prins Willem Stryden <rhyme label="d" type="m">wy</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Staan wy pal, of Sterven wy. rom bom <rhyme label="d" type="m" corresp="#s4:l7">bom</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Het Oproer brande, Willem <rhyme label="a" type="m">riep</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Op te Wapens, alles <rhyme label="a" type="m" corresp="#s5:l1">sliep</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ryk en Arm gord Wapens <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s5:l4" met="6" type="+-">Op het Alarm van de Trom. rom bom <rhyme label="a" type="m" corresp="#s5:l1">bom</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Zoo zag men hun in de <rhyme label="c" type="m">Stryd</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Zelfs maar in Tien Dagen <rhyme label="c" type="m" corresp="#s5:l5">tyd</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Riep elk met een Trotsche <rhyme label="d" type="m">Moed</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Broeders vrezen wy geen Bloed. rom bom <rhyme label="d" type="m" corresp="#s5:l7">bom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">In de Marsch, zelfs in de <rhyme label="a" type="m">Stryd</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Het Kalfsvel brengt ons <rhyme label="a" type="m" corresp="#s6:l1">Vrolykheid</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Op Bevel, en <rhyme label="b" type="m">Trommelslag</rhyme>,</l>
        <l id="s6:l4" met="6" type="+-">Komt Neêrlands Volk voor den Dag. rom bom <rhyme label="a" type="m" corresp="#s6:l1">bom</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Neen, wy Stryden niet om <rhyme label="c" type="m">Buit</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Maar voor Ouders, Kind, of <rhyme label="c" type="m" corresp="#s6:l5">Bruid</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Op het slaan van onze <rhyme label="d" type="m">Trom</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Komen wy Langzaam reeds weerom. rom bom <rhyme label="d" type="m" corresp="#s6:l7">bom</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sterven wy op het Veld van <rhyme label="a" type="m">Eer</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Zien wy ons Huislyks drok niet <rhyme label="a" type="m" corresp="#s7:l1">meer</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dan brengt ons het <rhyme label="b" type="m">Kalfsvel</rhyme>,</l>
        <l id="s7:l4" met="6" type="+-">In het Graf, voor laast Vaartwel; rom bom <rhyme label="a" type="m" corresp="#s7:l1">bom</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Als wy eens naar Huis toe <rhyme label="c" type="m">gaan</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">ô! wat zal die Trommel <rhyme label="c" type="m" corresp="#s7:l5">slaan</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Danschen Springen, op de <rhyme label="d" type="m">Trom</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Jongens, komen wy weerom. rom bom <rhyme label="d" type="m" corresp="#s7:l7">bom</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
