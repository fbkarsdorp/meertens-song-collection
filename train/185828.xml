<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185828</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Puer nobis nascitur</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Waer is die dochter van <rhyme label="a" type="m">Zyon</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">Ick soudese so blijde <rhyme label="b" type="f">maken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ick soude haer een bootschap <rhyme label="a" type="m" corresp="#s1:l1">doen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Van alsoo groote <rhyme label="b" type="f" corresp="#s1:l2">saken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Doemen al de Werelt <rhyme label="a" type="m">beschreef</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Doe ginck een Maghet <rhyme label="b" type="f">sware</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Te Bethlehem daer zy doe <rhyme label="a" type="m" corresp="#s2:l1">bleef</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ende ontfinck haer kint al <rhyme label="b" type="f" corresp="#s2:l2">dare</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Aen glas al schijnter <rhyme label="a" type="m">doore</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ten breeckt niet vander <rhyme label="b" type="f">Sonnen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dus heeft die Ionckvrou na een <rhyme label="a" type="m" corresp="#s3:l1">voore</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Ionckvrou, een kint <rhyme label="b" type="f" corresp="#s3:l2">ghewonnen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Aen duysternis is ons <rhyme label="a" type="m">verclaer</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Een licht is ons <rhyme label="b" type="f">geresen</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Een Maghet heeft een kint <rhyme label="a" type="m" corresp="#s4:l1">ghebaert</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Dat dunckt ons wonder <rhyme label="b" type="f" corresp="#s4:l2">wesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Nu is hy teder ende <rhyme label="a" type="m">cranck</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Een Maghet sal hem <rhyme label="b" type="f">voeden</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Wy moghent hem alle wel weten <rhyme label="a" type="m" corresp="#s5:l1">danck</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Van zijnder grooter <rhyme label="b" type="f" corresp="#s5:l2">armoeden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy toondt zijn <rhyme label="a" type="m">goedertierenheyt</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Noyt en was hy <rhyme label="b" type="f">verbolghen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Hy drijft soo grooten <rhyme label="a" type="m" corresp="#s6:l1">ootmoedicheyt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Wy en connen hem niet <rhyme label="b" type="f" corresp="#s6:l2">naervolghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maria nam hem op haren <rhyme label="a" type="m">schoot</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Sy custen aen zijnen <rhyme label="b" type="f">monde</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die minne hadde zy alsoo <rhyme label="a" type="m" corresp="#s7:l1">groot</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Tot hem in alle <rhyme label="b" type="f" corresp="#s7:l2">stonde</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O Heere, lof moet u altoos <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Wilt ons dan <rhyme label="b" type="f">gheleyden</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Met die weerde Moeder <rhyme label="a" type="m" corresp="#s8:l1">dijn</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Als wy alle van hier <rhyme label="b" type="f" corresp="#s8:l2">scheyden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
