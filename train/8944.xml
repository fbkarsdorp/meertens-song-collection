<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8944</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Nun treiben wir den Papst heraus</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VAn HEmel quam der Eng'len <rhyme label="a" type="m">schaer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Verscheen den Herders <rhyme label="a" type="m" corresp="#s1:l1">Openbaer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Sy seyden hun een Kindlijn <rhyme label="b" type="m">t'zaert</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat leydt daer in der Kribben <rhyme label="b" type="m" corresp="#s1:l3">hart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Te Bethlehem in Davids <rhyme label="a" type="m">Stadt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Als Micha heeft vercondicht <rhyme label="a" type="m" corresp="#s2:l1">dat</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Het is die HEEre Jesu <rhyme label="b" type="m">Christ</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die uwer aller Heylandt <rhyme label="b" type="m" corresp="#s2:l3">is</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dies sult ghy bly zijn int <rhyme label="a" type="m">gemeen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Dat Godt met u is worden <rhyme label="a" type="m" corresp="#s3:l1">een</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hy is gheboor'n u Vleesch en <rhyme label="b" type="m">Bloet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">V Broeder is dat eewig' <rhyme label="b" type="m" corresp="#s3:l3">Goet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat can  u doen die Sond' en <rhyme label="a" type="m">Doot</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ghy hebt met u den waren <rhyme label="a" type="m" corresp="#s4:l1">Godt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Laet thoornen Duyvel en die <rhyme label="b" type="m">Hel</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Gods Soon is worden u <rhyme label="b" type="m" corresp="#s4:l3">Ghesel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy wil en kan u laten <rhyme label="a" type="m">nicht</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Set ghy op hem u <rhyme label="a" type="m" corresp="#s5:l1">Toeversicht</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Het moghen u veel vechten <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dien sy Trotz diet niet laten <rhyme label="b" type="m" corresp="#s5:l3">kan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Opt laetst moet ghy doch hebben <rhyme label="a" type="m">recht</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Ghy zijt nu worden Gods <rhyme label="a" type="m" corresp="#s6:l1">gheslecht</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dies Dancket Godt in <rhyme label="b" type="m">Eewicheyt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Gheduldich, vrolick alle <rhyme label="b" type="m" corresp="#s6:l3">tijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
