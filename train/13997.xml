<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13997</id>
    <date>
      <year-start>1566</year-start>
      <year-end>1566</year-end>
    </date>
    <location>
      <orginal-name>emden</orginal-name>
      <standard-name>Emden, Germany</standard-name>
      <coordinates>
        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Es ist das Heil uns kommen her</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WAerom ist Volck oproerich <rhyme label="a" type="m">eenpaer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ouerdenckende ydel <rhyme label="b" type="f">dinghen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Die Princen en Coninghen alle <rhyme label="a" type="m" corresp="#s1:l1">gaer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ouer een haren raatslach <rhyme label="b" type="f" corresp="#s1:l2">bringhen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Om te strijden teghen <rhyme label="c" type="m">sHeerenhandt</rhyme></l>
        <l id="s1:l6" met="4" type="-+">En Christum dien hi heeft <rhyme label="c" type="m" corresp="#s1:l5">ghesandt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Om alle Menschen <rhyme label="d" type="f">hulpe</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sy willen wesen <rhyme label="a" type="m">onghestraft</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En blijuen in boosheyt <rhyme label="b" type="f">steken</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dies spreken sy, niet wel <rhyme label="a" type="m" corresp="#s2:l1">bedacht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Laet ons haer banden <rhyme label="b" type="f" corresp="#s2:l2">breken</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Laet ons verwerpen haer leringhen <rhyme label="c" type="m">al</rhyme></l>
        <l id="s2:l6" met="4" type="-+">En leuen nae ons herten <rhyme label="c" type="m" corresp="#s2:l5">gheual</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En gheensins daervan <rhyme label="d" type="f">wijcken</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maer Godt die in die Hemelen <rhyme label="a" type="m">woont</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die salse noch <rhyme label="b" type="f">belachen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Bespotten oock haer bosen <rhyme label="a" type="m" corresp="#s3:l1">gront</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ende haeren aenslach <rhyme label="b" type="f" corresp="#s3:l2">beschachen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Hy salse met gramschap spreken <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s3:l6" met="4" type="-+">En straffen wat si hebben <rhyme label="c" type="m" corresp="#s3:l5">ghedaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Toornich sal hijse <rhyme label="d" type="f">benauwen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick hebbe v tot een Coninck <rhyme label="a" type="m">ghestelt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ouer Syon dat is mijn <rhyme label="b" type="f">Kercke</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Christum, daer de Schriftuer af <rhyme label="a" type="m" corresp="#s4:l1">melt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die ghy nochtans <rhyme label="b" type="f" corresp="#s4:l2">verwerpe</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ouer al hi recht vercondighen <rhyme label="c" type="m">sal</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Mijnen sin, en wel gheuallen <rhyme label="c" type="m" corresp="#s4:l5">al</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En mijnen wille <rhyme label="d" type="f">leeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy sprack tot my, ghi zijt mijn <rhyme label="a" type="m">Soon</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ick hebbe v huyden <rhyme label="b" type="f">ghebooren</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">En voort ghebracht mijns herten <rhyme label="a" type="m" corresp="#s5:l1">croon</rhyme></l>
        <l id="s5:l4" met="3" type="-+">De Volcken moet ghy <rhyme label="b" type="f" corresp="#s5:l2">vergharen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">De Heydenen ick v schincken <rhyme label="c" type="m">sal</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Tot eender eruen groot <rhyme label="c" type="m" corresp="#s5:l5">ghetal</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Die zuldy alle <rhyme label="d" type="f">regieren</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy zultse met uwen woorde <rhyme label="a" type="m">voen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Haers Vleesch lusten <rhyme label="b" type="f">bederuen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">En mijnen naem haer prijsen <rhyme label="a" type="m" corresp="#s6:l1">doen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">In alle hoecken der <rhyme label="b" type="f" corresp="#s6:l2">aerden</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Ghy sult den weder spannighen <rhyme label="c" type="m">straf</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Breken met een Yseren <rhyme label="c" type="m" corresp="#s6:l5">staf</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En brijselen ghaer als <rhyme label="d" type="f">scheruen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hierom ghi Coninghen daer op <rhyme label="a" type="m">merct</rhyme></l>
        <l id="s7:l2" met="3" type="-+">En ghi Rechters laet v <rhyme label="b" type="f">leeren</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Ende in desen Coninck v <rhyme label="a" type="m" corresp="#s7:l1">verstercken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Sijn Woort houdende in <rhyme label="b" type="f" corresp="#s7:l2">eeren</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Op dat ghi hem leert vresen <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Oock hem betrout en niemant <rhyme label="c" type="m" corresp="#s7:l5">el</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat is Godt recht wel <rhyme label="d" type="f">dienen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Cust doch den Soone <rhyme label="a" type="m">vriendelick</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat hy hem niet en <rhyme label="b" type="f">vergramme</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Dat ghi vanden wech niet <rhyme label="a" type="m" corresp="#s8:l1">eewelick</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Verghaet ontfanckt zijn <rhyme label="b" type="f" corresp="#s8:l2">straffe</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Als zijnen toorn als vier op <rhyme label="c" type="m">ghaet</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Wel hem die alleen op hem <rhyme label="c" type="m" corresp="#s8:l5">staet</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Met vast hoop en <rhyme label="d" type="f">betrouwen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
