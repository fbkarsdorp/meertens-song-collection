      <category translation="narratives">verhalen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlt gy Heeren, Zoetheit <rhyme label="a" type="f">smaken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">En in plaets van Bachus <rhyme label="b" type="m">Vreugd</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">U by 't Jufferschap <rhyme label="a" type="f" corresp="#s1:l1">vermaken</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Zoek u voordeel by dees' <rhyme label="b" type="m" corresp="#s1:l2">Jeugd</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dien ik als de Zang <rhyme label="c" type="f">Godinnen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Ellik geven zal haer <rhyme label="d" type="m">Naem</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Haere Schoonheit waerd te <rhyme label="c" type="f" corresp="#s1:l5">Minnen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Sweeft op Wieken van de <rhyme label="d" type="m" corresp="#s1:l6">Faem</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zie Thalia Vrolyk <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Prys met my haer vluggen <rhyme label="b" type="m">Geest</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Laet Zy uwe Ley Star <rhyme label="a" type="f" corresp="#s2:l1">wezen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Zoek dat gy haer Hart <rhyme label="b" type="m" corresp="#s2:l2">beleest</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Om haer groot Verstand en <rhyme label="c" type="f">Deugden</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Zoekt haer ider tot zyn <rhyme label="d" type="m">helft</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Maer zy schuw van Minne <rhyme label="c" type="f" corresp="#s2:l5">vreugden</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Houd het Vuur in d'As <rhyme label="d" type="m" corresp="#s2:l6">gedelft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Melpomene 't aerdig <rhyme label="a" type="f">Meisje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Van Apol voor lang <rhyme label="b" type="m">Bemind</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Om haer poesel blanke <rhyme label="a" type="f" corresp="#s3:l1">Vleisje</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Een der Schoonste die men <rhyme label="b" type="m" corresp="#s3:l2">vind</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Om haer deugt en Pronk <rhyme label="c" type="f">Cieraden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Minden hy dees' Schoone <rhyme label="d" type="m">Roos</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Om in haere gunst te <rhyme label="c" type="f" corresp="#s3:l5">baden</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Agt hy Nectar nog <rhyme label="d" type="m" corresp="#s3:l6">Ambroos</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kan Uraen uw Minlust <rhyme label="a" type="f">helpen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Neemtze als hulpster van uw <rhyme label="b" type="m">Pyn</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Wil Cupied U <rhyme label="a" type="f" corresp="#s4:l1">overstelpen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Laet haer Oog uw baken <rhyme label="b" type="m" corresp="#s4:l2">zyn</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Wilt gy haere lessen <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Hebt gy in haer byzyn <rhyme label="d" type="m">smaek</rhyme>?</l>
        <l id="s4:l7" met="4" type="+-">Kan haer Schoonheit u <rhyme label="c" type="f" corresp="#s4:l5">bekooren</rhyme>?</l>
        <l id="s4:l8" met="4" type="+-">Kust haer Lely blanke <rhyme label="d" type="m" corresp="#s4:l6">Kaek</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zou Euterpe u best <rhyme label="a" type="f">bekooren</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Schoon niet groot; dog heus van <rhyme label="b" type="m">Aert</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">Tragt u in haer gunst te <rhyme label="a" type="f" corresp="#s5:l1">booren</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Haere Gaven zyn het <rhyme label="b" type="m" corresp="#s5:l2">waerd</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Lieve Lonken, Schitterende <rhyme label="c" type="f">Oogen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Fier van gang als een <rhyme label="d" type="m">Diaen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Was Jupyn nog in <rhyme label="c" type="f" corresp="#s5:l5">vermogen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Hy wierd vast om haer een <rhyme label="d" type="m" corresp="#s5:l6">Zwaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Is Terpsichor√© haer <rhyme label="a" type="f">Zusje</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Meerder uwe Liefde <rhyme label="b" type="m">waerd</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Om haer braef gestalt' of <rhyme label="a" type="f" corresp="#s6:l1">Lusje</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met haer Eer te zyn <rhyme label="b" type="m" corresp="#s6:l2">gepaert</rhyme>?</l>
        <l id="s6:l5" met="4" type="+-">Kan haer Boezem u doen <rhyme label="c" type="f">blaken</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Steelt haer Lonkend Oog uw <rhyme label="d" type="m">Hart</rhyme>;</l>
        <l id="s6:l7" met="4" type="+-">Wilt vry nae haer Schoonheit <rhyme label="c" type="f" corresp="#s6:l5">haken</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Zy verdient uw <rhyme label="d" type="m" corresp="#s6:l6">Minnesmart</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wilt gy anders Clio <rhyme label="a" type="f">Eeren</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Wiens gelaet elks Harte <rhyme label="b" type="m">steelt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Gy kunt Wysheid van haer <rhyme label="a" type="f" corresp="#s7:l1">leeren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Zy schynd van Minerv' <rhyme label="b" type="m" corresp="#s7:l2">geteelt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Al haer Gaven op te <rhyme label="c" type="f">halen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Laet ik voor een groter <rhyme label="d" type="m">Geest</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Want haer Luisterryke <rhyme label="c" type="f" corresp="#s7:l5">Stralen</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Maeken Stem en Pen <rhyme label="d" type="m" corresp="#s7:l6">bedeest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Erato die mag men 't <rhyme label="a" type="f">Knopje</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Noemen van dees' <rhyme label="b" type="m">Rozeboom</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Want ik sweer u by myn <rhyme label="a" type="f" corresp="#s8:l1">Stropje</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Dat ze is van dees' Melk den <rhyme label="b" type="m" corresp="#s8:l2">Room</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Blank van Vel, waer langs de <rhyme label="c" type="f">Rozen</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Paren met de Lely <rhyme label="d" type="m">blaen</rhyme>:</l>
        <l id="s8:l7" met="4" type="+-">Daer de schaemte en het <rhyme label="c" type="f" corresp="#s8:l5">blozen</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Met de Jonkheit spelen <rhyme label="d" type="m" corresp="#s8:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Caliope 't soete <rhyme label="a" type="f">Meisje</rhyme></l>
        <l id="s9:l2" met="4" type="+-">En haer Zuster <rhyme label="b" type="m">Polymny</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Zyn de minste niet van 't <rhyme label="a" type="f" corresp="#s9:l1">Preisje</rhyme>;</l>
        <l id="s9:l4" met="4" type="+-">Maer zy hooren niet hier <rhyme label="b" type="m" corresp="#s9:l2">by</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Schoon zy beide wel voor <rhyme label="c" type="f">dezen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Hoorden by dees' Juffer <rhyme label="d" type="m">schaer</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Diend haer Lof hier niet <rhyme label="c" type="f" corresp="#s9:l5">geprezen</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Wagt haer liever op een <rhyme label="d" type="m" corresp="#s9:l6">aer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
