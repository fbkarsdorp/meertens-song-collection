  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Heer ghedenckt aen Dauidt <rhyme label="a" type="m">gendt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Aen alle syn moeyt, en groot <rhyme label="a" type="m" corresp="#s1:l1">elendt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want hy swoer Godt den <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Hy beloefden Jacobs Godt: dat hy <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">In zijn Huys gaen soud: noch liggen <rhyme label="c" type="m" corresp="#s1:l4">niet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Op zijn Bedde daer <rhyme label="b" type="f" corresp="#s1:l3">neere</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Ick en laet niet slapen de ooghen <rhyme label="d" type="m">mijn</rhyme>/'</l>
        <l id="s1:l8" met="4" type="-+">Noch sluymen mijne Oogschelen <rhyme label="d" type="m" corresp="#s1:l7">fijn</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Tot dat ick toeruste, een <rhyme label="e" type="f">fyne</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Wooninghe des Jacobs Godt en <rhyme label="f" type="m">Heer</rhyme>:</l>
        <l id="s1:l11" met="4" type="+-">Daer aff hooren wy in Ephrata <rhyme label="f" type="m" corresp="#s1:l10">meer</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">En wy vonden die inde <rhyme label="e" type="f" corresp="#s1:l9">Woestijne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy willen in syne wooninghe <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Voor syn voet Schabbel hem bidden <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Heer maeckt u op tot uwer <rhyme label="b" type="f">rusten</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Met de Kiste van uwer macht <rhyme label="c" type="m">bereydt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">sPriesters Cleedt, laet syn <rhyme label="c" type="m" corresp="#s2:l4">rechtuaerdicheydt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Laet u heylighen blijd syn met <rhyme label="b" type="f" corresp="#s2:l3">lusten</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">tRegiment ws Gesalfden, en neemt niet <rhyme label="d" type="m">wech</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Om Dauidts wil, uwen goeden <rhyme label="d" type="m" corresp="#s2:l7">Knecht</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">De Heer heeft den Dauidt <rhyme label="e" type="f">ghesworen</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Eenen Eedt, dien hy oock houden <rhyme label="f" type="m">sal</rhyme>:</l>
        <l id="s2:l11" met="4" type="+-">Op uwen Stoel u Zaedt sitten <rhyme label="f" type="m" corresp="#s2:l10">sal</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Want dat heb ick <rhyme label="e" type="f" corresp="#s2:l9">wtuercoren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Houden u kindren myn Verbondt all <rhyme label="a" type="m">glijck</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">So blijuen sy daer op sitten <rhyme label="a" type="m" corresp="#s3:l1">eewlijck</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Godt heeft Zyon vercoren <rhyme label="b" type="f">euen</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Aldaer sal myne rustplaetze <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Want ick hebber groot behaghen <rhyme label="c" type="m" corresp="#s3:l4">aen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Spijse genoech wil ick hun <rhyme label="b" type="f" corresp="#s3:l3">gheuen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Met salicheyt cleed ick den Priestern <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Hun heylighen die sullen vrolijck <rhyme label="d" type="m" corresp="#s3:l7">zijn</rhyme>:</l>
        <l id="s3:l9" met="3" type="-+">Dauidts Horen aldaer opgaet <rhyme label="e" type="f">schoone</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Mijnen Ghesalfden is een Lantern <rhyme label="f" type="m">bereedt</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Sijn vyanden die worden met schanden <rhyme label="f" type="m" corresp="#s3:l10">becleedt</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Maer ouer hem so bloeyt syn <rhyme label="e" type="f" corresp="#s3:l9">Croone</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
