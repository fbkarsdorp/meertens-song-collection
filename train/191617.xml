<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191617</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hoor hoe aardig dat ik voer</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ROemwaardige <rhyme label="a" type="m">Rozelyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Pronk-Juweel van alle <rhyme label="b" type="f">Vrouwe</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Oorzaak van myn <rhyme label="a" type="m" corresp="#s1:l1">Minne-pyn</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Ik kan my niet meer <rhyme label="b" type="f" corresp="#s1:l2">onthouwen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Want door u <rhyme label="c" type="m">bekoorlykheid</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Ben ik myne Vryheid <rhyme label="c" type="m" corresp="#s1:l5">quit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Uwe oogen als een <rhyme label="a" type="m">git</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Doen de koudste boezem <rhyme label="b" type="f">blaken</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En haar voorhooft hagel <rhyme label="a" type="m" corresp="#s2:l1">wit</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Kont my opgetogen <rhyme label="b" type="f" corresp="#s2:l2">maken</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Uwe kaakjes Roze <rhyme label="c" type="m">rood</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Maaken my al levend <rhyme label="c" type="m" corresp="#s2:l5">dood</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Uw propre vergode <rhyme label="a" type="m">mond</rhyme></l>
        <l id="s3:l2" met="4" type="+-">En uw zuyker zoete <rhyme label="b" type="f">lipjes</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hebben myn jong hert <rhyme label="a" type="m" corresp="#s3:l1">gewond</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ja die lieve <rhyme label="b" type="f" corresp="#s3:l2">Minne-Klipjes</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Doen door zugten en <rhyme label="c" type="m">getraan</rhyme></l>
        <l id="s3:l6" met="4" type="+-">My gelyk het Sneeuw <rhyme label="c" type="m" corresp="#s3:l5">vergaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Uw halsje als een <rhyme label="a" type="m">Paarlemoer</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Blank als Lelyen uw <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Tanden als een <rhyme label="a" type="m" corresp="#s4:l1">Paarlen-snoer</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Ach waar zou ik toch <rhyme label="b" type="f" corresp="#s4:l2">belanden</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Zo 'k zou noemen al uw <rhyme label="c" type="m">schoon</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Want gy spand van al de <rhyme label="c" type="m" corresp="#s4:l5">Kroon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">In uw weezen ziet men <rhyme label="a" type="m">staan</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Alle de <rhyme label="b" type="f">bekoorlykheden</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Venus, Pallas nog <rhyme label="a" type="m" corresp="#s5:l1">Diaan</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Komen by uw schoone <rhyme label="b" type="f" corresp="#s5:l2">Leden</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Juno by uwe luyster <rhyme label="c" type="m">pronk</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Paphos by uw lief <rhyme label="c" type="m" corresp="#s5:l5">gelonk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wie zou u, ô schoon <rhyme label="a" type="m">Godin</rhyme>!</l>
        <l id="s6:l2" met="4" type="+-">Konnen zien en niet <rhyme label="b" type="f">beminnen</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Niemant is zo koel van <rhyme label="a" type="m" corresp="#s6:l1">Min</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Die gebruyk heeft van zyn <rhyme label="b" type="f" corresp="#s6:l2">zinnen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Dat hy zien kan uw <rhyme label="c" type="m">Albast</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En zyn hert niet raakt <rhyme label="c" type="m" corresp="#s6:l5">verrast</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Mag ik uw vergoode <rhyme label="a" type="m">Leen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Nad'ren en met eerbied <rhyme label="b" type="f">Kussen</rhyme>?</l>
        <l id="s7:l3" met="4" type="+-">En met eerb'ere <rhyme label="a" type="m" corresp="#s7:l1">zoetigheen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Myn ontfonkte vonken <rhyme label="b" type="f" corresp="#s7:l2">blussen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Want Cupido heeft felle <rhyme label="c" type="m">brand</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Gesticht in myn <rhyme label="c" type="m" corresp="#s7:l5">Ingewand</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">En als u dit niet <rhyme label="a" type="m">behaagt</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Dan bid ik toond <rhyme label="b" type="f">meededogen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Sluyt, ey sluyt, ô zoete <rhyme label="a" type="m" corresp="#s8:l1">Maagt</rhyme>!</l>
        <l id="s8:l4" met="4" type="+-">Uw betoverende <rhyme label="b" type="f" corresp="#s8:l2">oogen</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Want die heldre <rhyme label="c" type="m">Blixem-straal</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Quetst meer dan 't vernielend <rhyme label="c" type="m" corresp="#s8:l5">Staal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
