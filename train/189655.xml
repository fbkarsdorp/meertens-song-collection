  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MY dunkt ik zie myn <rhyme label="a" type="f">Engel</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Al nadren aan deez' <rhyme label="b" type="m">bron</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ai Lief! waar naar ik <rhyme label="a" type="f" corresp="#s1:l1">hengel</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vertoeft niet wyl de <rhyme label="b" type="m" corresp="#s1:l2">zon</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Myn brand noch felder <rhyme label="c" type="m">sticht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Of kan vrouw Venus <rhyme label="c" type="m" corresp="#s1:l5">wicht</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">U tot <rhyme label="d" type="f">minnen</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Niet <rhyme label="d" type="f" corresp="#s1:l7">verwinnen</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Door zyn <rhyme label="c" type="m" corresp="#s1:l5">schigt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ai! komt dan <rhyme label="a" type="f">Leonore</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En lest myn heevge <rhyme label="b" type="m">brant</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Laat af, Laat af uw <rhyme label="a" type="f" corresp="#s2:l1">tooren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ã´ Schoonste Nimf van 't <rhyme label="b" type="m" corresp="#s2:l2">land</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Laat ik myn minne <rhyme label="c" type="m">wondt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Aan uw nektar <rhyme label="c" type="m" corresp="#s2:l5">mondt</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Door veel <rhyme label="d" type="f">kussen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Eensjes <rhyme label="d" type="f" corresp="#s2:l7">blussen</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Op dees <rhyme label="c" type="m" corresp="#s2:l5">stont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zoo zyn dan al myn <rhyme label="a" type="f">zuchten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En klagen, al om <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wyl gy voor my gaat <rhyme label="a" type="f" corresp="#s3:l1">vluchten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ik sterf dan in <rhyme label="b" type="m" corresp="#s3:l2">verdriet</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Maar neen de <rhyme label="c" type="m">schikgodin</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Heeft my haar <rhyme label="c" type="m" corresp="#s3:l5">wedermin</rhyme></l>
        <l id="s3:l7" met="2" type="+-">Vast <rhyme label="d" type="f">gesworen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">En <rhyme label="d" type="f" corresp="#s3:l7">verkoren</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Na haar <rhyme label="c" type="m" corresp="#s3:l5">zin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
