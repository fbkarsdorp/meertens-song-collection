  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Onze M..... tot <rhyme label="a" type="m">Abcou'</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Let eens op die rare <rhyme label="b" type="f">kuren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Kreeg zyn J..... pas tot <rhyme label="a" type="m" corresp="#s1:l1">vrouw</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s1:l2">Turelure</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Of zy wouw haar piek gaan <rhyme label="b" type="f" corresp="#s1:l2">schuren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s1:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Moest die gryze <rhyme label="a" type="m">schuddebol</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Juist een jonge dienstmaagd <rhyme label="b" type="f">huren</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Moest hy, door de min op <rhyme label="a" type="m" corresp="#s2:l1">hol</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s2:l2">Turelure</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Op haar weefgetouw <rhyme label="b" type="f" corresp="#s2:l2">borduren</rhyme>?</l>
        <l id="s2:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s2:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Docht hy dat zyn <rhyme label="a" type="m">meesterknegt</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Zouw zo gek zyn als zyn <rhyme label="b" type="f">buren</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Neen, die borst is veel te <rhyme label="a" type="m" corresp="#s3:l1">oprecht</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s3:l2">Turelure</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Vry van geile <rhyme label="b" type="f" corresp="#s3:l2">minnevuren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s3:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Houw je geld maar <rhyme label="a" type="m">bestemaat</rhyme>;</l>
        <l id="s4:l2" met="4" type="+-">Want de mof wil 't niet <rhyme label="b" type="f">begluren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wyl hy Koe en Kalf <rhyme label="a" type="m" corresp="#s4:l1">verlaat</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s4:l2">Turelure</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En jouw mooi laat zitten <rhyme label="b" type="f" corresp="#s4:l2">turen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s4:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Mofje toond <rhyme label="a" type="m">Notariëel</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dat hy uit de <rhyme label="b" type="f">Confituren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Die zyn baas vond schoon en <rhyme label="a" type="m" corresp="#s5:l1">eêl</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s5:l2">Turelure</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Nimmer zoet zocht uit te <rhyme label="b" type="f" corresp="#s5:l2">puren</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s5:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Stel jy 't meisje maar <rhyme label="a" type="m">gerust</rhyme>;</l>
        <l id="s6:l2" met="4" type="+-">Wilt terstond naar Loenen <rhyme label="b" type="f">sturen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Opdat zy u niet meer <rhyme label="a" type="m" corresp="#s6:l1">brust</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s6:l2">Turelure</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Koop haar fytels, luiërs <rhyme label="b" type="f" corresp="#s6:l2">luren</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s6:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Nu de jonge vrouw <rhyme label="a" type="m">getroost</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Die vast tusschen twee paar <rhyme label="b" type="f">muren</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Zucht en droeve tranen <rhyme label="a" type="m" corresp="#s7:l1">loost</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s7:l2">Turelure</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Over haar gemaal's <rhyme label="b" type="f" corresp="#s7:l2">figuren</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s7:l2">lure</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Volg jy maar goê raad, myn <rhyme label="a" type="m">vrind</rhyme>.</l>
        <l id="s8:l2" met="4" type="+-">Houw je buiten <rhyme label="b" type="f">Proçeduren</rhyme>;</l>
        <l id="s8:l3" met="4" type="+-">Denk die meisjes maakt met <rhyme label="a" type="m" corresp="#s8:l1">kind</rhyme>,</l>
        <l id="s8:l4" met="2" type="+-"><rhyme label="b" type="f" corresp="#s8:l2">Turelure</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Moet het in 't gemeen <rhyme label="b" type="f" corresp="#s8:l2">bezuren</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Robin ture lure <rhyme label="b" type="f" corresp="#s8:l2">lure</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
