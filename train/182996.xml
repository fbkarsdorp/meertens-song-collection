<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182996</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hoe zoet is het daar de vriendschap woont</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ô Vyftal alwaar ge u <rhyme label="a" type="m">bevind</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In Stad of Vorstelyk <rhyme label="b" type="m">Loo</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">De Vriendschap is 't die u <rhyme label="a" type="m" corresp="#s1:l1">bemind</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ja, eeuwig blyft het <rhyme label="b" type="m" corresp="#s1:l2">zoo</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Verborgen of in 't <rhyme label="c" type="m">openbaar</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Mint 't grooste deel van Neêrlands <rhyme label="c" type="m" corresp="#s1:l5">schaar</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Den Vorst, den Vorst, den <rhyme label="d" type="m">Vorst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat staaft de Vriendschap, ja ons <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Gloeid van Oranje <rhyme label="b" type="m">Men</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Geen Draak hoe of hy blaast of <rhyme label="a" type="m" corresp="#s2:l1">sart</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dryft die uit onze <rhyme label="b" type="m" corresp="#s2:l2">zin</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Veel meer groeit nog de Vriendschap <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Tot spyt van die ze tegen <rhyme label="c" type="m" corresp="#s2:l5">staan</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Hoezée! Hoezée! <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ja, Dierb're Vorst voor 't <rhyme label="a" type="m">Vaderland</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Of schoon gy word <rhyme label="b" type="m">gezard</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Door Onweetende <rhyme label="a" type="m" corresp="#s3:l1">aangerand</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">God steunt u in deez' <rhyme label="b" type="m" corresp="#s3:l2">smart</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Blyf in u hoope steeds <rhyme label="c" type="m">voortgaan</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En neemt het in geduld tog <rhyme label="c" type="m" corresp="#s3:l5">aan</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Gestaag, Gestaag, <rhyme label="d" type="m">Gestaag</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wy bieden u beminde <rhyme label="a" type="m">Vorst</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Onz' Vriendschap heeden <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Schoon helsch fenyn naar wraake <rhyme label="a" type="m" corresp="#s4:l1">dorst</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Houd moet en blyft steeds <rhyme label="b" type="m" corresp="#s4:l2">staan</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Wagt op des Hemels <rhyme label="c" type="m">Albesluit</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Die zal 't ten goede werken <rhyme label="c" type="m" corresp="#s4:l5">uit</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Door tyd! door tyd! door <rhyme label="d" type="m">tyd</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Vriendschap die ons hart <rhyme label="a" type="m">bewoont</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Voor all' die de uwen <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die lyd niet dat gy word <rhyme label="a" type="m" corresp="#s5:l1">gehoont</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van eenig helsch <rhyme label="b" type="m" corresp="#s5:l2">fenyn</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Zy noemen zig een <rhyme label="c" type="m">Vryheids-Zoon</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">En staan uw Erfregt na de <rhyme label="c" type="m" corresp="#s5:l5">kroon</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Te snood ... ja al ... te <rhyme label="d" type="m">snood</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Verdrukte Vorst van <rhyme label="a" type="m">Nederland</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Blyft gy ons <rhyme label="b" type="m">Palinuur</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Vriendschap rykt u hart en <rhyme label="a" type="m" corresp="#s6:l1">hand</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Voor eeuwig op den <rhyme label="b" type="m" corresp="#s6:l2">duur</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Kom aan, Kom aan, spreekt voor u <rhyme label="c" type="m">regt</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Gelooft ons-hart dat is <rhyme label="c" type="m" corresp="#s6:l5">opregt</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Hoezée! Hoezée! <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
