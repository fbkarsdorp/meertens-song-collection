<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185852</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O rad van avontuur</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Die waerheyt <rhyme label="a" type="f">wtvercoren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die heeft mijn Ziel <rhyme label="b" type="m">begeert</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die door de sondt <rhyme label="a" type="f" corresp="#s1:l1">verloren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Veer van mijn was <rhyme label="b" type="m" corresp="#s1:l2">gekeert</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Al buyten mijn <rhyme label="c" type="f">begeeren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Door den Adam <rhyme label="d" type="m">gedaen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Hierom soeck ick te <rhyme label="c" type="f" corresp="#s1:l5">leeren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Hoe ick sal weder <rhyme label="c" type="f" corresp="#s1:l5">keeren</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Int bidden God <rhyme label="d" type="m" corresp="#s1:l6">aenghenaem</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O heere, leert mijn <rhyme label="a" type="f">bidden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat is toch, v, <rhyme label="b" type="m">gebodt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die ons hier zijt int <rhyme label="a" type="f" corresp="#s2:l1">midden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Mensch en warachtich <rhyme label="b" type="m" corresp="#s2:l2">God</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Die in v naem <rhyme label="c" type="f">vergaren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sijn hier <rhyme label="d" type="m">ghebenedijt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Verlicht Heer v <rhyme label="c" type="f" corresp="#s2:l5">dienaren</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Gheeft v woort te <rhyme label="c" type="f" corresp="#s2:l5">verclaren</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Mijn Ziel verheught, <rhyme label="d" type="m" corresp="#s2:l6">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vader, oorsprong en <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ons, door, v, woort <rhyme label="b" type="m">verclaert</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ghy, die goet wort <rhyme label="a" type="f" corresp="#s3:l1">gepresen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sijt, eenen God van <rhyme label="b" type="m" corresp="#s3:l2">aert</rhyme></l>
        <l id="s3:l5" met="3" type="-+">In eeuwicheyt <rhyme label="c" type="f">verheven</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die, ons altijdt <rhyme label="d" type="m">bemindt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hemelen, zijn oock <rhyme label="c" type="f" corresp="#s3:l5">desen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Al die v name <rhyme label="c" type="f" corresp="#s3:l5">vresen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">En worden als een <rhyme label="d" type="m" corresp="#s3:l6">kint</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gheheylicht, door <rhyme label="a" type="f">t’herboren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sy, die nu heylich <rhyme label="b" type="m">leeft</rhyme></l>
        <l id="s4:l3" met="3" type="-+">V, kinders <rhyme label="a" type="f" corresp="#s4:l1">wtvercoren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Naem, lof en glory <rhyme label="b" type="m" corresp="#s4:l2">gheeft</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Laet comen, dat ghy <rhyme label="c" type="f">loofden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">V rijck, al in mijn <rhyme label="d" type="m">gheest</rhyme></l>
        <l id="s4:l7" met="3" type="-+">V wil gheschiet hier <rhyme label="c" type="f" corresp="#s4:l5">boven</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Opter aerden, oock <rhyme label="c" type="f" corresp="#s4:l5">loven</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Als inden Hemel, <rhyme label="d" type="m" corresp="#s4:l6">meest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gheest, der Enghelen <rhyme label="a" type="f">spijse</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ons <rhyme label="b" type="m">overvloedicheyt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Huyden, ghy comt <rhyme label="a" type="f" corresp="#s5:l1">bewijsen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ons, liefd’ en <rhyme label="b" type="m" corresp="#s5:l2">Salicheyt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Daghelicx, doet ghy <rhyme label="c" type="f">gheven</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Broot, vanden Hemel <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Vol wel-lusticheyt <rhyme label="c" type="f" corresp="#s5:l5">verheven</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In soeticheyt <rhyme label="c" type="f" corresp="#s5:l5">t’aencleven</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Tot aendacht int <rhyme label="d" type="m" corresp="#s5:l6">ghemoet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">En vergheeft, Heer onse <rhyme label="a" type="f">sonden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ons, boosheyt voor ons <rhyme label="b" type="m">misdaen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Onse, quaet tot allen <rhyme label="a" type="f" corresp="#s6:l1">stonden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Schulden, t’ghemoet comt <rhyme label="b" type="m" corresp="#s6:l2">slaen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ghelijck, ons comt <rhyme label="c" type="f">bestrijen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Wy, die in sondt gheboren <rhyme label="d" type="m">zijn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Vergheven, ons door u <rhyme label="c" type="f" corresp="#s6:l5">lijen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Onse, naesten aen alle <rhyme label="c" type="f" corresp="#s6:l5">zijen</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Schuldenaers, voor u <rhyme label="d" type="m" corresp="#s6:l6">aenschijn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">En leyt, toch in u <rhyme label="a" type="f">weghen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ons, die zijn af <rhyme label="b" type="m">gedwaelt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Niet, sonder u <rhyme label="a" type="f" corresp="#s7:l1">verkreghen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">In, ons u Gheest doch <rhyme label="b" type="m" corresp="#s7:l2">straelt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Becoringh, comt nae <rhyme label="c" type="f">Iaghen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Maer, Heere ghy moest ons <rhyme label="d" type="m">goedt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Verlost, nae u <rhyme label="c" type="f" corresp="#s7:l5">behaghen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Ons, gheeft verduldich te <rhyme label="c" type="f" corresp="#s7:l5">draghen</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Van t’quaet, door, v heylich <rhyme label="d" type="m" corresp="#s7:l6">bloet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Amen, dat moet soo <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Prins dat ghy my <rhyme label="b" type="m">verlost</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Wt al dat quaet <rhyme label="a" type="f" corresp="#s8:l1">gheresen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Daer ick was <rhyme label="b" type="m" corresp="#s8:l2">inghebrocht</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Al door dat <rhyme label="c" type="f">ingheboren</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In s’moeders lichaem <rhyme label="d" type="m">ziet</rhyme></l>
        <l id="s8:l7" met="3" type="-+">O Heer wilt, my <rhyme label="c" type="f" corresp="#s8:l5">verhooren</rhyme></l>
        <l id="s8:l8" met="3" type="-+">In uwen zoone <rhyme label="c" type="f" corresp="#s8:l5">vercoren</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Die hem in ons <rhyme label="d" type="m" corresp="#s8:l6">wtghiet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
