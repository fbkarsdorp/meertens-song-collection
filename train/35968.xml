      <year-start>1674</year-start>
      <year-end>1674</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
    </tags>
    <melody>Las qui hâtera le temps</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Herman Harts</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AH! uwe Naem is soo <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Hooghste <rhyme label="a" type="m" corresp="#s1:l1">Goet</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Dat myn herte <rhyme label="b" type="m">breckt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als myn tonge <rhyme label="b" type="m" corresp="#s1:l3">spreckt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s1:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ey! segh my waer syd' hier <rhyme label="a" type="m">t'huys</rhyme>?</l>
        <l id="s2:l2" met="2" type="+-">Aen het <rhyme label="a" type="m" corresp="#s2:l1">Cruys</rhyme>.</l>
        <l id="s2:l3" met="3" type="+-">Ick wil dan oock <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Om met u te <rhyme label="b" type="m" corresp="#s2:l3">syn</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s2:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Oft ick waer geweest dat <rhyme label="a" type="m">punt</rhyme></l>
        <l id="s3:l2" met="2" type="+-">VVel <rhyme label="a" type="m" corresp="#s3:l1">gemunt</rhyme>!</l>
        <l id="s3:l3" met="3" type="+-">Oft ick waer <rhyme label="b" type="m">geweest</rhyme></l>
        <l id="s3:l4" met="3" type="+-">In die Cruyce <rhyme label="b" type="m" corresp="#s3:l3">feest</rhyme>!</l>
        <l id="s3:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s3:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Het punt, segh ick van de <rhyme label="a" type="m">Lans</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">VVelck ons <rhyme label="a" type="m" corresp="#s4:l1">gans</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Opende u <rhyme label="b" type="m">zy</rhyme>:</l>
        <l id="s4:l4" met="3" type="+-">Ick waer noch in <rhyme label="b" type="m" corresp="#s4:l3">dy</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s4:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">VVaerom heeft een yser <rhyme label="a" type="m">stuck</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Dat <rhyme label="a" type="m" corresp="#s5:l1">geluck</rhyme>?</l>
        <l id="s5:l3" met="3" type="+-">Het ghen' niet en <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Noch verstandt en <rhyme label="b" type="m" corresp="#s5:l3">heeft</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s5:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">O Gebenedyde <rhyme label="a" type="m">vrucht</rhyme>!</l>
        <l id="s6:l2" met="2" type="+-">Vol <rhyme label="a" type="m" corresp="#s6:l1">genucht</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Van de Moeder <rhyme label="b" type="m">Maeght</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Die my wel <rhyme label="b" type="m" corresp="#s6:l3">behaeght</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s6:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Gy brenght ons in 't Hyligh <rhyme label="a" type="m">Landt</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Door u <rhyme label="a" type="m" corresp="#s7:l1">handt</rhyme>:</l>
        <l id="s7:l3" met="3" type="+-">VVant toch uwe <rhyme label="b" type="m">Naem</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Hie toe is <rhyme label="b" type="m" corresp="#s7:l3">bequaem</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s7:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Wat Moyses, oft <rhyme label="a" type="m">Aaron</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Niet en <rhyme label="a" type="m" corresp="#s8:l1">kon'</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Mocht den vromen <rhyme label="b" type="m">Helt</rhyme>.</l>
        <l id="s8:l4" met="3" type="+-">Die Jesus <rhyme label="b" type="m" corresp="#s8:l3">afbelt</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s8:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">O! Naem boven alle <rhyme label="a" type="m">Naem</rhyme>!</l>
        <l id="s9:l2" met="2" type="+-">VViens <rhyme label="a" type="m" corresp="#s9:l1">faem</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Hemel, Aertrijck, <rhyme label="b" type="m">Hel</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Gewaer worden <rhyme label="b" type="m" corresp="#s9:l3">wel</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Jesus ô! mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s9:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Nu dan schuyf'et voil, of <rhyme label="a" type="m">doeck</rhyme>;</l>
        <l id="s10:l2" met="2" type="+-">Ick <rhyme label="a" type="m" corresp="#s10:l1">versoeck</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Die <rhyme label="b" type="m">presentië</rhyme></l>
        <l id="s10:l4" met="3" type="+-">In de <rhyme label="b" type="m" corresp="#s10:l3">Glorië</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Jesus ô mynen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Jesus u liefde, en niet <rhyme label="c" type="m" corresp="#s10:l5">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
