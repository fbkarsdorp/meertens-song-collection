  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HEer, wees ons na <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">De zielen, die <rhyme label="a" type="m" corresp="#s1:l1">Gy</rhyme></l>
        <l id="s1:l3" met="3" type="+-">In liefde <rhyme label="b" type="m">verbindt</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Zyn hier. Daarom zegen ons, Broeder en <rhyme label="b" type="m" corresp="#s1:l3">Vrind</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wy waaren gaarn <rhyme label="a" type="m">rein</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">En in ons regt <rhyme label="a" type="m" corresp="#s2:l1">klein</rhyme>.</l>
        <l id="s2:l3" met="3" type="+-">Komt vlammen en <rhyme label="b" type="m">wind</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Verteert, al wat zich nog van d'Eigenheit <rhyme label="b" type="m" corresp="#s2:l3">vind</rhyme>.</l>
      </lg>
    </lg>
  </text>
