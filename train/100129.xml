  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Hoe ongelukkig is hy die minne <rhyme label="a" type="m">draagt</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Tot zulken die hy t'eenemaal <rhyme label="a" type="m" corresp="#s1:l1">mishaagt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die altyd haakt Na die hem <rhyme label="b" type="m">laakt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Hou ik, dat AEtnaas vlammen <rhyme label="b" type="m" corresp="#s1:l3">smaakt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En in hem voeld een helsche <rhyme label="c" type="m">gloet</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Verzeng van aders, zien van <rhyme label="c" type="m" corresp="#s1:l5">bloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
