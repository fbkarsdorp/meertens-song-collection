<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157469</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Te mei als alle vogels zingen</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">JOnathan klom de steylte <rhyme label="a" type="m">op</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Van eenen Klip, op welckers <rhyme label="a" type="m" corresp="#s1:l1">top</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat sijn vyanden <rhyme label="b" type="f">lagen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En heeft haer heyr // met sijn <rhyme label="c" type="m">geweer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Kloeckmoedighlijck <rhyme label="b" type="f" corresp="#s1:l3">verslaghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En doe nu dese dapp'ren <rhyme label="a" type="m">Heldt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Den vyandt had ter neer <rhyme label="a" type="m" corresp="#s2:l1">geveldt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En moede was van 't <rhyme label="b" type="f">slaghten</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Soo is sijn Gheest // soo mat <rhyme label="c" type="m">geweest</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat hy daght te <rhyme label="b" type="f" corresp="#s2:l3">versmachten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maer Godt, die op sijn kind'ren <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Verliet Jonathan hier in <rhyme label="a" type="m" corresp="#s3:l1">niet</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Want doe hy scheen <rhyme label="b" type="f">verlaten</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Verquickt de Heer // hem haestigh <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Met leck're <rhyme label="b" type="f" corresp="#s3:l3">honighraten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dit is de stant der kind'ren <rhyme label="a" type="m">Godts</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Voor wien dat leyt een steyle <rhyme label="a" type="m" corresp="#s4:l1">Rots</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gelijck als voor <rhyme label="b" type="f">Jonathan</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Vraeght ghy na dees? het is het <rhyme label="c" type="m">vlees</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Een Schantse voor den <rhyme label="b" type="f" corresp="#s4:l3">Sathan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De welcke sy met hoogen <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Vertrappen moeten onder <rhyme label="a" type="m" corresp="#s5:l1">voet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Tot vlught van den <rhyme label="b" type="f">besitter</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Wel wreet en sterck // 'twelck is een <rhyme label="c" type="m">werck</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Voor haer seer suur en <rhyme label="b" type="f" corresp="#s5:l3">bitter</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maer eer Godts volck hier in <rhyme label="a" type="m">versmacht</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Toont Godt sijn goedigheyt, en <rhyme label="a" type="m" corresp="#s6:l1">macht</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Verquickt haer met <rhyme label="b" type="f">genaden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Honigh, en Wijn // sal voor haer <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Haer ziele te <rhyme label="b" type="f" corresp="#s6:l3">versaden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Eerst gheeft hy vrughten van sijn <rhyme label="a" type="m">Gheest</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Doch namaels in het <rhyme label="a" type="m" corresp="#s7:l1">Hemels-Feest</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wordt d' Eeren-kroon <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Voor haer bereydt // en <rhyme label="c" type="m">wegh-geleydt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">In 't eeuwigh saligh <rhyme label="b" type="f" corresp="#s7:l3">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
