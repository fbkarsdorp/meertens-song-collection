<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178380</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Prins Karel</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Prins Willem Fredrik <rhyme label="a" type="m">viert</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zyn elfden Jaardag <rhyme label="b" type="f">heden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">'t Is vreugd in Neerlands <rhyme label="b" type="f" corresp="#s1:l2">Steeden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">'t Oranje Vaandel <rhyme label="a" type="m" corresp="#s1:l1">zwiert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">'s Lands Erfprins hoog <rhyme label="c" type="f">Verjaaren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Geeft ieder lust en <rhyme label="d" type="m">moed</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hoe klimmen reeds uw <rhyme label="c" type="f" corresp="#s1:l5">Jaaren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Prins, uit 't Oranje <rhyme label="d" type="m" corresp="#s1:l6">Bloed</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Oranjes Erfprins <rhyme label="a" type="m">groei</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die hoop der <rhyme label="b" type="f">Nederlanders</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En vrygevogte <rhyme label="b" type="f" corresp="#s2:l2">Standers</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat hy steeds meerder <rhyme label="a" type="m" corresp="#s2:l1">bloei</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Wasch op in alle <rhyme label="c" type="f">wysheid</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En overwin al <rhyme label="d" type="m">vroeg</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Daar in ook zelfs de <rhyme label="c" type="f" corresp="#s2:l5">grysheid</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat in de Troon best <rhyme label="d" type="m" corresp="#s2:l6">voeg</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wasch op in <rhyme label="a" type="m">dapperheid</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">De Oranjes waren <rhyme label="b" type="f">Helden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wier lof men mag <rhyme label="b" type="f" corresp="#s3:l2">vermelden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met tevens hun <rhyme label="a" type="m" corresp="#s3:l1">beleidt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Zy zyn de braafste <rhyme label="c" type="f">lieden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die NeÃªrland ooit nog <rhyme label="d" type="m">had</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">De dwingelandy moest <rhyme label="c" type="f" corresp="#s3:l5">Vlieden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Waar 't land toen onder <rhyme label="d" type="m" corresp="#s3:l6">zat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gy zyt uit Pruisisch <rhyme label="a" type="m">bloed</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En Nassouws <rhyme label="b" type="f">voortgesprooten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Aan onze <rhyme label="b" type="f" corresp="#s4:l2">Landgenooten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Geeft dit een nievwen <rhyme label="a" type="m" corresp="#s4:l1">moep</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Elk moet Oranje <rhyme label="c" type="f">pryzen</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">Dat Huis gaf ons <rhyme label="d" type="m">altyd</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van zyne deugd <rhyme label="c" type="f" corresp="#s4:l5">bewyzen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Elk is met regt <rhyme label="d" type="m" corresp="#s4:l6">verblyd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Onze Erf-Prins, dit geeft <rhyme label="a" type="m">vreugd</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Is heden elf <rhyme label="b" type="f">Jaaren</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Laat zang en spel zig <rhyme label="b" type="f" corresp="#s5:l2">paaren</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Hy is een Prins vol <rhyme label="a" type="m" corresp="#s5:l1">deugd</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Nu eens in 't rond <rhyme label="c" type="f">geschonken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Voor Willem <rhyme label="d" type="m">Frederik</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En vrolyk <rhyme label="c" type="f" corresp="#s5:l5">uitgedronken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Als in een <rhyme label="d" type="m" corresp="#s5:l6">ogenblik</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Oranje vaar steeds <rhyme label="a" type="m">wel</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Lang moet ons Erf Prins <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En altyd zyn <rhyme label="b" type="f" corresp="#s6:l2">verheven</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met Zang en <rhyme label="a" type="m" corresp="#s6:l1">Snarenspel</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Paart samen uw <rhyme label="c" type="f">gebeeden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Al wie Oranje <rhyme label="d" type="m">mint</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Gy hebt nu Juichens <rhyme label="c" type="f" corresp="#s6:l5">reden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Zyn twaalfde Jaar <rhyme label="d" type="m" corresp="#s6:l6">begint</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Viva lang leef de <rhyme label="a" type="m">Prins</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">Laat ons te samen <rhyme label="b" type="f">zingen</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Verheugt U <rhyme label="b" type="f" corresp="#s7:l2">Stedelingen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Zyt gy iets goeds van <rhyme label="a" type="m" corresp="#s7:l1">zins</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Dat kunt gy heden <rhyme label="c" type="f">toonen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Onze Erf Prins is het <rhyme label="d" type="m">waard</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Vlegt hem geboorte <rhyme label="c" type="f" corresp="#s7:l5">Kroonen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Dees dag ons blydschap <rhyme label="d" type="m" corresp="#s7:l6">baard</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dat lang ons Erf Prins <rhyme label="a" type="m">leeft</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">Hy koom' tot hooge <rhyme label="b" type="f">Jaaren</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat hy der <rhyme label="b" type="f" corresp="#s8:l2">Burgerscharen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Veel lof tot vreugde <rhyme label="a" type="m" corresp="#s8:l1">geeft</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Hy moog gezond zyn <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Afleven en word <rhyme label="d" type="m">oud</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Hy blyve ons <rhyme label="c" type="f" corresp="#s8:l5">welbehagen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Met vreugd van elk <rhyme label="d" type="m" corresp="#s8:l6">beschouwd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
