<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160591</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Petite royale</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk moet noch eens uyt vryen <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn lust begint <rhyme label="b" type="f">t'ontfoncken</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Als ick sie het <rhyme label="a" type="m" corresp="#s1:l1">loncken</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Van ons Bruytjen <rhyme label="b" type="f" corresp="#s1:l2">aen</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Ick sal niet langer <rhyme label="c" type="f">wachten</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Maer sien daerom <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">By daegh en oock by <rhyme label="c" type="f" corresp="#s1:l5">nachten</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Met een Kar of <rhyme label="d" type="m" corresp="#s1:l6">Schuyt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Ick Trouw oock om een <rhyme label="d" type="m" corresp="#s1:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hoe maeck ick best een goet <rhyme label="a" type="m">begin</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Dat dient voor al <rhyme label="b" type="f">versonnen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Eer het werdt <rhyme label="a" type="m" corresp="#s2:l1">begonnen</rhyme>:</l>
        <l id="s2:l4" met="3" type="+-">Hoe raeck ick daer <rhyme label="b" type="f" corresp="#s2:l2">in</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Ick ben soo <rhyme label="c" type="f">onbedreven</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Als een droncken <rhyme label="d" type="m">snuyt</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Geef raed, want ick sal <rhyme label="c" type="f" corresp="#s2:l5">geven</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Eenen moeyen <rhyme label="d" type="m" corresp="#s2:l6">duyt</rhyme>:</l>
        <l id="s2:l9" met="3" type="-+">Ick Trouw oock om een <rhyme label="d" type="m" corresp="#s2:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wel aen, het beste dient <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ick sal my mooytjes <rhyme label="b" type="f">kleeden</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Boven en <rhyme label="a" type="m" corresp="#s3:l1">beneden</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Dan soo henen <rhyme label="b" type="f" corresp="#s3:l2">gaen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Een wijde broeck vol <rhyme label="c" type="f">vouwen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Linten tot de <rhyme label="d" type="m">kuyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Soo gae ick henen <rhyme label="c" type="f" corresp="#s3:l5">douwen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Sonder Veel of <rhyme label="d" type="m" corresp="#s3:l6">Luyt</rhyme>:</l>
        <l id="s3:l9" met="3" type="-+">Ick Trouw oock om een <rhyme label="d" type="m" corresp="#s3:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer als ick by de Vryster <rhyme label="a" type="m">kom</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wat woorden sal ick <rhyme label="b" type="f">spreken</rhyme>?</l>
        <l id="s4:l3" met="3" type="+-">Want veel loose <rhyme label="a" type="m" corresp="#s4:l1">treken</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Gaender by haer <rhyme label="b" type="f" corresp="#s4:l2">om</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">'k Sal seggen, Suycker <rhyme label="c" type="f">gouwen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Hoor nae mijn <rhyme label="d" type="m">besluyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ick soeck met jou te <rhyme label="c" type="f" corresp="#s4:l5">Trouwen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">En ben Fielt noch <rhyme label="d" type="m" corresp="#s4:l6">Guyt</rhyme>:</l>
        <l id="s4:l9" met="3" type="-+">Ick Trouw oock om een <rhyme label="d" type="m" corresp="#s4:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Soo ick daer kreegh een blaeuwe <rhyme label="a" type="m">scheen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat was tot mijner <rhyme label="b" type="f">schanden</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Waer soud' ick <rhyme label="a" type="m" corresp="#s5:l1">belanden</rhyme>?</l>
        <l id="s5:l4" met="3" type="+-">Waer gingh ick dan <rhyme label="b" type="f" corresp="#s5:l2">heen</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">Soo veel by nacht te <rhyme label="c" type="f">swerven</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Met een koude <rhyme label="d" type="m">huyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat wil voor my niet <rhyme label="c" type="f" corresp="#s5:l5">kerven</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Licht het noch eens <rhyme label="d" type="m" corresp="#s5:l6">stuyt</rhyme>:</l>
        <l id="s5:l9" met="3" type="-+">Ick Trouw oock om een <rhyme label="d" type="m" corresp="#s5:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">'t Is best, Speel-meysjes, u <rhyme label="a" type="m">aen-boort</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ick ben een lustigh <rhyme label="b" type="f">Vryer</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Maeck my een <rhyme label="a" type="m" corresp="#s6:l1">bedyer</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Geef my het jae <rhyme label="b" type="f" corresp="#s6:l2">woort</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Laet ons oock Bruyloft <rhyme label="c" type="f">maken</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Neem my voor Hans <rhyme label="d" type="m">Knuyt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Aen my kont gy noch <rhyme label="c" type="f" corresp="#s6:l5">raken</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Loop niet om de <rhyme label="d" type="m" corresp="#s6:l6">Zuydt</rhyme>:</l>
        <l id="s6:l9" met="3" type="-+">Ick Trouw oock om een <rhyme label="d" type="m" corresp="#s6:l6">Bruyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
