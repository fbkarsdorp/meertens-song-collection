  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ã´! KLyne <rhyme label="a" type="m">Minne-God</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe lang zult gy my <rhyme label="b" type="f">plagen</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Gy dryft met my de <rhyme label="a" type="m" corresp="#s1:l1">spot</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En doet my stadig <rhyme label="b" type="f" corresp="#s1:l2">klagen</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Ja ik <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">De smerte niet meer <rhyme label="b" type="f" corresp="#s1:l2">daagen</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Gy zyt een <rhyme label="c" type="m" corresp="#s1:l5">Tieran</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">s' Nagts voel ik raare <rhyme label="a" type="m">lust</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En denk ook <rhyme label="b" type="f">meenigmalen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat my myn Liefje <rhyme label="a" type="m" corresp="#s2:l1">kust</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Myn zinnen zyn aan 't <rhyme label="b" type="f" corresp="#s2:l2">dwalen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Ach! laat <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dog eenmaal adem <rhyme label="b" type="f" corresp="#s2:l2">halen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Ik ben in de <rhyme label="c" type="m" corresp="#s2:l5">lei</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ach! wierd ik eens <rhyme label="a" type="m">gehegt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met myne <rhyme label="b" type="f">uitverkoore</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Door de zuiv're <rhyme label="a" type="m" corresp="#s3:l1">Echt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dan waar myn vreugt <rhyme label="b" type="f" corresp="#s3:l2">geboore</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Ach! die <rhyme label="c" type="m">Maagt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zal my nog wel <rhyme label="b" type="f" corresp="#s3:l2">verhoore</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Zo het u <rhyme label="c" type="m" corresp="#s3:l5">behaagt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
