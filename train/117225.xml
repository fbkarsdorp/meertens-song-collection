      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">CEladon waer wilt gy <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met uw schaepjens in den <rhyme label="b" type="f">morgen</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Laet Filander met uw <rhyme label="a" type="m" corresp="#s1:l1">tre’en</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Want syn hert dat is vol <rhyme label="b" type="f" corresp="#s1:l2">sorgen</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Vol van Liefden is syn hert en <rhyme label="c" type="m">sin</rhyme>:</l>
        <l id="s1:l6" met="5" type="+-">Neygd uw hertje toch eens tot syn <rhyme label="c" type="m" corresp="#s1:l5">Min</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Alderzoetste <rhyme label="c" type="m" corresp="#s1:l5">Herderin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Weg Filander laet my <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">‘k Wil de Vleyery niet <rhyme label="b" type="f">horen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Uwe Min staet myn niet <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Uwe moeyte is <rhyme label="b" type="f" corresp="#s2:l2">verloren</rhyme></l>
        <l id="s2:l5" met="5" type="+-">Want uw hertje en uw valze <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s2:l6" met="5" type="+-">Is verbonden door u trouw en <rhyme label="c" type="m" corresp="#s2:l5">min</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Celinde <rhyme label="c" type="m" corresp="#s2:l5">Herderin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dat getuygde Son en <rhyme label="a" type="m">Maen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sterren, Hemel, Zee en <rhyme label="b" type="f">d’Aerde</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat Celind’ noyt Min <rhyme label="a" type="m" corresp="#s3:l1">ontfaen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Heeft van my; Maer gy O <rhyme label="b" type="f" corresp="#s3:l2">Waerde</rhyme>!</l>
        <l id="s3:l5" met="5" type="+-">Zijt alleenig mynen Engel <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s3:l6" met="5" type="+-">Die myn hert van liefde branden <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Ach ik val voor u te <rhyme label="c" type="m" corresp="#s3:l5">voet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">O gy valze Herder <rhyme label="a" type="m">staek</rhyme>!</l>
        <l id="s4:l2" met="4" type="+-">Al die Minne <rhyme label="b" type="f">Kozeryen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Ik en vinde geen <rhyme label="a" type="m" corresp="#s4:l1">vermaek</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">In uw’ snoode <rhyme label="b" type="f" corresp="#s4:l2">Vleyeryen</rhyme>:</l>
        <l id="s4:l5" met="5" type="+-">Want uw Eden zyn maer enkel <rhyme label="c" type="m">Wind</rhyme>,</l>
        <l id="s4:l6" met="5" type="+-">Daer sy meenig Ieugdig Herders <rhyme label="c" type="m" corresp="#s4:l5">Kind</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Menigmael mee weer <rhyme label="c" type="m" corresp="#s4:l5">verblind</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sal dan noyt myn trouw <rhyme label="a" type="m">gemoed</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Zyn beloond met weder <rhyme label="b" type="f">Minne</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Sal ik noyt myn Engel <rhyme label="a" type="m" corresp="#s5:l1">zoet</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Door myn smekingen <rhyme label="b" type="f" corresp="#s5:l2">Verwinne</rhyme>?</l>
        <l id="s5:l5" met="5" type="+-">Tot verlichting van myn droev’ge <rhyme label="c" type="m">pyn</rhyme>,</l>
        <l id="s5:l6" met="5" type="+-">Goden wilt my toch genadig <rhyme label="c" type="m" corresp="#s5:l5">zyn</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Eer ik in myn Min <rhyme label="c" type="m" corresp="#s5:l5">verdwijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ey Filiander zyt te <rhyme label="a" type="m">vre’en</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Staek vry all’ uw droev’ge <rhyme label="b" type="f">klagte</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Ik ben verwonne door u <rhyme label="a" type="m" corresp="#s6:l1">re’en</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Ik sal uwe pijn <rhyme label="b" type="f" corresp="#s6:l2">versagte</rhyme></l>
        <l id="s6:l5" met="5" type="+-">Om dat ik u nu bevind <rhyme label="c" type="m">getrouw</rhyme>,</l>
        <l id="s6:l6" met="5" type="+-">Sal ik wezen u egte <rhyme label="c" type="m" corresp="#s6:l5">Vrouw</rhyme></l>
        <l id="s6:l7" met="4" type="+-">Laet tog varen al u <rhyme label="c" type="m" corresp="#s6:l5">rouw</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Daer vlied myne ramp weer <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Als een golf voor sterke <rhyme label="b" type="f">winden</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Door myn klagen en <rhyme label="a" type="m" corresp="#s7:l1">gebeen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Heb gy my verhoord <rhyme label="b" type="f" corresp="#s7:l2">beminden</rhyme>:</l>
        <l id="s7:l5" met="5" type="+-">Daer ’s myn Trouw en gy zyt myn <rhyme label="c" type="m">Bruyd</rhyme>,</l>
        <l id="s7:l6" met="5" type="+-">Laet ons vreugde maken tot <rhyme label="c" type="m" corresp="#s7:l5">besluyt</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Want myn Vryen dat is <rhyme label="c" type="m" corresp="#s7:l5">uyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Vlecht nu krans van <rhyme label="a" type="m">Lauwerier</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Herders ende <rhyme label="b" type="f">Herderinnen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Neemt met vreugde uw <rhyme label="a" type="m" corresp="#s8:l1">pleyzier</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Speeld het zoete spel der <rhyme label="b" type="f" corresp="#s8:l2">Minnen</rhyme>,</l>
        <l id="s8:l5" met="5" type="+-">Plukt nu Maegdepalm en <rhyme label="c" type="m">Kruyd</rhyme>:</l>
        <l id="s8:l6" met="5" type="+-">Kroond den Bruydegom en <rhyme label="c" type="m" corresp="#s8:l5">Bruyd</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Speeld op Bas, Phiool en <rhyme label="c" type="m" corresp="#s8:l5">Fluyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
