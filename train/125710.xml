    <melody>Zullen dan mijn droeve klachten</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ZUllen dan mijn droeve <rhyme label="a" type="f">klagten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Ach roemwaarde <rhyme label="b" type="m">Philida</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Noyt uw straf gemoed <rhyme label="a" type="f" corresp="#s1:l1">verzagten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Staak, ey staak u <rhyme label="b" type="m" corresp="#s1:l2">ongena</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Zoeten Engel toon <rhyme label="c" type="f">medoogen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zie tog met ontfermde <rhyme label="c" type="f" corresp="#s1:l5">Oogen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Uw bedrukten Tyter <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Of het is met // Of het is <rhyme label="e" type="f">met</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Of het is met hem <rhyme label="d" type="m" corresp="#s1:l7">gedaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zeg my Tyter hoe kan 't <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat gy dus u zelf <rhyme label="b" type="m">ontrust</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">En my de oorzaak noemt in <rhyme label="a" type="f" corresp="#s2:l1">dezen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Daar gy beter zijt <rhyme label="b" type="m" corresp="#s2:l2">bewust</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Nimmer gaf ik van mijn <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ymand reden om te <rhyme label="c" type="f" corresp="#s2:l5">klagen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Wijl ik alle strafheyd <rhyme label="d" type="m">zwigt</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Schoon gy my dus // Schoon gy my <rhyme label="e" type="f">dus</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Schoon gy my dus straf <rhyme label="d" type="m" corresp="#s2:l7">betigt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Philida u frisse <rhyme label="a" type="f">ontloken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Ieugt, en gaven der <rhyme label="b" type="m">Natuur</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Heeft in my de gloed <rhyme label="a" type="f" corresp="#s3:l1">ontstoken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Van 't inwendig <rhyme label="b" type="m" corresp="#s3:l2">Minne-vuur</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Door u lieve <rhyme label="c" type="f">toverlonken</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">De Oogjes die als Sterre <rhyme label="c" type="f" corresp="#s3:l5">pronken</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Trof my Venus kleynste <rhyme label="d" type="m">Wigt</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Met zijn felle // Met zijn <rhyme label="e" type="f">felle</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Met zijn felle <rhyme label="d" type="m" corresp="#s3:l7">Minne-schigt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wel wat dat een Mensch moet <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'k Agt zijn Pijlen <rhyme label="b" type="m">kragteloos</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Cupido dien Blind <rhyme label="a" type="f" corresp="#s4:l1">gebooren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Heeft op mijn geen magt <rhyme label="b" type="m" corresp="#s4:l2">altoos</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Laat gy u verstand en <rhyme label="c" type="f">zinnen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Van dat Blinde Wigt <rhyme label="c" type="f" corresp="#s4:l5">verwinnen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Klaagt u brand dan aan die <rhyme label="d" type="m">Bloed</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die u deze // Die u <rhyme label="e" type="f">deze</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Die u deze smert aan <rhyme label="d" type="m" corresp="#s4:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Philida mijn zoeten <rhyme label="a" type="f">Engel</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ach! wat spreekt gy <rhyme label="b" type="m">onbedogt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Cupido dien kleynen <rhyme label="a" type="f" corresp="#s5:l1">Bengel</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Houd de heel Natuur <rhyme label="b" type="m" corresp="#s5:l2">verknogt</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Zonder hem viel 't al in <rhyme label="c" type="f">duygen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">'t Moet al voor zijn Scepter <rhyme label="c" type="f" corresp="#s5:l5">buygen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Goden, Menschen, en al 't <rhyme label="d" type="m">Vee</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Zelfs de Vissen // Zelfs de <rhyme label="e" type="f">Vissen</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Zelfs de Vissen in de <rhyme label="d" type="m" corresp="#s5:l7">Zee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Weet wel Tyter dat <rhyme label="a" type="f">PoeÃ«ten</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Zijn gewoon, zeer wijd en <rhyme label="b" type="m">breed</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En 't heel konstig uyt te <rhyme label="a" type="f" corresp="#s6:l1">meeten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Maar ik agt het niet een <rhyme label="b" type="m" corresp="#s6:l2">beet</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">'k Lag met dezen blinden <rhyme label="c" type="f">Schutter</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Ronde Schyven zijn veel <rhyme label="c" type="f" corresp="#s6:l5">nutter</rhyme></l>
        <l id="s6:l7" met="4" type="+-">Schigten van de liefde <rhyme label="d" type="m">Min</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Want die treffen // Want die <rhyme label="e" type="f">treffen</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Want die treffen yders <rhyme label="d" type="m" corresp="#s6:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Moet 'k u dan door Geld <rhyme label="a" type="f">bekooren</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Ach zo dat de Herte <rhyme label="b" type="m">bind</rhyme>!</l>
        <l id="s7:l3" met="4" type="+-">Agt ik al mijn hoop <rhyme label="a" type="f" corresp="#s7:l1">verlooren</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">'k Ben een slegte <rhyme label="b" type="m" corresp="#s7:l2">Huysmans-kind</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">En heb gelijk <rhyme label="c" type="f">d'Herders-knaapjes</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Al mijn Rijkdom in mijn <rhyme label="c" type="f" corresp="#s7:l5">Schaapjes</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Is dan u Min met Goud <rhyme label="d" type="m">t'koop</rhyme>?</l>
        <l id="s7:l8" met="4" type="+-">Ach! dan Min ik // Ach! dan Min <rhyme label="e" type="f">ik</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">Ach! dan Min ik buyten <rhyme label="d" type="m" corresp="#s7:l7">hoop</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Neen standvaste trouwe <rhyme label="a" type="f">Minnaar</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">My bekoord geen Geld nog <rhyme label="b" type="m">Goed</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Ziet ik Kroon u nu <rhyme label="a" type="f" corresp="#s8:l1">Verwinnaar</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Om d'opregtheyd uws <rhyme label="b" type="m" corresp="#s8:l2">gemoed</rhyme></l>
        <l id="s8:l5" met="4" type="+-">'k Zal u Kruyn als Bruygom <rhyme label="c" type="f">cieren</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Met geheyligde <rhyme label="c" type="f" corresp="#s8:l5">Laurieren</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">En een Krans van Ieugdig <rhyme label="d" type="m">Veyl</rhyme>.</l>
        <l id="s8:l8" met="4" type="+-">Ik dank d'Hemel // Ik dank <rhyme label="e" type="f">d'Hemel</rhyme>,</l>
        <l id="s8:l9" met="4" type="+-">Ik dank d'Hemel voor dit <rhyme label="d" type="m" corresp="#s8:l7">Heyl</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Gun my onder deze <rhyme label="a" type="f">Linden</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">U te omhelzen met een <rhyme label="b" type="m">Kus</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Zagt mijn Herder mijn <rhyme label="a" type="f" corresp="#s9:l1">Beminden</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Zagt hoe streeld, en kust gy <rhyme label="b" type="m" corresp="#s9:l2">dus</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Ik eysch voldoening van mijn <rhyme label="c" type="f">Lusjes</rhyme>.</l>
        <l id="s9:l6" met="4" type="+-">Tyter Lief en wagt tot <rhyme label="c" type="f" corresp="#s9:l5">slusjes</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Tot wy komen aan die <rhyme label="d" type="m">Laan</rhyme>.</l>
        <l id="s9:l8" met="4" type="+-">'k Ben te vreden // 'k Ben te <rhyme label="e" type="f">vreden</rhyme>,</l>
        <l id="s9:l9" met="4" type="+-">'k Ben te vreden laat ons <rhyme label="d" type="m" corresp="#s9:l7">gaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
