<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182995</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Van de nachtegaal</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Onderwater moet <rhyme label="a" type="f">marseeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Het zyn orders van de <rhyme label="a" type="f" corresp="#s1:l1">Heeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Om te geeven <rhyme label="b" type="m">commando</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Om te trekken na het <rhyme label="b" type="m" corresp="#s1:l3">Loo</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Speelt op Bas, Fiool en <rhyme label="a" type="f">Snaaren</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Wilt het Oranje-huis <rhyme label="a" type="f" corresp="#s2:l1">bewaaren</rhyme></l>
        <l id="s2:l3" met="4" type="-+">En bevryden voor <rhyme label="b" type="m">tegenspoên</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Voor luy die hem geweld aan <rhyme label="b" type="m" corresp="#s2:l3">doen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Oranje is 't daar ik voor <rhyme label="a" type="f">spreeken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Oranje daar ik 't voor <rhyme label="a" type="f" corresp="#s3:l1">wreeken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Oranje die word veel <rhyme label="b" type="m">gehaat</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Men wil afneemen zynen <rhyme label="b" type="m" corresp="#s3:l3">staat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Oranje mag men niet meer <rhyme label="a" type="f">noemen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">De Oranje-boom die houd zyn <rhyme label="a" type="f" corresp="#s4:l1">bloemen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Oranje is de eêlste <rhyme label="b" type="m">Stam</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Waardoor ons Land in vryheid <rhyme label="b" type="m" corresp="#s4:l3">kwam</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Moet ik dan Oranje <rhyme label="a" type="f">laaten</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Op order van de Heeren <rhyme label="a" type="f" corresp="#s5:l1">Staaten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Mag nu myn Vrouw of <rhyme label="b" type="m">Kind</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Draagen geen <rhyme label="b" type="m" corresp="#s5:l3">Oranje-lint</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Oranje zal wel weerom <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Als eens de twist is <rhyme label="a" type="f" corresp="#s6:l1">weggenomen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dan roept men met een zoet <rhyme label="b" type="m">geluid</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Viva onze <rhyme label="b" type="m" corresp="#s6:l3">Oranje-Spruit</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Oranje zal ik altyd <rhyme label="a" type="f">schryven</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En ook altoos Oranje <rhyme label="a" type="f" corresp="#s7:l1">blyven</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Oranje die zal blyven <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En nooit of nimmer meer <rhyme label="b" type="m" corresp="#s7:l3">vergaan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Al die van Oranje <rhyme label="a" type="f">begeeren</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Zal ik dit Oranje-lied <rhyme label="a" type="f" corresp="#s8:l1">verëeren</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Al is ons Prinsje nog zoo <rhyme label="b" type="m">klein</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Evenwel zal hy Stadhouder <rhyme label="b" type="m" corresp="#s8:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die dit lied hebben gaan <rhyme label="a" type="f">maaken</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Dat waren drie <rhyme label="a" type="f" corresp="#s9:l1">Oranje-snaaken</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En Onderwater, kloek van <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Die stryd voor het <rhyme label="b" type="m" corresp="#s9:l3">Oranje-bloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
