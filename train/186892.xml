<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186892</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Van Paul Jonas en de Engelsman</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">ZEg Paul <rhyme label="a" type="m">Jones</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Hoe spreekt gy zoo <rhyme label="b" type="m">assurant</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Ik ben een Held op <rhyme label="c" type="m">Zee</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Tegen myn <rhyme label="b" type="m" corresp="#s1:l2">Vyand</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Als het 'er komt op <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ik zal u <rhyme label="d" type="m" corresp="#s1:l5">wederstaan</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Myn trouwe <rhyme label="e" type="f">Knegten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die zullen u <rhyme label="e" type="f" corresp="#s1:l7">bevegten</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">En tot talieter <rhyme label="d" type="m" corresp="#s1:l5">slaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Ik ben Paul <rhyme label="a" type="m">Jones</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En nog <rhyme label="b" type="m">konstant</rhyme>!</l>
        <l id="s2:l3" met="3" type="-+">Ik vrees geen <rhyme label="c" type="m">Koning</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Van <rhyme label="b" type="m" corresp="#s2:l2">Engeland</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Kryg ik u maar op <rhyme label="d" type="m">Zee</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gy moet wel met ons <rhyme label="d" type="m" corresp="#s2:l5">mee</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Na <rhyme label="e" type="f">Duinkerke</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wilt maar <rhyme label="e" type="f" corresp="#s2:l7">aanmerken</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Dan breng ik u op die <rhyme label="d" type="m" corresp="#s2:l5">Ree</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Wel Paul <rhyme label="a" type="m">Jones</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hoe zyt gy zoo <rhyme label="b" type="m">assurant</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Denkt tegen wie gy <rhyme label="c" type="m">spreekt</rhyme>;</l>
        <l id="s3:l4" met="3" type="+-">Ik ben Koning van <rhyme label="b" type="m" corresp="#s3:l2">Engeland</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ik zal door myn <rhyme label="d" type="m">Canon</rhyme></l>
        <l id="s3:l6" met="3" type="-+">U brengen na <rhyme label="d" type="m" corresp="#s3:l5">London</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Kryg ik u in myn <rhyme label="e" type="f">handen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">'k Zal u met schip <rhyme label="e" type="f" corresp="#s3:l7">verbranden</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">U toonen wat ik <rhyme label="d" type="m" corresp="#s3:l5">kan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Wel <rhyme label="a" type="m">Engeland</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Overgeven doen ik <rhyme label="b" type="m">niet</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Liever met de <rhyme label="c" type="m">Duivel</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Als na u <rhyme label="b" type="m" corresp="#s4:l2">gebied</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Matroozen en <rhyme label="d" type="m">Zoldaat</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">Dat is myn <rhyme label="d" type="m" corresp="#s4:l5">toeverlaat</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Het zyn trouwe <rhyme label="e" type="f">knegten</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Die dapper kunnen <rhyme label="e" type="f" corresp="#s4:l7">vegten</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Zy vreezen voor geen <rhyme label="d" type="m" corresp="#s4:l5">kwaad</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Gy bent een <rhyme label="a" type="m">Amerikaan</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Al staat gy nog zo <rhyme label="b" type="m">pal</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zal ik a zoo lang <rhyme label="c" type="m">slaan</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Tot gy raakt ten <rhyme label="b" type="m" corresp="#s5:l2">val</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">Ik had laest <rhyme label="d" type="m">terstond</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">U Schip al in de <rhyme label="d" type="m" corresp="#s5:l5">grond</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Nu kunt gy wel <rhyme label="e" type="f">denken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ik zal u ook wel <rhyme label="e" type="f" corresp="#s5:l7">krenken</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Doen zwemmen als een <rhyme label="d" type="m" corresp="#s5:l5">hond</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Ik ben Paul <rhyme label="a" type="m">Jones</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En nog <rhyme label="b" type="m">konstant</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Vyf Scheepen leggen <rhyme label="c" type="m">klaar</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Tegen <rhyme label="b" type="m" corresp="#s6:l2">Engeland</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Als 't er komt op <rhyme label="d" type="m">aan</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Zal ik u <rhyme label="d" type="m" corresp="#s6:l5">wederstaan</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Daarom al dat <rhyme label="e" type="f">praaaten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Kan in 't minst niet <rhyme label="e" type="f" corresp="#s6:l7">baaten</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Allon 'er maar weer <rhyme label="d" type="m" corresp="#s6:l5">aan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">Ik ben de <rhyme label="a" type="m">Koning</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van 't magtip <rhyme label="b" type="m">Engeland</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Gy Paul <rhyme label="c" type="m">Jones</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Moet vallen in myn <rhyme label="b" type="m" corresp="#s7:l2">hand</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ik heb geen <rhyme label="d" type="m">ontzag</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Al voor u valsche <rhyme label="d" type="m" corresp="#s7:l5">Vlag</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Ik zal uw wel <rhyme label="e" type="f">payen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Al weet gy 't zoo te <rhyme label="e" type="f" corresp="#s7:l7">draayen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Door een felle <rhyme label="d" type="m" corresp="#s7:l5">slag</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">Wel <rhyme label="a" type="m">Engeland</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ik ben een Man op <rhyme label="b" type="m">Zee</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Al wat ik van u <rhyme label="c" type="m">zie</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Dat weetje, neem ik <rhyme label="b" type="m" corresp="#s8:l2">mee</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Myn volk is wel <rhyme label="d" type="m">klaar</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat wierd gy laast <rhyme label="d" type="m" corresp="#s8:l5">gewaar</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Ik heb <rhyme label="e" type="f">gezwooren</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Wil myn reeden <rhyme label="e" type="f" corresp="#s8:l7">hooren</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Ik breng u in 't <rhyme label="d" type="m" corresp="#s8:l5">gevaar</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="+-">Ach Paul <rhyme label="a" type="m">Jones</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Spreekt niet zoo <rhyme label="b" type="m">assurant</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ik ben nog <rhyme label="c" type="m">Koning</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Van 't magtig <rhyme label="b" type="m" corresp="#s9:l2">Engeland</rhyme>!</l>
        <l id="s9:l5" met="3" type="-+">Wy hebben nog geen <rhyme label="d" type="m">nood</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Verzien van kruid en <rhyme label="d" type="m" corresp="#s9:l5">lood</rhyme>.</l>
        <l id="s9:l7" met="2" type="-+">Wil liever <rhyme label="e" type="f">zwygen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Ik zal u wel eens <rhyme label="e" type="f" corresp="#s9:l7">krygen</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Al met myn groote <rhyme label="d" type="m" corresp="#s9:l5">Vloot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
