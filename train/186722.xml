    <tags/>
    <melody>Psalm 047 Datheen</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Volken, wyd, en <rhyme label="a" type="m">syd</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Klapt, al t saam <rhyme label="a" type="m" corresp="#s1:l1">verblydt</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">In der handen <rhyme label="b" type="m">palm</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Maakt een schellen <rhyme label="b" type="m" corresp="#s1:l3">galm</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Dat uw stemme <rhyme label="c" type="m">juich</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">En Gods roem <rhyme label="c" type="m" corresp="#s1:l5">getuig</rhyme>.</l>
        <l id="s1:l7" met="3" type="+-">Want de Heer, met <rhyme label="d" type="m">eer</rhyme></l>
        <l id="s1:l8" met="3" type="+-">D'allerhoogste <rhyme label="d" type="m" corresp="#s1:l7">heer</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Dien men vreesen <rhyme label="e" type="m">moet</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Een grootkooning, <rhyme label="e" type="m" corresp="#s1:l9">doet</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Wat hy wil op <rhyme label="f" type="m">aard</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">Oover al <rhyme label="f" type="m" corresp="#s1:l11">vermaart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Meenig volkryk <rhyme label="a" type="m">land</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Brengt hy, door syn <rhyme label="a" type="m" corresp="#s2:l1">hand</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Onder onse <rhyme label="b" type="m">magt</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">'t Allervreemdst <rhyme label="b" type="m" corresp="#s2:l3">geslacht</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Werpt hy dweeg van <rhyme label="c" type="m">moed</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Onder onsen <rhyme label="c" type="m" corresp="#s2:l5">voet</rhyme>.</l>
        <l id="s2:l7" met="3" type="+-">Hy verkoos ons <rhyme label="d" type="m">lot</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Ons ten <rhyme label="d" type="m" corresp="#s2:l7">erfgenot</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Op dat Iakob <rhyme label="e" type="m">bloei</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Hoog, en heerlik <rhyme label="e" type="m" corresp="#s2:l9">groei</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Want syn liefde <rhyme label="f" type="m">viel</rhyme></l>
        <l id="s2:l12" met="3" type="+-">Op die vroome <rhyme label="f" type="m" corresp="#s2:l11">ziel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Siet, die God, de <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Vaart nu op met <rhyme label="a" type="m" corresp="#s3:l1">eer</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Op geschal, <rhyme label="b" type="m">gesank</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">En <rhyme label="b" type="m" corresp="#s3:l3">basuingeklank</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Singt dan, singt Gods <rhyme label="c" type="m">lof</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Singt hem <rhyme label="c" type="m" corresp="#s3:l5">psalmenstof</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Singt hem, die <rhyme label="d" type="m">gewis</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Onse kooning <rhyme label="d" type="m" corresp="#s3:l7">is</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">Want God is <rhyme label="e" type="m">alleen</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Kooning van de <rhyme label="e" type="m" corresp="#s3:l9">zeen</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Van des aardryks <rhyme label="f" type="m">perk</rhyme>,</l>
        <l id="s3:l12" met="3" type="+-">En haar gansche <rhyme label="f" type="m" corresp="#s3:l11">werk</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Psalmsingt hem ten <rhyme label="a" type="m">prys</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Tot elx <rhyme label="a" type="m" corresp="#s4:l1">onderwys</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">God bestiert, en <rhyme label="b" type="m">hoedt</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Ook het heidensch <rhyme label="b" type="m" corresp="#s4:l3">bloed</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">God sit, heerlik <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Op syn heilgen <rhyme label="c" type="m" corresp="#s4:l5">throon</rhyme>.</l>
        <l id="s4:l7" met="3" type="+-">Veel vrywillig <rhyme label="d" type="m">saad</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Volken eÃªl van <rhyme label="d" type="m" corresp="#s4:l7">staat</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Nu gedweeg <rhyme label="e" type="m">versaamt</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Zyn Gods volk <rhyme label="e" type="m" corresp="#s4:l9">genaamt</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Als de vroome <rhyme label="f" type="m">stam</rhyme></l>
        <l id="s4:l12" met="3" type="+-">Van syn <rhyme label="f" type="m" corresp="#s4:l11">Abraham</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
