  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DOnkere kuilen, Nagt <rhyme label="a" type="f">spelonken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Eenzaem Bos vol angst en <rhyme label="b" type="m">smert</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Wyl ik Venus <rhyme label="a" type="f" corresp="#s1:l1">minne-vonken</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Smoren moet in mijn Jeugdig <rhyme label="b" type="m" corresp="#s1:l2">hert</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Ach mijn Cloris <rhyme label="c" type="f">ziels-vrindinne</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Waerom weigert gy mijn u <rhyme label="d" type="m">min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Treft haer, treft haer <rhyme label="a" type="f">min-godinne</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Kan het zijn bestiert dees' <rhyme label="b" type="m">Maegd</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Dat zy my dog weder <rhyme label="a" type="f" corresp="#s2:l1">minne</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Tot dat haer mijn tael <rhyme label="b" type="m" corresp="#s2:l2">behaegt</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Ach mijn Cloris <rhyme label="c" type="f">ziels-vrindinne</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Waerom weigert gy mijn u <rhyme label="d" type="m">min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ik Plag voor desen hier te <rhyme label="a" type="f">quelen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Met mijn Liefje in 't jeugdig <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En om mijne smert te <rhyme label="a" type="f" corresp="#s3:l1">heelen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Sogt ik mijn Lusjes te <rhyme label="b" type="m" corresp="#s3:l2">voldoen</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Ach mijn Cloris <rhyme label="c" type="f">ziels-vrindinne</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Waerom weigert gy mijn u <rhyme label="d" type="m">min</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'k Hoor de Liefde tot mijn <rhyme label="a" type="f">spreken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'k Sie dat mijn Geluk-Zon <rhyme label="b" type="m">rijst</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Droefheid is van my <rhyme label="a" type="f" corresp="#s4:l1">geweken</rhyme>!</l>
        <l id="s4:l4" met="4" type="+-">Cloris mijn haer min <rhyme label="b" type="m" corresp="#s4:l2">bewijst</rhyme>!</l>
        <l id="s4:l5" met="4" type="+-">Ach mijn Cloris <rhyme label="c" type="f">ziels-vriendinne</rhyme>!</l>
        <l id="s4:l6" met="4" type="+-">Waerom weigert gy mijn u <rhyme label="d" type="m">min</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'k Heb mijn aen u min <rhyme label="a" type="f">verbonden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Eeuwig zal ik de uwe <rhyme label="b" type="m">zijn</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">Mits ik werd door min <rhyme label="a" type="f" corresp="#s5:l1">verslonden</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Door mijn Rou en <rhyme label="b" type="m" corresp="#s5:l2">minne-pijn</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Ach mijn Cloris <rhyme label="c" type="f">ziels-vriendinne</rhyme>!</l>
        <l id="s5:l6" met="4" type="+-">Blaekt eens zuiv're weder <rhyme label="d" type="m">min</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
