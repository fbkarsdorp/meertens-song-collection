<song>
  <meta>
    <id>20902</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Al hebben de prinsen haar wens</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Jesv lief, der Maeghden <rhyme label="a" type="m">kroon</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Geboren uyt een Maghet <rhyme label="a" type="m" corresp="#s1:l1">schoon</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die Moeder was, en Maegd met <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Verhoort genadich ons' <rhyme label="b" type="m" corresp="#s1:l3">gebe'en</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy Heer, die onder d' Leel-jen <rhyme label="a" type="m">weydt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Rond-om betuynt, al-om <rhyme label="a" type="m" corresp="#s2:l1">beleydt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Met Maeghden Chooren <rhyme label="b" type="m">kostelijck</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ghy schenckt uw' Bruyds het <rhyme label="b" type="m" corresp="#s2:l3">hemelrijck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waer dat Ghy gaet, der Maeghden <rhyme label="a" type="m">Treyn</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die volght U, Ghy gaet niet <rhyme label="a" type="m" corresp="#s3:l1">alleyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sy volgen U met soete <rhyme label="b" type="m">sanck</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Sy loven U, en weten <rhyme label="b" type="m" corresp="#s3:l3">danck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wy bidden U met <rhyme label="a" type="m">nedricheyd</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ons' herten 't uwer dienst <rhyme label="a" type="m" corresp="#s4:l1">bereydt</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Laet sonder vleck, en smet der <rhyme label="b" type="m">sond</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Ons' herten zijn ten allen <rhyme label="b" type="m" corresp="#s4:l3">stond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat glory, eer, en lof, en <rhyme label="a" type="m">prijs</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Zy op de alderbeste <rhyme label="a" type="m" corresp="#s5:l1">wijs</rhyme></l>
        <l id="s5:l3" met="4" type="-+">God Vader, Soon, en d' Heyl'gen <rhyme label="b" type="m">Geest</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die 't allen tyd moet zijn <rhyme label="b" type="m" corresp="#s5:l3">gevreest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
