  <author>E. Pels de Jonge</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AL de Luy die vraghen <rhyme label="a" type="f">Grietje</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Waerom datse niet en <rhyme label="b" type="m">trout</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Mit Jan Heymen, of met <rhyme label="a" type="f" corresp="#s1:l1">Pietje</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wangt se hebben hoopen <rhyme label="b" type="m" corresp="#s1:l2">Gout</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Grietje // Lietje, 't soete volle <rhyme label="c" type="f">Pietje</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'Kwet het jou ier langh al <rhyme label="b" type="m" corresp="#s1:l2">rout</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Grietje say wat sou ick <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Dat ick nam die grooten <rhyme label="b" type="m">Vreck</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Hy en ken schryven noch <rhyme label="a" type="f" corresp="#s2:l1">lesen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">En hy het maer ien <rhyme label="b" type="m" corresp="#s2:l2">ghebreck</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">Praetje // maetje, van dit <rhyme label="c" type="f">Cameraetje</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Hy is dol en hallef <rhyme label="b" type="m" corresp="#s2:l2">geck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Peet Neel raet Grietje aen <rhyme label="a" type="f">Jantje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">'t Is ien giest, ien eelen <rhyme label="b" type="m">baes</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Neen sayt sie die is voor <rhyme label="a" type="f" corresp="#s3:l1">Antje</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ick heb beter sin in <rhyme label="b" type="m" corresp="#s3:l2">Claes</rhyme>,</l>
        <l id="s3:l5" met="5" type="+-">Jantje // 'tmantje, dat is sucken <rhyme label="c" type="f">quantje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Die 'tvolckje soeckt van deus <rhyme label="b" type="m" corresp="#s3:l2">Aes</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Pietje is hiel uyt sen <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Jan Heymen al het hy <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Hy ken niet ien stuyver <rhyme label="a" type="f" corresp="#s4:l1">winnen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Als mijn Klaesje Krelis <rhyme label="b" type="m" corresp="#s4:l2">doet</rhyme>,</l>
        <l id="s4:l5" met="5" type="+-">Weetje // Peetje, al ist ien kael <rhyme label="c" type="f">neetje</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Dat ick hem doch hebben <rhyme label="b" type="m" corresp="#s4:l2">moet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Claesje ken dansen en <rhyme label="a" type="f">springhen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Piet noch Jan en keunen <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En hy ken soo giestich <rhyme label="a" type="f" corresp="#s5:l1">singhen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Mennighen nieu kluchtich <rhyme label="b" type="m" corresp="#s5:l2">Liedt</rhyme>,</l>
        <l id="s5:l5" met="5" type="+-">Singhen // springhen, en al sucke <rhyme label="c" type="f">dinghen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Peetje die becooren <rhyme label="b" type="m" corresp="#s5:l2">Griet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Grietje sayt dat soete <rhyme label="a" type="f">Knechje</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Is die ick allien <rhyme label="b" type="m">begheer</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Maer hy vrijt gien griet, maer <rhyme label="a" type="f" corresp="#s6:l1">Brechje</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Die hy mint, en sy hem <rhyme label="b" type="m" corresp="#s6:l2">weer</rhyme>,</l>
        <l id="s6:l5" met="5" type="+-">Brechje // wechje, met dit moye <rhyme label="c" type="f">Knechje</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Ier dat Grietje krenckt jou <rhyme label="b" type="m" corresp="#s6:l2">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
