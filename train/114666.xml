<song>
  <meta>
    <id>114666</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Margot meurt toujours de faim</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HOe seer hebd' in <rhyme label="a" type="m">nedricheydt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Francisce, altijdt <rhyme label="b" type="f">uytgheschenen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die, in sulcken <rhyme label="a" type="m" corresp="#s1:l1">heylicheydt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Waert in u oogh als niet <rhyme label="b" type="f" corresp="#s1:l2">verdwenen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Hoe seer is d' ootmoedicheydt in <rhyme label="c" type="m">u</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s1:l5">nu</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s1:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die u altijdt hebt <rhyme label="a" type="m">veracht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Begheert by ieder niet te <rhyme label="b" type="f">schynen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Alle smaet altijdt <rhyme label="a" type="m" corresp="#s2:l1">verwaght</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">De achtingh' ded' u duysent <rhyme label="b" type="f" corresp="#s2:l2">pijnen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s2:l5">nu</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s2:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wat een vier der liefden <rhyme label="a" type="m">u</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Het hert tot Iesvm heeft <rhyme label="b" type="f">ontsteken</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die ghy noot te dienen <rhyme label="a" type="m" corresp="#s3:l1">mu</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Gheen pijn, gheen moet oot hebt <rhyme label="b" type="f" corresp="#s3:l2">ontweken</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s3:l5">nu</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s3:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Diep in u reyn ziel <rhyme label="a" type="m">gheprint</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Stont Iesvus met sijn bitter <rhyme label="b" type="f">lijden</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die ghy hebt te seer <rhyme label="a" type="m" corresp="#s4:l1">bemint</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Sijn Cruys in 't hert droeght t' alle <rhyme label="b" type="f" corresp="#s4:l2">tijden</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s4:l5">nu</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s4:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Iesvs door 'nen <rhyme label="a" type="m">Seraphien</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Aen u ghecruyst sich heeft <rhyme label="b" type="f">ghesonden</rhyme></l>
        <l id="s5:l3" met="4" type="+-">In 't ghebedt van u <rhyme label="a" type="m" corresp="#s5:l1">ghesien</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ghedruct in u heeft sijn vijf <rhyme label="b" type="f" corresp="#s5:l2">wonden</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s5:l5">nu</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s5:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hoe seer stond' altijdt <rhyme label="a" type="m">ghekeert</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Met alle liefde tot den <rhyme label="b" type="f">armen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Van u altijdt seer <rhyme label="a" type="m" corresp="#s6:l1">gheeert</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Waer over m'u soo sagh <rhyme label="b" type="f" corresp="#s6:l2">ontfermen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s6:l5">nu</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s6:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Simpel u ghemaect heeft <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s7:l2" met="4" type="-+">(Soo wijs die bleeft, en rijp van <rhyme label="b" type="f">sinnen</rhyme>)</l>
        <l id="s7:l3" met="4" type="+-">Nedricheyt, die meer en <rhyme label="a" type="m" corresp="#s7:l1">meer</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Nam haeren was in u van <rhyme label="b" type="f" corresp="#s7:l2">binnen</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s7:l5">nu</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s7:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hoe onnoosel waerdy <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Oock met de beesten seer <rhyme label="b" type="f">meweerdich</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Daerom oock 't onred'lijck <rhyme label="a" type="m" corresp="#s8:l1">dier</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Tot uwen dienst, was wonder <rhyme label="b" type="f" corresp="#s8:l2">veerdich</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s8:l5">nu</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s8:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Nu staet u op 't hooft de <rhyme label="a" type="m">croon</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Daer ghy hebt wettich om <rhyme label="b" type="f">ghestreden</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Nu betaelt u Godt den <rhyme label="a" type="m" corresp="#s9:l1">loon</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Voor hem die hebt soo veel <rhyme label="b" type="f" corresp="#s9:l2">gheleden</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Hoe seer is d' oodtmoedicheydt in <rhyme label="c" type="m">u</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Van Godt eeuwelijck <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Die soo heerlijck in glorie <rhyme label="c" type="m" corresp="#s9:l5">nu</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Besit het eeuwich <rhyme label="d" type="f" corresp="#s9:l6">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
