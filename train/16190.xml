<song>
  <meta>
    <id>16190</id>
    <date>
      <year-start>1903</year-start>
      <year-end>1908</year-end>
    </date>
    <tags/>
    <melody>Es war ein junger Held</melody>
  <author>Florimond van Duyse</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Es war ein junger helt,</l>
        <l id="s1:l2">sein hertz war ihm ge -stelt</l>
        <l id="s1:l3">nach einer jungfrauwe schone.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Er dient ihr tag und nacht,</l>
        <l id="s2:l2">dass sie doch weinig achtt,</l>
        <l id="s2:l3">gab ihm doch gar kein lohne.</l>
      </lg>
    </lg>
  </text>
</song>
