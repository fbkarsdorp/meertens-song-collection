        <l id="s1:l1" met="3" type="-+">'t Hertje zijt ghy <rhyme label="a" type="m">Me-vrouw</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat ick nae jaegh met <rhyme label="a" type="m" corresp="#s1:l1">rouw</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Het ander Hert is 't <rhyme label="b" type="f">mijne</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Soo 't een het aer <rhyme label="c" type="m">verlaet</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Leef ick staegh in <rhyme label="b" type="f" corresp="#s1:l3">pijne</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En sweef in droeve <rhyme label="c" type="m" corresp="#s1:l4">staet</rhyme>.</l>
