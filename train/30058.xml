    <melody>Psalm 099 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philips van Marnix van Sint Aldegonde</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">NV heeft God de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">s'Conincx recht end <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Al tvolck datter <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Vreest hem ende <rhyme label="b" type="m" corresp="#s1:l3">beeft</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Hy sit als een <rhyme label="c" type="m">Prins</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Tusschen <rhyme label="c" type="m" corresp="#s1:l5">Cherubins</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">End is hooch <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Dies moet d'eirde <rhyme label="d" type="f" corresp="#s1:l7">beven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Voirwaer onsen <rhyme label="a" type="m">God</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Is in Sions <rhyme label="a" type="m" corresp="#s2:l1">slot</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Groot end hoog <rhyme label="b" type="m">geacht</rhyme></l>
        <l id="s2:l4" met="3" type="+-">End heeft volle <rhyme label="b" type="m" corresp="#s2:l3">macht</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Op tvolck al te <rhyme label="c" type="m">saem</rhyme>.</l>
        <l id="s2:l6" met="3" type="+-">Men loift sijnen <rhyme label="c" type="m" corresp="#s2:l5">naem</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Groot end wonder <rhyme label="d" type="f">crachtig</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Heylig end <rhyme label="d" type="f" corresp="#s2:l7">almachtig</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Desen Coninck <rhyme label="a" type="m">sterck</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Maeckt oock seer groot <rhyme label="a" type="m" corresp="#s3:l1">werck</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Van een goet <rhyme label="b" type="m">gericht</rhyme>:</l>
        <l id="s3:l4" met="3" type="+-">Du hebst Heer <rhyme label="b" type="m" corresp="#s3:l3">gesticht</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Inder <rhyme label="c" type="m">eewicheyt</rhyme></l>
        <l id="s3:l6" met="3" type="+-">De <rhyme label="c" type="m" corresp="#s3:l5">rechtveirdicheyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">End doen Iacob <rhyme label="d" type="f">treden</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">In <rhyme label="d" type="f" corresp="#s3:l7">gerechtigheden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Prijst den Heere <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Onsen God end <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Sijnen voetbanck <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Hy is heylich <rhyme label="b" type="m" corresp="#s4:l3">seer</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Moses sijnen <rhyme label="c" type="m">knecht</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Heeft met Aaron <rhyme label="c" type="m" corresp="#s4:l5">recht</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Voir God met <rhyme label="d" type="f">gebeden</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Tpriesterschap <rhyme label="d" type="f" corresp="#s4:l7">betreden</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Ende <rhyme label="a" type="m">Samuel</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Deed' oock dickmael <rhyme label="a" type="m" corresp="#s5:l1">wel</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Tot God sijn <rhyme label="b" type="m">gebedt</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Voir des volcks <rhyme label="b" type="m" corresp="#s5:l3">ontset</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">End Gods goetheyt <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Telkens inden <rhyme label="c" type="m" corresp="#s5:l5">noot</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Als sy hem <rhyme label="d" type="f">aenbaden</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Hoordes' wt <rhyme label="d" type="f" corresp="#s5:l7">genaden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Want hy wt de <rhyme label="a" type="m">wolck</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Sprack self tot het <rhyme label="a" type="m" corresp="#s6:l1">volck</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Dies sy oock <rhyme label="b" type="m">getrou</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Met een recht <rhyme label="b" type="m" corresp="#s6:l3">berou</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Hoorden nae de <rhyme label="c" type="m">wet</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Van hem <rhyme label="c" type="m" corresp="#s6:l5">ingeset</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Poogende te <rhyme label="d" type="f">treden</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Nae sijn heylge <rhyme label="d" type="f" corresp="#s6:l7">seden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">O Heer onse <rhyme label="a" type="m">God</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Heere <rhyme label="a" type="m" corresp="#s7:l1">Sebaoth</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Du hebst voort end <rhyme label="b" type="m">voort</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Haer gebedt <rhyme label="b" type="m" corresp="#s7:l3">verhoort</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">End nae dijnen <rhyme label="c" type="m">aert</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Hebstu tvolck <rhyme label="c" type="m" corresp="#s7:l5">gespaert</rhyme>:</l>
        <l id="s7:l7" met="3" type="+-">Doch na straf <rhyme label="d" type="f">genadich</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Van haer werck <rhyme label="d" type="f" corresp="#s7:l7">misdadich</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Loift den Heer <rhyme label="a" type="m">altijt</rhyme></l>
        <l id="s8:l2" met="3" type="+-">God <rhyme label="a" type="m" corresp="#s8:l1">gebenedijt</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Komt, valt hem te <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">End knielt met <rhyme label="b" type="m" corresp="#s8:l3">ootmoet</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Op den berg <rhyme label="c" type="m">bereyt</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Voir sijn <rhyme label="c" type="m" corresp="#s8:l5">heyligheyt</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Want God onse <rhyme label="d" type="f">Heere</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Is doch heylig <rhyme label="d" type="f" corresp="#s8:l7">seere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
