<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164893</id>
    <date>
      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pange lingua</melody>
  <author>Augustinus van Teylingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wilt ontspringen, lof-sang <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Onsen Heer <rhyme label="b" type="m">ghebenedijt</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Want hy doet ons groote <rhyme label="a" type="f" corresp="#s1:l1">dinghen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hierom looft hem met <rhyme label="b" type="m" corresp="#s1:l2">jolijt</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">En wilt u van ’t quaet <rhyme label="a" type="f" corresp="#s1:l1">bedwingen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zijn weldaet ghedachtich <rhyme label="b" type="m" corresp="#s1:l2">zijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">In-geseten // wilt ghy <rhyme label="a" type="f">weten</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Wat den Heer ons heeft <rhyme label="b" type="m">gedaen</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Door zijn goetheyt <rhyme label="a" type="f" corresp="#s2:l1">onghemeten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Gheeft hy ons zijn lichaem <rhyme label="b" type="m" corresp="#s2:l2">saen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Door een spijse om te <rhyme label="a" type="f" corresp="#s2:l1">eeten</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">Wilt hem waerelijck <rhyme label="b" type="m" corresp="#s2:l2">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Proeft u sinnen // wel van <rhyme label="a" type="f">binnen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Eer dat ghy ontfanghen <rhyme label="b" type="m">sult</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Desen grooten schat der <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Die den wijden Hemel <rhyme label="b" type="m" corresp="#s3:l2">vult</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Eer dat ghy dit sult <rhyme label="a" type="f" corresp="#s3:l1">beginnen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Spreeckt ootmoedelijck u <rhyme label="b" type="m" corresp="#s3:l2">schult</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wat mocht Godt ons meerder <rhyme label="a" type="f">geven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dan dit lieffelijck <rhyme label="b" type="m">present</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Van zijn vleesch en bloed <rhyme label="a" type="f" corresp="#s4:l1">verheven</rhyme></l>
        <l id="s4:l4" met="4" type="+-">In het heyligh <rhyme label="b" type="m" corresp="#s4:l2">Sacrament</rhyme>?</l>
        <l id="s4:l5" met="4" type="+-">Op dat wy in ‘t eeuwigh <rhyme label="a" type="f" corresp="#s4:l1">leven</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Suden komen <rhyme label="b" type="m" corresp="#s4:l2">ongheschendt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dese spijse // hoogh van <rhyme label="a" type="f">prijse</rhyme></l>
        <l id="s5:l2" met="4" type="+-">In ons zielen liefde <rhyme label="b" type="m">straeldt</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Wt des Hemels <rhyme label="a" type="f" corresp="#s5:l1">Paradijse</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Is zy voor ons <rhyme label="b" type="m" corresp="#s5:l2">neer-ghedaelt</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Looft hem met der Eng’len <rhyme label="a" type="f" corresp="#s5:l1">wijse</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Van dees spijse <rhyme label="b" type="m" corresp="#s5:l2">onghefaelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Glori’, hoogheyt, lof en <rhyme label="a" type="f">eere</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Sy de hooghe <rhyme label="b" type="m">Triniteyt</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Die ons arme menschen <rhyme label="a" type="f" corresp="#s6:l1">teere</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Dese spijse heeft <rhyme label="b" type="m" corresp="#s6:l2">bereydt</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Die ons Godt is en ons <rhyme label="a" type="f" corresp="#s6:l1">Heere</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Nu en in der <rhyme label="b" type="m" corresp="#s6:l2">eeuwigheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
