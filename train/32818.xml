<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>32818</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>O schepper fier</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Schepper <rhyme label="a" type="m">fier</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe lustigh om <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Daer ghy zijt <rhyme label="b" type="f" corresp="#s1:l2">ghepresen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van d'Enghels inden <rhyme label="c" type="m">throon</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Wilt ons toch <rhyme label="a" type="m" corresp="#s1:l1">hier</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van allen druck <rhyme label="b" type="f" corresp="#s1:l2">ghenesen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Heere uyt <rhyme label="b" type="f" corresp="#s1:l2">ghelesen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Gheeft naemaels sulcken <rhyme label="c" type="m" corresp="#s1:l4">loon</rhyme>;</l>
        <l id="s1:l9" met="4" type="-+">Dat ick u aenschijn mach <rhyme label="d" type="f">anschouwen</rhyme></l>
        <l id="s1:l10" met="4" type="-+">En u gheboden <rhyme label="d" type="f" corresp="#s1:l9">onderhouwen</rhyme>.</l>
        <l id="s1:l11" met="2" type="+-">Heere Iesus <rhyme label="e" type="m">soet</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Gheeft ons dat eeuwigh <rhyme label="e" type="m" corresp="#s1:l11">goet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wie sou die <rhyme label="a" type="m">vreught</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En blijschap konnen <rhyme label="b" type="f">schrijven</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Die sy daer <rhyme label="b" type="f" corresp="#s2:l2">bedrijven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In Syons schoone <rhyme label="c" type="m">pleyn</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Sy sijn <rhyme label="a" type="m" corresp="#s2:l1">verheught</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Om dat sy moghen <rhyme label="b" type="f" corresp="#s2:l2">blijven</rhyme></l>
        <l id="s2:l7" met="2" type="-+">En altijts <rhyme label="b" type="f" corresp="#s2:l2">beklijven</rhyme></l>
        <l id="s2:l8" met="3" type="-+">By't Lam Godts suyver en <rhyme label="c" type="m" corresp="#s2:l4">reyn</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">'t Lammeken doet open ten <rhyme label="d" type="f">thoone</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Den boeck met seven seghels <rhyme label="d" type="f" corresp="#s2:l9">schoone</rhyme>.</l>
        <l id="s2:l11" met="2" type="+-">Heere Iesus <rhyme label="e" type="m">soet</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Gheeft ons dat eeuwigh <rhyme label="e" type="m" corresp="#s2:l11">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dees stad <rhyme label="a" type="m">eerbaer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Is schoon boven <rhyme label="b" type="f">maten</rhyme></l>
        <l id="s3:l3" met="3" type="+-">(Aenmerckt) haere <rhyme label="b" type="f" corresp="#s3:l2">straten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Zijn van goudt <rhyme label="c" type="m">gheblaveyt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">De Enghels <rhyme label="a" type="m" corresp="#s3:l1">daer</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Met gulde <rhyme label="b" type="f" corresp="#s3:l2">wieroock-vaten</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Loven sonder <rhyme label="b" type="f" corresp="#s3:l2">laten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Tot inder <rhyme label="c" type="m" corresp="#s3:l4">eeuwigheyt</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">En prijsen Godt van <rhyme label="d" type="f">hemelrijcke</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Met soet lof-sanck ende <rhyme label="d" type="f" corresp="#s3:l9">musijcke</rhyme>.</l>
        <l id="s3:l11" met="2" type="+-">Heere Iesus <rhyme label="e" type="m">soet</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Gheeft ons dat eeuwigh <rhyme label="e" type="m" corresp="#s3:l11">goet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Die <rhyme label="a" type="m">duysternacht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sietmen daer niet <rhyme label="b" type="f">regneren</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Altijdt <rhyme label="b" type="f" corresp="#s4:l2">triumpheren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Godts vrienden claer en <rhyme label="c" type="m">schoon</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Met groote <rhyme label="a" type="m" corresp="#s4:l1">kracht</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sietmense <rhyme label="b" type="f" corresp="#s4:l2">jubileren</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En sonder <rhyme label="b" type="f" corresp="#s4:l2">cesseren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Singhen met soeten <rhyme label="c" type="m" corresp="#s4:l4">thoon</rhyme>:</l>
        <l id="s4:l9" met="4" type="-+">Daer en ghebreckt Son' noch <rhyme label="d" type="f">Mane</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Noch ander licht by haer te <rhyme label="d" type="f" corresp="#s4:l9">stane</rhyme>.</l>
        <l id="s4:l11" met="2" type="+-">Heere Iesus <rhyme label="e" type="m">soet</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Gheeft ons dat eeuwigh <rhyme label="e" type="m" corresp="#s4:l11">goet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Prince <rhyme label="a" type="m">minioot</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wilt u met vlijt op <rhyme label="b" type="f">maken</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Bidden ende <rhyme label="b" type="f" corresp="#s5:l2">waken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Op dese vreught toch <rhyme label="c" type="m">let</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Cleyn ende <rhyme label="a" type="m" corresp="#s5:l1">groot</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Naer dese stadt wilt <rhyme label="b" type="f" corresp="#s5:l2">haken</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Die daer in <rhyme label="b" type="f" corresp="#s5:l2">gheraken</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Moeten zijn <rhyme label="c" type="m" corresp="#s5:l4">onbesmet</rhyme>:</l>
        <l id="s5:l9" met="4" type="-+">Zy zijn becleet met witte <rhyme label="d" type="f">zijde</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Loven den Heer' met herten <rhyme label="d" type="f" corresp="#s5:l9">blijde</rhyme>.</l>
        <l id="s5:l11" met="2" type="+-">Heere Iesus <rhyme label="e" type="m">soet</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Gheeft ons dat eeuwigh <rhyme label="e" type="m" corresp="#s5:l11">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
