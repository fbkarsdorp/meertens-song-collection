<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156500</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mijn allerliefste verheven</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEen hooger staet op <rhyme label="a" type="f">aerden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Noch loffelijcker <rhyme label="b" type="m">stant</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Als 't Huw'lijk groot van <rhyme label="a" type="f" corresp="#s1:l1">waerden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Int 't Paradijs <rhyme label="b" type="m" corresp="#s1:l2">geplant</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Geen vast noch soeter <rhyme label="b" type="m" corresp="#s1:l2">bant</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En worter oyt <rhyme label="c" type="f">gevonden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Twee Zieltjes t'saem <rhyme label="c" type="f" corresp="#s1:l6">verbonden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">O dat 's een vreught <rhyme label="b" type="m" corresp="#s1:l2">playsant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wie kan de trou <rhyme label="a" type="f">misprijsen</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Godt heeftse <rhyme label="b" type="m">in-gestelt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Elck moetse eer <rhyme label="a" type="f" corresp="#s2:l1">bewijsen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gelijck sijn woort <rhyme label="b" type="m" corresp="#s2:l2">vermelt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">U selven niet en <rhyme label="b" type="m" corresp="#s2:l2">quelt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wat wilt ghy <rhyme label="c" type="f">tegen-spreken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wie blijfter in <rhyme label="c" type="f" corresp="#s2:l6">gebreken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Natuer daer selfs na <rhyme label="b" type="m" corresp="#s2:l2">helt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'t Is noodigh om te <rhyme label="a" type="f">trouwen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Want door de <rhyme label="b" type="m">Maeghde-Staet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Kunt ghy niet <rhyme label="a" type="f" corresp="#s3:l1">onderhouwen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">'t Geslacht dat haest <rhyme label="b" type="m" corresp="#s3:l2">vergaet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">'t Huwlijck is Godes <rhyme label="b" type="m" corresp="#s3:l2">raet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">'t Zijn haters van de <rhyme label="c" type="f">menschen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dien niet na trouwen <rhyme label="c" type="f" corresp="#s3:l6">wenschen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Den mensch hier door <rhyme label="b" type="m" corresp="#s3:l2">bestaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Op aerd wort niet <rhyme label="a" type="f">gevonden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Daer meerder Liefd' <rhyme label="b" type="m">uyt-spruyt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Als uyt de soete <rhyme label="a" type="f" corresp="#s4:l1">gronden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Van 't Huwelijcks <rhyme label="b" type="m" corresp="#s4:l2">besluyt</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Siet onse waerde <rhyme label="b" type="m" corresp="#s4:l2">Bruyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hoe dat haer ooghjes <rhyme label="c" type="f">loncken</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Meer als gemeene <rhyme label="c" type="f" corresp="#s4:l6">voncken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Denckt wat of dit <rhyme label="b" type="m" corresp="#s4:l2">beduyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het zijn de Liefdes <rhyme label="a" type="f">banden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Pylaren van 't <rhyme label="b" type="m">gebouw</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Het zijn de vaste <rhyme label="a" type="f" corresp="#s5:l1">panden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De zegels van de <rhyme label="b" type="m" corresp="#s5:l2">trouw</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wanneer als man en <rhyme label="b" type="m" corresp="#s5:l2">vrouw</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In Liefde t'samen <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Godt sal sijn zegen <rhyme label="c" type="f" corresp="#s5:l6">geven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Geluckigh ick haer <rhyme label="b" type="m" corresp="#s5:l2">houw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Prins Bruydegom <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Wilt met u lieve <rhyme label="b" type="m">Bruyt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">In vreed en liefde <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Blust alle tweedracht <rhyme label="b" type="m" corresp="#s6:l2">uyt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Hier op met soet <rhyme label="b" type="m" corresp="#s6:l2">geluyt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Soo wenschen wy u <rhyme label="c" type="f">mede</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Veel soete vreught en <rhyme label="c" type="f" corresp="#s6:l6">vrede</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Den Hemel tot <rhyme label="b" type="m" corresp="#s6:l2">besluyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
