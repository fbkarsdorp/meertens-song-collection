<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188327</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Als ik mijn Phyllis kussen mag</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALs twee gelieven eens van <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Zyn t'zaam in 't Egtverbond <rhyme label="b" type="f">getreeden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Op 't Spoor der onbevlekte <rhyme label="a" type="m" corresp="#s1:l1">Min</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Geleid na 't Rigtsnoer van de <rhyme label="b" type="f" corresp="#s1:l2">reden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dan smaakt men 't zoet, het merg, en <rhyme label="a" type="m">Pit</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">De weelde en wellust, rein van <rhyme label="b" type="f">Zeeden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat in des Huwelyks liefde <rhyme label="a" type="m" corresp="#s2:l1">zit</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van God geteeld in 't Lusthof <rhyme label="b" type="f" corresp="#s2:l2">Eden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maar als een Paar door wellust <rhyme label="a" type="m">huwt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Of eigen baat, vol looze <rhyme label="b" type="f">streken</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">De Deugt en reine Liefde <rhyme label="a" type="m" corresp="#s3:l1">schuwt</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Vol wederzydze Ziels <rhyme label="b" type="f" corresp="#s3:l2">gebreken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dan is het een Spelonk van <rhyme label="a" type="m">druk</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Van akelig naberouw en <rhyme label="b" type="f">knagen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dan rekend' men voor een <rhyme label="a" type="m" corresp="#s4:l1">Geluk</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Door 's anders dood te zyn <rhyme label="b" type="f" corresp="#s4:l2">ontslagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maar daar standvaste deug staat <rhyme label="a" type="m">pal</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gelyk by dees opregte <rhyme label="b" type="f">Vrinden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Is men beschut by <rhyme label="a" type="m" corresp="#s5:l1">ongeval</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En 't Stormen van de Zee en <rhyme label="b" type="f" corresp="#s5:l2">Winden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
