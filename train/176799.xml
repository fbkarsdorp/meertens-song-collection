      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">In ons <rhyme label="a" type="m">ghemoet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Verheucht voor alle <rhyme label="b" type="f">dinghen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Laet ons nu <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Eenen Lof-sangh, gaen <rhyme label="b" type="f" corresp="#s1:l2">singhen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Den Vader <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Want hy is vol <rhyme label="d" type="f">ghenaden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Die metter <rhyme label="c" type="m" corresp="#s1:l5">spoet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ons rijcklijck doet <rhyme label="d" type="f" corresp="#s1:l6">versaden</rhyme>:</l>
        <l id="s1:l9" met="2" type="-+">Van hem <rhyme label="e" type="m">ghemeyn</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Wordt het ons al <rhyme label="f" type="f">ghegheven</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">‘t Zy groot of <rhyme label="e" type="m" corresp="#s1:l9">kleyn</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Wat ons dient in dit <rhyme label="f" type="f" corresp="#s1:l10">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Allmachtigh <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ghy zijt weerdigh om <rhyme label="b" type="f">peijsen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Om ‘t goede <rhyme label="a" type="m" corresp="#s2:l1">lot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Moetmen u eer <rhyme label="b" type="f" corresp="#s2:l2">bewijsen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ghy die ons <rhyme label="c" type="m">grof</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Hebt gheschapen <rhyme label="d" type="f">waerachtigh</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Van aerd’ en <rhyme label="c" type="m" corresp="#s2:l5">stof</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Door uwe handt seer <rhyme label="d" type="f" corresp="#s2:l6">krachtigh</rhyme>:</l>
        <l id="s2:l9" met="2" type="-+">End’ oock <rhyme label="e" type="m">vervult</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Heerlijck met allen <rhyme label="f" type="f">goeden</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Dat wy uyt <rhyme label="e" type="m" corresp="#s2:l9">schult</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Tot goet doen souden <rhyme label="f" type="f" corresp="#s2:l10">spoeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Noch boven <rhyme label="a" type="m">al</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gheeft gy ons <rhyme label="b" type="f">onghemeten</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">In ‘t aersche <rhyme label="a" type="m" corresp="#s3:l1">dal</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat wy drincken en <rhyme label="b" type="f" corresp="#s3:l2">eeten</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Het leven <rhyme label="c" type="m">fris</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En dat in dese <rhyme label="d" type="f">tijden</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ons noodt-druft <rhyme label="c" type="m" corresp="#s3:l5">is</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Gheeft ghy ons met <rhyme label="d" type="f" corresp="#s3:l6">verblijden</rhyme>:</l>
        <l id="s3:l9" met="2" type="-+">Plaets, Huys en <rhyme label="e" type="m">Hof</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">En al dat wy <rhyme label="f" type="f">useeren</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Daerom moet <rhyme label="e" type="m" corresp="#s3:l9">lof</rhyme></l>
        <l id="s3:l12" met="3" type="-+">En prijs, t’uwwaerts <rhyme label="f" type="f" corresp="#s3:l10">vermeeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ghenadigh <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wilt onse zielen <rhyme label="b" type="f">spijsen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Met uwe <rhyme label="a" type="m" corresp="#s4:l1">Leer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om oprecht te <rhyme label="b" type="f" corresp="#s4:l2">verrijsen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">In ‘t leven <rhyme label="c" type="m">jent</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat u meest doet <rhyme label="d" type="f">behaghen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Knechten ons <rhyme label="c" type="m" corresp="#s4:l5">sendt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Die ons u woordt <rhyme label="d" type="f" corresp="#s4:l6">voor-draghen</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">In rechter <rhyme label="e" type="m">moet</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Ons dat moghen <rhyme label="f" type="f">uyt-deelen</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Dat wy <rhyme label="e" type="m" corresp="#s4:l9">versaedt</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Worden aen onse <rhyme label="f" type="f" corresp="#s4:l10">zielen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">En wy <rhyme label="a" type="m">ghesont</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Mogen worden <rhyme label="b" type="f">bevonden</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Met daet en <rhyme label="a" type="m" corresp="#s5:l1">Mont</rhyme></l>
        <l id="s5:l4" met="3" type="-+">U goet daden <rhyme label="b" type="f" corresp="#s5:l2">verkonden</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">’t Gheloove <rhyme label="c" type="m">sterckt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In ons, wy swacke <rhyme label="d" type="f">vaten</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">End’ in ons <rhyme label="c" type="m" corresp="#s5:l5">werckt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met uwen Gheest der <rhyme label="d" type="f" corresp="#s5:l6">maten</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Dat wy <rhyme label="e" type="m">bequaem</rhyme>:</l>
        <l id="s5:l10" met="3" type="-+">Onse Lichaem <rhyme label="f" type="f">regeeren</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">En hy <rhyme label="e" type="m" corresp="#s5:l9">deuchtsaem</rhyme></l>
        <l id="s5:l12" met="3" type="-+">U weghen ons mach <rhyme label="f" type="f" corresp="#s5:l10">leeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">
          <rhyme label="a" type="m">Rechtveerdelijck</rhyme>
        </l>
        <l id="s6:l2" met="3" type="-+">Te leven en te <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Het eeuwigh <rhyme label="a" type="m" corresp="#s6:l1">rijck</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hier namaels te <rhyme label="b" type="f" corresp="#s6:l2">be-erven</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Als ‘t sal <rhyme label="c" type="m">vergaen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Al dat wy sien met <rhyme label="d" type="f">ooghen</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Dat wy dan <rhyme label="c" type="m" corresp="#s6:l5">staen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En met u volck <rhyme label="d" type="f" corresp="#s6:l6">verhooghen</rhyme>:</l>
        <l id="s6:l9" met="2" type="-+">Gheeft ons <rhyme label="e" type="m">verstant</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Dat wy ‘t eeuwighe <rhyme label="f" type="f">soecken</rhyme>,</l>
        <l id="s6:l11" met="2" type="+-">In ‘t Hemels <rhyme label="e" type="m" corresp="#s6:l9">Landt</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Verschijnen als de <rhyme label="f" type="f" corresp="#s6:l10">kloecken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">End’ <rhyme label="a" type="m">onbelet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Aen uwe Tafel <rhyme label="b" type="f">Eten</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Reyn <rhyme label="a" type="m" corresp="#s7:l1">onbesmet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Daer moghen zijn <rhyme label="b" type="f" corresp="#s7:l2">gheseten</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Voor u <rhyme label="c" type="m">aenschijn</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En wy daer moghen <rhyme label="d" type="f">drincken</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Den nieuwen <rhyme label="c" type="m" corresp="#s7:l5">wijn</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Die Christus daer sal <rhyme label="d" type="f" corresp="#s7:l6">schincken</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">O lieve <rhyme label="e" type="m">Vaer</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Komt ghy ons doch te <rhyme label="f" type="f">baten</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">Om zonden <rhyme label="e" type="m" corresp="#s7:l9">swaer</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">En wilt ons niet <rhyme label="f" type="f" corresp="#s7:l10">verlaten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Toont u <rhyme label="a" type="m">goetheyt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ons Armen vol <rhyme label="b" type="f">ellenden</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">‘t Onschuldiggh <rhyme label="a" type="m" corresp="#s8:l1">kleyt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Wilt uyt ghenaden <rhyme label="b" type="f" corresp="#s8:l2">senden</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Ons die sijn <rhyme label="c" type="m">naeckt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Berooft van allen <rhyme label="d" type="f">deughden</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Gheschickt ons <rhyme label="c" type="m" corresp="#s8:l5">maeckt</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Te verschijnen met <rhyme label="d" type="f" corresp="#s8:l6">vreughden</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Op deenen <rhyme label="e" type="m">dagh</rhyme>,</l>
        <l id="s8:l10" met="3" type="-+">Als u lief Kindt sal <rhyme label="f" type="f">komen</rhyme></l>
        <l id="s8:l11" met="2" type="+-">Sonder <rhyme label="e" type="m" corresp="#s8:l9">verdrach</rhyme>:</l>
        <l id="s8:l12" met="3" type="-+">T’ontfanghen alle <rhyme label="f" type="f" corresp="#s8:l10">vromen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Seer hoogh <rhyme label="a" type="m">vermaert</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Zy den name des <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Hemel end’ <rhyme label="a" type="m" corresp="#s9:l1">Aerd</rhyme>’,</l>
        <l id="s9:l4" met="3" type="-+">Moet hem prijsen met <rhyme label="b" type="f" corresp="#s9:l2">eeren</rhyme>?</l>
        <l id="s9:l5" met="2" type="-+">Ons ziel en <rhyme label="c" type="m">lijf</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">De wercken sijner <rhyme label="d" type="f">handen</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Man ende <rhyme label="c" type="m" corresp="#s9:l5">Wijf</rhyme>.</l>
        <l id="s9:l8" met="3" type="-+">Iongh, oudt, in alle <rhyme label="d" type="f" corresp="#s9:l6">Landen</rhyme>,</l>
        <l id="s9:l9" met="2" type="-+">U leven <rhyme label="e" type="m">lanck</rhyme>,</l>
        <l id="s9:l10" met="3" type="-+">Singht doch met ons te <rhyme label="f" type="f">samen</rhyme></l>
        <l id="s9:l11" met="2" type="+-">Los, prijs en <rhyme label="e" type="m" corresp="#s9:l9">danck</rhyme>,</l>
        <l id="s9:l12" met="3" type="-+">Sy onsen Vader, <rhyme label="f" type="f" corresp="#s9:l10">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
