<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184898</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <tags/>
    <melody>Comtesse de Corsol</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOm Iesus Godes <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Kom Heer van alle <rhyme label="a" type="m" corresp="#s1:l1">Goon</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Door wond mijn Ziel van <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Op dat ick vry van <rhyme label="c" type="m">sond'</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Uyt mijner Zielen <rhyme label="c" type="m" corresp="#s1:l4">grondt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">U vyerigh magh <rhyme label="b" type="f" corresp="#s1:l3">beminnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O ware <rhyme label="a" type="m">Zielen-rust</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">O bron van alle <rhyme label="a" type="m" corresp="#s2:l1">lust</rhyme>!</l>
        <l id="s2:l3" met="3" type="-+">O rijckdom der <rhyme label="b" type="f">genade</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Geeft dat ick om u <rhyme label="c" type="m">min</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En kennis al 't <rhyme label="c" type="m" corresp="#s2:l4">gewin</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Des Wereldts acht voor <rhyme label="b" type="f" corresp="#s2:l3">schade</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O volheydt alles <rhyme label="a" type="m">goedts</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">O Sonne mijn's <rhyme label="a" type="m" corresp="#s3:l1">gemoedts</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">O Iesu mijn <rhyme label="b" type="f">beminde</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">Send in mijn Ziel een <rhyme label="c" type="m">blick</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Uws klaerheyds, op dat <rhyme label="c" type="m" corresp="#s3:l4">ick</rhyme></l>
        <l id="s3:l6" met="3" type="-+">U soeckende magh <rhyme label="b" type="f" corresp="#s3:l3">vinden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O waerde <rhyme label="a" type="m">Bruydegom</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">O soete Iesu! <rhyme label="a" type="m" corresp="#s4:l1">kom</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Komt noyt genoegh <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Treckt my uyt 'sWereldts <rhyme label="c" type="m">slijck</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En doet my <rhyme label="c" type="m" corresp="#s4:l4">eeuwighlijck</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In vreughde by u <rhyme label="b" type="f" corresp="#s4:l3">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
