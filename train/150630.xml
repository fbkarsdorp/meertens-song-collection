    <melody>Psalm 128 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEluckigh boven <rhyme label="a" type="f">maten</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Meer dan oyt hert <rhyme label="b" type="m">bedocht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Oyt keele uyt kon <rhyme label="a" type="f" corresp="#s1:l1">laten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Oyt pen te voorschijn <rhyme label="b" type="m" corresp="#s1:l2">brocht</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die, door Godts vrees <rhyme label="c" type="f">bewogen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sijn hert ter Deuchde <rhyme label="d" type="m">geeft</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En voor eens yeders <rhyme label="c" type="f" corresp="#s1:l5">oogen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Oprecht Godtsdienstigh <rhyme label="d" type="m" corresp="#s1:l6">leeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat we’erwil kan u <rhyme label="a" type="f">drucken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wiens hert sich soo <rhyme label="b" type="m">bevindt</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Vw’ doen sal wel <rhyme label="a" type="f" corresp="#s2:l1">gelucken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In al wat ghy <rhyme label="b" type="m" corresp="#s2:l2">begint</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Vw’ werck wordt niet te <rhyme label="c" type="f">schanden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gelijckm’ in and’re <rhyme label="d" type="m">siet</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Den arbeydt uwer <rhyme label="c" type="f" corresp="#s2:l5">handen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Geeft u ’tverhoopt <rhyme label="d" type="m" corresp="#s2:l6">geniet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wel mooght ghy saligh <rhyme label="a" type="f">heeten</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">In alles hebt ghy’t <rhyme label="b" type="m">soet</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Met vreuchde gaet ghy <rhyme label="a" type="f" corresp="#s3:l1">eten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van ’trecht-gewonnen <rhyme label="b" type="m" corresp="#s3:l2">goedt</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Met vreuchde gaen de <rhyme label="c" type="f">leden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ter diep’ en sachter <rhyme label="d" type="m">rust</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Door geen <rhyme label="c" type="f" corresp="#s3:l5">bekommertheden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In’t slapen oyt <rhyme label="d" type="m" corresp="#s3:l6">ontrust</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Als (door Natuers <rhyme label="a" type="f">verblijden</rhyme>)</l>
        <l id="s4:l2" met="3" type="-+">Het oogh’ vermaeck <rhyme label="b" type="m">geschiedt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wanneer’t by <rhyme label="a" type="f" corresp="#s4:l1">Somer-tijden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den schoonen Wijn-stock <rhyme label="b" type="m" corresp="#s4:l2">siet</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Den Wijn-stock, die, <rhyme label="c" type="f">geladen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Met vrucht, door dicht <rhyme label="d" type="m">gevlecht</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Sijn telgen, rijck van <rhyme label="c" type="f" corresp="#s4:l5">bladen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Langs heel den huyse <rhyme label="d" type="m" corresp="#s4:l6">hecht</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Soo sal uw’ <rhyme label="a" type="f">hert-vrundinne</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De trouwst’ in allen <rhyme label="b" type="m">noodt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De lust-hoff van uw’ <rhyme label="a" type="f" corresp="#s5:l1">sinnen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Vw’ waerde <rhyme label="b" type="m" corresp="#s5:l2">bed-genoot</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">’tGeselschap van uw’ <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Door veelderhande <rhyme label="d" type="m">Deuchdt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Vw’hert de volte <rhyme label="c" type="f" corresp="#s5:l5">geven</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Van wenschelijcke <rhyme label="d" type="m" corresp="#s5:l6">vreuchdt</rhyme>:</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Soo salse door haer <rhyme label="a" type="f">zeden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Gevoechelijck <rhyme label="b" type="m">gelaet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Bescheyden heusche <rhyme label="a" type="f" corresp="#s6:l1">reden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Verstandt in alle <rhyme label="b" type="m" corresp="#s6:l2">daedt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En duysendt soete <rhyme label="c" type="f">dingen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die reyne Echt <rhyme label="d" type="m">bevat</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Vw’ hert toe komen <rhyme label="c" type="f" corresp="#s6:l5">bringen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Een rijcken vreuchden <rhyme label="d" type="m" corresp="#s6:l6">schat</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Als jong’ <rhyme label="a" type="f">olijven-boomen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die op een ry <rhyme label="b" type="m">geplant</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Mildt uyt der aerden <rhyme label="a" type="f" corresp="#s7:l1">komen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">In vet en vruchtbaer <rhyme label="b" type="m" corresp="#s7:l2">landt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Haer looffjen weeldigh <rhyme label="c" type="f">uyten</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En groenen meer en <rhyme label="d" type="m">meer</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En dag’lijx hooger <rhyme label="c" type="f" corresp="#s7:l5">spruyten</rhyme></l>
        <l id="s7:l8" met="3" type="-+">In sacht en groeybaer <rhyme label="d" type="m" corresp="#s7:l6">we’er</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Soo sullen (o! <rhyme label="a" type="f">genuchten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Eens herts voor Godt <rhyme label="b" type="m">oprecht</rhyme>!)</l>
        <l id="s8:l3" met="3" type="-+">De wettelijcke <rhyme label="a" type="f" corresp="#s8:l1">vruchten</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Der onbevleckter <rhyme label="b" type="m" corresp="#s8:l2">Echt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Vw’ Dochters en uw’ <rhyme label="c" type="f">Sonen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In ordentlijck <rhyme label="d" type="m">gelidt</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Vw’ disch rontsom <rhyme label="c" type="f" corresp="#s8:l5">bekroonen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Als ghy ter maeltijdt <rhyme label="d" type="m" corresp="#s8:l6">sit</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O! die van dat getal <rhyme label="a" type="f">is</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat soo recht-padigh <rhyme label="b" type="m">gaet</rhyme>!</l>
        <l id="s9:l3" met="3" type="-+">Wien Godt sijn Een en Al <rhyme label="a" type="f" corresp="#s9:l1">is</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En strack in deuchde <rhyme label="b" type="m" corresp="#s9:l2">staet</rhyme>!</l>
        <l id="s9:l5" met="3" type="-+">Wat quaedt kan u <rhyme label="c" type="f">bekrencken</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Die Godt en’t Goedt geeft <rhyme label="d" type="m">ste’e</rhyme>?</l>
        <l id="s9:l7" met="3" type="-+">By al uw’ doen en <rhyme label="c" type="f" corresp="#s9:l5">dencken</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Gaet Godes zegen <rhyme label="d" type="m" corresp="#s9:l6">me’e</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wat onheyl kondt ghy <rhyme label="a" type="f">schroomen</rhyme>?</l>
        <l id="s10:l2" met="3" type="-+">De Heer, de hooghste <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Vloeyt met sijn <rhyme label="a" type="f" corresp="#s10:l1">lieffde-stroomen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Mildadigh op u <rhyme label="b" type="m" corresp="#s10:l2">ne’er</rhyme>;</l>
        <l id="s10:l5" met="3" type="-+">Glanst met sijns goedtheydts <rhyme label="c" type="f">stralen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Van Syon op u <rhyme label="d" type="m">aff</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">En laet sijn gunst soo <rhyme label="c" type="f" corresp="#s10:l5">dalen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Als hys’ aen mensch oyt <rhyme label="d" type="m" corresp="#s10:l6">gaff</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O! Heyl niet om <rhyme label="a" type="f">beschrijven</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Van die de Deuchdt trouw <rhyme label="b" type="m">meent</rhyme>!</l>
        <l id="s11:l3" met="3" type="-+">Soo lang als met den <rhyme label="a" type="f" corresp="#s11:l1">Lijven</rhyme></l>
        <l id="s11:l4" met="3" type="-+">De Ziele blijft <rhyme label="b" type="m" corresp="#s11:l2">vereent</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Sal hy uw’ doen <rhyme label="c" type="f">aenschouwen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Ierusalems <rhyme label="d" type="m">geluck</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">En in haer schoon’ <rhyme label="c" type="f" corresp="#s11:l5">gebouwen</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Noyt wederwil off <rhyme label="d" type="m" corresp="#s11:l6">druck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">En als nu is <rhyme label="a" type="f">vervaren</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Des levens meeste <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En door den last der <rhyme label="a" type="f" corresp="#s12:l1">jaren</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Te met de leeff-lust <rhyme label="b" type="m" corresp="#s12:l2">slijt</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Soo staet tot nieuwe <rhyme label="c" type="f">vreuchde</rhyme></l>
        <l id="s12:l6" met="3" type="-+">De stoff al we’er <rhyme label="d" type="m">bereydt</rhyme>.</l>
        <l id="s12:l7" met="3" type="-+">(Ontbeert ghy noyt de <rhyme label="c" type="f" corresp="#s12:l5">Deuchde</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Gh’ontbeert noyt <rhyme label="d" type="m" corresp="#s12:l6">salicheydt</rhyme>.)</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">’tGeen ghy in u voelt <rhyme label="a" type="f">mind’ren</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Vindt ghy in and’ren <rhyme label="b" type="m">we’er</rhyme>:</l>
        <l id="s13:l3" met="3" type="-+">Vw’ stamme, door kindts <rhyme label="a" type="f" corresp="#s13:l1">kind’ren</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Verspreydt sich wijdt en <rhyme label="b" type="m" corresp="#s13:l2">veer</rhyme>;</l>
        <l id="s13:l5" met="3" type="-+">En als ghy nu uw’ <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Gereedt siet om te <rhyme label="d" type="m">vlien</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Sult ghy in uw’ <rhyme label="c" type="f" corresp="#s13:l5">na-neven</rhyme></l>
        <l id="s13:l8" met="3" type="-+">V selfs onsterff’lijck <rhyme label="d" type="m" corresp="#s13:l6">sien</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wat rester dat de <rhyme label="a" type="f">zegen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Op’t hooghste zy <rhyme label="b" type="m">gebracht</rhyme>?</l>
        <l id="s14:l3" met="3" type="-+">Hy die de reyne <rhyme label="a" type="f" corresp="#s14:l1">wegen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Vws deuchdts soo waerdigh <rhyme label="b" type="m" corresp="#s14:l2">acht</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Sal schicken dat uw’ <rhyme label="c" type="f">oogen</rhyme>.</l>
        <l id="s14:l6" met="3" type="-+">Tot aller tijdt en <rhyme label="d" type="m">ste’e</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Isr’el aenschouwen <rhyme label="c" type="f" corresp="#s14:l5">mogen</rhyme></l>
        <l id="s14:l8" met="3" type="-+">In lieffelijcken <rhyme label="d" type="m" corresp="#s14:l6">vre’e</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
