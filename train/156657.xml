<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156657</id>
    <date>
      <year-start>1770</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb lange tijd bemind</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">‘k Ging laest te <rhyme label="a" type="m">Monnikendam</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Myn vermaken met een <rhyme label="b" type="f">wandel</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Eer ik daer binnen <rhyme label="a" type="m" corresp="#s1:l1">kwam</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hoort wat ik daer <rhyme label="a" type="m" corresp="#s1:l1">vernam</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Her eerst daer ik aen <rhyme label="c" type="m">lag</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat was by Arie <rhyme label="d" type="f">buiten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Daer dronk ik met <rhyme label="c" type="m" corresp="#s1:l5">pleysier</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ienever Wyn en <rhyme label="c" type="m" corresp="#s1:l5">Bier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik ging wat verder <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">By Janus zonder <rhyme label="b" type="f">Minnen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ik ging wat verder <rhyme label="a" type="m" corresp="#s2:l1">voort</rhyme></l>
        <l id="s2:l4" met="3" type="-+">By Janus aen de <rhyme label="a" type="m" corresp="#s2:l1">Poort</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Daer dronk ik met <rhyme label="c" type="m">vermaek</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Een borrel vijf zes <rhyme label="d" type="f">zeven</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Al met een goede <rhyme label="c" type="m" corresp="#s2:l5">smaek</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Eer dat ik verder <rhyme label="c" type="m" corresp="#s2:l5">raek</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik trad de Stad waerd <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">By Chris in de <rhyme label="b" type="f">Eenhoorn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat was regt na zyn <rhyme label="a" type="m" corresp="#s3:l1">zin</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zoo als ik daer kwam <rhyme label="a" type="m" corresp="#s3:l1">in</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hy schonk reys in voor <rhyme label="c" type="m">myn</rhyme>;</l>
        <l id="s3:l6" met="3" type="-+">Een borrel klaere <rhyme label="d" type="f">Jannever</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Toen nog weer <rhyme label="c" type="m" corresp="#s3:l5">Brandewyn</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wy dronken zonder <rhyme label="c" type="m" corresp="#s3:l5">pyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Toen ging ik verder <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">By Kok weer <rhyme label="b" type="f">aengelegen</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Daer rusten ik wat <rhyme label="a" type="m" corresp="#s4:l1">uyt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Want ik kwam uyt de <rhyme label="a" type="m" corresp="#s4:l1">Schuyt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">By onze Kok ook <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Daer weer een sliert <rhyme label="d" type="f">genoomen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Doen ging ik zonder <rhyme label="c" type="m" corresp="#s4:l5">smart</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">By Gerrit in ‘t <rhyme label="c" type="m" corresp="#s4:l5">Roothart</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ik vroeg aen hem <rhyme label="a" type="m">faljant</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Of ik daer kon <rhyme label="b" type="f">logeeren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Hy antwoord myn ook <rhyme label="a" type="m" corresp="#s5:l1">dra</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hy zeide Koopman <rhyme label="a" type="m" corresp="#s5:l1">ja</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ik ging als Jong <rhyme label="c" type="m">gezel</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">De weyje Steeg <rhyme label="d" type="f">doortreden</rhyme></l>
        <l id="s5:l7" met="3" type="-+">By Jan Roelofs in de <rhyme label="c" type="m" corresp="#s5:l5">hel</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Daer ik trok ras de <rhyme label="c" type="m" corresp="#s5:l5">bel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die kwam daer met <rhyme label="a" type="m">accoord</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Weer met een Soopje <rhyme label="b" type="f">binnen</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Ik paste hem daer <rhyme label="a" type="m" corresp="#s6:l1">voort</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Een stuyver soo ‘t <rhyme label="a" type="m" corresp="#s6:l1">behoort</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">‘k Ging op de Haven <rhyme label="c" type="m">ras</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">In ‘t Prinsje nog eens <rhyme label="d" type="f">drinken</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Een Glaesje lekker <rhyme label="c" type="m" corresp="#s6:l5">bier</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Ik had dorst als met <rhyme label="c" type="m" corresp="#s6:l5">pleysier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Toen na de bonten <rhyme label="a" type="m">Os</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">By onze Moeder <rhyme label="b" type="f">Tryntje</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ik haelde daer ook <rhyme label="a" type="m" corresp="#s7:l1">strak</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Vier duytjes uyt myn <rhyme label="a" type="m" corresp="#s7:l1">zak</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Daer schonck zy myn voor <rhyme label="c" type="m">in</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Een lekker fris <rhyme label="d" type="f">rantzoentje</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Want Neering is myn <rhyme label="c" type="m" corresp="#s7:l5">zin</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Sy schonk myn nog eens <rhyme label="c" type="m" corresp="#s7:l5">in</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De Swaen ook niet <rhyme label="a" type="m">verby</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En roelof niet <rhyme label="b" type="f">vergeeten</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Daer rusten ik eens <rhyme label="a" type="m" corresp="#s8:l1">weer</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Ik zette myn wat <rhyme label="a" type="m" corresp="#s8:l1">neer</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">‘k Dronk met pleysier myn <rhyme label="c" type="m">Glas</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In zoete rust en <rhyme label="d" type="f">vreede</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Myn Geltje weer <rhyme label="c" type="m" corresp="#s8:l5">gepast</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">‘k Ontsloeg myn van die <rhyme label="c" type="m" corresp="#s8:l5">last</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ik keek eens na myn <rhyme label="a" type="m">zom</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">‘k Ging de brug nog <rhyme label="b" type="f">over</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Ik keek eens na myn <rhyme label="a" type="m" corresp="#s9:l1">zom</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ik ging by Dirk <rhyme label="a" type="m" corresp="#s9:l1">blom</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Die tapte myn weer <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Op schille weer een <rhyme label="d" type="f">Soopje</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Dat was regt na myn <rhyme label="c" type="m" corresp="#s9:l5">sin</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Ik wandelde weer <rhyme label="c" type="m" corresp="#s9:l5">heen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">‘k Ging weer op den <rhyme label="a" type="m">pad</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En zoo na ‘t Schaesje <rhyme label="b" type="f">treeden</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Dit tapte myn uyt ‘t <rhyme label="a" type="m" corresp="#s10:l1">Vat</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Een stuyver van dat <rhyme label="a" type="m" corresp="#s10:l1">nat</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Doen ging ik weer <rhyme label="c" type="m">plaizant</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Na het Suyden in <rhyme label="d" type="f">vreeden</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Doen verder na het <rhyme label="c" type="m" corresp="#s10:l5">Sant</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Dat ley wat byder <rhyme label="c" type="m" corresp="#s10:l5">hand</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Daer is in de <rhyme label="a" type="m">Kerkstraet</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Een krep nog <rhyme label="b" type="f">gelegen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Die tapte ook een <rhyme label="a" type="m" corresp="#s11:l1">maet</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Die Heeft myn ook <rhyme label="a" type="m" corresp="#s11:l1">verzaed</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">De doelen niet <rhyme label="c" type="m">verby</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Daer ik niet kan <rhyme label="d" type="f">verkeren</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Maer ‘t legt niet aen die <rhyme label="c" type="m" corresp="#s11:l5">zy</rhyme>:</l>
        <l id="s11:l8" met="3" type="-+">Daer dronk ik nog eens <rhyme label="c" type="m" corresp="#s11:l5">bly</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ik neem nu myn <rhyme label="a" type="m">afscheid</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Hier mee wil ik <rhyme label="b" type="f">besluiten</rhyme>.</l>
        <l id="s12:l3" met="3" type="-+">Ik neem nu myn <rhyme label="a" type="m" corresp="#s12:l1">afscheid</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Want het is heden <rhyme label="a" type="m" corresp="#s12:l1">tyd</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Ik ging de Poord weer <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">By roelof roelofs <rhyme label="d" type="f">treeden</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">‘k Naem afscheid met <rhyme label="c" type="m" corresp="#s12:l5">pleisier</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Hy tapte myn ook <rhyme label="c" type="m" corresp="#s12:l5">bier</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Vaer wel dan <rhyme label="a" type="m">burgerschaer</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Van Monnikendam <rhyme label="b" type="f">verheeven</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Vaer wel dan <rhyme label="a" type="m" corresp="#s13:l1">burgerschaer</rhyme></l>
        <l id="s13:l4" met="3" type="-+">In vreugd wat by <rhyme label="a" type="m" corresp="#s13:l1">malkaer</rhyme>:</l>
        <l id="s13:l5" met="3" type="-+">Ontfang dit aerdig <rhyme label="c" type="m">Lied</rhyme>:</l>
        <l id="s13:l6" met="3" type="-+">Het is met haest <rhyme label="d" type="f">geschreeven</rhyme>.</l>
        <l id="s13:l7" met="3" type="-+">Ik schel hier niemand <rhyme label="c" type="m" corresp="#s13:l5">niet</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Ik sluyt hier mee dit <rhyme label="c" type="m" corresp="#s13:l5">Lied</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
