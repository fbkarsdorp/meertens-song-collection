    <tags>
      <tag standard-name="gezongen tien geboden" translation="Ten Commandments song">gezongen tien geboden</tag>
    </tags>
    <melody>Tien geboden</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEft op v Hert, opent v <rhyme label="a" type="f">Oren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ghi hart volc en traech in <rhyme label="b" type="m">tverstaen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Wilt uwes Gods stemme nv <rhyme label="a" type="f" corresp="#s1:l1">hooren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">En zijn Gheboden gade <rhyme label="b" type="m" corresp="#s1:l2">slaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick ben, seyt hi v Godt en <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Die v hebbe ghemaect gantsch <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Van t Jock twelck v beswaerde <rhyme label="a" type="f" corresp="#s2:l1">seere</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Hebt gheen ander Godt neuen <rhyme label="b" type="m" corresp="#s2:l2">my</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laet v gheen Beelt maken noch <rhyme label="a" type="f">snijden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Van eenich dinck int eertsche <rhyme label="b" type="m">dal</rhyme></l>
        <l id="s3:l3" met="4" type="-+">So ghi die eert teenighen <rhyme label="a" type="f" corresp="#s3:l1">tijden</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Vwe Godt seer Zeloers sijn <rhyme label="b" type="m" corresp="#s3:l2">sal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ydelick sijnen naem <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Sult ghi niet nemen inden <rhyme label="b" type="m">mont</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Want hy sal niet onschuldich <rhyme label="a" type="f" corresp="#s4:l1">wese</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Dien die misbruyct teenigher <rhyme label="b" type="m" corresp="#s4:l2">stont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Arbeyt ses daghen en met <rhyme label="a" type="f">lusten</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Viert den seuensten, Gods woort <rhyme label="b" type="m">smaect</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Want Godt wilde op dien dach self <rhyme label="a" type="f" corresp="#s5:l1">rusten</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Vant Werck dat hi hadde <rhyme label="b" type="m" corresp="#s5:l2">ghemaect</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vader en Moeder sult ghi <rhyme label="a" type="f">eeren</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Op dat ghi lang in voorspoet <rhyme label="b" type="m">leeft</rhyme></l>
        <l id="s6:l3" met="4" type="-+">En op eerden seer moecht <rhyme label="a" type="f" corresp="#s6:l1">vermeeren</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Die v Godt tot een Herberch <rhyme label="b" type="m" corresp="#s6:l2">gheeft</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dootslach en toorne sult ghy <rhyme label="a" type="f">mijden</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Alle Oncusheyt, vliedt en <rhyme label="b" type="m">afbreect</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Steelt niet, al moest ghi ghebreck <rhyme label="a" type="f" corresp="#s7:l1">lijden</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Gheen Valsch ghetuychnisse en <rhyme label="b" type="m" corresp="#s7:l2">spreect</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Van begheeren ghi v ooc sult <rhyme label="a" type="f">wachten</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Des naesten Huys en Wijf nv <rhyme label="b" type="m">voort</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Nae sijn Knecht of vee wilt niet <rhyme label="a" type="f" corresp="#s8:l1">trachten</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Noch na al dat hem <rhyme label="b" type="m" corresp="#s8:l2">toebehoort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O God v Woort seer groot van <rhyme label="a" type="f">machte</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Luyde claerder dan eenich <rhyme label="b" type="m">Metael</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Gheeft ons die ghenaede ende <rhyme label="a" type="f" corresp="#s9:l1">crachte</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Om dit te houden <rhyme label="b" type="m" corresp="#s9:l2">altemael</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
