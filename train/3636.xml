    <melody>Psalm 033 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WEEST nu verhueght al ghy <rhyme label="a" type="f">oprechten</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In Godt den Heer' v al <rhyme label="b" type="m">verblydt</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Dat lof in den mond sijner <rhyme label="a" type="f" corresp="#s1:l1">knechten</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Is heerlick end schoon t'aller <rhyme label="b" type="m" corresp="#s1:l2">tyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Met harpen vol <rhyme label="c" type="f">snaeren</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Wilt nu <rhyme label="c" type="f" corresp="#s1:l5">openbaeren</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Sijnen prys end <rhyme label="d" type="m">eer'</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Dat Psalters end <rhyme label="e" type="f">kelen</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Nu singhen end <rhyme label="e" type="f" corresp="#s1:l8">spelen</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Onsen Godt end <rhyme label="d" type="m" corresp="#s1:l7">Heer'</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Singht den Heeren sijnde met <rhyme label="a" type="f">vrueghden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Nieuw' lofsangen lieflick en <rhyme label="b" type="m">soet</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Op den Psalter sijn lof, sijn <rhyme label="a" type="f" corresp="#s2:l1">deughden</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Spelet, end maeckt ghenoechte <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Want t'beuel des <rhyme label="c" type="f">Heeren</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Dat hy ons wil <rhyme label="c" type="f" corresp="#s2:l5">leeren</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Is recht end' <rhyme label="d" type="m">eerbaer</rhyme>.</l>
        <l id="s2:l8" met="3" type="+-">Sijn woorden end <rhyme label="e" type="f">wercken</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Sijn so men kan <rhyme label="e" type="f" corresp="#s2:l8">mercken</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Seker end gantsch <rhyme label="d" type="m" corresp="#s2:l7">waer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy bemindt seer tot allen <rhyme label="a" type="f">tyden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dat recht end die <rhyme label="b" type="m">gherechticheit</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">D' ęerd' is oock vol aen alle <rhyme label="a" type="f" corresp="#s3:l1">zyden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Van sijn groote <rhyme label="b" type="m" corresp="#s3:l2">barmherticheit</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Godt doer t'woort <rhyme label="c" type="f">ghepresen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Schiep, also wy <rhyme label="c" type="f" corresp="#s3:l5">lesen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Dat hemelsche <rhyme label="d" type="m">pleyn</rhyme>.</l>
        <l id="s3:l8" met="3" type="+-">Doer sijnen geest <rhyme label="e" type="f">krachtigh</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Maeckte hy <rhyme label="e" type="f" corresp="#s3:l8">waerachtigh</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">S'hemels krachten <rhyme label="d" type="m" corresp="#s3:l7">reyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Godt vergaederd' in't Męer te <rhyme label="a" type="f">saemen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Als in een vat dat water <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hy bewaerde dat seer <rhyme label="a" type="f" corresp="#s4:l1">bequaeme</rhyme></l>
        <l id="s4:l4" met="4" type="-+">In't afgrondische diepe <rhyme label="b" type="m" corresp="#s4:l2">dal</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Dies moet nu elck <rhyme label="c" type="f">vruchten</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Godt vreesen end <rhyme label="c" type="f" corresp="#s4:l5">duchten</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Die't al maeckt wt <rhyme label="d" type="m">niet</rhyme>.</l>
        <l id="s4:l8" met="3" type="+-">Dat een <rhyme label="e" type="f">jeghelicke</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Godt in krachten <rhyme label="e" type="f" corresp="#s4:l8">rycke</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Vrees' als hy dit <rhyme label="d" type="m" corresp="#s4:l7">siet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want al wat de Heer' heeft <rhyme label="a" type="f">gesproken</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Is haestelick gheweest <rhyme label="b" type="m">ghedaen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Sijn ghebodt heeft niemant <rhyme label="a" type="f" corresp="#s5:l1">gebroken</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Maer t'is geschiet van stonden <rhyme label="b" type="m" corresp="#s5:l2">aen</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Der heyden <rhyme label="c" type="f">raetslaeghen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Die hem niet <rhyme label="c" type="f" corresp="#s5:l5">behaeghen</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Hy seer haest <rhyme label="d" type="m">verstoort</rhyme>.</l>
        <l id="s5:l8" met="3" type="+-">Der volcken <rhyme label="e" type="f">ghedachten</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">End oock al haer <rhyme label="e" type="f" corresp="#s5:l8">krachten</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Verwerpt hy nu <rhyme label="d" type="m" corresp="#s5:l7">voort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maer die voorsichticheit des <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Doet sijn voornemen vast <rhyme label="b" type="m">bestaen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dat hy eens besluyt t'sijner <rhyme label="a" type="f" corresp="#s6:l1">eeren</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Sal sonder hinderingh' <rhyme label="b" type="m" corresp="#s6:l2">voortgaen</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">Wel saeligh moet <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">T'volck dat Godt met <rhyme label="c" type="f" corresp="#s6:l5">desen</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Houdt voor sijnen <rhyme label="d" type="m">Heer'</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Wel saeligh al <rhyme label="e" type="f">voren</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Sijnse die <rhyme label="e" type="f" corresp="#s6:l8">verkoren</rhyme></l>
        <l id="s6:l10" met="3" type="+-">Sijn, tot Godes <rhyme label="d" type="m" corresp="#s6:l7">eer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Godt siet nederwaerts vroegh end <rhyme label="a" type="f">spaede</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Wt den hemel in die węerelt <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die menschen all' slaet hy wel <rhyme label="a" type="f" corresp="#s7:l1">gaede</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Se sijn al voor sijn ooghen <rhyme label="b" type="m" corresp="#s7:l2">bloot</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Godt wt sijnen <rhyme label="c" type="f">throone</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Die vast' is end <rhyme label="c" type="f" corresp="#s7:l5">schoone</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Met vlijt' oock acht <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Op den mensch' <rhyme label="e" type="f">elendigh</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">End wat hier <rhyme label="e" type="f" corresp="#s7:l8">behendigh</rhyme>,</l>
        <l id="s7:l10" met="3" type="+-">Hem beweeght end <rhyme label="d" type="m" corresp="#s7:l7">leeft</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Want God doer sijn kracht hoogh' <rhyme label="a" type="f">verheuen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Maeckte des menschen hert' <rhyme label="b" type="m">alleyn</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dies weet hy allerbest daer <rhyme label="a" type="f" corresp="#s8:l1">neuen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Dat sijn wercken gaer sijn <rhyme label="b" type="m" corresp="#s8:l2">onreyn</rhyme>.</l>
        <l id="s8:l5" met="3" type="+-">Krijghsknechten met <rhyme label="c" type="f">hoopen</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">In stormen end <rhyme label="c" type="f" corresp="#s8:l5">loopen</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Konnen doer haer <rhyme label="d" type="m">macht</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Koningen noch <rhyme label="e" type="f">Helden</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">Helpen in den <rhyme label="e" type="f" corresp="#s8:l8">velden</rhyme>,</l>
        <l id="s8:l10" met="3" type="+-">Doer haer stercke <rhyme label="d" type="m" corresp="#s8:l7">kracht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hy dwaelt seer die daer meynt te <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Beschermt doer sijn pęerdt sterck end <rhyme label="b" type="m">groot</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Doer sijn kracht werdt niemant <rhyme label="a" type="f" corresp="#s9:l1">genesen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Noch geholpen wt strydt of <rhyme label="b" type="m" corresp="#s9:l2">noot</rhyme>.</l>
        <l id="s9:l5" met="3" type="+-">Maer Godt wilt <rhyme label="c" type="f">bewaeren</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Alle sijn <rhyme label="c" type="f" corresp="#s9:l5">dienaeren</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">Die hem vruchten <rhyme label="d" type="m">vroedt</rhyme>.</l>
        <l id="s9:l8" met="3" type="+-">End die in't <rhyme label="e" type="f">benauwen</rhyme>,</l>
        <l id="s9:l9" met="3" type="+-">Op hem vast <rhyme label="e" type="f" corresp="#s9:l8">betrauwen</rhyme>,</l>
        <l id="s9:l10" met="3" type="+-">End op sijn Woort <rhyme label="d" type="m" corresp="#s9:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hy sal wel bevryden haer <rhyme label="a" type="f">leuen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Als hen die doodt sal doen <rhyme label="b" type="m">geweldt</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">End sal hen oock haer spyse <rhyme label="a" type="f" corresp="#s10:l1">geuen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Als se met honger sijn <rhyme label="b" type="m" corresp="#s10:l2">gequelt</rhyme>.</l>
        <l id="s10:l5" met="3" type="+-">Dat ons hert' hem <rhyme label="c" type="f">pryse</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">End' eere <rhyme label="c" type="f" corresp="#s10:l5">bewyse</rhyme>,</l>
        <l id="s10:l7" met="3" type="+-">Met hoop' <rhyme label="d" type="m">onbesorght</rhyme>.</l>
        <l id="s10:l8" met="3" type="+-">Op hem wy ons <rhyme label="e" type="f">gronden</rhyme>,</l>
        <l id="s10:l9" met="3" type="+-">Hy is t'allen <rhyme label="e" type="f" corresp="#s10:l8">stonden</rhyme>,</l>
        <l id="s10:l10" met="3" type="+-">Onse schildt en <rhyme label="d" type="m" corresp="#s10:l7">borght</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">In hem sullen wy ons <rhyme label="a" type="f">verheughen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Van herten nae sijn Godtlick <rhyme label="b" type="m">Woort</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Want wy op sijn kracht end <rhyme label="a" type="f" corresp="#s11:l1">vermueghen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Alleyn staen, also dat <rhyme label="b" type="m" corresp="#s11:l2">behoort</rhyme>.</l>
        <l id="s11:l5" met="3" type="+-">O myn Godt <rhyme label="c" type="f">almachtigh</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">Maeckt ons doch <rhyme label="c" type="f" corresp="#s11:l5">deelachtigh</rhyme>.</l>
        <l id="s11:l7" met="3" type="+-">Vwer <rhyme label="d" type="m">goedicheit</rhyme>.</l>
        <l id="s11:l8" met="3" type="+-">Want wy op v <rhyme label="e" type="f">rusten</rhyme>,</l>
        <l id="s11:l9" met="3" type="+-">End hopen met <rhyme label="e" type="f" corresp="#s11:l8">lusten</rhyme>,</l>
        <l id="s11:l10" met="3" type="+-">In der <rhyme label="d" type="m" corresp="#s11:l7">eewicheit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
