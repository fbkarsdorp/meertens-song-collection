    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 068 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DAnckt Godt den Heer in <rhyme label="a" type="m">Eewicheyt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Looft, prijst end Eert syn <rhyme label="a" type="m" corresp="#s1:l1">Majesteyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">All'gaer in eenen <rhyme label="b" type="f">Stande</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dies dat hy altijdt is <rhyme label="c" type="m">bereydt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Met syne groote <rhyme label="c" type="m" corresp="#s1:l4">Vriend'lickheyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Te helpen Luyd' en <rhyme label="b" type="f" corresp="#s1:l3">Lande</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Syn goetheyt Eewich duert, en <rhyme label="d" type="m">t'gaet</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Aen allen Oorden syn <rhyme label="d" type="m" corresp="#s1:l7">Ghenaed'</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Hy gheeft u Wijff en <rhyme label="e" type="f">kinder</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Huys, Gelt en Goet in <rhyme label="f" type="m">overvloet</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Daer toe een Huysghesindt seer <rhyme label="f" type="m" corresp="#s1:l10">goet</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">End voeder voor de <rhyme label="e" type="f" corresp="#s1:l9">Rinder</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy all' die zijt int <rhyme label="a" type="m">Regiment</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Loofft, Dancket, Prijset, recht <rhyme label="a" type="m" corresp="#s2:l1">bekendt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Gods goetheyt <rhyme label="b" type="f">Overvloedich</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Want hy sterckt uwe handen <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">De Heer gheeft u oock <rhyme label="c" type="m" corresp="#s2:l4">All'gaer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Een Hert Cloeck end <rhyme label="b" type="f" corresp="#s2:l3">Vrymoedich</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Dat ghy met aller Macht en <rhyme label="d" type="m">Crafft</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">De Boosen, end haer Boosheyt <rhyme label="d" type="m" corresp="#s2:l7">straft</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Dies gheeftmen uwen <rhyme label="e" type="f">stande</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Oock Gelt en Goet, Huys ende <rhyme label="f" type="m">Eer</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Om te Beschutten altijdt <rhyme label="f" type="m" corresp="#s2:l10">meer</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Den Armen in den <rhyme label="e" type="f" corresp="#s2:l9">Lande</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hooch Prijset Godt ghy <rhyme label="a" type="m">Allegaer</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die met syn Woort Oprecht en <rhyme label="a" type="m" corresp="#s3:l1">Claer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daer Heyrschet en <rhyme label="b" type="f">Regeeren</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">End hebt Bevel beyd Arm en <rhyme label="c" type="m">Rijck</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">De Wet des HEEren, <rhyme label="c" type="m" corresp="#s3:l4">insgelijck</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Het recht Ghelooff te <rhyme label="b" type="f" corresp="#s3:l3">Leeren</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Alleen Gods Edel Woort so <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Laet dat u Troost en Sterckte <rhyme label="d" type="m" corresp="#s3:l7">zijn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">End alle u <rhyme label="e" type="f">Betrouwen</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">Dat Licht van alle Werelt <rhyme label="f" type="m">wijt</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Zijt ghy ghestelt in desen <rhyme label="f" type="m" corresp="#s3:l10">Tijdt</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Dies wilt den Gront vast <rhyme label="e" type="f" corresp="#s3:l9">bouwen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy alle die oock Christen <rhyme label="a" type="m">heet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Van syne groote <rhyme label="a" type="m" corresp="#s4:l1">vriendtlickheet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Danckt Godt van gantscher <rhyme label="b" type="f">herten</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Want hy is goet tot alder <rhyme label="c" type="m">Tijdt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">End helpt ghenaedich en <rhyme label="c" type="m" corresp="#s4:l4">Bevrijdt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van alle pijn en <rhyme label="b" type="f" corresp="#s4:l3">smerten</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Daeromme wilt u troosten <rhyme label="d" type="m">nu</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Dat hy soo goede Rust gheeft <rhyme label="d" type="m" corresp="#s4:l7">u</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Nae Jammer en veel <rhyme label="e" type="f">Lijden</rhyme>.</l>
        <l id="s4:l10" met="4" type="-+">Het wordt oock u Ghebedt end <rhyme label="f" type="m">Woort</rhyme></l>
        <l id="s4:l11" met="4" type="+-">Van Godt seer wel en ras <rhyme label="f" type="m" corresp="#s4:l10">verhoort</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Dies vrolick zijt <rhyme label="e" type="f" corresp="#s4:l9">altijden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Op Godt wil ick verlaten <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Wat boos mach my dan t'Een'gher <rhyme label="a" type="m" corresp="#s5:l1">Ty</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Gheschieden oft <rhyme label="b" type="f">benouwen</rhyme>?</l>
        <l id="s5:l4" met="4" type="-+">Van al de Werelt Boos en <rhyme label="c" type="m">wreet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Sond', Duyvel ende Doot oock <rhyme label="c" type="m" corresp="#s5:l4">meed'</rhyme>?</l>
        <l id="s5:l6" met="3" type="-+">Want ick daer noch sal <rhyme label="b" type="f" corresp="#s5:l3">schouwen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Myn Lust aen dese Boose <rhyme label="d" type="m">Schaer</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Die soo gheheel end <rhyme label="d" type="m" corresp="#s5:l7">allegaer</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Tot grimmicheyt haer <rhyme label="e" type="f">gheven</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Om Godts beminde <rhyme label="f" type="m">Huysghesind'</rhyme></l>
        <l id="s5:l11" met="4" type="+-">Te brenghen (en syn lieve <rhyme label="f" type="m" corresp="#s5:l10">Kindt</rhyme>)</l>
        <l id="s5:l12" met="3" type="-+">Om Lijve ende <rhyme label="e" type="f" corresp="#s5:l9">leven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Godt is myn Psalm, myn Heyl en <rhyme label="a" type="m">Macht</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Om niet (dit merckt, end wel <rhyme label="a" type="m" corresp="#s6:l1">betracht</rhyme>)</l>
        <l id="s6:l3" met="3" type="-+">Vergramt u niet ghy <rhyme label="b" type="f">Heeren</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Het gaet (so ickt ghedacht had) <rhyme label="c" type="m">my</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Van verde hoorden ick daer <rhyme label="c" type="m" corresp="#s6:l4">bly</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Een stemme ons te <rhyme label="b" type="f" corresp="#s6:l3">leeren</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Die Luyden even end <rhyme label="d" type="m">also</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Zijt allegader bly end <rhyme label="d" type="m" corresp="#s6:l7">Vro</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Die t'Recht gheloove <rhyme label="e" type="f">weeten</rhyme>.</l>
        <l id="s6:l10" met="4" type="-+">Wt alle Pijn, Vaer, Ancxt en <rhyme label="f" type="m">Noot</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Helpt ons doch onse HEEre <rhyme label="f" type="m" corresp="#s6:l10">Godt</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Dies wy hem Vader <rhyme label="e" type="f" corresp="#s6:l9">heeten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ick sal niet sterven noch <rhyme label="a" type="m">vergaen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Maer leven end altijdt <rhyme label="a" type="m" corresp="#s7:l1">bestaen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Godts Wercken te <rhyme label="b" type="f">verconden</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Offt ghy schoon thoornich plaghet <rhyme label="c" type="m">myn</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Den doot sal ick ontloopen <rhyme label="c" type="m" corresp="#s7:l4">fijn</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Nu ende t'allen <rhyme label="b" type="f" corresp="#s7:l3">stonden</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Want Godt de Heere die gheeft <rhyme label="d" type="m">my</rhyme></l>
        <l id="s7:l8" met="4" type="-+">Dat ick hem hier van herten <rhyme label="d" type="m" corresp="#s7:l7">bly</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Can Loff en Danck voor <rhyme label="e" type="f">draghen</rhyme>.</l>
        <l id="s7:l10" met="4" type="-+">Ghy zijt alleen myn <rhyme label="f" type="m">Salicheyt</rhyme>,</l>
        <l id="s7:l11" met="4" type="+-">O HEER tot aller tijdt <rhyme label="f" type="m" corresp="#s7:l10">bereyt</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">Helpt my wt allen <rhyme label="e" type="f" corresp="#s7:l9">Plaghen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De Werlt verwerpet <rhyme label="a" type="m">Openbaer</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Wat Godt de Heere aenneemt <rhyme label="a" type="m" corresp="#s8:l1">daer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">T'is Christo soo <rhyme label="b" type="f">verganghen</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">T'ghelooff verneemt sulcx altoos <rhyme label="c" type="m">meer</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Want aen den Cruyce is de <rhyme label="c" type="m" corresp="#s8:l4">HEER</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Voor all' de Werlt <rhyme label="b" type="f" corresp="#s8:l3">ghehanghen</rhyme>.</l>
        <l id="s8:l7" met="4" type="-+">Maer als hy weer Verresen <rhyme label="d" type="m">was</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Schoon over onse Sonden <rhyme label="d" type="m" corresp="#s8:l7">ras</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Die Coop geheel <rhyme label="e" type="f">besloten</rhyme>.</l>
        <l id="s8:l10" met="4" type="-+">Jae op dien Dach hield op die <rhyme label="f" type="m">Plaech</rhyme>.</l>
        <l id="s8:l11" met="4" type="+-">Dat heeft (diet geeren anders <rhyme label="f" type="m" corresp="#s8:l10">saech</rhyme>)</l>
        <l id="s8:l12" met="3" type="-+">Den Duyvel gaer <rhyme label="e" type="f" corresp="#s8:l9">verdrooten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Bid't Godt verheucht u metter <rhyme label="a" type="m">Daet</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Dat hy dit so gheschieden <rhyme label="a" type="m" corresp="#s9:l1">laet</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Wilt steedts daer aen <rhyme label="b" type="f">ghedincken</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">En dat hy keer <rhyme label="c" type="m">ghenadelijck</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Des Duyvels Aenslach <rhyme label="c" type="m" corresp="#s9:l4">Listelijck</rhyme>.</l>
        <l id="s9:l6" met="3" type="-+">Op dat alleen mach <rhyme label="b" type="f" corresp="#s9:l3">Clincken</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">O Christe dijne reyne <rhyme label="d" type="m">Leer</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">In alle Werelt Langs, hoe <rhyme label="d" type="m" corresp="#s9:l7">meer</rhyme>.</l>
        <l id="s9:l9" met="3" type="-+">O Godt wilt ons <rhyme label="e" type="f">Begnaden</rhyme>.</l>
        <l id="s9:l10" met="4" type="-+">Verciert den Feestdach t'Allen <rhyme label="f" type="m">Cant</rhyme></l>
        <l id="s9:l11" met="4" type="+-">Singt op dat best, die HEEre <rhyme label="f" type="m" corresp="#s9:l10">want</rhyme></l>
        <l id="s9:l12" met="3" type="-+">Hoed' ons voor allen <rhyme label="e" type="f" corresp="#s9:l9">schaden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Nu danckt den Heer end u <rhyme label="a" type="m">verblijdt</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Die also Vriend'lijck is <rhyme label="a" type="m" corresp="#s10:l1">altijdt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Singt hem van gantscher <rhyme label="b" type="f">Herten</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Wilt Loven oock en Dancken <rhyme label="c" type="m">seer</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Den Sone Jesum Christ de <rhyme label="c" type="m" corresp="#s10:l4">HEER</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Die ons van allen <rhyme label="b" type="f" corresp="#s10:l3">smerten</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Verlosset heeft al metter <rhyme label="d" type="m">Daet</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">End ons daer neven wt <rhyme label="d" type="m" corresp="#s10:l7">Ghenaedt</rhyme></l>
        <l id="s10:l9" met="3" type="-+">Syn Heyl'gen Gheest <rhyme label="e" type="f">ghegeven</rhyme>,</l>
        <l id="s10:l10" met="4" type="-+">Jae alles wat daer oock is <rhyme label="f" type="m">goet</rhyme>,</l>
        <l id="s10:l11" met="4" type="+-">Die lieve Heere Christus <rhyme label="f" type="m" corresp="#s10:l10">soet</rhyme></l>
        <l id="s10:l12" met="3" type="-+">Help ons int Eewich <rhyme label="e" type="f" corresp="#s10:l9">Leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
