<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183586</id>
    <date>
      <year-start>1774</year-start>
      <year-end>1774</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nadien uw goddelijkheid</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat hoort men nu al <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In 't Deenemarks <rhyme label="a" type="m" corresp="#s1:l1">gebied</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sal heugen dezen <rhyme label="b" type="f">Iaere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">De Koning wierd <rhyme label="c" type="m">verraen</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Het stuk kwam <rhyme label="b" type="f" corresp="#s1:l3">openbaere</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door twee Graeve <rhyme label="c" type="m" corresp="#s1:l4">gedaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nooit sag men booser <rhyme label="a" type="m">list</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zy zyn in 't stuk <rhyme label="a" type="m" corresp="#s2:l1">gemist</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Maer aers was dat <rhyme label="b" type="f">Ryke</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ten ondergang <rhyme label="c" type="m">geweest</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Belydenis gaf <rhyme label="b" type="f" corresp="#s2:l3">blyken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">'t Was niet vergeefs <rhyme label="c" type="m" corresp="#s2:l4">gevreest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Moeder <rhyme label="a" type="m">Koningin</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ontdekte het <rhyme label="a" type="m" corresp="#s3:l1">begin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Van die <rhyme label="b" type="f">verraderyen</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">De Koning gaf <rhyme label="c" type="m">gehoor</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">En ging het kwaed <rhyme label="b" type="f" corresp="#s3:l3">bestryden</rhyme>;</l>
        <l id="s3:l6" met="3" type="-+">Hy vond het regte <rhyme label="c" type="m" corresp="#s3:l4">spoor</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De twee Grave <rhyme label="a" type="m">gevat</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Byzonder yder <rhyme label="a" type="m" corresp="#s4:l1">sat</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En bekende de <rhyme label="b" type="f">stukke</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Veel andere raekte <rhyme label="c" type="m">mee</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">In swaere <rhyme label="b" type="f" corresp="#s4:l3">ongelukken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Gebangen daer ter <rhyme label="c" type="m" corresp="#s4:l4">stee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Elk een het stuk <rhyme label="a" type="m">vervloekt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De Regters <rhyme label="a" type="m" corresp="#s5:l1">onderzoekt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En gruwde van 't <rhyme label="b" type="f">aenhooren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Het was 'er op <rhyme label="c" type="m">gelaen</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Sy hadde t'saem <rhyme label="b" type="f" corresp="#s5:l3">gezwooren</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">'t Ryk stond onder te <rhyme label="c" type="m" corresp="#s5:l4">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Vonnissen syn <rhyme label="a" type="m">geveld</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Een Mensch 'er van <rhyme label="a" type="m" corresp="#s6:l1">ontstelt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Twee Edele Graeven <rhyme label="b" type="f">ryden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Na het Schavot daer <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Hoe dat op dese <rhyme label="b" type="f" corresp="#s6:l3">tyden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">De Iustitie <rhyme label="c" type="m" corresp="#s6:l4">geschied</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Regterhanden <rhyme label="a" type="m">syn</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Afgekapt met veel <rhyme label="a" type="m" corresp="#s7:l1">pyn</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">De Hoofde <rhyme label="b" type="f">insgelyken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Het Hart gehaelt uit 't <rhyme label="c" type="m">lyf</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Wie het siet an <rhyme label="b" type="f" corresp="#s7:l3">beswyken</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En door schrik worden <rhyme label="c" type="m" corresp="#s7:l4">styf</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De Hoofde op een <rhyme label="a" type="m">Staek</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gezet word tot een <rhyme label="a" type="m" corresp="#s8:l1">blaek</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Voor andere na <rhyme label="b" type="f">deese</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">De Rompe voort in 't <rhyme label="c" type="m">vier</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Gehakt het hart moet <rhyme label="b" type="f" corresp="#s8:l3">weesen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Gerukt uit 't lighaem <rhyme label="c" type="m" corresp="#s8:l4">schier</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Daer syn nu 't Grave <rhyme label="a" type="m">paer</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In 't gerigt van een <rhyme label="a" type="m" corresp="#s9:l1">schaer</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Van duysende <rhyme label="b" type="f">verslagen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Sy einde op 't <rhyme label="c" type="m">Schavot</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Tot schanden hunne <rhyme label="b" type="f" corresp="#s9:l3">dagen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Ag! wat een nare <rhyme label="c" type="m" corresp="#s9:l4">lot</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Verrader die het <rhyme label="a" type="m">Ryk</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Verraen hebt te <rhyme label="a" type="m" corresp="#s10:l1">gelyk</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En andre mee moest <rhyme label="b" type="f">sleepen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Gy syt na regt <rhyme label="c" type="m">gestraft</rhyme>;</l>
        <l id="s10:l5" met="3" type="+-">En aan uw snoode <rhyme label="b" type="f" corresp="#s10:l3">greepen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Is Schavots loon <rhyme label="c" type="m" corresp="#s10:l4">verschaft</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Men moet sig spiegelen <rhyme label="a" type="m">aen</rhyme>:</l>
        <l id="s11:l2" met="3" type="-+">De straf door u <rhyme label="a" type="m" corresp="#s11:l1">ontfaen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Sou 't kwaed ongestraft <rhyme label="b" type="f">blyven</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Dan kan geen <rhyme label="c" type="m">Koningryk</rhyme>:</l>
        <l id="s11:l5" met="3" type="+-">Of Staat ook meer <rhyme label="b" type="f" corresp="#s11:l3">beklyven</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Moet vallen te <rhyme label="c" type="m" corresp="#s11:l4">gelyk</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Gelyk men in dit <rhyme label="a" type="m">Lied</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Kan leezen 't is <rhyme label="a" type="m" corresp="#s12:l1">geschied</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">In Deenemarken <rhyme label="b" type="f">schoone</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Nu is de rust <rhyme label="c" type="m">hersteld</rhyme>:</l>
        <l id="s12:l5" met="3" type="+-">Men moet geen Vorsten <rhyme label="b" type="f" corresp="#s12:l3">hoonen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Door liefde of <rhyme label="c" type="m" corresp="#s12:l4">gewelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
