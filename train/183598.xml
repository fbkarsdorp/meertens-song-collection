<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183598</id>
    <date>
      <year-start>1774</year-start>
      <year-end>1774</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wie zag ooit schonere schone</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WIe sag ooit schoonder <rhyme label="a" type="f">schoone</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Dan mynen <rhyme label="b" type="m">Engelin</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Die 'k eeuwig min sal <rhyme label="a" type="f" corresp="#s1:l1">toonen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Opoffer ziel en <rhyme label="b" type="m" corresp="#s1:l2">zin</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Haar net beroemde <rhyme label="c" type="f">Leeden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Haar suyvere Deugt en <rhyme label="c" type="f" corresp="#s1:l5">Seeden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Waar over dat myn <rhyme label="d" type="m">hert</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Door min <rhyme label="d" type="m" corresp="#s1:l7">verwert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe blinken haare <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Haar net besneeden <rhyme label="b" type="m">Mont</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Haar Lippies vol <rhyme label="a" type="f" corresp="#s2:l1">vermoogen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die hebben my <rhyme label="b" type="m" corresp="#s2:l2">doorwont</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Als ik mogt kussies <rhyme label="c" type="f">plukken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En op haar Boesem <rhyme label="c" type="f" corresp="#s2:l5">drukken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Waar over dat myn <rhyme label="d" type="m">hert</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Door min <rhyme label="d" type="m" corresp="#s2:l7">verwert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Geen Abron die kan <rhyme label="a" type="f">haalen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De Nektar uyt haar <rhyme label="b" type="m">Mond</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dien ik soo meenig <rhyme label="a" type="f" corresp="#s3:l1">maalen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ia telkens <rhyme label="b" type="m" corresp="#s3:l2">ondervond</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Ik meen aan haar Borst te <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ia slegts het Leeven <rhyme label="c" type="f" corresp="#s3:l5">derven</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Toen schonk sy myn haer <rhyme label="d" type="m">Hert</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Door min <rhyme label="d" type="m" corresp="#s3:l7">verwert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat onrust is het <rhyme label="a" type="f">Leeven</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">Als men keert van de <rhyme label="b" type="m">min</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Het komt uw Deel te <rhyme label="a" type="f" corresp="#s4:l1">geeven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Opoffer ziel en <rhyme label="b" type="m" corresp="#s4:l2">zin</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Het wert al door min <rhyme label="c" type="f">gedreeven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wat men op aard siet <rhyme label="c" type="f" corresp="#s4:l5">leeven</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Het speelt, streeld met <rhyme label="d" type="m">gevleid</rhyme>:</l>
        <l id="s4:l8" met="2" type="-+">In <rhyme label="d" type="m" corresp="#s4:l7">Eerbaarheyd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Lente komt uw <rhyme label="a" type="f">toonen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Vrouw Venus met haar <rhyme label="b" type="m">kind</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De voogelen in de <rhyme label="a" type="f" corresp="#s5:l1">boomen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die hebben myn <rhyme label="b" type="m" corresp="#s5:l2">bemind</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hy dwaeld by dag en <rhyme label="c" type="f">nagten</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Verliest also syn <rhyme label="c" type="f" corresp="#s5:l5">kragten</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Waer dat ik ga of <rhyme label="d" type="m">rust</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Myn hert is nooit <rhyme label="d" type="m" corresp="#s5:l7">gerust</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wie sulk een lief moet <rhyme label="a" type="f">derven</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die nadert staag de <rhyme label="b" type="m">Dood</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">Ia duyzend Dooden <rhyme label="a" type="f" corresp="#s6:l1">sterven</rhyme>!</l>
        <l id="s6:l4" met="3" type="-+">Syn angst is veel te <rhyme label="b" type="m" corresp="#s6:l2">groot</rhyme>!</l>
        <l id="s6:l5" met="3" type="-+">Hy valt in <rhyme label="c" type="f">rasernyen</rhyme>!</l>
        <l id="s6:l6" met="3" type="-+">Hy mist geheel het <rhyme label="c" type="f" corresp="#s6:l5">vlyen</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Al van syn zoete <rhyme label="d" type="m">lief</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Die <rhyme label="d" type="m" corresp="#s6:l7">Honing-Dief</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
