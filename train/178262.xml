<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178262</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Psalm 006 Datheen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Heb dank, ô Heer der <rhyme label="a" type="f">Heeren</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Die Vorsten doet <rhyme label="a" type="f" corresp="#s1:l1">regeeren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Door uwe groote <rhyme label="b" type="m">kragt</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Gy, die Neêrlands <rhyme label="a" type="f" corresp="#s1:l1">Vorstinne</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Waardig om te <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Beschermt hebt door uw <rhyme label="b" type="m" corresp="#s1:l3">Magt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">ô God wil als voor <rhyme label="a" type="f">deezen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Haar dog genaadig <rhyme label="a" type="f" corresp="#s2:l1">weezen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ons Landt is in 't <rhyme label="b" type="m">gevaar</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Schoon de Boosdoenders <rhyme label="a" type="f" corresp="#s2:l1">woeden</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">ô Heer zyt haar ten <rhyme label="a" type="f" corresp="#s2:l1">Goeden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Zyt Haar een ligt zeer <rhyme label="b" type="m" corresp="#s2:l3">klaar</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'t Is XXXIV. <rhyme label="a" type="f">Jaaren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Goël; wat Blyde <rhyme label="a" type="f" corresp="#s3:l1">Maaren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat gy Haar heb <rhyme label="b" type="m">bewaart</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Tot Blydschap van Haar <rhyme label="a" type="f" corresp="#s3:l1">Oomme</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die draagt die PRUYSSE <rhyme label="a" type="f" corresp="#s3:l1">KROONE</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En die zyn Gramschap <rhyme label="b" type="m" corresp="#s3:l3">spaard</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maar hier zal ik van <rhyme label="a" type="f">scheyën</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik mogt te veel <rhyme label="a" type="f" corresp="#s4:l1">uitbreyën</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De tyd is vol <rhyme label="b" type="m">gevaar</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maar 'k wil nog een zugt <rhyme label="a" type="f" corresp="#s4:l1">opzenden</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Tot u myn Beede <rhyme label="a" type="f" corresp="#s4:l1">wenden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ay, blyft dog voor en <rhyme label="b" type="m" corresp="#s4:l3">naar</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">By de Vorst en <rhyme label="a" type="f">Vorstinne</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En al die hun <rhyme label="a" type="f" corresp="#s5:l1">Beminne</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zyn Spruite ook, tot <rhyme label="b" type="m">slot</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Ay geeft dat wy haast <rhyme label="a" type="f" corresp="#s5:l1">sluiten</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Van Binnen en van <rhyme label="a" type="f" corresp="#s5:l1">Buiten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">De Vreede, GROOTE <rhyme label="b" type="m" corresp="#s5:l3">GODT</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
