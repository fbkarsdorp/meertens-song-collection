    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Van de nachtegaal</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Nagtegael ik sie je <rhyme label="a" type="f">lopen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Uitg jou nessi ben jy <rhyme label="a" type="f" corresp="#s1:l1">gekomen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Jy gaet aen klare <rhyme label="b" type="m">fliet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daer u ook niemand <rhyme label="b" type="m" corresp="#s1:l3">ziet</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Daar u ook niemand <rhyme label="b" type="m" corresp="#s1:l3">ziet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Nagtegael ik hoor u <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Aen ‘t beekje gaenje <rhyme label="a" type="f" corresp="#s2:l1">springen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En jy doop jou bekkje <rhyme label="b" type="m">in</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Meent het is de beste <rhyme label="b" type="m" corresp="#s2:l3">wyn</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Meent het is de beste <rhyme label="b" type="m" corresp="#s2:l3">wyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Nagtegael waer is ‘t goet <rhyme label="a" type="f">woone</rhyme>;</l>
        <l id="s3:l2" met="4" type="+-">By diana onder de <rhyme label="a" type="f" corresp="#s3:l1">bomen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">By den fieren <rhyme label="b" type="m">Nagtegael</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Groot myn liefst <rhyme label="b" type="m" corresp="#s3:l3">duisent-mael</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Groot myn liefst <rhyme label="b" type="m" corresp="#s3:l3">duisent-mael</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wilt u hert aen tweje <rhyme label="a" type="f">kloven</rhyme>.</l>
        <l id="s4:l2" met="4" type="+-">Komt tot myn tegen de eggo <rhyme label="a" type="f" corresp="#s4:l1">over</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Slaje grillen uyt u <rhyme label="b" type="m">sinnen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Laet u Liefde varen <rhyme label="b" type="m" corresp="#s4:l3">heen</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Laet u Liefde varen <rhyme label="b" type="m" corresp="#s4:l3">heen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Laet de liefde heene <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ga maer weg met zulke <rhyme label="a" type="f" corresp="#s5:l1">narren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Die haer zo veel beelden <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Denkt dat hy de schoonste wil <rhyme label="b" type="m" corresp="#s5:l3">zyn</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Denkt dat hy de schoonste wil <rhyme label="b" type="m" corresp="#s5:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ga maer weg met jou trosse <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ik zal jou niet meer <rhyme label="a" type="f" corresp="#s6:l1">beminnen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Gy hebt geen oorsaek trots te <rhyme label="b" type="m">zyn</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">Kyk maer in jou hert <rhyme label="b" type="m" corresp="#s6:l3">erys</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Kyk maer in jou hert <rhyme label="b" type="m" corresp="#s6:l3">erys</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Nou wil ik myn weer <rhyme label="a" type="f">begeven</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">In ‘t Bosje wil ik <rhyme label="a" type="f" corresp="#s7:l1">zweven</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Tusschen bergen in diepen <rhyme label="b" type="m">dael</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Daer woont de schoonste <rhyme label="b" type="m" corresp="#s7:l3">Nagtegael</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Daer woont de schoonste <rhyme label="b" type="m" corresp="#s7:l3">Nagtegael</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
