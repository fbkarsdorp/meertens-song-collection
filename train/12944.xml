        <l id="s1:l5" met="4" type="+-">Daar hy loert om weÃªr te <rhyme label="b" type="f" corresp="#s1:l2">vinden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Die hy slapend' had <rhyme label="c" type="m">ge-knipt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Toen Diana, juist uit <rhyme label="d" type="f">jagen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Had aan Damon 't nare <rhyme label="d" type="f" corresp="#s1:l7">klagen</rhyme></l>
