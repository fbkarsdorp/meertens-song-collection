<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2972</id>
    <date>
      <year-start>1624</year-start>
      <year-end>1624</year-end>
    </date>
    <location>
      <orginal-name>[hoorn]</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Phoebus is lang over de zee</melody>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MAeck, mijn snaren, een <rhyme label="a" type="m">geklanck</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Dat de zinnen kan <rhyme label="b" type="f">vervreuchden</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Zing, mijn keel, een nieu <rhyme label="a" type="m" corresp="#s1:l1">gezanck</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Van een Deuchd de schoonst der <rhyme label="b" type="f" corresp="#s1:l2">Deuchden</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Van een Deuchdt die Ootmoêt <rhyme label="c" type="m">hiet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Daermen 't voor-beeldt klaer van <rhyme label="c" type="m" corresp="#s1:l5">ziet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">In hem diez' ons <rhyme label="c" type="m" corresp="#s1:l5">gebiedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hem, die nu der Heeren <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Diep in ootmoêt ne'er <rhyme label="b" type="f">gestegen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Heeft der eeren hooghste <rhyme label="a" type="m" corresp="#s2:l1">eer</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Door vernedering <rhyme label="b" type="f" corresp="#s2:l2">verkregen</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Al wie, zat van aerdtsche <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">'t Hemelsch zoeckt met hert en <rhyme label="c" type="m" corresp="#s2:l5">zin</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Moet dezen wegh oock <rhyme label="c" type="m" corresp="#s2:l5">in</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Door Ootmoêt en <rhyme label="a" type="m">Ned'richeydt</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Wordt de hooghste Hooght' <rhyme label="b" type="f">gevonden</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">('t Hert door hoogheydts lust <rhyme label="a" type="m" corresp="#s3:l1">verleydt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Blijft hier vast om laegh <rhyme label="b" type="f" corresp="#s3:l2">gebonden</rhyme>.)</l>
        <l id="s3:l5" met="4" type="+-">Hoe de leeder die hoogh <rhyme label="c" type="m">gaet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Lager in de aerde <rhyme label="c" type="m" corresp="#s3:l5">staet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Hoe minder datz' <rhyme label="c" type="m" corresp="#s3:l5">omslaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ootmoêt leyt niet, in, het <rhyme label="a" type="m">lijf</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Met onnoodigh quaedt te <rhyme label="b" type="f">quellen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Noch in 't waen-heyligh <rhyme label="a" type="m" corresp="#s4:l1">bedrijf</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Daer veel menschen Deuchd' in <rhyme label="b" type="f" corresp="#s4:l2">stellen</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Deuchdt hangt niet aen 's menschen <rhyme label="c" type="m">breyn</rhyme>:</l>
        <l id="s4:l6" met="4" type="+-">Maer Godts heyligh Woordt <rhyme label="c" type="m" corresp="#s4:l5">alleyn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Is Leers en Deuchdts <rhyme label="c" type="m" corresp="#s4:l5">Fonteyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Is oock d' Ootmoêt niet die <rhyme label="a" type="m">geldt</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Datmen (om in zondt te <rhyme label="b" type="f">blijven</rhyme>)</l>
        <l id="s5:l3" met="4" type="+-">Zich, als rechts, ootmoedigh <rhyme label="a" type="m" corresp="#s5:l1">stelt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En zoo zondigh gaet <rhyme label="b" type="f" corresp="#s5:l2">beschrijven</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Of men staêgh moest zijn in <rhyme label="c" type="m">zondt</rhyme>.</l>
        <l id="s5:l6" met="4" type="+-">Dit 's geen Deuchdt, maer <rhyme label="c" type="m" corresp="#s5:l5">leugen-vondt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Op Ootmoêts schijn <rhyme label="c" type="m" corresp="#s5:l5">gegrondt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ootmoêt leyt gants niet in <rhyme label="a" type="m">schijn</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Oock niet heel in slechte <rhyme label="b" type="f">kleeden</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Uyt het hert, daerz' in moet <rhyme label="a" type="m" corresp="#s6:l1">zijn</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Moetze stralen door de <rhyme label="b" type="f" corresp="#s6:l2">zeden</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Schijn-deuchdt krijght wel 't lijf tot <rhyme label="c" type="m">haer</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Maer Deuchdt, lijf en ziel te <rhyme label="c" type="m" corresp="#s6:l5">ga'er</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">In 't hert, en <rhyme label="c" type="m" corresp="#s6:l5">openbaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
