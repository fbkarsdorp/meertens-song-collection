    <melody>Iets moet ik u Laura vragen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
  <author>anoniem</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LEstmael als al mijn <rhyme label="a" type="f">gedachten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Door het brengen en het <rhyme label="a" type="f" corresp="#s1:l1">wachten</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Van het omme-gaende <rhyme label="b" type="m">glas</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Waren soetjens <rhyme label="c" type="f">ingenomen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Met geneucht, die stoutheyt <rhyme label="c" type="f" corresp="#s1:l4">komen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dede daerse niet en <rhyme label="b" type="m" corresp="#s1:l3">was</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ginck ik onse Cloris <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">By de dichte groene <rhyme label="a" type="f" corresp="#s2:l1">Linden</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Onse Cloris wiens <rhyme label="b" type="m">gelijck</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Onder al de <rhyme label="c" type="f">Herderinne</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Diemen waerdigh acht te <rhyme label="c" type="f" corresp="#s2:l4">minnen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Niet en is in onse <rhyme label="b" type="m" corresp="#s2:l3">wijck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doe begost ick te <rhyme label="a" type="f">verhalen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Hoe wy tot verscheyde <rhyme label="a" type="f" corresp="#s3:l1">malen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Speelden met haer soete <rhyme label="b" type="m">Naem</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En op haer gesontheydt <rhyme label="c" type="f">droncken</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">‘t Glaasje behoorde vol <rhyme label="c" type="f" corresp="#s3:l4">geschoncken</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat ons was so <rhyme label="b" type="m" corresp="#s3:l3">aengenaem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Daer na ginck ick haer mee <rhyme label="a" type="f">deelen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">‘t Geen ick hadd’ uyt de <rhyme label="a" type="f" corresp="#s4:l1">platteelen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Met een gauwe hant <rhyme label="b" type="m">gesakt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En behendelick <rhyme label="c" type="f">genomen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Als het Ooft was <rhyme label="c" type="f" corresp="#s4:l4">op-gekomen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">In veel schoteltjes <rhyme label="b" type="m" corresp="#s4:l3">gepackt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Onder ‘t praten en het <rhyme label="a" type="f">eten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dat my dee mijn tijdt <rhyme label="a" type="f" corresp="#s5:l1">vergeten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En te hebben scheen een <rhyme label="b" type="m">val</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Beurdet, dat ick (eer sy ‘t <rhyme label="c" type="f">keerde</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">En eer sy my kost af <rhyme label="c" type="f" corresp="#s5:l4">weeren</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Haer een lieve kus <rhyme label="b" type="m" corresp="#s5:l3">ontstal</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ach hoe soet was het <rhyme label="a" type="f">genaken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En het lieffelick <rhyme label="a" type="f" corresp="#s6:l1">aen-raecken</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Van haer sachte lipkens <rhyme label="b" type="m">teer</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Al de diensten van mij <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Sou ick tot belooningh <rhyme label="c" type="f" corresp="#s6:l4">gheven</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Om een kus te stelen <rhyme label="b" type="m" corresp="#s6:l3">weer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Herderinne dwinght u <rhyme label="a" type="f">tooren</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">En wilt u soo niet <rhyme label="a" type="f" corresp="#s7:l1">verstooren</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Want soo ‘k hier yet heb <rhyme label="b" type="m">misdaen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Om de misdaet te <rhyme label="c" type="f">versoeten</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Sal ick brenghen selfs de <rhyme label="c" type="f" corresp="#s7:l4">boeten</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Die ick hoor dat daer toe <rhyme label="b" type="m" corresp="#s7:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Onlangs als ick raet <rhyme label="a" type="f">begeerde</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Seyde my een <rhyme label="a" type="f" corresp="#s8:l1">Rechts-gheleerde</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Dat de Keyserlijcke <rhyme label="b" type="m">wet</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Die yet openbaerlijck <rhyme label="c" type="f">stalen</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Vier-vout sulcx weer te <rhyme label="c" type="f" corresp="#s8:l4">betalen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Tot een straffe had’ <rhyme label="b" type="m" corresp="#s8:l3">gheset</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Vier-mael weder u te <rhyme label="a" type="f">kussen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Soo; dat kan u gramschap <rhyme label="a" type="f" corresp="#s9:l1">blussen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Ben ick willigh en <rhyme label="b" type="m">bereydt</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Iae kan dit u niet <rhyme label="c" type="f">vernoeghen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Heel en al wil ick my <rhyme label="c" type="f" corresp="#s9:l4">voegen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Naer u lieve <rhyme label="b" type="m" corresp="#s9:l3">sinlijckheydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
