<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184852</id>
    <date>
      <year-start>1639</year-start>
      <year-end>1639</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Tien geboden</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">UErhoort o God mijn bidden <rhyme label="a" type="f">klachtigh</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">'s Levens Fonteyn daer alle <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Alleen uyt spruyt, behoet my <rhyme label="a" type="f" corresp="#s1:l1">krachtigh</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Voor alle stoute hooghe <rhyme label="b" type="m" corresp="#s1:l2">moet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick versaeck, O Heer, al den <rhyme label="a" type="f">ghenen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die op haer eyghen krachten <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En alle die daer souyden <rhyme label="a" type="f" corresp="#s2:l1">meenen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat wy van u 't goedt niet <rhyme label="b" type="m" corresp="#s2:l2">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy zijt alleen ghever van 't <rhyme label="a" type="f">goede</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Sonder u konnen wy niet <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Behoudt my Heer reyn van <rhyme label="a" type="f" corresp="#s3:l1">ghemoede</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat ick my nae u Woordt mach <rhyme label="b" type="m" corresp="#s3:l2">spoen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick ben vol zwackheydt en <rhyme label="a" type="f">ellenden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ach! Heere komt my doch te <rhyme label="b" type="m">baet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wilt my tot eenen Trooster <rhyme label="a" type="f" corresp="#s4:l1">senden</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Uwen Gheest, Heer mijn <rhyme label="b" type="m" corresp="#s4:l2">toeverlaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick ben, O Heere, zwaer <rhyme label="a" type="f">bestreden</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Van mijn eygen vleesch nacht en <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En door den Vyandt boos van <rhyme label="a" type="f" corresp="#s5:l1">zeden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Helpt, Heer, dat ick verwinnen <rhyme label="b" type="m" corresp="#s5:l2">mach</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Voor onkuysheydt wilt my <rhyme label="a" type="f">bewaren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Voor onschamel ghesicht <rhyme label="b" type="m">altoos</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Behoedt my, O Heer, al mijn <rhyme label="a" type="f" corresp="#s6:l1">Iaren</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Oock voor een onreyn leven <rhyme label="b" type="m" corresp="#s6:l2">boos</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Suypen, brassen, dees qua <rhyme label="a" type="f">manieren</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Alle overdaedt van my <rhyme label="b" type="m">went</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Wilt my in matigheydt <rhyme label="a" type="f" corresp="#s7:l1">regieren</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">O Heer, blijft my altijdt <rhyme label="b" type="m" corresp="#s7:l2">ontrent</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Een herte stout en <rhyme label="a" type="f">opgheblasen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Keert doch, O Heer, altijdt van <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Behoedt my voor hoovaerdigh <rhyme label="a" type="f" corresp="#s8:l1">rasen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Dat ick voor u ootmoedigh <rhyme label="b" type="m" corresp="#s8:l2">zy</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Voor een tonge valsch, boos en <rhyme label="a" type="f">schadigh</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Vol achterklaps en leughens <rhyme label="b" type="m">fel</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Die sijnen Naesten schendt <rhyme label="a" type="f" corresp="#s9:l1">ghestadigh</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Och Heer, bewaert my daer voor <rhyme label="b" type="m" corresp="#s9:l2">wel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Onvriendelijcke stuere <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Of een bitter wesens <rhyme label="b" type="m">begin</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Een twistigh hert als den <rhyme label="a" type="f" corresp="#s10:l1">verstoorden</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Bewaert my Heer voor sulck een <rhyme label="b" type="m" corresp="#s10:l2">sin</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Bewaert my doch tot allen <rhyme label="a" type="f">tijden</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Voor lichtvaerdigh gheselschap, <rhyme label="b" type="m">Heer</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Leert my de goddeloosen <rhyme label="a" type="f" corresp="#s11:l1">mijden</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Die uwe Wet vesmaden <rhyme label="b" type="m" corresp="#s11:l2">seer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Verlost my Heer van die <rhyme label="a" type="f">oproerders</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Die oordeelen onschuldigh <rhyme label="b" type="m">bloet</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Van Secte-makers en <rhyme label="a" type="f" corresp="#s12:l1">vervoerders</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Die ons smeecken met woorden <rhyme label="b" type="m" corresp="#s12:l2">soet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Bedroch en list laet van my <rhyme label="a" type="f">blijven</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Met alle qua gheveynsden <rhyme label="b" type="m">schijn</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Oock alle die twisten en <rhyme label="a" type="f" corresp="#s13:l1">kijven</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Leert my die schouwen als <rhyme label="b" type="m" corresp="#s13:l2">fenijn</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">O Heer, wilt my altijdt <rhyme label="a" type="f">beschermen</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Voor alle sonden groot en <rhyme label="b" type="m">kleen</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">En wilt u over my <rhyme label="a" type="f" corresp="#s14:l1">ontfermen</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Bevrijdt my van't eeuwigh <rhyme label="b" type="m" corresp="#s14:l2">gheween</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Edel Princier wilt doch <rhyme label="a" type="f">verhooren</rhyme></l>
        <l id="s15:l2" met="4" type="-+">My, u Schepsel van aerd en <rhyme label="b" type="m">stof</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Wilt u over my niet <rhyme label="a" type="f" corresp="#s15:l1">verstooren</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">V komt alleen toe prijs en <rhyme label="b" type="m" corresp="#s15:l2">lof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
