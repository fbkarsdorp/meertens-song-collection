<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>129553</id>
    <date>
      <year-start>1708</year-start>
      <year-end>1708</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>O Holland schoon gij leeft in vrede</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Liefhebbers blijf een weynig <rhyme label="a" type="m">staan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ick moet uw wat <rhyme label="b" type="f">vereeren</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Van d' Sardammers Maagts   hoort <rhyme label="a" type="m" corresp="#s1:l1">aan</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En van haar dragt en <rhyme label="b" type="f" corresp="#s1:l2">kleeren</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Hoe dat sy dag’lijks proncke <rhyme label="c" type="m">gaan</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Over de straat fraay <rhyme label="c" type="m" corresp="#s1:l5">opgedaan</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">In ‘t ciere van haar <rhyme label="d" type="f">leeden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Haar meeste tyt <rhyme label="d" type="f" corresp="#s1:l7">besteeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een onder muts hebben s’ op ‘t <rhyme label="a" type="m">hooft</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met alderhande <rhyme label="b" type="f">dieren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Een strooyen hoet, heel fyn <rhyme label="a" type="m" corresp="#s2:l1">geklooft</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al na de nieuwste <rhyme label="b" type="f" corresp="#s2:l2">swieren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Een Kamerdoeckse muts   met <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die wel geblaauwt   na haar <rhyme label="c" type="m" corresp="#s2:l5">verstant</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Een ooryser met <rhyme label="d" type="f">spelde</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wie souw daar niet van <rhyme label="d" type="f" corresp="#s2:l7">melde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een doeck heel fyn al om haar <rhyme label="a" type="m">hals</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Bepeerelt en <rhyme label="b" type="f">besteecken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Oock wel net gevouwen <rhyme label="a" type="m" corresp="#s3:l1">als</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Gesteeven en <rhyme label="b" type="f" corresp="#s3:l2">gestreecken</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Twee swarte plysters,   hups en <rhyme label="c" type="m">fijn</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Die moete ook aan ‘t voorhooft <rhyme label="c" type="m" corresp="#s3:l5">zyn</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Daar aan kan men <rhyme label="d" type="f">beschouwen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Die garen wille <rhyme label="d" type="f" corresp="#s3:l7">trouwen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Kroplap   had ick vergete <rhyme label="a" type="m">schier</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En die met silvere <rhyme label="b" type="f">haken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met roode koorden na de <rhyme label="a" type="m" corresp="#s4:l1">swier</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En passament   Syze <rhyme label="b" type="f" corresp="#s4:l2">maken</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Roo bloed kraalen met een <rhyme label="c" type="m">slot</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Schicken sy haar cierlyk <rhyme label="c" type="m" corresp="#s4:l5">op</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Met geblanckette <rhyme label="d" type="f">wange</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Om d' Jongmans te <rhyme label="d" type="f" corresp="#s4:l7">vange</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De borstrok fyn en die van <rhyme label="a" type="m">brat</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En nog meer mooye <rhyme label="b" type="f">kleere</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Met een schorteldoeck   aen ‘t <rhyme label="a" type="m" corresp="#s5:l1">gat</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat veele niet mag <rhyme label="b" type="f" corresp="#s5:l2">gebeure</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Van boove lyf sijn zy heel <rhyme label="c" type="m">fijn</rhyme>:</l>
        <l id="s5:l6" met="4" type="-+">Moet met een veter geregen <rhyme label="c" type="m" corresp="#s5:l5">zijn</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Ook onder de borstrock, wil <rhyme label="d" type="f">wete</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">‘t Hemt dient niet <rhyme label="d" type="f" corresp="#s5:l7">vergete</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dan zynze van bove lyf heel <rhyme label="a" type="m">fyn</rhyme>:</l>
        <l id="s6:l2" met="3" type="-+">Een Syde schort met <rhyme label="b" type="f">plooyen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">En een roode rock moet daar <rhyme label="a" type="m" corresp="#s6:l1">zyn</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Om de Jonckmans te <rhyme label="b" type="f" corresp="#s6:l2">nooyen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Twee swarte kouse, fijn en <rhyme label="c" type="m">net</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Daar me syn zy braaf <rhyme label="c" type="m" corresp="#s6:l5">opgezet</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Kleurt   of swarte <rhyme label="d" type="f">hantschoene</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Sy aan haar hande <rhyme label="d" type="f" corresp="#s6:l7">doene</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Al in haar roode rock, hoort <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hebbe zy een zak, wilt <rhyme label="b" type="f">weete</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Als gy u hand daar in laat <rhyme label="a" type="m" corresp="#s7:l1">gaan</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Daar vint gy wat te <rhyme label="b" type="f" corresp="#s7:l2">eete</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Haveren Gord, daar zuyker <rhyme label="c" type="m">door</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Sy houde dat als haar <rhyme label="c" type="m" corresp="#s7:l5">trizoor</rhyme></l>
        <l id="s7:l7" met="3" type="-+">- O, die verdwaalde   <rhyme label="d" type="f">todden</rhyme>   -</l>
        <l id="s7:l8" met="3" type="-+">Om daar blank van te <rhyme label="d" type="f" corresp="#s7:l7">worden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Des Sondags dan zijn sy heel <rhyme label="a" type="m">net</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Als Vrouwe van den <rhyme label="b" type="f">Lande</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Een boek met zilverwerk <rhyme label="a" type="m" corresp="#s8:l1">beset</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Gouwe ringen aan de <rhyme label="b" type="f" corresp="#s8:l2">hande</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Daar mede gaanze na de <rhyme label="c" type="m">Kerck</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Daar sy af maken weynig <rhyme label="c" type="m" corresp="#s8:l5">werck</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">Maar met een Jongman <rhyme label="d" type="f">spansseere</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Dat is na haar <rhyme label="d" type="f" corresp="#s8:l7">begeere</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Elck hebbe sy een tuyg heel <rhyme label="a" type="m">kant</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Van silverwerck <rhyme label="b" type="f">swaare</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Het is de moode van dat <rhyme label="a" type="m" corresp="#s9:l1">lant</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">‘t Staat wel ter deege <rhyme label="b" type="f" corresp="#s9:l2">raare</rhyme>:</l>
        <l id="s9:l5" met="4" type="-+">Een speldekusse met een <rhyme label="c" type="m">schee</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Aan de schaar een silvere ketting <rhyme label="c" type="m" corresp="#s9:l5">mee</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Muyle en ook dunne <rhyme label="d" type="f">schoene</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Sy aan haar voete <rhyme label="d" type="f" corresp="#s9:l7">doene</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Als sy dan weder koome <rhyme label="a" type="m">thuys</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En dan tot haarder <rhyme label="b" type="f">bate</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Soo leest men wat in ‘t boeck <rhyme label="a" type="m" corresp="#s10:l1">quansuys</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Maar straks weer op de <rhyme label="b" type="f" corresp="#s10:l2">strate</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Soo na de Molenbuert of den <rhyme label="c" type="m">Dam</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Met Joris, Piet of <rhyme label="c" type="m" corresp="#s10:l5">Abraham</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Symen, Gerret of <rhyme label="d" type="f">Douwe</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Daer sy me Kermis <rhyme label="d" type="f" corresp="#s10:l7">Houwe</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Daar gaanse dan seer net en <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Met een Jonkman <rhyme label="b" type="f">spansseere</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">De Speelman moet daar ook by <rhyme label="a" type="m" corresp="#s11:l1">zijn</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Om dat sy dansse <rhyme label="b" type="f" corresp="#s11:l2">leere</rhyme>.</l>
        <l id="s11:l5" met="4" type="-+">Men singt en speelt daar lustig <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">De Wyn loopt haar als   in de <rhyme label="c" type="m" corresp="#s11:l5">kop</rhyme>:</l>
        <l id="s11:l7" met="3" type="-+">Als het gaat op een <rhyme label="d" type="f">scheyden</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Moet mense thuys <rhyme label="d" type="f" corresp="#s11:l7">geleyden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
