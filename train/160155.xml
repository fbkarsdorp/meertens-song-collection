<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160155</id>
    <date>
      <year-start>1712</year-start>
      <year-end>1712</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Op een tijd op een tijd</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Maakje klaer, maakje <rhyme label="a" type="m">klaar</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">De Kallefs kop is <rhyme label="a" type="m" corresp="#s1:l1">gaar</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Ei, hoor hem eensjes <rhyme label="b" type="f">kooken</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Mijn maagje danst op dat <rhyme label="c" type="m">geluid</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">De kop die wil de ketel <rhyme label="c" type="m" corresp="#s1:l4">uit</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ik heb hem al <rhyme label="b" type="f" corresp="#s1:l3">gerooken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dat ’s ‘er een, dat ’s ‘er <rhyme label="a" type="m">een</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Ter waereld schoonder <rhyme label="a" type="m" corresp="#s2:l1">geen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat yder moet <rhyme label="b" type="f">belyen</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Ontgin hem maer eens   heen en <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s2:l5" met="4" type="-+">En denk, dat uit een anders <rhyme label="c" type="m" corresp="#s2:l4">leer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Goed riemen is te <rhyme label="b" type="f" corresp="#s2:l3">snyen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Was ‘er maar, was 'er <rhyme label="a" type="m">maar</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Een fret of <rhyme label="a" type="m" corresp="#s3:l1">avegaar</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Om d’ oogen uit te <rhyme label="b" type="f">booren</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Maer deese lepel is ook <rhyme label="c" type="m">goed</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Hou op je tafelbord of <rhyme label="c" type="m" corresp="#s3:l4">hoed</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Kan u ’t gezigt <rhyme label="b" type="f" corresp="#s3:l3">bekooren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dit ’s zijn tong, dit ’s zijn <rhyme label="a" type="m">tong</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daer hy muzyk mee <rhyme label="a" type="m" corresp="#s4:l1">zong</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Terwijl zijn makkers <rhyme label="b" type="f">dansten</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Wat dunkje van dat <rhyme label="c" type="m">kaakebeen</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">Daer zit de beste knap   om <rhyme label="c" type="m" corresp="#s4:l4">heen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die wy ons leeven <rhyme label="b" type="f" corresp="#s4:l3">schransten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wie zou niet, wie zou <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Als hy ’t verhemelt <rhyme label="a" type="m" corresp="#s5:l1">ziet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Een stukje daer van <rhyme label="b" type="f">wenssen</rhyme>?</l>
        <l id="s5:l4" met="4" type="-+">Wat zyn de harssens <rhyme label="c" type="m">hagelwit</rhyme>!</l>
        <l id="s5:l5" met="4" type="-+">Zie of ’er ook een kei in <rhyme label="c" type="m" corresp="#s5:l4">zit</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Als in de meeste <rhyme label="b" type="f" corresp="#s5:l3">mensen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Blank en mals, blank en <rhyme label="a" type="m">mals</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Is hy, gelyk de <rhyme label="a" type="m" corresp="#s6:l1">hals</rhyme></l>
        <l id="s6:l3" met="3" type="-+">En prammen   van een <rhyme label="b" type="f">meisje</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Men roep vry op de klieren <rhyme label="c" type="m">schrik</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">En tart de schoonste <rhyme label="c" type="m" corresp="#s6:l4">zwezerik</rhyme>:</l>
        <l id="s6:l6" met="3" type="-+">Ik bidje proef een <rhyme label="b" type="f" corresp="#s6:l3">reisje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Eêle kost, eêle <rhyme label="a" type="m">kost</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Uit Spanje vers <rhyme label="a" type="m" corresp="#s7:l1">gelost</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Geen koppen op kan <rhyme label="b" type="f">weegen</rhyme>!</l>
        <l id="s7:l4" met="4" type="-+">Indien Spekjan   de lugt maer <rhyme label="c" type="m">rook</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Hy zou gewis zyn lap en <rhyme label="c" type="m" corresp="#s7:l4">pook</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Aen deese kop <rhyme label="b" type="f" corresp="#s7:l3">verveegen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">d’ Indiaan, d’ <rhyme label="a" type="m">Indiaan</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Liet al zyn kruiden <rhyme label="a" type="m" corresp="#s8:l1">staen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Indien hy meê mogt <rhyme label="b" type="f">eeten</rhyme>:</l>
        <l id="s8:l4" met="4" type="-+">Hy schonk aen ons zijn <rhyme label="c" type="m">googheltas</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Zyn piessang en zyn <rhyme label="c" type="m" corresp="#s8:l4">annanas</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Zou hy wel haest <rhyme label="b" type="f" corresp="#s8:l3">vergeeten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Gans en kip, gans en <rhyme label="a" type="m">kip</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Kapoen en <rhyme label="a" type="m" corresp="#s9:l1">watersnip</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wie zou meer na je <rhyme label="b" type="f">taalen</rhyme>?</l>
        <l id="s9:l4" met="4" type="-+">Geen pluimgedierte, wild of <rhyme label="c" type="m">tam</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Geen osse tong, geen <rhyme label="c" type="m" corresp="#s9:l4">haanekam</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Mag by de koppen <rhyme label="b" type="f" corresp="#s9:l3">haalen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Hy is op, hy is <rhyme label="a" type="m">op</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Die schoonder kallefs <rhyme label="a" type="m" corresp="#s10:l1">kop</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En in ons lyf <rhyme label="b" type="f">begraaven</rhyme>:</l>
        <l id="s10:l4" met="4" type="-+">Kom, laet ons met de koele <rhyme label="c" type="m">Wyn</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Zijn uitvaart nu gedagtig <rhyme label="c" type="m" corresp="#s10:l4">zijn</rhyme></l>
        <l id="s10:l6" met="3" type="-+">En onse keeltjes <rhyme label="b" type="f" corresp="#s10:l3">laaven</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Beste maat, beste <rhyme label="a" type="m">maat</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Ey, hoor eens hoe hy <rhyme label="a" type="m" corresp="#s11:l1">blaat</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Om muskadelle <rhyme label="b" type="f">droppen</rhyme>!</l>
        <l id="s11:l4" met="4" type="-+">Al ben ik aen mijn tweede <rhyme label="c" type="m">fles</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Kom doen we nog een teug, vyf, <rhyme label="c" type="m" corresp="#s11:l4">ses</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Ter eeren van de <rhyme label="b" type="f" corresp="#s11:l3">koppen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">O hoe zoet, o hoe <rhyme label="a" type="m">zoet</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Verheugd de wijn het <rhyme label="a" type="m" corresp="#s12:l1">bloed</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Der vriendelyke <rhyme label="b" type="f">snaaken</rhyme>!</l>
        <l id="s12:l4" met="4" type="-+">Maer hoe, wat hoor ik voor <rhyme label="c" type="m">gewoel</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Of druipt’ er   iemand door de <rhyme label="c" type="m" corresp="#s12:l4">stoel</rhyme>?</l>
        <l id="s12:l6" met="3" type="-+">My dunkt, de sporten <rhyme label="b" type="f" corresp="#s12:l3">kraaken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">’t Is geen nood, ’t is geen <rhyme label="a" type="m">nood</rhyme>:</l>
        <l id="s13:l2" met="3" type="-+">De man is nog niet <rhyme label="a" type="m" corresp="#s13:l1">dood</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Hy staet weer op zijn <rhyme label="b" type="f">beenen</rhyme>.</l>
        <l id="s13:l4" met="4" type="-+">Maer zagt, wie is ’er buiten <rhyme label="c" type="m">aâm</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">En spreekt ’er woorden van een <rhyme label="c" type="m" corresp="#s13:l4">vaâm</rhyme>?</l>
        <l id="s13:l6" met="3" type="-+">Ik hoor soo byster <rhyme label="b" type="f" corresp="#s13:l3">steenen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Laet ons gaen, laet ons <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Het heerschop   is <rhyme label="a" type="m" corresp="#s14:l1">voldaen</rhyme></l>
        <l id="s14:l3" met="3" type="-+">En ’t maantje schynt nog <rhyme label="b" type="f">helder</rhyme>:</l>
        <l id="s14:l4" met="4" type="-+">Wij hebben al te mael de <rhyme label="c" type="m">vragt</rhyme>.</l>
        <l id="s14:l5" met="4" type="-+">Nu vrienden, ‘k wensje goeden <rhyme label="c" type="m" corresp="#s14:l4">nagt</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Maer val niet in die <rhyme label="b" type="f" corresp="#s14:l3">kelder</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Is dit feest, is dit <rhyme label="a" type="m">feest</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Vol snaakery <rhyme label="a" type="m" corresp="#s15:l1">geweest</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Tot geen verwijt kan ’t <rhyme label="b" type="f">strekken</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Want, na de tael van <rhyme label="c" type="m">Diogeen</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Zo is de waereld maar <rhyme label="c" type="m" corresp="#s15:l4">alleen</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Een groote kooi vol <rhyme label="b" type="f" corresp="#s15:l3">gekken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
