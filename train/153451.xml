<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>153451</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Cette cruelle</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">AL wat het leven heeft <rhyme label="a" type="f">ontfangen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Van God, dat is met levens <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Begaeft, en soeckt met groot <rhyme label="a" type="f" corresp="#s1:l1">verlangen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat noyt sijn vonck werdt <rhyme label="b" type="m" corresp="#s1:l2">uyt-geblust</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Dies gaet het <rhyme label="c" type="f">Paren</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Door een Echte <rhyme label="d" type="m">min</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">En wil <rhyme label="c" type="f" corresp="#s1:l5">vergaren</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Dat hem in't <rhyme label="d" type="m" corresp="#s1:l6">begin</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Eerst was geschapen <rhyme label="d" type="m" corresp="#s1:l6">in</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Aen-siet het Pluym-gediert <rhyme label="a" type="f">behendigh</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Het soeckt seer vlugh een <rhyme label="b" type="m">weder-gae</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">En 't Vee op 't Landt houd sich <rhyme label="a" type="f" corresp="#s2:l1">ellendigh</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Als 't is alleen, jae acht het <rhyme label="b" type="m" corresp="#s2:l2">schae</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">De Schubbe <rhyme label="c" type="f">Vinnen</rhyme></l>
        <l id="s2:l6" met="2" type="-+">In de <rhyme label="d" type="m">Pekel-plas</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Sietmen door 't <rhyme label="c" type="f" corresp="#s2:l5">minnen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Ia hoe kout het <rhyme label="d" type="m" corresp="#s2:l6">was</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Gesont en wel te <rhyme label="d" type="m" corresp="#s2:l6">pas</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Kan de Natuur in alles <rhyme label="a" type="f">wercken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Oock in het ongevoeligh <rhyme label="b" type="m">hout</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">Sal dan de mensch dit stil <rhyme label="a" type="f" corresp="#s3:l1">aen-mercken</rhyme>?</l>
        <l id="s3:l4" met="4" type="-+">En blijven altijdt <rhyme label="b" type="m" corresp="#s3:l2">ongetrouwt</rhyme>?</l>
        <l id="s3:l5" met="2" type="-+">Neen, dat te <rhyme label="c" type="f">prijsen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Heeft een slecht <rhyme label="d" type="m">bescheyt</rhyme>;</l>
        <l id="s3:l7" met="2" type="-+">Godt den <rhyme label="c" type="f" corresp="#s3:l5">al-wijsen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Heeft voor eens <rhyme label="d" type="m" corresp="#s3:l6">geseyt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">U zaet sy <rhyme label="d" type="m" corresp="#s3:l6">uyt-gebreyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dit heeft ons Bruydegom <rhyme label="a" type="f">geweten</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Wanneer hy socht sijn Lieve <rhyme label="b" type="m">Bruyt</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Hoe dickmael keeck hy door de <rhyme label="a" type="f" corresp="#s4:l1">spleten</rhyme>?</l>
        <l id="s4:l4" met="4" type="-+">En quamse dan eens tot hem <rhyme label="b" type="m" corresp="#s4:l2">uyt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Met hem te <rhyme label="c" type="f">praten</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Lieve wat een <rhyme label="d" type="m">vreught</rhyme>!</l>
        <l id="s4:l7" met="2" type="-+">Of niet de <rhyme label="c" type="f" corresp="#s4:l5">straten</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Waren mee <rhyme label="d" type="m" corresp="#s4:l6">verheught</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Ghy-lieden dencken <rhyme label="d" type="m" corresp="#s4:l6">meught</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wel Nieuw-getrouwde, nu Godts <rhyme label="a" type="f">zegen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">U beyden toe-gebeden <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Gedenckt daer aen in al u <rhyme label="a" type="f" corresp="#s5:l1">wegen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En oock aen u <rhyme label="b" type="m" corresp="#s5:l2">beloftenis</rhyme>;</l>
        <l id="s5:l5" met="2" type="-+">Gedenckt doch <rhyme label="c" type="f">heden</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Aen u trouwe <rhyme label="d" type="m">plicht</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Gedenckt de <rhyme label="c" type="f" corresp="#s5:l5">reden</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Die ghy in het <rhyme label="d" type="m" corresp="#s5:l6">licht</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Spraeckt voor Godes <rhyme label="d" type="m" corresp="#s5:l6">aen-gesicht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Stelt u Godts Wet en wil voor <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Begint met hem, en gaet soo <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ey vreest den Heer nae u <rhyme label="a" type="f" corresp="#s6:l1">vermogen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En draeght u vroomlijck nae sijn <rhyme label="b" type="m" corresp="#s6:l2">Woordt</rhyme>;</l>
        <l id="s6:l5" met="2" type="-+">Geeft dan <rhyme label="c" type="f">malkander</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Oock terstont <rhyme label="d" type="m">gehoor</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Als d'een den <rhyme label="c" type="f" corresp="#s6:l5">ander</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Leydt in 't rechte <rhyme label="d" type="m" corresp="#s6:l6">spoor</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En gaet int goede <rhyme label="d" type="m" corresp="#s6:l6">voor</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daer toe wensch ick u Godes <rhyme label="a" type="f">zegen</rhyme>;</l>
        <l id="s7:l2" met="4" type="-+">Hy zegene u voort en <rhyme label="b" type="m">voort</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">Hy leer u sijn gebodt en <rhyme label="a" type="f" corresp="#s7:l1">wegen</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Oprecht beleven nae sijn <rhyme label="b" type="m" corresp="#s7:l2">Woordt</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">Dat ghy meugt <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s7:l6" met="2" type="-+">D'Een tot d'anders <rhyme label="d" type="m">vreught</rhyme>.</l>
        <l id="s7:l7" met="2" type="-+">Dat elck mach <rhyme label="c" type="f" corresp="#s7:l5">geven</rhyme></l>
        <l id="s7:l8" met="2" type="-+">'t Geen den aer <rhyme label="d" type="m" corresp="#s7:l6">verheught</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">In liefd' en ware <rhyme label="d" type="m" corresp="#s7:l6">deught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
