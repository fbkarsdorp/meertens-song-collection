    <melody>Onze vader in hemelrijk</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vader ons in <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dit roep de Werelt <rhyme label="a" type="m" corresp="#s1:l1">alghelijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hoe wel sy in zonden <rhyme label="b" type="m">altoos</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Leven in haer lusten <rhyme label="b" type="m" corresp="#s1:l3">boos</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Teghen ‘s Heerene gheboden <rhyme label="c" type="m">mis</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wisten sy wie haer Vader <rhyme label="c" type="m" corresp="#s1:l5">is</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Christus gheeft selven klaer <rhyme label="a" type="m">bescheyt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Het volck die niet doen dat hy <rhyme label="a" type="m" corresp="#s2:l1">seyt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Den Duyvel dat is uwen <rhyme label="b" type="m">Vaer</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Nae sijnen wille doet ghy <rhyme label="b" type="m" corresp="#s2:l3">klaer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Oock sonder Godt inde <rhyme label="c" type="m">werelt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ephesien twee Paulus ‘t <rhyme label="c" type="m" corresp="#s2:l5">vermelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die sullen bidden-Vader <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Moeten ghehoorsaem Kinderen <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Die hem van alle quaet <rhyme label="b" type="m">af-scheyt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En roept niet aen soo Paulus <rhyme label="b" type="m" corresp="#s3:l3">seyt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat zijn Sonen, Dochters <rhyme label="c" type="m">Christi</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Oock recht Eerf-ghenamen daer <rhyme label="c" type="m" corresp="#s3:l5">by</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die hem ghelijck een Kindt <rhyme label="a" type="m">verneert</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Soo Christus selven heeft <rhyme label="a" type="m" corresp="#s4:l1">gheleert</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Van nieus ghebooren uyt den <rhyme label="b" type="m">Gheest</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Nieu Creatuyren soo men <rhyme label="b" type="m" corresp="#s4:l3">leest</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Want niemant Christum heet een <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dan door den Heyl’ghen Gheestes <rhyme label="c" type="m" corresp="#s4:l5">leer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Vaders lieve kKinderen <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Die Erf-ghenamen sullen <rhyme label="a" type="m" corresp="#s5:l1">zijn</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hebben sijn Wet in ‘t hert <rhyme label="b" type="m">gheprent</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Soo vast datmens daer niet af <rhyme label="b" type="m" corresp="#s5:l3">went</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">‘s Morgens, avonts, dach ende <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Bidden Vader om nieuwe <rhyme label="c" type="m" corresp="#s5:l5">kracht</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Alsoo hy is in ‘t <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Moet men met hert en sin <rhyme label="a" type="m" corresp="#s6:l1">ghelijck</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Keeren tot den ghenaden <rhyme label="b" type="m">throon</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Door kracht van sijn beminden <rhyme label="b" type="m" corresp="#s6:l3">Soon</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Bidden in Gheest en waerheyt <rhyme label="c" type="m">weert</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Want Vader sulck bidders <rhyme label="c" type="m" corresp="#s6:l5">begheert</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Salmen heylighen ‘s Vaders <rhyme label="a" type="m">naem</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Men moet geschickt zijn heel <rhyme label="a" type="m" corresp="#s7:l1">bequaem</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Een heylich leven <rhyme label="b" type="m">onbevleckt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Anders ‘t ghebedt niet op en <rhyme label="b" type="m" corresp="#s7:l3">treckt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Niet onreyns in ‘t hert zy <rhyme label="c" type="m">ghespoort</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Want Vader anders niet <rhyme label="c" type="m" corresp="#s7:l5">verhoort</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dus moet men met een reyn <rhyme label="a" type="m">ghemoet</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Neerstigh vallen Vader te <rhyme label="a" type="m" corresp="#s8:l1">voet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">‘t Hert en ghedachten moet daer <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Gheheylighet uw’ name <rhyme label="b" type="m" corresp="#s8:l3">zy</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Leeft hoe de Ouders alle <rhyme label="c" type="m">ghelijck</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Hem heylich eeren in sijn <rhyme label="c" type="m" corresp="#s8:l5">Rijck</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Salmen bidden U Rijck <rhyme label="a" type="m">toekoom</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En daer op wachten sonder <rhyme label="a" type="m" corresp="#s9:l1">schroom</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Soo moet men daer gheschickt toe <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Met een Godtzaligh wesen <rhyme label="b" type="m" corresp="#s9:l3">fijn</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Datmen haest en wacht met <rhyme label="c" type="m">verlangh</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Nae die toe-komst des Heeren <rhyme label="c" type="m" corresp="#s9:l5">strangh</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Salmen bidden om Vaders <rhyme label="a" type="m">wil</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Soo maeckt daer teghen gheen <rhyme label="a" type="m" corresp="#s10:l1">gheschil</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Wy moeten tot allen <rhyme label="b" type="m">termijn</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Onsen wil stellen in den <rhyme label="b" type="m" corresp="#s10:l3">sijn</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Niemant sijn eyghen wil doch <rhyme label="c" type="m">leeft</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Maer al dat Vader gheseyt <rhyme label="c" type="m" corresp="#s10:l5">heeft</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Niet-overtreden sijn <rhyme label="a" type="m">Ghebodt</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Op dat men met hem niet en <rhyme label="a" type="m" corresp="#s11:l1">spot</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Anders en heeft het woort gheen <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">U wille gheschiet hier wel op <rhyme label="b" type="m" corresp="#s11:l3">acht</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Op Aerden als in <rhyme label="c" type="m">Hemelrijck</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Woorden en werck moet zijn <rhyme label="c" type="m" corresp="#s11:l5">ghelijck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Te doen Vaders wil en <rhyme label="a" type="m">bevel</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Als d’Enghelen in den <rhyme label="a" type="m" corresp="#s12:l1">Hemel</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Ondersoeckt doch de Schrift tot <rhyme label="b" type="m">baet</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Houtse in ghedachten vroech en <rhyme label="b" type="m" corresp="#s12:l3">laet</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">En daer oock wel na leven <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Smaeckt en proeft soo de wil ons <rhyme label="c" type="m" corresp="#s12:l5">Vaer</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Salmen bidden om daghelijcx <rhyme label="a" type="m">broot</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Dat en zijn gheen Rijckdommen <rhyme label="a" type="m" corresp="#s13:l1">groot</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">En trachten nae schandich <rhyme label="b" type="m">ghewin</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">En stellen daer dan aen het <rhyme label="b" type="m" corresp="#s13:l3">sin</rhyme>.</l>
        <l id="s13:l5" met="4" type="-+">Valdt rijckdom, doet Davidt <rhyme label="c" type="m">vermaen</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Soo stelt u herte daer niet <rhyme label="c" type="m" corresp="#s13:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Want die daer willen worden <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Die vallen ellendich in <rhyme label="a" type="m" corresp="#s14:l1">strijck</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Men moet mildt zijn en gheven <rhyme label="b" type="m">veel</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Op hope tot een Hemels <rhyme label="b" type="m" corresp="#s14:l3">deel</rhyme>,</l>
        <l id="s14:l5" met="4" type="-+">Want Vader gheeft en neemt oock <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">’t Is prijckel om ghebruycken <rhyme label="c" type="m" corresp="#s14:l5">wel</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Soeckt voor al Godes rijcke <rhyme label="a" type="m">ghy</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Met sijn gherechticheyt daer <rhyme label="a" type="m" corresp="#s15:l1">by</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Het Hemels broot dit wel <rhyme label="b" type="m">betracht</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Daermen om biddet dach en <rhyme label="b" type="m" corresp="#s15:l3">nacht</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Dat zijn Gods gaven <rhyme label="c" type="m">menichfout</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Ghesucht uyt den Hemel <rhyme label="c" type="m" corresp="#s15:l5">ghedout</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Dit gheeft Vader alle daghen <rhyme label="a" type="m">vers</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Die ‘t Gheestich smaeckt, wort het niet <rhyme label="a" type="m" corresp="#s16:l1">wers</rhyme></l>
        <l id="s16:l3" met="4" type="-+">Hy wil daerom ghebeden <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Met neersticheyt t’allen <rhyme label="b" type="m" corresp="#s16:l3">termijn</rhyme>,</l>
        <l id="s16:l5" met="4" type="-+">Avonts en morghens laet en <rhyme label="c" type="m">vroegh</rhyme>,</l>
        <l id="s16:l6" met="4" type="-+">Hy gheeft dan sijn Kinders <rhyme label="c" type="m" corresp="#s16:l5">ghenoegh</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Dit broot is al wat men <rhyme label="a" type="m">begheert</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">In ‘t ghebet soo ons de Schrift <rhyme label="a" type="m" corresp="#s17:l1">leert</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Als daer Heydens sorgh buyten <rhyme label="b" type="m">blijff</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Al u begheert Gheestich <rhyme label="b" type="m" corresp="#s17:l3">gherijff</rhyme>,</l>
        <l id="s17:l5" met="4" type="-+">Laet komen voor Godt <rhyme label="c" type="m">openbaer</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">Wat u van noot is dat gheeft <rhyme label="c" type="m" corresp="#s17:l5">Vaer</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Ons zonden die vergheeft ons <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Want sy ons drucken alsoo <rhyme label="a" type="m" corresp="#s18:l1">seer</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Wy vergheven op dit <rhyme label="b" type="m">termijn</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">Al die aen ons yet schuldigh <rhyme label="b" type="m" corresp="#s18:l3">zijn</rhyme>,</l>
        <l id="s18:l5" met="4" type="-+">Want dat is doch maer Penninck <rhyme label="c" type="m">schult</rhyme>,</l>
        <l id="s18:l6" met="4" type="-+">Vader hebt doch met ons <rhyme label="c" type="m" corresp="#s18:l5">ghedult</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">‘t Hert moet tot Vader in <rhyme label="a" type="m">Hemel</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Maken voor hem een opset <rhyme label="a" type="m" corresp="#s19:l1">wel</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Alderley goet doen nae sijn <rhyme label="b" type="m">leer</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">Een nieu Creatuyr langhs hoe <rhyme label="b" type="m" corresp="#s19:l3">meer</rhyme></l>
        <l id="s19:l5" met="4" type="-+">Altijt betreen van nu <rhyme label="c" type="m">voortaen</rhyme>,</l>
        <l id="s19:l6" met="4" type="-+">Of ‘t Ghebedt is vergeefs, <rhyme label="c" type="m" corresp="#s19:l5">ghedaen</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Leyt ons niet in bekooringh <rhyme label="a" type="m">quaet</rhyme></l>
        <l id="s20:l2" met="4" type="-+">U handt gheender tijdt van ons <rhyme label="a" type="m" corresp="#s20:l1">laet</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">Ghy zijt een hulper <rhyme label="b" type="m">meenichfout</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">Al die maer recht op u <rhyme label="b" type="m" corresp="#s20:l3">betrout</rhyme>,</l>
        <l id="s20:l5" met="4" type="-+">In bekooringh als ‘t u <rhyme label="c" type="m">behaecht</rhyme>,</l>
        <l id="s20:l6" met="4" type="-+">Ghy gheeft uyt-komst dat ment <rhyme label="c" type="m" corresp="#s20:l5">verdraecht</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Al is een mensche <rhyme label="a" type="m">welbedroeft</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">Als de bekooringhe <rhyme label="a" type="m" corresp="#s21:l1">beproeft</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">Blijft men oprecht en niet <rhyme label="b" type="m">verdooft</rhyme>,</l>
        <l id="s21:l4" met="4" type="-+">Die Kroon des levens is <rhyme label="b" type="m" corresp="#s21:l3">belooft</rhyme>,</l>
        <l id="s21:l5" met="4" type="-+">Die straf met lijdtsemheyt <rhyme label="c" type="m">verdraecht</rhyme>,</l>
        <l id="s21:l6" met="4" type="-+">Die Vader aen sijn Kinders <rhyme label="c" type="m" corresp="#s21:l5">waecht</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Verlost ons Heer van alle <rhyme label="a" type="m">quaet</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">Sathan als een Leeu om ons <rhyme label="a" type="m" corresp="#s22:l1">gaet</rhyme>,</l>
        <l id="s22:l3" met="4" type="-+">Dat is ons groot <rhyme label="b" type="m">wederperty</rhyme>,</l>
        <l id="s22:l4" met="4" type="-+">Ons ziel verslinden dat soeckt <rhyme label="b" type="m" corresp="#s22:l3">hy</rhyme>,</l>
        <l id="s22:l5" met="4" type="-+">Sterckt ons gelove door u <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s22:l6" met="4" type="-+">Om hem wederstaen dach en <rhyme label="c" type="m" corresp="#s22:l5">nacht</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Moeten wy woonen och <rhyme label="a" type="m">lacy</rhyme>!</l>
        <l id="s23:l2" met="4" type="-+">Al daer den Sathans stoel is <rhyme label="a" type="m" corresp="#s23:l1">by</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">Wy moghen rusten <rhyme label="b" type="m">nimmermeer</rhyme>,</l>
        <l id="s23:l4" met="4" type="-+">Altijdt bidden tot onsen <rhyme label="b" type="m" corresp="#s23:l3">Heer</rhyme>,</l>
        <l id="s23:l5" met="4" type="-+">Dat hy ons nieuwe kracht <rhyme label="c" type="m">belooft</rhyme>,</l>
        <l id="s23:l6" met="4" type="-+">De Sathan te vertreen sijn <rhyme label="c" type="m" corresp="#s23:l5">hooft</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">U is dat Rijck, o Vader <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s24:l2" met="4" type="-+">Ons ziel verlanght by u te <rhyme label="a" type="m" corresp="#s24:l1">zijn</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Al door uwe ghenade <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s24:l4" met="4" type="-+">Want ons verdiensten zijn soo <rhyme label="b" type="m" corresp="#s24:l3">kleyn</rhyme>,</l>
        <l id="s24:l5" met="4" type="-+">Dus treuren wy op u <rhyme label="c" type="m">voetbanck</rhyme>,</l>
        <l id="s24:l6" met="4" type="-+">Over ons boose natuyr <rhyme label="c" type="m" corresp="#s24:l5">kranck</rhyme>,</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Nu wy dus kleyn zijn van <rhyme label="a" type="m">vermooch</rhyme>,</l>
        <l id="s25:l2" met="4" type="-+">Moetmen met gantscher herten <rhyme label="a" type="m" corresp="#s25:l1">hooch</rhyme></l>
        <l id="s25:l3" met="4" type="-+">Keeren in ‘t heylichdom tot <rhyme label="b" type="m">Vaer</rhyme>,</l>
        <l id="s25:l4" met="4" type="-+">Eeren hem in sijn Rijcke <rhyme label="b" type="m" corresp="#s25:l3">daer</rhyme>,</l>
        <l id="s25:l5" met="4" type="-+">Alle woorden van ‘t Ghebedt <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s25:l6" met="4" type="-+">Het hert tot Vader in <rhyme label="c" type="m" corresp="#s25:l5">d’Hemel</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Ghy Vader hebt oock aller <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s26:l2" met="4" type="-+">Daerom niemant hem selfs veel <rhyme label="a" type="m" corresp="#s26:l1">acht</rhyme>,</l>
        <l id="s26:l3" met="4" type="-+">Want niemant van hem selven <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s26:l4" met="4" type="-+">Een goet ghedacht al hoemen <rhyme label="b" type="m" corresp="#s26:l3">leeft</rhyme>,</l>
        <l id="s26:l5" met="4" type="-+">Die Kinders hebben ‘t al van <rhyme label="c" type="m">Vaer</rhyme>,</l>
        <l id="s26:l6" met="4" type="-+">Hoe kleyn, hoe groot, hy geeft het <rhyme label="c" type="m" corresp="#s26:l5">haer</rhyme></l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Vader ghy zijt soo heerlijck <rhyme label="a" type="m">sijn</rhyme>,</l>
        <l id="s27:l2" met="4" type="-+">Ons verlanghet by u te <rhyme label="a" type="m" corresp="#s27:l1">zijn</rhyme>,</l>
        <l id="s27:l3" met="4" type="-+">U heerlijckheyt is sonder <rhyme label="b" type="m">endt</rhyme>,</l>
        <l id="s27:l4" met="4" type="-+">Met grooter vreucht in ‘t Hemels <rhyme label="b" type="m" corresp="#s27:l3">tent</rhyme></l>
        <l id="s27:l5" met="4" type="-+">Wanneer sal ons worden <rhyme label="c" type="m">gheseyt</rhyme>,</l>
        <l id="s27:l6" met="4" type="-+">Koomt woont by my in <rhyme label="c" type="m" corresp="#s27:l5">eeuwigheyt</rhyme>?</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Dit is de somma van ‘t <rhyme label="a" type="m">ghebedt</rhyme>,</l>
        <l id="s28:l2" met="4" type="-+">Dat men wel op de saecke <rhyme label="a" type="m" corresp="#s28:l1">let</rhyme>,</l>
        <l id="s28:l3" met="4" type="-+">Ghelijck te vooren is <rhyme label="b" type="m">gheseyt</rhyme>,</l>
        <l id="s28:l4" met="4" type="-+">Nu noch met grooter <rhyme label="b" type="m" corresp="#s28:l3">neersticheyt</rhyme>,</l>
        <l id="s28:l5" met="4" type="-+">Dat ‘t herts ghedachten hooch <rhyme label="c" type="m">optreckt</rhyme>,</l>
        <l id="s28:l6" met="4" type="-+">En ‘t Ghebedt door de wolcken <rhyme label="c" type="m" corresp="#s28:l5">streckt</rhyme></l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Op dat het alsoo komt <rhyme label="a" type="m">voorwaer</rhyme>,</l>
        <l id="s29:l2" met="4" type="-+">En eenen soeten reuck gheeff <rhyme label="a" type="m" corresp="#s29:l1">daer</rhyme>,</l>
        <l id="s29:l3" met="4" type="-+">In ‘t heylichdom hooch is den <rhyme label="b" type="m">throon</rhyme>,</l>
        <l id="s29:l4" met="4" type="-+">Dit is ‘t Ghebedt o Kinders <rhyme label="b" type="m" corresp="#s29:l3">schoon</rhyme>,</l>
        <l id="s29:l5" met="4" type="-+">Dus is het woort Amen <rhyme label="c" type="m">bediet</rhyme>,</l>
        <l id="s29:l6" met="4" type="-+">‘t Ghebedt behoeft meer woorden <rhyme label="c" type="m" corresp="#s29:l5">niet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
