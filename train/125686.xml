  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">ZY heeft ook twee <rhyme label="a" type="f">Wangen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Die zijn <rhyme label="b" type="m">Hemels-blauw</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Als ik die zie <rhyme label="a" type="f" corresp="#s1:l1">hangen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Word mijn Geest heel <rhyme label="b" type="m" corresp="#s1:l2">gauw</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Om naar 't Meysje toe te <rhyme label="c" type="f">loopen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Yder Wang staat stijf van 't <rhyme label="d" type="m">Nat</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Dat uyt <rhyme label="d" type="m" corresp="#s1:l6">Ceres-vat</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">In het Glaasje word <rhyme label="c" type="f" corresp="#s1:l5">gedropen</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Dat uyt <rhyme label="d" type="m" corresp="#s1:l6">Ceres-vat</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">In het Glas en Keel-gat <rhyme label="d" type="m" corresp="#s1:l6">spat</rhyme>.</l>
      </lg>
    </lg>
  </text>
