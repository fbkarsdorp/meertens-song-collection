<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>163770</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat voor dagen heeft een hond</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAt daegen heeft myn <rhyme label="a" type="m">hond</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s1:l1">hond</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Myn hond hoeft niet te <rhyme label="b" type="f">werke</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hy gaet ook niet te <rhyme label="b" type="f" corresp="#s1:l3">kerke</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wat daegen heeft myn <rhyme label="a" type="m" corresp="#s1:l1">hond</rhyme>?</l>
        <l id="s1:l6" met="3" type="-+">Wat daegen heeft myn <rhyme label="a" type="m" corresp="#s1:l1">hond</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nooit is zyn kop <rhyme label="a" type="m">berooit</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat hy heeft te veel <rhyme label="a" type="m" corresp="#s2:l1">gepooit</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Daerom is hy nooit <rhyme label="b" type="f">dronke</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Nog nimmer nooit <rhyme label="b" type="f" corresp="#s2:l3">beschonken</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wat daegen heeft myn <rhyme label="a" type="m" corresp="#s2:l1">hond</rhyme>?</l>
        <l id="s2:l6" met="3" type="-+">Wat daegen heeft myn <rhyme label="a" type="m" corresp="#s2:l1">hond</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy is altyd te <rhyme label="a" type="m">vreê</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Zyn goet neemt hy altyd <rhyme label="a" type="m" corresp="#s3:l1">meê</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Prokkereurs en <rhyme label="b" type="f">Avokaete</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zulle hem geen geld <rhyme label="b" type="f" corresp="#s3:l3">afpraeten</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s3:l1">hond</rhyme>?</l>
        <l id="s3:l6" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s3:l1">hond</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al krygt hy zomtyds <rhyme label="a" type="m">kak</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hy gaet nooit na het <rhyme label="a" type="m" corresp="#s4:l1">gemak</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Kouse, Stoelen kan hy <rhyme label="b" type="f">missen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hoeft geen pot om op te <rhyme label="b" type="f" corresp="#s4:l3">pissen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s4:l1">hond</rhyme>?</l>
        <l id="s4:l6" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s4:l1">hond</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hy hout wel van <rhyme label="a" type="m">vryjery</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Maer hy blyft altyd <rhyme label="a" type="m" corresp="#s5:l1">vry</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hy hoeft geen wyf te <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Huishouwen te onder <rhyme label="b" type="f" corresp="#s5:l3">houwen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s5:l1">hond</rhyme>?</l>
        <l id="s5:l6" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s5:l1">hond</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy is 't meeste op zyn <rhyme label="a" type="m">dreef</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Als men hem wat lekkers <rhyme label="a" type="m" corresp="#s6:l1">geef</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hy geeft om geen lelyk <rhyme label="b" type="f">bakhuis</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Hy kakt ook nooit op een <rhyme label="b" type="f" corresp="#s6:l3">Kakhuis</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s6:l1">hond</rhyme>?</l>
        <l id="s6:l6" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s6:l1">hond</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy komt maer altyd <rhyme label="a" type="m">tuys</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Al zonder munt of <rhyme label="a" type="m" corresp="#s7:l1">kruis</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hy vraegt nooit na geen <rhyme label="b" type="f">kyven</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Schyt op de vloer van ouwe <rhyme label="b" type="f" corresp="#s7:l3">wyven</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s7:l1">hond</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s7:l1">hond</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Zyn pot is altyd <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Al is het nog zoo <rhyme label="a" type="m" corresp="#s8:l1">schrael</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Daerom gaet hy wel uit het <rhyme label="b" type="f">snorren</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">'s Morgens komt hy my <rhyme label="b" type="f" corresp="#s8:l3">porren</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s8:l1">hond</rhyme>?</l>
        <l id="s8:l6" met="3" type="-+">Wat daege heeft myn <rhyme label="a" type="m" corresp="#s8:l1">hond</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
