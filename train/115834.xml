    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Schoone Vorstinne, hoe <rhyme label="a" type="f">betov'ren</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Uw zoete <rhyme label="b" type="m">aantreklykheên</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Gy overwint door uwe <rhyme label="b" type="m" corresp="#s1:l2">zêen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat steeden kan <rhyme label="a" type="f" corresp="#s1:l1">verov'ren</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Schoone Vorstinne, hoe <rhyme label="a" type="f" corresp="#s1:l1">betov'ren</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Uw zoete <rhyme label="b" type="m" corresp="#s1:l2">aantreklykhêen</rhyme>!</l>
        <l id="s1:l7" met="4" type="+-">Gy overwint door uwe <rhyme label="b" type="m" corresp="#s1:l2">zêen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dat steden kan vero- <rhyme label="a" type="f" corresp="#s1:l1">v'ren</rhyme>.</l>
        <l id="s1:l9" met="4" type="-+">Beroemde Gôon van 't wyd <rhyme label="c" type="m">Heel-al</rhyme></l>
        <l id="s1:l10" met="5" type="+-">U eeren, u eeren met een groot <rhyme label="c" type="m" corresp="#s1:l9">getal</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">Beroemde Gôon van 't wyd <rhyme label="c" type="m" corresp="#s1:l9">Heel-al</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">Beroemde Gôon van 't wyd <rhyme label="c" type="m" corresp="#s1:l9">Heel-al</rhyme>,</l>
        <l id="s1:l13" met="5" type="+-">U eeren, u eeren met een groot <rhyme label="c" type="m" corresp="#s1:l9">getal</rhyme>,</l>
