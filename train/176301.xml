<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>176301</id>
    <date>
      <year-start>1719</year-start>
      <year-end>1719</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="zeemanslied" translation="sailor's song">zeemanslied</tag>
    </tags>
    <melody>Gij wispelturige matroos</melody>
    <categories>
      <category translation="work">werk</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Matroosjes die de Zee <rhyme label="a" type="m">bemint</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En kleyne schatten <rhyme label="a" type="m" corresp="#s1:l1">overwint</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die vaert na ’t Oosten na ’t <rhyme label="b" type="m">west</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En houd de warmte voor het <rhyme label="b" type="m" corresp="#s1:l3">best</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En moet daer waters <rhyme label="c" type="f">drincken</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Die wel <rhyme label="c" type="f" corresp="#s1:l5">stincken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zijt gy het werme land nu <rhyme label="a" type="m">moe</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">So puert eens mee na Groenlant <rhyme label="a" type="m" corresp="#s2:l1">toe</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer sulje over vuylen <rhyme label="b" type="m">stanck</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van ’t stinckend’ water tot u <rhyme label="b" type="m" corresp="#s2:l3">dranck</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Noch over heete <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Niet veel <rhyme label="c" type="f" corresp="#s2:l5">klagen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hebt gy een broeck vijf ses aen ’t <rhyme label="a" type="m">gat</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En dan noch om de lenden <rhyme label="a" type="m" corresp="#s3:l1">wat</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Een wanbays en een rock of <rhyme label="b" type="m">dry</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En dan een bollickvangers <rhyme label="b" type="m" corresp="#s3:l3">py</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ses kousen om twee <rhyme label="c" type="f">beenen</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Gaet dan <rhyme label="c" type="f" corresp="#s3:l5">heenen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Noch dient’er dan verstaet wel <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Een goeden bultsack by te <rhyme label="a" type="m" corresp="#s4:l1">zijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Een dicken beest van een <rhyme label="b" type="m">Combaers</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die treckt dan dichjes om de <rhyme label="b" type="m" corresp="#s4:l3">naers</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En dan de neus <rhyme label="c" type="f">gesteecken</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Onder <rhyme label="c" type="f" corresp="#s4:l5">deecken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">En legje dan wel fraey te <rhyme label="a" type="m">kooy</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En slaept seer mackelijck en <rhyme label="a" type="m" corresp="#s5:l1">mooy</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dan roept daer een als was hy <rhyme label="b" type="m">mal</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Door ’t heele Schip, sa val, val, <rhyme label="b" type="m" corresp="#s5:l3">val</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Stracks moetje op dat <rhyme label="c" type="f">roepen</rhyme></l>
        <l id="s5:l6" met="2" type="+-">In de <rhyme label="c" type="f" corresp="#s5:l5">Sloepen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">En vraegje dan wat dat <rhyme label="a" type="m">beduyt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Daer loopter een aen ly voor <rhyme label="a" type="m" corresp="#s6:l1">uyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Het is een Walvis machtig <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Sa lustig heen en steeckt hem <rhyme label="b" type="m" corresp="#s6:l3">doodt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Hy sal Oranje <rhyme label="c" type="f">blasen</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Sa als <rhyme label="c" type="f" corresp="#s6:l5">basen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sa lustig roept hem na sijn <rhyme label="a" type="m">gat</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Eer dat hy kiest het <rhyme label="a" type="m" corresp="#s7:l1">hase-padt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Daer krijgt hy een harpoen in’t <rhyme label="b" type="m">lijf</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Byget hy loopt geweldig <rhyme label="b" type="m" corresp="#s7:l3">stijf</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Daer krijgt hy lenssen <rhyme label="c" type="f">steecken</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Hy is <rhyme label="c" type="f" corresp="#s7:l5">bezweecken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dan spreeckt den Commandeur sijn <rhyme label="a" type="m">woort</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Sa mannen met de Vis aen <rhyme label="a" type="m" corresp="#s8:l1">boort</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daer sal men t’samen lustig <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Met een rantsoentjen <rhyme label="b" type="m" corresp="#s8:l3">brandewijn</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wilt lustig maer <rhyme label="c" type="f">boegseeren</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">Sa als <rhyme label="c" type="f" corresp="#s8:l5">Heeren</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Als men dan wat geslapen <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En dat de Vis wel boven <rhyme label="a" type="m" corresp="#s9:l1">zweeft</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dan salt’er op een flentsen <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Men brengt speck-messen op de <rhyme label="b" type="m" corresp="#s9:l3">baen</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">En doe de spil wel <rhyme label="c" type="f">draeijen</rhyme>,</l>
        <l id="s9:l6" met="2" type="+-">So laet <rhyme label="c" type="f" corresp="#s9:l5">waeijen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Sa lustig mannen als een <rhyme label="a" type="m">pop</rhyme></l>
        <l id="s10:l2" met="4" type="-+">En wind dan ’t dryvend hagjen <rhyme label="a" type="m" corresp="#s10:l1">op</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Al hoog genoeg, of ga, of <rhyme label="b" type="m">ga</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Sny stuken, smijt in’t Flens-gat <rhyme label="b" type="m" corresp="#s10:l3">dra</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Flus sullen wy dan <rhyme label="c" type="f">baerden</rhyme></l>
        <l id="s10:l6" met="2" type="+-">Aen gaen <rhyme label="c" type="f" corresp="#s10:l5">vaerden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Na dat het flenssen is <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Dan sal ’t op een snijen <rhyme label="a" type="m" corresp="#s11:l1">gaen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Jan hagel wordt nu <rhyme label="b" type="m">Officier</rhyme></l>
        <l id="s11:l4" met="4" type="-+">De minste is een <rhyme label="b" type="m" corresp="#s11:l3">Pieckenier</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Daer moet elck een sijn <rhyme label="c" type="f">saken</rhyme></l>
        <l id="s11:l6" met="2" type="+-">Dan na <rhyme label="c" type="f" corresp="#s11:l5">maken</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Sa mannen sonder veel <rhyme label="a" type="m">versuym</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">En heyst wat hachjes uyt het <rhyme label="a" type="m" corresp="#s12:l1">ruym</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Een ander roept speck aen de <rhyme label="b" type="m">banck</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">In’t Flensgat roept men wacht so <rhyme label="b" type="m" corresp="#s12:l3">lanck</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Ick moet wat hachjes <rhyme label="c" type="f">malen</rhyme>,</l>
        <l id="s12:l6" met="2" type="+-">Na my <rhyme label="c" type="f" corresp="#s12:l5">halen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">In ’t ruym roept Schieman met <rhyme label="a" type="m">gewelt</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Bruyt speck in’t ruym, hoe is’t hier <rhyme label="a" type="m" corresp="#s13:l1">gestelt</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Een ander roept nu weer sa <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">En bruy de krengen buyten <rhyme label="b" type="m" corresp="#s13:l3">boort</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Sa jongen tapt eens <rhyme label="c" type="f">drincken</rhyme>,</l>
        <l id="s13:l6" met="2" type="+-">Of ’t sal <rhyme label="c" type="f" corresp="#s13:l5">stincken</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Als dan den arbeydt is <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Dan sal den Kock aen’t koocken <rhyme label="a" type="m" corresp="#s14:l1">gaen</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Een wak’re ketel bier en <rhyme label="b" type="m">broodt</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">En die in’t lief, dan is’t geen <rhyme label="b" type="m" corresp="#s14:l3">noodt</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">En daer mee wil ick <rhyme label="c" type="f">seggen</rhyme>,</l>
        <l id="s14:l6" met="2" type="+-">Weer aen ’t <rhyme label="c" type="f" corresp="#s14:l5">leggen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">So doende raeckt het schip vol <rhyme label="a" type="m">speck</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Van traen en isser geen <rhyme label="a" type="m" corresp="#s15:l1">gebreck</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Jan hagel wordt de kop schier <rhyme label="b" type="m">kroes</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Hy is so smeerig als de <rhyme label="b" type="m" corresp="#s15:l3">Droes</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Maer het geldje sal het <rhyme label="c" type="f">moeten</rhyme></l>
        <l id="s15:l6" met="2" type="+-">Weer <rhyme label="c" type="f" corresp="#s15:l5">versoeten</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">En komt men dan weer <rhyme label="a" type="m">t’Amsterdam</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Van daer men laest sijn Afscheyt <rhyme label="a" type="m" corresp="#s16:l1">nam</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Dan soeckt elk weer na sijnen <rhyme label="b" type="m">Weert</rhyme>.</l>
        <l id="s16:l4" met="4" type="-+">Tot dat het geldjen is <rhyme label="b" type="m" corresp="#s16:l3">verteert</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Met dag en nacht te <rhyme label="c" type="f">drincken</rhyme>,</l>
        <l id="s16:l6" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s16:l5">rinkinken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
