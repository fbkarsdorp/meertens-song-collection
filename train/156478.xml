  <author>Aernout van Overbeke</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GEeft hem water in sijn <rhyme label="a" type="m">gat</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die geen smaeck in wijn en <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wat verbruyder Beest is <rhyme label="a" type="m" corresp="#s1:l1">dat</rhyme>,</l>
        <l id="s1:l4" met="6" type="+-">Die niet waert is, die niet waert is, dat hy <rhyme label="b" type="m" corresp="#s1:l2">leeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Onsmaeck'lijcken <rhyme label="a" type="m">Smaeckeloos</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Is het Backhuys nu <rhyme label="b" type="m">vereelt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Of is het verhemelt <rhyme label="a" type="m" corresp="#s2:l1">voos</rhyme>,</l>
        <l id="s2:l4" met="6" type="+-">Dat veel kuyckens, dat veel kuyckens, heeft <rhyme label="b" type="m" corresp="#s2:l2">gekeelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Loof de darmen sijn te <rhyme label="a" type="m">vol</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En de spijs wil niet <rhyme label="b" type="m">bene'en</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Want hy porcust hier als <rhyme label="a" type="m" corresp="#s3:l1">dol</rhyme>,</l>
        <l id="s3:l4" met="6" type="+-">Misschien smaeckt hem, misschien smaeckt hem, dat <rhyme label="b" type="m" corresp="#s3:l2">alleen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Soo je wilt na huys toe <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Maeckt dat u geen varcken <rhyme label="b" type="m">hoort</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Of het ranst u daet'lijck <rhyme label="a" type="m" corresp="#s4:l1">aen</rhyme>,</l>
        <l id="s4:l4" met="6" type="+-">En vermaent u, en vermaent u, uw <rhyme label="b" type="m" corresp="#s4:l2">geboort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
