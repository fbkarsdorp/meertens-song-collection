<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180700</id>
    <date>
      <year-start>1688</year-start>
      <year-end>1688</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 100 Datheen</melody>
  <author>Philippus van Sorgen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Lieve Iesu! Ziele <rhyme label="a" type="m">vrindt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">O soetste die mijn ziele <rhyme label="a" type="m" corresp="#s1:l1">mindt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Emanuel ô eeuwig <rhyme label="b" type="m">Godt</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Ey weest mijn eeuwig deel en <rhyme label="b" type="m" corresp="#s1:l3">lodt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O lieve Iesu <rhyme label="a" type="m">Bruydegom</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ey lieve Iesu, kom ey <rhyme label="a" type="m" corresp="#s2:l1">kom</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">O lieve Iesu, Iesu <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ey komt in een onreyn <rhyme label="b" type="m" corresp="#s2:l3">gemoet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O lieve Iesu, Heer van <rhyme label="a" type="m">Al</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ey siet eens op mijn <rhyme label="a" type="m" corresp="#s3:l1">Ziele-val</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ey Iesu, Iesu, Iesu <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Weest gy mijn Heyl, mijn eeuwig <rhyme label="b" type="m" corresp="#s3:l3">goet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O lieve Iesu, Heer en <rhyme label="a" type="m">Hooft</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En doet het geen gy hebt <rhyme label="a" type="m" corresp="#s4:l1">belooft</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">O Iesu, Iesu rijcken <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ey doet doch eens mijn ziels <rhyme label="b" type="m" corresp="#s4:l3">begeer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ey lieve Iesu, salig <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Weest gy mijn rijckdom, schat, en <rhyme label="a" type="m" corresp="#s5:l1">eer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">O Iesu! vol van <rhyme label="b" type="m">goedigheyt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Schenckt my doch uwe <rhyme label="b" type="m" corresp="#s5:l3">Zaligheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O lieve Iesu, reyne <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ey daelt in mijne ziele <rhyme label="a" type="m" corresp="#s6:l1">neer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ey lieve lieve Iesu <rhyme label="b" type="m">kom</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Ach! Heere Iesu siet eens <rhyme label="b" type="m" corresp="#s6:l3">om</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O soete Iesu, vol <rhyme label="a" type="m">genae</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Immers kom ick noch niet te <rhyme label="a" type="m" corresp="#s7:l1">spae</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Na u, na u is ‘t dat ick <rhyme label="b" type="m">schrey</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En om u liefde, dat ick <rhyme label="b" type="m" corresp="#s7:l3">vry</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ach! isser noch een open <rhyme label="a" type="m">oor</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">En voor een Sondaer noch <rhyme label="a" type="m" corresp="#s8:l1">gehoor</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ey soete Iesu, hoort my <rhyme label="b" type="m">doch</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ey helpt my doch och! och! och! <rhyme label="b" type="m" corresp="#s8:l3">och</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Iesu, Iesu, Heyligh <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Ick kniel ootmoedigh voor u <rhyme label="a" type="m" corresp="#s9:l1">neer</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Ey Iesu, Iesu, weest mijn <rhyme label="b" type="m">ligt</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Toont my u vriendelijk <rhyme label="b" type="m" corresp="#s9:l3">aengesigt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O Iesu, Iesu, wijse <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Roept tot mijn ziele, keer, op <rhyme label="a" type="m" corresp="#s10:l1">keer</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Ey lieve Iesu, Iacobs <rhyme label="b" type="m">ster</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Ey Iesu roept mijn ziel van <rhyme label="b" type="m" corresp="#s10:l3">ver</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">O Iesu, Iesu, levens <rhyme label="a" type="m">Bron</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">O Iesu, salige Ziele <rhyme label="a" type="m" corresp="#s11:l1">Son</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">O Iesu, Iesu, groot van <rhyme label="b" type="m">macht</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Ach! Ach! hebt op mijn Ziele <rhyme label="b" type="m" corresp="#s11:l3">aght</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">O Iesu, hoog-verheven <rhyme label="a" type="m">Vorst</rhyme>.</l>
        <l id="s12:l2" met="4" type="-+">Ach! komt en woont in dese <rhyme label="a" type="m" corresp="#s12:l1">borst</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Ach Heere Iesu! Davids <rhyme label="b" type="m">Soon</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Ey hoor, ey hoor mijn nare <rhyme label="b" type="m" corresp="#s12:l3">toon</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ach! was ick eens van u <rhyme label="a" type="m">gekust</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Dan was mijn droeve Ziel <rhyme label="a" type="m" corresp="#s13:l1">gerust</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Ey Iesu, Iesu, spreng u <rhyme label="b" type="m">bloedt</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Op een onreyn, en vuyl <rhyme label="b" type="m" corresp="#s13:l3">gemoedt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Waer blijft mijn lief, waer blijft mijn <rhyme label="a" type="m">vrient</rhyme>?</l>
        <l id="s14:l2" met="4" type="-+">Ach! hoor het schryen van u <rhyme label="a" type="m" corresp="#s14:l1">Kint</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">‘t Geslachte Lam, ‘t geslachte <rhyme label="b" type="m">Lam</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Ach! dat gy quam, ach! dat gy <rhyme label="b" type="m" corresp="#s14:l3">quam</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">‘t Moet Iesus, Iesus, Iesus <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Sal ick verlost zijn van mijn <rhyme label="a" type="m" corresp="#s15:l1">pijn</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Mijn Ziel beswijk, ey sijgt daer <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Na u, na u, ô Heer! ô <rhyme label="b" type="m" corresp="#s15:l3">Heer</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">O lieve Iesu, Iesu <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">O Iesu! Iesu! Iesu! <rhyme label="a" type="m" corresp="#s16:l1">soet</rhyme>!</l>
        <l id="s16:l3" met="4" type="-+">Ey, ey, gebie, gebie, <rhyme label="b" type="m">gebie</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Dat maer u wil, u wil <rhyme label="b" type="m" corresp="#s16:l3">geschie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
