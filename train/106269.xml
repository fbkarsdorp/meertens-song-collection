  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Lang gewensten <rhyme label="a" type="m">avontstond</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Soo vol van soete <rhyme label="b" type="f">Lusjes</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">En van grage <rhyme label="b" type="f" corresp="#s1:l2">Kusjes</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Die ons zyn <rhyme label="a" type="m" corresp="#s1:l1">gejond</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Door het heylsaam <rhyme label="c" type="f">paren</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Van dees lieve <rhyme label="d" type="m">Twee</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die in haar Ionge <rhyme label="c" type="f" corresp="#s1:l5">Iaren</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Quamen op de <rhyme label="d" type="m" corresp="#s1:l6">Ree</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Uyt Cypres Holle <rhyme label="d" type="m" corresp="#s1:l6">Zee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Nacht voor onsen <rhyme label="a" type="m">Bruydegom</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">De soetste Nacht der <rhyme label="b" type="f">Nachten</rhyme></l>
        <l id="s2:l3" met="3" type="+-">'t Loon van zyn lang <rhyme label="b" type="f" corresp="#s2:l2">wachten</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Ghy zyt <rhyme label="a" type="m" corresp="#s2:l1">wellekom</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Al de sture <rhyme label="c" type="f">vlagen</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Hebt ghy doen <rhyme label="d" type="m">vergaan</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die in verleden <rhyme label="c" type="f" corresp="#s2:l5">dagen</rhyme>.</l>
        <l id="s2:l8" met="3" type="+-">'t Hert met druck <rhyme label="d" type="m" corresp="#s2:l6">belaan</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Soo dapper vielen <rhyme label="d" type="m" corresp="#s2:l6">aan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daar meed soo maackt hy t Scheepje <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En strijcke het seyltje <rhyme label="b" type="f">neder</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Om van 't fel <rhyme label="b" type="f" corresp="#s3:l2">onweder</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Niet te zyn <rhyme label="a" type="m" corresp="#s3:l1">verrast</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Hy duyckt int <rhyme label="c" type="f">Vooronder</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Voor het naer <rhyme label="d" type="m">geluyt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Van Hagel, Wind, en <rhyme label="c" type="f" corresp="#s3:l5">Donder</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Met zyn lieve <rhyme label="d" type="m" corresp="#s3:l6">Bruyd</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">En speelt het <rhyme label="d" type="m" corresp="#s3:l6">speeltje</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
