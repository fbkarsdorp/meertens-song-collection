<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185830</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Puer nobis nascitur</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Had ick vlogelen als een Arent <rhyme label="a" type="m">grijs</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick soude so hooge <rhyme label="b" type="f">vliegen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Daer boven in dat <rhyme label="a" type="m" corresp="#s1:l1">Paradijs</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Tot mijnen soete <rhyme label="b" type="f" corresp="#s1:l2">lieve</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dan soude ick seggen: Vader <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wanneer suldy my <rhyme label="b" type="f">halen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Wt deser ellenden van <rhyme label="a" type="m" corresp="#s2:l1">Aertrijck</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In uwer hoogster <rhyme label="b" type="f" corresp="#s2:l2">salen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waer is die dochter van <rhyme label="a" type="m">Syon</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ick wildese vrolijck <rhyme label="b" type="f">maken</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Ick wil haer een bootschap <rhyme label="a" type="m" corresp="#s3:l1">doen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van also blijden <rhyme label="b" type="f" corresp="#s3:l2">saken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die hoochste inder <rhyme label="a" type="m">minnen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat is Iesus Marien <rhyme label="b" type="f">Sone</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Godt laet ons hem <rhyme label="a" type="m" corresp="#s4:l1">gewinnen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hier boven inden <rhyme label="b" type="f" corresp="#s4:l2">throone</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat Hemelrijck ende <rhyme label="a" type="m">Aerdtrijck</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Noyt conste <rhyme label="b" type="f">ontluycken</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Die sal een suyver Maget <rhyme label="a" type="m" corresp="#s5:l1">reyn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In haren lichaem <rhyme label="b" type="f" corresp="#s5:l2">besluyten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die duysternisse is <rhyme label="a" type="m">verclaert</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat licht is ons <rhyme label="b" type="f">gheresen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Een Maghet heeft een kint <rhyme label="a" type="m" corresp="#s6:l1">ghebaert</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat mach wel wonder <rhyme label="b" type="f" corresp="#s6:l2">wesen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dat kint was teerder ende <rhyme label="a" type="m">kranck</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Een Maghet moestet <rhyme label="b" type="f">voeden</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Tâ€™is recht dat wijt hem weten <rhyme label="a" type="m" corresp="#s7:l1">danck</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Van sijner grooter <rhyme label="b" type="f" corresp="#s7:l2">ootmoeden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Heer Iesus kerst, wat hebdy <rhyme label="a" type="m">ghemeent</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Wy souden u soo gaerne <rhyme label="b" type="f">vraghen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Dat ghy dus leght en <rhyme label="a" type="m" corresp="#s8:l1">weent</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ghy zijt soo ionck van <rhyme label="b" type="f" corresp="#s8:l2">daghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat dus viel ende was <rhyme label="a" type="m">onghereet</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat moghen wy nu <rhyme label="b" type="f">ghebruycken</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat Adam inden Appel <rhyme label="a" type="m" corresp="#s9:l1">beet</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat dede den Hemel <rhyme label="b" type="f" corresp="#s9:l2">sluyten</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O Moeder Gods van <rhyme label="a" type="m">Hemelrijcke</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Des Conincx inden <rhyme label="b" type="f">throone</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">Bidt voor ons allen <rhyme label="a" type="m" corresp="#s10:l1">innerlijcke</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Iesum uwen <rhyme label="b" type="f" corresp="#s10:l2">sone</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dat wy daer boven <rhyme label="a" type="m">moeten</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Daer die Enghelen vrolijck <rhyme label="b" type="f">singhen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">V ende uwen kinde <rhyme label="a" type="m" corresp="#s11:l1">groeten</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Dat wil ons Godt <rhyme label="b" type="f" corresp="#s11:l2">ghehinghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
