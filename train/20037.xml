  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VRienden ick kom u mee <rhyme label="a" type="f">deelen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Laburdon, <rhyme label="b" type="m">tiereliron</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Bruylofts-stof, voor tongh en <rhyme label="a" type="f" corresp="#s1:l1">keelen</rhyme>,</l>
        <l id="s1:l4" met="8" type="+-">Laburette tierelirette, laburdon <rhyme label="b" type="m" corresp="#s1:l2">tierelieron</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">’k Loofje neust hiel wel mijn <rhyme label="a" type="f">seggen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Laburdon, <rhyme label="b" type="m">tiereliron</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">‘k Hoef u dan niet uyt te <rhyme label="a" type="f" corresp="#s2:l1">leggen</rhyme>,</l>
        <l id="s2:l4" met="8" type="+-">Laburette tierelirette, laburdon <rhyme label="b" type="m" corresp="#s2:l2">tierelieron</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Kom, ‘tsa, ‘tsa, aen ’t keele <rhyme label="a" type="f">smeeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Laburdon, <rhyme label="b" type="m">tiereliron</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Singht fris op dit paer ter <rhyme label="a" type="f" corresp="#s3:l1">eeren</rhyme>,</l>
        <l id="s3:l4" met="8" type="+-">Laburette tierelirette, laburdon <rhyme label="b" type="m" corresp="#s3:l2">tierelieron</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
