<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157422</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IK ben nu <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Positief te <rhyme label="a" type="f" corresp="#s1:l1">woonen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ik verzoeke klyn en <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Te koopen van myn <rhyme label="b" type="m" corresp="#s1:l3">broot</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Gy zelt doen <rhyme label="c" type="f">zienen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ik zal u wel <rhyme label="c" type="f" corresp="#s1:l5">bedienen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Allon monsjeur tree maer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En kiest uit na u <rhyme label="d" type="m" corresp="#s1:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ik heb Banket broot <rhyme label="a" type="f">fyn</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Of wel de schoonen <rhyme label="a" type="f" corresp="#s2:l1">mastelyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Goet gebakke 't is hier <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En allerbeste <rhyme label="b" type="m" corresp="#s2:l3">waer</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Wil maer eens <rhyme label="c" type="f">probeeren</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Myn dames en gy <rhyme label="c" type="f" corresp="#s2:l5">Heeren</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Allon monsjeur tree maer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En kiest uit na u <rhyme label="d" type="m" corresp="#s2:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ziet de Tarvebroot ten <rhyme label="a" type="f">toon</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die is als een roos zo <rhyme label="a" type="f" corresp="#s3:l1">schoon</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Niet beter vinden <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In de stad van <rhyme label="b" type="m" corresp="#s3:l3">Amsterdam</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">stuivers dubeltjes <rhyme label="c" type="f">brootjes</rhyme>:</l>
        <l id="s3:l6" met="4" type="-+">Ook varse France <rhyme label="c" type="f" corresp="#s3:l5">knoopjes</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Allon monsjeur tree maer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En kiest uyt na u <rhyme label="d" type="m" corresp="#s3:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Roggebroot heb ik zeer <rhyme label="a" type="f">goet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Gerezen en die is ook <rhyme label="a" type="f" corresp="#s4:l1">zoet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die de Boer en <rhyme label="b" type="m">burgerman</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Met plaizier ook eeten <rhyme label="b" type="m" corresp="#s4:l3">kan</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Het is waer hout 't in u <rhyme label="c" type="f">gedagten</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dat het geeft ook veel <rhyme label="c" type="f" corresp="#s4:l5">kragten</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Allon monsjeur tree maer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En kiest uyt na u <rhyme label="d" type="m" corresp="#s4:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Raspebrootjes niet <rhyme label="a" type="f">vergeten</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Voor Renteniers te <rhyme label="a" type="f" corresp="#s5:l1">eeten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">s'Morgens by koffy en <rhyme label="b" type="m">tee</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ook krente brootjes <rhyme label="b" type="m" corresp="#s5:l3">mee</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Witten bollen, timpies <rhyme label="c" type="f">fyn</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Alles kanse vinden by <rhyme label="c" type="f" corresp="#s5:l5">myn</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Allon monsjeur tree maer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En kiest uyt na u <rhyme label="d" type="m" corresp="#s5:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Krakelingen met <rhyme label="a" type="f">komyn</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Om te eten by een glas <rhyme label="a" type="f" corresp="#s6:l1">wyn</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Ze bennen lekker in der <rhyme label="b" type="m">daet</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Kostelyk en <rhyme label="b" type="m" corresp="#s6:l3">dillekaet</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Ook anys <rhyme label="c" type="f">beschuyten</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">De zes voor 9 <rhyme label="c" type="f" corresp="#s6:l5">duiten</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Allon monsjeur tree maer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En kiest uit na u <rhyme label="d" type="m" corresp="#s6:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ik wil nog te toon <rhyme label="a" type="f">stellen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Van myn varze goet <rhyme label="a" type="f" corresp="#s7:l1">bestellen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die gy kan steken in de <rhyme label="b" type="m">zak</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En eten op u <rhyme label="b" type="m" corresp="#s7:l3">gemak</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Gebakke boles en <rhyme label="c" type="f">galas</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Alle vrydag voor <rhyme label="c" type="f" corresp="#s7:l5">zabas</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Allon monsjeur tree maer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En kiest uyt na u <rhyme label="d" type="m" corresp="#s7:l7">zin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Maer de Paes komt nu <rhyme label="a" type="f">aen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Dat men moet paeskoek bakken <rhyme label="a" type="f" corresp="#s8:l1">gaen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Ik werke goet ende <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Rekommandeere myn <rhyme label="b" type="m" corresp="#s8:l3">perzoon</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Die daar neemt zyn dienst aen <rhyme label="c" type="f">myn</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Die zal ook gezegent <rhyme label="c" type="f" corresp="#s8:l5">zyn</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Allon monsjeur denkt daer op <rhyme label="d" type="m">maer</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Dat ik altyd ben <rhyme label="d" type="m" corresp="#s8:l7">klaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Nu moet ik nog <rhyme label="a" type="f">zeggen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Eer dat ik gae <rhyme label="a" type="f" corresp="#s9:l1">weggen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">In de tyd dat gy myn doet <rhyme label="b" type="m">zien</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Laet my wat gelt <rhyme label="b" type="m" corresp="#s9:l3">verdien</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Leeft gezont en vrolyk <rhyme label="c" type="f">altyd</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Bevryt van rampe haat of <rhyme label="c" type="f" corresp="#s9:l5">nyt</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Ik gae nu heen adie <rhyme label="d" type="m">monsjeur</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Ik blyf u <rhyme label="d" type="m" corresp="#s9:l7">zerviteur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
