<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164854</id>
    <date>
      <year-start>1671</year-start>
      <year-end>1671</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie hoe gelukkig zijn ze dan</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SIet hoe geluckigh zijnse <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die een schoon lief mach <rhyme label="b" type="f">spreken</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En mee gaen wandelen <rhyme label="a" type="m" corresp="#s1:l1">kan</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En hy geeftse dan een <rhyme label="c" type="m">kus</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dan is zijn hart <rhyme label="c" type="m" corresp="#s1:l4">gerust</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat komt al van de <rhyme label="c" type="m" corresp="#s1:l4">lust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'k Heb twee bruyn Oogjens in mijn <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die mijn jonck Hart <rhyme label="b" type="f">beminnen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Och kreegh ick weder <rhyme label="a" type="m" corresp="#s2:l1">min</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Och mocht ick by haer <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Zoo was ick uyt de <rhyme label="c" type="m" corresp="#s2:l4">pijn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hart alderliefste <rhyme label="c" type="m" corresp="#s2:l4">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick heb soo dick en <rhyme label="a" type="m">meenighmael</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Over u schoon Lief <rhyme label="b" type="f">gevallen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En weder <rhyme label="a" type="m" corresp="#s3:l1">op-gestaen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ick heb u langh <rhyme label="c" type="m">gevrijt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maer ghy soo spijtigh <rhyme label="c" type="m" corresp="#s3:l4">zijt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En maeckt my niet <rhyme label="c" type="m" corresp="#s3:l4">verblijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'s Nachts als ick legge in mijn <rhyme label="a" type="m">rust</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Komt sy in mijn <rhyme label="b" type="f">gedachten</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">In 't midden van de <rhyme label="a" type="m" corresp="#s4:l1">lust</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Keer ick mijn hooft eens <rhyme label="c" type="m">om</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En segh schoon Liefje <rhyme label="c" type="m" corresp="#s4:l4">koom</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Het is maer eenen <rhyme label="c" type="m" corresp="#s4:l4">droom</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Savonts staen ick voor haer <rhyme label="a" type="m">deur</rhyme>:</l>
        <l id="s5:l2" met="3" type="-+">Roep ick met luyder <rhyme label="b" type="f">stemme</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Schoon Lief ick bender <rhyme label="a" type="m" corresp="#s5:l1">veur</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Moet ick noch langer <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En geenen troost <rhyme label="c" type="m" corresp="#s5:l4">ontfaen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Waer sal ick henen <rhyme label="c" type="m" corresp="#s5:l4">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ick blijf daer dan heel treurigh <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sy wil mijn droeve <rhyme label="b" type="f">klachten</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Niet eensjes horen <rhyme label="a" type="m" corresp="#s6:l1">aen</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Of ick wat singh of <rhyme label="c" type="m">queel</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En voor haer deurtjen <rhyme label="c" type="m" corresp="#s6:l4">speel</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Sy acht dat niet heel <rhyme label="c" type="m" corresp="#s6:l4">veel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Iongman al loopt ghy voor mijn <rhyme label="a" type="m">deur</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ick mach u doch niet <rhyme label="b" type="f">luchten</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick heb een <rhyme label="a" type="m" corresp="#s7:l1">Serviteur</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Veel beter na mijn <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Hy staet geschreven <rhyme label="c" type="m" corresp="#s7:l4">in</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Mijn hartje, door de <rhyme label="c" type="m" corresp="#s7:l4">min</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die blijft daer vast gewortelt <rhyme label="a" type="m">staen</rhyme>:</l>
        <l id="s8:l2" met="3" type="-+">Ick sal hem niet <rhyme label="b" type="f">verlaten</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ghy mooght wel heenen <rhyme label="a" type="m" corresp="#s8:l1">gaen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En soeckt een ander <rhyme label="c" type="m">Maeght</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Want ghy my niet <rhyme label="c" type="m" corresp="#s8:l4">behaeght</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Hoe dickwils ghy ‘t my <rhyme label="c" type="m" corresp="#s8:l4">vraeght</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Als eens de liefde is vast <rhyme label="a" type="m">geset</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En dat sy is <rhyme label="b" type="f">verzegelt</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Voorts op 't Echte <rhyme label="a" type="m" corresp="#s9:l1">bedt</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Voorwaer een bruydtje <rhyme label="c" type="m">teer</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Is dan geen Vrijster <rhyme label="c" type="m" corresp="#s9:l4">meer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Sy doet haer Liefs <rhyme label="c" type="m" corresp="#s9:l4">begeer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Sy mach dan niet meer speelen <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En na die tijdt geen <rhyme label="b" type="f">minnaers</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Uyt liefde spreecken <rhyme label="a" type="m" corresp="#s10:l1">aen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dus Ionghman houdt u <rhyme label="c" type="m">rust</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Ick heb al wat mijn <rhyme label="c" type="m" corresp="#s10:l4">lust</rhyme>:</l>
        <l id="s10:l6" met="3" type="-+">Mijn hart is al <rhyme label="c" type="m" corresp="#s10:l4">gerust</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">O dood! wat komt mijn hier aen <rhyme label="a" type="m">boort</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Mijn hart is seer <rhyme label="b" type="f">verslagen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Nu ick dit heb <rhyme label="a" type="m" corresp="#s11:l1">gehoort</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">Nu wensch ick dat de <rhyme label="c" type="m">doodt</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">In desen grooten <rhyme label="c" type="m" corresp="#s11:l4">noodt</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Met sijne Pijlen <rhyme label="c" type="m" corresp="#s11:l4">schoot</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Adieu ô alderliefste <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Kan ick geen troost <rhyme label="b" type="f">verwerven</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Soo sterf ick vol <rhyme label="a" type="m" corresp="#s12:l1">pijn</rhyme>:</l>
        <l id="s12:l4" met="3" type="-+">Adieu dan lieve <rhyme label="c" type="m">Vrouw</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">U Minnaer is <rhyme label="c" type="m" corresp="#s12:l4">ghetrouw</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">En sterft om u van <rhyme label="c" type="m" corresp="#s12:l4">rouw</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">'t Is droevig voor een Iong <rhyme label="a" type="m">Minnaer</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Wanneer een wreede <rhyme label="b" type="f">Vryster</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Niet en wil hooren <rhyme label="a" type="m" corresp="#s13:l1">naer</rhyme></l>
        <l id="s13:l4" met="3" type="-+">De klachten van haer <rhyme label="c" type="m">Lief</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Noch doetse hem <rhyme label="c" type="m" corresp="#s13:l4">gerief</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Al schrijft hy haer een <rhyme label="c" type="m" corresp="#s13:l4">Brief</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
