  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Comt laet ons t’samen, by namen vrolijck <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Laet varen <rhyme label="b" type="m">ongeneught</rhyme>,</l>
        <l id="s1:l3" met="5" type="+-">Treurigheydts vlagen, doet jagen uyt elcks <rhyme label="a" type="m" corresp="#s1:l1">aenschijn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Weest vrolijck in der <rhyme label="b" type="m" corresp="#s1:l2">deught</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Singht bly van geeste, dees Bruylofts <rhyme label="c" type="f">feeste</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Dees Bruylofts <rhyme label="c" type="f" corresp="#s1:l5">feeste</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Wenscht geluck ons Bruydegom en Vrou <rhyme label="a" type="m">Bruydt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Geluck in desen <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s2:l3" met="5" type="+-">Geluck en vrede, hier mede welckes <rhyme label="a" type="m" corresp="#s2:l1">besluyt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In saligheydt <rhyme label="b" type="m" corresp="#s2:l2">bestaet</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Laet u licht lichten, tot Godes <rhyme label="c" type="f">stichten</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Tot Godes <rhyme label="c" type="f" corresp="#s2:l5">stichten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Als onbeschromen sal komen u levens <rhyme label="a" type="m">endt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door den geloove <rhyme label="b" type="m">gesterckt</rhyme>,</l>
        <l id="s3:l3" met="5" type="+-">Dragende vruchten, met duchten alomme <rhyme label="a" type="m" corresp="#s3:l1">bekent</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Godt die dit in u <rhyme label="b" type="m" corresp="#s3:l2">werckt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Sal u dan geven, ‘t eeuwige <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s3:l6" met="2" type="+-">‘t Eeuwige <rhyme label="c" type="f" corresp="#s3:l5">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
