<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24553</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Help nu maria <rhyme label="a" type="f">dichten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">een liedeken totter eren van <rhyme label="b" type="m">di</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ende wilt myn hartken <rhyme label="a" type="f" corresp="#s1:l1">verlichten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">van sonden maeken <rhyme label="b" type="m" corresp="#s1:l2">vry</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wes wil ic my <rhyme label="a" type="f">vermeten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">ick arme <rhyme label="b" type="m">creatuer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">die worme soelen my <rhyme label="a" type="f" corresp="#s2:l1">eten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">die doet    doet staet voer myn <rhyme label="b" type="m" corresp="#s2:l2">doer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maria woe was v toe <rhyme label="a" type="f">moede</rhyme></l>
        <l id="s3:l2" met="3" type="-+">al buten <rhyme label="b" type="m">iherusalem</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Och doe ghi uwen enyghen <rhyme label="a" type="f" corresp="#s3:l1">soene</rhyme></l>
        <l id="s3:l4" met="3" type="-+">soe droeflick toe ghemoete <rhyme label="b" type="m" corresp="#s3:l2">quemen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doe was my recht toe <rhyme label="a" type="f">moede</rhyme></l>
        <l id="s4:l2" met="3" type="-+">als ene die staruen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ende ghi seghen uwen enyghen <rhyme label="a" type="f" corresp="#s4:l1">soene</rhyme></l>
        <l id="s4:l4" met="3" type="-+">verordelen totter <rhyme label="b" type="m" corresp="#s4:l2">doet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maria doer uwer <rhyme label="a" type="f">mynnen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">synne ghi hem nae <rhyme label="b" type="m">ghegaen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">al op den barch van <rhyme label="a" type="f" corresp="#s5:l1">caluarien</rhyme></l>
        <l id="s5:l4" met="3" type="-+">seghe ghi hem aent cruce <rhyme label="b" type="m" corresp="#s5:l2">slaen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat heeft hi daer <rhyme label="a" type="f">ghesproeken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">ghedaen een schoen <rhyme label="b" type="m">sermoen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Och vader vergheuet hem hoer <rhyme label="a" type="f" corresp="#s6:l1">sonden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">want si en weten nyet wat si <rhyme label="b" type="m" corresp="#s6:l2">doen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wat sprack hi totten <rhyme label="a" type="f">schaeker</rhyme></l>
        <l id="s7:l2" met="3" type="-+">ic sals v maken <rhyme label="b" type="m">wys</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ghi soelt noch haden <rhyme label="a" type="f" corresp="#s7:l1">wesen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">myt my ynt <rhyme label="b" type="m" corresp="#s7:l2">paradys</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wie heeft hi daer <rhyme label="a" type="f">beuoelen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">die lieueste moeder <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s8:l3" met="3" type="-+">sante iohans toe enen <rhyme label="a" type="f" corresp="#s8:l1">behoeder</rhyme></l>
        <l id="s8:l4" met="3" type="-+">dat was die neue <rhyme label="b" type="m" corresp="#s8:l2">syn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ghi heeft syn hoeuet <rhyme label="a" type="f">gheneyghet</rhyme></l>
        <l id="s9:l2" met="3" type="-+">ghesoncken al op syne <rhyme label="b" type="m">boerst</rhyme></l>
        <l id="s9:l3" met="3" type="-+">hi heeft soe lude <rhyme label="a" type="f" corresp="#s9:l1">gheroepen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">ick hebbe soe groeten <rhyme label="b" type="m" corresp="#s9:l2">doerst</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wat heeft hi meer <rhyme label="a" type="f">ghesproeken</rhyme></l>
        <l id="s10:l2" met="3" type="-+">doe hi aenden cruce <rhyme label="b" type="m">henck</rhyme></l>
        <l id="s10:l3" met="3" type="-+">die schoelt hebbe ick <rhyme label="a" type="f" corresp="#s10:l1">betaelet</rhyme></l>
        <l id="s10:l4" met="3" type="-+">veruollet is alle <rhyme label="b" type="m" corresp="#s10:l2">dynck</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wat heeft hi meer <rhyme label="a" type="f">ghesproeken</rhyme></l>
        <l id="s11:l2" met="3" type="-+">doe hi anden cruce <rhyme label="b" type="m">henck</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Och vader yn uwen <rhyme label="a" type="f" corresp="#s11:l1">handen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">beuele ic mynen <rhyme label="b" type="m" corresp="#s11:l2">gheest</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Nu bidde ic v <rhyme label="a" type="f">maria</rhyme></l>
        <l id="s12:l2" met="3" type="-+">doer uwen liden <rhyme label="b" type="m">groet</rhyme></l>
        <l id="s12:l3" met="3" type="-+">dat ghi ons arme sondighe <rhyme label="a" type="f" corresp="#s12:l1">menschen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">wilt helpen wt alre <rhyme label="b" type="m" corresp="#s12:l2">noet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
