<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184842</id>
    <date>
      <year-start>1639</year-start>
      <year-end>1639</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Blijdschap van mij vlied</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">IOnghe spruyten <rhyme label="a" type="m">teer</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Gheplant in den <rhyme label="b" type="f">Hoven</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Van den <rhyme label="a" type="m" corresp="#s1:l1">Opper-heer</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Die in 't <rhyme label="b" type="f" corresp="#s1:l2">bovenst-boven</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Sit in sijnen <rhyme label="c" type="m">Throon</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">In den hooghsten <rhyme label="d" type="f">Hemel</rhyme>:</l>
        <l id="s1:l7" met="3" type="+-">Wt dit schoonste <rhyme label="c" type="m" corresp="#s1:l5">schoon</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Siet hy het <rhyme label="d" type="f" corresp="#s1:l6">ghewemel</rhyme></l>
        <l id="s1:l9" met="6" type="+-">Van ons menschen al // die hier in dit <rhyme label="e" type="m">dal</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Loopen, draven gins en <rhyme label="f" type="m">weer</rhyme>,</l>
        <l id="s1:l11" met="6" type="+-">En neemt oogemerck // op al onse <rhyme label="g" type="m">werck</rhyme></l>
        <l id="s1:l12" met="4" type="+-">Oft het oock is nae sijn <rhyme label="f" type="m" corresp="#s1:l10">leer</rhyme>,</l>
        <l id="s1:l13" met="3" type="+-">En oft wy oock <rhyme label="h" type="f">letten</rhyme></l>
        <l id="s1:l14" met="3" type="+-">Op sijn reyne <rhyme label="h" type="f" corresp="#s1:l13">wetten</rhyme>,</l>
        <l id="s1:l15" met="4" type="+-">Die hy met sijn vingher <rhyme label="i" type="m">schreef</rhyme>:</l>
        <l id="s1:l16" met="3" type="+-">Laet ons daer op <rhyme label="j" type="f">achten</rhyme>,</l>
        <l id="s1:l17" met="3" type="+-">Laet ons daer nae <rhyme label="j" type="f" corresp="#s1:l16">trachten</rhyme>.</l>
        <l id="s1:l18" met="4" type="+-">Elck sie toe dat hy soo <rhyme label="i" type="m" corresp="#s1:l15">leef</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ionghelinghen <rhyme label="a" type="m">kloeck</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Vyandts machts <rhyme label="b" type="f">verwinners</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Aen u ick <rhyme label="a" type="m" corresp="#s2:l1">versoeck</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Weest rechte <rhyme label="b" type="f" corresp="#s2:l2">beminners</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Van u Vaders <rhyme label="c" type="m">Woordt</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Om dat te <rhyme label="d" type="f">beleven</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Recht alsoo 't <rhyme label="c" type="m" corresp="#s2:l5">behoort</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Soo werdt u <rhyme label="d" type="f" corresp="#s2:l6">ghegheven</rhyme></l>
        <l id="s2:l9" met="6" type="+-">Van sijn stercke handt // hulp ende <rhyme label="e" type="m">bystant</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Om op 's levens enge <rhyme label="f" type="m">baen</rhyme></l>
        <l id="s2:l11" met="6" type="+-">Die leydt nae die Stadt // vol van alle <rhyme label="g" type="m">schadt</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Voorsichtigh derwaerts te <rhyme label="f" type="m" corresp="#s2:l10">gaen</rhyme>.</l>
        <l id="s2:l13" met="3" type="+-">Wilt 't ruyme padt <rhyme label="h" type="f">mijden</rhyme>,</l>
        <l id="s2:l14" met="3" type="+-">Want het eyndt is <rhyme label="h" type="f" corresp="#s2:l13">lijden</rhyme>,</l>
        <l id="s2:l15" met="4" type="+-">Lijden en eeuwigh <rhyme label="i" type="m">ghequel</rhyme>,</l>
        <l id="s2:l16" met="3" type="+-">Wroegingh, harte <rhyme label="j" type="f">parssen</rhyme>,</l>
        <l id="s2:l17" met="3" type="+-">Droevigh tande <rhyme label="j" type="f" corresp="#s2:l16">knarssen</rhyme>.</l>
        <l id="s2:l18" met="4" type="+-">Elck sie toe en wacht hem <rhyme label="i" type="m" corresp="#s2:l15">wel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Mannen in 't <rhyme label="a" type="m">verstandt</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Weest hier nu <rhyme label="b" type="f">stantvastigh</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">In 't beloofde <rhyme label="a" type="m" corresp="#s3:l1">Landt</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Sal 't niet wesen <rhyme label="b" type="f" corresp="#s3:l2">lastigh</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Als in desen <rhyme label="c" type="m">tijdt</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Daer men staegh moet <rhyme label="d" type="f">vechten</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Teghen die 't <rhyme label="c" type="m" corresp="#s3:l5">benijdt</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Dat Gods trouwe <rhyme label="d" type="f" corresp="#s3:l6">knechten</rhyme></l>
        <l id="s3:l9" met="6" type="+-">Volghen al gemeyn // haren <rhyme label="e" type="m">Capiteyn</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Daerom is hy seer <rhyme label="f" type="m">verstoort</rhyme>:</l>
        <l id="s3:l11" met="6" type="+-">Met list en bedrogh // omgaet hy ons <rhyme label="g" type="m">Och</rhyme>!</l>
        <l id="s3:l12" met="4" type="+-">Wilt doch manlijck treden <rhyme label="f" type="m" corresp="#s3:l10">voort</rhyme></l>
        <l id="s3:l13" met="3" type="+-">Dat g'hier nae met <rhyme label="h" type="f">luste</rhyme>,</l>
        <l id="s3:l14" met="3" type="+-">In de goede <rhyme label="h" type="f" corresp="#s3:l13">ruste</rhyme></l>
        <l id="s3:l15" met="4" type="+-">Komen meught, by al die <rhyme label="i" type="m">rest</rhyme></l>
        <l id="s3:l16" met="3" type="+-">Van die lieve <rhyme label="j" type="f">vrienden</rhyme></l>
        <l id="s3:l17" met="3" type="+-">Die den Heer oyt <rhyme label="j" type="f" corresp="#s3:l16">dienden</rhyme>.</l>
        <l id="s3:l18" met="4" type="+-">Elck sie toe verkiest het <rhyme label="i" type="m" corresp="#s3:l15">best</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Vaders van 't <rhyme label="a" type="m">ghesin</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Rechte Gods huys <rhyme label="b" type="f">houders</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Die, om doen <rhyme label="a" type="m" corresp="#s4:l1">ghewin</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Hebt op uwe <rhyme label="b" type="f" corresp="#s4:l2">schouders</rhyme></l>
        <l id="s4:l5" met="3" type="+-">'t Iock dat Christus <rhyme label="c" type="m">droegh</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Willigh ende <rhyme label="d" type="f">wacker</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Om sijn Vaders <rhyme label="c" type="m" corresp="#s4:l5">ploegh</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Door des Heeren <rhyme label="d" type="f" corresp="#s4:l6">Acker</rhyme></l>
        <l id="s4:l9" met="6" type="+-">Trecken, dat het zaet // vruchtbarich op <rhyme label="e" type="m">gaet</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Dat daer in werde <rhyme label="f" type="m">gezaeyt</rhyme></l>
        <l id="s4:l11" met="6" type="+-">Vrucht als uyt goe Aerd // mach werden <rhyme label="g" type="m">vergaert</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Als die tijdt komt datmen <rhyme label="f" type="m" corresp="#s4:l10">maeyt</rhyme></l>
        <l id="s4:l13" met="3" type="+-">Hy voor u <rhyme label="h" type="f">arbeyden</rhyme>,</l>
        <l id="s4:l14" met="3" type="+-">V 't loon sal <rhyme label="h" type="f" corresp="#s4:l13">bereyden</rhyme>:</l>
        <l id="s4:l15" met="4" type="+-">Op dese belooningh <rhyme label="i" type="m">siet</rhyme>,</l>
        <l id="s4:l16" met="3" type="+-">Dat ghy maer <rhyme label="j" type="f">ghetrouwe</rhyme></l>
        <l id="s4:l17" met="3" type="+-">Gods Acker wilt <rhyme label="j" type="f" corresp="#s4:l16">bouwe</rhyme>,</l>
        <l id="s4:l18" met="4" type="+-">Anders soecktmen aen u <rhyme label="i" type="m" corresp="#s4:l15">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Hemels <rhyme label="a" type="m">Hovenier</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Wilt doch nu <rhyme label="b" type="f">begieten</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Uwe planten <rhyme label="a" type="m" corresp="#s5:l1">hier</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">En laet op ons <rhyme label="b" type="f" corresp="#s5:l2">vlieten</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Van dien klaren <rhyme label="c" type="m">stroom</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Die als Cristal <rhyme label="d" type="f">vloeyet</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Tusschen menigh <rhyme label="c" type="m" corresp="#s5:l5">boom</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Op dat van ons <rhyme label="d" type="f" corresp="#s5:l6">groeyet</rhyme></l>
        <l id="s5:l9" met="6" type="+-">Vrucht seer menighfout//van des Levens <rhyme label="e" type="m">hout</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Staende in het <rhyme label="f" type="m">Paradijs</rhyme>,</l>
        <l id="s5:l11" met="6" type="+-">Met u geestes nat // ons, o Heer <rhyme label="g" type="m">bespat</rhyme>.</l>
        <l id="s5:l12" met="4" type="+-">Soo salmen tot uwen <rhyme label="f" type="m" corresp="#s5:l10">prijs</rhyme></l>
        <l id="s5:l13" met="3" type="+-">Goede vruchten <rhyme label="h" type="f">dragen</rhyme>,</l>
        <l id="s5:l14" met="3" type="+-">Die u wel <rhyme label="h" type="f" corresp="#s5:l13">behagen</rhyme>.</l>
        <l id="s5:l15" met="4" type="+-">Onbegrijpelijcke <rhyme label="i" type="m">Godt</rhyme>,</l>
        <l id="s5:l16" met="3" type="+-">Drie-en-een <rhyme label="j" type="f">on-deyligh</rhyme>,</l>
        <l id="s5:l17" met="3" type="+-">Drie-mael Heyligh, <rhyme label="j" type="f" corresp="#s5:l16">Heyligh</rhyme>,</l>
        <l id="s5:l18" met="4" type="+-">Heyligh Heere <rhyme label="i" type="m" corresp="#s5:l15">Zebaoth</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
