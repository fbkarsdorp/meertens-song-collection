<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183279</id>
    <date>
      <year-start>1752</year-start>
      <year-end>1752</year-end>
    </date>
    <location>
      <orginal-name>gorinchem</orginal-name>
      <standard-name>Gorinchem, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8296133</lon>
        <lat>4.9738743</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Tien geboden</melody>
  <author>Jacob Groenewegen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Komt hier die lust hebt om te <rhyme label="a" type="f">bouwen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">U zielen op een vaste <rhyme label="b" type="m">gront</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die op geen rietstaf wilt <rhyme label="a" type="f" corresp="#s1:l1">vertrouwen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Maar gaaren eeuwig veylig <rhyme label="b" type="m" corresp="#s1:l2">stont</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Bevrijt van zonde, vloek en <rhyme label="a" type="f">tooren</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Door duyvel, dood en helsche <rhyme label="b" type="m">magt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hier zietg’ een grondsteen <rhyme label="a" type="f" corresp="#s2:l1">uytverkooren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van God in Zion <rhyme label="b" type="m" corresp="#s2:l2">voortgebragt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Geloovig volk u heyl en <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hier verpliglijk aan toe <rhyme label="b" type="m">vertrout</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Gelukkig hy die niet <rhyme label="a" type="f" corresp="#s3:l1">beneven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maar regt zijn ziele daar op <rhyme label="b" type="m" corresp="#s3:l2">bout</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een fundament van <rhyme label="a" type="f">eeuwigheeden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Van God gelegt voor ’s Werelds <rhyme label="b" type="m">grond</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gelegt in ’t eerste woord van <rhyme label="a" type="f" corresp="#s4:l1">vreeden</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">De opregting van ’t gena <rhyme label="b" type="m" corresp="#s4:l2">verbond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gelegt vervolgens door de <rhyme label="a" type="f">vadere</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Van d’eerst en tweede Werelds <rhyme label="b" type="m">tijd</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">De grond en stem van al haar <rhyme label="a" type="f" corresp="#s5:l1">nadere</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ia van haar gantsche <rhyme label="b" type="m" corresp="#s5:l2">zaligheid</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Gelegt in al de schaduw <rhyme label="a" type="f">wetten</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Afbeeldzel van toekoment <rhyme label="b" type="m">goed</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Waar door ’t geloof zig neder <rhyme label="a" type="f" corresp="#s6:l1">zetten</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Op Iezus en zijn <rhyme label="b" type="m" corresp="#s6:l2">offer-bloed</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Gelegt van alle de <rhyme label="a" type="f">Propheeten</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die van dit eene <rhyme label="b" type="m">fundament</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Gesproken hebben en <rhyme label="a" type="f" corresp="#s7:l1">geweeten</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En anders geen gemaakt <rhyme label="b" type="m" corresp="#s7:l2">bekent</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Gelegt voor al in volheids <rhyme label="a" type="f">tijden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Waar van zoo lange was <rhyme label="b" type="m">vermelt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Waarmee hy door zijn dood en <rhyme label="a" type="f" corresp="#s8:l1">lijden</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Wreet tot verzoening <rhyme label="b" type="m" corresp="#s8:l2">voorgestelt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">En aan Gods regeerhant <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Tot Koning, Hoofd en <rhyme label="b" type="m">Middelaar</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">In wien dat alle ligt en <rhyme label="a" type="f" corresp="#s9:l1">leeven</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Zou zijn voor d’uytverkoorne <rhyme label="b" type="m" corresp="#s9:l2">schaar</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Waarom d’Apostelen met <rhyme label="a" type="f">malkanderen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Uytriepen als met eene <rhyme label="b" type="m">mond</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">De zaligheid is in geen <rhyme label="a" type="f" corresp="#s10:l1">anderen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">In hem is al u heyl <rhyme label="b" type="m" corresp="#s10:l2">gegrond</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">O Iezus ’k wil dan op u <rhyme label="a" type="f">bouwen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Ag maakt mijn ziele regt <rhyme label="b" type="m">ontblood</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Van alle gronde van <rhyme label="a" type="f" corresp="#s11:l1">vertrouwen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Weest gt ’t alleen in nood en <rhyme label="b" type="m" corresp="#s11:l2">dood</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">’K wil mijn geheel dan <rhyme label="a" type="f">overgeeven</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Aan u o Iezus en ik <rhyme label="b" type="m">weet</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Dat ik dan hier zal veylig <rhyme label="a" type="f" corresp="#s12:l1">leeven</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">En dood nog duyvel my geen <rhyme label="b" type="m" corresp="#s12:l2">leet</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Zal doen of ooyt mijn ziele <rhyme label="a" type="f">haalen</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Van dit geleyde <rhyme label="b" type="m">fundament</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Ik weet dat ik zal <rhyme label="a" type="f" corresp="#s13:l1">zegen-praalen</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">En zalig leeven zonder <rhyme label="b" type="m" corresp="#s13:l2">ent</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
