    <melody>Psalm 091 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WIe sich en alles wat hy <rhyme label="a" type="m">heeft</rhyme></l>
        <l id="s1:l2" met="3" type="-+">(Sijn have, lijff, en <rhyme label="b" type="f">leven</rhyme>)</l>
        <l id="s1:l3" met="4" type="-+">Den Alder-hooghsten <rhyme label="a" type="m" corresp="#s1:l1">over-geeft</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die sal (als and’re <rhyme label="b" type="f" corresp="#s1:l2">beven</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Voor’t grimmen des aenstaenden <rhyme label="c" type="m">lots</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En bange <rhyme label="d" type="f">oorlooghs-hitten</rhyme>)</l>
        <l id="s1:l7" met="4" type="-+">Omvaen als met de schadu <rhyme label="c" type="m" corresp="#s1:l5">Godts</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gerust en veyligh <rhyme label="d" type="f" corresp="#s1:l6">sitten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Des heb ick deel; en daerom <rhyme label="a" type="m">moedt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Tot Godt mijn Heer te <rhyme label="b" type="f">spreken</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Ghy zijt wiens gunst mijn hope <rhyme label="a" type="m" corresp="#s2:l1">voedt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Door blijckbaer <rhyme label="b" type="f" corresp="#s2:l2">liefde-teecken</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Ghy zijt mijn Godt en <rhyme label="c" type="m">vasticheydt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Waer op mijns herts <rhyme label="d" type="f">vertrouwen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Den grondt sijns hopens heeft <rhyme label="c" type="m" corresp="#s2:l5">geleyt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En op sal blijven <rhyme label="d" type="f" corresp="#s2:l6">houwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dus ick. soo ghy oock wie ghy <rhyme label="a" type="m">zijt</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Verwerren u de <rhyme label="b" type="f">stricken</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Des ramp-bejagers: hoopt en <rhyme label="a" type="m" corresp="#s3:l1">lijdt</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Godt sal verlossing <rhyme label="b" type="f" corresp="#s3:l2">schicken</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Wanneer des pestes <rhyme label="c" type="m">kleef-venijn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dreyght met ontijdigh <rhyme label="d" type="f">sterven</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Hy sal u ongeraeckt doen <rhyme label="c" type="m" corresp="#s3:l5">zijn</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En weeren haer <rhyme label="d" type="f" corresp="#s3:l6">verderven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De schadu sijner gunst sal <rhyme label="a" type="m">dy</rhyme></l>
        <l id="s4:l2" met="3" type="-+">(Waer toe ’tonnut <rhyme label="b" type="f">bekomm’ren</rhyme>?)</l>
        <l id="s4:l3" met="4" type="-+">Sal dy gedurigh wesen <rhyme label="a" type="m" corresp="#s4:l1">by</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En troost’lijck <rhyme label="b" type="f" corresp="#s4:l2">over-lomm’ren</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Sijn troost-smaeck sal ’tgeroerde <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In hope doen <rhyme label="d" type="f">verstijven</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Sijn trouw in allen noodt en <rhyme label="c" type="m" corresp="#s4:l5">smert</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Vw’ schildt en schutsel <rhyme label="d" type="f" corresp="#s4:l6">blijven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Geen evel sal u <rhyme label="a" type="m">onverwacht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In’t doncker <rhyme label="b" type="f">overkomen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">De ijslijckheden van de <rhyme label="a" type="m" corresp="#s5:l1">nacht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Doen geen Godts vrunden <rhyme label="b" type="f" corresp="#s5:l2">schroomen</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Soo weynigh ’sdaeghs de pijl u <rhyme label="c" type="m">ques’t</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Van die claer-vyandtsch <rhyme label="d" type="f">handelt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Soo weynigh heeft op u de <rhyme label="c" type="m" corresp="#s5:l5">pest</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die ’snachts in’t doncker <rhyme label="d" type="f" corresp="#s5:l6">wandelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wanneerder op den hoogen <rhyme label="a" type="m">dagh</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Verwoesting is <rhyme label="b" type="f">beschoren</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En ’svyandts heyr gereedt ten <rhyme label="a" type="m" corresp="#s6:l1">slagh</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Moordt-pleging heeft <rhyme label="b" type="f" corresp="#s6:l2">geswooren</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Soo sullen aen uw’ rechter <rhyme label="c" type="m">zy</rhyme>’</l>
        <l id="s6:l6" met="3" type="-+">Der mannen duysendt <rhyme label="d" type="f">vallen</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Ia thienmael duysendt noch; en <rhyme label="c" type="m" corresp="#s6:l5">ghy</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Blijft onverseert van <rhyme label="d" type="f" corresp="#s6:l6">allen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Met uwen oogen sult ghy’t <rhyme label="a" type="m">sien</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hoe eenmael den <rhyme label="b" type="f">godloosen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Des quaedts vergelding sal <rhyme label="a" type="m" corresp="#s7:l1">geschien</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat sy voor ’tgoede <rhyme label="b" type="f" corresp="#s7:l2">kosen</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">(Ghy, Heer, mijn hoop’.) Doch die met <rhyme label="c" type="m">my</rhyme></l>
        <l id="s7:l6" met="3" type="-+">De ziele durft <rhyme label="d" type="f">verheffen</rhyme></l>
        <l id="s7:l7" met="4" type="-+">En woont in Godt: uw’ hoofdt is <rhyme label="c" type="m" corresp="#s7:l5">vry</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Van onheyls klack’loos <rhyme label="d" type="f" corresp="#s7:l6">treffen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Geen ramp magh t’uwer hutten <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Geen plage salse <rhyme label="b" type="f">naken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">(Te diep staet Deuchd in Godes <rhyme label="a" type="m" corresp="#s8:l1">sin</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Om ten verderff te <rhyme label="b" type="f" corresp="#s8:l2">raken</rhyme>.)</l>
        <l id="s8:l5" met="4" type="-+">Vry is uw’ huys van <rhyme label="c" type="m">ongeval</rhyme>:</l>
        <l id="s8:l6" met="3" type="-+">Want Godt, in allen <rhyme label="d" type="f">nooden</rhyme></l>
        <l id="s8:l7" met="4" type="-+">De hoede dijns bevelen <rhyme label="c" type="m" corresp="#s8:l5">sal</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Aen sijnen <rhyme label="d" type="f" corresp="#s8:l6">Hemel-boden</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Om uwe wegen ga te <rhyme label="a" type="m">slaen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Den gantschen loop van <rhyme label="b" type="f">’tleven</rhyme></l>
        <l id="s9:l3" met="4" type="-+">En uwer voeten alle <rhyme label="a" type="m" corresp="#s9:l1">pa’en</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Haer veylicheydt te <rhyme label="b" type="f" corresp="#s9:l2">geven</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Sy sullen u, (soo teer en <rhyme label="c" type="m">veer</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Lieft Godt die hem <rhyme label="d" type="f">behagen</rhyme>)</l>
        <l id="s9:l7" met="4" type="-+">Op dat geen steen uw’ voet <rhyme label="c" type="m" corresp="#s9:l5">bezeer</rhyme>’,</l>
        <l id="s9:l8" met="3" type="-+">Als op haer handen <rhyme label="d" type="f" corresp="#s9:l6">dragen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Op Leeuw en adder sal uw’ <rhyme label="a" type="m">voet</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Sich onbeschadight <rhyme label="b" type="f">setten</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Den fellen draeck en <rhyme label="a" type="m" corresp="#s10:l1">leeuws-gebroedt</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Sult ghy den kop <rhyme label="b" type="f" corresp="#s10:l2">verpletten</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">In my (aldus spreeckt Godt van <rhyme label="c" type="m">dy</rhyme>)</l>
        <l id="s10:l6" met="3" type="-+">Heeft hy sich gants <rhyme label="d" type="f">ergeven</rhyme>:</l>
        <l id="s10:l7" met="4" type="-+">Dies ick hem red. Mijn naem kent <rhyme label="c" type="m" corresp="#s10:l5">hy</rhyme>:</l>
        <l id="s10:l8" met="3" type="-+">Dies wordt hy hoogh <rhyme label="d" type="f" corresp="#s10:l6">verheven</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hy roep’ my aen ter quader <rhyme label="a" type="m">tijdt</rhyme>:</l>
        <l id="s11:l2" met="3" type="-+">Verhooring sal hem <rhyme label="b" type="f">beuren</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">’kWil met hem zijn wanneer hy <rhyme label="a" type="m" corresp="#s11:l1">lijdt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">En eynden al sijn <rhyme label="b" type="f" corresp="#s11:l2">treuren</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">En doen hem eer, en ’tluck met <rhyme label="c" type="m">een</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Van sat te leven <rhyme label="d" type="f">mogen</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">En gunnen mijner <rhyme label="c" type="m" corresp="#s11:l5">saliche’en</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Aenschouwen, sijnen <rhyme label="d" type="f" corresp="#s11:l6">oogen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
