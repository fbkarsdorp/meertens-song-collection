<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183309</id>
    <date>
      <year-start>1752</year-start>
      <year-end>1752</year-end>
    </date>
    <location>
      <orginal-name>gorinchem</orginal-name>
      <standard-name>Gorinchem, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8296133</lon>
        <lat>4.9738743</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O Holland schoon gij leeft in vrede</melody>
  <author>Jacob Groenewegen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Verblyd u ’s Hemels <rhyme label="a" type="m">Gunstgenoot</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In God uw eeuwig <rhyme label="b" type="f">Erven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">U Heyl en Rots in nood en <rhyme label="a" type="m" corresp="#s1:l1">dood</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In leeven en in <rhyme label="b" type="f" corresp="#s1:l2">sterven</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wijl gy uyt kragt van Iezus <rhyme label="c" type="m">bloed</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Hebt deel aan al des Heeren <rhyme label="c" type="m" corresp="#s1:l5">goed</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ia God moogd Vader <rhyme label="d" type="f">noemen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En u in hem <rhyme label="d" type="f" corresp="#s1:l7">beroemen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al schoon de Satan en de <rhyme label="a" type="m">Hel</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">U onheyl had <rhyme label="b" type="f">gezwooren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Had, ’s Hemels gunst u <rhyme label="a" type="m" corresp="#s2:l1">evenwel</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot zaligheid <rhyme label="b" type="f" corresp="#s2:l2">verkooren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Tot vaten der <rhyme label="c" type="m">barmhartigheid</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Voor ’s Werelds grond u al <rhyme label="c" type="m" corresp="#s2:l5">bereyd</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">In ’t Boek des Lams ten <rhyme label="d" type="f">leeven</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Voor eeuwig <rhyme label="d" type="f" corresp="#s2:l7">aangeschreeven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gods Zoon treed by zijn Vader <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Voor d’aanvang aller <rhyme label="b" type="f">tyden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Verbind zig om uyt zuyv’re <rhyme label="a" type="m" corresp="#s3:l1">min</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als Borg voor u te <rhyme label="b" type="f" corresp="#s3:l2">lyden</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Die liefde deed zijn Vaders <rhyme label="c" type="m">schoot</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Verlaten, en zig in den <rhyme label="c" type="m" corresp="#s3:l5">dood</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">O Goedheid hoog <rhyme label="d" type="f">verheeven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Vrywillig <rhyme label="d" type="f" corresp="#s3:l7">overgeeven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoe wierd dat Lam geperst <rhyme label="a" type="m">gedrukt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Door ’t dragen van u <rhyme label="b" type="f">zonden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En onder vloek en schuld <rhyme label="a" type="m" corresp="#s4:l1">gebukt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Doe hy zig liet <rhyme label="b" type="f" corresp="#s4:l2">doorwonden</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dys heeft dat Lam te weeg <rhyme label="c" type="m">gebrogt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Verlossing en u vry <rhyme label="c" type="m" corresp="#s4:l5">gekogt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En door zijn bloed <rhyme label="d" type="f">verkregen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Iehovaas eeuwige <rhyme label="d" type="f" corresp="#s4:l7">zeegen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dus ben ik nu van alle <rhyme label="a" type="m">kwaat</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van zonden vloek en <rhyme label="b" type="f">tooren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Verlost, en een geheyligt <rhyme label="a" type="m" corresp="#s5:l1">zaat</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Gods Kind zijn <rhyme label="b" type="f" corresp="#s5:l2">Uytverkooren</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Gods gunstig vry en liefde <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Al kom ik eens in kruys en <rhyme label="c" type="m" corresp="#s5:l5">smert</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Is t’mywaards steeds <rhyme label="d" type="f">bewoogen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Die liefd’ zal nooyt <rhyme label="d" type="f" corresp="#s5:l7">gedoogen</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dat ik na lighaam of na <rhyme label="a" type="m">geest</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zal smert of honger <rhyme label="b" type="f">lyden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maar zal my als ik ’t <rhyme label="a" type="m" corresp="#s6:l1">allermeest</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Van noden heb <rhyme label="b" type="f" corresp="#s6:l2">verblyden</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Zijn goed is mijn, hy zelfs mijn <rhyme label="c" type="m">God</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Het deel en mijner erv en <rhyme label="c" type="m" corresp="#s6:l5">lot</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die my nu en na <rhyme label="d" type="f">deezen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Tot zaligheid zal <rhyme label="d" type="f" corresp="#s6:l7">weezen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De Heewre is mijn heyl en <rhyme label="a" type="m">ligt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mijn sterkte en mijn <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Hy zal my leeren mynen <rhyme label="a" type="m" corresp="#s7:l1">pligt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En my genade <rhyme label="b" type="f" corresp="#s7:l2">geeven</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Om dat te lyden en te <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Waar door ik moet ten hemel <rhyme label="c" type="m" corresp="#s7:l5">spoen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Zijn Geest zal my <rhyme label="d" type="f">geleyden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">My troosten en <rhyme label="d" type="f" corresp="#s7:l7">verblyden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ag mogt ik maar tot lof en <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Van God Drie-eenig <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ag dat de Heere daar tot <rhyme label="a" type="m" corresp="#s8:l1">meer</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Genade aan my <rhyme label="b" type="f" corresp="#s8:l2">geeven</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Om steeds die God van <rhyme label="c" type="m">zaligheid</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Die voor den Hemel my <rhyme label="c" type="m" corresp="#s8:l5">bereyd</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Met hertelijke <rhyme label="d" type="f">vreugden</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Te roemen in zijn <rhyme label="d" type="f" corresp="#s8:l7">deugden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat is dog ’t eeuwigduurent <rhyme label="a" type="m">Werk</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Der reets volmaakte <rhyme label="b" type="f">zielen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En boven lugt en boven <rhyme label="a" type="m" corresp="#s9:l1">zwerk</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Voor haar ontfermer <rhyme label="b" type="f" corresp="#s9:l2">knielen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">En met Gods beeld in zuyvere <rhyme label="c" type="m">deugd</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">En storelooze <rhyme label="c" type="m" corresp="#s9:l5">vreugd</rhyme>,.</l>
        <l id="s9:l7" met="3" type="-+">Met al de <rhyme label="d" type="f">Hemellingen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Gods lof voor eeuwig <rhyme label="d" type="f" corresp="#s9:l7">zingen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
