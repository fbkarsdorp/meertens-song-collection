    <date>
      <year-start>1806</year-start>
      <year-end>1806</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Quand la bergère vient des champs</melody>
  <author>Jan Steven van Esveldt Holtrop, Willem Holtrop</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">A notre Maître, dans ce <rhyme label="a" type="m">jour</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Frères, témeignons notre <rhyme label="a" type="m" corresp="#s1:l1">amour</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Par les honneurs qui lui sont <rhyme label="b" type="m">dùs</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Que tout <rhyme label="c" type="f">s'apprête</rhyme>!</l>
        <l id="s1:l5" met="2" type="-+">Chantons sa <rhyme label="c" type="f" corresp="#s1:l4">lête</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Et ses <rhyme label="b" type="m" corresp="#s1:l3">vertus</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dans sa bouche la <rhyme label="a" type="m">verité</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dans ses yeux la <rhyme label="a" type="m" corresp="#s2:l1">sérènité</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sur son sront la noble <rhyme label="b" type="m">candeur</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Montrent le <rhyme label="c" type="f">sage</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Et son <rhyme label="c" type="f" corresp="#s2:l4">visage</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Nous peint son <rhyme label="b" type="m" corresp="#s2:l3">coeur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Charitable et <rhyme label="a" type="m">compatissant</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Affable, doux et <rhyme label="a" type="m" corresp="#s3:l1">complaisant</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Modeste dans tout ce qu'il <rhyme label="b" type="m">sait</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Chacun <rhyme label="c" type="f">l'honnore</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Lui seul <rhyme label="c" type="f" corresp="#s3:l4">ignore</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Le bien qu'il <rhyme label="b" type="m" corresp="#s3:l3">sait</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sa présence anime nos <rhyme label="a" type="m">jeux</rhyme>.</l>
        <l id="s4:l2" met="4" type="-+">Il se plait à nous rendre <rhyme label="a" type="m" corresp="#s4:l1">heureux</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Sous l'aimable joug de ses <rhyme label="b" type="m">loix</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+">C'est à sa <rhyme label="c" type="f">gloire</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Qu'il nous faut <rhyme label="c" type="f" corresp="#s4:l4">boire</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Par trois sois <rhyme label="b" type="m" corresp="#s4:l3">trois</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
