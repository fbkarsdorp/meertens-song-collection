    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Bernardus Busschoff</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'t HOuw'lijck is een goede <rhyme label="a" type="m">saeck</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Groot <rhyme label="a" type="m" corresp="#s1:l1">vermaeck</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Als een Hemel hier <rhyme label="b" type="f">beneden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als die twee die zijn <rhyme label="a" type="m" corresp="#s1:l1">versaemt</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Soo 't <rhyme label="a" type="m" corresp="#s1:l1">betaemt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zijn als een door liefd' en <rhyme label="b" type="f" corresp="#s1:l3">vreden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als een Man van goeder <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Is <rhyme label="a" type="m" corresp="#s2:l1">gepaert</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Met een Vrouw' naer sijn <rhyme label="b" type="f">behagen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'t Moet al zware droefheydt <rhyme label="a" type="m" corresp="#s2:l1">zijn</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Groote <rhyme label="a" type="m" corresp="#s2:l1">pijn</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Die sy t'saem niet konnen <rhyme label="b" type="f" corresp="#s2:l3">dragen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Saem gevlochten handen <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Groote <rhyme label="a" type="m" corresp="#s3:l1">last</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dragen door eendrachtigh <rhyme label="b" type="f">helpen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">t' Tsaem-gevoeghde herten <rhyme label="a" type="m" corresp="#s3:l1">trouw'</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Grooten <rhyme label="a" type="m" corresp="#s3:l1">rouw'</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Door eendrachtigh dragen <rhyme label="b" type="f" corresp="#s3:l3">stelpen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wee den mensch, 't zy groot of <rhyme label="a" type="m">kleen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Die <rhyme label="a" type="m" corresp="#s4:l1">alleen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Leeft en zweeft 't is noyt te <rhyme label="b" type="f">degen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Maer geluckigh is de <rhyme label="a" type="m" corresp="#s4:l1">Mensch</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Die naer <rhyme label="a" type="m" corresp="#s4:l1">wensch</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Goet geselschap heeft <rhyme label="b" type="f" corresp="#s4:l3">gekregen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dit geluck hebt gy <rhyme label="a" type="m">voorwaer</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Deughdigh <rhyme label="a" type="m" corresp="#s5:l1">paer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Siet om hoogh het komt van <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">'t Komt van Godt <rhyme label="a" type="m" corresp="#s5:l1">gebenedijt</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Wilt <rhyme label="a" type="m" corresp="#s5:l1">altijdt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Hem met danckbaer herten <rhyme label="b" type="f" corresp="#s5:l3">loven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Altijdt looft en leeft den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Geeft hem <rhyme label="a" type="m" corresp="#s6:l1">eer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Hy sal u wed'rom <rhyme label="b" type="f">vereeren</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Zeeg'nen met een milde <rhyme label="a" type="m" corresp="#s6:l1">handt</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Hier in 't <rhyme label="a" type="m" corresp="#s6:l1">Landt</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Beyd' u zaet en staet <rhyme label="b" type="f" corresp="#s6:l3">vermeeren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Soo de Heer steedts <rhyme label="a" type="m">Vreughden-wijn</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Noyt <rhyme label="a" type="m" corresp="#s7:l1">azijn</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Komt in uwen beecker <rhyme label="b" type="f">gieten</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Denckt dat sijne <rhyme label="a" type="m" corresp="#s7:l1">Majesteyt</rhyme></l>
        <l id="s7:l5" met="2" type="+-"><rhyme label="a" type="m" corresp="#s7:l1">Danckbaerheydt</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Voor die weldaet wil <rhyme label="b" type="f" corresp="#s7:l3">genieten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Soo hy oock u sendet <rhyme label="a" type="m">t'huys</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">'t Bitter <rhyme label="a" type="m" corresp="#s8:l1">Kruys</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Weest niet al te seer <rhyme label="b" type="f">verslagen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Denckt dat sulcks uw' Vader <rhyme label="a" type="m" corresp="#s8:l1">goet</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Wijslijck <rhyme label="a" type="m" corresp="#s8:l1">doet</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Weest gerust in sijn <rhyme label="b" type="f" corresp="#s8:l3">behagen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wilt dit Paer doch veel <rhyme label="a" type="m">gelucks</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Weynigh <rhyme label="a" type="m" corresp="#s9:l1">drucks</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Soo 't kan saligh zijn Heer <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Maer voor al geeft ons en <rhyme label="a" type="m" corresp="#s9:l1">haer</rhyme></l>
        <l id="s9:l5" met="2" type="+-">Heer hier <rhyme label="a" type="m" corresp="#s9:l1">naer</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Uyt genaed' het eeuwigh <rhyme label="b" type="f" corresp="#s9:l3">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
