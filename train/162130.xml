<song>
  <meta>
    <id>162130</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Een kindje is ons geboren</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Liefde seer hoogh <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Duert in der <rhyme label="b" type="m">eeuwigheyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">De Liefde gelijck wy <rhyme label="a" type="f" corresp="#s1:l1">lesen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Is vol van <rhyme label="b" type="m" corresp="#s1:l2">deuchtsaemheyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Sy baert een soete <rhyme label="c" type="f">vreught</rhyme></l>
        <l id="s1:l6" met="2" type="-+">In 't Huwelijck <rhyme label="d" type="m">gewis</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Geluckigh zijn de <rhyme label="c" type="f" corresp="#s1:l5">paren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Wiens Trouringh Liefden <rhyme label="d" type="m" corresp="#s1:l6">is</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Liefde tusschen <rhyme label="a" type="f">beyden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is oorsaeck van <rhyme label="b" type="m">geneught</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En kan de zieltjes oock <rhyme label="a" type="f" corresp="#s2:l1">leyden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In vreed en soete <rhyme label="b" type="m" corresp="#s2:l2">vreught</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">De Liefde die <rhyme label="c" type="f">verdrijft</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Het schadelijck <rhyme label="d" type="m">fenijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Laet Liefd', o soete <rhyme label="c" type="f" corresp="#s2:l5">Bruytje</rhyme></l>
        <l id="s2:l8" met="3" type="-+">U rechte Trouringh <rhyme label="d" type="m" corresp="#s2:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Liefde die is <rhyme label="a" type="f">lanckmoedigh</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En vol van <rhyme label="b" type="m">vriend'lijckheyt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Vol deuchden en seer <rhyme label="a" type="f" corresp="#s3:l1">sachtmoedigh</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En vol <rhyme label="b" type="m" corresp="#s3:l2">barmhertigheyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Sy is in 't <rhyme label="c" type="f">Huwelijck</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Een gulde <rhyme label="d" type="m">Sonne-schijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Laet Liefd', o soete <rhyme label="c" type="f" corresp="#s3:l5">Bruytje</rhyme></l>
        <l id="s3:l8" met="3" type="-+">U rechte Trouringh <rhyme label="d" type="m" corresp="#s3:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Liefde is niet <rhyme label="a" type="f">afgonstigh</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gelijck men dickmael <rhyme label="b" type="m">siet</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Maer nederigh en <rhyme label="a" type="f" corresp="#s4:l1">goed-jonstigh</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Zy soeckt haer eygen <rhyme label="b" type="m" corresp="#s4:l2">niet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zy is oprecht en <rhyme label="c" type="f">goet</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Zy toont geen valsche <rhyme label="d" type="m">schijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Laet Liefd' o soete <rhyme label="c" type="f" corresp="#s4:l5">Bruytje</rhyme></l>
        <l id="s4:l8" met="3" type="-+">U rechte Trouringh <rhyme label="d" type="m" corresp="#s4:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Liefde die kan <rhyme label="a" type="f">verbinden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Twee hertjes in de <rhyme label="b" type="m">deught</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Door Liefde twee <rhyme label="a" type="f" corresp="#s5:l1">eens-gesinde</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die leven in soete <rhyme label="b" type="m" corresp="#s5:l2">vreught</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">De Liefde bindt den <rhyme label="c" type="f">Echt</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Seer lieffelijck en <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Laet Liefd', o soete <rhyme label="c" type="f" corresp="#s5:l5">Bruytje</rhyme></l>
        <l id="s5:l8" met="3" type="-+">U rechte Trouringh <rhyme label="d" type="m" corresp="#s5:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wy wenschen al t'samen u <rhyme label="a" type="f">heden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">O Bruydegom en <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Des Heeren gunst ende <rhyme label="a" type="f" corresp="#s6:l1">vrede</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">De Liefde tot <rhyme label="b" type="m" corresp="#s6:l2">besluyt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat ghy de ware <rhyme label="c" type="f">deught</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Steets werdet in <rhyme label="d" type="m">gelijft</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Dat Liefde al u <rhyme label="c" type="f" corresp="#s6:l5">leven</rhyme></l>
        <l id="s6:l8" met="3" type="-+">U rechte Trouringh <rhyme label="d" type="m" corresp="#s6:l6">blijft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
