      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Maar wat hoor, maar wat <rhyme label="a" type="m">hoor</rhyme></l>
        <l id="s1:l2" met="4" type="+-">ik hier aan myn regter <rhyme label="a" type="m" corresp="#s1:l1">oor</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">De stomme blaadjes <rhyme label="b" type="f">spreken</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Zou myn lieve <rhyme label="c" type="m">Veld-Diaan</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Om 't steken van de Zon zig <rhyme label="c" type="m" corresp="#s1:l4">gaan</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In 't dichte lof <rhyme label="b" type="f" corresp="#s1:l3">versteken</rhyme>?</l>
