<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>26433</id>
    <date>
      <year-start>1584</year-start>
      <year-end>1600</year-end>
    </date>
    <location>
      <orginal-name>gent</orginal-name>
      <standard-name>Gent, Belgium</standard-name>
      <coordinates>
        <lon>51.0538286</lon>
        <lat>3.7250121</lat>
      </coordinates>
    </location>
    <tags>
      <tag>political song</tag>
    </tags>
    <melody>Der Filistijnen stam kwam</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Bemerct nu uwen val <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghendts Calvinist <rhyme label="a" type="m" corresp="#s1:l1">ghetal</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Docteuren, naer u cas, <rhyme label="b" type="f">ras</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Surgijn oft wyse <rhyme label="c" type="m">liên</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Wilt se <rhyme label="c" type="m" corresp="#s1:l4">bespiên</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">En haest <rhyme label="c" type="m" corresp="#s1:l4">ontbiên</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Oft gaet tot Mr <rhyme label="b" type="f" corresp="#s1:l3">Lucas</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die zal u uryne <rhyme label="c" type="m" corresp="#s1:l4">besien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Tot Dierick Besa om raedt <rhyme label="a" type="m">gaet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Eer 't u ter herten <rhyme label="a" type="m" corresp="#s2:l1">slaet</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Vraeght Zwinglius oft <rhyme label="b" type="f">Melon</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En Mr Pieter <rhyme label="c" type="m">Viret</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Wat dat u <rhyme label="c" type="m" corresp="#s2:l4">let</rhyme></l>
        <l id="s2:l6" met="2" type="-+">An hooft oft <rhyme label="c" type="m" corresp="#s2:l4">zet</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Die zullen u zegghen <rhyme label="b" type="f" corresp="#s2:l3">waerom</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat valsch es u <rhyme label="c" type="m" corresp="#s2:l4">opset</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Al uwen valschen raedt, <rhyme label="a" type="m">daet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die tendeert al tot <rhyme label="a" type="m" corresp="#s3:l1">quaet</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Anziet die eerste <rhyme label="b" type="f">practycque</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die ghy deet binnen <rhyme label="c" type="m">Ghendt</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Vier ordens <rhyme label="c" type="m" corresp="#s3:l4">gheschent</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">En hemlieden <rhyme label="c" type="m" corresp="#s3:l4">convent</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Maer roovage <rhyme label="b" type="f" corresp="#s3:l3">publycque</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Die was allom <rhyme label="c" type="m" corresp="#s3:l4">ontrent</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Kercken en cloosters daer <rhyme label="a" type="m">naer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Zagh men bederven <rhyme label="a" type="m" corresp="#s4:l1">claer</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Beelden schoffieren en <rhyme label="b" type="f">schenden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En rooven al het <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Te peerde en te <rhyme label="c" type="m" corresp="#s4:l4">voet</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Vry metter <rhyme label="c" type="m" corresp="#s4:l4">spoet</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Ghy slacht al <rhyme label="b" type="f" corresp="#s4:l3">Calis-benden</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">Valsch is 't al dat ghy <rhyme label="c" type="m" corresp="#s4:l4">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'t En was den afgod niet, <rhyme label="a" type="m">ziet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die ghylieden hadt <rhyme label="a" type="m" corresp="#s5:l1">bespiet</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Maer 't was rooven en <rhyme label="b" type="f">stelen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat ghylieden hadt voor <rhyme label="c" type="m">dhant</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">An elcken <rhyme label="c" type="m" corresp="#s5:l4">cant</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Goud, zelver en <rhyme label="c" type="m" corresp="#s5:l4">pant</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">'t En mochte u niet <rhyme label="b" type="f" corresp="#s5:l3">vervelen</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Robynen noch <rhyme label="c" type="m" corresp="#s5:l4">diamant</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Tin, yser en metael, <rhyme label="a" type="m">stael</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dienden u <rhyme label="a" type="m" corresp="#s6:l1">t'eenemael</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Loot, coper, ketels, <rhyme label="b" type="f">pannen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Moesten 't becoopen <rhyme label="c" type="m">zaen</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">'t Moest mede <rhyme label="c" type="m" corresp="#s6:l4">gaen</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">'t Was haest <rhyme label="c" type="m" corresp="#s6:l4">ghedaen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Pot, schotels, lepels, <rhyme label="b" type="f" corresp="#s6:l3">cannen</rhyme>;</l>
        <l id="s6:l8" met="3" type="-+">Daer en moght niet blyven <rhyme label="c" type="m" corresp="#s6:l4">staen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Rijs, blocken, turven, haut, <rhyme label="a" type="m">smaut</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Coorne, taerwe ende <rhyme label="a" type="m" corresp="#s7:l1">maut</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Coeyen, peerden ofte <rhyme label="b" type="f">schapen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Swynen en gansen in 't <rhyme label="c" type="m">cot</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Was den <rhyme label="c" type="m" corresp="#s7:l4">afgodt</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Verstaet wel 't <rhyme label="c" type="m" corresp="#s7:l4">slot</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Die men u zach <rhyme label="b" type="f" corresp="#s7:l3">betrapen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En deelen in elck <rhyme label="c" type="m" corresp="#s7:l4">rot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Erfve, bosch, boomen, landt, <rhyme label="a" type="m">sant</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Zagh men an elcken <rhyme label="a" type="m" corresp="#s8:l1">cant</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Thiende, pachtbrieven oft <rhyme label="b" type="f">rente</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Trecken tot ulieder <rhyme label="c" type="m">boort</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">Ghy gaet'er mê <rhyme label="c" type="m" corresp="#s8:l4">voort</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Het wordt <rhyme label="c" type="m" corresp="#s8:l4">gheschoort</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Ghy crijght'er af goede <rhyme label="b" type="f" corresp="#s8:l3">vente</rhyme>;</l>
        <l id="s8:l8" met="3" type="-+">Ghy meent dat u <rhyme label="c" type="m" corresp="#s8:l4">behoort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Als 't al ghesloten was, <rhyme label="a" type="m">ras</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Zach men al up dat <rhyme label="a" type="m" corresp="#s9:l1">pas</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Stellen <rhyme label="b" type="f">Religions-vrede</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Die men heet <rhyme label="c" type="m">Liberteyt</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">Een cleen <rhyme label="c" type="m" corresp="#s9:l4">bescheyt</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Zoo elck wel <rhyme label="c" type="m" corresp="#s9:l4">weet</rhyme>;</l>
        <l id="s9:l7" met="3" type="-+">Men dede, te Ghendt in <rhyme label="b" type="f" corresp="#s9:l3">stede</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Recht, reden noch <rhyme label="c" type="m" corresp="#s9:l4">equiteyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">'t Accord dat was ghedaen, <rhyme label="a" type="m">zaen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Besworen, alzo ic <rhyme label="a" type="m" corresp="#s10:l1">waen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">By broeder Pieter <rhyme label="b" type="f">Gaethenen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En Hembyze <rhyme label="c" type="m">excellent</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Heere en wet <rhyme label="c" type="m" corresp="#s10:l4">present</rhyme>.</l>
        <l id="s10:l6" met="2" type="-+">'t Gheestelick <rhyme label="c" type="m" corresp="#s10:l4">content</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Al waeydet ieghens haer <rhyme label="b" type="f" corresp="#s10:l3">schenen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Bezwoeren 't ooc <rhyme label="c" type="m" corresp="#s10:l4">promptement</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Vier kercken excellent, <rhyme label="a" type="m">gent</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Haddet 't gheestelick binnen <rhyme label="a" type="m" corresp="#s11:l1">Ghent</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">Broêr Paulus preecte in <rhyme label="b" type="f">d'eene</rhyme>;</l>
        <l id="s11:l4" met="3" type="-+">Dat was een <rhyme label="c" type="m">Augustijn</rhyme>.</l>
        <l id="s11:l5" met="2" type="-+">Een <rhyme label="c" type="m" corresp="#s11:l4">Jacoppijn</rhyme>,</l>
        <l id="s11:l6" met="2" type="-+">Potter, hielt hem <rhyme label="c" type="m" corresp="#s11:l4">fijn</rhyme>;</l>
        <l id="s11:l7" met="3" type="-+">T'Sente Michiels <rhyme label="b" type="f" corresp="#s11:l3">ghemeene</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Was Frermineurs <rhyme label="c" type="m" corresp="#s11:l4">termijn</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Dees predicanten vier, <rhyme label="a" type="m">schier</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Quamen in groot <rhyme label="a" type="m" corresp="#s12:l1">dangier</rhyme>;</l>
        <l id="s12:l3" met="3" type="-+">Te preken was haer <rhyme label="b" type="f">verboden</rhyme>;</l>
        <l id="s12:l4" met="3" type="-+">Zy moesten 't laten <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s12:l5" met="2" type="-+">En elders <rhyme label="c" type="m" corresp="#s12:l4">gaen</rhyme>.</l>
        <l id="s12:l6" met="2" type="-+">Hoort mijn <rhyme label="c" type="m" corresp="#s12:l4">vermaen</rhyme>;</l>
        <l id="s12:l7" met="3" type="-+">Papen en nonnen <rhyme label="b" type="f" corresp="#s12:l3">vloden</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">'t En hielp niet wat zy <rhyme label="c" type="m" corresp="#s12:l4">baên</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Frederic Haeck, snoot callant, <rhyme label="a" type="m">quant</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">En Scheerpaert, den <rhyme label="a" type="m" corresp="#s13:l1">trawant</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Quamen in die vier <rhyme label="b" type="f">kercken</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Ghewapent, harde <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s13:l5" met="2" type="-+">In dit <rhyme label="c" type="m" corresp="#s13:l4">bestel</rhyme>,</l>
        <l id="s13:l6" met="2" type="-+">Als honden <rhyme label="c" type="m" corresp="#s13:l4">fel</rhyme>.</l>
        <l id="s13:l7" met="3" type="-+">Catholijcke ende <rhyme label="b" type="f" corresp="#s13:l3">clercken</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Deden zy groot <rhyme label="c" type="m" corresp="#s13:l4">ghequel</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Doen wasser meer dan eerst, <rhyme label="a" type="m">meest</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Gherooft, ghetrocken, <rhyme label="a" type="m" corresp="#s14:l1">gheteest</rhyme>;</l>
        <l id="s14:l3" met="3" type="-+">Kercken en cloosters <rhyme label="b" type="f">mede</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Zeer bynaer <rhyme label="c" type="m">gheraseert</rhyme>;</l>
        <l id="s14:l5" met="2" type="-+">Jae <rhyme label="c" type="m" corresp="#s14:l4">ghemineert</rhyme>,</l>
        <l id="s14:l6" met="2" type="-+">En <rhyme label="c" type="m" corresp="#s14:l4">ghecasseert</rhyme>.</l>
        <l id="s14:l7" met="3" type="-+">Adieu, <rhyme label="b" type="f" corresp="#s14:l3">Religions-vrede</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Ghy en wordt niet meer <rhyme label="c" type="m" corresp="#s14:l4">gheuseert</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Dit was zeer obstinaet, <rhyme label="a" type="m">quaet</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Gaethenus valschen <rhyme label="a" type="m" corresp="#s15:l1">raet</rhyme>.</l>
        <l id="s15:l3" met="3" type="-+">Dat hy eerst hadde <rhyme label="b" type="f">besworen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Daer en hilt hy nu af <rhyme label="c" type="m">niet</rhyme>;</l>
        <l id="s15:l5" met="2" type="-+">Wanckelbaer <rhyme label="c" type="m" corresp="#s15:l4">ziet</rhyme>,</l>
        <l id="s15:l6" met="2" type="-+">Al waer 't een <rhyme label="c" type="m" corresp="#s15:l4">riet</rhyme>;</l>
        <l id="s15:l7" met="3" type="-+">Valschen apostaet <rhyme label="b" type="f" corresp="#s15:l3">gheschoren</rhyme></l>
        <l id="s15:l8" met="3" type="-+">Es hy, en anders <rhyme label="c" type="m" corresp="#s15:l4">niet</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Zijn trauwe en zynen eedt, <rhyme label="a" type="m">breet</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Heeft hy vervalscht, God <rhyme label="a" type="m" corresp="#s16:l1">weet</rhyme>!</l>
        <l id="s16:l3" met="3" type="-+">Zijn cap ghehanghen op <rhyme label="b" type="f">d'haghe</rhyme></l>
        <l id="s16:l4" met="3" type="-+">T'Ypre, als <rhyme label="c" type="m">carmelijt</rhyme>,</l>
        <l id="s16:l5" met="2" type="-+">Die elc <rhyme label="c" type="m" corresp="#s16:l4">beschijt</rhyme></l>
        <l id="s16:l6" met="2" type="-+">In allen <rhyme label="c" type="m" corresp="#s16:l4">tijt</rhyme>;</l>
        <l id="s16:l7" met="3" type="-+">Hy zal 't in corten <rhyme label="b" type="f" corresp="#s16:l3">daghen</rhyme></l>
        <l id="s16:l8" met="3" type="-+">Beweenen breet en <rhyme label="c" type="m" corresp="#s16:l4">wijt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Up papen en nonnen acht <rhyme label="a" type="m">zacht</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Al eyst dat ghy zeer <rhyme label="a" type="m" corresp="#s17:l1">lacht</rhyme>.</l>
        <l id="s17:l3" met="3" type="-+">Den tijt moght noch wel <rhyme label="b" type="f">commen</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Dat ghy se zoudt <rhyme label="c" type="m">ontbiên</rhyme>,</l>
        <l id="s17:l5" met="2" type="-+">Begheeren te <rhyme label="c" type="m" corresp="#s17:l4">zien</rhyme>,</l>
        <l id="s17:l6" met="2" type="-+">Up beede u <rhyme label="c" type="m" corresp="#s17:l4">knien</rhyme>;</l>
        <l id="s17:l7" met="3" type="-+">Up die leere <rhyme label="b" type="f" corresp="#s17:l3">gheclommen</rhyme>,</l>
        <l id="s17:l8" met="3" type="-+">'t En zal u nauwe <rhyme label="c" type="m" corresp="#s17:l4">gheschiên</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Borghers, anziet nu hier <rhyme label="a" type="m">schier</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Datthenus snoo <rhyme label="a" type="m" corresp="#s18:l1">bestier</rhyme>.</l>
        <l id="s18:l3" met="3" type="-+">Hier aen mueghdy wel <rhyme label="b" type="f">mercken</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Dat valsch es zijn <rhyme label="c" type="m">upstel</rhyme>;</l>
        <l id="s18:l5" met="2" type="-+">En zynen <rhyme label="c" type="m" corresp="#s18:l4">rel</rhyme></l>
        <l id="s18:l6" met="2" type="-+">Es boos en <rhyme label="c" type="m" corresp="#s18:l4">fel</rhyme>,</l>
        <l id="s18:l7" met="3" type="-+">Om rooven cluusen en <rhyme label="b" type="f" corresp="#s18:l3">kercken</rhyme>,</l>
        <l id="s18:l8" met="3" type="-+">En den Conynck te zijn <rhyme label="c" type="m" corresp="#s18:l4">rebel</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Ghy calversteerten voort, <rhyme label="a" type="m">hoort</rhyme>;</l>
        <l id="s19:l2" met="3" type="-+">Vertrect nu rechte <rhyme label="a" type="m" corresp="#s19:l1">voort</rhyme></l>
        <l id="s19:l3" met="3" type="-+">Uut Ghendt, die schoone <rhyme label="b" type="f">stede</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">En laet die zoo zy <rhyme label="c" type="m">was</rhyme>;</l>
        <l id="s19:l5" met="2" type="-+">Vlucht over den <rhyme label="c" type="m" corresp="#s19:l4">plas</rhyme>,</l>
        <l id="s19:l6" met="2" type="-+">Haest u zeer <rhyme label="c" type="m" corresp="#s19:l4">ras</rhyme>;</l>
        <l id="s19:l7" met="3" type="-+">Leedt u ministers <rhyme label="b" type="f" corresp="#s19:l3">mede</rhyme>,</l>
        <l id="s19:l8" met="3" type="-+">Raed ic u up dit <rhyme label="c" type="m" corresp="#s19:l4">pas</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Prince, bekeert u pleyn <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">'t Es meer dan tijt <rhyme label="a" type="m" corresp="#s20:l1">certeyn</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Haudt u aen 't oude <rhyme label="b" type="f">gheloove</rhyme>.</l>
        <l id="s20:l4" met="3" type="-+">Den catholicken <rhyme label="c" type="m">pilaer</rhyme>,</l>
        <l id="s20:l5" met="2" type="-+">Vijfthien hondert <rhyme label="c" type="m" corresp="#s20:l4">jaer</rhyme></l>
        <l id="s20:l6" met="2" type="-+">Gheduert <rhyme label="c" type="m" corresp="#s20:l4">voorwaer</rhyme>;</l>
        <l id="s20:l7" met="3" type="-+">Hy zal 't noch al <rhyme label="b" type="f" corresp="#s20:l3">deurclooven</rhyme>,</l>
        <l id="s20:l8" met="3" type="-+">'t Staet ons bescreven <rhyme label="c" type="m" corresp="#s20:l4">claer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
