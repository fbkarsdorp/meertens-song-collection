<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>1437</id>
    <date>
      <year-start>1613</year-start>
      <year-end>1613</year-end>
    </date>
    <location>
      <orginal-name>amsterdam?</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon liefje jent zeer excellent</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SChoon liefken jent, seer <rhyme label="a" type="m">excellent</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Ick moet u <rhyme label="b" type="f">claghen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Dat ick <rhyme label="a" type="m" corresp="#s1:l1">absent</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van u dus blijf <rhyme label="a" type="m" corresp="#s1:l1">ghewent</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dus lijdt mijn hert <rhyme label="a" type="m" corresp="#s1:l1">torment</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Van daghe te <rhyme label="b" type="f" corresp="#s1:l2">daghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hoe dickwils hebben wy met alle <rhyme label="a" type="m">melody</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Gheneuchte <rhyme label="b" type="f">bedreven</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Mijn hert was <rhyme label="a" type="m" corresp="#s2:l1">bly</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Als ick mocht comen <rhyme label="a" type="m" corresp="#s2:l1">by</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Schoon Liefken aen u <rhyme label="a" type="m" corresp="#s2:l1">sy</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Door u soet <rhyme label="b" type="f" corresp="#s2:l2">aencleven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Al waer ick quaet, haer fiere <rhyme label="a" type="m">gelaet</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Cant al <rhyme label="b" type="f">ghenesen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Mijn hart dat <rhyme label="a" type="m" corresp="#s3:l1">vaet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Al waert in <rhyme label="a" type="m" corresp="#s3:l1">desperaet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Al door u soete <rhyme label="a" type="m" corresp="#s3:l1">praet</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Als ghy hebt <rhyme label="b" type="f" corresp="#s3:l2">bewesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick roep o wach, nacht ende <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Dat ickse moet <rhyme label="b" type="f">derven</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Die ick <rhyme label="a" type="m" corresp="#s4:l1">aensach</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dickwils met soet <rhyme label="a" type="m" corresp="#s4:l1">gheclach</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Niet langer spreken en <rhyme label="a" type="m" corresp="#s4:l1">mach</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Ick salt <rhyme label="b" type="f" corresp="#s4:l2">besterven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Waerom lief reyn, soet edel <rhyme label="a" type="m">greyn</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Niet en hebt <rhyme label="b" type="f">ontfanghen</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Mijn trou <rhyme label="a" type="m" corresp="#s5:l1">alleyn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die ick u boot <rhyme label="a" type="m" corresp="#s5:l1">pleyn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Wt liefden heel <rhyme label="a" type="m" corresp="#s5:l1">ghemeyn</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Hebt laten <rhyme label="b" type="f" corresp="#s5:l2">verganghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dat ick niet meer, mach comen <rhyme label="a" type="m">weer</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Dat doen qua <rhyme label="b" type="f">tonghen</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Dus ick <rhyme label="a" type="m" corresp="#s6:l1">verteer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Mijn hert in lijden <rhyme label="a" type="m" corresp="#s6:l1">seer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Sy houden my ter <rhyme label="a" type="m" corresp="#s6:l1">neer</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Als een Kindeken <rhyme label="b" type="f" corresp="#s6:l2">bedwonghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Princesse confoort, weest niet  <rhyme label="a" type="m">verstoort</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Wilt u doch <rhyme label="b" type="f">keeren</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Nae 't soet <rhyme label="a" type="m" corresp="#s7:l1">accoort</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Schoon liefken u doch <rhyme label="a" type="m" corresp="#s7:l1">spoort</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Onthout van my dit <rhyme label="a" type="m" corresp="#s7:l1">woort</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Met deuchden ende <rhyme label="b" type="f" corresp="#s7:l2">eeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
