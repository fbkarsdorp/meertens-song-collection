<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100008</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags>
      <tag>mythological song</tag>
    </tags>
    <melody>Zal ik nog langer in hete tranen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">TOen Arethusa docht haar <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Door 't Jagen in de <rhyme label="b" type="m">Middag-Zon</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Besweet, en 't enden aam <rhyme label="a" type="f" corresp="#s1:l1">gezeden</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Te koelen in een koele <rhyme label="b" type="m" corresp="#s1:l2">bron</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Smeet zy in't <rhyme label="c" type="m">gras</rhyme></l>
        <l id="s1:l6" met="2" type="-+">daar niemant <rhyme label="c" type="m" corresp="#s1:l5">was</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Haar Jacht <rhyme label="d" type="m">gewaat</rhyme></l>
        <l id="s1:l8" met="2" type="-+">en zich <rhyme label="d" type="m" corresp="#s1:l7">verzaat</rhyme>,</l>
        <l id="s1:l9" met="1" type="-+">Met <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s1:l10" met="1" type="-+">met <rhyme label="e" type="m" corresp="#s1:l9">een</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">In 't zien van haar albaste <rhyme label="e" type="m" corresp="#s1:l9">leen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zy treet met wel gemaakte <rhyme label="a" type="f">voetjes</rhyme></l>
        <l id="s2:l2" met="4" type="-+">In 't water, uit een luchten <rhyme label="b" type="m">zin</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">En stapt, al zachjes en al <rhyme label="a" type="f" corresp="#s2:l1">zoetjes</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Te met daar noch wat dieper <rhyme label="b" type="m" corresp="#s2:l2">in</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Doch hoort <rhyme label="c" type="m">terstont</rhyme></l>
        <l id="s2:l6" met="2" type="-+">hoe dat de <rhyme label="c" type="m" corresp="#s2:l5">gront</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Een gallem <rhyme label="d" type="m">sloeg</rhyme></l>
        <l id="s2:l8" met="2" type="-+">die haar <rhyme label="d" type="m" corresp="#s2:l7">verjoeg</rhyme>,</l>
        <l id="s2:l9" met="1" type="-+">En <rhyme label="e" type="m">bracht</rhyme>,</l>
        <l id="s2:l10" met="1" type="-+">en <rhyme label="e" type="m" corresp="#s2:l9">bracht</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">In vreeze van te zijn <rhyme label="e" type="m" corresp="#s2:l9">verkracht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zy keert wees om, vol schricks en <rhyme label="a" type="f">bevens</rhyme>.</l>
        <l id="s3:l2" met="4" type="-+">En springt ter halver lijf daar <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maar voelt het watertje vol <rhyme label="a" type="f" corresp="#s3:l1">levens</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En zag den Water-God: wiens <rhyme label="b" type="m" corresp="#s3:l2">zin</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Terstont de <rhyme label="c" type="m">Maacht</rhyme> //</l>
        <l id="s3:l6" met="2" type="-+">heeft <rhyme label="c" type="m" corresp="#s3:l5">nagejaacht</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Zy vloog en <rhyme label="d" type="m">vloot</rhyme> //</l>
        <l id="s3:l8" met="2" type="-+">als die de <rhyme label="d" type="m" corresp="#s3:l7">doot</rhyme></l>
        <l id="s3:l9" met="1" type="-+">Eer <rhyme label="e" type="m">koos</rhyme>,</l>
        <l id="s3:l10" met="1" type="-+">eer <rhyme label="e" type="m" corresp="#s3:l9">koos</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Als 't plukken van haar <rhyme label="e" type="m" corresp="#s3:l9">Maagde-Roos</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy volcht met onvermoeide <rhyme label="a" type="f">schreden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Geprikkelt door <rhyme label="b" type="m">d'aanminnigheit</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En 't zien van zulk <rhyme label="a" type="f" corresp="#s4:l1">aanvalligheden</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Hy roept, hy bid, hy smeekt, hy <rhyme label="b" type="m" corresp="#s4:l2">vleit</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Zie hoe een <rhyme label="c" type="m">God</rhyme> //</l>
        <l id="s4:l6" met="2" type="-+">u tot een <rhyme label="c" type="m" corresp="#s4:l5">lot</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Valt inde <rhyme label="d" type="m">schoot</rhyme> //</l>
        <l id="s4:l8" met="2" type="-+">uit hoge <rhyme label="d" type="m" corresp="#s4:l7">noot</rhyme>,</l>
        <l id="s4:l9" met="1" type="-+">En <rhyme label="e" type="m">'tschicht</rhyme>,</l>
        <l id="s4:l10" met="1" type="-+">en't <rhyme label="e" type="m" corresp="#s4:l9">schicht</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Van u aantrekkelik <rhyme label="e" type="m" corresp="#s4:l9">gezicht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Zy roept, ach kuisse der <rhyme label="a" type="f">Godessen</rhyme>.</l>
        <l id="s5:l2" met="4" type="-+">Bewaar, driedubbele <rhyme label="b" type="m">Diaan</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Bescherm u kuisse <rhyme label="a" type="f" corresp="#s5:l1">Priesteresse</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Ei zie haar uit u Hemel <rhyme label="b" type="m" corresp="#s5:l2">aan</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Die voor en <rhyme label="c" type="m">naar</rhyme> //</l>
        <l id="s5:l6" met="2" type="-+">op u <rhyme label="c" type="m" corresp="#s5:l5">Altaar</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Heeft reuk <rhyme label="d" type="m">gerookt</rhyme> //</l>
        <l id="s5:l8" met="2" type="-+">en vlam <rhyme label="d" type="m" corresp="#s5:l7">gestookt</rhyme>,</l>
        <l id="s5:l9" met="1" type="-+"><rhyme label="e" type="m">Bevry</rhyme>,</l>
        <l id="s5:l10" met="1" type="-+"><rhyme label="e" type="m" corresp="#s5:l9">bevry</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Die nu voor <rhyme label="e" type="m" corresp="#s5:l9">Maagde-schendery</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De schoone Iacht-Godin, <rhyme label="a" type="f">bewogen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Bedekt de naakte maacht <rhyme label="b" type="m">terstont</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">En trekt z'uit zijn begeerlijk <rhyme label="a" type="f" corresp="#s6:l1">oogen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Door wolken, die zy neder <rhyme label="b" type="m" corresp="#s6:l2">zont</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Maar hy <rhyme label="c" type="m">verwacht</rhyme> //</l>
        <l id="s6:l6" met="2" type="-+">tot zy <rhyme label="c" type="m" corresp="#s6:l5">versmacht</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Van angst en <rhyme label="d" type="m">leet</rhyme> //</l>
        <l id="s6:l8" met="2" type="-+">in 't <rhyme label="d" type="m" corresp="#s6:l7">Maagde-zweet</rhyme>,</l>
        <l id="s6:l9" met="1" type="-+">En <rhyme label="e" type="m">wort</rhyme>,</l>
        <l id="s6:l10" met="1" type="-+">en <rhyme label="e" type="m" corresp="#s6:l9">wort</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Een beek, die van een Heuvel <rhyme label="e" type="m" corresp="#s6:l9">stort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy roept, voor vluchtige <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Waar schuilt gy voor mijn Goddelijk <rhyme label="b" type="m">oog</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Al vloocht gy snelder als de <rhyme label="a" type="f" corresp="#s7:l1">winden</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Of pijl van een gespannen <rhyme label="b" type="m" corresp="#s7:l2">boog</rhyme>;</l>
        <l id="s7:l5" met="2" type="-+">Ik blijf u <rhyme label="c" type="m">by</rhyme> //</l>
        <l id="s7:l6" met="2" type="-+">met een maakt <rhyme label="c" type="m" corresp="#s7:l5">hy</rhyme></l>
        <l id="s7:l7" met="2" type="-+">(Die lichtlijk <rhyme label="d" type="m">kon</rhyme>) //</l>
        <l id="s7:l8" met="2" type="-+">zich weer een <rhyme label="d" type="m" corresp="#s7:l7">bron</rhyme></l>
        <l id="s7:l9" met="1" type="-+">En <rhyme label="e" type="m">kiest</rhyme>,</l>
        <l id="s7:l10" met="1" type="-+">en <rhyme label="e" type="m" corresp="#s7:l9">kiest</rhyme>,</l>
        <l id="s7:l11" met="4" type="+-">De kuilen, daar hy haar <rhyme label="e" type="m" corresp="#s7:l9">verliest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
