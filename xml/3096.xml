      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Windje daar het bos af drilt</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOort lodderlijcke <rhyme label="a" type="m">Galathee</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Laet u Herdertje <rhyme label="a" type="m" corresp="#s1:l1">Stribee</rhyme></l>
        <l id="s1:l3" met="2" type="+-">Zijn u <rhyme label="b" type="m">Minnaer</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s1:l3">verwinnaer</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En die geen, die in dit <rhyme label="c" type="m">Dal</rhyme></l>
        <l id="s1:l6" met="4" type="-+">U Maeghdoms blompje plucken <rhyme label="c" type="m" corresp="#s1:l5">sal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dan sullen Roosjes ende <rhyme label="a" type="m">Cruyt</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Spreyden hare Blaetjes <rhyme label="a" type="m" corresp="#s2:l1">uyt</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">En <rhyme label="b" type="m">Aurora</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Die sal <rhyme label="b" type="m" corresp="#s2:l3">Flora</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Met haer Suster <rhyme label="c" type="m">Pomone</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Streelen gelijckse Titan <rhyme label="c" type="m" corresp="#s2:l5">dee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ach uwen purpere roode <rhyme label="a" type="m">mondt</rhyme></l>
        <l id="s3:l2" met="4" type="+-">En u witte Borsjes <rhyme label="a" type="m" corresp="#s3:l1">rondt</rhyme></l>
        <l id="s3:l3" met="2" type="+-">Met u <rhyme label="b" type="m">vlechjes</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Daer de <rhyme label="b" type="m" corresp="#s3:l3">Knechjes</rhyme></l>
        <l id="s3:l5" met="4" type="+-">En de Minnaers van het <rhyme label="c" type="m">Landt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Yder sijn lusjes heeft <rhyme label="c" type="m" corresp="#s3:l5">geplant'</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die dwinghen my tot uwe <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dies mijn soete <rhyme label="a" type="m" corresp="#s4:l1">Herderin</rhyme></l>
        <l id="s4:l3" met="2" type="+-">Laet <rhyme label="b" type="m">Stribeetje</rhyme></l>
        <l id="s4:l4" met="2" type="+-">Met sijn <rhyme label="b" type="m" corresp="#s4:l3">Veetje</rhyme></l>
        <l id="s4:l5" met="4" type="+-">In u sneetje spelen <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dan voelt ghy wat Stribeetje <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Eerst sal ick uwe Lipjes <rhyme label="a" type="m">teer</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Kussende zijn, en dan wat <rhyme label="a" type="m" corresp="#s5:l1">neer</rhyme></l>
        <l id="s5:l3" met="2" type="+-">(Tot <rhyme label="b" type="m">verkoelen</rhyme>)</l>
        <l id="s5:l4" met="2" type="+-">Borsjes <rhyme label="b" type="m" corresp="#s5:l3">voelen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">En daer na met lust en <rhyme label="c" type="m">vree</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Spelen, als Mars en Venus <rhyme label="c" type="m" corresp="#s5:l5">dee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Nimphje hoe kijckje, wel hoe <rhyme label="a" type="m">dus</rhyme>?</l>
        <l id="s6:l2" met="4" type="+-">Meenjet lief soo geeftm' een <rhyme label="a" type="m" corresp="#s6:l1">Kus</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Siet hoe <rhyme label="b" type="m">bromtse</rhyme></l>
        <l id="s6:l4" met="2" type="+-">Och daer <rhyme label="b" type="m" corresp="#s6:l3">komtse</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Ach daer treetse na mijn <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Weet ick wat ick van vreughden <rhyme label="c" type="m" corresp="#s6:l5">doe</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Liefje komt leght u leden <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Aen de kant van de <rhyme label="a" type="m" corresp="#s7:l1">Fonteyn</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Op de <rhyme label="b" type="m">kruytjes</rhyme></l>
        <l id="s7:l4" met="2" type="+-">By de <rhyme label="b" type="m" corresp="#s7:l3">Spruytjes</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Aen den Oever van dees <rhyme label="c" type="m">Beeck</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Daer ick mijn teere Schaepjes <rhyme label="c" type="m" corresp="#s7:l5">queeck</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer smijt ick heen mijn Herders <rhyme label="a" type="m">Schop</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Heft u witte Kleertjes <rhyme label="a" type="m" corresp="#s8:l1">op</rhyme>,</l>
        <l id="s8:l3" met="2" type="+-">Och mijn <rhyme label="b" type="m">soetje</rhyme></l>
        <l id="s8:l4" met="2" type="+-">Al u <rhyme label="b" type="m" corresp="#s8:l3">bloetje</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Dat verandert en <rhyme label="c" type="m">verbleeckt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Liefje hoe komt dat ghy niet <rhyme label="c" type="m" corresp="#s8:l5">spreeckt</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer Galathe ick hebt <rhyme label="a" type="m">ghemerckt</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">'t Is U Maeghdom die dus <rhyme label="a" type="m" corresp="#s9:l1">werckt</rhyme>:</l>
        <l id="s9:l3" met="2" type="+-">Kom mijn <rhyme label="b" type="m">Susje</rhyme></l>
        <l id="s9:l4" met="2" type="+-">Neem een <rhyme label="b" type="m" corresp="#s9:l3">Kusje</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Siet eens u Stribeetje <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Waer dat sijn soete lonckjes <rhyme label="c" type="m" corresp="#s9:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Nu sal geen groven Satyr <rhyme label="a" type="m">stout</rhyme></l>
        <l id="s10:l2" met="4" type="+-">(In het dicht-beladen <rhyme label="a" type="m" corresp="#s10:l1">Wout</rhyme>)</l>
        <l id="s10:l3" met="2" type="+-">By der <rhyme label="b" type="m">Nachten</rhyme></l>
        <l id="s10:l4" met="2" type="+-">U <rhyme label="b" type="m" corresp="#s10:l3">verkrachten</rhyme>;</l>
        <l id="s10:l5" met="4" type="+-">Dus, als ghy om blompjes <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Neemt Stribeetje Lief te <rhyme label="c" type="m" corresp="#s10:l5">baet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
