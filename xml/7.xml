  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DE Trompette bliesen <rhyme label="a" type="f">allarme</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">onder de Spaensse ruyter <rhyme label="b" type="m">wacht</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">onder de Spaensse Ruyter <rhyme label="b" type="m" corresp="#s1:l2">wacht</rhyme> //</l>
        <l id="s1:l4" met="4" type="-+">soo de bende // sich wende, na <rhyme label="c" type="f">'tende</rhyme></l>
        <l id="s1:l5" met="5" type="+-">daer wy gedoocken lagen in de <rhyme label="d" type="m">gracht</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">daer wy gedoocken // daer wy <rhyme label="e" type="f">gedoocken</rhyme></l>
        <l id="s1:l7" met="3" type="+-">lagen in de <rhyme label="d" type="m" corresp="#s1:l5">gracht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Sa Soldaten laet ons <rhyme label="a" type="f">chargeeren</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Riep onsen vroomen <rhyme label="b" type="m">Officier</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Riep onsen vroomen <rhyme label="b" type="m" corresp="#s2:l2">Officier</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Doen haer Ruyters met Roers, en <rhyme label="c" type="f">Pistolen</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">en Carabijnen Gaven vier op <rhyme label="d" type="m">vier</rhyme></l>
        <l id="s2:l6" met="4" type="-+">En Carabijnen // en <rhyme label="e" type="f">Carabijnen</rhyme> //</l>
        <l id="s2:l7" met="3" type="+-">Gaven vier op <rhyme label="d" type="m" corresp="#s2:l5">vier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">d' Embuschade Rees met <rhyme label="a" type="f">gedonder</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Van Kruyt en loot en naar <rhyme label="b" type="m">geschreeu</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Van Kruyt en loot en naar <rhyme label="b" type="m" corresp="#s3:l2">geschreeu</rhyme></l>
        <l id="s3:l4" met="4" type="-+">En besetten // de Spaense <rhyme label="c" type="f">genetten</rhyme></l>
        <l id="s3:l5" met="5" type="+-">Soo vol couragie yder als een <rhyme label="d" type="m">Leeu</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Soo vol kouragie // soo vol <rhyme label="e" type="f">kouragie</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Yder als een <rhyme label="d" type="m" corresp="#s3:l5">Leeu</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Haer Cornetten viel in ons <rhyme label="a" type="f">handen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Flur gaf sich Ider op de vlucht, <rhyme label="b" type="m">flur</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Flur gaf sich Ider op de vlucht, <rhyme label="b" type="m" corresp="#s4:l2">flur</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Maer zy wierden meest al daer <rhyme label="c" type="f">gevangen</rhyme>,</l>
        <l id="s4:l5" met="5" type="+-">Doen ons victory galmde door de <rhyme label="d" type="m">Lucht</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Doen ons victory // doen ons <rhyme label="e" type="f">victory</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Galmde door de <rhyme label="d" type="m" corresp="#s4:l5">lucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
