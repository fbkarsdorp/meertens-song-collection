<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100147</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Petite royale</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EEntich, by 't midden vanden <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Beving de slaap mijn <rhyme label="b" type="f">Oogen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">'k Heb mijn neer <rhyme label="b" type="f" corresp="#s1:l2">gebogen</rhyme>:</l>
        <l id="s1:l4" met="3" type="+-">Als ick Slapend <rhyme label="a" type="m" corresp="#s1:l1">lagh</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Sagh ick mijn <rhyme label="c" type="f">Lerinde</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Komen naar mijn <rhyme label="d" type="m">toe</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Of zy mijn weder <rhyme label="c" type="f" corresp="#s1:l5">minde</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">s' Was haar wreetheyt <rhyme label="d" type="m" corresp="#s1:l6">moe</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">Sy lonckten 'k weet niet <rhyme label="d" type="m" corresp="#s1:l6">hoe</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'k Verstout terstont mijn droevich <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En begin te <rhyme label="b" type="f">beklagen</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Al mijn droeve <rhyme label="b" type="f" corresp="#s2:l2">dagen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Al mijn ramp en <rhyme label="a" type="m" corresp="#s2:l1">smert</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">'k Bad: Lerind <rhyme label="c" type="f">genade</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Jont mijn <rhyme label="d" type="m">wedermin</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wilt mijn van druck <rhyme label="c" type="f" corresp="#s2:l5">ontladen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Want gy <rhyme label="d" type="m" corresp="#s2:l6">Engelin</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Beheerst mijn Ziel en <rhyme label="d" type="m" corresp="#s2:l6">Sin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">s' Onsluyt haar schoon Korale <rhyme label="a" type="m">mondt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De Seyl-steen van mijn <rhyme label="b" type="f">kusjes</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Speel Hof van mijn <rhyme label="b" type="f" corresp="#s3:l2">lusjes</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Oorsaack van mijn <rhyme label="a" type="m" corresp="#s3:l1">wondt</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">En vol <rhyme label="c" type="f">mededogen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Spreeck myn aldus <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Vermoort toch niet uwe <rhyme label="c" type="f" corresp="#s3:l5">Oogen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Laat u wenen <rhyme label="d" type="m" corresp="#s3:l6">staan</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Mijn wreetheyt is <rhyme label="d" type="m" corresp="#s3:l6">gedaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laat 't bed syn Stycx, ick Charons <rhyme label="a" type="m">Schuyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Vergeet hier uwe <rhyme label="b" type="f">ellenden</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">'k Wil u lyden <rhyme label="b" type="f" corresp="#s4:l2">enden</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Ende zijn u <rhyme label="a" type="m" corresp="#s4:l1">Bruydt</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Stoot aan dese <rhyme label="c" type="f">klippen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Drenckt u in mijn <rhyme label="d" type="m">schoot</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Laat daar u Zieltjen <rhyme label="c" type="f" corresp="#s4:l5">glippen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">O gewenste <rhyme label="d" type="m" corresp="#s4:l6">Doot</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">'k Vergeet schier al mijn <rhyme label="d" type="m" corresp="#s4:l6">noodt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mit dat ick met een kus haar <rhyme label="a" type="m">naack</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">O soete <rhyme label="b" type="f">dromerijen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">O ydel <rhyme label="b" type="f" corresp="#s5:l2">verblijen</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Voel ick dat 'k <rhyme label="a" type="m" corresp="#s5:l1">ontwaack</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">'k Veynsde weer te <rhyme label="c" type="f">dromen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Maarde vaack <rhyme label="d" type="m">verdween</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">'k Heb geen Lerind <rhyme label="c" type="f" corresp="#s5:l5">vernomen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Maar 'k vondt mijn <rhyme label="d" type="m" corresp="#s5:l6">alleen</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Mijn vreucht was naar <rhyme label="d" type="m" corresp="#s5:l6">gesteen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
