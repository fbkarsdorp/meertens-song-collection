<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155820</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Rozemond die lag gedoken</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs daer twee te saem <rhyme label="a" type="f">Ver-eenen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">In den soeten <rhyme label="b" type="m">Echten-staet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die malkaer van herten <rhyme label="a" type="f" corresp="#s1:l1">meenen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Geen geluck oyt hooger <rhyme label="b" type="m" corresp="#s1:l2">gaet</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">'t Is een Hemel op der <rhyme label="c" type="m">Aerd</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En wel dubbel prijsens <rhyme label="c" type="m" corresp="#s1:l5">waerd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Is al Suycker uyt een <rhyme label="a" type="f">Hooren</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Is al vreughde sonder <rhyme label="b" type="m">leet</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Wie kan soo een Paer <rhyme label="a" type="f" corresp="#s2:l1">verstooren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Die te samen sijn <rhyme label="b" type="m" corresp="#s2:l2">gesmeet</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">Valt de een, de ander <rhyme label="c" type="m">staet</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Leyt de een, de ander <rhyme label="c" type="m" corresp="#s2:l5">gaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wel Speel-nootjes, treckt aen 't <rhyme label="a" type="f">vryen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Laet geen tijdt verlooren <rhyme label="b" type="m">gaen</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Ghy soeckt immers te <rhyme label="a" type="f" corresp="#s3:l1">bedyen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Blijft dan hier niet langer <rhyme label="b" type="m" corresp="#s3:l2">staen</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">'t Klockje slaet al hallef <rhyme label="c" type="m">thien</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Of ghy komt te laet <rhyme label="c" type="m" corresp="#s3:l5">misschien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Vreesje voor de <rhyme label="a" type="f">Buur-begapers</rhyme>?</l>
        <l id="s4:l2" met="4" type="+-">Vreesje voor een blaeuwe <rhyme label="b" type="m">scheen</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">'t Vryen is niet voor de <rhyme label="a" type="f" corresp="#s4:l1">slapers</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Lieve man, gaet dan voort <rhyme label="b" type="m" corresp="#s4:l2">heen</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Maeckje by des Meysjes <rhyme label="c" type="m">deur</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Eer een ander daer komt <rhyme label="c" type="m" corresp="#s4:l5">veur</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Licht de Vryster zit te <rhyme label="a" type="f">wachten</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Nae u komst met groot <rhyme label="b" type="m">gedult</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Soud ghy dan die kans <rhyme label="a" type="f" corresp="#s5:l1">verachten</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Als ghy welkoom wesen <rhyme label="b" type="m" corresp="#s5:l2">sult</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Klop maer sachjes nae haer <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dan soo raeckje strackjes <rhyme label="c" type="m" corresp="#s5:l5">in</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Weetje dan soo net te <rhyme label="a" type="f">kouten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat u praetjen haer <rhyme label="b" type="m">behaeght</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wilt u daer op wat <rhyme label="a" type="f" corresp="#s6:l1">verstouten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Licht sy jae seyt, als ghy 't <rhyme label="b" type="m" corresp="#s6:l2">vraeght</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Dan hebt ghy wel haest een <rhyme label="c" type="m">Bruyt</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Daer mee is u loopen <rhyme label="c" type="m" corresp="#s6:l5">uyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
