<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185349</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O gij nimf hoog vermaard</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Weest ghegroet, ghy hoogh <rhyme label="a" type="m">gheslacht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van Konincklijcke <rhyme label="b" type="m">stam</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Seer doorluchtigh groot van <rhyme label="b" type="m" corresp="#s1:l2">macht</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Geboren door het <rhyme label="a" type="m" corresp="#s1:l1">Lam</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">‘k Mien u Maeghtndie <rhyme label="c" type="m">mishaeght</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Al wat ‘t Lam in teghen <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die ‘t al draeght // en al <rhyme label="c" type="m" corresp="#s1:l5">waeght</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Om de liefde die God <rhyme label="d" type="m" corresp="#s1:l6">is</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Die haer weelden <rhyme label="e" type="m">quijt</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">En gheen beelden <rhyme label="e" type="m" corresp="#s1:l9">lijdt</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Die haer minn’ <rhyme label="e" type="m" corresp="#s1:l9">bestrijdt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">‘k Wens dat ghy u staet <rhyme label="a" type="m">erkent</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die ghy deur ‘t Lam <rhyme label="b" type="m">vercreeght</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want u staet en heeft geen <rhyme label="b" type="m" corresp="#s2:l2">endt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Soo ghy niet wort <rhyme label="a" type="m" corresp="#s2:l1">beweeght</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Buyght u neer // meer, en <rhyme label="c" type="m">meer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Met hier gaern de minst te <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Voor u Heer, met in <rhyme label="c" type="m" corresp="#s2:l5">keer</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Niet ghenoech, moet sijn, u <rhyme label="d" type="m" corresp="#s2:l6">pijn</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Siet hoe ‘t Lam hier <rhyme label="e" type="m">daelt</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Siet hoe dat het <rhyme label="e" type="m" corresp="#s2:l9">gaelt</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">En so vierigh <rhyme label="e" type="m" corresp="#s2:l9">straelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Kent sijn overgroote <rhyme label="a" type="m">minn</rhyme>’</l>
        <l id="s3:l2" met="3" type="-+">Tot u, o Maeghden <rhyme label="b" type="m">beelt</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Siet sijn Liefd’ te deegh eens <rhyme label="b" type="m" corresp="#s3:l2">in</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En wat hy deur u <rhyme label="a" type="m" corresp="#s3:l1">teelt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">’t Aller uur // niet als <rhyme label="c" type="m">puur</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Louter af <rhyme label="d" type="m">gescheydenheyt</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Al sijn kuyr // is sijn <rhyme label="c" type="m" corresp="#s3:l5">vuyr</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Dat in Liefd’ besloten <rhyme label="d" type="m" corresp="#s3:l6">leyt</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Dits u Maeghden <rhyme label="e" type="m">deught</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">En u Maeghden <rhyme label="e" type="m" corresp="#s3:l9">vreught</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Dat g’u, Heer <rhyme label="e" type="m" corresp="#s3:l9">verneught</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Spoort u lam sijn wegen <rhyme label="a" type="m">na</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">’t Sy waert oock henen <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want sijn paden sijn <rhyme label="b" type="m" corresp="#s4:l2">genae</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Schoon hoes’ hier sijn <rhyme label="a" type="m" corresp="#s4:l1">versmaet</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dits u wegh // ’t <rhyme label="c" type="m">overlegh</rhyme></l>
        <l id="s4:l6" met="4" type="+-">’t Moet sijn alles in den <rhyme label="d" type="m">door</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">‘t Ick moet wech // uyte <rhyme label="c" type="m" corresp="#s4:l5">wegh</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Maer dees noot // en baert geen <rhyme label="d" type="m" corresp="#s4:l6">noot</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">‘t Is wel ‘t nauste <rhyme label="e" type="m">padt</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Maer in dese <rhyme label="e" type="m" corresp="#s4:l9">stadt</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Leyt een Maeght haer <rhyme label="e" type="m" corresp="#s4:l9">schadt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Al. om ‘t al, schijnt vry wat <rhyme label="a" type="m">veul</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Voor die ‘t al niet <rhyme label="b" type="m">gelooft</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Wats ons al? maer ‘t al is <rhyme label="b" type="m" corresp="#s5:l2">eel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Want ‘t is der Maeghden <rhyme label="a" type="m" corresp="#s5:l1">hooft</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Haer verdriet // anders <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dat sy geen meer levens <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Tot geniet // voor hem <rhyme label="c" type="m" corresp="#s5:l5">die’t</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Alles voor haer <rhyme label="d" type="m" corresp="#s5:l6">overgeeft</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Dits een Maeghden <rhyme label="e" type="m">stant</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">En haer <rhyme label="e" type="m" corresp="#s5:l9">ingewand</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Dat soo vuerigh <rhyme label="e" type="m" corresp="#s5:l9">brandt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Sulken Bruyt weet van geen <rhyme label="a" type="m">loon</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sy heeft maer een <rhyme label="b" type="m">waerom</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Maer om geen van d’eygen <rhyme label="b" type="m" corresp="#s6:l2">Goon</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Alleen haer <rhyme label="a" type="m" corresp="#s6:l1">Bruydegom</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Al sijn goet // is haer <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Al sijn wijsheyt haer <rhyme label="d" type="m">vermaeck</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Al sijn bloedt // dat haer <rhyme label="c" type="m" corresp="#s6:l5">voedt</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Is haer d’alderbeste <rhyme label="d" type="m" corresp="#s6:l6">smaeck</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Sijn ghecruyste <rhyme label="e" type="m">lijf</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Is tot haer <rhyme label="e" type="m" corresp="#s6:l9">gherijf</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">En haer tijt <rhyme label="e" type="m" corresp="#s6:l9">verdrijf</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">S’is vervreemt van ‘t eyghen <rhyme label="a" type="m">beest</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat haer so smertigh <rhyme label="b" type="m">beedt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En van ‘t geen ‘t vleys lieft, en <rhyme label="b" type="m" corresp="#s7:l2">vreest</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">‘Daer sy so veul van <rhyme label="a" type="m" corresp="#s7:l1">leedt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Van ‘s vleys lust // en sijn <rhyme label="c" type="m">rust</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Dat so noy was uyt <rhyme label="d" type="m">gheblust</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Van ‘t bewust // dat ‘t vleys <rhyme label="c" type="m" corresp="#s7:l5">kust</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En niet licht en is <rhyme label="d" type="m" corresp="#s7:l6">gesust</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">Van ‘t vleys <rhyme label="e" type="m">sin’lijckheyt</rhyme>,</l>
        <l id="s7:l10" met="3" type="+-">Dat soo wijdt uyt <rhyme label="e" type="m" corresp="#s7:l9">spreydt</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Daer ‘t soo langh op <rhyme label="e" type="m" corresp="#s7:l9">dreyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Al zins leden sijn <rhyme label="a" type="m">ghecruyst</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En in ‘t hoofts doot <rhyme label="b" type="m">geplant</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">‘s Is uyt haer in Godt <rhyme label="b" type="m" corresp="#s8:l2">verhuyst</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Sy staet in Godes <rhyme label="a" type="m" corresp="#s8:l1">handt</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Al ‘t verschill’ // en <rhyme label="c" type="m">bedill</rhyme>’,</l>
        <l id="s8:l6" met="4" type="+-">Is nu aen een cant <rhyme label="d" type="m">gheraeckt</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Al haer wil // leyt nu <rhyme label="c" type="m" corresp="#s8:l5">stil</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">S’is nu los en vry <rhyme label="d" type="m" corresp="#s8:l6">gemaeckt</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">‘t Hooft heeft al ‘t <rhyme label="e" type="m">bewint</rhyme>,</l>
        <l id="s8:l10" met="3" type="+-">S’is nu als een <rhyme label="e" type="m" corresp="#s8:l9">kindt</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Dat ghehoorsaem <rhyme label="e" type="m" corresp="#s8:l9">mint</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">S’is deur alles deur <rhyme label="a" type="m">gegaen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Sy woont nu heel om <rhyme label="b" type="m">hoogh</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Doet haer yemant lijden <rhyme label="b" type="m" corresp="#s9:l2">aen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Die raeckt het hooft sijn <rhyme label="a" type="m" corresp="#s9:l1">oogh</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">‘t Hooft dat strijdt, en <rhyme label="c" type="m">bevrijdt</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Nu sijn Bruydt voor <rhyme label="d" type="m">d’overlast</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Maer sy ludt // en <rhyme label="c" type="m" corresp="#s9:l5">verblijdt</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Haer om ‘t geen uyt ‘t lijden <rhyme label="d" type="m" corresp="#s9:l6">wast</rhyme>,</l>
        <l id="s9:l9" met="3" type="+-">‘t Geeft soo schoonen <rhyme label="e" type="m">vrucht</rhyme>,</l>
        <l id="s9:l10" met="3" type="+-">En so soeten <rhyme label="e" type="m" corresp="#s9:l9">lucht</rhyme>,</l>
        <l id="s9:l11" met="3" type="+-">Datmer staegh om <rhyme label="e" type="m" corresp="#s9:l9">sucht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Siet nu op u Lieff’ sijn <rhyme label="a" type="m">gunst</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Hoe dat hy u <rhyme label="b" type="m">vereert</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">En eens op sijn groote <rhyme label="b" type="m" corresp="#s10:l2">cunst</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Hoe niemant u meer <rhyme label="a" type="m" corresp="#s10:l1">deert</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Al haer quaet // is u <rhyme label="c" type="m">baet</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">En haer vloeck, u <rhyme label="d" type="m">zegeningh</rhyme></l>
        <l id="s10:l7" met="4" type="+-">Al haer daet // is haer <rhyme label="c" type="m" corresp="#s10:l5">schaet</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">En u best, hoe <rhyme label="d" type="m" corresp="#s10:l6">sonderlingh</rhyme>,</l>
        <l id="s10:l9" met="3" type="+-">Doet dan u <rhyme label="e" type="m">profijt</rhyme>,</l>
        <l id="s10:l10" met="3" type="+-">Ia u duren <rhyme label="e" type="m" corresp="#s10:l9">tijt</rhyme></l>
        <l id="s10:l11" met="3" type="+-">Die soo haest <rhyme label="e" type="m" corresp="#s10:l9">verslijt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Singht u lesjen alle <rhyme label="a" type="m">daegh</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Wat hier een Maeght <rhyme label="b" type="m">beraemt</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">In geen deugden nimmer <rhyme label="b" type="m" corresp="#s11:l2">traegh</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Veel min om deught <rhyme label="a" type="m" corresp="#s11:l1">beschaemt</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Met ootmoet // al ‘t <rhyme label="c" type="m">ontmoet</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Moet vrywilligh zijn <rhyme label="d" type="m">geleen</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">En versoet // en <rhyme label="c" type="m" corresp="#s11:l5">gegroet</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Om u lief, en anders <rhyme label="d" type="m" corresp="#s11:l6">geen</rhyme>,</l>
        <l id="s11:l9" met="3" type="+-">Gaet gaern om een <rhyme label="e" type="m">hoeck</rhyme>,</l>
        <l id="s11:l10" met="3" type="+-">Met u minnaers <rhyme label="e" type="m" corresp="#s11:l9">boeck</rhyme>,</l>
        <l id="s11:l11" met="3" type="+-">En een oogen <rhyme label="e" type="m" corresp="#s11:l9">doeck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Weest verblijdt Princesse <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Want ghy wort hier <rhyme label="b" type="m">beloont</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Met een die u Ziel <rhyme label="b" type="m" corresp="#s12:l2">behaeght</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Daer wort j’oock van <rhyme label="a" type="m" corresp="#s12:l1">gecroont</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Dits zijn woort // van nu <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Sult ghy sitten op mijn <rhyme label="d" type="m">troon</rhyme>,</l>
        <l id="s12:l7" met="4" type="+-">‘t Soet accoort // datmen <rhyme label="c" type="m" corresp="#s12:l5">hoort</rhyme>,</l>
        <l id="s12:l8" met="4" type="+-">Is o Lam, o Maeghden <rhyme label="d" type="m" corresp="#s12:l6">Kroon</rhyme>!</l>
        <l id="s12:l9" met="3" type="+-">Singht dan ‘t Hemels <rhyme label="e" type="m">liet</rhyme>,</l>
        <l id="s12:l10" met="3" type="+-">Dat Godts naem <rhyme label="e" type="m" corresp="#s12:l9">bediedt</rhyme>,</l>
        <l id="s12:l11" met="3" type="+-">En sijn lof <rhyme label="e" type="m" corresp="#s12:l9">gheschiet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
