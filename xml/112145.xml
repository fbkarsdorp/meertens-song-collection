  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AFwezen vol <rhyme label="a" type="f">smerte</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ach wat hebt gy aan <rhyme label="b" type="m">myn</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Getroffen <rhyme label="a" type="f" corresp="#s1:l1">herte</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Gekost al <rhyme label="b" type="m" corresp="#s1:l2">pyn</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Wat heb ik <rhyme label="c" type="f">geleden</rhyme>?</l>
        <l id="s1:l6" met="2" type="-+">Nu 'k van uw <rhyme label="c" type="f" corresp="#s1:l5">heden</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Heb moeten <rhyme label="b" type="m" corresp="#s1:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'k Voelde myn <rhyme label="a" type="f">tranen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Langs myn benauwde <rhyme label="b" type="m">wang</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Een schreygroef <rhyme label="a" type="f" corresp="#s2:l1">baanen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Veel uuren <rhyme label="b" type="m" corresp="#s2:l2">lang</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Geen troost was te <rhyme label="c" type="f">geven</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Aan my, voor't <rhyme label="c" type="f" corresp="#s2:l5">leven</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Van 't Boomkind <rhyme label="b" type="m" corresp="#s2:l2">bang</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maar nu 'k vol <rhyme label="a" type="f">genoegen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Aan myn Adonis <rhyme label="b" type="m">zy</rhyme></l>
        <l id="s3:l3" met="2" type="-+">My weer mag <rhyme label="a" type="f" corresp="#s3:l1">voegen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Wat ben ik <rhyme label="b" type="m" corresp="#s3:l2">bly</rhyme>!</l>
        <l id="s3:l5" met="3" type="-+">Vlucht, Vlucht nu Ã´ <rhyme label="c" type="f">smerte</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Uit Venus <rhyme label="c" type="f" corresp="#s3:l5">herte</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Van zorgen <rhyme label="b" type="m" corresp="#s3:l2">vry</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al maakt u <rhyme label="a" type="f">verlegen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Afwezens <rhyme label="b" type="m">ongeneugt</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Het baand de <rhyme label="a" type="f" corresp="#s4:l1">wegen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Tot grooter <rhyme label="b" type="m" corresp="#s4:l2">vreugd</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Als Ge uw heete <rhyme label="c" type="f">lasjes</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Met dart'le <rhyme label="c" type="f" corresp="#s4:l5">kusjes</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Hereenen <rhyme label="b" type="m" corresp="#s4:l2">meugt</rhyme>.</l>
      </lg>
    </lg>
  </text>
