  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Seg my dan, volmaakte <rhyme label="a" type="m">Beelt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zal ik uw noyt <rhyme label="b" type="f">verwinnen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Al te straffe <rhyme label="c" type="m">Rosemond</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Nu mijn zieltjen is <rhyme label="c" type="m" corresp="#s1:l3">gewont</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van binnen, van binnen, van <rhyme label="b" type="f" corresp="#s1:l2">binnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zal ik dan door mijn <rhyme label="a" type="m">gebeen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Noyt komen in u <rhyme label="b" type="f">gracy</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">So en zie ik noyt het <rhyme label="c" type="m">end</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van mijn droefheid en <rhyme label="c" type="m" corresp="#s2:l3">ellend</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Og lacy, og lacy, og <rhyme label="b" type="f" corresp="#s2:l2">lacy</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Al te volmaakte <rhyme label="a" type="m">Rosemond</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ik weet geen troost te <rhyme label="b" type="f">vinden</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Laat dog van uw wreetheid <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Anders moet ik in het <rhyme label="c" type="m" corresp="#s3:l3">graf</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Beminde, Beminde, <rhyme label="b" type="f" corresp="#s3:l2">Beminde</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hemel, ik en weet geen <rhyme label="a" type="m">raat</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn klagten sijn <rhyme label="b" type="f">verlooren</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Ik sal sonder <rhyme label="c" type="m">medecijn</rhyme></l>
        <l id="s4:l4" met="4" type="+-">In mijn droeve minne <rhyme label="c" type="m" corresp="#s4:l3">pijn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Versmooren, versmooren, <rhyme label="b" type="f" corresp="#s4:l2">versmooren</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Kan ik niet myn <rhyme label="a" type="m">Rosemond</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Bewegen met een <rhyme label="b" type="f">suchje</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Ag, myn Rosemondje, <rhyme label="c" type="m">kom</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Anders raakt u Minnaar <rhyme label="c" type="m" corresp="#s5:l3">om</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Een luchje, een luchje, een <rhyme label="b" type="f" corresp="#s5:l2">luchje</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laat ik u maar tot een <rhyme label="a" type="m">kus</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">O Rosemont, <rhyme label="b" type="f">bekooren</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">So is al myn smert <rhyme label="c" type="m">gedaan</rhyme></l>
        <l id="s6:l4" met="4" type="+-">En sal ik weer gaan en <rhyme label="c" type="m" corresp="#s6:l3">staan</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Als vooren, als vooren, als <rhyme label="b" type="f" corresp="#s6:l2">vooren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ag, hoe kan uw killig <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s7:l2" met="3" type="-+">So onbeweeglijk <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Daar gy door u lieve <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Ligtelyk mijn droefheid <rhyme label="c" type="m" corresp="#s7:l3">kond</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Genesen, genesen, <rhyme label="b" type="f" corresp="#s7:l2">genesen</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Laet u dan, o <rhyme label="a" type="m">Rosemond</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Mijn smeeken <rhyme label="b" type="f">overwinnen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dat ik al 't geleeden <rhyme label="c" type="m">leet</rhyme></l>
        <l id="s8:l4" met="4" type="+-">En mijn hertzeer weer <rhyme label="c" type="m" corresp="#s8:l3">vergeet</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Van 't minnen, Van 't minnen, van 't <rhyme label="b" type="f" corresp="#s8:l2">minnen</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Dan zal ik u, waarde <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Altijd als mijn <rhyme label="b" type="f">Beminde</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Geven 't allerzoetst <rhyme label="c" type="m">vermaak</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Waar in dat gy goeden <rhyme label="c" type="m" corresp="#s9:l3">smaek</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Zult vinden, zult vinden, zult <rhyme label="b" type="f" corresp="#s9:l2">vinden</rhyme>.</l>
      </lg>
    </lg>
  </text>
