<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184856</id>
    <date>
      <year-start>1639</year-start>
      <year-end>1639</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De mei die komt ons bij zeer blij</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SInght vrolijck weest verblijt // <rhyme label="a" type="m">altijdt</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Looft Godt die boven sit in 's Hemels <rhyme label="b" type="m">Throon</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Sijn goetheydt ons ontmoet // en <rhyme label="c" type="m">groet</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Ons, door sijn een eenige geboren <rhyme label="b" type="m" corresp="#s1:l2">Soon</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Daer de Propheten <rhyme label="d" type="m">seer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En Coninghen wel <rhyme label="d" type="m" corresp="#s1:l5">eer</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Met lust hebben verlanghet <rhyme label="e" type="m">naer</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Is nu te <rhyme label="f" type="m">Betl'hem</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Ghebooren, met <rhyme label="f" type="m" corresp="#s1:l8">hem</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Een goet Nieuwe <rhyme label="e" type="m" corresp="#s1:l7">Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nu is de Prophecy // <rhyme label="a" type="m">Ecy</rhyme>:</l>
        <l id="s2:l2" met="5" type="-+">Vervult, als dat een Maegt heeft voort <rhyme label="b" type="m">ghebrocht</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Een Kindeken seer kleyn // maer <rhyme label="c" type="m">reyn</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Wie sou 't sijn leven oyt hebben <rhyme label="b" type="m" corresp="#s2:l2">gedogt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Als dat een Maghet <rhyme label="d" type="m">teer</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Bracht voort de grootste <rhyme label="d" type="m" corresp="#s2:l5">Heer</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">O! wonder boven wonder <rhyme label="e" type="m">daer</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">D'armen al <rhyme label="f" type="m">ghelijck</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Ghemaeckt werden <rhyme label="f" type="m" corresp="#s2:l8">rijck</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">In het Nieuwe <rhyme label="e" type="m" corresp="#s2:l7">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Des Hemels held're Oog // om <rhyme label="a" type="m">hoog</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Verlicht beneen het Menschelijck <rhyme label="b" type="m">gheslacht</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">'t Geen ons welvaert beklijft // <rhyme label="c" type="m">verdrijft</rhyme></l>
        <l id="s3:l4" met="5" type="-+">De bitt're, koud, en langhe, donck're <rhyme label="b" type="m" corresp="#s3:l2">nacht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maer! 's gherechtigheydts <rhyme label="d" type="m">schicht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ons inwendigh <rhyme label="d" type="m" corresp="#s3:l5">verlicht</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Door dit Kints komst, in 't <rhyme label="e" type="m">openbaer</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Het werdt eerst <rhyme label="f" type="m">vertelt</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">De Herders op 't <rhyme label="f" type="m" corresp="#s3:l8">velt</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">In het Nieuwe <rhyme label="e" type="m" corresp="#s3:l7">Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy Herders doch niet weest // <rhyme label="a" type="m">bevreest</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Daer na ghy volck'ren al in 's Werelts <rhyme label="b" type="m">ront</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Een blyde bootschap groot // in <rhyme label="c" type="m">noot</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Doet den Enghel u hier altemael <rhyme label="b" type="m" corresp="#s4:l2">kont</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Gaet doch na Betl'hem <rhyme label="d" type="m">heen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Daer sult ghy sien met <rhyme label="d" type="m" corresp="#s4:l5">een</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Des Werelts Vorst, seer <rhyme label="e" type="m">wonder-baer</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Onnosel <rhyme label="f" type="m">bedreyt</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">In een Crib <rhyme label="f" type="m" corresp="#s4:l8">gheleyt</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">In het Nieuwe <rhyme label="e" type="m" corresp="#s4:l7">Iaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Terstont was daer ontrent // <rhyme label="a" type="m">bekent</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Den Engel bekleet met 's Hemels <rhyme label="b" type="m">claerheydt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En noch een groot ghetal // die <rhyme label="c" type="m">al</rhyme>:</l>
        <l id="s5:l4" met="5" type="-+">Ghetuygen zijn van des Hemels <rhyme label="b" type="m" corresp="#s5:l2">waerheydt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Daer by soo loofden <rhyme label="d" type="m">zy</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met soete <rhyme label="d" type="m" corresp="#s5:l5">melody</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En spraken Gods lof met <rhyme label="e" type="m">mal'kaer</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Eer zy Godt van <rhyme label="f" type="m">waerd'</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">En vreede op <rhyme label="f" type="m" corresp="#s5:l8">aerd'</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">In het Nieuwe <rhyme label="e" type="m" corresp="#s5:l7">Iaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dit is de stercke Helt // <rhyme label="a" type="m">vermelt</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Ons Esaias den waren <rhyme label="b" type="m">Propheet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Die Hel en doodt verwint // hem <rhyme label="c" type="m">mint</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Hy is eeuwig Vader, en Vorst des <rhyme label="b" type="m" corresp="#s6:l2">vreed'</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">O! wonderlijcke <rhyme label="d" type="m">raet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die 't al te boven <rhyme label="d" type="m" corresp="#s6:l5">gaet</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Des wijsheydts verstant <rhyme label="e" type="m">allegaer</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Die is nu <rhyme label="f" type="m">alleyn</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Ons levens <rhyme label="f" type="m" corresp="#s6:l8">Fonteyn</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">In het Nieuwe <rhyme label="e" type="m" corresp="#s6:l7">Iaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dees aldergrootste Prins // <rhyme label="a" type="m">geensins</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Vint ghy meer o Mensch in een beesten <rhyme label="b" type="m">stal</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Maer in den Hemel schoon // ten <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">Van waer hy u door sijn Gheest leyden <rhyme label="b" type="m" corresp="#s7:l2">sal</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">In alle waerheydts <rhyme label="d" type="m">kracht</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Op dat de donck're <rhyme label="d" type="m" corresp="#s7:l5">nacht</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">V niet en brengh in groot <rhyme label="e" type="m">ghevaer</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Dat g'u voet niet <rhyme label="f" type="m">stoot</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">Aen de Berghen <rhyme label="f" type="m" corresp="#s7:l8">groot</rhyme>,</l>
        <l id="s7:l10" met="3" type="+-">In het Nieuwe <rhyme label="e" type="m" corresp="#s7:l7">Iaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Oorlof hier mee, ons druck // in <rhyme label="a" type="m">luck</rhyme>:</l>
        <l id="s8:l2" met="5" type="-+">Is verandert, door al des Werelts <rhyme label="b" type="m">ent</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dees aldergrootst' genaed' // <rhyme label="c" type="m">weldaed'</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">Werdt ons door dit Kindekens komst <rhyme label="b" type="m" corresp="#s8:l2">bekent</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ghy Vollekeren <rhyme label="d" type="m">hoort</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Gaet doch in liefden <rhyme label="d" type="m" corresp="#s8:l5">voort</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Verjeught u altesaem <rhyme label="e" type="m">eenpaer</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Weest in Liefd <rhyme label="f" type="m">Bloeyend</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">En in deughdt <rhyme label="f" type="m" corresp="#s8:l8">groeyend</rhyme>,</l>
        <l id="s8:l10" met="3" type="+-">In het Nieuwe <rhyme label="e" type="m" corresp="#s8:l7">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
