    </tags>
    <melody>O gloriosa Domina</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O gloriose <rhyme label="a" type="f">vrouwe</rhyme></l>
        <l id="s1:l2" met="3" type="-+">verheuen bouen <rhyme label="b" type="m">al</rhyme></l>
        <l id="s1:l3" met="3" type="-+">die v in sijnder <rhyme label="a" type="f" corresp="#s1:l1">trouwe</rhyme></l>
        <l id="s1:l4" met="3" type="-+">gestelt heft sonder <rhyme label="b" type="m" corresp="#s1:l2">val</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die spijst ghij met v <rhyme label="a" type="f">borsten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">worden voor ons v <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s2:l3" met="3" type="-+">dus wij tot v nu <rhyme label="a" type="f" corresp="#s2:l1">dorsten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">ghij sijt ons alder <rhyme label="b" type="m" corresp="#s2:l2">vrint</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">dat ons Eua wreet <rhyme label="a" type="f">beneempt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">ghij sijt hemels <rhyme label="b" type="m">doore</rhyme></l>
        <l id="s3:l3" met="3" type="-+">wort door v ons weer <rhyme label="a" type="f" corresp="#s3:l1">verleent</rhyme></l>
        <l id="s3:l4" met="3" type="-+">wij ten hemel <rhyme label="b" type="m" corresp="#s3:l2">spooren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghij sijt des Conincx <rhyme label="a" type="f">moder</rhyme></l>
        <l id="s4:l2" met="3" type="-+">des lichts blinckende <rhyme label="b" type="m">poort</rhyme></l>
        <l id="s4:l3" met="3" type="-+">door v worden ons <rhyme label="a" type="f" corresp="#s4:l1">brooder</rhyme></l>
        <l id="s4:l4" met="3" type="-+">is goods warachtich <rhyme label="b" type="m" corresp="#s4:l2">woort</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Waerdoor wij ons <rhyme label="a" type="f">verhoogen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">met eenpaerlijck <rhyme label="b" type="m">auijs</rhyme></l>
        <l id="s5:l3" met="3" type="-+">die v borst heft <rhyme label="a" type="f" corresp="#s5:l1">gesogen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">dien sij loff, eer en <rhyme label="b" type="m" corresp="#s5:l2">prijs</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die v borsten heeft <rhyme label="a" type="f">gesogen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">die sijn loff eer en <rhyme label="b" type="m">prijs</rhyme></l>
        <l id="s6:l3" met="3" type="-+">hij wil ons al <rhyme label="a" type="f" corresp="#s6:l1">veruoogen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">in s'hemels <rhyme label="b" type="m" corresp="#s6:l2">paradijs</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
