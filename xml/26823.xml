  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Reich gott wie sall ich <rhyme label="a" type="f">clagenn</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">wie sall ich clagenn mein <rhyme label="b" type="m">nott</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">mein hertt is mir d <rhyme label="a" type="f" corresp="#s1:l1">rchstochenn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">verw ndt all jn denn <rhyme label="b" type="m" corresp="#s1:l2">thott</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">mitt bernender liebdenn <rhyme label="c" type="f">vmbfangenn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">gleich einenn gronenn <rhyme label="d" type="m">zwich</rhyme></l>
        <l id="s1:l7" met="3" type="-+">mitt lofferenn is sei <rhyme label="c" type="f" corresp="#s1:l5">vmbfangenn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">al jn des Meies <rhyme label="d" type="m" corresp="#s1:l6">titt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">J nckfraw wiltt hiran <rhyme label="a" type="f">dencken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">vnnd betrachtenn die redenn <rhyme label="b" type="m">woll</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Vnnd wiltt von my nitt <rhyme label="a" type="f" corresp="#s2:l1">wenckenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">dweill ich das lebenn  <rhyme label="b" type="m" corresp="#s2:l2">hab</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Gy seitt allein vnnd anders <rhyme label="c" type="f">kein</rhyme></l>
        <l id="s2:l6" met="3" type="-+">die ich hab jn meinem <rhyme label="d" type="m">syn</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">vnnd latt v nitt <rhyme label="c" type="f" corresp="#s2:l5">berouwenn</rhyme></l>
        <l id="s2:l8" met="3" type="-+">v eigenn so will <rhyme label="d" type="m" corresp="#s2:l6">sein</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ein ba m der moß <rhyme label="a" type="f">verdorrenn</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">der keine wortzell <rhyme label="b" type="m">hett</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">Die lieb die moß versa <rhyme label="a" type="f" corresp="#s3:l1">ren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">die nitt jntt herte <rhyme label="b" type="m" corresp="#s3:l2">gahett</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Die wortzell die iß <rhyme label="c" type="f">gewachsenn</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">all durch das herte <rhyme label="d" type="m">mein</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">vnnd die sie dara ß will <rhyme label="c" type="f" corresp="#s3:l5">habenn</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">der benemett das lebenn <rhyme label="d" type="m" corresp="#s3:l6">mein</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hyr bei will ich es lassenn <rhyme label="a" type="f">pleibenn</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">dar ich vmb gebedenn  <rhyme label="b" type="m">hab</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Jch en will vonn dir nitt <rhyme label="a" type="f" corresp="#s4:l1">wickenn</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">dweill ich das lebenn  <rhyme label="b" type="m" corresp="#s4:l2">hab</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">so lang als ich sein a ff <rhyme label="c" type="f">erdenn</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">so seitt jr mein hochste g <rhyme label="d" type="m">ett</rhyme> /</l>
        <l id="s4:l7" met="3" type="-+">Th itt jr mir <rhyme label="c" type="f" corresp="#s4:l5">desgleichenn</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">so scheidt vnns beide der <rhyme label="d" type="m" corresp="#s4:l6">thott</rhyme>.</l>
      </lg>
    </lg>
