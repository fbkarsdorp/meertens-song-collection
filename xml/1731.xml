  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Venus v <rhyme label="a" type="f">ontrouwe</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Brengt mi lacen inden <rhyme label="a" type="f" corresp="#s1:l1">rouwe</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dat heeft v wanckebaerheyt <rhyme label="b" type="m">ghedaen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Als ic v lief <rhyme label="a" type="f" corresp="#s1:l1">aenschouwe</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Mijn alder liefste <rhyme label="a" type="f" corresp="#s1:l1">vrouwe</rhyme></l>
        <l id="s1:l6" met="4" type="-+">So dunct mi mijnn herteken van drucke <rhyme label="b" type="m" corresp="#s1:l3">vergaen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Ghi cost mi so menighen <rhyme label="b" type="m" corresp="#s1:l3">traen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O edel god der <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wat sal ick nv <rhyme label="a" type="f" corresp="#s2:l1">beghinnen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dat ic van die liefste dus ben <rhyme label="b" type="m">versmaet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Mach ick gheenen troost <rhyme label="a" type="f" corresp="#s2:l1">gewinnen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Mi dunct ic sal <rhyme label="a" type="f" corresp="#s2:l1">ontsinnen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Ic bliue in lijden puer <rhyme label="b" type="m" corresp="#s2:l3">desperaet</rhyme></l>
        <l id="s2:l7" met="4" type="-+">dat heeft ghedaen v vroulijck <rhyme label="b" type="m" corresp="#s2:l3">saet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O suyuer bloeme <rhyme label="a" type="f">reene</rhyme></l>
        <l id="s3:l2" met="3" type="-+">V dienaer hout ghi inden <rhyme label="a" type="f" corresp="#s3:l1">weene</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Laet dalen uwen hooghen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sidi gelijc den <rhyme label="a" type="f" corresp="#s3:l1">steene</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Verhert al totten <rhyme label="a" type="f" corresp="#s3:l1">bene</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Ghi doet mi veranderen lijf ende <rhyme label="b" type="m" corresp="#s3:l3">bloet</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Ick bedde v lieueken den sin <rhyme label="b" type="m" corresp="#s3:l3">versoet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O vrouwelike <rhyme label="a" type="f">ymagie</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Reyn edel <rhyme label="a" type="f" corresp="#s4:l1">personagie</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ick bidde v om een troostelijck <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Soude v hooghe <rhyme label="a" type="f" corresp="#s4:l1">couragie</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Schouwen sulcke <rhyme label="a" type="f" corresp="#s4:l1">villeragie</rhyme></l>
        <l id="s4:l6" met="4" type="-+">So doedy uwen dienaer een groot <rhyme label="b" type="m" corresp="#s4:l3">confoort</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">die liefde van v mijn ionck herte <rhyme label="b" type="m" corresp="#s4:l3">doorboort</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ten baet  schat  goet noch <rhyme label="a" type="f">eruen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Wi moeten alle <rhyme label="a" type="f" corresp="#s5:l1">steruen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Hoe hooghe gheboren op dit <rhyme label="b" type="m">termijn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">dus laet mi nv <rhyme label="a" type="f" corresp="#s5:l1">verweruen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Troost mi of ic moet <rhyme label="a" type="f" corresp="#s5:l1">steruen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Ghi zijt die alderliefste <rhyme label="b" type="m" corresp="#s5:l3">medecijn</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Verblijt den bedructen schoon claer <rhyme label="b" type="m" corresp="#s5:l3">Robijn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Princersselijcke <rhyme label="a" type="f">beelde</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die mi noyt en <rhyme label="a" type="f" corresp="#s6:l1">verueelde</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Wilt doch dit liedeken in dancke <rhyme label="b" type="m">ontfaen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ghi waert bi mi een <rhyme label="a" type="f" corresp="#s6:l1">veelde</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Bi v ic noyt en <rhyme label="a" type="f" corresp="#s6:l1">queelde</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Al hebben ons dese nijders leyt <rhyme label="b" type="m" corresp="#s6:l3">beraen</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Oorlof schoon lief ende ick moet <rhyme label="b" type="m" corresp="#s6:l3">gaen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
