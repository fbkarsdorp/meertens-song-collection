  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">MYn hert dat trillet my, it iz nin <rhyme label="a" type="m">rôy</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Mijn ljeafste <rhyme label="b" type="m">Wobbelke</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Mijn swietste <rhyme label="b" type="m" corresp="#s1:l2">Wobbelke</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Mijn wirdste <rhyme label="b" type="m" corresp="#s1:l2">Wobbelke</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Het biste <rhyme label="a" type="m" corresp="#s1:l1">moy</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Ick fiel oon 't kreauweljen, ljeaf, het my <rhyme label="a" type="m">dert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Mijn gouden <rhyme label="b" type="m">Ingelke</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Mijn gouden <rhyme label="b" type="m" corresp="#s2:l2">Ingelke</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Mijn gouden <rhyme label="b" type="m" corresp="#s2:l2">Ingelke</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Du biste' yn 't <rhyme label="a" type="m" corresp="#s2:l1">hert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">O! 't hert ontfljuecht my beyd' mijn eagen <rhyme label="a" type="m">trog</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Ja 't rint uwt my yn <rhyme label="b" type="m">dy</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">It tjocht uwt my yn <rhyme label="b" type="m" corresp="#s3:l2">dy</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">It ljeapt uwt my yn <rhyme label="b" type="m" corresp="#s3:l2">dy</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Az ick dy <rhyme label="a" type="m" corresp="#s3:l1">sjog</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ick kin neat wezze fen dy, swiete <rhyme label="a" type="m">djier</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">OEf ick mot stearre <rhyme label="b" type="m">ljeaf</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">OEf ick mot stearre <rhyme label="b" type="m" corresp="#s4:l2">ljeaf</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">OEf ick mot stearre <rhyme label="b" type="m" corresp="#s4:l2">ljeaf</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Dat 's inckel <rhyme label="a" type="m" corresp="#s4:l1">wier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Help Ljeafste', ô litste my yn docken <rhyme label="a" type="m">quea</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Paetke my swiete <rhyme label="b" type="m">tuwt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Paetke my swiete <rhyme label="b" type="m" corresp="#s5:l2">tuwt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Paetke my swiete <rhyme label="b" type="m" corresp="#s5:l2">tuwt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">OEf ick bin <rhyme label="a" type="m" corresp="#s5:l1">dea</rhyme>.</l>
      </lg>
    </lg>
