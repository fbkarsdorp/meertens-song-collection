    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoe aardig smaakt de <rhyme label="a" type="m">wyn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe zoet zyn onze <rhyme label="b" type="f">daagen</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Kander noch wel iets <rhyme label="a" type="m" corresp="#s1:l1">zyn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat ons meÃªr zou <rhyme label="b" type="f" corresp="#s1:l2">behaagen</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Ik weet van geen <rhyme label="c" type="m">gekyf</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En leef heel zonder <rhyme label="d" type="f">kommer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ik lach eens met een <rhyme label="c" type="m" corresp="#s1:l5">Wyf</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Den Wyngaard is de <rhyme label="d" type="f" corresp="#s1:l6">lommer</rhyme></l>
        <l id="s1:l9" met="3" type="-+">En schaaduw, die myn <rhyme label="e" type="m">jeugd</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Met zyne vogt <rhyme label="e" type="m" corresp="#s1:l9">verheugd</rhyme>.</l>
      </lg>
