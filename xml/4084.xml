  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O suuer maecht van <rhyme label="a" type="f">Israhel</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wilt seer verbliden <rhyme label="b" type="m">v</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Om der schoonder boetscap <rhyme label="a" type="f" corresp="#s1:l1">wil</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die ick v brenghe <rhyme label="b" type="m" corresp="#s1:l2">nu</rhyme></l>
        <l id="s1:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s1:l5">valasus</rhyme></l>
        <l id="s1:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s1:l5">dominus</rhyme></l>
        <l id="s1:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s1:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al metter boetscap <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die denghel heeft <rhyme label="b" type="m">ghedaen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Hebdy van shemels <rhyme label="a" type="f" corresp="#s2:l1">throone</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Den heyligen gheest <rhyme label="b" type="m" corresp="#s2:l2">ontfaen</rhyme></l>
        <l id="s2:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s2:l5">valasus</rhyme></l>
        <l id="s2:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s2:l5">dominus</rhyme></l>
        <l id="s2:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s2:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als Ioseph dat <rhyme label="a" type="f">verstonde</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat Maria was <rhyme label="b" type="m">bevrucht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hi peysde in sinen <rhyme label="a" type="f" corresp="#s3:l1">gronde</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hem ware beter <rhyme label="b" type="m" corresp="#s3:l2">ghevlucht</rhyme></l>
        <l id="s3:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s3:l5">valasus</rhyme></l>
        <l id="s3:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s3:l5">dominus</rhyme></l>
        <l id="s3:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s3:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Des Hem die enghel <rhyme label="a" type="f">schone</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Toe sprack / ende <rhyme label="b" type="m">riet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ioseph Dauids <rhyme label="a" type="f" corresp="#s4:l1">sone</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En wiltse begheuen <rhyme label="b" type="m" corresp="#s4:l2">niet</rhyme></l>
        <l id="s4:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s4:l5">valasus</rhyme></l>
        <l id="s4:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s4:l5">dominus</rhyme></l>
        <l id="s4:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s4:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Noch en ontsiet te <rhyme label="a" type="f">nemen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Der moeder Gods <rhyme label="b" type="m">Marien</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dat van haer wert <rhyme label="a" type="f" corresp="#s5:l1">gheboren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sal ons van sonden <rhyme label="b" type="m" corresp="#s5:l2">vrien</rhyme></l>
        <l id="s5:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s5:l5">valasus</rhyme></l>
        <l id="s5:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s5:l5">dominus</rhyme></l>
        <l id="s5:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s5:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maria ende Ioseph <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Voeren beyde te <rhyme label="b" type="m">samen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Tot Bethleem der <rhyme label="a" type="f" corresp="#s6:l1">stede</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Daer sij haer herberghe <rhyme label="b" type="m" corresp="#s6:l2">namen</rhyme></l>
        <l id="s6:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s6:l5">valasus</rhyme></l>
        <l id="s6:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s6:l5">dominus</rhyme></l>
        <l id="s6:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s6:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">In een arm <rhyme label="a" type="f">huyseken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Al sonder weeghen oft <rhyme label="b" type="m">doren</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Om dat si waren van hauen <rhyme label="a" type="f" corresp="#s7:l1">bloot</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Moestet hen also <rhyme label="b" type="m" corresp="#s7:l2">ghebueren</rhyme></l>
        <l id="s7:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s7:l5">valasus</rhyme></l>
        <l id="s7:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s7:l5">dominus</rhyme></l>
        <l id="s7:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s7:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Alst quam ter rechter <rhyme label="a" type="f">middernacht</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Soe heeft die maecht <rhyme label="b" type="m">ghebaert</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Een kindeken van groter <rhyme label="a" type="f" corresp="#s8:l1">macht</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Also die enghel ons <rhyme label="b" type="m" corresp="#s8:l2">verclaert</rhyme></l>
        <l id="s8:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s8:l5">valasus</rhyme></l>
        <l id="s8:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s8:l5">dominus</rhyme></l>
        <l id="s8:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s8:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Daer baerde die <rhyme label="a" type="f">suyuerlike</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Den heere <rhyme label="b" type="m">ghebenedijt</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Van hemel ende <rhyme label="a" type="f" corresp="#s9:l1">aertrijcke</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Lof si haer tot alder <rhyme label="b" type="m" corresp="#s9:l2">tijt</rhyme></l>
        <l id="s9:l5" met="6" type="+-">Benedict tu in <rhyme label="c" type="m">mulieribus</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Valasus <rhyme label="c" type="m" corresp="#s9:l5">valasus</rhyme></l>
        <l id="s9:l7" met="6" type="+-">Aue plena gratia tecum <rhyme label="c" type="m" corresp="#s9:l5">dominus</rhyme></l>
        <l id="s9:l8" met="6" type="+-">Benedicta tu in <rhyme label="c" type="m" corresp="#s9:l5">mulieribus</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Doen sanc met bliden <rhyme label="a" type="f">ghescalle</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ioseph <rhyme label="b" type="m">alleluya</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Ende die enghelen <rhyme label="a" type="f" corresp="#s10:l1">alle</rhyme></l>
        <l id="s10:l4" met="3" type="-+">In excelsis <rhyme label="b" type="m" corresp="#s10:l2">gloria</rhyme></l>
        <l id="s10:l5" met="6" type="+-">Et in terra pax <rhyme label="c" type="m">hominibus</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Va la sus / va la <rhyme label="c" type="m" corresp="#s10:l5">sus</rhyme></l>
        <l id="s10:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s10:l5">Iesus</rhyme>.</l>
        <l id="s10:l8" met="6" type="+-">Ghi sijt ons <rhyme label="c" type="m" corresp="#s10:l5">dominus</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die herdekens opten <rhyme label="a" type="f">velde</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Bi haren scapen <rhyme label="b" type="m">laghen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Met eender claerheyt <rhyme label="a" type="f" corresp="#s11:l1">snelle</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Den enghel dat sij <rhyme label="b" type="m" corresp="#s11:l2">saghen</rhyme></l>
        <l id="s11:l5" met="6" type="+-">Et in terra pax <rhyme label="c" type="m">hominibus</rhyme></l>
        <l id="s11:l6" met="4" type="+-">Va la sus / va la <rhyme label="c" type="m" corresp="#s11:l5">sus</rhyme></l>
        <l id="s11:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s11:l5">Iesus</rhyme>.</l>
        <l id="s11:l8" met="6" type="+-">Ghi sijt ons <rhyme label="c" type="m" corresp="#s11:l5">dominus</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Met groter <rhyme label="a" type="f">anxticheden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Werden sij doen <rhyme label="b" type="m">beuaen</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Die enghel tot hen <rhyme label="a" type="f" corresp="#s12:l1">seyde</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Laet v ghedachten <rhyme label="b" type="m" corresp="#s12:l2">staen</rhyme></l>
        <l id="s12:l5" met="6" type="+-">Et in terra pax <rhyme label="c" type="m">hominibus</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Va la sus / va la <rhyme label="c" type="m" corresp="#s12:l5">sus</rhyme></l>
        <l id="s12:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s12:l5">Iesus</rhyme>.</l>
        <l id="s12:l8" met="6" type="+-">Ghi sijt ons <rhyme label="c" type="m" corresp="#s12:l5">dominus</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Gaet tot Bethleem <rhyme label="a" type="f">binnen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Daer suldy nu ten <rhyme label="b" type="m">stonden</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Iesum Cristum gheboren <rhyme label="a" type="f" corresp="#s13:l1">vinden</rhyme></l>
        <l id="s13:l4" met="3" type="-+">In doeckelkijns <rhyme label="b" type="m" corresp="#s13:l2">ghewonden</rhyme></l>
        <l id="s13:l5" met="6" type="+-">Et in terra pax <rhyme label="c" type="m">hominibus</rhyme></l>
        <l id="s13:l6" met="4" type="+-">Va la sus / va la <rhyme label="c" type="m" corresp="#s13:l5">sus</rhyme></l>
        <l id="s13:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s13:l5">Iesus</rhyme>.</l>
        <l id="s13:l8" met="6" type="+-">Ghi sijt ons <rhyme label="c" type="m" corresp="#s13:l5">dominus</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Doen vonden si dat <rhyme label="a" type="f">kindekijn</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Gheleyt in eender <rhyme label="b" type="m">crebben</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Van hoye so was sijn <rhyme label="a" type="f" corresp="#s14:l1">beddekijn</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Hi en wout niet anders <rhyme label="b" type="m" corresp="#s14:l2">hebben</rhyme>.</l>
        <l id="s14:l5" met="6" type="+-">Et in terra pax <rhyme label="c" type="m">hominibus</rhyme></l>
        <l id="s14:l6" met="4" type="+-">Va la sus / va la <rhyme label="c" type="m" corresp="#s14:l5">sus</rhyme></l>
        <l id="s14:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s14:l5">Iesus</rhyme>.</l>
        <l id="s14:l8" met="6" type="+-">Ghi sijt ons <rhyme label="c" type="m" corresp="#s14:l5">dominus</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Tkindeken wert na Moyses <rhyme label="a" type="f">wet</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Ten achtsten dagen <rhyme label="b" type="m">besneden</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Ouermits sijn grote <rhyme label="a" type="f" corresp="#s15:l1">ootmoedicheyt</rhyme></l>
        <l id="s15:l4" met="3" type="-+">In sine ionghe <rhyme label="b" type="m" corresp="#s15:l2">leden</rhyme></l>
        <l id="s15:l5" met="6" type="+-">Et in terra pax <rhyme label="c" type="m">hominibus</rhyme></l>
        <l id="s15:l6" met="4" type="+-">Va la sus / va la <rhyme label="c" type="m" corresp="#s15:l5">sus</rhyme></l>
        <l id="s15:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s15:l5">Iesus</rhyme>.</l>
        <l id="s15:l8" met="6" type="+-">Ghi sijt ons <rhyme label="c" type="m" corresp="#s15:l5">dominus</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Den naem die hem de engel <rhyme label="a" type="f">gaf</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Eer dat kint gheboren <rhyme label="b" type="m">was</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Die wert hem daer <rhyme label="a" type="f" corresp="#s16:l1">ghegeuen</rhyme>.</l>
        <l id="s16:l4" met="3" type="-+">Iesus men seker <rhyme label="b" type="m" corresp="#s16:l2">las</rhyme></l>
        <l id="s16:l5" met="6" type="+-">Et in terra pax <rhyme label="c" type="m">hominibus</rhyme></l>
        <l id="s16:l6" met="4" type="+-">Va la sus / va la <rhyme label="c" type="m" corresp="#s16:l5">sus</rhyme></l>
        <l id="s16:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s16:l5">Iesus</rhyme>.</l>
        <l id="s16:l8" met="6" type="+-">Ghi sijt ons <rhyme label="c" type="m" corresp="#s16:l5">dominus</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Nu laet ons louen desen <rhyme label="a" type="f">heere</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Die is ghenoemt <rhyme label="b" type="m">Iesus</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Stillen wi hem hi weent so <rhyme label="a" type="f" corresp="#s17:l1">seere</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Ende singhen hem <rhyme label="b" type="m" corresp="#s17:l2">aldus</rhyme></l>
        <l id="s17:l5" met="6" type="+-">Te deum <rhyme label="c" type="m">laudamus</rhyme></l>
        <l id="s17:l6" met="4" type="+-">Vala sus vala <rhyme label="c" type="m" corresp="#s17:l5">sus</rhyme></l>
        <l id="s17:l7" met="6" type="+-">Swighet soete <rhyme label="c" type="m" corresp="#s17:l5">Iesus</rhyme></l>
        <l id="s17:l8" met="6" type="+-">Ghi sijt onse <rhyme label="c" type="m" corresp="#s17:l5">dominus</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
