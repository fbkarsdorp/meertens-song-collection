    <melody>Wees allen verheugd stel druk ter zijde</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Tot laudaty myns lieffs wil ick <rhyme label="a" type="f">beginnen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Te singen een vroolyck <rhyme label="b" type="m">liet</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Want haere graty passeert alle <rhyme label="a" type="f" corresp="#s1:l1">godinnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Geen schoonder met oogen men <rhyme label="b" type="m" corresp="#s1:l2">siet</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Dat ick haer moet syn <rhyme label="c" type="m">absent</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Tis voorwaer een swaer <rhyme label="c" type="m" corresp="#s1:l5">torment</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Mocht ick verwarmen in haer <rhyme label="d" type="f">narmen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Soo waer alle myn druck <rhyme label="c" type="m" corresp="#s1:l5">geynt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Eerbaer getrouw vreedsaemich van <rhyme label="a" type="f">natueren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Minnelyck en wel <rhyme label="b" type="m">bespraeckt</rhyme></l>
        <l id="s2:l3" met="5" type="-+">Is dees karsouw bevonden taller <rhyme label="a" type="f" corresp="#s2:l1">uren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Naer troost myn herteken <rhyme label="b" type="m" corresp="#s2:l2">haeckt</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Dat ick haer moet syn <rhyme label="c" type="m">absent</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Tis voorwaer een swaer <rhyme label="c" type="m" corresp="#s2:l5">torment</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Mocht ick verwarmen in haer <rhyme label="d" type="f">narmen</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Soo waer alle myn druck <rhyme label="c" type="m" corresp="#s2:l5">geent</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Niemant op aerden mach haer te recht <rhyme label="a" type="f">beclagen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Want ouer all maeckt sy <rhyme label="b" type="m">vreucht</rhyme></l>
        <l id="s3:l3" met="5" type="-+">Hooch van waerden ick moetet v gaen <rhyme label="a" type="f" corresp="#s3:l1">vragen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Is myn lieff verciert met <rhyme label="b" type="m" corresp="#s3:l2">deucht</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Dat ick haer moet syn <rhyme label="c" type="m">absent</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Tis voorwaer een swaer <rhyme label="c" type="m" corresp="#s3:l5">torment</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Mocht ick verwarmen in haer <rhyme label="d" type="f">narmen</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Soo waer alle myn druck <rhyme label="c" type="m" corresp="#s3:l5">geent</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Alle princessen die moogen haer wel <rhyme label="a" type="f">croonen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Bouen all heeft sy den <rhyme label="b" type="m">prys</rhyme></l>
        <l id="s4:l3" met="5" type="-+">Geene matressen oock waer sy moogen <rhyme label="a" type="f" corresp="#s4:l1">woonen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">So verstandich noch soo <rhyme label="b" type="m" corresp="#s4:l2">wys</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Dat ick haer moet syn <rhyme label="c" type="m">absent</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Tis voorwaer een swaer <rhyme label="c" type="m" corresp="#s4:l5">torment</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Mocht ick verwarmen in haer <rhyme label="d" type="f">narmen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Soo waer alle myn druck <rhyme label="c" type="m" corresp="#s4:l5">geent</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
