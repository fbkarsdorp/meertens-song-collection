    <tags/>
    <melody>Luister eens Jeanette</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">Luister eens Jannette, staat iets moois u <rhyme label="a" type="m">aan</rhyme></l>
        <l id="s1:l2" met="2" type="+-"><rhyme label="b" type="f">Laridette</rhyme>,</l>
        <l id="s1:l3" met="6" type="-+">Luister eens Jannette, om na Parys te <rhyme label="a" type="m" corresp="#s1:l1">gaan</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Ja dog, myn Heer, ja dog; myn <rhyme label="c" type="m">Heer</rhyme>?</l>
        <l id="s1:l5" met="2" type="-+">Zei de <rhyme label="b" type="f" corresp="#s1:l2">Brunette</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Maar waarom my, maar waarom <rhyme label="d" type="m">my</rhyme>;</l>
        <l id="s1:l7" met="2" type="+-">Dit <rhyme label="e" type="m">aangeboon</rhyme>?</l>
        <l id="s1:l8" met="6" type="+-">Hoe lieve Jannette gy met zo veel <rhyme label="e" type="m" corresp="#s1:l7">schoon</rhyme></l>
        <l id="s1:l9" met="2" type="+-"><rhyme label="b" type="f" corresp="#s1:l2">Laridette</rhyme>,</l>
        <l id="s1:l10" met="6" type="+-">Hoe lieve Jannette, vind dit <rhyme label="e" type="m" corresp="#s1:l7">ongewoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Luister eens Jannette, zo gy geld <rhyme label="a" type="m">bemind</rhyme>.</l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="b" type="f">Laridette</rhyme>?</l>
        <l id="s2:l3" met="6" type="-+">Luister eens; Jannette, daar: neem aan myn <rhyme label="a" type="m" corresp="#s2:l1">Kind</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Og, og, myn Heer, Og, og, myn <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Zei de <rhyme label="b" type="f" corresp="#s2:l2">Brunette</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Om wat te doen? Om wat te <rhyme label="d" type="m">doen</rhyme>?</l>
        <l id="s2:l7" met="2" type="+-">My geld <rhyme label="e" type="m">geboon</rhyme>,</l>
        <l id="s2:l8" met="6" type="+-">Hoe lieve Jannette? gy met zo veel <rhyme label="e" type="m" corresp="#s2:l7">schoon</rhyme>?</l>
        <l id="s2:l9" met="2" type="+-"><rhyme label="b" type="f" corresp="#s2:l2">Laridette</rhyme>,</l>
        <l id="s2:l10" met="6" type="+-">Hoe lieve Jannette, vind dit <rhyme label="e" type="m" corresp="#s2:l7">ongewoon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Luister eens, Jannette? geef me een lekkre <rhyme label="a" type="m">zoen</rhyme>.</l>
        <l id="s3:l2" met="2" type="+-"><rhyme label="b" type="f">Laridette</rhyme>,</l>
        <l id="s3:l3" met="6" type="-+">Luister eens Jannette, toe; dat moet je <rhyme label="a" type="m" corresp="#s3:l1">doen</rhyme>?</l>
        <l id="s3:l4" met="4" type="-+">Og, og, myn Heer, Og, og, myn <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Zei de <rhyme label="b" type="f" corresp="#s3:l2">Brunette</rhyme>;</l>
        <l id="s3:l6" met="4" type="-+">Ik weet niet hoe; Ik weet niet <rhyme label="d" type="m">hoe</rhyme>;</l>
        <l id="s3:l7" met="2" type="+-">Ik ’t zeggen <rhyme label="e" type="m">zal</rhyme>?</l>
        <l id="s3:l8" met="6" type="+-">Weet dan als Jannette, mint in dit <rhyme label="e" type="m" corresp="#s3:l7">geval</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-"><rhyme label="b" type="f" corresp="#s3:l2">Laridette</rhyme>,</l>
        <l id="s3:l10" met="6" type="+-">Weet dan dat Jannette, ’t geeft voor <rhyme label="e" type="m" corresp="#s3:l7">niemendal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
