  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Och edel ziel verstaet my <rhyme label="a" type="m">wal</rhyme></l>
        <l id="s1:l2" met="4" type="-+">hoe suetelic sanc die fiere <rhyme label="a" type="m" corresp="#s1:l1">nachtegael</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al onder den doernen daer hi <rhyme label="a" type="m">stont</rhyme></l>
        <l id="s2:l2" met="4" type="-+">van mynnen was hi al <rhyme label="a" type="m" corresp="#s2:l1">doerwont</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Och heuet daer ymant honger of <rhyme label="a" type="m">dorst</rhyme></l>
        <l id="s3:l2" met="4" type="-+">die come ende drinke wt mynre <rhyme label="a" type="m" corresp="#s3:l1">borst</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Myns harten bloet wilic hem <rhyme label="a" type="m">scencken</rhyme></l>
        <l id="s4:l2" met="4" type="-+">o mensche wilt myns dodes <rhyme label="a" type="m" corresp="#s4:l1">gedencken</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ende latent di ter harten <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">o edel ziel om | v is my dat <rhyme label="a" type="m" corresp="#s5:l1">gedaen</rhyme></l>
      </lg>
