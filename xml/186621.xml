<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186621</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1790</year-end>
    </date>
    <tags/>
    <melody>Le traquenard</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Herfstyd schenkt ons druiven <rhyme label="a" type="m">bloed</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dat de vreugde leven <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Wyn, wyn, frisschen <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Schenkt zy zeker, in den <rhyme label="c" type="f">Beker</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Wyn, wyn, koelen <rhyme label="b" type="m" corresp="#s1:l3">wyn</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Is de beste <rhyme label="b" type="m" corresp="#s1:l3">medicyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Herfstyd schenkt ons vlees en <rhyme label="a" type="m">spek</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Die 't niet lust is stapel <rhyme label="a" type="m" corresp="#s2:l1">gek</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Vlees, vlees, spek en <rhyme label="b" type="m">vlees</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Corbonaden, <rhyme label="c" type="f">welgebraaden</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Vlees, vlees, <rhyme label="b" type="m" corresp="#s2:l3">Ossenvlees</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Haat geen Turk, Polak, of <rhyme label="b" type="m" corresp="#s2:l3">Snoes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wel wat geeft het een <rhyme label="a" type="m">rumoer</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Als de slagt komt langs de <rhyme label="a" type="m" corresp="#s3:l1">vloer</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Vet, vet, alles <rhyme label="b" type="m">vet</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">In de Keuken, zoo te <rhyme label="c" type="f">smeuken</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Vet, vet, alles <rhyme label="b" type="m" corresp="#s3:l3">vet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ã´ Wat geeft die slagt een <rhyme label="b" type="m" corresp="#s3:l3">pret</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dikke worsten, van een <rhyme label="a" type="m">vaam</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Beulingen: zoo <rhyme label="a" type="m" corresp="#s4:l1">aangenaam</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Bloed, bloed, wel <rhyme label="b" type="m">gandsbloed</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Herfst dagen, <rhyme label="c" type="f">volbehaagen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Bloed, bloed, wel gands <rhyme label="b" type="m" corresp="#s4:l3">bloed</rhyme>!</l>
        <l id="s4:l6" met="4" type="+-">Wat is doch de Herfst <rhyme label="b" type="m" corresp="#s4:l3">goed</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Welkom Jager, welkom <rhyme label="a" type="m">baas</rhyme>!</l>
        <l id="s5:l2" met="4" type="+-">Met het knapgeschooten <rhyme label="a" type="m" corresp="#s5:l1">Haas</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Poef, paf, poef poef <rhyme label="b" type="m">paf</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Daar geen snippen, U <rhyme label="c" type="f">ontglippen</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Poef, paf, poef poef <rhyme label="b" type="m" corresp="#s5:l3">paf</rhyme>.</l>
        <l id="s5:l6" met="4" type="+-">Jager! leg uw vangst hier <rhyme label="b" type="m" corresp="#s5:l3">af</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Jonge Haanen <rhyme label="a" type="m">vetgemest</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Smaaken nu ook <rhyme label="a" type="m" corresp="#s6:l1">allerbest</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Poen, poen, een <rhyme label="b" type="m">Kapoen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Welgebraden, by <rhyme label="c" type="f">Saladen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Poen, poen, een <rhyme label="b" type="m" corresp="#s6:l3">Kapoen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Kan ook aan de smaak <rhyme label="b" type="m" corresp="#s6:l3">voldoen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Lieve Herfst, gulle <rhyme label="a" type="m">maagd</rhyme>!</l>
        <l id="s7:l2" met="4" type="+-">Wie zich over jou <rhyme label="a" type="m" corresp="#s7:l1">beklaagt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Is, is, voor <rhyme label="b" type="m">gewis</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">'t Lekker eten <rhyme label="c" type="f">alvergeeten</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Is, is, voor <rhyme label="b" type="m" corresp="#s7:l3">gewis</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Zeker ook niet al te <rhyme label="b" type="m" corresp="#s7:l3">fris</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
