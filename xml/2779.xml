    <melody>Ik heb gevrijd zo schone vrouw</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ick heb geurit wel seeue <rhyme label="a" type="m">Jaer</rhyme></l>
        <l id="s1:l2" met="4" type="-+">een weinich min maer dan ick <rhyme label="b" type="m">waen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Een schoen Jonck wyf een beelt was <rhyme label="a" type="m" corresp="#s1:l1">klaer</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Hadt Ick In mynen sin <rhyme label="b" type="m" corresp="#s1:l2">gedaen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Wou syt dan In statie <rhyme label="b" type="m" corresp="#s1:l2">staen</rhyme></l>
        <l id="s1:l6" met="4" type="-+">soe waer myn trueren al <rhyme label="b" type="m" corresp="#s1:l2">gedaen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">wou syt myn <rhyme label="c" type="f">gunnen</rhyme></l>
        <l id="s1:l8" met="4" type="-+">syt stoeck goet fier sy brout goet <rhyme label="d" type="m">bier</rhyme></l>
        <l id="s1:l9" met="3" type="-+">sy wast sy boent de <rhyme label="e" type="f">tonne</rhyme></l>
        <l id="s1:l10" met="4" type="-+">sy stapelt het het hout sy roert het <rhyme label="f" type="m">mout</rhyme></l>
        <l id="s1:l11" met="3" type="-+">sy veecht de keuken <rhyme label="e" type="f" corresp="#s1:l9">omme</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hoe gaeren sou Ick haer helpe <rhyme label="a" type="m">koeken</rhyme></l>
        <l id="s2:l2" met="4" type="-+">als de vrou te keuke <rhyme label="b" type="m">gaet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">En dat viertge helpe <rhyme label="a" type="m" corresp="#s2:l1">stoecken</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Wilsit dan In danck <rhyme label="b" type="m" corresp="#s2:l2">voerstaen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Die volle kan die soude wy <rhyme label="b" type="m" corresp="#s2:l2">lichten</rhyme></l>
        <l id="s2:l6" met="4" type="-+">En die leege laeten <rhyme label="b" type="m" corresp="#s2:l2">staen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">wou syt myn <rhyme label="c" type="f">gunnen</rhyme></l>
        <l id="s2:l8" met="4" type="-+">syt stoeck goet fier sy brout goet <rhyme label="d" type="m">bier</rhyme></l>
        <l id="s2:l9" met="3" type="-+">sy wast sy boent de <rhyme label="e" type="f">tonne</rhyme></l>
        <l id="s2:l10" met="4" type="-+">sy stapelt het het hout sy roert het <rhyme label="f" type="m">mout</rhyme></l>
        <l id="s2:l11" met="3" type="-+">sy veecht de keuken <rhyme label="e" type="f" corresp="#s2:l9">omme</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hoe gaeren sou Ick by haer <rhyme label="a" type="m">slaepe</rhyme></l>
        <l id="s3:l2" met="4" type="-+">als dy vrou te bedde <rhyme label="b" type="m">gaet</rhyme></l>
        <l id="s3:l3" met="4" type="-+">In myn hertge met haer <rhyme label="a" type="m" corresp="#s3:l1">voermaeeke</rhyme></l>
        <l id="s3:l4" met="4" type="-+">wou sit dan in danck <rhyme label="b" type="m" corresp="#s3:l2">voerstaen</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Ick sou soe gaeren by haer <rhyme label="b" type="m" corresp="#s3:l2">slaepen</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Ick sou en noch veel lieuer met haer <rhyme label="b" type="m" corresp="#s3:l2">waecken</rhyme></l>
        <l id="s3:l7" met="2" type="-+">wou syt myn <rhyme label="c" type="f">gunnen</rhyme></l>
        <l id="s3:l8" met="4" type="-+">syt stoeck goet fier sy brout goet <rhyme label="d" type="m">bier</rhyme></l>
        <l id="s3:l9" met="3" type="-+">sy wast sy boent de <rhyme label="e" type="f">tonne</rhyme></l>
        <l id="s3:l10" met="4" type="-+">sy stapelt het het hout sy roert het <rhyme label="f" type="m">mout</rhyme></l>
        <l id="s3:l11" met="3" type="-+">sy veecht de keuken <rhyme label="e" type="f" corresp="#s3:l9">omme</rhyme></l>
      </lg>
    </lg>
  </text>
