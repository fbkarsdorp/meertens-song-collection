        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 006 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL jeld'ne' och! Jo mijn <rhyme label="a" type="f">suwnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">t' Oon-hiergjen azz' <rhyme label="a" type="f" corresp="#s1:l1">jiet-uwnen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Och! dijlgje my naet <rhyme label="b" type="m">uwt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ynn' toarn'-fjoers gleauwne' <rhyme label="c" type="f">ontflamjen</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Och! jou my jon <rhyme label="c" type="f" corresp="#s1:l4">forgramjen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Naet oer (ô HEER'!) to <rhyme label="b" type="m" corresp="#s1:l3">buwt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al bin 'k uwt-medd' trog 't <rhyme label="a" type="f">suwnlijck</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Jiette' iz jon gunste'. <rhyme label="a" type="f" corresp="#s2:l1">on-gruwn'-rijck</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Mijn to-flechte'. Het 's my <rhyme label="b" type="m">bang</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Mijn bient', mijn siel eang <rhyme label="c" type="f">triljet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">HEER' firdd' Jo, heelje' in stilje' <rhyme label="c" type="f" corresp="#s2:l4">it</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Och! Y mijn God, ho <rhyme label="b" type="m" corresp="#s2:l3">langh</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Keer' weer, litt' naet, (ljeave' <rhyme label="a" type="f">HEERE</rhyme>)</l>
        <l id="s3:l2" met="3" type="-+">Mijn siel jon holpe' <rhyme label="a" type="f" corresp="#s3:l1">ombeere</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om jon goertiern'heyts <rhyme label="b" type="m">will</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">HEER! hâd m'yn lib in <rhyme label="c" type="f">witt'niss'</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Deade' iz jons dwaens <rhyme label="c" type="f" corresp="#s3:l4">forjitt'niss'</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">'t Graef swijgget jon lof <rhyme label="b" type="m" corresp="#s3:l3">stil</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'k Bin wirgh, lef, jâ t' <rhyme label="a" type="f">uwt-eyne</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Trog trien', <rhyme label="a" type="f" corresp="#s4:l1">eag-dripp'-stirt-reyne</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'t Bod swomt m' yn sât hiet <rhyme label="b" type="m">wiet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat 'k (ô hert-breck!) <rhyme label="c" type="f">uwt-sluwzje</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Mits 'k nacht oon nacht <rhyme label="c" type="f" corresp="#s4:l4">gâl-guwzje</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">O suwn'-lest! swierst <rhyme label="b" type="m" corresp="#s4:l3">fortriet</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn eagen binne' <rhyme label="a" type="f">uwt-litsen</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Och! 't soene' iz my <rhyme label="a" type="f" corresp="#s5:l1">tobritsen</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">'k Bin, for mijn jieren, <rhyme label="b" type="m">âd</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Om 't my mijn fijnn' <rhyme label="c" type="f">oon-spijtgje</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Dy 't jamm're' yn my <rhyme label="c" type="f" corresp="#s5:l4">gluwp-nijtgje</rhyme>.</l>
        <l id="s5:l6" met="3" type="-+">O! 't hert besjiet m' <rhyme label="b" type="m" corresp="#s5:l3">yz-kâd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wijckje', ô fen my, <rhyme label="a" type="f">quea-dieders</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ongerjuechtheyts <rhyme label="a" type="f" corresp="#s6:l1">wirck-beschieders</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Want de' HEERE' het, ney <rhyme label="b" type="m">bejeart'</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Mijn neads-rops wolck' trog <rhyme label="c" type="f">brecken</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Boet-firddigh herts-gruwn' <rhyme label="c" type="f" corresp="#s6:l4">sprecken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Uwt yn-goe njue <rhyme label="b" type="m" corresp="#s6:l3">forheard</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn luwkjen, ljuentjen, <rhyme label="a" type="f">trienjen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Heart God, mey <rhyme label="a" type="f" corresp="#s7:l1">holp'-forlienjen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dy 't hosck-mijld oon-nimt. <rhyme label="b" type="m">Wier</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">Wier binne nu mijn <rhyme label="c" type="f">fijnnen</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">Beschamme', oer-stjealpt mey <rhyme label="c" type="f" corresp="#s7:l4">pijnnen</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Loack', so 's God my <rhyme label="b" type="m" corresp="#s7:l3">goertier</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
