<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>52330</id>
    <date>
      <year-start>1819</year-start>
      <year-end>1842</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De wereld is in rep en roer</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wat ziet men door de gantsche <rhyme label="a" type="m">stad</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Elk prankt zich weer in ryker <rhyme label="a" type="m" corresp="#s1:l1">schad</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Met nieuw gemaakte <rhyme label="b" type="f">kleeren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met nieuw gemaakte <rhyme label="b" type="f" corresp="#s1:l3">kleeren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Men ziet een dienstmeid met een <rhyme label="c" type="m">knecht</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Al is hun loontje noch zoo <rhyme label="c" type="m" corresp="#s1:l5">slecht</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Als damens en als <rhyme label="b" type="f" corresp="#s1:l3">heeren</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Als damens en als <rhyme label="b" type="f" corresp="#s1:l3">heeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een jonge meid gaat met haar <rhyme label="a" type="m">vent</rhyme>;</l>
        <l id="s2:l2" met="4" type="-+">De mode maakt haar <rhyme label="a" type="m" corresp="#s2:l1">exselent</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Haar wyde poolsche <rhyme label="b" type="f">mouwen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Haar wyde poolsche <rhyme label="b" type="f" corresp="#s2:l3">mouwen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Een omslagsdoek en een <rhyme label="c" type="m">japon</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">'T is al wat men tot pracht <rhyme label="c" type="m" corresp="#s2:l5">verzon</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wie kan zoon' boel <rhyme label="b" type="f" corresp="#s2:l3">vertrouwen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Wie kan zoon' boel <rhyme label="b" type="f" corresp="#s2:l3">vertrouwen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zoo pronken zy net als een <rhyme label="a" type="m">pop</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Elk haald ook breed zyn zylen <rhyme label="a" type="m" corresp="#s3:l1">op</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met haar geploid <rhyme label="b" type="f">cornetje</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met haar geploid <rhyme label="b" type="f" corresp="#s3:l3">cornetje</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">'T is alles mode wat men <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">'T schralenstyn en anders <rhyme label="c" type="m" corresp="#s3:l5">niet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Van Kaa, van Mie en <rhyme label="b" type="f" corresp="#s3:l3">Betje</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Van Kaa, van Mie en <rhyme label="b" type="f" corresp="#s3:l3">Betje</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En dan een groote strooije <rhyme label="a" type="m">hoed</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die mode die staat elk <rhyme label="a" type="m" corresp="#s4:l1">goed</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Een kleed met drie vier <rhyme label="b" type="f">kraagen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Een kleed met drie vier <rhyme label="b" type="f" corresp="#s4:l3">kraagen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Zoo loopen zy al door de <rhyme label="c" type="m">stad</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Al hebben zy geen hembt aan 't <rhyme label="c" type="m" corresp="#s4:l5">gat</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Om jonkmans te <rhyme label="b" type="f" corresp="#s4:l3">behaagen</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Om jonkmans te <rhyme label="b" type="f" corresp="#s4:l3">behaagen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
