<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191658</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>C'est trop courir les eaux</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">PRonk-Paarl aan Amstels <rhyme label="a" type="m">Kroon</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Als ik 't allokb're <rhyme label="a" type="m" corresp="#s1:l1">schoon</rhyme>,</l>
        <l id="s1:l3" met="6" type="-+">Van uw gezigt bestraal, voel ik ô groote <rhyme label="b" type="m">kragt</rhyme>!</l>
        <l id="s1:l4" met="6" type="-+">Door 't schieten van een Lonk, myn Ziel in Cypris <rhyme label="b" type="m" corresp="#s1:l3">magt</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Was ooyt de God <rhyme label="a" type="m">Apoll'</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Door minnend' yver <rhyme label="a" type="m" corresp="#s2:l1">dol</rhyme></l>
        <l id="s2:l3" met="6" type="-+">Om Dafnis schoonheid? is het wonders dat myn <rhyme label="b" type="m">Ziel</rhyme></l>
        <l id="s2:l4" met="6" type="-+">Op d'alverwinb're Burg van uwe schoonheid <rhyme label="b" type="m" corresp="#s2:l3">viel</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Al waar Natuur haar <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zo zegenryk <rhyme label="a" type="m" corresp="#s3:l1">vertoond</rhyme>;</l>
        <l id="s3:l3" met="6" type="-+">Dat, wien het Zonneligt van uwe glans <rhyme label="b" type="m">bestraalt</rhyme>,</l>
        <l id="s3:l4" met="6" type="-+">Verwonnen en gewond, voor uwe voeten <rhyme label="b" type="m" corresp="#s3:l3">daalt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Myn Jeugdig Brein <rhyme label="a" type="m">verkracht</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Opoffert klacht op <rhyme label="a" type="m" corresp="#s4:l1">klacht</rhyme>,</l>
        <l id="s4:l3" met="6" type="-+">Op 't Autaar uw's gehoor, op hope dat  myn <rhyme label="b" type="m">Beê</rhyme>,</l>
        <l id="s4:l4" met="6" type="-+">De Diamante Rotzen van uw' Zinnen <rhyme label="b" type="m" corresp="#s4:l3">Kneê</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laat 't goude woordje, <rhyme label="a" type="m">vlot</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ontrendelen het <rhyme label="a" type="m" corresp="#s5:l1">slot</rhyme></l>
        <l id="s5:l3" met="6" type="-+">Van uw gekluysde Ziel, op dat myn jagend <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s5:l4" met="6" type="-+">Van 't lang gewilde wit, gelukkig winnaar <rhyme label="b" type="m" corresp="#s5:l3">werd</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
