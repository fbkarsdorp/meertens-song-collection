    <melody>Kasteel van Rijpermonde</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Vveest welkom lieve <rhyme label="a" type="f">Truytje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Weest welkom hier ter <rhyme label="b" type="m">stee</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Koomje met de waghen oft <rhyme label="a" type="f" corresp="#s1:l1">schuytje</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wat brenghse ons nu al <rhyme label="b" type="m" corresp="#s1:l2">mee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hier is de soete koeck en <rhyme label="a" type="f">vijghe</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Maer Krelis hoe vaerje <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Seght selje de Meyt haest <rhyme label="a" type="f" corresp="#s2:l1">krijghe</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Of heeft 'et met jou geen <rhyme label="b" type="m" corresp="#s2:l2">val</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Nu soeckje mijn wat te <rhyme label="a" type="f">quellen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Och Truytje 'k weet van geen <rhyme label="b" type="m">Meyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">'k Kom niet als by de <rhyme label="a" type="f" corresp="#s3:l1">gheselle</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als ickje veel heb <rhyme label="b" type="m" corresp="#s3:l2">gheseyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat wast dan voor een <rhyme label="a" type="f">Diertje</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daer jey noch laest by <rhyme label="b" type="m">ginck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wast niet de Nicht van <rhyme label="a" type="f" corresp="#s4:l1">Kniertje</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die jeyder soo soet <rhyme label="b" type="m" corresp="#s4:l2">ontfinck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoe 'k weet waer jey wil <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">'t Was Tietje Teun Crijnen <rhyme label="b" type="m">snaer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat Meysje is wel te <rhyme label="a" type="f" corresp="#s5:l1">belesen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maer 't schort al meest aen de <rhyme label="b" type="m" corresp="#s5:l2">Vaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wel tijdt dan na Tryn <rhyme label="a" type="f">Wayne</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dats me al een brave <rhyme label="b" type="m">tas</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die selje me wel <rhyme label="a" type="f" corresp="#s6:l1">doudeyne</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Soo wel int hooy als int <rhyme label="b" type="m" corresp="#s6:l2">gras</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Neen als ick doch most <rhyme label="a" type="f">kiesen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ick koosjou voor de <rhyme label="b" type="m">keur</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">'k Heb jou liever als Aeltje <rhyme label="a" type="f" corresp="#s7:l1">Miese</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Och was ick jou <rhyme label="b" type="m" corresp="#s7:l2">Serviteur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
