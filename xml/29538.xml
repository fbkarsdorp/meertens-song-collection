  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Venus <rhyme label="a" type="f">puurlijck</rhyme>!</l>
        <l id="s1:l2" met="2" type="-+">Aenhoort mijn <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Die ick soo <rhyme label="a" type="f" corresp="#s1:l1">suurlijck</rhyme></l>
        <l id="s1:l4" met="2" type="-+">By nacht en <rhyme label="b" type="f" corresp="#s1:l2">dagen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Als <rhyme label="b" type="f" corresp="#s1:l2">regen-vlagen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Moet storten <rhyme label="c" type="m">siet</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Mijn liefde <rhyme label="a" type="f" corresp="#s1:l1">muurlijck</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Hoort mijn <rhyme label="b" type="f" corresp="#s1:l2">gewagen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Is <rhyme label="a" type="f" corresp="#s1:l1">onverduurlijck</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Ick moet <rhyme label="b" type="f" corresp="#s1:l2">vertsagen</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Soo u <rhyme label="b" type="f" corresp="#s1:l2">behagen</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Mijn geen troost <rhyme label="c" type="m" corresp="#s1:l6">biedt</rhyme>.</l>
      </lg>
    </lg>
  </text>
