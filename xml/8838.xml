<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8838</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>God gij die ons zo zeer verstoot</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOdt ghy die ons soo seer <rhyme label="a" type="m">verstoot</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Verstroyt dijns Volcks <rhyme label="b" type="f">Geleder</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En thoornich waert in onser <rhyme label="a" type="m" corresp="#s1:l1">Noot</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vertroost end' heelt ons <rhyme label="b" type="f" corresp="#s1:l2">weder</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Ghy die hebt heel het Landt <rhyme label="c" type="m">beruert</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Gheneest haer Breucken <rhyme label="d" type="f">HEERe</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Want t'is (siet) over al <rhyme label="c" type="m" corresp="#s1:l5">verscheurt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">End' oock vervallen <rhyme label="d" type="f" corresp="#s1:l6">seere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy thoond' u volck een wesen <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ghy gaeft ons al met <rhyme label="b" type="f">eenen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Een sueren dronck, dat wy met <rhyme label="a" type="m" corresp="#s2:l1">smert</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Al tuymelden daer <rhyme label="b" type="f" corresp="#s2:l2">heenen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Doch gheeft ghy noch een teycken <rhyme label="c" type="m">dien</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die vreesen uwen <rhyme label="d" type="f">Name</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Welck sy op werpen, dat daer <rhyme label="c" type="m" corresp="#s2:l5">vlien</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Haer Vyanden al <rhyme label="d" type="f" corresp="#s2:l6">tsame</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Op dat dan worden t'allen <rhyme label="a" type="m">kant</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Verlost, die ghy lieft <rhyme label="b" type="f">HEERE</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">So helpt nu met v <rhyme label="a" type="m" corresp="#s3:l1">rechterhant</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ghenaed'lijck tot ons <rhyme label="b" type="f" corresp="#s3:l2">keere</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Godt hoort my in syn <rhyme label="c" type="m">Heylichdom</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dies is bly myn <rhyme label="d" type="f">gheweten</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Spreeckt: Sichem wil ick deylen <rhyme label="c" type="m" corresp="#s3:l5">om</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En Surchots Dal af <rhyme label="d" type="f" corresp="#s3:l6">meten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Manasse, Gilead is <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die macht myns hoofts daer <rhyme label="b" type="f">neven</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Is Ephraim, en Juda <rhyme label="a" type="m" corresp="#s4:l1">fijn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Myn Raedt en Vorst <rhyme label="b" type="f" corresp="#s4:l2">verheven</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Heel Moab is my <rhyme label="c" type="m">onderdaen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In Edom ick <rhyme label="d" type="f">spatzeere</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Philisten comen my <rhyme label="c" type="m" corresp="#s4:l5">t'ontfaen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Met vreughd' en grooter <rhyme label="d" type="f" corresp="#s4:l6">eere</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wie voert my in een vaste <rhyme label="a" type="m">Stadt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daer ick mach seker <rhyme label="b" type="f">wesen</rhyme>?</l>
        <l id="s5:l3" met="4" type="-+">Wie brengt my T'Edom? sult ghy <rhyme label="a" type="m" corresp="#s5:l1">dat</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Niet doen O Godt <rhyme label="b" type="f" corresp="#s5:l2">ghepresen</rhyme>?</l>
        <l id="s5:l5" met="4" type="-+">Ghy die ons hebt verstroyet <rhyme label="c" type="m">wijt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">End' oock niet wt treckt <rhyme label="d" type="f">meere</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Met onser Heyrcracht in den <rhyme label="c" type="m" corresp="#s5:l5">Strijt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ghelijck ghy deedt wel <rhyme label="d" type="f" corresp="#s5:l6">Eere</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doet ons (Heer) bystant inder <rhyme label="a" type="m">Noot</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Want Menschen Hulp <rhyme label="b" type="f">(Waerachtich</rhyme>)</l>
        <l id="s6:l3" met="4" type="-+">Is gantsch en gaer niet nut, Hoe <rhyme label="a" type="m" corresp="#s6:l1">groot</rhyme></l>
        <l id="s6:l4" met="3" type="-+">De selfde oock is <rhyme label="b" type="f" corresp="#s6:l2">Crachtich</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Met Godt wy willen Daden <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die is ons Cracht <rhyme label="d" type="f">alleyne</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Die onse Vyanden soo <rhyme label="c" type="m" corresp="#s6:l5">koen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Vertredet all' <rhyme label="d" type="f" corresp="#s6:l6">ghemeyne</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
