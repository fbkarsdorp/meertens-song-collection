        <l id="s1:l1" met="4" type="+-">Somelicher wive <rhyme label="a" type="f">unstede</rhyme>.</l>
        <l id="s1:l2" met="6" type="+-">wil ich dat die den guoden wiven ere <rhyme label="b" type="m">si</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">of ir in geynne <rhyme label="a" type="f" corresp="#s1:l1">missedede</rhyme>.</l>
        <l id="s1:l4" met="6" type="+-">wa er kente man die beste danne <rhyme label="b" type="m" corresp="#s1:l2">bi</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">vinden wir einer <rhyme label="c" type="f">wive</rhyme></l>
        <l id="s1:l6" met="2" type="+-"><rhyme label="d" type="m">missedait</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">da bi eine. dusint <rhyme label="c" type="f" corresp="#s1:l5">wive</rhyme></l>
        <l id="s1:l8" met="2" type="+-">dogende <rhyme label="d" type="m" corresp="#s1:l6">hait</rhyme>.</l>
