  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'t FEest van Bachus is <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Drink nu, gy hebt niet te <rhyme label="a" type="f" corresp="#s1:l1">schroomen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wellust treet op 't <rhyme label="b" type="m">schouw-tonneel</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Span nu lustig op uw' <rhyme label="b" type="m" corresp="#s1:l3">keel</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Die niet <rhyme label="c" type="f">dronken</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Legt te <rhyme label="c" type="f" corresp="#s1:l5">ronken</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Kan op 't Kermis Feest niet <rhyme label="d" type="m">meê</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Slaap op taafels en op <rhyme label="e" type="f">banken</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Schuuw de veeren voor de <rhyme label="e" type="f" corresp="#s1:l8">planken</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Als eertyds Silenus <rhyme label="d" type="m" corresp="#s1:l7">deê</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
