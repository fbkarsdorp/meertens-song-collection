    <melody>Laura zat laatst bij de beek</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">PAn die volgt in 't water <rhyme label="a" type="m">haar</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Om die schoone te <rhyme label="b" type="f">be-knippen</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Rozemondje, vol <rhyme label="a" type="m" corresp="#s1:l1">gevaar</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Riep, met half-besturven <rhyme label="b" type="f" corresp="#s1:l2">lippen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">O ghy Satyr, wilt vry <rhyme label="b" type="f" corresp="#s1:l2">slippen</rhyme>!</l>
        <l id="s1:l6" met="4" type="+-">Komt Diana met u <rhyme label="c" type="m">stoet</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Met u Nimphjes en <rhyme label="d" type="f">Goddinnen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Helpt dees Knippert <rhyme label="d" type="f" corresp="#s1:l7">overwinnen</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">En my deze bystant <rhyme label="c" type="m" corresp="#s1:l6">doet</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
