  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TE Bethleem int <rhyme label="a" type="m">pleyn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Heeft ons een maecht <rhyme label="b" type="m">verblijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van God <rhyme label="a" type="m" corresp="#s1:l1">certeyn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Is sy <rhyme label="b" type="m" corresp="#s1:l2">ghebenedijt</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Wij sijn nu <rhyme label="b" type="m" corresp="#s1:l2">beurijt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Hier in dit aertsche <rhyme label="c" type="m">dal</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Als nu ter <rhyme label="b" type="m" corresp="#s1:l2">tijt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Soe sijn wij <rhyme label="b" type="m" corresp="#s1:l2">ghequijt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Van onsen sonden <rhyme label="c" type="m" corresp="#s1:l6">al</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Tot haer was <rhyme label="a" type="m">ghesonden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die enghel <rhyme label="b" type="m">Gabriel</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ende heeft haer <rhyme label="a" type="m" corresp="#s2:l1">ontbonden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met soeten woerden <rhyme label="b" type="m" corresp="#s2:l2">snel</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Doende des vaders <rhyme label="b" type="m" corresp="#s2:l2">beuel</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Soe ick scriftuere <rhyme label="c" type="m">versta</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Seggende dese woorden <rhyme label="b" type="m" corresp="#s2:l2">mede</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Tot Maria reyn van <rhyme label="b" type="m" corresp="#s2:l2">sede</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Aue plena <rhyme label="c" type="m" corresp="#s2:l6">gratia</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dominus <rhyme label="a" type="m">tecum</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Maghet sonder <rhyme label="b" type="m">blame</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Den heere <rhyme label="a" type="m" corresp="#s3:l1">Ihesum</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In dinen <rhyme label="b" type="m" corresp="#s3:l2">lichame</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Suldy des menschen <rhyme label="b" type="m" corresp="#s3:l2">vrame</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Baren sonder <rhyme label="c" type="m">pijn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Der sielen <rhyme label="b" type="m" corresp="#s3:l2">bequame</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Daer bi verlost sal <rhyme label="b" type="m" corresp="#s3:l2">sijn</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Voert helsche quaet <rhyme label="c" type="m" corresp="#s3:l6">fenijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dese suyuer <rhyme label="a" type="m">maecht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In deuotigher <rhyme label="b" type="m">bede</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Sprack seere <rhyme label="a" type="m" corresp="#s4:l1">versaecht</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ick en kende noyt mans <rhyme label="b" type="m" corresp="#s4:l2">sede</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Denghel sprac hout v <rhyme label="b" type="m" corresp="#s4:l2">vrede</rhyme></l>
        <l id="s4:l6" met="3" type="-+">God wil dat alsoe <rhyme label="c" type="m">si</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Doen sprac sy / <rhyme label="b" type="m" corresp="#s4:l2">Ecce</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ancilla <rhyme label="b" type="m" corresp="#s4:l2">domini</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Dese suyuer maghet <rhyme label="c" type="m" corresp="#s4:l6">vri</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dese moederlike <rhyme label="a" type="m">dracht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die baerde corts hier <rhyme label="b" type="m">naer</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Op een die coutste <rhyme label="a" type="m" corresp="#s5:l1">nacht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat licht der enghelen <rhyme label="b" type="m" corresp="#s5:l2">claer</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Sonder wee oft <rhyme label="b" type="m" corresp="#s5:l2">vaer</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Het beliefde God <rhyme label="c" type="m">also</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dengelen songen Gloria <rhyme label="b" type="m" corresp="#s5:l2">claer</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In exelsis <rhyme label="b" type="m" corresp="#s5:l2">deo</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Die herderkens die waren <rhyme label="c" type="m" corresp="#s5:l6">vro</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Iesus wert <rhyme label="a" type="m">besneden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Al opten achtsten <rhyme label="b" type="m">dach</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Onlancx daer na <rhyme label="a" type="m" corresp="#s6:l1">leden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Datmen Maria <rhyme label="b" type="m" corresp="#s6:l2">sach</rhyme></l>
        <l id="s6:l5" met="3" type="+-">So men ons doet <rhyme label="b" type="m" corresp="#s6:l2">ghewach</rhyme></l>
        <l id="s6:l6" met="3" type="-+">In drucken deerlijck <rhyme label="c" type="m">sien</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Met grooten <rhyme label="b" type="m" corresp="#s6:l2">gheclach</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Moeste in Egipten <rhyme label="b" type="m" corresp="#s6:l2">vlien</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Doer Herodes <rhyme label="c" type="m" corresp="#s6:l6">bespien</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Doent kindeken <rhyme label="a" type="m">was</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Derthien daghen <rhyme label="b" type="m">out</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Loefden hem op dat <rhyme label="a" type="m" corresp="#s7:l1">pas</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Drie coninghen <rhyme label="b" type="m" corresp="#s7:l2">menichfout</rhyme></l>
        <l id="s7:l5" met="3" type="+-">Myrre / wieroock / ende <rhyme label="b" type="m" corresp="#s7:l2">gout</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Sij hebben tkijnt <rhyme label="c" type="m">gheeert</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Thelsche <rhyme label="b" type="m" corresp="#s7:l2">verbout</rhyme></l>
        <l id="s7:l8" met="3" type="-+">In pinen <rhyme label="b" type="m" corresp="#s7:l2">verseert</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Met vruechden is <rhyme label="c" type="m" corresp="#s7:l6">bekeert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
