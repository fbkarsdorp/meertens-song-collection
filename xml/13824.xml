    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DAer men aen dees Sparens <rhyme label="a" type="f">stroompjes</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In d’Hout onder d’Eycke <rhyme label="a" type="f" corresp="#s1:l1">boompjes</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Singht tot Astens lof en <rhyme label="b" type="m">pronck</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daer men aen ‘t Crystalle <rhyme label="c" type="f">water</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Hoort Echoos hel <rhyme label="c" type="f" corresp="#s1:l4">geschater</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Lof Warendorp, en <rhyme label="b" type="m" corresp="#s1:l3">Verdonck</rhyme>.</l>
