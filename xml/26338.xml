    <melody>Het viel een hemelse dauw</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE  winter is <rhyme label="a" type="f">verganghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick sie des Meys <rhyme label="b" type="m">virtuyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick sie de loouers <rhyme label="a" type="f" corresp="#s1:l1">hanghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">De bloemkens staen int <rhyme label="b" type="m" corresp="#s1:l2">cruyt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Seer wijdt dat sy <rhyme label="c" type="f">ontluycken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sy staen in haer <rhyme label="d" type="m">saysoen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">De cruyden lustich <rhyme label="c" type="f" corresp="#s1:l5">ruycken</rhyme></l>
        <l id="s1:l8" met="3" type="-+">De Mey staet schoon en <rhyme label="d" type="m" corresp="#s1:l6">groen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick meyn den Mey met <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die de werelt niet en <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat is tgheloof <rhyme label="a" type="f" corresp="#s2:l1">ghepresen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Cruyt, gras en meyn ick <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Maer alle vruchtbaer <rhyme label="c" type="f">rancken</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Wt Christum den <rhyme label="d" type="m">wijngaert</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Die Godt louen en <rhyme label="c" type="f" corresp="#s2:l5">dancken</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dit zijn druyuen <rhyme label="d" type="m" corresp="#s2:l6">vermaert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De vijchboom staet <rhyme label="a" type="f">ontloken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met botten wt <rhyme label="b" type="m">ghebloeyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">De wijnstock soet van <rhyme label="a" type="f" corresp="#s3:l1">roken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Seer vruchtbaer dat hy <rhyme label="b" type="m" corresp="#s3:l2">groeyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Godts wtuercoren <rhyme label="c" type="f">songhen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Daer een seer soet <rhyme label="d" type="m">accoort</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Met nieu gheloouighe <rhyme label="c" type="f" corresp="#s3:l5">tonghen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Soeter sanck was noyt <rhyme label="d" type="m" corresp="#s3:l6">ghehoort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Mijn gheest <rhyme label="a" type="f">veriubileerde</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van desen lof <rhyme label="b" type="m">ghesanck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Mijn hert hem omme <rhyme label="a" type="f" corresp="#s4:l1">keerde</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En gaf Godt prijs en <rhyme label="b" type="m" corresp="#s4:l2">danck</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Doen ginck ick my <rhyme label="c" type="f">vermeyen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In Sions hof <rhyme label="d" type="m">playsant</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En sochte mijn lief met <rhyme label="c" type="f" corresp="#s4:l5">schreyen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Die ick te Ierusalem <rhyme label="d" type="m" corresp="#s4:l6">vant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick gaf hem mijne <rhyme label="a" type="f">trouwe</rhyme></l>
        <l id="s5:l2" met="3" type="-+">De Bruygom <rhyme label="b" type="m">triumphant</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Hy nam my tot zijn <rhyme label="a" type="f" corresp="#s5:l1">vrouwe</rhyme></l>
        <l id="s5:l4" met="3" type="-+">tack een rinck aen mijn <rhyme label="b" type="m" corresp="#s5:l2">hant</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Wie sou de groote <rhyme label="c" type="f">vruechde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Doch moghen spreken <rhyme label="d" type="m">vuyt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Hy riep, dies ick <rhyme label="c" type="f" corresp="#s5:l5">verhuechde</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Coemt dochter lieue <rhyme label="d" type="m" corresp="#s5:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Coemt hier mijn Bruyt, mijn <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Aensiet nu dees <rhyme label="b" type="m">contrey</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ontfangt nu uwe <rhyme label="a" type="f" corresp="#s6:l1">croone</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En maeckt niet meer <rhyme label="b" type="m" corresp="#s6:l2">gheschrey</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ghy moet O schoone <rhyme label="c" type="f">roose</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Onder de dorens <rhyme label="d" type="m">fel</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Wassen een cleyne <rhyme label="c" type="f" corresp="#s6:l5">poose</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En lijden groot <rhyme label="d" type="m" corresp="#s6:l6">ghequel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Van bouen wt den <rhyme label="a" type="f">hooghen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wert ghy mijn Bruyt <rhyme label="b" type="m">bespoeyt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Hoe can de boom <rhyme label="a" type="f" corresp="#s7:l1">verdrooghen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Daer die fonteyn op <rhyme label="b" type="m" corresp="#s7:l2">vloeyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Wint, coude, vier noch <rhyme label="c" type="f">hetten</rhyme></l>
        <l id="s7:l6" met="3" type="-+">En grijpt aen hem gheen <rhyme label="d" type="m">stat</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Gheen dinck en mach hem <rhyme label="c" type="f" corresp="#s7:l5">letten</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Van hem en valt gheen <rhyme label="d" type="m" corresp="#s7:l6">blat</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Op seuen berghen <rhyme label="a" type="f">pleynen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Stonden Roos en Lelien <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s8:l3" met="3" type="-+">En oock seer veel <rhyme label="a" type="f" corresp="#s8:l1">fonteynen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Vol melck en honich <rhyme label="b" type="m" corresp="#s8:l2">soet</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Ick hoorde bouen <rhyme label="c" type="f">musijcke</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Singhen met soet <rhyme label="d" type="m">gheluyt</rhyme></l>
        <l id="s8:l7" met="3" type="-+">De edel Bruyt <rhyme label="c" type="f" corresp="#s8:l5">Cantijcke</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Dat tortelduyfken <rhyme label="d" type="m" corresp="#s8:l6">ruyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Mijn tong macht niet <rhyme label="a" type="f">vermonden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">tVerstant is veel te <rhyme label="b" type="m">cleyn</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Gheen mensch en macht <rhyme label="a" type="f" corresp="#s9:l1">doorgronden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Die schoone vruecht <rhyme label="b" type="m" corresp="#s9:l2">ghemeyn</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Als my mijn lief <rhyme label="c" type="f">ontmoette</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Die hem tot mijwaert <rhyme label="d" type="m">de</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Lieffelijck hy my <rhyme label="c" type="f" corresp="#s9:l5">groette</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Met eenen cus des <rhyme label="d" type="m" corresp="#s9:l6">vre</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Een druyuen coffer <rhyme label="a" type="f">pleyne</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Is mijn vrient <rhyme label="b" type="m">delicaet</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Een bussel Myrren <rhyme label="a" type="f" corresp="#s10:l1">reyne</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Een appelhof <rhyme label="b" type="m" corresp="#s10:l2">granaet</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Sijn ooghen als duyuen <rhyme label="c" type="f">ooghen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Ons bedde groenet <rhyme label="d" type="m">fijn</rhyme></l>
        <l id="s10:l7" met="3" type="-+">tHuys balcken sonder <rhyme label="c" type="f" corresp="#s10:l5">drooghen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Ceder en Cypressen <rhyme label="d" type="m" corresp="#s10:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Tot Saron wast een <rhyme label="a" type="f">blomme</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Een Roos int soete <rhyme label="b" type="m">dal</rhyme></l>
        <l id="s11:l3" met="3" type="-+">De wreede dorens <rhyme label="a" type="f" corresp="#s11:l1">cromme</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Staken haer ouer <rhyme label="b" type="m" corresp="#s11:l2">al</rhyme></l>
        <l id="s11:l5" met="3" type="-+">De Sonne quam haer <rhyme label="c" type="f">pijnen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Steken als brant en <rhyme label="d" type="m">gloet</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Maer schadu doort <rhyme label="c" type="f" corresp="#s11:l5">omschijnen</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Maeckte haer smerte <rhyme label="d" type="m" corresp="#s11:l6">soet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Sijn aenschijn is so <rhyme label="a" type="f">rustich</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Als Cruythof vol <rhyme label="b" type="m">beplant</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Sijn lippen als roosen <rhyme label="a" type="f" corresp="#s12:l1">lustich</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Van myrren vloeyt <rhyme label="b" type="m" corresp="#s12:l2">vailliant</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Sijn handen wilt dit <rhyme label="c" type="f">hooren</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Als gulden ringhen <rhyme label="d" type="m">ient</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Vol Turkoysen, als <rhyme label="c" type="f" corresp="#s12:l5">yuooren</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Is zijn lichaem <rhyme label="d" type="m" corresp="#s12:l6">pertinent</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Hy is in mijnder <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Ghedruckt als seghel <rhyme label="b" type="m">bloot</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Noot, angst, pijn, druck oft <rhyme label="a" type="f" corresp="#s13:l1">smerte</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Sweert, hongher, vier noch <rhyme label="b" type="m" corresp="#s13:l2">doot</rhyme></l>
        <l id="s13:l5" met="3" type="-+">En sal my moghen <rhyme label="c" type="f">scheyden</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Als hy my bystant <rhyme label="d" type="m">doet</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Dus wil ick hem <rhyme label="c" type="f" corresp="#s13:l5">verbeyden</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Bewaert Heer mijnen <rhyme label="d" type="m" corresp="#s13:l6">voet</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Prince Godt van hier <rhyme label="a" type="f">bouen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Vader, Soon, heylich <rhyme label="b" type="m">Gheest</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Tis recht dat wy v <rhyme label="a" type="f" corresp="#s14:l1">louen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Ghy die ons siel <rhyme label="b" type="m" corresp="#s14:l2">gheneest</rhyme></l>
        <l id="s14:l5" met="3" type="-+">O Heere wilt my <rhyme label="c" type="f">stercken</rhyme></l>
        <l id="s14:l6" met="3" type="-+">In alle lijdens <rhyme label="d" type="m">noot</rhyme></l>
        <l id="s14:l7" met="3" type="-+">O Godt wilt in my <rhyme label="c" type="f" corresp="#s14:l5">wercken</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Volstandicheyt ter <rhyme label="d" type="m" corresp="#s14:l6">doot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
