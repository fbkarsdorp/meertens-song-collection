    <melody>J'ai pris de moi-mÃªme campos</melody>
  <author>Aernout van Overbeke</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">NU ben ick weer mijn eygen <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">'k Heb oorlof van mijn selfs <rhyme label="b" type="f">genomen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Nu ick 't gebruyck weer heb <rhyme label="b" type="f" corresp="#s1:l2">bekomen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Van wijn, van glasen, en van <rhyme label="a" type="m" corresp="#s1:l1">kan</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Adieu dan schoone <rhyme label="c" type="f">Aminthe</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">Ick segen desen <rhyme label="d" type="m">dagh</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dien in 't nat van de <rhyme label="c" type="f" corresp="#s1:l5">pinte</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Mijn liefd' verdroncken <rhyme label="d" type="m" corresp="#s1:l6">lagh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick heb mijn sinnen weer <rhyme label="a" type="m">althans</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Geen schoonheyt kan meer op my <rhyme label="b" type="f">wercken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ick heb Cupido by sijn <rhyme label="b" type="f" corresp="#s2:l2">vlercken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Om hem te plucken als een <rhyme label="a" type="m" corresp="#s2:l1">gans</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Adieu dan schoone <rhyme label="c" type="f">Aminthe</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Ick segen desen <rhyme label="d" type="m">dagh</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dien in 't nat van de <rhyme label="c" type="f" corresp="#s2:l5">pinte</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Mijn liefd' verdroncken <rhyme label="d" type="m" corresp="#s2:l6">lagh</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sijn hooft-doeck wil ick binden <rhyme label="a" type="m">los</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">'t Sou recht een slet sijn voor mijn <rhyme label="b" type="f">schoenen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Of om mijn laersen af te <rhyme label="b" type="f" corresp="#s3:l2">boenen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Hem wil ick villen als een <rhyme label="a" type="m" corresp="#s3:l1">Os</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Adieu dan schoone <rhyme label="c" type="f">Aminthe</rhyme>!</l>
        <l id="s3:l6" met="3" type="-+">Ick segen desen <rhyme label="d" type="m">dagh</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dien in 't nat van de <rhyme label="c" type="f" corresp="#s3:l5">pinte</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Mijn liefd' verdroncken <rhyme label="d" type="m" corresp="#s3:l6">lagh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick hebb' sijn pijlen met sijn <rhyme label="a" type="m">boogh</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Daer even op mijn knie doen <rhyme label="b" type="f">breecken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die my 't hart plachten aen te <rhyme label="b" type="f" corresp="#s4:l2">steecken</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Door d'ingangh van mijn weeldrig <rhyme label="a" type="m" corresp="#s4:l1">hoogh</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Adieu dan schoone <rhyme label="c" type="f">Aminthe</rhyme>!</l>
        <l id="s4:l6" met="3" type="-+">Ick segen desen <rhyme label="d" type="m">dagh</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dien in 't nat van de <rhyme label="c" type="f" corresp="#s4:l5">pinte</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Mijn liefd' verdroncken <rhyme label="d" type="m" corresp="#s4:l6">lagh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick acht my nu geluckigh <rhyme label="a" type="m">vrient</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Niet meer in slaverny te <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">'k Heb nu geen onrust als voor <rhyme label="b" type="f" corresp="#s5:l2">desen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Doen ick vrouw Venus heb <rhyme label="a" type="m" corresp="#s5:l1">gedient</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Adieu dan schoone <rhyme label="c" type="f">Aminthe</rhyme>!</l>
        <l id="s5:l6" met="3" type="-+">Ick segen desen <rhyme label="d" type="m">dagh</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dien in 't nat van de <rhyme label="c" type="f" corresp="#s5:l5">pinte</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Mijn liefd' verdroncken <rhyme label="d" type="m" corresp="#s5:l6">lagh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
