    <tags>
      <tag standard-name="dialooglied" translation="dialogue song">dialooglied</tag>
    </tags>
    <melody>De nachtegaal die zong een lied</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author>Joan Ysermans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">LEst hoorden ick een Venus <rhyme label="a" type="m">dier</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Staen claghen <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die sonder man en conde <rhyme label="a" type="m" corresp="#s1:l1">schier</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Gheleuen <rhyme label="b" type="m" corresp="#s1:l2">meer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ick hoorde, half <rhyme label="c" type="f">besvveken</rhyme>,</l>
        <l id="s1:l6" met="1" type="-+">Haer <rhyme label="c" type="f" corresp="#s1:l5">spreken</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Mijn jeucht <rhyme label="d" type="m">verdvvijnt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Den maeghdom <rhyme label="d" type="m" corresp="#s1:l7">schijnt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Wilt my laes! t'hert af <rhyme label="c" type="f" corresp="#s1:l5">steken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De moeder die dit deirlijck <rhyme label="a" type="m">stuck</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Heeft <rhyme label="b" type="m">aenghehoort</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Versuchte, siende t'kint <rhyme label="a" type="m" corresp="#s2:l1">voldruck</rhyme></l>
        <l id="s2:l4" met="2" type="-+">En sprack <rhyme label="b" type="m" corresp="#s2:l2">verstoort</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ghy moet u duldich <rhyme label="c" type="f">lijden</rhyme></l>
        <l id="s2:l6" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s2:l5">mijden</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Van vrijers <rhyme label="d" type="m">noch</rhyme>;</l>
        <l id="s2:l8" met="2" type="-+">Wilt setten <rhyme label="d" type="m" corresp="#s2:l7">doch</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Dees ydelheyt <rhyme label="c" type="f" corresp="#s2:l5">besijden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het selsaem ouer-peysen, <rhyme label="a" type="m">doet</rhyme></l>
        <l id="s3:l2" met="2" type="-+">My groote <rhyme label="b" type="m">pijn</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Dats d'oorsaeck moerken dat ick <rhyme label="a" type="m" corresp="#s3:l1">moet</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Gheholpen <rhyme label="b" type="m" corresp="#s3:l2">sijn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Oock doet my t'langh <rhyme label="c" type="f">vertoeuen</rhyme>,</l>
        <l id="s3:l6" met="1" type="-+"><rhyme label="c" type="f" corresp="#s3:l5">Bedroeueu</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Want vvie vveet <rhyme label="d" type="m">bet</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Waer dat hem <rhyme label="d" type="m" corresp="#s3:l7">let</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Dan die de pijn moet <rhyme label="c" type="f" corresp="#s3:l5">proeuen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy sijt noch jonck al sijdy <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s4:l2" met="2" type="-+">t'Is teghen <rhyme label="b" type="m">re'en</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ick vrees ghy bleefter onder <rhyme label="a" type="m" corresp="#s4:l1">doot</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Sprack d'oude <rhyme label="b" type="m" corresp="#s4:l2">queen</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Beydt vry noch seuen <rhyme label="c" type="f">jaren</rhyme></l>
        <l id="s4:l6" met="1" type="-+">B Laet <rhyme label="c" type="f" corresp="#s4:l5">varen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Dees lusten <rhyme label="d" type="m">cranck</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Oft ghy eer <rhyme label="d" type="m" corresp="#s4:l7">lanck</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Sult comen in <rhyme label="c" type="f" corresp="#s4:l5">besvvaren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Te cleyn? vvaert dit dat my <rhyme label="a" type="m">ghebrack</rhyme>?</l>
        <l id="s5:l2" met="2" type="-+">Neen moerken <rhyme label="b" type="m">neen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Doch, ick sal vvel eens proeuen, <rhyme label="a" type="m" corresp="#s5:l1">sprack</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Dit meysken <rhyme label="b" type="m" corresp="#s5:l2">kleen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En maken gheen <rhyme label="c" type="f">gheschille</rhyme></l>
        <l id="s5:l6" met="1" type="-+">Al <rhyme label="c" type="f" corresp="#s5:l5">stille</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Want t'is al <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Wat dat-men <rhyme label="d" type="m" corresp="#s5:l7">doet</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Alst is om beters <rhyme label="c" type="f" corresp="#s5:l5">wille</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De moeder dacht sy meynet <rhyme label="a" type="m">t'ian</rhyme></l>
        <l id="s6:l2" met="2" type="-+">En sprack lief <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Nemt lieuer bidd' ick eenen <rhyme label="a" type="m" corresp="#s6:l1">man</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Die u <rhyme label="b" type="m" corresp="#s6:l2">bemint</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ghy moeght die quaey <rhyme label="c" type="f">ghedachten</rhyme></l>
        <l id="s6:l6" met="1" type="-+">Niet <rhyme label="c" type="f" corresp="#s6:l5">achten</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Ghebruyckt u <rhyme label="d" type="m">jeucht</rhyme></l>
        <l id="s6:l8" met="2" type="-+">In eer, en <rhyme label="d" type="m" corresp="#s6:l7">deucht</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Wilt niet de hoeren <rhyme label="c" type="f" corresp="#s6:l5">slachten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
