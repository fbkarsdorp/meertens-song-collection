<song>
  <meta>
    <id>159366</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Rozemond die lag gedoken</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">JAël daghte uyt te <rhyme label="a" type="f">roeyen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Sissera, tot haer <rhyme label="b" type="m">gekeerdt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Noch gaf zy hem 't nat van <rhyme label="a" type="f" corresp="#s1:l1">Koeyen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Doe hy 't Beke-voght <rhyme label="b" type="m" corresp="#s1:l2">begeerd</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Toond hem gunst in alle <rhyme label="c" type="f">dingh</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Tot de Slaep sijn Oogh <rhyme label="c" type="f" corresp="#s1:l5">bevingh</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maer doe hy sliep in haer <rhyme label="a" type="f">Kamer</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ende vreesde nergens <rhyme label="b" type="m">voor</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Nam z' een Nagel, en een <rhyme label="a" type="f" corresp="#s2:l1">Hamer</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Klopte daer sijn Breyn mee <rhyme label="b" type="m" corresp="#s2:l2">door</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daer op kromd hy eens sijn <rhyme label="c" type="f">Leên</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En sijn Ziele vloogh daer <rhyme label="c" type="f" corresp="#s2:l5">heen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">In de Rol die Jaël <rhyme label="a" type="f">speelde</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Met haer vyant <rhyme label="b" type="m">Sissera</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Siet men 't doen van d' yd'le <rhyme label="a" type="f" corresp="#s3:l1">weelde</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Want die bootst die handel <rhyme label="b" type="m" corresp="#s3:l2">na</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">t'Wijl door 't Ziel-verleydb're <rhyme label="c" type="f">soet</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Sy doet slapen het <rhyme label="c" type="f" corresp="#s3:l5">gemoet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer in 't dart'le sondigh <rhyme label="a" type="f">mallen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Onbekommert, <rhyme label="b" type="m">sorgeloos</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wordt de Mensche <rhyme label="a" type="f" corresp="#s4:l1">overvallen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Van't verderf, dat loos en <rhyme label="b" type="m" corresp="#s4:l2">boos</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Doet den dwasen mensch seer <rhyme label="c" type="f">snel</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Neder-storten in de <rhyme label="c" type="f" corresp="#s4:l5">Hel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
