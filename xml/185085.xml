<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185085</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom laat ons vrolyk <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O Haarlem’s <rhyme label="b" type="m">Schuttery</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Door tyds – <rhyme label="a" type="f" corresp="#s1:l1">omwentelingen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Zo mogen wy weer <rhyme label="b" type="m" corresp="#s1:l2">bly</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Bevryd van dwangzucht, <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ons wierd, tot onze <rhyme label="d" type="m">vreugd</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">De wapens weer <rhyme label="c" type="f" corresp="#s1:l5">gegeeven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Kom, zingen wy <rhyme label="d" type="m" corresp="#s1:l6">verheugd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ons wierden de <rhyme label="a" type="f">Geweeren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ontnomen ’t voor’ge <rhyme label="b" type="m">jaar</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Men zogt ons te <rhyme label="a" type="f" corresp="#s2:l1">overheeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wy zaten in <rhyme label="b" type="m" corresp="#s2:l2">gevaar</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">In druk en <rhyme label="c" type="f">tegenheden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Om dat wy onzen <rhyme label="d" type="m">Eed</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Niet wilden <rhyme label="c" type="f" corresp="#s2:l5">overtreeden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Gelyk een ander <rhyme label="d" type="m" corresp="#s2:l6">deed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Om dat wy nimmer <rhyme label="a" type="f">smaaden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En scholden nassau’s <rhyme label="b" type="m">Vorst</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die, lang met smart <rhyme label="a" type="f" corresp="#s3:l1">belaaden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De laster heeft <rhyme label="b" type="m" corresp="#s3:l2">getorst</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maar hem oprecht <rhyme label="c" type="f">beminden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ook toen hy wierd <rhyme label="d" type="m">ontëerd</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Toen wierden wy, zyn <rhyme label="c" type="f" corresp="#s3:l5">Vrinden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In Haarlem ook <rhyme label="d" type="m" corresp="#s3:l6">verneêrd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maar nu, uit <rhyme label="a" type="f">tegenheden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die Vorst, in <rhyme label="b" type="m">Nederland</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gered is, op <rhyme label="a" type="f" corresp="#s4:l1">gebeden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Der Vroomen, door Gods <rhyme label="b" type="m" corresp="#s4:l2">Hand</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Nu hy in vreugd mag <rhyme label="c" type="f">leeven</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Met zyn Doorluchtig <rhyme label="d" type="m">Huis</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Door God, Die heil wou <rhyme label="c" type="f" corresp="#s4:l5">geeven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zyn wy ook vry van <rhyme label="d" type="m" corresp="#s4:l6">kruis</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu hy weer is <rhyme label="a" type="f">verheeven</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In Eer, in <rhyme label="b" type="m">Nederland</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Nu zyne Haaters <rhyme label="a" type="f" corresp="#s5:l1">beeven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En leggen aan een <rhyme label="b" type="m" corresp="#s5:l2">band</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ny wy ons zien <rhyme label="c" type="f">regeeren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Door Braaven, als wel <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Zo juichen wy ter <rhyme label="c" type="f" corresp="#s5:l5">eeren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Van Neêrlands God en <rhyme label="d" type="m" corresp="#s5:l6">Heer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nu is ’t Oranje <rhyme label="a" type="f">Boven</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Nu zwygt de Oproer – <rhyme label="b" type="m">kreet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De dwangzucht is <rhyme label="a" type="f" corresp="#s6:l1">verstooven</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wy zyn bevryd van <rhyme label="b" type="m" corresp="#s6:l2">leed</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Nu mogen wy weer <rhyme label="c" type="f">leeven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Verheugd, in <rhyme label="d" type="m">Nederland</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Nu word ons weer <rhyme label="c" type="f" corresp="#s6:l5">gegeeven</rhyme></l>
        <l id="s6:l8" met="3" type="-+">De wapens in de <rhyme label="d" type="m" corresp="#s6:l6">hand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die zullen wy <rhyme label="a" type="f">gebruiken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Om, volgens plicht en <rhyme label="b" type="m">eed</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Het snood geweld te <rhyme label="a" type="f" corresp="#s7:l1">fnuiken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wy zyn te zaam <rhyme label="b" type="m" corresp="#s7:l2">gereed</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Om voor de rust te <rhyme label="c" type="f">waaken</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Der Stad, by dag en <rhyme label="d" type="m">nacht</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Terwyl wy vreede <rhyme label="c" type="f" corresp="#s7:l5">smaaken</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En vreugde op de <rhyme label="d" type="m" corresp="#s7:l6">Wacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
