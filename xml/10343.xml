      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Het vinnige stralen van de zon</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Het vinnigh stralen van de <rhyme label="a" type="m">Son</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ontschuyl ick in <rhyme label="b" type="f">Bosschagie</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Indien dit Bosjen klappen <rhyme label="a" type="m" corresp="#s1:l1">kon</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wat meldet al <rhyme label="b" type="f" corresp="#s1:l2">vryagie</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vryagie Neen, vryagie <rhyme label="a" type="m">Ia</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Vryagie sonder <rhyme label="b" type="f">meenen</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Van Hondert Harders (ist niet <rhyme label="a" type="m" corresp="#s2:l1">scha</rhyme>)</l>
        <l id="s2:l4" met="3" type="-+">Vintmer getrou niet <rhyme label="b" type="f" corresp="#s2:l2">eenen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een wullips knaepjen altijt <rhyme label="a" type="m">stuurt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Na nieuwe lust sijn <rhyme label="b" type="f">sinnen</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Niet langher dan het weygeren <rhyme label="a" type="m" corresp="#s3:l1">duurt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Niet langher duurt het <rhyme label="b" type="f" corresp="#s3:l2">minnen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Mijn hartje treckt my wel soo <rhyme label="a" type="m">seer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Soo seer derfd'ick het <rhyme label="b" type="f">waghen</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Maer neen ick waegh het <rhyme label="a" type="m" corresp="#s4:l1">nimmermeer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hun minnen zijn maer <rhyme label="b" type="f" corresp="#s4:l2">vlagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer vlagen die t'hans over <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En op een ander <rhyme label="b" type="f">vallen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Nochtans ick sie mijn vryer <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Voor d'trouwste van hun <rhyme label="b" type="f" corresp="#s5:l2">allen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maer oft u miste domme <rhyme label="a" type="m">Maecht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ghy siet hem niet van <rhyme label="b" type="f">binnen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Dan 't schijnt wel die gheen rust en <rhyme label="a" type="m" corresp="#s6:l1">waeght</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Kan qualijck lust <rhyme label="b" type="f" corresp="#s6:l2">gewinnen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Oft ick hem oock lichtvaerdigh <rhyme label="a" type="m">von</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En bleef in dit <rhyme label="b" type="f">Bosschagie</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Indien dit Bosjen klappen <rhyme label="a" type="m" corresp="#s7:l1">kon</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wal meldet al <rhyme label="b" type="f" corresp="#s7:l2">boelagie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
