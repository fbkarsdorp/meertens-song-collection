<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>117187</id>
    <date>
      <year-start>1717</year-start>
      <year-end>1717</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="afscheidslied" translation="farewell song">afscheidslied</tag>
      <tag standard-name="soldatenlied" translation="soldier's song">soldatenlied</tag>
    </tags>
    <melody>Wel Mieke stout</melody>
    <categories>
      <category translation="occasions">gelegenheden</category>
      <category translation="work">werk</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WEest nu <rhyme label="a" type="m">verblijt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Soldaten van <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Den Koninck van <rhyme label="b" type="f" corresp="#s1:l2">Brittanje</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Maeckt hem weer <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat gy sult trecken <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Na de Revu hoort <rhyme label="d" type="m" corresp="#s1:l5">aen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Al op de <rhyme label="e" type="f">Moocker-Heyde</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Sult op kampreren <rhyme label="d" type="m" corresp="#s1:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hebt gy een <rhyme label="a" type="m">Lief</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Minnaers wilt haer <rhyme label="b" type="f">vertoosten</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat sy haer u an <rhyme label="b" type="f" corresp="#s2:l2">verpoosten</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En dat tot <rhyme label="c" type="m">wedersiens</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Al gade gy van <rhyme label="d" type="m">kant</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gy treckt niet uyt het <rhyme label="d" type="m" corresp="#s2:l5">lant</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gy Meysjes als de <rhyme label="e" type="f">vroomen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Hout u maer <rhyme label="d" type="m" corresp="#s2:l5">valjant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Als wy daer <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">So sullen wy u <rhyme label="b" type="f">schrijven</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hoe lang dat wy daer <rhyme label="b" type="f" corresp="#s3:l2">blyven</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Alderliefste <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Schoon kindt hout goede <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wy sullen metter <rhyme label="d" type="m" corresp="#s3:l5">spoet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In kort hier weder <rhyme label="e" type="f">komen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Hoorje Meysjes <rhyme label="d" type="m" corresp="#s3:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Men siet van alle <rhyme label="a" type="m">kant</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Veel Ruyters en <rhyme label="b" type="f">Soldaten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met duytse en <rhyme label="b" type="f" corresp="#s4:l2">Krawaten</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Komen heel <rhyme label="c" type="m">valjant</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die gaen na de <rhyme label="d" type="m">Revu</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Seggen haer Lief <rhyme label="d" type="m" corresp="#s4:l5">adieu</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Schoon Lief laet ons eens <rhyme label="e" type="f">drincken</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">De gesontheyt <rhyme label="d" type="m" corresp="#s4:l5">nu</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Men siet <rhyme label="a" type="m">voorwaer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De neering daer <rhyme label="b" type="f">bedrijven</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Marcke-Tenters met haer <rhyme label="b" type="f" corresp="#s5:l2">wijven</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Staen te samen <rhyme label="c" type="m">klaer</rhyme> ,</l>
        <l id="s5:l5" met="3" type="-+">Met Brandewijn en <rhyme label="d" type="m">Taeback</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ienever en <rhyme label="d" type="m" corresp="#s5:l5">Krack</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Daer mee siet mense <rhyme label="e" type="f">loopen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Soo den heelen <rhyme label="d" type="m" corresp="#s5:l5">dag</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Men speelt op de <rhyme label="a" type="m">Trom</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Velt-Fluyten en <rhyme label="b" type="f">Schermeijen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat men daer gaet <rhyme label="b" type="f" corresp="#s6:l2">verbeijden</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">En op de <rhyme label="c" type="m">Bason</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Wie sou niet zijn <rhyme label="d" type="m">verheugt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Over de groote <rhyme label="d" type="m" corresp="#s6:l5">vreugt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Noyt meer siet men den <rhyme label="e" type="f">Koning</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Daer vol eer en <rhyme label="d" type="m" corresp="#s6:l5">deugt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Gy Meysjes <rhyme label="a" type="m">fier</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Voort laest laet ons eens <rhyme label="b" type="f">zwieren</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Gelijck de Venus <rhyme label="b" type="f" corresp="#s7:l2">dieren</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Nemen ons <rhyme label="c" type="m">pleysier</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">De vreught hebt gy <rhyme label="d" type="m">gehadt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Gy hebt geproeft van <rhyme label="d" type="m" corresp="#s7:l5">dat</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Lustig hebt gy <rhyme label="e" type="f">gegeten</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">De Soldaten <rhyme label="d" type="m" corresp="#s7:l5">Pap</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Adieu <rhyme label="a" type="m">Annemie</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Margrietje en <rhyme label="b" type="f">Pierijntje</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Antonet en <rhyme label="b" type="f" corresp="#s8:l2">Franzijntje</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Hout het maer voor <rhyme label="c" type="m">lief</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Neemt maer courasie <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Liesabeth en <rhyme label="d" type="m" corresp="#s8:l5">Anna</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Wy sullen haest weer <rhyme label="e" type="f">komen</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Tot ons <rhyme label="d" type="m" corresp="#s8:l5">vermaeck</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Als wy hier <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Schoon lief ick sal u <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Bruyloft en Kraem-mael <rhyme label="b" type="f" corresp="#s9:l2">houwen</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Weest dan maer <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Wy sullen vrolijck <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Met Bier en <rhyme label="d" type="m" corresp="#s9:l5">Brandewijn</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Meysjes neemt goe <rhyme label="e" type="f">courasie</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">Met u Kintje <rhyme label="d" type="m" corresp="#s9:l5">kleyn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Adieu Berg-op <rhyme label="a" type="m">Zoom</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Breda en Bos <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">De Graef daer toe <rhyme label="b" type="f" corresp="#s10:l2">Nimwegen</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Arem tot sijn <rhyme label="c" type="m">loon</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Adieu gy meysies <rhyme label="d" type="m">jent</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Wy krijgen ons <rhyme label="d" type="m" corresp="#s10:l5">patent</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Na de Revu te <rhyme label="e" type="f">recken</rhyme>,</l>
        <l id="s10:l8" met="3" type="+-">Op het <rhyme label="d" type="m" corresp="#s10:l5">Campement</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
