  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIe   lieue coele mey die is ons <rhyme label="a" type="m">ondaen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">ic sie dat cruce soe hoeghe <rhyme label="a" type="m" corresp="#s1:l1">staen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daer an soe hanghet een <rhyme label="a" type="m">iongelinck</rhyme></l>
        <l id="s2:l2" met="4" type="-+">daer onder steyt  die lieue moeder <rhyme label="a" type="m" corresp="#s2:l1">syn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Vyf rosen staen daer aenden <rhyme label="a" type="m">mey</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ende ander bloemkens mennygher <rhyme label="a" type="m" corresp="#s3:l1">ley</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O edele ziele merket vromeliche <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">hoe suetelicke singhet die wylde nacht <rhyme label="a" type="m" corresp="#s4:l1">gael</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Als aenden cruce daer hi aen <rhyme label="a" type="m">stont</rhyme></l>
        <l id="s5:l2" met="4" type="-+">wt lieften was syn herte <rhyme label="a" type="m" corresp="#s5:l1">ghewont</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ende heeft daer yemant hongher of <rhyme label="a" type="m">doerst</rhyme></l>
        <l id="s6:l2" met="4" type="-+">die come ende drynck wt ihus   <rhyme label="a" type="m" corresp="#s6:l1">boerst</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Myn herten bloet wyl ic hem <rhyme label="a" type="m">schenken</rhyme></l>
        <l id="s7:l2" met="4" type="-+">O mensche wilt op mynen doet <rhyme label="a" type="m" corresp="#s7:l1">gedencken</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ende laetens w niet wtten uwen herten <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">O edele siele ome  w ist al <rhyme label="a" type="m" corresp="#s8:l1">ghedaen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
