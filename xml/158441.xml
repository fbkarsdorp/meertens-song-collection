    <tags/>
    <melody>Psalm 091 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy jonghe Ieuchden <rhyme label="a" type="m">altesaem</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tot Munckedam <rhyme label="b" type="f">woonachtich</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die moet ick nu al naet <rhyme label="a" type="m" corresp="#s1:l1">betaem</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met een Liet sijn <rhyme label="b" type="f" corresp="#s1:l2">ghedachtigh</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Al is certeyn // mijn wijsheyt <rhyme label="c" type="m">kleyn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Tgheschiet uyt Liefden <rhyme label="d" type="f">reyne</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Wilt het altsaem // in danck <rhyme label="e" type="m">ontfaen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dat bidt ick u <rhyme label="d" type="f" corresp="#s1:l6">ghemeyne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al sijt ghy noch seer <rhyme label="a" type="m">jeuchdelijck</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van Iaren niet seer <rhyme label="b" type="f">oude</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dient doch den Heere <rhyme label="a" type="m" corresp="#s2:l1">vierichlijck</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Laet liefde niet <rhyme label="b" type="f" corresp="#s2:l2">vercouden</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Als kinder Godts heylich <rhyme label="c" type="m">gheaert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Bewijst u taller <rhyme label="d" type="f">uren</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Door Godes Gheest weder <rhyme label="e" type="m">ghebaert</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Tot nieuwe <rhyme label="d" type="f" corresp="#s2:l6">Creatueren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wilt doch altijdt ghedachtich <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wat ginck uyt uwen <rhyme label="b" type="f">monden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Doen ghy u hebt vereenicht <rhyme label="a" type="m" corresp="#s3:l1">fijn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hoe ghy al tot dien <rhyme label="b" type="f" corresp="#s3:l2">stonden</rhyme></l>
        <l id="s3:l5" met="4" type="-+">V selven ganslijck hebt <rhyme label="c" type="m">versaeckt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Sijt goetwillich <rhyme label="d" type="f">ghetreden</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Met Christo een verbont <rhyme label="e" type="m">ghemaeckt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Te leven na sijn <rhyme label="d" type="f" corresp="#s3:l6">seden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Mijn lieve kinders hebt ghy <rhyme label="a" type="m">nu</rhyme></l>
        <l id="s4:l2" met="3" type="-+">V totten Heer <rhyme label="b" type="f">begheven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dient hem doch nu, met herten <rhyme label="a" type="m" corresp="#s4:l1">trou</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Soo sal hy u <rhyme label="b" type="f" corresp="#s4:l2">verheven</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Des levens Croon, gheven ten <rhyme label="c" type="m">loon</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En ghy sult <rhyme label="d" type="f">tryumpheren</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">In eeuwicheyt, soo die Schrift <rhyme label="e" type="m">seyt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Met hem <rhyme label="d" type="f" corresp="#s4:l6">verjubyleren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hebt ghy Christum nu <rhyme label="a" type="m">aenghedaen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En sijt met hem <rhyme label="b" type="f">verresen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wilt int nieuwe leven voort <rhyme label="a" type="m" corresp="#s5:l1">gaen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Legt af als nu in <rhyme label="b" type="f" corresp="#s5:l2">desen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Den ouden mensch met lust en <rhyme label="c" type="m">wens</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En s'werelts <rhyme label="d" type="f">ydelheden</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En dient den heer, van herten <rhyme label="e" type="m">seer</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Soo comt u Ziel in <rhyme label="d" type="f" corresp="#s5:l6">vreden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sijt ghy s'werelts <rhyme label="a" type="m">onsuyverheyt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Door Godts kennis <rhyme label="b" type="f">ontvloden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En hebt die werelt <rhyme label="a" type="m" corresp="#s6:l1">afgheseyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Met al haer lusten <rhyme label="b" type="f" corresp="#s6:l2">snoden</rhyme></l>
        <l id="s6:l5" met="4" type="-+">En wilt nu doch, niet mette <rhyme label="c" type="m">soch</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">V weder onreyn <rhyme label="d" type="f">maecken</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Of ghy sout claer, moeten hier <rhyme label="e" type="m">naer</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Die Helsche pijne <rhyme label="d" type="f" corresp="#s6:l6">smaecken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Mijn alderliefste inden <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hebt ghy nu <rhyme label="b" type="f">aenghetoghen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dat reyne kleet tot Godes <rhyme label="a" type="m" corresp="#s7:l1">eer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wilt u nu doch neerstich <rhyme label="b" type="f" corresp="#s7:l2">poghen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Om dat eenpaer, te houden <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Voor des werelts <rhyme label="d" type="f">besmetten</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Op dat ghy niet, comt int <rhyme label="e" type="m">verdriet</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wilt daer neerstich op <rhyme label="d" type="f" corresp="#s7:l6">letten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Hier meedt beveel ick u den <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En twoort sijnder <rhyme label="b" type="f">ghenaden</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Blijft doch ghesont in Christi <rhyme label="a" type="m" corresp="#s8:l1">leer</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Wilt u daer met <rhyme label="b" type="f" corresp="#s8:l2">beraden</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">O Ionghe Ieucht, hebt doch u <rhyme label="c" type="m">vreucht</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">In Godes Wet <rhyme label="d" type="f">alleyne</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Dat ghy hier naer, meucht comen <rhyme label="e" type="m">claer</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">In Godts Rijck <rhyme label="d" type="f" corresp="#s8:l6">alghemeyne</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wilt my altijdt ghedachtich <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s9:l2" met="3" type="-+">In u ghebedt <rhyme label="b" type="f">altsaemen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat bidt ick u o Ieuchde <rhyme label="a" type="m" corresp="#s9:l1">fijn</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En wilt oock naet <rhyme label="b" type="f" corresp="#s9:l2">betamen</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Dit simpel sanck, nemen in <rhyme label="c" type="m">danck</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Ende ons <rhyme label="d" type="f">altesaemen</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Wat schencken weer, dats mijn <rhyme label="e" type="m">begheer</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Sijt Godt bevolen <rhyme label="d" type="f" corresp="#s9:l6">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
