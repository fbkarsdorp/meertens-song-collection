  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Bemerckt nu <rhyme label="a" type="m">vergaet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">des nachts duijsterheijt <rhyme label="b" type="m">wreet</rhyme></l>
        <l id="s1:l3" met="2" type="-+">de morgenstond <rhyme label="a" type="m" corresp="#s1:l1">slaet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">met schijn roet veruich <rhyme label="b" type="m" corresp="#s1:l2">breet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">laet ons metter <rhyme label="a" type="m" corresp="#s1:l1">daet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">den heer vergift van <rhyme label="b" type="m" corresp="#s1:l2">leet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">bidden, vroech <rhyme label="b" type="m" corresp="#s1:l2">bereet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Dat godt <rhyme label="a" type="m">almachtich</rhyme></l>
        <l id="s2:l2" met="3" type="-+">door sijn <rhyme label="b" type="m">bermharticheijt</rhyme></l>
        <l id="s2:l3" met="2" type="-+">verdrijft sieckt <rhyme label="a" type="m" corresp="#s2:l1">crachtich</rhyme></l>
        <l id="s2:l4" met="3" type="-+">en geeft <rhyme label="b" type="m" corresp="#s2:l2">volherdicheijt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">gunt oeck <rhyme label="a" type="m" corresp="#s2:l1">warachticheijt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">d'ewijge <rhyme label="b" type="m" corresp="#s2:l2">vrolickheijt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">doer vaders <rhyme label="b" type="m" corresp="#s2:l2">goetheijt</rhyme></l>
      </lg>
    </lg>
