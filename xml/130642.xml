<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>130642</id>
    <date>
      <year-start>1710</year-start>
      <year-end>1710</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>May song</tag>
    </tags>
    <melody>Mijn schone herderin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">De Mey, den soeten <rhyme label="a" type="m">tijd</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Baart vreugd en <rhyme label="a" type="m" corresp="#s1:l1">vrolijkheid</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Waar door een yders herd <rhyme label="a" type="m" corresp="#s1:l1">verblyd</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">De Boomen, Gras en <rhyme label="b" type="m">Kruit</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En veelderhande <rhyme label="b" type="m" corresp="#s1:l4">Fruit</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Seer lieffelijk uit der aarden <rhyme label="b" type="m" corresp="#s1:l4">spruit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Mey brengt alles <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat oog en hert <rhyme label="a" type="m" corresp="#s2:l1">bekoort</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Al wat tot 's menschen voedzel <rhyme label="a" type="m" corresp="#s2:l1">hoord</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Het veld met dauw <rhyme label="b" type="m">besproeit</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Daar gras en kruid af <rhyme label="b" type="m" corresp="#s2:l4">groeit</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Waar door dat melk en honing <rhyme label="b" type="m" corresp="#s2:l4">vloeid</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Al dat men Flora <rhyme label="a" type="m">noem</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van meenig schoone <rhyme label="a" type="m" corresp="#s3:l1">Bloem</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Krijgt in de Mey zijn geur en <rhyme label="a" type="m" corresp="#s3:l1">roem</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Het Pluim gedierten <rhyme label="b" type="m">queeld</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Het Wollig-Vee dat <rhyme label="b" type="m" corresp="#s3:l4">speeld</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Yder met vreugd sijn vrugten <rhyme label="b" type="m" corresp="#s3:l4">teeld</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daar ziet men <rhyme label="a" type="m">Zilvia</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Al met haar Schaapjes <rhyme label="a" type="m" corresp="#s4:l1">dra</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Philander volgt haar agter <rhyme label="a" type="m" corresp="#s4:l1">na</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Gins ziet men <rhyme label="b" type="m">Coridon</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Aan Beek of koele <rhyme label="b" type="m" corresp="#s4:l4">Bron</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Al met zyn soete <rhyme label="b" type="m" corresp="#s4:l4">Philemon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Leander ziet men <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Al met zyn <rhyme label="a" type="m" corresp="#s5:l1">Galathee</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hoe dat sy saam haar wollig <rhyme label="a" type="m" corresp="#s5:l1">Vee</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Gaan drijven uit de <rhyme label="b" type="m">Stal</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Door Wey en jeugdig <rhyme label="b" type="m" corresp="#s5:l4">dal</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Tot aan de frisse <rhyme label="b" type="m" corresp="#s5:l4">water-val</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Soo meenig schoone <rhyme label="a" type="m">dant</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ziet men met haar <rhyme label="a" type="m" corresp="#s6:l1">galant</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Gaan wandelen door 't bedoude <rhyme label="a" type="m" corresp="#s6:l1">land</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Om in de Mey <rhyme label="b" type="m">zaizoen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Te plukken 't jeugdig <rhyme label="b" type="m" corresp="#s6:l4">groen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Smorgens vroeg al voor den <rhyme label="b" type="m" corresp="#s6:l4">noen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Al wat 'er leeft op <rhyme label="a" type="m">aard</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hem in de Mey <rhyme label="a" type="m" corresp="#s7:l1">vergaard</rhyme></l>
        <l id="s7:l3" met="4" type="-+">En yder soekt te zyn <rhyme label="a" type="m" corresp="#s7:l1">gepaard</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">De Vissen in de <rhyme label="b" type="m">vloed</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Gevoelen mee die <rhyme label="b" type="m" corresp="#s7:l4">gloed</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Het geen haar Zaat vermeeren <rhyme label="b" type="m" corresp="#s7:l4">doet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Soo dra den nagt <rhyme label="a" type="m">verdwijnt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Apollos glans <rhyme label="a" type="m" corresp="#s8:l1">verschijnt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ziet men het veld met dauw <rhyme label="a" type="m" corresp="#s8:l1">omheint</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Het Nagtegaaltje <rhyme label="b" type="m">tiert</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Het Vinkje <rhyme label="b" type="m" corresp="#s8:l4">tireliert</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Al 't gevogelt vliegt en <rhyme label="b" type="m" corresp="#s8:l4">zwiert</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Den Leeuwerik vliegt om <rhyme label="a" type="m">hoog</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Soo ver tot uit het <rhyme label="a" type="m" corresp="#s9:l1">oog</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Looft daar zyn Schepper na <rhyme label="a" type="m" corresp="#s9:l1">vermoog</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">De wey en 't vrugbaar <rhyme label="b" type="m">Lof</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Kruit, Bloemen, veel van <rhyme label="b" type="m" corresp="#s9:l4">stof</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Die geven haren Schepper <rhyme label="b" type="m" corresp="#s9:l4">Lof</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De Boer zyn Haantje <rhyme label="a" type="m">kraeit</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Het Somerlugje <rhyme label="a" type="m" corresp="#s10:l1">waait</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">'t Is al vreugden waar men <rhyme label="a" type="m" corresp="#s10:l1">draait</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Laat ons met <rhyme label="b" type="m">soetigheid</rhyme></l>
        <l id="s10:l5" met="3" type="-+">In dese lenten <rhyme label="b" type="m" corresp="#s10:l4">tyd</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Loven den <rhyme label="b" type="m" corresp="#s10:l4">Opper-Majesteit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
