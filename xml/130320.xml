      <year-start>1710</year-start>
      <year-end>1710</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Écoutez bergers la musique angélique</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ag schoone <rhyme label="a" type="m">Maagt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Uw lieve <rhyme label="b" type="f">lonken</rhyme></l>
        <l id="s1:l3" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s1:l2">Ontvonken</rhyme>
        </l>
        <l id="s1:l4" met="3" type="-+">Mijn hert, wel eer <rhyme label="a" type="m" corresp="#s1:l1">behaagt</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Gy stelt mijn <rhyme label="c" type="m">min</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Gants uit uw <rhyme label="c" type="m" corresp="#s1:l5">sin</rhyme></l>
        <l id="s1:l7" met="4" type="-+">En wilt maar   met uw schoonheit <rhyme label="b" type="f" corresp="#s1:l2">pronken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Laat af, laat <rhyme label="a" type="m">af</rhyme>!</l>
        <l id="s2:l2" met="2" type="-+">Ik kan uw <rhyme label="b" type="f">plagen</rhyme></l>
        <l id="s2:l3" met="1" type="-+">Niet <rhyme label="b" type="f" corresp="#s2:l2">dragen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En moet in het <rhyme label="a" type="m" corresp="#s2:l1">graf</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Komt, <rhyme label="c" type="m">Engelin</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Gun myn uw <rhyme label="c" type="m" corresp="#s2:l5">min</rhyme></l>
        <l id="s2:l7" met="4" type="-+">En maakt een eynde van myn <rhyme label="b" type="f" corresp="#s2:l2">klagen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ey, haast u <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Mijn tweede <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s3:l3" met="1" type="-+">Ik sal uw <rhyme label="b" type="f" corresp="#s3:l2">geeven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Meer als je denken <rhyme label="a" type="m" corresp="#s3:l1">kan</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Daar op <rhyme label="c" type="m">gerust</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Nog eens <rhyme label="c" type="m" corresp="#s3:l5">gekust</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Wijl al de smert dan is <rhyme label="b" type="f" corresp="#s3:l2">verdreven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ag <rhyme label="a" type="m">Clorimeen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Wilt gy my <rhyme label="b" type="f">moorden</rhyme></l>
        <l id="s4:l3" met="1" type="-+">Met <rhyme label="b" type="f" corresp="#s4:l2">woorden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En seggen Eeuwig: <rhyme label="a" type="m" corresp="#s4:l1">‘Neen</rhyme>’?</l>
        <l id="s4:l5" met="2" type="-+">Ik kan uw <rhyme label="c" type="m">hart</rhyme></l>
        <l id="s4:l6" met="2" type="-+">En gy mijn <rhyme label="c" type="m" corresp="#s4:l5">smert</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Ey, dat ik eens het einde <rhyme label="b" type="f" corresp="#s4:l2">hoorde</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ik wens gy wierd <rhyme label="a" type="m">dra</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Met stralende <rhyme label="b" type="f">oogen</rhyme></l>
        <l id="s5:l3" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s5:l2">Bewogen</rhyme>
        </l>
        <l id="s5:l4" met="3" type="-+">En gaf het lieve <rhyme label="a" type="m" corresp="#s5:l1">ja</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Dan kwam myn <rhyme label="c" type="m">druk</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Tot groot <rhyme label="c" type="m" corresp="#s5:l5">geluk</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Gebruik dan haast uw <rhyme label="b" type="f" corresp="#s5:l2">mededogen</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
