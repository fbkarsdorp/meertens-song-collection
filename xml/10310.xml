      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
    </tags>
    <melody>Phyllis kwam zich buigen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Galathe waer <rhyme label="a" type="f">heenen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Soo vroegh op de <rhyme label="a" type="f" corresp="#s1:l1">beene</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Siet g'uw Tijter <rhyme label="b" type="m">niet</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">'t Schijnt voorwaer ghy voor hem <rhyme label="b" type="m" corresp="#s1:l3">vliet</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Wilt u dienaer niet <rhyme label="c" type="f">bedroeven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Want ghy hebt de reden <rhyme label="b" type="m" corresp="#s1:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Tijter 'k heb voor <rhyme label="a" type="f">desen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">V wel eer <rhyme label="a" type="f" corresp="#s2:l1">gepresen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Maer dat 's nu <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Sedert heb ick yets <rhyme label="b" type="m" corresp="#s2:l3">verstaen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Oorsaeck om voor u te <rhyme label="c" type="f">vresen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">En u vry te laten <rhyme label="b" type="m" corresp="#s2:l3">gaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Galathe ick <rhyme label="a" type="f">hoorde</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Noyt soo straffe <rhyme label="a" type="f" corresp="#s3:l1">woorde</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Wt u soete <rhyme label="b" type="m">mont</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">(Wist ghy hoe ghy Tijter <rhyme label="b" type="m" corresp="#s3:l3">wont</rhyme>)</l>
        <l id="s3:l5" met="4" type="+-">Stelt doch sulcx aen een <rhyme label="c" type="f">zijde</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Of sijn zieltje gaet te <rhyme label="b" type="m" corresp="#s3:l3">gront</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Tijter 'k moet gaen <rhyme label="a" type="f">weyden</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">'k Mach niet langer <rhyme label="a" type="f" corresp="#s4:l1">beyden</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Houd u vry <rhyme label="b" type="m">gerust</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Galathe doch eens <rhyme label="b" type="m" corresp="#s4:l3">gekust</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Laet u Tijter dan <rhyme label="c" type="f">geleyden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En stelt oock sijn hert <rhyme label="b" type="m" corresp="#s4:l3">gerust</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Ach wat soet <rhyme label="a" type="f">vermaecke</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Vond ick op de <rhyme label="a" type="f" corresp="#s5:l1">kaecken</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Van mijn <rhyme label="b" type="m">Galathe</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ey neemt doch u Tijter <rhyme label="b" type="m" corresp="#s5:l3">me</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Hy sal u maer heusch <rhyme label="c" type="f">genaecke</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En ghy stelt sijn hert te <rhyme label="b" type="m" corresp="#s5:l3">vree</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Al mijn Vee en <rhyme label="a" type="f">Landen</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Stelt hy in u <rhyme label="a" type="f" corresp="#s6:l1">handen</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">En al dat hy <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Oock daer by sijn ziel u <rhyme label="b" type="m" corresp="#s6:l3">geeft</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Hoe soud ghy dan konnen <rhyme label="c" type="f">seggen</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Dat u Tyter ontrouw <rhyme label="b" type="m" corresp="#s6:l3">leeft</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Tyter 't zijn maer <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Dien ick van u <rhyme label="a" type="f" corresp="#s7:l1">hoorden</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Doch ick achtse <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Wijl mijn oogh nu beter <rhyme label="b" type="m" corresp="#s7:l3">siet</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Stelt u nu dan vry <rhyme label="c" type="f">geruster</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Want ghy hebt alrees 't <rhyme label="b" type="m" corresp="#s7:l3">gebiedt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Lof zy dan de <rhyme label="a" type="f">Goden</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Die u quamen <rhyme label="a" type="f" corresp="#s8:l1">nooden</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Tot mijn med' <rhyme label="b" type="m">gesel</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Langer neem ick geen <rhyme label="b" type="m" corresp="#s8:l3">uytstel</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Als tot den <rhyme label="c" type="f">dageraet</rhyme></l>
        <l id="s8:l6" met="4" type="+-">My by u weer komen <rhyme label="b" type="m" corresp="#s8:l3">laet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
