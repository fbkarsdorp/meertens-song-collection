  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Myn vreugd is myn <rhyme label="a" type="f">ontnomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik Sterf van <rhyme label="b" type="m">ongeduld</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Zal myn Annet niet <rhyme label="a" type="f" corresp="#s1:l1">komen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Het nurtje is <rhyme label="b" type="m" corresp="#s1:l2">vervuld</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ik word gedreven door de <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Gy speelt gestadig in myn <rhyme label="c" type="m" corresp="#s1:l5">zin</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wie heeft dan zulks <rhyme label="d" type="m">gedaan</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f">liefde</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Heeft my zo ver <rhyme label="f" type="m">gebragt</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f" corresp="#s1:l8">liefde</rhyme>,</l>
        <l id="s1:l11" met="3" type="-+">Brengt my tot in het <rhyme label="f" type="m" corresp="#s1:l9">graf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat mag Annette <rhyme label="a" type="f">deren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik wagt met groote <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Daar zal wat aan <rhyme label="a" type="f" corresp="#s2:l1">mankeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat voel ik aan myn <rhyme label="b" type="m" corresp="#s2:l2">hert</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ik wandel lang de klaare <rhyme label="c" type="m">vliet</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Maar ag ik vind Annette <rhyme label="c" type="m" corresp="#s2:l5">niet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wie heeft dan zulks <rhyme label="d" type="m">gedaan</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f">liefde</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Heeft myn zo ver <rhyme label="f" type="m">gebragt</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f" corresp="#s2:l8">liefde</rhyme>,</l>
        <l id="s2:l11" met="3" type="-+">Brengt myn tot in het <rhyme label="f" type="m" corresp="#s2:l9">graf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zou zy myn ook <rhyme label="a" type="f">verachten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">O ongelukkig <rhyme label="b" type="m">lot</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat ik hier zo moet <rhyme label="a" type="f" corresp="#s3:l1">wagten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Steken met myn de <rhyme label="b" type="m" corresp="#s3:l2">spot</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ik word gedreven heen en <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Ik zoek de rust en vind niet <rhyme label="c" type="m" corresp="#s3:l5">meer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wie heeft dan zulks <rhyme label="d" type="m">gedaan</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f">liefde</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Heeft myn zo ver <rhyme label="f" type="m">gebragt</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f" corresp="#s3:l8">liefde</rhyme>,</l>
        <l id="s3:l11" met="3" type="-+">Brengt myn tot in 't <rhyme label="f" type="m" corresp="#s3:l9">graf</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Myn dunkt ik zie haar <rhyme label="a" type="f">komen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Al door de groene <rhyme label="b" type="m">laan</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Daar ginsen in de <rhyme label="a" type="f" corresp="#s4:l1">Bomen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Annette komt daar <rhyme label="b" type="m" corresp="#s4:l2">aan</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Het vrolyk uur zo lang <rhyme label="c" type="m">verwagt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Annette die myn druk <rhyme label="c" type="m" corresp="#s4:l5">verzagt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wie heeft dan zulks <rhyme label="d" type="m">gedaan</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f">liefde</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Heeft myn door min <rhyme label="f" type="m">gekwelt</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">De liefde, ag de <rhyme label="e" type="f" corresp="#s4:l8">liefde</rhyme>,</l>
        <l id="s4:l11" met="3" type="-+">Heeft haast myn dood <rhyme label="f" type="m" corresp="#s4:l9">gevelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
