    <melody>Je rencontrais endormie</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HEer, hoe weynich van u <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Naer dat gh' uyt Egipten <rhyme label="b" type="m">zijt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Thuys ghekeert, men vindt <rhyme label="a" type="f" corresp="#s1:l1">beschreven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En werom van heel den <rhyme label="b" type="m" corresp="#s1:l2">tijdt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Naer u vindingh' in de <rhyme label="c" type="m">kerck</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wat is doen gheweest u <rhyme label="c" type="m" corresp="#s1:l5">werck</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wat is 't dat gh' in achtien <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Voor u preken hebt <rhyme label="b" type="m">ghedaen</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Want u Schrijvers niet <rhyme label="a" type="f" corresp="#s2:l1">verclaren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Dan hoe dat ghy hebt <rhyme label="b" type="m" corresp="#s2:l2">ghestaen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Onder Joseph en de <rhyme label="c" type="m">Maeght</rhyme>?</l>
        <l id="s2:l6" met="4" type="+-">Die staegh op hun ooghen <rhyme label="c" type="m" corresp="#s2:l5">saeght</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Is soo langh de Son <rhyme label="a" type="f">ghebleven</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Soo verborghen in dit <rhyme label="b" type="m">dal</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">Kost ghy onbekent hier <rhyme label="a" type="f" corresp="#s3:l1">leven</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Als 'nen niet, die zijt den <rhyme label="b" type="m" corresp="#s3:l2">Al</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Die den al hout uyt den <rhyme label="c" type="m">niet</rhyme>?</l>
        <l id="s3:l6" met="4" type="+-">Wie op dit groot wonder <rhyme label="c" type="m" corresp="#s3:l5">siet</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">G' hebt met Joseph al die <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">'t Broot ghewonnen <rhyme label="b" type="m">neerstelijck</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die doet t' aertrijck' 't koren <rhyme label="a" type="f" corresp="#s4:l1">baren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die zijt 't broodt van 't eewich <rhyme label="b" type="m" corresp="#s4:l2">rijck</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Haren cost door uwen <rhyme label="c" type="m">sweet</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Vondt u Moeder sich <rhyme label="c" type="m" corresp="#s4:l5">bereet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hoe verwondert, hoe <rhyme label="a" type="f">verslaghen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Dickwils stont 't gheluckich <rhyme label="b" type="m">paer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als s' u soo vernedert <rhyme label="a" type="f" corresp="#s5:l1">saghen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Daer gh' aen waert soo <rhyme label="b" type="m" corresp="#s5:l2">openbaer</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Hen soo seer als Godt <rhyme label="c" type="m">bekent</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">U t' aenbidden soo <rhyme label="c" type="m" corresp="#s5:l5">ghewent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ah wie sal des' <rhyme label="a" type="f">wonderheden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Machtich zijn wel te <rhyme label="b" type="m">verstaen</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Dat die teghen een niet <rhyme label="a" type="f" corresp="#s6:l1">streden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Hoe 't in 't werck kan zijn <rhyme label="b" type="m" corresp="#s6:l2">ghegaen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Dat ghy waert hun <rhyme label="c" type="m">onderdaen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Onder u die moesten <rhyme label="c" type="m" corresp="#s6:l5">staen</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Maer wat doede, snoode <rhyme label="a" type="f">dieren</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Die u hier verheft soo <rhyme label="b" type="m">seer</rhyme>?</l>
        <l id="s7:l3" met="4" type="+-">Zijt beschaemt tot in de <rhyme label="a" type="f" corresp="#s7:l1">nieren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Als ghy siet dat soo den <rhyme label="b" type="m" corresp="#s7:l2">Heer</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Booght sijn wonder <rhyme label="c" type="m">Majesteyt</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Onder ons <rhyme label="c" type="m" corresp="#s7:l5">verworpentheydt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Datti onbekent <rhyme label="a" type="f">ghebleven</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Is soo langh, ghelijck <rhyme label="b" type="m">verniet</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En dat ghy wilt zijn <rhyme label="a" type="f" corresp="#s8:l1">verheven</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Dat een ieder op u <rhyme label="b" type="m" corresp="#s8:l2">siet</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Wyt en breet wilt zijn <rhyme label="c" type="m">vernaemt</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Zijt hier van wel seer <rhyme label="c" type="m" corresp="#s8:l5">beschaemt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">O ghenadich Heer der <rhyme label="a" type="f">heeren</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Onsen niet ons kennen <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En door uwen voorganck <rhyme label="a" type="f" corresp="#s9:l1">leeren</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Zijn ootmoedich van <rhyme label="b" type="m" corresp="#s9:l2">ghemoedt</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Op dat wy eens met <rhyme label="c" type="m">wellust</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Eewich vinden in u <rhyme label="c" type="m" corresp="#s9:l5">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
