    <melody>Adieu natuurlijk leven mijn</melody>
    <categories>
      <category translation="occasions">gelegenheden</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Aedde naturlic leven <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Adde mijn trost, adde mijn <rhyme label="b" type="f">vrucht</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Adde het moet gescheijden <rhyme label="a" type="m" corresp="#s1:l1">zijn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Adde schoen bloem, reijn suver <rhyme label="b" type="f" corresp="#s1:l2">jucht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Adde dije mij duck heeft <rhyme label="b" type="f" corresp="#s1:l2">verhoecht</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Adde nu sietdij mij <rhyme label="c" type="f">onthoegen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Dat heft gedaen vrou Venus <rhyme label="b" type="f" corresp="#s1:l2">ontucht</rhyme>:</l>
        <l id="s1:l8" met="4" type="-+">Bij wael betruwen ben ick <rhyme label="c" type="f" corresp="#s1:l6">bedrogen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
