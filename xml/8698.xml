    <melody>Florida zo het wezen mag</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">FLorida so het wesen <rhyme label="a" type="m">mach</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ick kom u doen een droef <rhyme label="a" type="m" corresp="#s1:l1">geklag</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van mijn verdrietigh <rhyme label="b" type="f">minnen</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Ick hebbet u fiere <rhyme label="c" type="m">Maeght</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Soo menichmael <rhyme label="c" type="m" corresp="#s1:l4">ghevraeght</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Maer noyt niet kennen <rhyme label="b" type="f" corresp="#s1:l3">winnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat komt van klagen als <rhyme label="a" type="m">verdriet</rhyme>?</l>
        <l id="s2:l2" met="4" type="-+">V klagen Damon en gelter <rhyme label="a" type="m" corresp="#s2:l1">niet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Noch oock u soete <rhyme label="b" type="f">queelen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ick ben niet die ick eertijts <rhyme label="c" type="m">was</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Doen ick int groene <rhyme label="c" type="m" corresp="#s2:l4">Gras</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ginck ombecommert <rhyme label="b" type="f" corresp="#s2:l3">speelen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Florida wat is dat <rhyme label="a" type="m">gheseyt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Is mijn soo grooten druck <rhyme label="a" type="m" corresp="#s3:l1">bereyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat mijn dat sal <rhyme label="b" type="f">ghebeuren</rhyme>?</l>
        <l id="s3:l4" met="4" type="-+">Ach doen ick leste Tirses <rhyme label="c" type="m">sagh</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hoe hy ontrent u zijde <rhyme label="c" type="m" corresp="#s3:l4">lagh</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Mocht ick met reden <rhyme label="b" type="f" corresp="#s3:l3">treuren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Iae Tirses vrient dat is de <rhyme label="a" type="m">man</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Die mijn al wel vermaken <rhyme label="a" type="m" corresp="#s4:l1">kan</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy is mijn eyghen <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Ick heb hem lest mijn eyghen <rhyme label="c" type="m">hant</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Al tot een <rhyme label="c" type="m" corresp="#s4:l4">onderpant</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Met Vrienden raet <rhyme label="b" type="f" corresp="#s4:l3">ghegheven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ach! ach! wat voor een <rhyme label="a" type="m">ongeluck</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Och! och wat voor een sware <rhyme label="a" type="m" corresp="#s5:l1">druck</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Komt my nu <rhyme label="b" type="f">nederstorten</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Och of eens de felle <rhyme label="c" type="m">doot</rhyme></l>
        <l id="s5:l5" met="3" type="-+">In desen mijnen hooghen <rhyme label="c" type="m" corresp="#s5:l4">noot</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Mijn leven wou <rhyme label="b" type="f" corresp="#s5:l3">verkorten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Damon en treurt daerom so <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Dat u gebeurt is meer <rhyme label="a" type="m" corresp="#s6:l1">gheschiet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">V druck sal haest <rhyme label="b" type="f">versoeten</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">So ghy maer eens een nieuwe <rhyme label="c" type="m">maegt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Die u Ziel <rhyme label="c" type="m" corresp="#s6:l4">behaeght</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Uyt liefde gaet <rhyme label="b" type="f" corresp="#s6:l3">begroeten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Florida al is het soo <rhyme label="a" type="m">gheschiet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">En scheyt daerom soo van ons <rhyme label="a" type="m" corresp="#s7:l1">niet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Maer hoort my noch eens <rhyme label="b" type="f">spreken</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Misschien sla u een braver <rhyme label="c" type="m">quant</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Al met een soeten nieuwen <rhyme label="c" type="m" corresp="#s7:l4">brant</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Een liever vuur <rhyme label="b" type="f" corresp="#s7:l3">ontsteecken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ach neen dat waer voor mijn geen <rhyme label="a" type="m">eer</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Een Bruyt en is gheen Vrijster <rhyme label="a" type="m" corresp="#s8:l1">meer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sy mach gheen Iongmans <rhyme label="b" type="f">spreken</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Als eens het Ia woort is <rhyme label="c" type="m">gheseyt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Dan is de knoop soo vast <rhyme label="c" type="m" corresp="#s8:l4">gheleyt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Die niet en is te <rhyme label="b" type="f" corresp="#s8:l3">breken</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">En offer dan een schoonder <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Die hooger en rijcker was van <rhyme label="a" type="m" corresp="#s9:l1">stam</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Sout ghy hem niet <rhyme label="b" type="f">ontfanghen</rhyme>?</l>
        <l id="s9:l4" met="4" type="-+">Ey neen ic heb mijn harten <rhyme label="c" type="m">wensch</rhyme></l>
        <l id="s9:l5" met="3" type="-+">En my en sal gheen aertsche <rhyme label="c" type="m" corresp="#s9:l4">Mensch</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Na schoonder doen <rhyme label="b" type="f" corresp="#s9:l3">verlanghen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hem die ick eens mijn trouwe <rhyme label="a" type="m">gaf</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Dien blijf ick trouw tot in het <rhyme label="a" type="m" corresp="#s10:l1">graf</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Dies stremt u loose <rhyme label="b" type="f">treken</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Mijn ooghen sal noyt elders <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">En ick en wil <rhyme label="c" type="m" corresp="#s10:l4">voortaen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Gheen linckers hooren <rhyme label="b" type="f" corresp="#s10:l3">spreken</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Sijn wij nu linckers soete <rhyme label="a" type="m">maegt</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Ons praet heeft u wel eer <rhyme label="a" type="m" corresp="#s11:l1">behaeght</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Waerom soo trots <rhyme label="b" type="f">ghesproken</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Ten woort noch eer mijn hartje <rhyme label="c" type="m">sluyt</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Neen neen, ey wegh ic ben de <rhyme label="c" type="m" corresp="#s11:l4">bruyt</rhyme></l>
        <l id="s11:l6" met="3" type="-+">V praet dient <rhyme label="b" type="f" corresp="#s11:l3">afghebroken</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Wanneer de liefde is <rhyme label="a" type="m">geset</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">En treet tot op het echte <rhyme label="a" type="m" corresp="#s12:l1">bet</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Dan macht niet anders <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">De trouw dat is een reyn <rhyme label="c" type="m">verbont</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Hy staet in mijn <rhyme label="c" type="m" corresp="#s12:l4">gegront</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Voor al mijn gantsche <rhyme label="b" type="f" corresp="#s12:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Om my en dient niet meer <rhyme label="a" type="m">gedocht</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">'t Vleysch dat ghy siet dat is <rhyme label="a" type="m" corresp="#s13:l1">verkocht</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Ten kan u niet <rhyme label="b" type="f">gewerden</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Gaet daerje vryen <rhyme label="c" type="m">meught</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">En soeckt daer u <rhyme label="c" type="m" corresp="#s13:l4">gheneught</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Voor mijn ick salt wel <rhyme label="b" type="f" corresp="#s13:l3">harder</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
