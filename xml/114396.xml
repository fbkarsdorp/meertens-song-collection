<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>114396</id>
    <date>
      <year-start>1745</year-start>
      <year-end>1745</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Polyphemus aan de stranden</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SOete Zusje, bolle <rhyme label="a" type="f">Meisje</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Zeg een <rhyme label="a" type="f" corresp="#s1:l1">Reisje</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Waerom ben je straf en <rhyme label="b" type="m">stuur</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Daer ik jou mit al men <rhyme label="c" type="f">Zinnen</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Wil <rhyme label="c" type="f" corresp="#s1:l4">beminnen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Tot men Alderlaeste <rhyme label="b" type="m" corresp="#s1:l3">Uur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Voel eens, Hartje; hoe men <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Om jou <rhyme label="a" type="f" corresp="#s2:l1">brande</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Om je Montje <rhyme label="b" type="m">Zuikersoet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Om jou nette doeke <rhyme label="c" type="f">Huifje</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Soete <rhyme label="c" type="f" corresp="#s2:l4">Duifje</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Smelt ik schier deur Minne <rhyme label="b" type="m" corresp="#s2:l3">gloet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ik ben Ummers 't ienig <rhyme label="a" type="f">Zeuntje</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Lieve <rhyme label="a" type="f" corresp="#s3:l1">Teuntje</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Kyk men Kliertjes staen zoo <rhyme label="b" type="m">net</rhyme></l>
        <l id="s3:l4" met="4" type="+-">En myn net gemangelt <rhyme label="c" type="f">Kraegje</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Zoete <rhyme label="c" type="f" corresp="#s3:l4">Maegje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Het ien styfster op <rhyme label="b" type="m" corresp="#s3:l3">ezet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als men Moertje kom te <rhyme label="a" type="f">Sterven</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Zal ik <rhyme label="a" type="f" corresp="#s4:l1">Erven</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Linne, Wolle, Geld in <rhyme label="b" type="m">kas</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Bed en <rhyme label="c" type="f">Veeren</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Ketels, <rhyme label="c" type="f" corresp="#s4:l4">Kleeren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En veel ongesponne <rhyme label="b" type="m" corresp="#s4:l3">Vlas</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Je behoeft geen Wieg te <rhyme label="a" type="f">kopen</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Of te <rhyme label="a" type="f" corresp="#s5:l1">lopen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Om een Kynder <rhyme label="b" type="m">Luyermand</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Nog gien hulletjes of <rhyme label="c" type="f">doeken</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Op te <rhyme label="c" type="f" corresp="#s5:l4">zoeken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Want men Moertje hyt et <rhyme label="b" type="m" corresp="#s5:l3">kant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daer is nog van Pete <rhyme label="a" type="f">Jannetje</rhyme></l>
        <l id="s6:l2" met="2" type="+-">'t <rhyme label="a" type="f" corresp="#s6:l1">Kindermandtje</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Net gewerkt van Witte <rhyme label="b" type="m">tien</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met vier Ebben houte <rhyme label="c" type="f">voetjes</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">'t Staet zoo <rhyme label="c" type="f" corresp="#s6:l4">zoetjes</rhyme>!</l>
        <l id="s6:l6" met="4" type="+-">Mooy deurwerkt mit Walvis <rhyme label="b" type="m" corresp="#s6:l3">bien</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Daer toe heb ik Pete <rhyme label="a" type="f">Giertjes</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Beste <rhyme label="a" type="f" corresp="#s7:l1">Kliertjes</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Op zin Steeluys, 't Staet zo <rhyme label="b" type="m">boud</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Mit een Swaere Gouwe <rhyme label="c" type="f">Keten</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Wel te <rhyme label="c" type="f" corresp="#s7:l4">weten</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Van klinklaer Dikkaten <rhyme label="b" type="m" corresp="#s7:l3">Goud</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dan ook nog drie Gouwe <rhyme label="a" type="f">Ringen</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Dat zyn <rhyme label="a" type="f" corresp="#s8:l1">dingen</rhyme>!</l>
        <l id="s8:l3" met="4" type="+-">Ellik mit een mooye <rhyme label="b" type="m">stien</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Mit nog twee Dezyn <rhyme label="c" type="f">Servetten</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Fyn en <rhyme label="c" type="f" corresp="#s8:l4">Nette</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Van Moers beste, zoo ik <rhyme label="b" type="m" corresp="#s8:l3">mien</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Ook nog drie Damaste <rhyme label="a" type="f">Rokken</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Zonder <rhyme label="a" type="f" corresp="#s9:l1">Jokken</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En twee Huysjes, op mien <rhyme label="b" type="m">woord</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">'t Iene in het Waermoes <rhyme label="c" type="f">straetje</rhyme></l>
        <l id="s9:l5" met="2" type="+-">Beste <rhyme label="c" type="f" corresp="#s9:l4">Maetje</rhyme>;</l>
        <l id="s9:l6" met="4" type="+-">'t Ander by sint Teunis <rhyme label="b" type="m" corresp="#s9:l3">Poort</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Ey dan Zoete bolle <rhyme label="a" type="f">Tasje</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">Lieve <rhyme label="a" type="f" corresp="#s10:l1">Gasje</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Dooft myn brandend <rhyme label="b" type="m">Minnevuur</rhyme>!</l>
        <l id="s10:l4" met="4" type="+-">Want ik jou mit Hart en <rhyme label="c" type="f">Zinnen</rhyme></l>
        <l id="s10:l5" met="2" type="+-">Zel <rhyme label="c" type="f" corresp="#s10:l4">Beminnen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Tot men alderlaeste <rhyme label="b" type="m" corresp="#s10:l3">Uur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
