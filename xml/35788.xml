<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35788</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>holy song</tag>
    </tags>
    <melody>Gezegend zijn mijn liefs bruine ogen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE Wijste in, en na sijn <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Salomon, nochtans hy derft <rhyme label="a" type="f" corresp="#s1:l1">vragen</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Een stercke Vrouw wie vinden <rhyme label="b" type="m">sal</rhyme>?</l>
        <l id="s1:l4" met="4" type="-+">Houdt Wijste op, langer te <rhyme label="c" type="f">soecken</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">De vroomheydt weet al 's werelts <rhyme label="c" type="f" corresp="#s1:l4">hoecken</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Van Ursula met haer <rhyme label="b" type="m" corresp="#s1:l3">getal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als Keyser Gratiaen <rhyme label="a" type="f">regeerde</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Maximus Tyran <rhyme label="a" type="f" corresp="#s2:l1">rebelleerde</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Tegens de Keyser; Keyser <rhyme label="b" type="m">wort</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van Eng'lant in Vranckrijck <rhyme label="c" type="f">gekomen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Heeft heel Brittangien <rhyme label="c" type="f" corresp="#s2:l4">ingenomen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">'t Lant verderft, bloet by bloet <rhyme label="b" type="m" corresp="#s2:l3">verstort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In deele deelt dese <rhyme label="a" type="f">Lantsdouwen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Aen sijn Soldaten om te <rhyme label="a" type="f" corresp="#s3:l1">bouwen</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Belooft een Maeght aen yder <rhyme label="b" type="m">een</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Hierom in al d'Engelsche <rhyme label="c" type="f">kusten</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Laet Maeghden presten sonder <rhyme label="c" type="f" corresp="#s3:l4">rusten</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Tot d' Hemel klinckt 't Maeghde <rhyme label="b" type="m" corresp="#s3:l3">geste'en</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Koninghs Dochter uyt <rhyme label="a" type="f">gelesen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ursula, noyt genoegh <rhyme label="a" type="f" corresp="#s4:l1">gepresen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Stelt opper-hooft van dese <rhyme label="b" type="m">schaer</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Dit Koninghs bloet heeft late <rhyme label="c" type="f">slepen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Met elf duysent Maeghden nae <rhyme label="c" type="f" corresp="#s4:l4">schepen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Die om te zeylen waren <rhyme label="b" type="m" corresp="#s4:l3">klaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ursula, 't schilt van dese <rhyme label="a" type="f">scharen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Waer in de deughden, gaven <rhyme label="a" type="f" corresp="#s5:l1">waren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die in een Maeght worden <rhyme label="b" type="m">begeert</rhyme>;</l>
        <l id="s5:l4" met="4" type="-+">Haer Godt en al heeft soo <rhyme label="c" type="f">gebeden</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Bewaert onse ziele en <rhyme label="c" type="f" corresp="#s5:l4">leden</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">En wint en koers contrary <rhyme label="b" type="m" corresp="#s5:l3">keert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Want ons dese onkuysche <rhyme label="a" type="f">Mannen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">In Venus wagen sullen <rhyme label="a" type="f" corresp="#s6:l1">spannen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Soo ghy ons Iesu! niet <rhyme label="b" type="m">bevrijdt</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Wy schencken ons tot <rhyme label="c" type="f">offerhanden</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Laet ons liever tot stof <rhyme label="c" type="f" corresp="#s6:l4">verbranden</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Als zuyverheyt te raken <rhyme label="b" type="m" corresp="#s6:l3">quijt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Door stormen, tempeesten met <rhyme label="a" type="f">hoopen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Engelandt, Vranckrijck verby <rhyme label="a" type="f" corresp="#s7:l1">loopen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Drijven op de genade <rhyme label="b" type="m">Goods</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">D' ellif duysent komen ten <rhyme label="c" type="f">lesten</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Den Rhijn op aen de Ceulsche <rhyme label="c" type="f" corresp="#s7:l4">vesten</rhyme>;</l>
        <l id="s7:l6" met="4" type="-+">Hier vinden zy d' Hunnen <rhyme label="b" type="m" corresp="#s7:l3">Godt-loos</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dese willen haer al <rhyme label="a" type="f">onteeren</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Tot sonden brengen door 't <rhyme label="a" type="f" corresp="#s8:l1">boeleeren</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Nae Sinte-Ursulaes <rhyme label="b" type="m">vermaen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">De Maeghden vrywilligh <rhyme label="c" type="f">verkiesen</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Liever haer leven te <rhyme label="c" type="f" corresp="#s8:l4">verliesen</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Als geyle lusten toe te <rhyme label="b" type="m" corresp="#s8:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Om dat al d' Hunnen haer hooft <rhyme label="a" type="f">stooten</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Met pijlen de Maeghden doodt <rhyme label="a" type="f" corresp="#s9:l1">schooten</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">De Palm krijgen en <rhyme label="b" type="m">Maeghde-kroon</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Dese gewont en doode <rhyme label="c" type="f">leden</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Tot Ceulen kregen haer graf <rhyme label="c" type="f" corresp="#s9:l4">steden</rhyme>.</l>
        <l id="s9:l6" met="4" type="-+">Haer aert ontfanght geen Kint hoe <rhyme label="b" type="m" corresp="#s9:l3">schoon</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Sint' Ursel Maeght, en <rhyme label="a" type="f">Martelersse</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Met u Maeghden zijt <rhyme label="a" type="f" corresp="#s10:l1">Patroonersse</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Voor my, en Maeghden <rhyme label="b" type="m">allegaer</rhyme>:</l>
        <l id="s10:l4" met="4" type="-+">Verkrijght, dat wy om 't vleysch noch <rhyme label="c" type="f">staten</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Noch doodt de Maeghde-kroon <rhyme label="c" type="f" corresp="#s10:l4">verlaten</rhyme>:</l>
        <l id="s10:l6" met="4" type="-+">En namaels Goodes aenschijn <rhyme label="b" type="m" corresp="#s10:l3">klaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
