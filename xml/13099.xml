<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13099</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>holy song</tag>
    </tags>
    <melody>Cette cruelle</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHeluck Vrouw Anna, waerde <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Van d'alder reynste suyvere <rhyme label="b" type="m">Maecht</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Gheluck o Joachum fraey <rhyme label="a" type="f" corresp="#s1:l1">behoeder</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Van dit schoon kint, dat Anna <rhyme label="b" type="m" corresp="#s1:l2">draecht</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Gheluck minjoot, o Moeder <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Die eertijts droecht tot in u <rhyme label="c" type="m" corresp="#s1:l5">schoot</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Mari' d'eel <rhyme label="c" type="m" corresp="#s1:l5">Jesse-loodt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Voorwaer 'twas aen u wel te <rhyme label="a" type="f">mercken</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">dat ghu, die eertijts waert tot <rhyme label="b" type="m">spot</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sout syn tot glorie onser <rhyme label="a" type="f" corresp="#s2:l1">Kercken</rhyme></l>
        <l id="s2:l4" met="4" type="-+">tot sulcke Moer, tot sulcke <rhyme label="b" type="m" corresp="#s2:l2">lot</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Geeft my doch, dat ick u goet <rhyme label="c" type="m">padt</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Naer treden mach, en krijgen <rhyme label="c" type="m" corresp="#s2:l5">wat</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Van u seer waerde <rhyme label="c" type="m" corresp="#s2:l5">schat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Des al ick hier weer op der <rhyme label="a" type="f">aerden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Handhaven u verkleynde <rhyme label="b" type="m">eer</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Van haer, die tot u groot <rhyme label="a" type="f" corresp="#s3:l1">onwaerde</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Wtstroyen, dat ghy trouwde <rhyme label="b" type="m" corresp="#s3:l2">weer</rhyme></l>
        <l id="s3:l5" met="4" type="-+">'tIs quaet ondiert, die dit <rhyme label="c" type="m">versiert</rhyme></l>
        <l id="s3:l6" met="4" type="-+">'tIs vleesch en bloedt <rhyme label="c" type="m" corresp="#s3:l5">onbestiert</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Dees' vuyle leugen <rhyme label="c" type="m" corresp="#s3:l5">vierdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wert dan beschaemt, die nae soo veel <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s4:l2" met="4" type="-+">En nae de gunst van sulcken <rhyme label="b" type="m">kindt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met 2/3 mans noch durft <rhyme label="a" type="f" corresp="#s4:l1">paeren</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Veel blinder, als cupido <rhyme label="b" type="m" corresp="#s4:l2">blindt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">'tIs Anna waert, die eens <rhyme label="c" type="m">gepaert</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Naer 'deerste man in <rhyme label="c" type="m" corresp="#s4:l5">suyverheyt</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Haer gantsche leven <rhyme label="c" type="m" corresp="#s4:l5">leydt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Kompt nu dan Weeuwen met de <rhyme label="a" type="f">Maechden</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Versoecken t'samen haer <rhyme label="b" type="m">ghebedt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Nae dien't den Heer door haer <rhyme label="a" type="f" corresp="#s5:l1">behaechden</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Te geven aen de nieuwe <rhyme label="b" type="m" corresp="#s5:l2">Wet</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Van zuyverheyt, dit eerst <rhyme label="c" type="m">bescheyt</rhyme></l>
        <l id="s5:l6" met="4" type="-+">'tWelck een exempel geven <rhyme label="c" type="m" corresp="#s5:l5">sou</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Voor Maecht, voor Weeuw, en <rhyme label="c" type="m" corresp="#s5:l5">Vrouw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Bidt Anna, dat sy doch <rhyme label="a" type="f">volharden</rhyme></l>
        <l id="s6:l2" met="4" type="-+">In haer begonnen <rhyme label="b" type="m">reynicheyt</rhyme></l>
        <l id="s6:l3" met="4" type="-+">En u getal vermeerdert <rhyme label="a" type="f" corresp="#s6:l1">werden</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Van hier tot inder <rhyme label="b" type="m" corresp="#s6:l2">eeuwicheyt</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Tot merck van syn waere <rhyme label="c" type="m">kerck</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Tot eer van haren Bruygom <rhyme label="c" type="m" corresp="#s6:l5">sterck</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Door dit volbrochte <rhyme label="c" type="m" corresp="#s6:l5">werck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
