<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8769</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De Heer die moet ik loven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">OM Ialousy te <rhyme label="a" type="f">schouwen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Sult ghy weten <rhyme label="b" type="m">hoe</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Cephalis quam in <rhyme label="a" type="f" corresp="#s1:l1">rouwe</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Dat bengde <rhyme label="b" type="m" corresp="#s1:l2">doe</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Procris zijn <rhyme label="c" type="m">lief</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die hy Trouw had <rhyme label="d" type="f">ghesworen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die door nijders <rhyme label="c" type="m" corresp="#s1:l5">ong'rief</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Mistroostigh ginck <rhyme label="d" type="f" corresp="#s1:l6">verlooren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Cephalis nae't <rhyme label="a" type="f">ghewaghen</rhyme>:</l>
        <l id="s2:l2" met="3" type="+-">Of Deliaes <rhyme label="b" type="m">waert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ginck daghelijcks uyt <rhyme label="a" type="f" corresp="#s2:l1">Iaghen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Sonder Hondt of <rhyme label="b" type="m" corresp="#s2:l2">Paert</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">In een Bosschagie <rhyme label="c" type="m">dicht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Tot loopen hy hem <rhyme label="d" type="f">snelde</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Daer hy met zijnen <rhyme label="c" type="m" corresp="#s2:l5">Schicht</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Menich Wildt neder <rhyme label="d" type="f" corresp="#s2:l6">velde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vermoeyt zijnd' in 't <rhyme label="a" type="f">ghemeene</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Van Iaghen, hy <rhyme label="b" type="m">quam</rhyme></l>
        <l id="s3:l3" met="3" type="-+">In een lommer <rhyme label="a" type="f" corresp="#s3:l1">alleene</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Hy zijn ruste <rhyme label="b" type="m" corresp="#s3:l2">nam</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">En hy riep tot den <rhyme label="c" type="m">Windt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Komt en wilt my <rhyme label="d" type="f">verkoelen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Aurora Lief <rhyme label="c" type="m" corresp="#s3:l5">bemindt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Laet my solaes <rhyme label="d" type="f" corresp="#s3:l6">ghevoelen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Eenen Quidam <rhyme label="a" type="f">versteken</rhyme></l>
        <l id="s4:l2" met="3" type="+-">In den Bossche <rhyme label="b" type="m">lagh</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Cephalis hoord' hy <rhyme label="a" type="f" corresp="#s4:l1">spreken</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Die hy niet en <rhyme label="b" type="m" corresp="#s4:l2">sagh</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Meenende dat <rhyme label="c" type="m">hy</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Met Aura in't <rhyme label="d" type="f">secrete</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Alleen boeleerde <rhyme label="c" type="m" corresp="#s4:l5">vry</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dede Procris de <rhyme label="d" type="f" corresp="#s4:l6">wete</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Procris gantsch vol <rhyme label="a" type="f">onwaerden</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Hoorden dit <rhyme label="b" type="m">bedrijf</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Viel in onmacht ter <rhyme label="a" type="f" corresp="#s5:l1">Aerden</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Wee! my arrem <rhyme label="b" type="m" corresp="#s5:l2">Wijf</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Sal ick in't <rhyme label="c" type="m">gepijn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Van mijn Lief met <rhyme label="d" type="f">geschille</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Aldus verstooten <rhyme label="c" type="m" corresp="#s5:l5">zijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Al om een anders <rhyme label="d" type="f" corresp="#s5:l6">wille</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Cephalis met <rhyme label="a" type="f">verlanghen</rhyme></l>
        <l id="s6:l2" met="3" type="+-">T'huys komende <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Al wat hy had <rhyme label="a" type="f" corresp="#s6:l1">ghevanghen</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Brocht hy Procris, <rhyme label="b" type="m" corresp="#s6:l2">maer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Zy nam voor haer <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van dese loose <rhyme label="d" type="f">lage</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Hem te seggen <rhyme label="c" type="m" corresp="#s6:l5">niet</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Voor dat sy't selve <rhyme label="d" type="f" corresp="#s6:l6">sage</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Des morgens vroeg als 't <rhyme label="a" type="f">paste</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Cephalus <rhyme label="b" type="m">vermaert</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Nam zijnen schilt en <rhyme label="a" type="f" corresp="#s7:l1">raste</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Hem ten Bossche <rhyme label="b" type="m" corresp="#s7:l2">waert</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En Procris zijn <rhyme label="c" type="m">Lief</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Quam daer den Quidam <rhyme label="d" type="f">seyde</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Secreet als een <rhyme label="c" type="m" corresp="#s7:l5">Dief</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Cephalus sy <rhyme label="d" type="f" corresp="#s7:l6">verbeyde</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Cephalus als te <rhyme label="a" type="f">voren</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Nae gewoonte <rhyme label="b" type="m">sprack</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Komt Aura Lief <rhyme label="a" type="f" corresp="#s8:l1">verkoren</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Ghy zijt mijn <rhyme label="b" type="m" corresp="#s8:l2">ghemack</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Dit werdt haest <rhyme label="c" type="m">verstaen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Van Procris die 't <rhyme label="d" type="f">besuchte</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Roerende in de <rhyme label="c" type="m" corresp="#s8:l5">blaen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Maeckte eenigh <rhyme label="d" type="f" corresp="#s8:l6">gheruchte</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Cephalus meen de <rhyme label="a" type="f">puere</rhyme></l>
        <l id="s9:l2" met="3" type="+-">'t Was een Dier dat <rhyme label="b" type="m">liep</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Schietend' op <rhyme label="a" type="f" corresp="#s9:l1">avontuere</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Amy! dat sy <rhyme label="b" type="m" corresp="#s9:l2">riep</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Daer sagh hy <rhyme label="c" type="m">terstont</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Zijn lief liggen in <rhyme label="d" type="f">smerte</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Doodelick <rhyme label="c" type="m" corresp="#s9:l5">ghewont</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Met den Schicht in haer <rhyme label="d" type="f" corresp="#s9:l6">herte</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Hem docht als Man <rhyme label="a" type="f">weemoedig</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Dat zijn herte <rhyme label="b" type="m">brack</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Als hy den Schicht seer <rhyme label="a" type="f" corresp="#s10:l1">bloedigh</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Uyt haer wonde <rhyme label="b" type="m" corresp="#s10:l2">track</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Maer als sy de <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Van haer komste <rhyme label="d" type="f">verhaelde</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Verstont hy datse <rhyme label="c" type="m" corresp="#s10:l5">in</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Den naem van Aura <rhyme label="d" type="f" corresp="#s10:l6">dwaelde</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Om t' kussen hy hem <rhyme label="a" type="f">wende</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Wie 't benijde <rhyme label="b" type="m">loos</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">In zijne Armen <rhyme label="a" type="f" corresp="#s11:l1">ende</rhyme></l>
        <l id="s11:l4" met="3" type="+-">Zij haer leven <rhyme label="b" type="m" corresp="#s11:l2">broos</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Al dit lijden <rhyme label="c" type="m">benout</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">'t Aensien heeft hem <rhyme label="d" type="f">verdroten</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Noyt Iagher in het <rhyme label="c" type="m" corresp="#s11:l5">Wout</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Heeft leeder wilt <rhyme label="d" type="f" corresp="#s11:l6">gheschoten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Prince door 't licht <rhyme label="a" type="f">ghelooven</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Quam dit <rhyme label="b" type="m">ongheval</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Laet u doch niet <rhyme label="a" type="f" corresp="#s12:l1">verdooven</rhyme></l>
        <l id="s12:l4" met="3" type="+-">Zijt gewaerschout <rhyme label="b" type="m" corresp="#s12:l2">al</rhyme>:</l>
        <l id="s12:l5" met="3" type="-+">Om te benijden <rhyme label="c" type="m">fel</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">'t Fenijn der quader <rhyme label="d" type="f">Tonghen</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Dient dit Liedeken <rhyme label="c" type="m" corresp="#s12:l5">wel</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Dickmael van ons <rhyme label="d" type="f" corresp="#s12:l6">ghesonghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
