  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Komt vrinden aanhoord eens wat <rhyme label="a" type="m">vermaak</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dat 'er was op den agtsten <rhyme label="a" type="m" corresp="#s1:l1">Maart</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Al in de Stad 's <rhyme label="b" type="f">Gravenhaage</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Als men het regt <rhyme label="c" type="m">betragt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Wat een vreugd men weer <rhyme label="d" type="m">beleefd</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat ons God weer heeft <rhyme label="e" type="m">verheugd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Oranje-Stam hoord men met <rhyme label="a" type="m">geschal</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">In alle Straaten <rhyme label="a" type="m" corresp="#s2:l1">overal</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Met veel kaarslicht en <rhyme label="b" type="f">vlambouw</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Het was waard dat men 't <rhyme label="c" type="m">beschouw</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Wat een vreugd men weer <rhyme label="d" type="m">beleefd</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat ons God weer heeft <rhyme label="e" type="m">verheugd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Men zag den agtsten Maart al in den <rhyme label="a" type="m">Haag</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Veel sieraad al van ons <rhyme label="a" type="m" corresp="#s3:l1">Jaar</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Doch het was het Jaargetal <rhyme label="b" type="f">geteld</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">1788 Wierd het klaar <rhyme label="c" type="m">gesteld</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Wat een vreugd men weer <rhyme label="d" type="m">beleefd</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat ons God weer heeft <rhyme label="e" type="m">verheugd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Men zag Zyn Hoogheid in alle <rhyme label="a" type="m">Straaten</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Om te zien dat wonder <rhyme label="a" type="m" corresp="#s4:l1">schoon</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met zyn Spruitjes zeer lief en <rhyme label="b" type="f">waard</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Veel Koetse zag men ook ten <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Wat een vreugd men weer <rhyme label="d" type="m">beleefd</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat ons God weer heeft <rhyme label="e" type="m">verheugd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Men zag veel volk op de <rhyme label="a" type="m">baan</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Om te zien de schoonheid <rhyme label="a" type="m" corresp="#s5:l1">aan</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Het was vreugde <rhyme label="b" type="f">overal</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met zingen, danzen duizend in <rhyme label="c" type="m">tal</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Wat een vreugd men weer <rhyme label="d" type="m">beleefd</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dat ons God weer heeft <rhyme label="e" type="m">verheugd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Oranje wier bemind al wat men <rhyme label="a" type="m">vind</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Van ieder Kind wierd gedragen <rhyme label="a" type="m" corresp="#s6:l1">Oranje-lint</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Men hoord met een bly <rhyme label="b" type="f">geschal</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Nu is Oranje <rhyme label="c" type="m">overal</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Wat een vreugd men weer <rhyme label="d" type="m">beleefd</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat ons God weer heeft <rhyme label="e" type="m">verheugd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Het was een geschal op Dorp en <rhyme label="a" type="m">Stede</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Als ook Leyden, Delft en <rhyme label="a" type="m" corresp="#s7:l1">Rotterdam</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Zag men ook veel <rhyme label="b" type="f">vrolykheid</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In Dordrecht was men ook <rhyme label="c" type="m">verblyd</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Wat een vreugd men weer <rhyme label="d" type="m">beleefd</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Dat ons God weer heeft <rhyme label="e" type="m">verheugd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
