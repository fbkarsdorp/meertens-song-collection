  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als ick eerst was <rhyme label="a" type="f">gecomen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">By v lieff <rhyme label="b" type="m">excellent</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In een hoff fray met <rhyme label="a" type="f" corresp="#s1:l1">bomen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Geplant seer <rhyme label="b" type="m" corresp="#s1:l2">partenent</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ick dacht in mynen <rhyme label="c" type="f">sinnen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Al op dat selue <rhyme label="d" type="m">pas</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Dat het waert te <rhyme label="c" type="f" corresp="#s1:l5">beminnen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Alsulck een jonckman <rhyme label="d" type="m" corresp="#s1:l6">was</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Doch ons kennis <rhyme label="a" type="f">vermeerde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Door v begeerte <rhyme label="b" type="m">fris</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En ick niet <rhyme label="a" type="f" corresp="#s2:l1">apsenteerde</rhyme></l>
        <l id="s2:l4" met="3" type="-+">De myne was <rhyme label="b" type="m" corresp="#s2:l2">tgewis</rhyme></l>
        <l id="s2:l5" met="3" type="-+">V sin ghy my <rhyme label="c" type="f">verclaerde</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Tonende lieffde <rhyme label="d" type="m">reyn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En spraeckt als <rhyme label="c" type="f" corresp="#s2:l5">donbeswaerde</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ghy syt myn lieff <rhyme label="d" type="m" corresp="#s2:l6">alleyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als vlyses <rhyme label="a" type="f">behendich</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hebt ghy my seer <rhyme label="b" type="m">oprecht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">O lieffde die <rhyme label="a" type="f" corresp="#s3:l1">inwendich</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In t'hert was <rhyme label="b" type="m" corresp="#s3:l2">wtgeleyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En spraeckt in myn <rhyme label="c" type="f">presenty</rhyme></l>
        <l id="s3:l6" met="3" type="-+">V liefft heeft my <rhyme label="d" type="m">doorwont</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Mocht ick tot myn <rhyme label="c" type="f" corresp="#s3:l5">intency</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Comen t'eniger <rhyme label="d" type="m" corresp="#s3:l6">stont</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Moedich was v <rhyme label="a" type="f">begeeren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Den sin toch wel <rhyme label="b" type="m">verstaet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wy souden gaen <rhyme label="a" type="f" corresp="#s4:l1">spanceren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daermen gemeenlyck <rhyme label="b" type="m" corresp="#s4:l2">gaet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wy ghingen oock seer <rhyme label="c" type="f">lustich</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Maer gecomen ter <rhyme label="d" type="m">stee</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Waert ghy al wat <rhyme label="c" type="f" corresp="#s4:l5">onrustich</rhyme></l>
        <l id="s4:l8" met="3" type="-+">T'welck my oock seer vreemt <rhyme label="d" type="m" corresp="#s4:l6">dee</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Buyten meningh <rhyme label="a" type="f">vermaende</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ghy had gesproken <rhyme label="b" type="m">dat</rhyme></l>
        <l id="s5:l3" met="3" type="-+">My selfs ick deed <rhyme label="a" type="f" corresp="#s5:l1">verstaende</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Denckende wat schaet <rhyme label="b" type="m" corresp="#s5:l2">dat</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ick daer mede <rhyme label="c" type="f">betoonde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Myn lieffde tvwerts <rhyme label="d" type="m">siet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Die in myn hert doen <rhyme label="c" type="f" corresp="#s5:l5">woonde</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Toch ick t'niet blycken <rhyme label="d" type="m" corresp="#s5:l6">liet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Voortaen ginckt ghy oock <rhyme label="a" type="f">stellen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Tot my v lieffde <rhyme label="b" type="m">gaet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Die ick al sonder <rhyme label="a" type="f" corresp="#s6:l1">quellen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Aennam al metter <rhyme label="b" type="m" corresp="#s6:l2">spoet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Dat my tot v deed <rhyme label="c" type="f">keeren</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Was v fray weesen <rhyme label="d" type="m">fyn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En v soet <rhyme label="c" type="f" corresp="#s6:l5">dinyseren</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Verwan dat herte <rhyme label="d" type="m" corresp="#s6:l6">myn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Roerende was <rhyme label="a" type="f">tonureden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">V gemoet so het <rhyme label="b" type="m">scheen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Om dat v en myn <rhyme label="a" type="f" corresp="#s7:l1">reden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Niet quaemen ouer <rhyme label="b" type="m" corresp="#s7:l2">een</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Maer ghy most sonder <rhyme label="c" type="f">toeuen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Vertrecken seer <rhyme label="d" type="m">versaecht</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dus ginck ick my <rhyme label="c" type="f" corresp="#s7:l5">bedroeuen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Want theeft my niet <rhyme label="d" type="m" corresp="#s7:l6">behaecht</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Treurich den tyt <rhyme label="a" type="f">genaeckte</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ghy aen my orloff <rhyme label="b" type="m">naemt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">En v op reyse <rhyme label="a" type="f" corresp="#s8:l1">maeckte</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Maer wederom ghy <rhyme label="b" type="m" corresp="#s8:l2">quaemt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">V noch eens <rhyme label="c" type="f">presenteren</rhyme></l>
        <l id="s8:l6" met="3" type="-+">En spraeckt met goet <rhyme label="d" type="m">bescheyt</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Ick bid v lieff vol <rhyme label="c" type="f" corresp="#s8:l5">eeren</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Myn wedercomst <rhyme label="d" type="m" corresp="#s8:l6">verbeyt</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Staedich ghy my waert <rhyme label="a" type="f">biende</rhyme></l>
        <l id="s9:l2" met="3" type="-+">V princelyck <rhyme label="b" type="m">persoon</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Toch ick my sulckx <rhyme label="a" type="f" corresp="#s9:l1">ontsiende</rhyme></l>
        <l id="s9:l4" met="3" type="-+">En sprack met soeten <rhyme label="b" type="m" corresp="#s9:l2">toon</rhyme></l>
        <l id="s9:l5" met="3" type="-+">So Godt ons wilde <rhyme label="c" type="f">paeren</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Het sal geschien seer <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Al gae ick niet <rhyme label="c" type="f" corresp="#s9:l5">beswaeren</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Myn trou en myn <rhyme label="d" type="m" corresp="#s9:l6">gemoet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
