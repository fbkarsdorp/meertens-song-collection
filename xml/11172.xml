  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gheen dinck ints werelts <rhyme label="a" type="f">erven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Twelck metter tijt niet <rhyme label="b" type="m">keert</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Het leven end' het <rhyme label="a" type="f" corresp="#s1:l1">sterven</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met beurten ons <rhyme label="b" type="m" corresp="#s1:l2">regeert</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Den hemel <rhyme label="c" type="m">ront</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Draeyt selver met sijn <rhyme label="d" type="f">hopen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">De Son comt nu <rhyme label="d" type="f" corresp="#s1:l6">gheloopen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Niet, daer sy gister <rhyme label="c" type="m" corresp="#s1:l5">stont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Besiet hoe Flora <rhyme label="a" type="f">blommen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Staan maar een cleynen <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Naar haar sal Ceres <rhyme label="a" type="f" corresp="#s2:l1">commen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die met Vertumnum <rhyme label="b" type="m" corresp="#s2:l2">strijt</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Pomona <rhyme label="c" type="m">root</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Haar fruyten brenght te <rhyme label="d" type="f">voren</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Den winter grijs vol <rhyme label="d" type="f" corresp="#s2:l6">toren</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Haar wederom <rhyme label="c" type="m" corresp="#s2:l5">verstoot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als alle dinghen <rhyme label="a" type="f">keeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">End' niet ghestadighs <rhyme label="b" type="m">zy</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wie can my dan <rhyme label="a" type="f" corresp="#s3:l1">blameren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat mijn liefd' keert van <rhyme label="b" type="m" corresp="#s3:l2">dy</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Der liefden <rhyme label="c" type="m">Godt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Selfs onghestadich <rhyme label="d" type="f">mede</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hun die door vaste <rhyme label="d" type="f" corresp="#s3:l6">bede</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Volherden doch <rhyme label="c" type="m" corresp="#s3:l5">bespot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Jck hebb' u <rhyme label="a" type="f">aangebeden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als eene Venus <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Maar nu heeft mijn <rhyme label="a" type="f" corresp="#s4:l1">vryheden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Diana ras te <rhyme label="b" type="m" corresp="#s4:l2">voet</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Diana <rhyme label="c" type="m">net</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Minlijck end' ranck van <rhyme label="d" type="f">leden</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wiens aenghesicht <rhyme label="d" type="f" corresp="#s4:l6">besneden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Mijn onghenoeght <rhyme label="c" type="m" corresp="#s4:l5">belet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghelijck eertijts <rhyme label="a" type="f">Diane</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Maanlichtich ginck ter <rhyme label="b" type="m">jagt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">End' met haar bogen <rhyme label="a" type="f" corresp="#s5:l1">sane</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Veel wilts heeft <rhyme label="b" type="m" corresp="#s5:l2">t'onderbragt</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Soo occk met <rhyme label="c" type="m">dien</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Mijn Nimph' door schoonheyts <rhyme label="d" type="f">deele</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Schiet herten alsoo <rhyme label="d" type="f" corresp="#s5:l6">veele</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Als ooghen haar <rhyme label="c" type="m" corresp="#s5:l5">besien</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Want die haer statich <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Eens merckelijck <rhyme label="b" type="m">besiet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Sal uyt haar voorhooft <rhyme label="a" type="f" corresp="#s6:l1">lesen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">De deugt en anders <rhyme label="b" type="m" corresp="#s6:l2">niet</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Haar voorhooft <rhyme label="c" type="m">hooch</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Spieghel van haar <rhyme label="d" type="f">nature</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Js oorsaack dat ick <rhyme label="d" type="f" corresp="#s6:l6">vure</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Jn my dees tering <rhyme label="c" type="m" corresp="#s6:l5">drooch</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Haar haarken daar en <rhyme label="a" type="f">boven</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Subtijl ghecrolt, <rhyme label="b" type="m">ontvout</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Glinstert sonder <rhyme label="a" type="f" corresp="#s7:l1">verdoven</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Ghelijck het Ebben <rhyme label="b" type="m" corresp="#s7:l2">hout</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Jn haar <rhyme label="c" type="m">aanschijn</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Twee oochskens bruyn wilt <rhyme label="d" type="f">weten</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Die als twee claar <rhyme label="d" type="f" corresp="#s7:l6">Planeten</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Van hare schoonheyt <rhyme label="c" type="m" corresp="#s7:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Verhemelt en <rhyme label="a" type="f">beloken</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met twee wijnbraukens <rhyme label="b" type="m">digt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Daaronder leyt <rhyme label="a" type="f" corresp="#s8:l1">ghedoken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">cupido met sijn <rhyme label="b" type="m" corresp="#s8:l2">schigt</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Somma <rhyme label="c" type="m">tghesigt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Vriendlijck ghestadich <rhyme label="d" type="f">mede</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Soud' u door ene <rhyme label="d" type="f" corresp="#s8:l6">vrede</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Tot haar beweghen <rhyme label="c" type="m" corresp="#s8:l5">ligt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Haar neusken boven <rhyme label="a" type="f">allen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Met gheenen Arents <rhyme label="b" type="m">bult</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Oock gheensins <rhyme label="a" type="f" corresp="#s9:l1">inghevallen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ghy aan haar schouwen <rhyme label="b" type="m" corresp="#s9:l2">sult</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Regt <rhyme label="c" type="m">afgemaalt</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Als met een passers <rhyme label="d" type="f">mate</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Ghelijck in een velt <rhyme label="d" type="f" corresp="#s9:l6">strate</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Een heuveltien steyl <rhyme label="c" type="m" corresp="#s9:l5">daalt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Haar mondeken <rhyme label="a" type="f">ghesloten</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ghelijck een <rhyme label="b" type="m">roosencrans</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Sal nimmermeer <rhyme label="a" type="f" corresp="#s10:l1">ontblooten</rhyme></l>
        <l id="s10:l4" met="3" type="-+">De tanden, wit <rhyme label="b" type="m" corresp="#s10:l2">nochtans</rhyme></l>
        <l id="s10:l5" met="2" type="-+">t' Mondeken <rhyme label="c" type="m">cleyn</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Soet proper end' <rhyme label="d" type="f">pruymachtich</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Taalt minlijck is <rhyme label="d" type="f" corresp="#s10:l6">waarachtich</rhyme></l>
        <l id="s10:l8" met="3" type="-+">End' in sijn woorden <rhyme label="c" type="m" corresp="#s10:l5">reyn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Haar kinneken <rhyme label="a" type="f">ghecloven</rhyme></l>
        <l id="s11:l2" met="3" type="-+">ghelijck een quepeer <rhyme label="b" type="m">is</rhyme></l>
        <l id="s11:l3" met="3" type="-+">De caachskens niet om <rhyme label="a" type="f" corresp="#s11:l1">loven</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Met putkens dalen <rhyme label="b" type="m" corresp="#s11:l2">wis</rhyme></l>
        <l id="s11:l5" met="2" type="-+">Haar vast <rhyme label="c" type="m">coleur</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Niet bleeck <rhyme label="d" type="f">melancolijcke</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Maar eenes roos <rhyme label="d" type="f" corresp="#s11:l6">ghelijcke</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Houd' ick vry voor de <rhyme label="c" type="m" corresp="#s11:l5">keur</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">De bleecke verwe <rhyme label="a" type="f">teere</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Verandert metter <rhyme label="b" type="m">vaart</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Een bruyntien ick <rhyme label="a" type="f" corresp="#s12:l1">begeere</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Sterck lustich end' <rhyme label="b" type="m" corresp="#s12:l2">galiaart</rhyme></l>
        <l id="s12:l5" met="2" type="-+">Diana <rhyme label="c" type="m">schoon</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Op u mijn sin ick <rhyme label="d" type="f">sette</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Or vive la <rhyme label="d" type="f" corresp="#s12:l6">Brunette</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Want zy spant veeler <rhyme label="c" type="m" corresp="#s12:l5">Croon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
