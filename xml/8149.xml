    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">Geestelijk lied</tag>
    </tags>
    <melody/>
    <categories/>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAnck wilt tot Godt op <rhyme label="a" type="f">heven</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Met herten <rhyme label="b" type="m">verblijdt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Voor dat hy ons gaet <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Dese vrijen <rhyme label="b" type="m" corresp="#s1:l2">tijdt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">niet om des vleys <rhyme label="c" type="m">behaech</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">maer sijn eer te <rhyme label="d" type="f">verbreyden</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">O, dat wy alle <rhyme label="c" type="m" corresp="#s1:l5">daech</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ons vlijdt hier toe aen <rhyme label="d" type="f" corresp="#s1:l6">leyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy hebben nu die <rhyme label="a" type="f">daghen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Daer de schrift van <rhyme label="b" type="m">seydt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om met vlijt te <rhyme label="a" type="f" corresp="#s2:l1">bejaghen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Onse <rhyme label="b" type="m" corresp="#s2:l2">saligheyt</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Dus Ider sie wel <rhyme label="c" type="m">toe</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dat hy dees gulden <rhyme label="d" type="f">vrijheydt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">In 'svleys lust niet <rhyme label="c" type="m" corresp="#s2:l5">verdoe</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">maer alleen tot 'sgeest <rhyme label="d" type="f" corresp="#s2:l6">blijheydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laet 'swerelts blijtschap <rhyme label="a" type="f">varen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Met al haer <rhyme label="b" type="m">getoy</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Want al die met haer <rhyme label="a" type="f" corresp="#s3:l1">paren</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Sy vergaen als <rhyme label="b" type="m" corresp="#s3:l2">hoy</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dus lieve Ionghe <rhyme label="c" type="m">Ieucht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wilt een reys <rhyme label="d" type="f">overlegghen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Of ghy door treck nae <rhyme label="c" type="m" corresp="#s3:l5">vreucht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dees wel adieu meucht <rhyme label="d" type="f" corresp="#s3:l6">segghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wel aen wilt dan <rhyme label="a" type="f">bewijsen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Dat ghy Christen <rhyme label="b" type="m">bent</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Het werck dat moet u <rhyme label="a" type="f" corresp="#s4:l1">prijsen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Van begin tot <rhyme label="b" type="m" corresp="#s4:l2">endt</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Dus wilt als Lichten <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In dese werelt, <rhyme label="d" type="f">blincken</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En doet niet als die <rhyme label="c" type="m" corresp="#s4:l5">daer</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Over twee shoen <rhyme label="d" type="f" corresp="#s4:l6">hincken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Of wy dan sotten <rhyme label="a" type="f">heten</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Van die sotten <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Al moet men smaet op <rhyme label="a" type="f" corresp="#s5:l1">eten</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Denckt dat is geen <rhyme label="b" type="m" corresp="#s5:l2">pijn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Want t'wit daer ick naer <rhyme label="c" type="m">schiet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Door het gheloove <rhyme label="d" type="f">brandich</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Neemt wech al mijn <rhyme label="c" type="m" corresp="#s5:l5">verdriet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Al moet het heten <rhyme label="d" type="f" corresp="#s5:l6">schandich</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick gae my Heer na <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Die soo veel <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hier voor my heeft <rhyme label="a" type="f" corresp="#s6:l1">gheleden</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Ia sijn troon <rhyme label="b" type="m" corresp="#s6:l2">verliet</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Ick wil met groot <rhyme label="c" type="m">vermaeck</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Wt Babel nu gaen <rhyme label="d" type="f">scheyen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat ick in vreucht <rhyme label="c" type="m" corresp="#s6:l5">gheraeck</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Als Babels dienaers <rhyme label="d" type="f" corresp="#s6:l6">schreyen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Waer wy rijden of <rhyme label="a" type="f">varen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Hout dit in den <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wilt geestes vruchten <rhyme label="a" type="f" corresp="#s7:l1">baren</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Deen wat meer of <rhyme label="b" type="m" corresp="#s7:l2">min</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Elck neem sijn roeping <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Wat hy heeft <rhyme label="d" type="f">aengenomen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dat wy met vreucht hier <rhyme label="c" type="m" corresp="#s7:l5">naer</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Voor onse rechter <rhyme label="d" type="f" corresp="#s7:l6">comen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dat wy dan mogen <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">'t Woordt seer <rhyme label="b" type="m">blijdelijck</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Comt hier mijne <rhyme label="a" type="f" corresp="#s8:l1">uytvercooren</rhyme></l>
        <l id="s8:l4" met="3" type="+-">In mijns Vaders <rhyme label="b" type="m" corresp="#s8:l2">Rijck</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ontfangt nu tsalich <rhyme label="c" type="m">loon</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Bereydt van <rhyme label="d" type="f">eeuwicheden</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Set op u hooft een <rhyme label="c" type="m" corresp="#s8:l5">croon</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">En leeft by my in <rhyme label="d" type="f" corresp="#s8:l6">vreden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maer eeuwich wee seer <rhyme label="a" type="f">schadich</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Sal toekomen <rhyme label="b" type="m">haer</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die mijnen tijt <rhyme label="a" type="f" corresp="#s9:l1">genadich</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Niet en namen <rhyme label="b" type="m" corresp="#s9:l2">waer</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Dus gaet nu wech van <rhyme label="c" type="m">mijn</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Al des duyvels <rhyme label="d" type="f">gesellen</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Ghevoelt der sielen <rhyme label="c" type="m" corresp="#s9:l5">pijn</rhyme></l>
        <l id="s9:l8" met="3" type="-+">En altijdt duerigh <rhyme label="d" type="f" corresp="#s9:l6">quellen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dus wilt wel <rhyme label="a" type="f">onderscheyden</rhyme></l>
        <l id="s10:l2" met="3" type="+-">En siet toch eens <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Een lieffelijck <rhyme label="a" type="f" corresp="#s10:l1">vermeyden</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">En dat hardt <rhyme label="b" type="m" corresp="#s10:l2">afslaen</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">O vrienden laet <rhyme label="c" type="m">altijdt</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Dit blijven in <rhyme label="d" type="f">gedachten</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">En soeckt met ganscher <rhyme label="c" type="m" corresp="#s10:l5">vlijdt</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Godts rijcke te <rhyme label="d" type="f" corresp="#s10:l6">betrachten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
