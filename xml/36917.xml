    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Livinus van der Minnen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VReught van myn <rhyme label="a" type="m">jeught</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die myn jonck hertjen <rhyme label="a" type="m" corresp="#s1:l1">verheught</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">En mynen gheest vermaeckt in eer' en <rhyme label="a" type="m" corresp="#s1:l1">deught</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Myn <rhyme label="b" type="m">Vriendin</rhyme>’</l>
        <l id="s1:l5" met="2" type="+-">die ick <rhyme label="b" type="m" corresp="#s1:l4">min</rhyme></l>
        <l id="s1:l6" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s1:l4">besin</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Die ick <rhyme label="c" type="m">vry</rhyme>’</l>
        <l id="s1:l8" met="2" type="+-">die ick <rhyme label="c" type="m" corresp="#s1:l7">vley</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Die ick <rhyme label="d" type="m">treck</rhyme>’</l>
        <l id="s1:l10" met="2" type="+-">en <rhyme label="d" type="m" corresp="#s1:l9">verweck</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Die ick <rhyme label="e" type="m">vier</rhyme>’</l>
        <l id="s1:l12" met="2" type="+-">en <rhyme label="e" type="m" corresp="#s1:l11">vercier</rhyme>,</l>
        <l id="s1:l13" met="2" type="+-">Die ick <rhyme label="f" type="m">acht</rhyme>’</l>
        <l id="s1:l14" met="2" type="+-">en <rhyme label="f" type="m" corresp="#s1:l13">verwacht</rhyme>,</l>
        <l id="s1:l15" met="5" type="-+">Voor wie ick tranen en gebeden <rhyme label="g" type="m">stort</rhyme>,</l>
        <l id="s1:l16" met="4" type="-+">Maer nimmermeer verhoort en <rhyme label="g" type="m" corresp="#s1:l15">wort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">O schoon <rhyme label="a" type="m">Persoon</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Schoon boven al die zyn <rhyme label="a" type="m" corresp="#s2:l1">schoon</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">In wie de schoonheydt staet soo schoon ten <rhyme label="a" type="m" corresp="#s2:l1">thoon</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Soo vol <rhyme label="b" type="m">lust</rhyme>’</l>
        <l id="s2:l5" met="2" type="+-">soo vol <rhyme label="b" type="m" corresp="#s2:l4">rust</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s2:l4">wellust</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Soo vol <rhyme label="c" type="m">deught</rhyme>’</l>
        <l id="s2:l8" met="2" type="+-">soo vol <rhyme label="c" type="m" corresp="#s2:l7">vreught</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Soo vol <rhyme label="d" type="m">goet</rhyme>’</l>
        <l id="s2:l10" met="2" type="+-">soo vol <rhyme label="d" type="m" corresp="#s2:l9">soet</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Soo vol <rhyme label="e" type="m">peys</rhyme>’</l>
        <l id="s2:l12" met="2" type="+-">soo vol <rhyme label="e" type="m" corresp="#s2:l11">prys</rhyme>,</l>
        <l id="s2:l13" met="2" type="+-">Soo vol <rhyme label="f" type="m">smaec</rhyme>’</l>
        <l id="s2:l14" met="2" type="+-">en <rhyme label="f" type="m" corresp="#s2:l13">vermaec</rhyme>,</l>
        <l id="s2:l15" met="5" type="-+">O liefste weest ten lesten soo <rhyme label="g" type="m">beleeft</rhyme>,</l>
        <l id="s2:l16" met="4" type="-+">Dat ghy my weder-liefde <rhyme label="g" type="m" corresp="#s2:l15">geeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Lief die ick <rhyme label="a" type="m">prys</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En voor myn Gayken <rhyme label="a" type="m" corresp="#s3:l1">vereys</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Schoon Lief op wie ick altydt denck en <rhyme label="a" type="m" corresp="#s3:l1">peys</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Oft ick <rhyme label="b" type="m">queel</rhyme>’</l>
        <l id="s3:l5" met="2" type="+-">oft ick <rhyme label="b" type="m" corresp="#s3:l4">speel</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Op de <rhyme label="b" type="m" corresp="#s3:l4">Veel</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Oft ick <rhyme label="c" type="m">gaen</rhyme>’</l>
        <l id="s3:l8" met="2" type="+-">oft ick <rhyme label="c" type="m" corresp="#s3:l7">staen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Oft ick <rhyme label="d" type="m">waeck</rhyme>’</l>
        <l id="s3:l10" met="2" type="+-">oft ick <rhyme label="d" type="m" corresp="#s3:l9">braeck</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Oft ick <rhyme label="e" type="m">hygh</rhyme>’</l>
        <l id="s3:l12" met="2" type="+-">oft ick <rhyme label="e" type="m" corresp="#s3:l11">swygh</rhyme>,</l>
        <l id="s3:l13" met="2" type="+-">Oft ick <rhyme label="f" type="m">schryf</rhyme>’</l>
        <l id="s3:l14" met="2" type="+-">oft ick <rhyme label="f" type="m" corresp="#s3:l13">vryf</rhyme>,</l>
        <l id="s3:l15" met="5" type="-+">O myn Beminde peyst oock eens op <rhyme label="g" type="m">my</rhyme>,</l>
        <l id="s3:l16" met="4" type="-+">Maer peyst wat godts en maeckt my <rhyme label="g" type="m" corresp="#s3:l15">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
