<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4080</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Nu laat ons allen God loven</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">NU laet ons dancken ende <rhyme label="a" type="f">louen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Den hoogen God van hier <rhyme label="a" type="f" corresp="#s1:l1">bouen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Die waerheyt en is niet <rhyme label="a" type="f" corresp="#s1:l1">ghelogen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Een pellicaen mach hi wel <rhyme label="c" type="f">heeten</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s1:l4">fijn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Te Nazareth is hi <rhyme label="a" type="f">ghecomen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Een tortelduue heeft hi <rhyme label="a" type="f" corresp="#s2:l1">vernomen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">De welc ontfinc tot onser <rhyme label="a" type="f" corresp="#s2:l1">vromen</rhyme>.</l>
        <l id="s2:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s2:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s2:l4">fijn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Doen nam Ioseph ter stont die <rhyme label="a" type="f">goede</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Die tortelduue in sijn <rhyme label="a" type="f" corresp="#s3:l1">behoede</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Dat wilde hi so ick <rhyme label="a" type="f" corresp="#s3:l1">beuroede</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s3:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s3:l4">fijn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Neghen maenden was <rhyme label="a" type="f">ghedraghen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Die duyue / ten sijn gheen <rhyme label="a" type="f" corresp="#s4:l1">saghen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Sij aenbaden ooc alle <rhyme label="a" type="f" corresp="#s4:l1">daghe</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s4:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s4:l4">fijn</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat huys des pays hadde hi <rhyme label="a" type="f">vercoren</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Daer in wast dat hi was <rhyme label="a" type="f" corresp="#s5:l1">gheboren</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Daer mede hielt hi dat was <rhyme label="a" type="f" corresp="#s5:l1">verloren</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s5:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s5:l4">fijn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dits eenen ghier nu comen te <rhyme label="a" type="f">voren</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Ende heeft dies in sijn herte <rhyme label="a" type="f" corresp="#s6:l1">thoren</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Ende ducht dat hem noc sal <rhyme label="a" type="f" corresp="#s6:l1">becoren</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s6:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s6:l4">fijn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sijn clauwen heeft die ghier <rhyme label="a" type="f">ontdaen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Ende wilt begripen den <rhyme label="a" type="f" corresp="#s7:l1">pellicaen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Het is om niet / hy is <rhyme label="a" type="f" corresp="#s7:l1">ontgaen</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s7:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s7:l4">fijn</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Aldaer Ioseph lach en <rhyme label="a" type="f">sliep</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Een heylich enghel tot hem <rhyme label="a" type="f" corresp="#s8:l1">riep</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Staet op Ioseph in egipten <rhyme label="a" type="f" corresp="#s8:l1">vliet</rhyme></l>
        <l id="s8:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s8:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s8:l4">fijn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Doen woude die ghier van rouwe <rhyme label="a" type="f">steruen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Om dat hi des vogelkens moeste <rhyme label="a" type="f" corresp="#s9:l1">deruen</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Menich ionck dede hi daer om <rhyme label="a" type="f" corresp="#s9:l1">steruen</rhyme></l>
        <l id="s9:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s9:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s9:l4">fijn</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Als den tijt was <rhyme label="a" type="f">ouerleden</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Van twee en dertich iaer / dat is <rhyme label="a" type="f" corresp="#s10:l1">waerhede</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Track hi tot Iherusalem der <rhyme label="a" type="f" corresp="#s10:l1">stede</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s10:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s10:l4">fijn</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Daer wert gheuaen die <rhyme label="a" type="f">pellicaen</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Ende sooch die ionghen met sinen bloede <rhyme label="a" type="f" corresp="#s11:l1">saen</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Met grooter ootmoet die hem quam <rhyme label="a" type="f" corresp="#s11:l1">aen</rhyme></l>
        <l id="s11:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Een pellicaen mach hi <rhyme label="c" type="f">wel</rhyme>.</l>
        <l id="s11:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s11:l4">fijn</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Nu laet ons bidden met herten <rhyme label="a" type="f">goet</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Dat hi ons behoede voer die helsche <rhyme label="a" type="f" corresp="#s12:l1">gloet</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Die voer ons storte sijn <rhyme label="a" type="f" corresp="#s12:l1">bloet</rhyme></l>
        <l id="s12:l4" met="2" type="-+">Dat <rhyme label="b" type="m">voghelkijn</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Een pellicaen mach hi wel <rhyme label="c" type="f">heeten</rhyme></l>
        <l id="s12:l6" met="2" type="-+">Hi is soe <rhyme label="b" type="m" corresp="#s12:l4">fijn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
