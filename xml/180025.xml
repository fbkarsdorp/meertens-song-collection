  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Aan een' welbereiden <rhyme label="a" type="m">disch</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Vind men 't geen <rhyme label="a" type="m" corresp="#s1:l1">vermaaklykis</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">'k Mag daar met een anders <rhyme label="b" type="f">gaven</rhyme></l>
        <l id="s1:l4" met="4" type="+-">'t Hart verkwikken en my <rhyme label="b" type="f" corresp="#s1:l3">laven</rhyme>;</l>
        <l id="s1:l5" met="2" type="+-">Ik drink, ik <rhyme label="c" type="m">lagch</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wyl 't my daar gebeuren <rhyme label="c" type="m" corresp="#s1:l5">mag</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Aan de Tafel en in 't <rhyme label="a" type="m">Bed</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Word op geen Crediet <rhyme label="a" type="m" corresp="#s2:l1">gelet</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">'t Is daar vry zyn' lust te <rhyme label="b" type="f">blusschen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Bagchus en de min te <rhyme label="b" type="f" corresp="#s2:l3">kusschen</rhyme>;</l>
        <l id="s2:l5" met="2" type="+-">Ik drink, ik <rhyme label="c" type="m">lagch</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Wyl 't my daar gebeuren <rhyme label="c" type="m" corresp="#s2:l5">mag</rhyme>.</l>
      </lg>
    </lg>
  </text>
