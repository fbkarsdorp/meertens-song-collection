<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161149</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ONstervelick voor <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Is den mensche <rhyme label="a" type="f" corresp="#s1:l1">ghepresen</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van Godt int Paradijs // <rhyme label="b" type="m">wijs</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gheschapen, soo wy <rhyme label="a" type="f" corresp="#s1:l1">lesen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Om eeuwelijck te <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">By den Heere <rhyme label="b" type="m" corresp="#s1:l3">propijs</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">MAer God hooghe <rhyme label="a" type="f">geseten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die heeft sonder <rhyme label="a" type="f" corresp="#s2:l1">vergeten</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Haer gegeven een Wet // <rhyme label="b" type="m">net</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van den boom niet te <rhyme label="a" type="f" corresp="#s2:l1">eten</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Daer hyt haer af liet <rhyme label="a" type="f" corresp="#s2:l1">weten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Int Paradijs <rhyme label="b" type="m" corresp="#s2:l3">geset</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">LIeten haer haest <rhyme label="a" type="f">bepraten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat zy hebben <rhyme label="a" type="f" corresp="#s3:l1">verlaten</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Al door des Duyvels raet // <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Gods Wet thaerder <rhyme label="a" type="f" corresp="#s3:l1">onbaten</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van die vruchten zy <rhyme label="a" type="f" corresp="#s3:l1">aten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat quam deurt Slangen <rhyme label="b" type="m" corresp="#s3:l3">zaet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">EN sijn alsoo <rhyme label="a" type="f">gecomen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Int verderf en <rhyme label="a" type="f" corresp="#s4:l1">verdomen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Gevallen inde doot // <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Gods Beelt is haer <rhyme label="a" type="f" corresp="#s4:l1">ontnomen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">En sagen met <rhyme label="a" type="f" corresp="#s4:l1">verschromen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Zy waren naeckt en <rhyme label="b" type="m" corresp="#s4:l3">bloot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">IN des doots <rhyme label="a" type="f">gewelde</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Waren dees al <rhyme label="a" type="f" corresp="#s5:l1">rebelde</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Gedreven naet besluyt // <rhyme label="b" type="m">uyt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat Paradijs ten <rhyme label="a" type="f" corresp="#s5:l1">Velde</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Aldaer haer Godt <rhyme label="a" type="f" corresp="#s5:l1">vertelde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met reden <rhyme label="b" type="m" corresp="#s5:l3">overluyt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">NAer dien dat ghy so <rhyme label="a" type="f">nouwe</rhyme></l>
        <l id="s6:l2" met="3" type="-+">De stemme van u <rhyme label="a" type="f" corresp="#s6:l1">vrouwe</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Gehoort hebt so sult gy // <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Die aerde moeten <rhyme label="a" type="f" corresp="#s6:l1">bouwe</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Met commer en met <rhyme label="a" type="f" corresp="#s6:l1">rouwe</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Daer op geneeren <rhyme label="b" type="m" corresp="#s6:l3">dy</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">ALs hy nu totten <rhyme label="a" type="f">wyve</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Seyde naer den <rhyme label="a" type="f" corresp="#s7:l1">beschrryve</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Nu gy die hebt gedaen // <rhyme label="b" type="m">saen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Sult ghy met veel <rhyme label="a" type="f" corresp="#s7:l1">catyve</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Tot uwen <rhyme label="a" type="f" corresp="#s7:l1">ongerijve</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dicwils worden <rhyme label="b" type="m" corresp="#s7:l3">belaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">EN ginc noch voort <rhyme label="a" type="f">verclaren</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat hy huer en den <rhyme label="a" type="f" corresp="#s8:l1">haren</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Toe senden hier int dal // <rhyme label="b" type="m">sal</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Veel smerte en <rhyme label="a" type="f" corresp="#s8:l1">beswaren</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Als sy oock soude <rhyme label="a" type="f" corresp="#s8:l1">baren</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Dit quam al door den <rhyme label="b" type="m" corresp="#s8:l3">val</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">REcht so die Heer <rhyme label="a" type="f">oorconde</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Met sijn Heyligen <rhyme label="a" type="f" corresp="#s9:l1">monde</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Also ist oock geschiet // <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Die oorsaeck is de <rhyme label="a" type="f" corresp="#s9:l1">sonde</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Dat die menschen te <rhyme label="a" type="f" corresp="#s9:l1">gronde</rhyme></l>
        <l id="s9:l6" met="3" type="-+">kommen in groot <rhyme label="b" type="m" corresp="#s9:l3">verdriet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ten helpt niet goet <rhyme label="a" type="f">geschaepen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Als men hem laet <rhyme label="a" type="f" corresp="#s10:l1">betrapen</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Vanden benijder boos // <rhyme label="b" type="m">loos</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Want die inde doot <rhyme label="a" type="f" corresp="#s10:l1">slaepen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">En gebruyken geen <rhyme label="a" type="f" corresp="#s10:l1">waepen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Tot tegenstant <rhyme label="b" type="m" corresp="#s10:l3">altoos</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">VVIe hem laet <rhyme label="a" type="f">overwinnen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Van buyten en van <rhyme label="a" type="f" corresp="#s11:l1">binnen</rhyme></l>
        <l id="s11:l3" met="4" type="+-">In dees weerelt verhaelt // <rhyme label="b" type="m">praelt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Duer vleeschelicke <rhyme label="a" type="f" corresp="#s11:l1">sinnen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">den hoogen moet <rhyme label="a" type="f" corresp="#s11:l1">beminnen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">die syn seere <rhyme label="b" type="m" corresp="#s11:l3">verdwaelt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">IN die wellust voor <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">doen veele haer <rhyme label="a" type="f" corresp="#s12:l1">vermogen</rhyme></l>
        <l id="s12:l3" met="4" type="+-">en brengen tegen twoort // <rhyme label="b" type="m">voort</rhyme>:</l>
        <l id="s12:l4" met="3" type="-+">Vruchten die niet en <rhyme label="a" type="f" corresp="#s12:l1">doogen</rhyme></l>
        <l id="s12:l5" met="3" type="-+">ijdel is al haer <rhyme label="a" type="f" corresp="#s12:l1">poogen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">alsomen siet en <rhyme label="b" type="m" corresp="#s12:l3">hoort</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">LEt op den appel <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Van die Weerelt <rhyme label="a" type="f" corresp="#s13:l1">ydoone</rhyme></l>
        <l id="s13:l3" met="4" type="+-">die menich ongetoeft // <rhyme label="b" type="m">proeft</rhyme></l>
        <l id="s13:l4" met="3" type="-+">crijgen selfs in <rhyme label="a" type="f" corresp="#s13:l1">persone</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Voor die werken te <rhyme label="a" type="f" corresp="#s13:l1">loone</rhyme></l>
        <l id="s13:l6" met="3" type="-+">dat int ende <rhyme label="b" type="m" corresp="#s13:l3">bedroeft</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">LEert mercken metten <rhyme label="a" type="f">wijsen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Die hier niet <rhyme label="a" type="f" corresp="#s14:l1">verjolijsen</rhyme></l>
        <l id="s14:l3" met="4" type="+-">dese Werelt als vrient // <rhyme label="b" type="m">dient</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Die wort (wie sal hem <rhyme label="a" type="f" corresp="#s14:l1">prijsen</rhyme>)</l>
        <l id="s14:l5" met="3" type="-+">Gods vyant met <rhyme label="a" type="f" corresp="#s14:l1">afgrijsen</rhyme></l>
        <l id="s14:l6" met="3" type="-+">dApostelen <rhyme label="b" type="m" corresp="#s14:l3">belient</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">EN soect noch vroech noch <rhyme label="a" type="f">spade</rhyme></l>
        <l id="s15:l2" met="3" type="-+">u eygen Ziele <rhyme label="a" type="f" corresp="#s15:l1">schade</rhyme></l>
        <l id="s15:l3" met="4" type="+-">Om verquisten u deel // <rhyme label="b" type="m">heel</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Want God vol van <rhyme label="a" type="f" corresp="#s15:l1">genade</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Heeft ons geslegen <rhyme label="a" type="f" corresp="#s15:l1">gade</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Als zijn weerdich <rhyme label="b" type="m" corresp="#s15:l3">juweel</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">MEt vreesen en met <rhyme label="a" type="f">beven</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Moeten wy altijt <rhyme label="a" type="f" corresp="#s16:l1">geven</rhyme></l>
        <l id="s16:l3" met="4" type="+-">Den Heer loff en danck // <rhyme label="b" type="m">stranck</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Dat wy ons uyt dat <rhyme label="a" type="f" corresp="#s16:l1">sneven</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Gebrocht heeft tot den <rhyme label="a" type="f" corresp="#s16:l1">leven</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Der eeuwicheyt seer <rhyme label="b" type="m" corresp="#s16:l3">lanc</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">SOnder Gods liefde <rhyme label="a" type="f">reene</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Waren wy al <rhyme label="a" type="f" corresp="#s17:l1">gemeene</rhyme></l>
        <l id="s17:l3" met="4" type="+-">Als gevangen gepast // <rhyme label="b" type="m">vast</rhyme></l>
        <l id="s17:l4" met="3" type="-+">In verdriet en in <rhyme label="a" type="f" corresp="#s17:l1">weene</rhyme></l>
        <l id="s17:l5" met="3" type="-+">Want daer en was niet <rhyme label="a" type="f" corresp="#s17:l1">eene</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Die niet en was <rhyme label="b" type="m" corresp="#s17:l3">belast</rhyme>,</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">OM ons uyt dat <rhyme label="a" type="f">verderven</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Te brengen tzijnder <rhyme label="a" type="f" corresp="#s18:l1">erven</rhyme>,</l>
        <l id="s18:l3" met="4" type="+-">Inden Hemel eenpaer // <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Soo moste Christum <rhyme label="a" type="f" corresp="#s18:l1">sterven</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Eer wy conden <rhyme label="a" type="f" corresp="#s18:l1">verwerven</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Tleven by hem hier <rhyme label="b" type="m" corresp="#s18:l3">naer</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">OFt elck wel <rhyme label="a" type="f">bedochte</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Wat dat Christum al <rhyme label="a" type="f" corresp="#s19:l1">wrochte</rhyme>,</l>
        <l id="s19:l3" met="4" type="+-">In dese Werelt zwaer // <rhyme label="b" type="m">maer</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Dat verloren hy <rhyme label="a" type="f" corresp="#s19:l1">sochte</rhyme></l>
        <l id="s19:l5" met="3" type="-+">En uyt liefde hy <rhyme label="a" type="f" corresp="#s19:l1">kochte</rhyme></l>
        <l id="s19:l6" met="3" type="-+">Ons met zijn bloet <rhyme label="b" type="m" corresp="#s19:l3">dierbaer</rhyme>,</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">NIemant en moet nu <rhyme label="a" type="f">klagen</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">Dat hy die schult moet <rhyme label="a" type="f" corresp="#s20:l1">dragen</rhyme></l>
        <l id="s20:l3" met="4" type="+-">Wie dat hem hier met vlijt // <rhyme label="b" type="m">quijt</rhyme></l>
        <l id="s20:l4" met="3" type="-+">En Godt soeckt te <rhyme label="a" type="f" corresp="#s20:l1">behagen</rhyme></l>
        <l id="s20:l5" met="3" type="-+">Die moet hem al zijn <rhyme label="a" type="f" corresp="#s20:l1">dagen</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Recht, Schicken naer den <rhyme label="b" type="m" corresp="#s20:l3">tijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
