<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>114886</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Sa voerman Job</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIe hier wat suynich is in 't <rhyme label="a" type="f">sparen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Daer van de oud en koude <rhyme label="b" type="m">Man</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Als hy niet wint, mach wel af <rhyme label="a" type="f" corresp="#s1:l1">varen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Van soo een houdt men weynigh <rhyme label="b" type="m" corresp="#s1:l2">van</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Maer die sijn goet ten besten <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Bevindt dat hy veel Vrienden <rhyme label="c" type="m" corresp="#s1:l5">heeft</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Bevindt dat hy veel Vrienden <rhyme label="c" type="m" corresp="#s1:l5">heeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die lustigh houdt van lecker <rhyme label="a" type="f">smullen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En nimmer sijne Gasten <rhyme label="b" type="m">telt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer alle weet de buyck te <rhyme label="a" type="f" corresp="#s2:l1">vullen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Als hy die aen sijn Tafel <rhyme label="b" type="m" corresp="#s2:l2">stelt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die heeft de toe-loop <rhyme label="c" type="m">al-te-mael</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Tot aen sijn doodt, wordt hy niet <rhyme label="c" type="m" corresp="#s2:l5">kael</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Tot aen sijn doodt, wordt hy niet <rhyme label="c" type="m" corresp="#s2:l5">kael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dit heeft aen Hommelus <rhyme label="a" type="f">gebleecken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die maeckte dagh aen dagh goet <rhyme label="b" type="m">cier</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En wist van krimpen niet te <rhyme label="a" type="f" corresp="#s3:l1">spreecken</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maer van een glas vol Wijn of <rhyme label="b" type="m" corresp="#s3:l2">Bier</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Met lecker eeten op den <rhyme label="c" type="m">Dis</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Voor elck die in 't geselschap <rhyme label="c" type="m" corresp="#s3:l5">is</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Voor elck die in 't geselschap <rhyme label="c" type="m" corresp="#s3:l5">is</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een yeder soeckt na hem te <rhyme label="a" type="f">hellen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Want soo een Waert men selden <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">De Buyck moet daer gedurig <rhyme label="a" type="f" corresp="#s4:l1">swellen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En in 't gelach en schrijft men <rhyme label="b" type="m" corresp="#s4:l2">niet</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">'t Is al, sie daer een volle <rhyme label="c" type="m">Kan</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">De Waert die wint'er weynigh <rhyme label="c" type="m" corresp="#s4:l5">an</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">De Waert die wint'er weynigh <rhyme label="c" type="m" corresp="#s4:l5">an</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Laet ons te saem eens vrolijck <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Ter eeren van dat soet <rhyme label="b" type="m">gelagh</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En lustigh met malkander <rhyme label="a" type="f" corresp="#s5:l1">springen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Van nu tot aen den lichten <rhyme label="b" type="m" corresp="#s5:l2">dagh</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Want hier en past nu geen <rhyme label="c" type="m">getreur</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Te meer de Meester ons gaet <rhyme label="c" type="m" corresp="#s5:l5">veur</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Te meer de Meester ons gaet <rhyme label="c" type="m" corresp="#s5:l5">veur</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Komt (seyt hy) Gasten wilt niet <rhyme label="a" type="f">swichten</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ghy zijt my alle lief en <rhyme label="b" type="m">weert</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Die best de Kroes weet op de <rhyme label="a" type="f" corresp="#s6:l1">lichten</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">By my het minste gelt <rhyme label="b" type="m" corresp="#s6:l2">verteert</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Want ick heb Fortunatus <rhyme label="c" type="m">beurs</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Ten kost u Mantel, Broeck, noch <rhyme label="c" type="m" corresp="#s6:l5">Keurs</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Ten kost u Mantel, Broeck, noch <rhyme label="c" type="m" corresp="#s6:l5">Keurs</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sa Speel-luy, wilt een deuntjen <rhyme label="a" type="f">speelen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Kom maeck voor ons een soet <rhyme label="b" type="m">accoort</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Stel Luyt en Fluyt, en Bas en <rhyme label="a" type="f" corresp="#s7:l1">Veelen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Te saem in een, soo dat <rhyme label="b" type="m" corresp="#s7:l2">behoort</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Want dit geselschap is het <rhyme label="c" type="m">weert</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Sy slapen selden aen mijn <rhyme label="c" type="m" corresp="#s7:l5">heert</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Sy slapen selden aen mijn <rhyme label="c" type="m" corresp="#s7:l5">heert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maer Hommelus die moet eerst <rhyme label="a" type="f">scheyden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">De doodt die roept hem aen de <rhyme label="b" type="m">Poort</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Komt, Vrienden, wilt my nu <rhyme label="a" type="f" corresp="#s8:l1">geleyden</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ey komt, en gaet wat met my <rhyme label="b" type="m" corresp="#s8:l2">voort</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">De tijdt gebiedt my nu te <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Dit droeve padt, blijft doch niet <rhyme label="c" type="m" corresp="#s8:l5">staen</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Dit droeve padt, blijft doch niet <rhyme label="c" type="m" corresp="#s8:l5">staen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer neen, sy alle staen te <rhyme label="a" type="f">dralen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En seggen, dat en past ons <rhyme label="b" type="m">niet</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">'t Is maer alleen om u te <rhyme label="a" type="f" corresp="#s9:l1">halen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Wy houden ons uyt dit <rhyme label="b" type="m" corresp="#s9:l2">verdriet</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Ghy meught in Vreed' wel heenen <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Wy blijven hier, en daer van <rhyme label="c" type="m" corresp="#s9:l5">daen</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Wy blijven hier, en daer van <rhyme label="c" type="m" corresp="#s9:l5">daen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dit is de danck voor al sijn <rhyme label="a" type="f">gaven</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Hy krijght van haer des Werelts <rhyme label="b" type="m">loon</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Of hy haer dickmael plach te <rhyme label="a" type="f" corresp="#s10:l1">laven</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Die Vriendschap achten sy geen <rhyme label="b" type="m" corresp="#s10:l2">boon</rhyme>:</l>
        <l id="s10:l5" met="4" type="-+">Als hier het geven is <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Dan sietmen vaeck de Vrienden <rhyme label="c" type="m" corresp="#s10:l5">gaen</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Dan sietmen vaeck de Vrienden <rhyme label="c" type="m" corresp="#s10:l5">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
