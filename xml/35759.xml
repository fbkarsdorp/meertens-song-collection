<song>
  <meta>
    <id>35759</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>O liefste lief geprezen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MIjn Iesu, mijn <rhyme label="a" type="f">Behoeder</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gunt my op dese <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ick van uwe <rhyme label="a" type="f" corresp="#s1:l1">Moeder</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Oprecht verhale <rhyme label="b" type="m" corresp="#s1:l2">mach</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Hoe sy is <rhyme label="c" type="f">op-genomen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hoe sy heden <rhyme label="c" type="f" corresp="#s1:l5">gekomen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Is, in het 's Hemels <rhyme label="d" type="m">hof</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Met vreughden, eer en <rhyme label="d" type="m" corresp="#s1:l7">lof</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maria oudt van <rhyme label="a" type="f">Iaren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met deughden al <rhyme label="b" type="m">vervult</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Goodts Engel hoort <rhyme label="a" type="f" corresp="#s2:l1">verklaren</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Weet dat ghy sterve <rhyme label="b" type="m" corresp="#s2:l2">sult</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Palm-tack heeft haer <rhyme label="c" type="f">gegeven</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Om dat sy in haer <rhyme label="c" type="f" corresp="#s2:l5">leven</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Verwonnen had den <rhyme label="d" type="m">Draeck</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">'t Vleesch, en 's Werelts <rhyme label="d" type="m" corresp="#s2:l7">vermaeck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Door al de Werelds <rhyme label="a" type="f">hoecken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">D'Apostelen <rhyme label="b" type="m">verspreyt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met Iesus haer <rhyme label="a" type="f" corresp="#s3:l1">besoecken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Eer dat sy van hier <rhyme label="b" type="m" corresp="#s3:l2">scheydt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Met blijdschap is <rhyme label="c" type="f">gestorven</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Haer lichaem niet <rhyme label="c" type="f" corresp="#s3:l5">bedorven</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Is, al leyt het in 't <rhyme label="d" type="m">graf</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Al is de ziel daer <rhyme label="d" type="m" corresp="#s3:l7">af</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">d' Eng'len en <rhyme label="a" type="f">Seraphijnen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En Mogentheden <rhyme label="b" type="m">bly</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Throonen en <rhyme label="a" type="f" corresp="#s4:l1">Cherubijnen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">d'And're Chooren daer <rhyme label="b" type="m" corresp="#s4:l2">by</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Met vreught op d'aerde <rhyme label="c" type="f">dalen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Om tot 'Hemelsche <rhyme label="c" type="f" corresp="#s4:l5">zalen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Mariaes lijf en <rhyme label="d" type="m">geest</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Te leyden in dit <rhyme label="d" type="m" corresp="#s4:l7">feest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Jesus, den Heer der <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laet zijn Engelen <rhyme label="b" type="m">schaer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Om sijn Moeder te <rhyme label="a" type="f" corresp="#s5:l1">eeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Singen in 't <rhyme label="b" type="m" corresp="#s5:l2">openbaer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hy laet sijn Moeder <rhyme label="c" type="f">steunen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En op sijn schoud'ren <rhyme label="c" type="f" corresp="#s5:l5">leunen</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Alsoo dit waerste <rhyme label="d" type="m">pandt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Geraeckt tot 's Hemels <rhyme label="d" type="m" corresp="#s5:l7">landt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Den Vader haer <rhyme label="a" type="f">ontmoete</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Met lof, en grooten <rhyme label="b" type="m">eer</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">d' Heylige Geest haer <rhyme label="a" type="f" corresp="#s6:l1">groete</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Soo doet haer Soon, haer <rhyme label="b" type="m" corresp="#s6:l2">Heer</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">De vroome <rhyme label="c" type="f">Martelaren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ende al Godes <rhyme label="c" type="f" corresp="#s6:l5">scharen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die vallen haer te <rhyme label="d" type="m">voet</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En singen: Weest <rhyme label="d" type="m" corresp="#s6:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Een kroon is haer <rhyme label="a" type="f">gegeven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Een kroon op 't heyligh <rhyme label="b" type="m">hooft</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Met Sterren vijf en <rhyme label="a" type="f" corresp="#s7:l1">seven</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wiens klaerheydt noyt <rhyme label="b" type="m" corresp="#s7:l2">verdooft</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En met de Zon haer <rhyme label="c" type="f">leden</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Zijn mede bekleedt <rhyme label="c" type="f" corresp="#s7:l5">heden</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Daer by haer voete <rhyme label="d" type="m">staen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Boven de klare <rhyme label="d" type="m" corresp="#s7:l7">Maen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Boven d'Engele <rhyme label="a" type="f">Thooren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Boven de Maeghden <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ia, al Gods <rhyme label="a" type="f" corresp="#s8:l1">uyt-verkooren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ontfangen heeft haer <rhyme label="b" type="m" corresp="#s8:l2">throon</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Aen Christi rechter <rhyme label="c" type="f">zijde</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Zij in 't Hemels <rhyme label="c" type="f" corresp="#s8:l5">verblijde</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Ontfanght de meeste <rhyme label="d" type="m">vreught</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Tot loon van hare <rhyme label="d" type="m" corresp="#s8:l7">deught</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Valt neder op der <rhyme label="a" type="f">aerde</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Catholijcken, en <rhyme label="b" type="m">prijst</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die Maeght, die Iesum <rhyme label="a" type="f" corresp="#s9:l1">baerde</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En grooten eer <rhyme label="b" type="m" corresp="#s9:l2">bewijst</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Uw' Hemels <rhyme label="c" type="f">Coninghinne</rhyme>;</l>
        <l id="s9:l6" met="3" type="-+">Naest Godt wilt haer <rhyme label="c" type="f" corresp="#s9:l5">beminne</rhyme>;</l>
        <l id="s9:l7" met="3" type="-+">Weet, weet dat Godt <rhyme label="d" type="m">behaeght</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Den lof van dese <rhyme label="d" type="m" corresp="#s9:l7">Maeght</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Soeckt ghy met ziel en <rhyme label="a" type="f">leden</rhyme></l>
        <l id="s10:l2" met="3" type="-+">In 's Hemels hof een <rhyme label="b" type="m">plaets</rhyme>?</l>
        <l id="s10:l3" met="3" type="-+">Haet 's wereldts <rhyme label="a" type="f" corresp="#s10:l1">ydelheden</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Loopt noyt na 't sondigh <rhyme label="b" type="m" corresp="#s10:l2">aes</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Na deughden al wilt <rhyme label="c" type="f">trachten</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Dient Godt, by dagh en <rhyme label="c" type="f" corresp="#s10:l5">nachten</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Doet noyt dat ghy <rhyme label="d" type="m">beklaeght</rhyme>;</l>
        <l id="s10:l8" met="3" type="-+">Doet noyt dat Godt <rhyme label="d" type="m" corresp="#s10:l7">mis-haeght</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Moeder! door uw <rhyme label="a" type="f">gebeden</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Verkrijcht van uwe <rhyme label="b" type="m">Soon</rhyme>:</l>
        <l id="s11:l3" met="3" type="-+">Dat wy ons hart en <rhyme label="a" type="f" corresp="#s11:l1">leden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Van sonden houwe <rhyme label="b" type="m" corresp="#s11:l2">schoon</rhyme>:</l>
        <l id="s11:l5" met="3" type="-+">Dat wy de boosheydt <rhyme label="c" type="f">vluchten</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Voort brengen goede <rhyme label="c" type="f" corresp="#s11:l5">vruchten</rhyme>.</l>
        <l id="s11:l7" met="3" type="-+">Versoent u Soon <rhyme label="d" type="m">verstoort</rhyme>;</l>
        <l id="s11:l8" met="3" type="-+">Weest onse Hemels <rhyme label="d" type="m" corresp="#s11:l7">poort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
