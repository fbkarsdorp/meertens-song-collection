    <melody>La Guillote</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GOeyen avond Betje <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">'k Zaye kik kik kik auw <rhyme label="b" type="f">Slaave</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">'k Staay tout jours tot auw <rhyme label="a" type="m" corresp="#s1:l1">gerief</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">'k Hauwe kik 'et voor een <rhyme label="b" type="f" corresp="#s1:l2">grave</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Datte kik Me-Uffrauw diene <rhyme label="c" type="m">mach</rhyme>:</l>
        <l id="s1:l6" met="2" type="-+">Ha, ha, ha, <rhyme label="c" type="m" corresp="#s1:l5">ach</rhyme>!</l>
        <l id="s1:l7" met="2" type="-+">Ik barst van <rhyme label="c" type="m" corresp="#s1:l5">lach</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Dat om jou drollig <rhyme label="c" type="m" corresp="#s1:l5">geprach</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">En waytze <rhyme label="d" type="f">Complementen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ke Madam <rhyme label="a" type="m">Elizabeth</rhyme>!</l>
        <l id="s2:l2" met="4" type="+-">Gay weet ierst byzart te <rhyme label="b" type="f">leven</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Dat's wear vaar, dear raed j'et <rhyme label="a" type="m" corresp="#s2:l1">net</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Wilt 'er zoo wat after <rhyme label="b" type="f" corresp="#s2:l2">geven</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Ma foy, gay zayd <rhyme label="c" type="m">Certaynement</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+"><rhyme label="c" type="m" corresp="#s2:l5">Extravagant</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Vif, en <rhyme label="c" type="m" corresp="#s2:l5">Playzant</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Ja heel <rhyme label="c" type="m" corresp="#s2:l5">Admirabelemant</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Merveilljeux Sans <rhyme label="d" type="f">Pareillje</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wel <rhyme label="a" type="m">Schijt-valkze-Kortezaan</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Reutelje, of benje <rhyme label="b" type="f">dronken</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">De Droely mocht jou <rhyme label="a" type="m" corresp="#s3:l1">verstaan</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Nou, ik zeg je weet van <rhyme label="b" type="f" corresp="#s3:l2">pronken</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dusken Hoed op 't nieuw <rhyme label="c" type="m">fatzoen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Je Kousjes <rhyme label="c" type="m" corresp="#s3:l5">groen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Met spitze <rhyme label="c" type="m" corresp="#s3:l5">Schoen</rhyme>?</l>
        <l id="s3:l8" met="4" type="-+">Quant, hoe moet jou veurbroek <rhyme label="c" type="m" corresp="#s3:l5">broÃªn</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Van al die strikk' en <rhyme label="d" type="f">quikke</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Eloquente rare <rhyme label="a" type="m">Maagd</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ba! gay discoureert <rhyme label="b" type="f">alard'kens</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'k Wed de Faam auw lof <rhyme label="a" type="m" corresp="#s4:l1">omdraagt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Gelayk onze Sgill' de <rhyme label="b" type="f" corresp="#s4:l2">Taartkens</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">'t Senseer Gracieusze <rhyme label="c" type="m">Goddin</rhyme>!</l>
        <l id="s4:l6" met="2" type="-+">Gay dwingt tot <rhyme label="c" type="m" corresp="#s4:l5">Min</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Mayn hart en <rhyme label="c" type="m" corresp="#s4:l5">zin</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Door auw Gesten: die kik <rhyme label="c" type="m" corresp="#s4:l5">vin'</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Gentiel en <rhyme label="d" type="f">agreable</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Trouwens Iaap, ik zeg jey <rhyme label="a" type="m">keunt</rhyme>!</l>
        <l id="s5:l2" met="4" type="+-">Waar leer jey die Hoofze <rhyme label="b" type="f">streeken</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">'k Payz Lief dae ge med may <rhyme label="a" type="m" corresp="#s5:l1">deunt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">'t Zayn mayn aige <rhyme label="b" type="f" corresp="#s5:l2">Minne-treeken</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Is dat meugelijk? Bay ja <rhyme label="c" type="m">'t</rhyme>!</l>
        <l id="s5:l6" met="2" type="-+">Maar watje <rhyme label="c" type="m" corresp="#s5:l5">praat</rhyme>!</l>
        <l id="s5:l7" met="2" type="-+">'t Es in der <rhyme label="c" type="m" corresp="#s5:l5">daat</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Moar een Cordiaal <rhyme label="c" type="m" corresp="#s5:l5">Bravaat</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">En nobele <rhyme label="d" type="f">Galyardiesse</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ach! Betje mayn zoete <rhyme label="a" type="m">bek</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Late kik auw toch eins <rhyme label="b" type="f">kuffen</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Houje hangde veurje, <rhyme label="a" type="m" corresp="#s6:l1">Gek</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Ay! laat ons te zamen <rhyme label="b" type="f" corresp="#s6:l2">bluffen</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Onzen <rhyme label="c" type="m">Amoureuze-brand</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Ke daar 's mayn <rhyme label="c" type="m" corresp="#s6:l5">hand</rhyme></l>
        <l id="s6:l7" met="2" type="-+">En trauw op <rhyme label="c" type="m" corresp="#s6:l5">pant</rhyme>.</l>
        <l id="s6:l8" met="4" type="-+">Loop, Gek loop, vrijd in jou <rhyme label="c" type="m" corresp="#s6:l5">Langt</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Daers mult van <rhyme label="d" type="f">Venus-mussen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
