  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">MArs un jour &amp; l'amour à <rhyme label="a" type="f">Cythere</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Prirent puerelle tous les <rhyme label="b" type="m">deux</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">L'amour luy dit je <rhyme label="b" type="m" corresp="#s1:l2">veux</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Te declarer la <rhyme label="a" type="f" corresp="#s1:l1">guerre</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Le dieu <rhyme label="c" type="m">Mars</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Prend ses <rhyme label="c" type="m" corresp="#s1:l5">dards</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Sa <rhyme label="d" type="f">cuirace</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Et l'amour tout <rhyme label="e" type="m">désarmé</rhyme>:</l>
        <l id="s1:l9" met="4" type="+-">Loing d'en être <rhyme label="e" type="m" corresp="#s1:l8">allarmé</rhyme>,</l>
        <l id="s1:l10" met="1" type="-+"><rhyme label="d" type="f" corresp="#s1:l7">Menace</rhyme>,</l>
        <l id="s1:l11" met="3" type="-+">Mars au combat <rhyme label="f" type="f">l'appelle</rhyme>;</l>
        <l id="s1:l12" met="3" type="-+">Cupidon d'un coup d <rhyme label="f" type="f" corresp="#s1:l11">aille</rhyme>,</l>
        <l id="s1:l13" met="2" type="+-">Rend ses <rhyme label="g" type="m">traits</rhyme>,</l>
        <l id="s1:l14" met="2" type="+-">Sans <rhyme label="g" type="m" corresp="#s1:l13">Effect</rhyme>:</l>
        <l id="s1:l15" met="2" type="+-">Et <rhyme label="h" type="f">Balance</rhyme>,</l>
        <l id="s1:l16" met="2" type="+-">Sa <rhyme label="h" type="f" corresp="#s1:l15">puissance</rhyme>,</l>
        <l id="s1:l17" met="4" type="+-">Dans le coeur du dieu <rhyme label="i" type="m">guerrier</rhyme>,</l>
        <l id="s1:l18" met="3" type="+-">L'amour d'un vole <rhyme label="i" type="m" corresp="#s1:l17">altier</rhyme>,</l>
        <l id="s1:l19" met="3" type="+-">Luy même tout <rhyme label="i" type="m" corresp="#s1:l17">entier</rhyme>:</l>
        <l id="s1:l20" met="1" type="+-">Se <rhyme label="h" type="f" corresp="#s1:l15">lance</rhyme>,</l>
        <l id="s1:l21" met="2" type="+-">Mars en <rhyme label="j" type="m">feux</rhyme>,</l>
        <l id="s1:l22" met="2" type="+-">Sent ce <rhyme label="j" type="m" corresp="#s1:l21">Dieu</rhyme>,</l>
        <l id="s1:l23" met="2" type="+-">Dans son <rhyme label="k" type="f">Ame</rhyme>:</l>
        <l id="s1:l24" met="3" type="-+">Et l'enfant <rhyme label="j" type="m" corresp="#s1:l21">audacieux</rhyme>,</l>
        <l id="s1:l25" met="3" type="-+">A laissé dans ces <rhyme label="j" type="m" corresp="#s1:l21">yeux</rhyme>;</l>
        <l id="s1:l26" met="3" type="-+">Son charme &amp; sa <rhyme label="k" type="f" corresp="#s1:l23">flamme</rhyme>,</l>
        <l id="s1:l27" met="2" type="+-">Mars <rhyme label="l" type="m">soumis</rhyme>,</l>
        <l id="s1:l28" met="3" type="-+">En apris plus <rhyme label="m" type="f">d'Empire</rhyme>,</l>
        <l id="s1:l29" met="4" type="+-">A present tot Cede a <rhyme label="n" type="m">Mars</rhyme>,</l>
        <l id="s1:l30" met="2" type="+-">Qui <rhyme label="o" type="m">soutient</rhyme>,</l>
        <l id="s1:l31" met="2" type="+-">Ses <rhyme label="n" type="m" corresp="#s1:l29">Regards</rhyme>,</l>
        <l id="s1:l32" met="1" type="-+"><rhyme label="m" type="f" corresp="#s1:l28">Soupire</rhyme>.</l>
      </lg>
    </lg>
  </text>
