    <tags/>
    <melody>Chambrière chambrière</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WInd hier om te <rhyme label="a" type="f">vermaken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">goede vrienden met d' <rhyme label="b" type="m">bron</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Nu sa lick d' Wijn gaen <rhyme label="a" type="f" corresp="#s1:l1">smaken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En eens drincken ronten <rhyme label="b" type="m" corresp="#s1:l2">tom</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dese Wijn salons van <rhyme label="c" type="f">binnen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Vrolijck maken wel <rhyme label="d" type="m">gesint</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Haelt pot en <rhyme label="d" type="m" corresp="#s1:l6">pint</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Maer dan moeten onse <rhyme label="c" type="f" corresp="#s1:l5">sinnen</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Draeyen na de Ooste <rhyme label="d" type="m" corresp="#s1:l6">wint</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hoort gebueren ick gae <rhyme label="a" type="f">Nincken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Is sante van 't <rhyme label="b" type="m">Compagnie</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ick sal 't wijntjen laten <rhyme label="a" type="f" corresp="#s2:l1">sincken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maer zijt ghy een viese <rhyme label="b" type="m" corresp="#s2:l2">pry</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Ick lest 't glaesjen <rhyme label="c" type="f">achterdoge</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Let op my o goede <rhyme label="d" type="m">lien</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Wat sal <rhyme label="d" type="m" corresp="#s2:l6">geschien</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Dan sal 't glaesjen werde <rhyme label="c" type="f" corresp="#s2:l5">drogen</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Als ick na den Hemel <rhyme label="d" type="m" corresp="#s2:l6">sien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Siet ick heb het <rhyme label="a" type="f">uytgedroncken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wat bibadt de <rhyme label="b" type="m">sante</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wilt nu op een ander <rhyme label="a" type="f" corresp="#s3:l1">loncken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Drinckt het uyt en leeft in <rhyme label="b" type="m" corresp="#s3:l2">vree</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Drinck het uyt en laet het <rhyme label="c" type="f">lopen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ach 't is sulken lieflijck <rhyme label="d" type="m">nat</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Daer is meer in 't <rhyme label="d" type="m" corresp="#s3:l6">vat</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En gelt om een moey te <rhyme label="c" type="f" corresp="#s3:l5">kopen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Lapt het nat in 't kele <rhyme label="d" type="m" corresp="#s3:l6">gat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
