    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ach vaders! soo ick <rhyme label="a" type="m">wouw</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met een oprecht <rhyme label="a" type="m" corresp="#s1:l1">berouw</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Aenroepen 's Hemels <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met hertelijcke <rhyme label="b" type="m" corresp="#s1:l3">klacht</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Souw Godt wel mijn <rhyme label="c" type="m">misdaet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En grouwelijck <rhyme label="c" type="m" corresp="#s1:l5">verraet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Vergeven uyt <rhyme label="d" type="m">gena</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Soo ick u volghde <rhyme label="d" type="m" corresp="#s1:l7">na</rhyme>?</l>
