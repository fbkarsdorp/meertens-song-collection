    <melody>Psalm 009 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEER ick wil v wt s'herten <rhyme label="a" type="m">grondt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Prysen end ouer al doen <rhyme label="a" type="m" corresp="#s1:l1">condt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">V' wonderen in alle <rhyme label="b" type="f">wycken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die niet en sijn om <rhyme label="b" type="f" corresp="#s1:l3">vergelijcken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In v wil ick wesen <rhyme label="a" type="m">verblydt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dat is myn vrueghd tot deser <rhyme label="a" type="m" corresp="#s2:l1">tydt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Vwen naem schoon wil ick oock <rhyme label="b" type="f">prysen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">End v met lofsangh eer' <rhyme label="b" type="f" corresp="#s2:l3">bewysen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Om dat doer uwe groote <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Myn viandt wyckt end vliedt met <rhyme label="a" type="m" corresp="#s3:l1">kracht</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End dat hy neder light <rhyme label="b" type="f">geslagen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Doer v aenschijn swaer om <rhyme label="b" type="f" corresp="#s3:l3">verdraegen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy sijt so goet in desen <rhyme label="a" type="m">stand</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat ghy myn saeck neemt by der <rhyme label="a" type="m" corresp="#s4:l1">hand</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ghy sitt tot myn hulpe <rhyme label="b" type="f">volueerdigh</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Op den stoel als Rechter <rhyme label="b" type="f" corresp="#s4:l3">rechtueerdigh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Myn vianden hebt ghy <rhyme label="a" type="m">verdaen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">End die boos' al haest doen <rhyme label="a" type="m" corresp="#s5:l1">vergaen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ghy hebt haer gedachtenis <rhyme label="b" type="f">prachtigh</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Te niet gedaen doer v hand <rhyme label="b" type="f" corresp="#s5:l3">krachtigh</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wel aen ghy loos mensch wel <rhyme label="a" type="m">bekent</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Hebt ghy v voornemen <rhyme label="a" type="m" corresp="#s6:l1">voleyndt</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">End vernielt ons steden <rhyme label="b" type="f">verheuen</rhyme>?</l>
        <l id="s6:l4" met="4" type="-+">Is haeren naem gantsch t'onder <rhyme label="b" type="f" corresp="#s6:l3">bleuen</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Och neen: want Godt met grooter <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die eewigh herschet hoogh <rhyme label="a" type="m" corresp="#s7:l1">geacht</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Is op den stoel soo't heeft <rhyme label="b" type="f">gebleken</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Geseten, om t'recht wt te <rhyme label="b" type="f" corresp="#s7:l3">spreken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer sal hy rechten met <rhyme label="a" type="m">bescheydt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">D' eerdryck dat hier beneden <rhyme label="a" type="m" corresp="#s8:l1">leydt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Wegende die saecken met <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Van alle menschen hier <rhyme label="b" type="f" corresp="#s8:l3">beneden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Daer werdt Godt die toevlucht <rhyme label="a" type="m">alleyn</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Des armen die men acht seer <rhyme label="a" type="m" corresp="#s9:l1">kleyn</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Iae sijn toevlucht die hem in't <rhyme label="b" type="f">lyden</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Sal verquicken end doen <rhyme label="b" type="f" corresp="#s9:l3">verblyden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dies sy, dien ghy Heer v maeckt <rhyme label="a" type="m">condt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Sullen op v vast staen <rhyme label="a" type="m" corresp="#s10:l1">ghegrondt</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Want die tot v gaen (Heer <rhyme label="b" type="f">gepresen</rhyme>)</l>
        <l id="s10:l4" met="4" type="-+">Sullen hier niet verlaeten <rhyme label="b" type="f" corresp="#s10:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Loeft nu met lofsangen seer <rhyme label="a" type="m">klaer</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Godt die tot Zion woent <rhyme label="a" type="m" corresp="#s11:l1">eenpaer</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Vertelt sijn groote <rhyme label="b" type="f">wonderwercken</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Maeckt dats' alle menschen <rhyme label="b" type="f" corresp="#s11:l3">bemercken</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Hy gedenckt aen der vromen <rhyme label="a" type="m">bloet</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">End wreeckt dat met een gram <rhyme label="a" type="m" corresp="#s12:l1">ghemoet</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Dat geschrey wil hy niet <rhyme label="b" type="f">verachten</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Der benaudder, noch oock haer <rhyme label="b" type="f" corresp="#s12:l3">klachten</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Heer ghy die myn Godt sijt in <rhyme label="a" type="m">noot</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Siet aen myn kruys end lyden <rhyme label="a" type="m" corresp="#s13:l1">groot</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Dat myn vianden my <rhyme label="b" type="f">opdringen</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Wt den wegh des doods wilt my <rhyme label="b" type="f" corresp="#s13:l3">bringen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Dat ick midden in v <rhyme label="a" type="m">gemeyn</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Vwen lof singh' end anders <rhyme label="a" type="m" corresp="#s14:l1">gheyn</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Synde verblydt end oock <rhyme label="b" type="f">ghedachtigh</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Dat ghy my verlost hebt <rhyme label="b" type="f" corresp="#s14:l3">waerachtigh</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Die boose sijn haestelick <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">In haer stricken kommen ten <rhyme label="a" type="m" corresp="#s15:l1">val</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Haer voeten syn in't net <rhyme label="b" type="f">gheuangen</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Dat sy voor my hadden <rhyme label="b" type="f" corresp="#s15:l3">ghehangen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">So is Godt geworden <rhyme label="a" type="m">bekent</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Hebbende dit oordeel <rhyme label="a" type="m" corresp="#s16:l1">gheeynt</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Daerin dat die schalk heeft <rhyme label="b" type="f">beuonden</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">T'quaet syner listen niet om <rhyme label="b" type="f" corresp="#s16:l3">gronden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Dits seker dat die boose <rhyme label="a" type="m">saen</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Haest sullen vallen end <rhyme label="a" type="m" corresp="#s17:l1">vergaen</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Godt sal die straffen <rhyme label="b" type="f">onuertoogen</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Die hem niet en hebben voor <rhyme label="b" type="f" corresp="#s17:l3">oogen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Maer den mensch ootmoedigh O <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Sult ghy vergheten <rhyme label="a" type="m" corresp="#s18:l1">nemmermeer</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Syn hope die hier is <rhyme label="b" type="f">mispresen</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">En sal by v' niet ydel <rhyme label="b" type="f" corresp="#s18:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Komt doch Heer toont nu v <rhyme label="a" type="m">gewelt</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Op dat de mensch' hem niet en <rhyme label="a" type="m" corresp="#s19:l1">stelt</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Teghen v, maer dat alle <rhyme label="b" type="f">scharen</rhyme></l>
        <l id="s19:l4" met="4" type="-+">Voor v gericht' hen <rhyme label="b" type="f" corresp="#s19:l3">openbaren</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">O Heer die daer eewighlick <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s20:l2" met="4" type="-+">Maeckt dat der boosen herte <rhyme label="a" type="m" corresp="#s20:l1">beeft</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">Doet hen verstaen, dit is myn <rhyme label="b" type="f">wenschen</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">Dat sy niet sijn dan swacke <rhyme label="b" type="f" corresp="#s20:l3">menschen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
