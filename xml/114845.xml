  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">CAtharina vander <rhyme label="a" type="f">Senen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wonder hebdy Godt <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die, van liefde heel <rhyme label="a" type="f" corresp="#s1:l1">verdwenen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Waert op hem alleen <rhyme label="b" type="m" corresp="#s1:l2">ghesint</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">All' u crachten // hem <rhyme label="c" type="f">betrachten</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Nacht en dagh van u <rhyme label="b" type="m" corresp="#s1:l2">ghedient</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">O wat vrijheydt is <rhyme label="a" type="f">gheploghen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Tusschen u en uwen <rhyme label="b" type="m">Al</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die tot u soo was <rhyme label="a" type="f" corresp="#s2:l1">ghetoghen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Soo ghemeyn u in dit <rhyme label="b" type="m" corresp="#s2:l2">dal</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Wie soo vinden // den <rhyme label="c" type="f">Beminden</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Met soo groote jonsten <rhyme label="b" type="m" corresp="#s2:l2">sal</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die van u als eenen <rhyme label="a" type="f">armen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Self hier somtijdts is <rhyme label="b" type="m">ghekleet</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die de naecte oock t' <rhyme label="a" type="f" corresp="#s3:l1">ontfermen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Met u kleeren waert <rhyme label="b" type="m" corresp="#s3:l2">bereet</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Alle menschen // saghm' u <rhyme label="c" type="f">wenschen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">By te staen, in liefde <rhyme label="b" type="m" corresp="#s3:l2">heet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die veel sondaers door <rhyme label="a" type="f">ghebeden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Seer versteend' oock, hebt <rhyme label="b" type="m">bekeert</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat de duyvels seer <rhyme label="a" type="f" corresp="#s4:l1">beneden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Aen den roof van hen <rhyme label="b" type="m" corresp="#s4:l2">verheert</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Die daer boven // eeuwich <rhyme label="c" type="f">loven</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Godt sal, in u seer <rhyme label="b" type="m" corresp="#s4:l2">vereert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Van Godt self hebdy <rhyme label="a" type="f">ontfanghen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Somtijdts 't heylich <rhyme label="b" type="m">Sacrament</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Dat met heel u ziel t' <rhyme label="a" type="f" corresp="#s5:l1">omvanghen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">In den meesten brandt <rhyme label="b" type="m" corresp="#s5:l2">ghewent</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Daer in droncken // heel <rhyme label="c" type="f">versoncken</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Op een wijs' ons <rhyme label="b" type="m" corresp="#s5:l2">onbekent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daer oock 't lichaem van <rhyme label="a" type="f">ghenoten</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Heeft de vrucht, versterckt dat <rhyme label="b" type="m">werdt</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Door de cracht daer uyt <rhyme label="a" type="f" corresp="#s6:l1">ghevloten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Waer ghy van soo waert <rhyme label="b" type="m" corresp="#s6:l2">ghehert</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Dat oock 't eten // werdt <rhyme label="c" type="f">vergheten</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat gh' onnoodich aet in <rhyme label="b" type="m" corresp="#s6:l2">smert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Iesvs oock tot u <rhyme label="a" type="f">ghecomen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">'t Hert uyt u heeft met sijn <rhyme label="b" type="m">handt</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Lans de slincke zy <rhyme label="a" type="f" corresp="#s7:l1">ghenomen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Nae den heysch van uwen <rhyme label="b" type="m" corresp="#s7:l2">brandt</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Is u 't leven // dan <rhyme label="c" type="f">ghebleven</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Sonder hert aen hem <rhyme label="b" type="m" corresp="#s7:l2">verpandt</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Doen ghy 't etter hadt <rhyme label="a" type="f">ghedroncken</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Van den kancker, heeft u <rhyme label="b" type="m">bly</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Christus wonderlijck <rhyme label="a" type="f" corresp="#s8:l1">beschoncken</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Uy de wonde van sijn <rhyme label="b" type="m" corresp="#s8:l2">zy</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Wat ghenuchten // liefdes <rhyme label="c" type="f">vruchten</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Ghy ghenoet daer al te <rhyme label="b" type="m" corresp="#s8:l2">vry</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">U, om meer hem te <rhyme label="a" type="f">ghelijcken</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Heeft begheert den soetsten <rhyme label="b" type="m">Heer</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Met sijn wonden te <rhyme label="a" type="f" corresp="#s9:l1">verrijcken</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Die u pijnden oock te <rhyme label="b" type="m" corresp="#s9:l2">seer</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Hoe wel stonden // u die <rhyme label="c" type="f">wonden</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Die u Lief draeght tot sijn <rhyme label="b" type="m" corresp="#s9:l2">eer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Oock de doorne croon uyt <rhyme label="a" type="f">keuren</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Ginghdy voor die van claer <rhyme label="b" type="m">goudt</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Doen u dese moght <rhyme label="a" type="f" corresp="#s10:l1">ghebeuren</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Die met Godt ghecruyst zijn <rhyme label="b" type="m" corresp="#s10:l2">wout</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Met hem lijden // 't alle <rhyme label="c" type="f">tijden</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Daer ghy mede waert <rhyme label="b" type="m" corresp="#s10:l2">ghetrouwt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Nu zijd' in de bruylofts <rhyme label="a" type="f">sale</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Langh ghecomen, op dat <rhyme label="b" type="m">u</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Op het hooft de croon daer <rhyme label="a" type="f" corresp="#s11:l1">strale</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Noot gheweest den strijdt hier <rhyme label="b" type="m" corresp="#s11:l2">mu</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Daer me croonen // eeuwich <rhyme label="c" type="f">loonen</rhyme></l>
        <l id="s11:l6" met="4" type="+-">U ghetrouheydt Godt sal <rhyme label="b" type="m" corresp="#s11:l2">nu</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Maer wilt ons daer niet <rhyme label="a" type="f">vergheten</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Die ghestelt noch zijn in <rhyme label="b" type="m">strijdt</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">In ghevaer, meer als wy <rhyme label="a" type="f" corresp="#s12:l1">weten</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Door des Vyants helschen <rhyme label="b" type="m" corresp="#s12:l2">nijdt</rhyme>:</l>
        <l id="s12:l5" met="4" type="+-">Zijt ons mede // Door u <rhyme label="c" type="f">bede</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Doet verwinnen ons <rhyme label="b" type="m" corresp="#s12:l2">altijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
