<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185092</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik lijd in het hart pijn ongewoon</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Als ik gedenck aen u o <rhyme label="a" type="m">Ieught</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Ick word <rhyme label="b" type="f">ontsteken</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En moet u <rhyme label="b" type="f" corresp="#s1:l2">smeken</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">1 O Ieugt versmaet u ydel <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En ‘s werelts <rhyme label="b" type="f" corresp="#s1:l2">streken</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">En wilt u <rhyme label="b" type="f" corresp="#s1:l2">breken</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Al ‘t quaet dat u <rhyme label="c" type="m">bestrijt</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Wilt tegen <rhyme label="d" type="f">stryden</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">2 En denckt, waer dat ghy <rhyme label="c" type="m" corresp="#s1:l7">zijt</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Mijn God die wil <rhyme label="c" type="m" corresp="#s1:l7">altijt</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Dat ick sal <rhyme label="d" type="f" corresp="#s1:l8">lyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">* V God, u Heer, die u soo <rhyme label="a" type="m">mint</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Wilt weer <rhyme label="b" type="f">beminnen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Wt al u <rhyme label="b" type="f" corresp="#s2:l2">sinnen</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Want hy noemt u zijn lieve <rhyme label="a" type="m" corresp="#s2:l1">kint</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Wilt dit <rhyme label="b" type="f" corresp="#s2:l2">versinnen</rhyme></l>
        <l id="s2:l6" met="2" type="-+">In ‘t herte <rhyme label="b" type="f" corresp="#s2:l2">binnen</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Indien ghy op hem <rhyme label="c" type="m">acht</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En zijn <rhyme label="d" type="f">bevelen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">4 Indien ghy op hem <rhyme label="c" type="m" corresp="#s2:l7">wacht</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Wt al u zielen <rhyme label="c" type="m" corresp="#s2:l7">kracht</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Hy sal u <rhyme label="d" type="f" corresp="#s2:l8">heelen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">* Eer u den besten tijd <rhyme label="a" type="m">ontgaet</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Eer dan de <rhyme label="b" type="f">jaren</rhyme></l>
        <l id="s3:l3" met="2" type="-+">V heel <rhyme label="b" type="f" corresp="#s3:l2">ontvaren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Eer dat den dagh u valt te <rhyme label="a" type="m" corresp="#s3:l1">laet</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Om deught te <rhyme label="b" type="f" corresp="#s3:l2">garen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Na Schrifts <rhyme label="b" type="f" corresp="#s3:l2">verklaren</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Want als u kracht <rhyme label="c" type="m">versmelt</rhyme></l>
        <l id="s3:l8" met="2" type="-+">In d’oude <rhyme label="d" type="f">daghen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Dan bent ghy niet <rhyme label="c" type="m" corresp="#s3:l7">gestelt</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Te dringhen met <rhyme label="c" type="m" corresp="#s3:l7">gewelt</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Door Sathans <rhyme label="d" type="f" corresp="#s3:l8">laghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">* ‘t Gesicht, ‘t ghehoor, den reuck de <rhyme label="a" type="m">smaeck</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">En u <rhyme label="b" type="f">gevoelen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Gaen uyt haer <rhyme label="b" type="f" corresp="#s4:l2">stoelen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">V lichaem krijgt dan kraeck op <rhyme label="a" type="m" corresp="#s4:l1">kraek</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">6 Dan moet ghy <rhyme label="b" type="f" corresp="#s4:l2">voelen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Beschuldingh <rhyme label="b" type="f" corresp="#s4:l2">woelen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">‘t Gedacht, wil en <rhyme label="c" type="m">verstant</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Die gaen dan <rhyme label="d" type="f">henen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">V alderbeste <rhyme label="c" type="m" corresp="#s4:l7">pant</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Dat vlieght dan aen een <rhyme label="c" type="m" corresp="#s4:l7">kant</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Dan baet geen <rhyme label="d" type="f" corresp="#s4:l8">weenen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">* ‘t Is dan vervloeyt en al <rhyme label="a" type="m">verleckt</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">V kracht <rhyme label="b" type="f">verloopen</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">En wegh <rhyme label="b" type="f" corresp="#s5:l2">gedropen</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">O Ieugt! u Ieught dan niet <rhyme label="a" type="m" corresp="#s5:l1">bevleckt</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Met ydel <rhyme label="b" type="f" corresp="#s5:l2">hoopen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">V Ieught <rhyme label="b" type="f" corresp="#s5:l2">verkoopen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">8 Denckt niet: ‘t is tijts <rhyme label="c" type="m">genoegh</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Tot God te <rhyme label="d" type="f">geven</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">En ‘t is noch al te <rhyme label="c" type="m" corresp="#s5:l7">vroegh</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Dat ick my daer na <rhyme label="c" type="m" corresp="#s5:l7">voegh</rhyme></l>
        <l id="s5:l11" met="2" type="+-">Om so te <rhyme label="d" type="f" corresp="#s5:l8">leven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">* Voorwaer soud die dan sijn ten <rhyme label="a" type="m">danck</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Voor al Godts <rhyme label="b" type="f">gaven</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Voor s’Geestes <rhyme label="b" type="f" corresp="#s6:l2">laven</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Voor Christus suur en swaren <rhyme label="a" type="m" corresp="#s6:l1">gangh</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Voor al sijn <rhyme label="b" type="f" corresp="#s6:l2">draven</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Voor al sijn <rhyme label="b" type="f" corresp="#s6:l2">Raven</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat ghy ‘t versleten <rhyme label="c" type="m">lijf</rhyme></l>
        <l id="s6:l8" met="2" type="-+">‘t Grontsop van <rhyme label="d" type="f">allen</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Godt schenckt tot sijn <rhyme label="c" type="m" corresp="#s6:l7">gerijf</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Hoe sou doch sulckx <rhyme label="c" type="m" corresp="#s6:l7">bedrijf</rhyme>,</l>
        <l id="s6:l11" met="2" type="+-">De Heer <rhyme label="d" type="f" corresp="#s6:l8">bevallen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">* O mensch hoe groot is u <rhyme label="a" type="m">ellent</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Die weynigh <rhyme label="b" type="f">achten</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">En cleyn <rhyme label="b" type="f" corresp="#s7:l2">betrachten</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">De Heer, u selfs, ghy weynigh <rhyme label="a" type="m" corresp="#s7:l1">kent</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">‘s Geests sucht en <rhyme label="b" type="f" corresp="#s7:l2">clachten</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Of rust <rhyme label="b" type="f" corresp="#s7:l2">verwachten</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Of ghy die uyr <rhyme label="c" type="m">toebrocht</rhyme></l>
        <l id="s7:l8" met="2" type="-+">En eens <rhyme label="d" type="f">bedaerde</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Voor ‘t aertsch het hemelsch <rhyme label="c" type="m" corresp="#s7:l7">socht</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Hoe waert ghy dan <rhyme label="c" type="m" corresp="#s7:l7">bekocht</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">Ay kent u <rhyme label="d" type="f" corresp="#s7:l8">waerde</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">* O Ieugt! wie heeft u tijd <rhyme label="a" type="m">belooft</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Dat tijd sal <rhyme label="b" type="f">duren</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Van dagh oft <rhyme label="b" type="f" corresp="#s8:l2">uren</rhyme>?</l>
        <l id="s8:l4" met="4" type="-+">Gelooft veel eer u Heer en <rhyme label="a" type="m" corresp="#s8:l1">Hooft</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Laet u vroegh <rhyme label="b" type="f" corresp="#s8:l2">huren</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Denckt; vroegh <rhyme label="b" type="f" corresp="#s8:l2">besuren</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Sal vroegh weer ‘t soet <rhyme label="c" type="m">ontfaen</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">En ‘t ruym <rhyme label="d" type="f">be-erven</rhyme>:</l>
        <l id="s8:l9" met="3" type="-+">Maer eerst door ‘t eng te <rhyme label="c" type="m" corresp="#s8:l7">gaen</rhyme></l>
        <l id="s8:l10" met="3" type="-+">En alles hier <rhyme label="c" type="m" corresp="#s8:l7">versmaen</rhyme>,</l>
        <l id="s8:l11" met="2" type="+-">En alles <rhyme label="d" type="f" corresp="#s8:l8">sterven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">* Herkauwt, herkaut dit woortje <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Van ‘t buyten <rhyme label="b" type="f">sluyten</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Ghy jonge <rhyme label="b" type="f" corresp="#s9:l2">spruyten</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Denckt hoe benauwt is haer <rhyme label="a" type="m" corresp="#s9:l1">gequel</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Voor die staen <rhyme label="b" type="f" corresp="#s9:l2">buyten</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Hoort haer eens <rhyme label="b" type="f" corresp="#s9:l2">uyten</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Ons tijd is ons <rhyme label="c" type="m">ontgaen</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">En <rhyme label="d" type="f">afgesneden</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">‘t Is met ons heel <rhyme label="c" type="m" corresp="#s9:l7">gedaen</rhyme>:</l>
        <l id="s9:l10" met="3" type="-+">Laet dit u hert <rhyme label="c" type="m" corresp="#s9:l7">verslaen</rhyme>,</l>
        <l id="s9:l11" met="2" type="+-">Want ‘t is noch <rhyme label="d" type="f" corresp="#s9:l8">heden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">* Wat baet ons nu rijcdom en <rhyme label="a" type="m">prael</rhyme>?</l>
        <l id="s10:l2" met="2" type="-+">Hoogh zijn <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">En lust <rhyme label="b" type="f" corresp="#s10:l2">bedreven</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Wat baet het ons nu <rhyme label="a" type="m" corresp="#s10:l1">altemael</rhyme>?</l>
        <l id="s10:l5" met="2" type="-+">‘t Is wegh <rhyme label="b" type="f" corresp="#s10:l2">gedreven</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">‘t Heeft ons <rhyme label="b" type="f" corresp="#s10:l2">begeven</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">‘t Is eeuwigh al <rhyme label="c" type="m">vergaen</rhyme>,</l>
        <l id="s10:l8" met="2" type="-+">En heel <rhyme label="d" type="f">verswonden</rhyme>:</l>
        <l id="s10:l9" met="3" type="-+">Wy ginghen sware <rhyme label="c" type="m" corresp="#s10:l7">pa’en</rhyme>,</l>
        <l id="s10:l10" met="3" type="-+">Nu moeten wy <rhyme label="c" type="m" corresp="#s10:l7">ontfaen</rhyme></l>
        <l id="s10:l11" met="2" type="+-">Het loon der <rhyme label="d" type="f" corresp="#s10:l8">sonden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">* Vol droef gequel is haer <rhyme label="a" type="m">ghedacht</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Haer <rhyme label="b" type="f">speculaty</rhyme></l>
        <l id="s11:l3" met="2" type="-+">En <rhyme label="b" type="f" corresp="#s11:l2">meditaty</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Is anders niet dan klacht op <rhyme label="a" type="m" corresp="#s11:l1">klacht</rhyme>,</l>
        <l id="s11:l5" met="2" type="-+">Want Godts <rhyme label="b" type="f" corresp="#s11:l2">gracy</rhyme></l>
        <l id="s11:l6" met="2" type="-+">Is wegh, <rhyme label="b" type="f" corresp="#s11:l2">eylacy</rhyme>!</l>
        <l id="s11:l7" met="3" type="-+">Sy sien haer schult-boeck <rhyme label="c" type="m">na</rhyme>,</l>
        <l id="s11:l8" met="2" type="-+">11 Maer al te <rhyme label="d" type="f">spade</rhyme>:</l>
        <l id="s11:l9" met="3" type="-+">Daer is dan geen <rhyme label="c" type="m" corresp="#s11:l7">gena</rhyme>,</l>
        <l id="s11:l10" met="3" type="-+">O zielen groote <rhyme label="c" type="m" corresp="#s11:l7">scha</rhyme>!</l>
        <l id="s11:l11" met="2" type="+-">O grootste <rhyme label="d" type="f" corresp="#s11:l8">schade</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">* Hoe claer is dan ‘t verstant by <rhyme label="a" type="m">haer</rhyme>,</l>
        <l id="s12:l2" met="2" type="-+">In ‘t <rhyme label="b" type="f">overweghen</rhyme>,</l>
        <l id="s12:l3" met="2" type="-+">Van al haer <rhyme label="b" type="f" corresp="#s12:l2">Weghen</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">‘t Ghesicht des ziels ‘t is alles <rhyme label="a" type="m" corresp="#s12:l1">claer</rhyme>,</l>
        <l id="s12:l5" met="2" type="-+">Maer heel <rhyme label="b" type="f" corresp="#s12:l2">versleghen</rhyme>,</l>
        <l id="s12:l6" met="2" type="-+">En gantsch <rhyme label="b" type="f" corresp="#s12:l2">verleghen</rhyme>:</l>
        <l id="s12:l7" met="3" type="-+">Let eens op haer <rhyme label="c" type="m">beclagh</rhyme>,</l>
        <l id="s12:l8" met="2" type="-+">Wat’s al <rhyme label="d" type="f">beclaghen</rhyme>,</l>
        <l id="s12:l9" met="3" type="-+">Och die nu niet meer <rhyme label="c" type="m" corresp="#s12:l7">mach</rhyme>,</l>
        <l id="s12:l10" met="3" type="-+">Dan roepen wee en <rhyme label="c" type="m" corresp="#s12:l7">ach</rhyme>!</l>
        <l id="s12:l11" met="2" type="+-">En heel <rhyme label="d" type="f" corresp="#s12:l8">vertsaghen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">* Hoe haest is alles nu al <rhyme label="a" type="m">wech</rhyme>,</l>
        <l id="s13:l2" met="2" type="-+">Ia wech <rhyme label="b" type="f">getogen</rhyme>,</l>
        <l id="s13:l3" met="2" type="-+">En snel <rhyme label="b" type="f" corresp="#s13:l2">vervlogen</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Gelijck een pijl en schip wiens <rhyme label="a" type="m" corresp="#s13:l1">wegh</rhyme>,</l>
        <l id="s13:l5" met="2" type="-+">Die wy niet <rhyme label="b" type="f" corresp="#s13:l2">moghen</rhyme></l>
        <l id="s13:l6" met="2" type="-+">Door ‘t oogh af <rhyme label="b" type="f" corresp="#s13:l2">oogen</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Haer wegh sien in de <rhyme label="c" type="m">locht</rhyme>,</l>
        <l id="s13:l8" met="2" type="-+">Moch in de <rhyme label="d" type="f">baren</rhyme>:</l>
        <l id="s13:l9" met="3" type="-+">So snel is onse <rhyme label="c" type="m" corresp="#s13:l7">tocht</rhyme>,</l>
        <l id="s13:l10" met="3" type="-+">En leven door <rhyme label="c" type="m" corresp="#s13:l7">ghebrocht</rhyme>,</l>
        <l id="s13:l11" met="2" type="+-">Tot ons <rhyme label="d" type="f" corresp="#s13:l8">beswaren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">* Siet eens wat sy in smert en <rhyme label="a" type="m">pijn</rhyme>,</l>
        <l id="s14:l2" met="2" type="-+">Nu gaen <rhyme label="b" type="f">studeeren</rhyme>,</l>
        <l id="s14:l3" met="2" type="-+">Tot quaels <rhyme label="b" type="f" corresp="#s14:l2">vermeeren</rhyme>.</l>
        <l id="s14:l4" met="4" type="-+">Siet seggen sy: hoe dees nu <rhyme label="a" type="m" corresp="#s14:l1">sijn</rhyme></l>
        <l id="s14:l5" met="2" type="-+">In acht en <rhyme label="b" type="f" corresp="#s14:l2">eere</rhyme></l>
        <l id="s14:l6" met="2" type="-+">By haren <rhyme label="b" type="f" corresp="#s14:l2">Heere</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Haer deucht wiert staegh <rhyme label="c" type="m">bespot</rhyme>,</l>
        <l id="s14:l8" met="2" type="-+">Door ons <rhyme label="d" type="f">ondeughde</rhyme>,</l>
        <l id="s14:l9" met="3" type="-+">Ach wy ontsinnigh <rhyme label="c" type="m" corresp="#s14:l7">sot</rhyme>,</l>
        <l id="s14:l10" met="3" type="-+">Nu is haer deel een <rhyme label="c" type="m" corresp="#s14:l7">lot</rhyme>,</l>
        <l id="s14:l11" met="2" type="+-">D’eeuwighe <rhyme label="d" type="f" corresp="#s14:l8">vreughde</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">* Van kints been aen tot <rhyme label="a" type="m">d’ouderdom</rhyme></l>
        <l id="s15:l2" met="2" type="-+">12 Soeckt God te <rhyme label="b" type="f">vreesen</rhyme>,</l>
        <l id="s15:l3" met="2" type="-+">Laet u <rhyme label="b" type="f" corresp="#s15:l2">genesen</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">O jonge Ieught en blijft niet <rhyme label="a" type="m" corresp="#s15:l1">dom</rhyme>,</l>
        <l id="s15:l5" met="2" type="-+">Wilt vlytigh <rhyme label="b" type="f" corresp="#s15:l2">wesen</rhyme></l>
        <l id="s15:l6" met="2" type="-+">Tot deught <rhyme label="b" type="f" corresp="#s15:l2">geresen</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Want deucht al ‘t quaet <rhyme label="c" type="m">verwint</rhyme>,</l>
        <l id="s15:l8" met="2" type="-+">En doet <rhyme label="d" type="f">vertsagen</rhyme>:</l>
        <l id="s15:l9" met="3" type="-+">Weest dan na deught <rhyme label="c" type="m" corresp="#s15:l7">gesint</rhyme>,</l>
        <l id="s15:l10" met="3" type="-+">En deught om deught <rhyme label="c" type="m" corresp="#s15:l7">bemint</rhyme>,</l>
        <l id="s15:l11" met="2" type="+-">‘t Is soet om <rhyme label="d" type="f" corresp="#s15:l8">dragen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">* Bidt onsen Prins, tot alder <rhyme label="a" type="m">stont</rhyme></l>
        <l id="s16:l2" met="2" type="-+">Om kracht van <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s16:l3" met="2" type="-+">Ieughts lust te <rhyme label="b" type="f" corresp="#s16:l2">doven</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Besteet wel i ontfangen <rhyme label="a" type="m" corresp="#s16:l1">pont</rhyme>,</l>
        <l id="s16:l5" met="2" type="-+">Laet dat niet <rhyme label="b" type="f" corresp="#s16:l2">roven</rhyme>,</l>
        <l id="s16:l6" met="2" type="-+">Maer wilt hem <rhyme label="b" type="f" corresp="#s16:l2">loven</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Met een gehoorsaem <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s16:l8" met="2" type="-+">In allen <rhyme label="d" type="f">dingen</rhyme>,</l>
        <l id="s16:l9" met="3" type="-+">Al valt y vleysch wat <rhyme label="c" type="m" corresp="#s16:l7">hart</rhyme>,</l>
        <l id="s16:l10" met="3" type="-+">En vreest noch kruys, noch <rhyme label="c" type="m" corresp="#s16:l7">smart</rhyme>,</l>
        <l id="s16:l11" met="2" type="+-">Maer wilt <rhyme label="d" type="f" corresp="#s16:l8">door-dringen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
