<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>16496</id>
    <date>
      <year-start>1903</year-start>
      <year-end>1908</year-end>
    </date>
    <tags/>
    <melody>Hemelse koningin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Hemelsche coninghinne,</l>
        <l id="s1:l2">verblijt u nu,</l>
        <l id="s1:l3">want Jesus die ick minne,</l>
        <l id="s1:l4">die comt tot u.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Die ghy weerdt waert te draghen</l>
        <l id="s2:l2">in uwen schoot,</l>
        <l id="s2:l3">verryst, naar schrifts ghewaghen,</l>
        <l id="s2:l4">rasch van der doot.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">Bidt Godt voor ons, Marie,</l>
        <l id="s3:l2">helpt onsen strijt,</l>
        <l id="s3:l3">laet ons verwerven blie</l>
        <l id="s3:l4">d'eeuwich iolijt.</l>
      </lg>
    </lg>
  </text>
</song>
