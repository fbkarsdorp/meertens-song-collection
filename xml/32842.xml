        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Beata immaculata</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">BEgga van edel <rhyme label="a" type="f">stamme</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hoogh gheboren <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Vwen Vader Pepin <rhyme label="a" type="f" corresp="#s1:l1">quamme</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van de deel <rhyme label="b" type="m" corresp="#s1:l2">Majesteyt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Eerst van <rhyme label="c" type="m">Brabandt</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Vruchtbaer <rhyme label="c" type="m" corresp="#s1:l5">landt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Ghemaeckt Hertogh <rhyme label="c" type="m" corresp="#s1:l5">triomphant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O edel <rhyme label="a" type="f">Princersse</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Meerder zijt <rhyme label="b" type="m">gheacht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ghy zijt Iesus <rhyme label="a" type="f" corresp="#s2:l1">minnersse</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Heel tot sijnen dienste <rhyme label="b" type="m" corresp="#s2:l2">tracht</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Want ghy uw' <rhyme label="c" type="m">jeught</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Met een <rhyme label="c" type="m" corresp="#s2:l5">vreught</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Hebt ghestiert tot alle <rhyme label="c" type="m" corresp="#s2:l5">deught</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">In den ghehouwden <rhyme label="a" type="f">state</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Al zijt ghy <rhyme label="b" type="m">ghestelt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En hebt u in desen <rhyme label="a" type="f" corresp="#s3:l1">grate</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met de wereldt niet <rhyme label="b" type="m" corresp="#s3:l2">ghemelt</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Want Iesus <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Is u <rhyme label="c" type="m" corresp="#s3:l5">goedt</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Die u alle vreughden <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy dan vierigh <rhyme label="a" type="f">beghoste</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In te minnen <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Als-men u van man <rhyme label="a" type="f" corresp="#s4:l1">verloste</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot uw' alder-grootste <rhyme label="b" type="m" corresp="#s4:l2">eer</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">In Iesus <rhyme label="c" type="m">min</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Schoon <rhyme label="c" type="m" corresp="#s4:l5">beghin</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Stelt gheheelijck uwen <rhyme label="c" type="m" corresp="#s4:l5">sin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Om hem hebt ghy de <rhyme label="a" type="f">reyse</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van Roomen <rhyme label="b" type="m">ghedaen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Begga tot uwen <rhyme label="a" type="f" corresp="#s5:l1">prijse</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Om den seghen te <rhyme label="b" type="m" corresp="#s5:l2">ontfaen</rhyme>;</l>
        <l id="s5:l5" met="2" type="-+">Want sijt <rhyme label="c" type="m">vermeert</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Oock <rhyme label="c" type="m" corresp="#s5:l5">vereert</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Als ghy met reliquien <rhyme label="c" type="m" corresp="#s5:l5">keert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Begga naer het <rhyme label="a" type="f">verlichten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van de Hemels <rhyme label="b" type="m">brandt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Gaet seven Kercken <rhyme label="a" type="f" corresp="#s6:l1">stichten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Om te dienen Godt <rhyme label="b" type="m" corresp="#s6:l2">plaisant</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Als Godes <rhyme label="c" type="m">Kindt</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Wel <rhyme label="c" type="m" corresp="#s6:l5">bemindt</rhyme></l>
        <l id="s6:l7" met="4" type="+-">Hem met alle blijdschap <rhyme label="c" type="m" corresp="#s6:l5">dient</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Vwe sustere <rhyme label="a" type="f">Geertruyt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wilt heel zijn <rhyme label="b" type="m">ghelijck</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Want als een vruchtbare <rhyme label="a" type="f" corresp="#s7:l1">spruyt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Brengt voorts vele deughden <rhyme label="b" type="m" corresp="#s7:l2">rijck</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Oock Maeghden <rhyme label="c" type="m">reyn</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Om te <rhyme label="c" type="m" corresp="#s7:l5">zijn</rhyme></l>
        <l id="s7:l7" met="4" type="+-">In de liefde <rhyme label="c" type="m" corresp="#s7:l5">souvereyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof Princersse <rhyme label="a" type="f">schoon</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met uw' deughden <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Iesus is uwen <rhyme label="a" type="f" corresp="#s8:l1">loone</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Iont ons Begghijnkens <rhyme label="b" type="m" corresp="#s8:l2">eerbaer</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Dat w' in't <rhyme label="c" type="m">jolijt</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">T'allen <rhyme label="c" type="m" corresp="#s8:l5">tijdt</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Mochten eeuwigh zijn <rhyme label="c" type="m" corresp="#s8:l5">verblijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
