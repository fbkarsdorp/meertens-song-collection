  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Edele <rhyme label="a" type="f">creature</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Maria, Moeder en Maghet <rhyme label="a" type="f" corresp="#s1:l1">puere</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Vw' blijdschap is sonder <rhyme label="b" type="m">endt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Als ghy d'aerde hebt <rhyme label="c" type="f">verlaten</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ten hemel schoon t uwer <rhyme label="c" type="f" corresp="#s1:l4">baten</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Sijt op-ghenomen <rhyme label="b" type="m" corresp="#s1:l3">jent</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Met Iesus gaet ghy u <rhyme label="a" type="f">paren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wie sal doch de triumph <rhyme label="a" type="f" corresp="#s2:l1">verklaren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van u, o Maria <rhyme label="b" type="m">schoon</rhyme>?</l>
        <l id="s2:l4" met="3" type="-+">Al de triumphen der <rhyme label="c" type="f">Romyenen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Zijn daer by maer ydele <rhyme label="c" type="f" corresp="#s2:l4">schijnen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ghy spant alleen de <rhyme label="b" type="m" corresp="#s2:l3">Kroon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als een Vrouwe <rhyme label="a" type="f">uyt-ghelesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met de kroon der Sterren <rhyme label="a" type="f" corresp="#s3:l1">ghepresen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wordt ghyer eeuwich <rhyme label="b" type="m">ghekroont</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">De Maen hebt ghy heel <rhyme label="c" type="f">vertreden</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Met de Son rondt-om <rhyme label="c" type="f" corresp="#s3:l4">geleden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">V blijdelijck <rhyme label="b" type="m" corresp="#s3:l3">verthoont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy moeten u dan <rhyme label="a" type="f">loven</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met de Enghelen hier <rhyme label="a" type="f" corresp="#s4:l1">boven</rhyme></l>
        <l id="s4:l3" met="3" type="-+">In uwe triumphe <rhyme label="b" type="m">bly</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Want Maria met uwe <rhyme label="c" type="f">deughden</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Stelt alle herten in <rhyme label="c" type="f" corresp="#s4:l4">vreughden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Maeckt ons van droefheydt <rhyme label="b" type="m" corresp="#s4:l3">vry</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het isser al in <rhyme label="a" type="f">ghenuchte</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Men hoorter noch kerm noch <rhyme label="a" type="f" corresp="#s5:l1">suchte</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">De Enghelen <rhyme label="b" type="m">openbaer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hebben met fluyten en <rhyme label="c" type="f">velen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Oock met alderhande <rhyme label="c" type="f" corresp="#s5:l4">spelen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Vereert de Maghet <rhyme label="b" type="m" corresp="#s5:l3">klaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Den Vader sijt <rhyme label="a" type="f">willekomme</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Oock Iesus uwen <rhyme label="a" type="f" corresp="#s6:l1">Bruygomme</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Daer by den heylighen <rhyme label="b" type="m">Gheest</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Eenighen Godt in dry <rhyme label="c" type="f">persoonen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">V eeuwelijcken sal <rhyme label="c" type="f" corresp="#s6:l4">loonen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Met glori' <rhyme label="b" type="m" corresp="#s6:l3">alder-meest</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Want Godt is uwen <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Heer Iesus uwen <rhyme label="a" type="f" corresp="#s7:l1">bewaerder</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Den heylighen Gheest u <rhyme label="b" type="m">bemindt</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Zijt naer de God'lijcke <rhyme label="c" type="f">nature</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Boven alle <rhyme label="c" type="f" corresp="#s7:l4">creature</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ghestelt als Godes <rhyme label="b" type="m" corresp="#s7:l3">Kindt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof Princers <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met mede-lijden <rhyme label="a" type="f" corresp="#s8:l1">ghedreven</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Bidden wy hier sonder <rhyme label="b" type="m">ghetal</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Wilt altijdt ons ghedachtich <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Dat wy in de glori' <rhyme label="c" type="f" corresp="#s8:l4">ghepresen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Eens mochten komen <rhyme label="b" type="m" corresp="#s8:l3">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
