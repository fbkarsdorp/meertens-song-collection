    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VRouw Bruydt gepresen, schoone <rhyme label="a" type="m">Bloem</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Pronck, Perel, Roose <rhyme label="b" type="f">uytgelesen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">V Bruydegoms kroon en sijn <rhyme label="a" type="m" corresp="#s1:l1">roem</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En boven al van hem <rhyme label="b" type="f" corresp="#s1:l2">gepresen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy zijt getreden in den <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">In d'Echte-staet zijt ghy <rhyme label="b" type="f">getreden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Een stuck dat 't al te boven <rhyme label="a" type="m" corresp="#s2:l1">gaet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Daer vreught en smert wert in <rhyme label="b" type="f" corresp="#s2:l2">geleden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dies bid ick u te deser <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Dat ghy geduldigh zijt in <rhyme label="b" type="f">lijden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat welcke u vaeck breet en <rhyme label="a" type="m" corresp="#s3:l1">wijt</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Van alle kanten komt <rhyme label="b" type="f" corresp="#s3:l2">bestrijden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En als ghy zijt in groot <rhyme label="a" type="m">geluck</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat ghy dan niet en wordt <rhyme label="b" type="f">hooghmoedigh</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Maer dat ghy dan (gelijck in <rhyme label="a" type="m" corresp="#s4:l1">druck</rhyme>)</l>
        <l id="s4:l4" met="4" type="-+">V wendet tot den Heere <rhyme label="b" type="f" corresp="#s4:l2">goedigh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">En denckt dat 't alles komt van <rhyme label="a" type="m">God</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Van sijnen handt moet ghy <rhyme label="b" type="f">verwachten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">'t Sy vreughde of een droevigh <rhyme label="a" type="m" corresp="#s5:l1">lot</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ey! wilt dit doch voor al <rhyme label="b" type="f" corresp="#s5:l2">betrachten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Siet ghy met God niet <rhyme label="a" type="m">murmureert</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Maer seght: de Heer heeft 'et <rhyme label="b" type="f">genomen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Heere is 't die 't  weder <rhyme label="a" type="m" corresp="#s6:l1">geeft</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dies heeft hy van mijn danck <rhyme label="b" type="f" corresp="#s6:l2">bekomen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Weest uwen Man gehoorsaem, <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Het is u plicht en 't staet <rhyme label="b" type="f">geschreven</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Het is dien grooten Godt zijn <rhyme label="a" type="m" corresp="#s7:l1">woordt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Dat hy u selver heeft <rhyme label="b" type="f" corresp="#s7:l2">gegeven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Betracht dees saeck soo doet ghy <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Het is een trap die u leydt <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Selfs in den Hemel uyt de <rhyme label="a" type="m" corresp="#s8:l1">Hel</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Een yeder mensch sal u dan <rhyme label="b" type="f" corresp="#s8:l2">loven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Geeft ghy een ware voor-beelt <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Aen yeder Vrouwe hier op <rhyme label="b" type="f">aerden</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat sy haer Echte-Man soo <rhyme label="a" type="m" corresp="#s9:l1">doet</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Als ghy doet uwen vriendt vol <rhyme label="b" type="f" corresp="#s9:l2">waerden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dus sal de vreede zijn <rhyme label="a" type="m">geplant</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">En eendracht zijn voor twist <rhyme label="b" type="f">gekomen</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">De liefde sal, door 't gansche <rhyme label="a" type="m" corresp="#s10:l1">landt</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Seer heerlijck blincken sonder <rhyme label="b" type="f" corresp="#s10:l2">schroomen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dan salmen prijsen uwe <rhyme label="a" type="m">daedt</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">V eere als een <rhyme label="b" type="f">vreedens-moeder</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Geluckigh achten die nu <rhyme label="a" type="m" corresp="#s11:l1">staet</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">En is u sorgh, u lief, en <rhyme label="b" type="f" corresp="#s11:l2">hoeder</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Het is een saeck die <rhyme label="a" type="m">aengenaem</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Is by den grooten Godt <rhyme label="b" type="f">gepresen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Die sijnen zegen, seer <rhyme label="a" type="m" corresp="#s12:l1">bequaem</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">De sulcke altijdt heeft <rhyme label="b" type="f" corresp="#s12:l2">bewesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
