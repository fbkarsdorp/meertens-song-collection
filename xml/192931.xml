<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>192931</id>
    <date>
      <year-start>1744</year-start>
      <year-end>1744</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De zoete tijden van het meiseizoen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">PRincen en <rhyme label="a" type="f">Potentaten</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Vorsten ook alle <rhyme label="a" type="f" corresp="#s1:l1">Staeten</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">'t Zy leeg of <rhyme label="b" type="m">hoog</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hebt hy in dit <rhyme label="b" type="m" corresp="#s1:l3">vertoog</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Ziet men op raden <rhyme label="c" type="f">draeijen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Op alle kanten <rhyme label="c" type="f" corresp="#s1:l5">swaijen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Men vat my <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Om seker vast te <rhyme label="d" type="m" corresp="#s1:l7">staen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ziet hier <rhyme label="a" type="f">beneden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoe Beijeren wiert <rhyme label="a" type="f" corresp="#s2:l1">gegeven</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Dees Keysers <rhyme label="b" type="m">Kroon</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En zijn verheven <rhyme label="b" type="m" corresp="#s2:l3">Throon</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Wat menig <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van menschen siet men <rhyme label="c" type="f" corresp="#s2:l5">sneven</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Op desen <rhyme label="d" type="m">tijdt</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Om dat m' hem die <rhyme label="d" type="m" corresp="#s2:l7">ontseyt</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Hoort onder <rhyme label="a" type="f">alle</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een ider zijn <rhyme label="a" type="f" corresp="#s3:l1">gevalle</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Die op 't Rad <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hoort ider hun <rhyme label="b" type="m" corresp="#s3:l3">vermaen</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Hoort in dees <rhyme label="c" type="f">tijden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Een ider komt <rhyme label="c" type="f" corresp="#s3:l5">verblijden</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Om Kroon en <rhyme label="d" type="m">goedt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Verstorten lyf en <rhyme label="d" type="m" corresp="#s3:l7">bloedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wilt Beijeren my <rhyme label="a" type="f">dwingen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En my met magt <rhyme label="a" type="f" corresp="#s4:l1">bespringen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">met menig <rhyme label="b" type="m">Helt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die hy heeft in het <rhyme label="b" type="m" corresp="#s4:l3">Velt</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Ik vrees weijnigh zyn <rhyme label="c" type="f">krachten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En hoop door ander <rhyme label="c" type="f" corresp="#s4:l5">machten</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Hem sal <rhyme label="d" type="m">verslaen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En 't onder doen <rhyme label="d" type="m" corresp="#s4:l7">gaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Neen <rhyme label="a" type="f">Koninginne</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Het spel gaet eerst <rhyme label="a" type="f" corresp="#s5:l1">beginnen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">En mijne <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">De geen gy soo <rhyme label="b" type="m" corresp="#s5:l3">veracht</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Sal met den <rhyme label="c" type="f">tijde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Soo deftig voor my <rhyme label="c" type="f" corresp="#s5:l5">stryden</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Dat heel u <rhyme label="d" type="m">kracht</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Kan zijn ter niet <rhyme label="d" type="m" corresp="#s5:l7">gebracht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">'k Zal niet naer <rhyme label="a" type="f">laten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met mijn macht van <rhyme label="a" type="f" corresp="#s6:l1">soldaten</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Des Keysers <rhyme label="b" type="m">Kroon</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En ook des selfs <rhyme label="b" type="m" corresp="#s6:l3">Troon</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Beyeren te <rhyme label="c" type="f">jonnen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Den strijdt is maer <rhyme label="c" type="f" corresp="#s6:l5">begonnen</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Hy moet op 't <rhyme label="d" type="m">sop</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Voor dat ik sal houden <rhyme label="d" type="m" corresp="#s6:l7">op</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Spanjien <rhyme label="a" type="f">beneven</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wilt Engelant doen <rhyme label="a" type="f" corresp="#s7:l1">beven</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Hy randt hun <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Zelfs op den <rhyme label="b" type="m" corresp="#s7:l3">Oceaen</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Om kloek te <rhyme label="c" type="f">vechten</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Met zijne trouwe <rhyme label="c" type="f" corresp="#s7:l5">knechten</rhyme></l>
        <l id="s7:l7" met="2" type="-+">En wel <rhyme label="d" type="m">bemant</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Komt hy in 't <rhyme label="d" type="m" corresp="#s7:l7">Nederland</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Ik staen vol <rhyme label="a" type="f">Zuchten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En weet waer gaen oft <rhyme label="a" type="f" corresp="#s8:l1">vluchten</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Het Spaens <rhyme label="b" type="m">gewelt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Houd my gelijk <rhyme label="b" type="m" corresp="#s8:l3">gevelt</rhyme></l>
        <l id="s8:l5" met="2" type="-+">In land en <rhyme label="c" type="f">steden</rhyme></l>
        <l id="s8:l6" met="3" type="-+">En al wat ik <rhyme label="c" type="f" corresp="#s8:l5">bekleden</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Bedroefden <rhyme label="d" type="m">tydt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Ik ben schier alles <rhyme label="d" type="m" corresp="#s8:l7">quyt</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Ter Zee ik <rhyme label="a" type="f">leyde</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Maar keer af ter <rhyme label="a" type="f" corresp="#s9:l1">zeyde</rhyme></l>
        <l id="s9:l3" met="2" type="-+">'t Waater niet <rhyme label="b" type="m">diend</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ter landt ik neerlaegd <rhyme label="b" type="m" corresp="#s9:l3">vind</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Wat sal my <rhyme label="c" type="f">naecken</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Kost ik den vree maer <rhyme label="c" type="f" corresp="#s9:l5">maecken</rhyme></l>
        <l id="s9:l7" met="2" type="-+">Met spanjen 't waer <rhyme label="d" type="m">gedaen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">'k Trok my geen saeck meer <rhyme label="d" type="m" corresp="#s9:l7">aan</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Ik sal noch <rhyme label="a" type="f">wachten</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Met alle myn <rhyme label="a" type="f" corresp="#s10:l1">krachten</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Het end sien <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Hoe alles sal <rhyme label="b" type="m" corresp="#s10:l3">vergaen</rhyme>:</l>
        <l id="s10:l5" met="2" type="-+">'k Sal dan ten <rhyme label="c" type="f">leste</rhyme></l>
        <l id="s10:l6" met="3" type="-+">My voegen by de <rhyme label="c" type="f" corresp="#s10:l5">beste</rhyme></l>
        <l id="s10:l7" met="2" type="-+">Het beste van <rhyme label="d" type="m">al</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Dient my in dit <rhyme label="d" type="m" corresp="#s10:l7">geval</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Ben ik hoog <rhyme label="a" type="f">geklommen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Godt geef ik niet leeg mag <rhyme label="a" type="f" corresp="#s11:l1">kommen</rhyme></l>
        <l id="s11:l3" met="2" type="-+">Maer met den <rhyme label="b" type="m">tydt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Verwin die my <rhyme label="b" type="m" corresp="#s11:l3">bestrydt</rhyme></l>
        <l id="s11:l5" met="2" type="-+">En myn <rhyme label="c" type="f">soldaeten</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Zoo duytsche als <rhyme label="c" type="f" corresp="#s11:l5">Crawaten</rhyme></l>
        <l id="s11:l7" met="2" type="-+">Die sullen <rhyme label="d" type="m">al</rhyme></l>
        <l id="s11:l8" met="3" type="-+">My helpen uyt <rhyme label="d" type="m" corresp="#s11:l7">geval</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">'t Radt wonder <rhyme label="a" type="f">draeyen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Den Fransen haen hoort <rhyme label="a" type="f" corresp="#s12:l1">kraeijen</rhyme></l>
        <l id="s12:l3" met="2" type="-+">Wy syn niet <rhyme label="b" type="m">vervaert</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Al van den Fransen <rhyme label="b" type="m" corresp="#s12:l3">haen</rhyme></l>
        <l id="s12:l5" met="2" type="-+">Wy sullen hem <rhyme label="c" type="f">verwagten</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Met alle onsen <rhyme label="c" type="f" corresp="#s12:l5">kragten</rhyme></l>
        <l id="s12:l7" met="2" type="-+">En hem <rhyme label="d" type="m">verslaan</rhyme></l>
        <l id="s12:l8" met="3" type="-+">En ten onderen doen <rhyme label="d" type="m" corresp="#s12:l7">gaan</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="2" type="-+">Ik draey <rhyme label="a" type="f">rommendomme</rhyme></l>
        <l id="s13:l2" met="3" type="-+">'t Zy wie dat boven <rhyme label="a" type="f" corresp="#s13:l1">komen</rhyme></l>
        <l id="s13:l3" met="2" type="-+">myn Radt moet <rhyme label="b" type="m">gaan</rhyme></l>
        <l id="s13:l4" met="3" type="-+">En neem het draeijen <rhyme label="b" type="m" corresp="#s13:l3">aen</rhyme></l>
        <l id="s13:l5" met="2" type="-+">myn <rhyme label="c" type="f">Avonture</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Staet nooyt stil met <rhyme label="c" type="f" corresp="#s13:l5">gedure</rhyme></l>
        <l id="s13:l7" met="2" type="-+">macht en <rhyme label="d" type="m">gewelt</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Het uw voor oogen <rhyme label="d" type="m" corresp="#s13:l7">stelt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="2" type="-+">Draeyt niet meer <rhyme label="a" type="f">omme</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Voor dat wy vast <rhyme label="a" type="f" corresp="#s14:l1">bekomme</rhyme>,</l>
        <l id="s14:l3" met="2" type="-+">Een <rhyme label="b" type="m">Potentaet</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Die alle twist <rhyme label="b" type="m" corresp="#s14:l3">versmaet</rhyme>,</l>
        <l id="s14:l5" met="2" type="-+">Nederlandt <rhyme label="c" type="f">geven</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">In waer Geloof mag <rhyme label="c" type="f" corresp="#s14:l5">leven</rhyme>,</l>
        <l id="s14:l7" met="2" type="-+">En dat het <rhyme label="d" type="m">sy</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Van alle onheyl <rhyme label="d" type="m" corresp="#s14:l7">vry</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
