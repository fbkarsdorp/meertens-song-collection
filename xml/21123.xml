<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21123</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Sei tanto graziosa</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEen dingh soo <rhyme label="a" type="f">aengenamigh</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Als kennis van Godts werck, wil, en <rhyme label="b" type="f">vermogen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Geen leven soo <rhyme label="a" type="f" corresp="#s1:l1">min-samigh</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Geen goet soo lief, beschout met Menschen <rhyme label="b" type="f" corresp="#s1:l2">oogen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Als dat die leeft, zijn wenschen <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Welck is zijn lief, zijn <rhyme label="d" type="f">waerde</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Die hy in <rhyme label="e" type="f">krachten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In wil, en in <rhyme label="e" type="f" corresp="#s1:l7">gedachten</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Staegh <rhyme label="d" type="f" corresp="#s1:l6">bewaerde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy wenschen hier <rhyme label="a" type="f">beneden</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Geleydt, gestuurt, door kracht uyt 's Hemels <rhyme label="b" type="f">Zalen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">In ons <rhyme label="a" type="f" corresp="#s2:l1">inwendigheden</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Het geest'lijck licht zijns klaerheydts voelen <rhyme label="b" type="f" corresp="#s2:l2">dalen</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Maer ons natuur, soo broos en <rhyme label="c" type="m">stuur</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Verwaereloost de <rhyme label="d" type="f">gaven</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Tot velen <rhyme label="e" type="f">stonden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Des wy als doodt in <rhyme label="e" type="f" corresp="#s2:l7">sonden</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Zijn <rhyme label="d" type="f" corresp="#s2:l6">begraven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat veelderhande <rhyme label="a" type="f">goeden</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Gunt ons den Gever aller goeder <rhyme label="b" type="f">dingen</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Wat heyl en wat <rhyme label="a" type="f" corresp="#s3:l1">voorspoeden</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Heeft hy gewilt door Jesum Christum <rhyme label="b" type="f" corresp="#s3:l2">bringen</rhyme>?</l>
        <l id="s3:l5" met="4" type="-+">Hoe noodigh wat, een waerde <rhyme label="c" type="m">schat</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wordt ons gegunt daer <rhyme label="d" type="f">neven</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Als ons vol <rhyme label="e" type="f">trouwen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Een deughtsaem waerde <rhyme label="e" type="f" corresp="#s3:l7">Vrouwe</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Wordt <rhyme label="d" type="f" corresp="#s3:l6">gegeven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O! volheydt der <rhyme label="a" type="f">genaden</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Die ons beheerscht van uyt u woonst' daer <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De rijckheydt der <rhyme label="a" type="f" corresp="#s4:l1">weldaden</rhyme>;</l>
        <l id="s4:l4" met="5" type="-+">Wat tongh, of mondt, sal die ten vollen <rhyme label="b" type="f" corresp="#s4:l2">loven</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">Sterckt onsen geest, vol krachten <rhyme label="c" type="m">meest</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat wy 't volbrengen <rhyme label="d" type="f">konden</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">V te <rhyme label="e" type="f">aen-beeden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Voor dit Paer, die ghy <rhyme label="e" type="f" corresp="#s4:l7">heden</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Hebt <rhyme label="d" type="f" corresp="#s4:l6">verbonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Geeft, goedertieren <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Dat zy den Echt (van u gestelt) <rhyme label="b" type="f">beleven</rhyme></l>
        <l id="s5:l3" met="3" type="-+">In uwe vrees-te <rhyme label="a" type="f" corresp="#s5:l1">gader</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">V zegen wilt hun beyd' te samen <rhyme label="b" type="f" corresp="#s5:l2">geven</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Hoedt haer voor druck, en <rhyme label="c" type="m">ongeluck</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Voor lijf en zilen <rhyme label="d" type="f">schaden</rhyme>;</l>
        <l id="s5:l7" met="2" type="-+">Wilt haer <rhyme label="e" type="f">bevryen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Nu en tot allen <rhyme label="e" type="f" corresp="#s5:l7">tyen</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Van den <rhyme label="d" type="f" corresp="#s5:l6">quaden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dus Bruydegom <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Dees lieve Godt beg'nadight u <rhyme label="b" type="f">beginnen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">V waerde Bruydt woondt <rhyme label="a" type="f" corresp="#s6:l1">neven</rhyme></l>
        <l id="s6:l4" met="5" type="-+">Met liefd', en wilt haer als u selfs <rhyme label="b" type="f" corresp="#s6:l2">beminnen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Haer swackheyt soet, Komt te <rhyme label="c" type="m">gemoet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Met wijsheydt en met <rhyme label="d" type="f">reden</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">'t Zy oock hoe <rhyme label="e" type="f">lange</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En nimmermeer met <rhyme label="e" type="f" corresp="#s6:l7">strange</rhyme></l>
        <l id="s6:l9" met="2" type="+-"><rhyme label="d" type="f" corresp="#s6:l6">Bitterheden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Desg'lijcks Bruydt al u <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s7:l2" met="5" type="-+">Bestiert u plicht, naer luydt van 't woordt des <rhyme label="b" type="f">Heeren</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Wilt trouwe liefde <rhyme label="a" type="f" corresp="#s7:l1">dragen</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">V Man, met Sara, als u Heere <rhyme label="b" type="f" corresp="#s7:l2">eeren</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Weest onderdaen, hem wilt <rhyme label="c" type="m">by-staen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En na 't behooren <rhyme label="d" type="f">passen</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Soo u Godt <rhyme label="e" type="f">vreedigh</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Geeft Kind'ren, datse <rhyme label="e" type="f" corresp="#s7:l7">zeedigh</rhyme></l>
        <l id="s7:l9" met="2" type="+-">Staegh <rhyme label="d" type="f" corresp="#s7:l6">op-wassen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
