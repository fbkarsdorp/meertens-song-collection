        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="lament">klaaglied</tag>
    </tags>
    <melody>Stortenbeker en Geerdeken</melody>
    <categories>
      <category translation="emotions">emoties</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Felle Winter <rhyme label="a" type="m">kout</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghy komt te vinnigh <rhyme label="b" type="m">an</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ghy laet mijn Turff noch <rhyme label="a" type="m" corresp="#s1:l1">Hout</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat ick my warmen <rhyme label="b" type="m" corresp="#s1:l2">kan</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ghy rooft decksel en <rhyme label="c" type="f">Kleeren</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Mijn pluymigh Bedt, en <rhyme label="d" type="m">al</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Dus moet ick armen <rhyme label="b" type="m" corresp="#s1:l2">Man</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Nu rusten op de langhe <rhyme label="c" type="f" corresp="#s1:l5">Veeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Koorts, al is hy <rhyme label="a" type="m">quaedt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die komt maer <rhyme label="b" type="m">al-te-met</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Maer ghy hebt vroegh en <rhyme label="a" type="m" corresp="#s2:l1">laet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Mijn Lichaem soo <rhyme label="b" type="m" corresp="#s2:l2">besmet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Met schudden en met <rhyme label="c" type="f">beven</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Ick heb mijn beste <rhyme label="d" type="m">Kleedt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Om uwent wil <rhyme label="b" type="m" corresp="#s2:l2">verset</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">'t Gheen my de Somer had <rhyme label="c" type="f" corresp="#s2:l5">ghegheven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo droevigh als ick <rhyme label="a" type="m">leef</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gheen tongh verhalen <rhyme label="b" type="m">ken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Den Baes daer ick voor <rhyme label="a" type="m" corresp="#s3:l1">weef</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Nu ick bevroren <rhyme label="b" type="m" corresp="#s3:l2">ben</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En wil niet langher <rhyme label="c" type="f">schieten</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Bot vangh ick <rhyme label="d" type="m">overal</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Waer ick my keer of <rhyme label="b" type="m" corresp="#s3:l2">wen</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Soud' my dat leven niet <rhyme label="c" type="f" corresp="#s3:l5">verdrieten</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daer is noch Vriendt noch <rhyme label="a" type="m">Maegh</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die vraeght hoe dat ick <rhyme label="b" type="m">vaer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dus leef ick alle <rhyme label="a" type="m" corresp="#s4:l1">daegh</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ghelijck een <rhyme label="b" type="m" corresp="#s4:l2">Kluysenaer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Maer 's avondts, hoort mijn <rhyme label="c" type="f">karmen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dan soeck ick by de <rhyme label="d" type="m">straet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wat spaenders hier of <rhyme label="b" type="m" corresp="#s4:l2">daer</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Om my een meynigh by te <rhyme label="c" type="f" corresp="#s4:l5">warmen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Tot Oompjes op den <rhyme label="a" type="m">hoeck</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Al van de <rhyme label="b" type="m">Gorte-steegh</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Daer staet mijn beste <rhyme label="a" type="m" corresp="#s5:l1">Broeck</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daer ick wat geldt op <rhyme label="b" type="m" corresp="#s5:l2">kreegh</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Maer dat en mach niet <rhyme label="c" type="f">maecken</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Ick voel mijn Buydel <rhyme label="d" type="m">plat</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En mijn Tresoor is <rhyme label="b" type="m" corresp="#s5:l2">leegh</rhyme>;</l>
        <l id="s5:l8" met="4" type="-+">Hoe sal ick aen de kost noch <rhyme label="c" type="f" corresp="#s5:l5">raecken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick maeck mijn wooningh <rhyme label="a" type="m">kael</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">De Plancken in de <rhyme label="b" type="m">Schouw</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Verbrand ick <rhyme label="a" type="m" corresp="#s6:l1">al-te-mael</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Door d'overgroote <rhyme label="b" type="m" corresp="#s6:l2">kouw</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Daer toe mijn <rhyme label="c" type="f">Onderlaghen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En oock de Sit-planck <rhyme label="d" type="m">mede</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Van mijn ghehuert <rhyme label="b" type="m" corresp="#s6:l2">Ghetouw</rhyme>:</l>
        <l id="s6:l8" met="4" type="-+">Ick speel, die vindt en heeft geen <rhyme label="c" type="f" corresp="#s6:l5">klagen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Prince, ten waer gheen <rhyme label="a" type="m">noodt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Waer ick dees Winter <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Soo kocht ick Kaes en <rhyme label="a" type="m" corresp="#s7:l1">Broodt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Daer kreegh ick weer <rhyme label="b" type="m" corresp="#s7:l2">credijt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Nu wil my niemandt <rhyme label="c" type="f">borghen</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Krijgh ick nae dese <rhyme label="d" type="m">Kouw</rhyme></l>
        <l id="s7:l7" met="3" type="-+">De soete <rhyme label="b" type="m" corresp="#s7:l2">Somer-tijdt</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Soo sal ick beter voor my <rhyme label="c" type="f" corresp="#s7:l5">sorgen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
