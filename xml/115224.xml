  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CAtoken scvhoon <rhyme label="a" type="m">vrindin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghy staet in mijnen <rhyme label="a" type="m" corresp="#s1:l1">sin</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ach siet hoe dat ick u <rhyme label="a" type="m" corresp="#s1:l1">bemin</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ick roep om u schoon <rhyme label="b" type="m">bruydt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ey steeckt u hooft eens <rhyme label="b" type="m" corresp="#s1:l4">uyt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Aenhoort doch eens mijn droef <rhyme label="b" type="m" corresp="#s1:l4">geluyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wie hoor ick suchten <rhyme label="a" type="m">nou</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die my biet sijne <rhyme label="a" type="m" corresp="#s2:l1">Trou</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Het is me Joncker van ’t <rhyme label="a" type="m" corresp="#s2:l1">Getou</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wech wech niet meer en <rhyme label="b" type="m">brult</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Sander met uwen <rhyme label="b" type="m" corresp="#s2:l4">bult</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Want ghy zijt door de min <rhyme label="b" type="m" corresp="#s2:l4">vervult</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn Lieveken <rhyme label="a" type="m">waerom</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Waer ick u <rhyme label="a" type="m" corresp="#s3:l1">bruydegom</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ick sou u eeren schoone <rhyme label="a" type="m" corresp="#s3:l1">blom</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ick sal u eeren schoon <rhyme label="b" type="m">maeght</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Soo ghy my liefde <rhyme label="b" type="m" corresp="#s3:l4">draeght</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En mijn beleeftheyt u <rhyme label="b" type="m" corresp="#s3:l4">behaeght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat sou ick met u <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghy siet als een <rhyme label="a" type="m" corresp="#s4:l1">Callekoen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En daer by noch en vies <rhyme label="a" type="m" corresp="#s4:l1">fatsoen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ghy siet als <rhyme label="b" type="m">baviaen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy en staet my niet <rhyme label="b" type="m" corresp="#s4:l4">aen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Wilt doch by een ander <rhyme label="b" type="m" corresp="#s4:l4">gaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Kaetje ghy zijt soo <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Stoot my niet met den <rhyme label="a" type="m" corresp="#s5:l1">voet</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En siet voor u doch wat ghy <rhyme label="a" type="m" corresp="#s5:l1">doet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ghy sult de Liefste <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En in den Arm van <rhyme label="b" type="m" corresp="#s5:l4">mijn</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Rusten in de <rhyme label="b" type="m" corresp="#s5:l4">mane-schijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">‘k Lagh liever in het <rhyme label="a" type="m">Schelt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Laet my doch <rhyme label="a" type="m" corresp="#s6:l1">ongequelt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Want ghy den Aep my te seer <rhyme label="a" type="m" corresp="#s6:l1">spelt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ghy vlugh u heel lijf <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Heeft eenen vuylen <rhyme label="b" type="m" corresp="#s6:l4">geur</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">En uwen bult die staet van <rhyme label="b" type="m" corresp="#s6:l4">veur</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Kaetjen wat swaerder <rhyme label="a" type="m">pack</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Leyt ghy op mijnen <rhyme label="a" type="m" corresp="#s7:l1">sack</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Lief ten is maer van den <rhyme label="a" type="m" corresp="#s7:l1">Tabak</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ick min u schoone <rhyme label="b" type="m">blom</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Komt dan Lieveken <rhyme label="b" type="m" corresp="#s7:l4">kom</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">En omhelst u <rhyme label="b" type="m" corresp="#s7:l4">bruydegom</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Veeght uwen baert eerst <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ghy zijt een schots <rhyme label="a" type="m" corresp="#s8:l1">persoon</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Waer in de Vogels houden <rhyme label="a" type="m" corresp="#s8:l1">woon</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Daer loopen in u <rhyme label="b" type="m">broeck</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">In soo menigen <rhyme label="b" type="m" corresp="#s8:l4">hoeck</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Luysen en vloyen ghy <rhyme label="b" type="m" corresp="#s8:l4">koekoeck</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick hebse al <rhyme label="a" type="m">verbrant</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">‘k Ben suyver als een <rhyme label="a" type="m" corresp="#s9:l1">vis-mant</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Liefken geeft my doch u <rhyme label="a" type="m" corresp="#s9:l1">hant</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Want ick ben waerachtigh in <rhyme label="b" type="m">ly</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">O ghy drollige <rhyme label="b" type="m" corresp="#s9:l4">pry</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Ick bid u Kaetjen staet my <rhyme label="b" type="m" corresp="#s9:l4">by</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ick smijt u diefschen <rhyme label="a" type="m">Uyl</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">In u backhuys ’t is soo <rhyme label="a" type="m" corresp="#s10:l1">vuyl</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">En met mijnen natten <rhyme label="a" type="m" corresp="#s10:l1">muyl</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Wegh buffel plompe <rhyme label="b" type="m">koe</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Ick smijt mijn Venster <rhyme label="b" type="m" corresp="#s10:l4">toe</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Want ick ben uwen praet heel <rhyme label="b" type="m" corresp="#s10:l4">moe</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Nu magh ick gaen naer <rhyme label="a" type="m">Huys</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Soo stillekens als een <rhyme label="a" type="m" corresp="#s11:l1">muys</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En alsoo kael gelijck een <rhyme label="a" type="m" corresp="#s11:l1">luys</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">‘k Meyn ’t meysken op te <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Met een goet <rhyme label="b" type="m" corresp="#s11:l4">fatsoen</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">maer maeckt my uyt voor een <rhyme label="b" type="m" corresp="#s11:l4">rappoen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
