<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184931</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O Wat is 't een groote <rhyme label="a" type="m">schat</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Voor die 't <rhyme label="a" type="m" corresp="#s1:l1">vat</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Reyn en kuysch te zijn van <rhyme label="b" type="f">binnen</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Te versaken 'svleesches <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Om hier <rhyme label="c" type="m" corresp="#s1:l4">rust</rhyme></l>
        <l id="s1:l6" met="4" type="+-">In 't gemoedt by Godt te <rhyme label="b" type="f" corresp="#s1:l3">winnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schoon d'onkuysche sich <rhyme label="a" type="m">verbeeldt</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Enck'le <rhyme label="a" type="m" corresp="#s2:l1">weeld'</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'t Is geen ware vreughd, maer <rhyme label="b" type="f">logen</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Hy die 't hert daer henen <rhyme label="c" type="m">wend</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Vind in 't <rhyme label="c" type="m" corresp="#s2:l4">end</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Sich te jammerlijck <rhyme label="b" type="f" corresp="#s2:l3">bedrogen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer die daer van sich <rhyme label="a" type="m">vermijd</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">En <rhyme label="a" type="m" corresp="#s3:l1">besnijd</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Sich inwendigh, schoon met <rhyme label="b" type="f">smerten</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Hy gedenck', hoe d'Heer toe <rhyme label="c" type="m">seyd</rhyme></l>
        <l id="s3:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s3:l4">Saligheydt</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Aen de reyn en kuysche <rhyme label="b" type="f" corresp="#s3:l3">herten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Herten, die van <rhyme label="a" type="m">slaverny</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Los en <rhyme label="a" type="m" corresp="#s4:l1">vry</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Deser aerde, alleen na <rhyme label="b" type="f">jagen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dats' ontleeght van vleesches <rhyme label="c" type="m">weeld</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s4:l4">Onverdeelt</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Gode dienen en <rhyme label="b" type="f" corresp="#s4:l3">behagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hier om buygh ick my, o <rhyme label="a" type="m">Heer</rhyme>!</l>
        <l id="s5:l2" met="2" type="+-">Voor u <rhyme label="a" type="m" corresp="#s5:l1">ne'er</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">O verstercker van de <rhyme label="b" type="f">swacken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Op dat ick des vleesches <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s5:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s5:l4">Overwin</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Laet u hulp my niet <rhyme label="b" type="f" corresp="#s5:l3">ontsacken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Heb gemaeckt een <rhyme label="a" type="m">oog-verbond</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">En met <rhyme label="a" type="m" corresp="#s6:l1">mond</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Om die na u wil te <rhyme label="b" type="f">richten</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Geeft, dat ick met <rhyme label="c" type="m">d'oogen-le'en</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Of met <rhyme label="c" type="m" corresp="#s6:l4">re'en</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Noyt my selfs, of and're <rhyme label="b" type="f" corresp="#s6:l3">ontstichte</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hoed mijn sinnen en <rhyme label="a" type="m">gedacht</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Door u <rhyme label="a" type="m" corresp="#s7:l1">wacht</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Datse 't herte noyt <rhyme label="b" type="f">verlocken</rhyme>;</l>
        <l id="s7:l4" met="4" type="+-">Dat ick noyt van binnen <rhyme label="c" type="m">voe</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Maer <rhyme label="c" type="m" corresp="#s7:l4">uytroe</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Al wat wellust aen kan <rhyme label="b" type="f" corresp="#s7:l3">focken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Treck mijn liefde van der <rhyme label="a" type="m">aerd'</rhyme></l>
        <l id="s8:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s8:l1">Hemelwaerd'</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Tot u, en die sael'ge <rhyme label="b" type="f">erve</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Laet geduerigh daer mijn <rhyme label="c" type="m">geest</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Houden <rhyme label="c" type="m" corresp="#s8:l4">feest</rhyme>.</l>
        <l id="s8:l6" met="4" type="+-">Dan sal aerdsche lust <rhyme label="b" type="f" corresp="#s8:l3">versterven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
