<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>29557</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1602</year-end>
    </date>
    <location>
      <orginal-name>zuid-holland (dordrecht?)</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag>admonishing song</tag>
      <tag>secular song</tag>
    </tags>
    <melody>Schoon liefje jent zeer excellent</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ghy minnaers weest van sinnen <rhyme label="a" type="m">vroet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Gaet met u lief <rhyme label="b" type="f">spanceren</rhyme></l>
        <l id="s1:l3" met="4" type="-+">En toent een onbeveynst <rhyme label="a" type="m" corresp="#s1:l1">gemoet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met lieve woorden <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Het sal syn u <rhyme label="a" type="m" corresp="#s1:l1">voorspoet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En haer vrundtlyck <rhyme label="b" type="f" corresp="#s1:l2">begeeren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een oprecht hert is <rhyme label="a" type="m">t'fundament</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Daer liefte compt <rhyme label="b" type="f">uutrysen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Tot sulcks u altyt naerstich <rhyme label="a" type="m" corresp="#s2:l1">went</rhyme></l>
        <l id="s2:l4" met="3" type="-+">T'is een minnelyck <rhyme label="a" type="m" corresp="#s2:l1">present</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wandt vrundtschap sonder <rhyme label="a" type="m" corresp="#s2:l1">ent</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Cunt ghy als dan <rhyme label="b" type="f" corresp="#s2:l2">vercryghen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Scheut vleys wellust neemp cuisheyt <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En leert u doch <rhyme label="b" type="f">bedwinghen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Of anders vint ghy u <rhyme label="a" type="m" corresp="#s3:l1">verraen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met boosheyt quaet <rhyme label="a" type="m" corresp="#s3:l1">belaen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Wilt daer vry <rhyme label="a" type="m" corresp="#s3:l1">tegenstaen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En met waerheyt <rhyme label="b" type="f" corresp="#s3:l2">omringhen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Quaedt ingheven doch altoos <rhyme label="a" type="m">mydt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wildt ghy het u niet <rhyme label="b" type="f">beclaeghen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Maer toont een cunst d'welck t'hert <rhyme label="a" type="m" corresp="#s4:l1">verblyt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Sonder eenich <rhyme label="a" type="m" corresp="#s4:l1">respyt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wandt sulcks tot alder <rhyme label="a" type="m" corresp="#s4:l1">tyt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Malcander doet <rhyme label="b" type="f" corresp="#s4:l2">behaegen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Laet musica niet syn <rhyme label="a" type="m">beschaempt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Een sanck seer soet <rhyme label="b" type="f">gepresen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Retorica oock daer by <rhyme label="a" type="m" corresp="#s5:l1">genaempt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Door goede cunst <rhyme label="a" type="m" corresp="#s5:l1">befaempt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Oock noch seer wel <rhyme label="a" type="m" corresp="#s5:l1">betaempt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Snaerspel daer by te <rhyme label="b" type="f" corresp="#s5:l2">wesen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dit is het welck t'hert <rhyme label="a" type="m">verheucht</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Melancoly laet <rhyme label="b" type="f">vaeren</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Bewyst malcander liefde in <rhyme label="a" type="m" corresp="#s6:l1">deucht</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soo ist dat ghy <rhyme label="a" type="m" corresp="#s6:l1">vermeucht</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Te maecken groot <rhyme label="a" type="m" corresp="#s6:l1">geneucht</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Sonder eenich <rhyme label="b" type="f" corresp="#s6:l2">beswaeren</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Coragieus met minne <rhyme label="a" type="m">eerbaer</rhyme></l>
        <l id="s7:l2" met="3" type="-+">U liefs secreet wilt <rhyme label="b" type="f">swigen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Oprecht eendracht toont ghy <rhyme label="a" type="m" corresp="#s7:l1">aldaer</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Heymelyck ofte <rhyme label="a" type="m" corresp="#s7:l1">openbaer</rhyme></l>
        <l id="s7:l5" met="3" type="-+">dat sal syn u <rhyme label="a" type="m" corresp="#s7:l1">voorwaer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Om lof prys te <rhyme label="b" type="f" corresp="#s7:l2">vercryghen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Oorlof princen den tyt <rhyme label="a" type="m">verbeyt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En wilt gerustich <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Wanneer ghy syt tot deucht <rhyme label="a" type="m" corresp="#s8:l1">bereyt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">U naem wort dan <rhyme label="a" type="m" corresp="#s8:l1">verspreyt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Veel eers dan u <rhyme label="a" type="m" corresp="#s8:l1">geseyt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In wellust wilt niet <rhyme label="b" type="f" corresp="#s8:l2">sneven</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
