<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178931</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ben ik van Duitschen <rhyme label="b" type="m">Bloed</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Het Vaderland <rhyme label="a" type="f" corresp="#s1:l1">getrouwe</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Blijf ik tot in den <rhyme label="b" type="m" corresp="#s1:l2">dood</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Een Prince van <rhyme label="c" type="f">Oranjen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ben ik vrij <rhyme label="d" type="m">onverveert</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Den Koning van <rhyme label="c" type="f" corresp="#s1:l5">Hispanjen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Heb ik altijd <rhyme label="d" type="m" corresp="#s1:l6">geëert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In Godes vrees te <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Heb ik altijd <rhyme label="b" type="m">betracht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Daarom ben ik <rhyme label="a" type="f" corresp="#s2:l1">verdreeven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Om land' en luid <rhyme label="b" type="m" corresp="#s2:l2">gebragt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Maar God zal mij <rhyme label="c" type="f">regeeren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Als een goed <rhyme label="d" type="m">Instrument</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat ik mag <rhyme label="c" type="f" corresp="#s2:l5">weederkeeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">In mijnen <rhyme label="d" type="m" corresp="#s2:l6">Regiment</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Lijdt u mijn <rhyme label="a" type="f">ondersaten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die oprecht zijn van <rhyme label="b" type="m">aart</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">God zal u niet <rhyme label="a" type="f" corresp="#s3:l1">verlaten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al zijt gij nu <rhyme label="b" type="m" corresp="#s3:l2">beswaart</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die vroom begeert te <rhyme label="c" type="f">leeven</rhyme>;</l>
        <l id="s3:l6" met="3" type="-+">Bid God nacht ende <rhyme label="d" type="m">dag</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat Hij mij kracht wil <rhyme label="c" type="f" corresp="#s3:l5">geven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dat ik u helpen <rhyme label="d" type="m" corresp="#s3:l6">mag</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Lyf end' goed al te <rhyme label="a" type="f">samen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Heb ik ook niet <rhyme label="b" type="m">verschoont</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Myn Broeders hoog van <rhyme label="a" type="f" corresp="#s4:l1">namen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hebben 't U oock <rhyme label="b" type="m" corresp="#s4:l2">vertoont</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Graaf Adolf is <rhyme label="c" type="f">gebleeven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In Vriesland in den <rhyme label="d" type="m">slag</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Zyn Ziel in 't eeuwig <rhyme label="c" type="f" corresp="#s4:l5">leven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Verwacht den jongsten <rhyme label="d" type="m" corresp="#s4:l6">dag</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Edel en hoog <rhyme label="a" type="f">geboren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van Keyzerlyke <rhyme label="b" type="m">Stam</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Een Vorst des ryks <rhyme label="a" type="f" corresp="#s5:l1">verkoren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Als een vroom Christen <rhyme label="b" type="m" corresp="#s5:l2">man</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Voor Godes woord <rhyme label="c" type="f">gepreesen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Heb ik vry <rhyme label="d" type="m">onvertsaegt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Als een Held sonder <rhyme label="c" type="f" corresp="#s5:l5">vreezen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Myn Edel bloed <rhyme label="d" type="m" corresp="#s5:l6">gewaagt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Myn schilt en de <rhyme label="a" type="f">betrouwen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zyt gy, ô God myn <rhyme label="b" type="m">Heer</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">Op u soo wil ik <rhyme label="a" type="f" corresp="#s6:l1">bouwen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Verlaat my <rhyme label="b" type="m" corresp="#s6:l2">nimmermeer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat ik doch vroom mag <rhyme label="c" type="f">blyven</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">U dienaar t'aller <rhyme label="d" type="m">stond</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">De Tieranny <rhyme label="c" type="f" corresp="#s6:l5">verdwynen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Die myn herte <rhyme label="d" type="m" corresp="#s6:l6">ontvonkt</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Van al die myn <rhyme label="a" type="f">beswaren</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">End' myn vervolgers <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">ô God wilt dog <rhyme label="a" type="f" corresp="#s7:l1">bewaaren</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Den trouwen Dienaar <rhyme label="b" type="m" corresp="#s7:l2">dyn</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dat sy my niet <rhyme label="c" type="f">verrasschen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">In haren bosen <rhyme label="d" type="m">moet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Haar handen niet en <rhyme label="c" type="f" corresp="#s7:l5">wasschen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">In myn onschuldig <rhyme label="d" type="m" corresp="#s7:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Als David moeste <rhyme label="a" type="f">vluchten</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Voor Saul den <rhyme label="b" type="m">Tyran</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Soo heb ik moeten <rhyme label="a" type="f" corresp="#s8:l1">zuchten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Met meenig <rhyme label="b" type="m" corresp="#s8:l2">Edelman</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Maar God heeft hem <rhyme label="c" type="f">verheeven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Verlost uit aller <rhyme label="d" type="m">nood</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Een Koninkryk <rhyme label="c" type="f" corresp="#s8:l5">gegeeven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">In Israel soo <rhyme label="d" type="m" corresp="#s8:l6">groot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Na 't suir sal ik <rhyme label="a" type="f">ontfangen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Van God myn Heer dat soet <rhyme label="b" type="m">j</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Daar na zoo <rhyme label="a" type="f" corresp="#s9:l1">verlangen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Myn Vorstelyk <rhyme label="b" type="m" corresp="#s9:l2">gemoed</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">'t Welk is dat ik mag <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Met eeren in het <rhyme label="d" type="m">veld</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Een eeuwigryk <rhyme label="c" type="f" corresp="#s9:l5">verwerven</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Als een getrouwen <rhyme label="d" type="m" corresp="#s9:l6">Held</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Niet doet my meer <rhyme label="a" type="f">erbarmen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">In mijnen <rhyme label="b" type="m">wederspoet</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Dan dat men siet <rhyme label="a" type="f" corresp="#s10:l1">verarmen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Des Koninks Landen <rhyme label="b" type="m" corresp="#s10:l2">goed</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Dat u de Spanjaarts <rhyme label="c" type="f">krenken</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">ô Edel Neêrland <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Als ik dat ga <rhyme label="c" type="f" corresp="#s10:l5">bedenken</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Mijn Edel hert dat <rhyme label="d" type="m" corresp="#s10:l6">bloet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Als een Prins <rhyme label="a" type="f">opgeseeten</rhyme>;</l>
        <l id="s11:l2" met="3" type="-+">Met mijnes Heirs <rhyme label="b" type="m">kragt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Van den Tieran <rhyme label="a" type="f" corresp="#s11:l1">vermeten</rhyme>;</l>
        <l id="s11:l4" met="3" type="-+">Heb ik den slag <rhyme label="b" type="m" corresp="#s11:l2">verwagt</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Die bij Maestricht <rhyme label="c" type="f">begraven</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Bevreesden mijn <rhyme label="d" type="m">geweld</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Mijn Riters sag men <rhyme label="c" type="f" corresp="#s11:l5">draven</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Seer moedig door het <rhyme label="d" type="m" corresp="#s11:l6">veld</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Soo het die wil des <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Op die tijd had <rhyme label="b" type="m">geweest</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Had ik wel willen <rhyme label="a" type="f" corresp="#s12:l1">keeren</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Van u dit zwaar <rhyme label="b" type="m" corresp="#s12:l2">tempeest</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Maar den Heer van hier <rhyme label="c" type="f">boven</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Die alle ding <rhyme label="d" type="m">regeert</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Die men altijd moet <rhyme label="c" type="f" corresp="#s12:l5">looven</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">En heeft het niet <rhyme label="d" type="m" corresp="#s12:l6">begeert</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Seer Christ'lijck was <rhyme label="a" type="f">gedreeven</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Mijn Prinselijk <rhyme label="b" type="m">gemoet</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Standvastig is <rhyme label="a" type="f" corresp="#s13:l1">gebleeven</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Mijn hert in <rhyme label="b" type="m" corresp="#s13:l2">tegenspoed</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Den Heer heb ik <rhyme label="c" type="f">gebeden</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Van mijnes herten <rhyme label="d" type="m">grond</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Dat hij mijn zaak wil <rhyme label="c" type="f" corresp="#s13:l5">reden</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Mijn onschuld doen <rhyme label="d" type="m" corresp="#s13:l6">oorkont</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Oorlof mijn arme <rhyme label="a" type="f">schapen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Die zijt in grooten <rhyme label="b" type="m">nood</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">U Herder sal niet <rhyme label="a" type="f" corresp="#s14:l1">slapen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Al zijt gij nu <rhyme label="b" type="m" corresp="#s14:l2">verstrooit</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Tot God wilt u <rhyme label="c" type="f">begeeven</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Zijn heilzaam woord neem <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Als vroome Christen <rhyme label="c" type="f" corresp="#s14:l5">leven</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">'t Zal hier haast zijn <rhyme label="d" type="m" corresp="#s14:l6">gedaan</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Voor God wil ik <rhyme label="a" type="f">belijden</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">En zijnen grooter <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Dat ik tot geenen <rhyme label="a" type="f" corresp="#s15:l1">tijden</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Den Konink heb <rhyme label="b" type="m" corresp="#s15:l2">veracht</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Dan dat ik God den <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Den hoogster <rhyme label="d" type="m">Majesteit</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Heb moeten <rhyme label="c" type="f" corresp="#s15:l5">obedieren</rhyme></l>
        <l id="s15:l8" met="3" type="-+">In der <rhyme label="d" type="m" corresp="#s15:l6">geregtigheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
