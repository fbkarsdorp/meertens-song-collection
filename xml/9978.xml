<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9978</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Fietje Floris mijn speelmeisje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IOosje wil een Vryster <rhyme label="a" type="f">kopen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">voor sijn langh verspaerde <rhyme label="b" type="m">gelt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Och de knecht is so <rhyme label="b" type="m" corresp="#s1:l2">ontstelt</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">ick moet hem wat helpen <rhyme label="a" type="f" corresp="#s1:l1">lopen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">In dees jammerlijcke <rhyme label="c" type="m">noodt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">of hy krijt sich selven <rhyme label="c" type="m" corresp="#s1:l5">doodt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">k'Weet by gort een aerdigh <rhyme label="a" type="f">Meysje</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Soo van Aensicht als van <rhyme label="b" type="m">Lijf</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Had hy dees maer tot sijn <rhyme label="b" type="m" corresp="#s2:l2">Wijf</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Wel is waer 'tis kost'lijck <rhyme label="a" type="f" corresp="#s2:l1">Vleysje</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Hy komt me van rijcke <rhyme label="c" type="m">Luy</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Dat en scheelt dan niet een <rhyme label="c" type="m" corresp="#s2:l5">Bruy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Heeft sy Paerden, hy de <rhyme label="a" type="f">Waghen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Heeft sy Vleys dat haer <rhyme label="b" type="m">verveelt</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Hy is oock niet mis <rhyme label="b" type="m" corresp="#s3:l2">ghedeelt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Want sijn Vaer heeft sorgh <rhyme label="a" type="f" corresp="#s3:l1">ghedraghen</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Dat 's gien Spaens, verstaje <rhyme label="c" type="m">dat</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">'t Is een Hylick in sijn <rhyme label="c" type="m" corresp="#s3:l5">gat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Schoon al isse Mal <rhyme label="a" type="f">ghebooren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Groot van Mondt, van Neus van <rhyme label="b" type="m">Kin</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat en brenght gien onlust <rhyme label="b" type="m" corresp="#s4:l2">in</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Want hy heeft me wat <rhyme label="a" type="f" corresp="#s4:l1">verlooren</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Daerom kies ick desen <rhyme label="c" type="m">uyt</rhyme>;</l>
        <l id="s4:l6" met="4" type="+-">'t Zijn twee Pen'gen om een <rhyme label="c" type="m" corresp="#s4:l5">Duyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hoe sel Ioosje dan gaen <rhyme label="a" type="f">proncken</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Met sijn Brakje op de <rhyme label="b" type="m">zy</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Struys, hier is gien weerga <rhyme label="b" type="m" corresp="#s5:l2">by</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Die soo mooy beknopt kan <rhyme label="a" type="f" corresp="#s5:l1">loncken</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Al het Volck raekt op de <rhyme label="c" type="m">bien</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Om dit Paertjen eens te <rhyme label="c" type="m" corresp="#s5:l5">zien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
