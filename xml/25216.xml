  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O god, der dingen <rhyme label="a" type="m">onderhout</rhyme></l>
        <l id="s1:l2" met="4" type="-+">in v bliuende <rhyme label="b" type="m">onberoert</rhyme></l>
        <l id="s1:l3" met="4" type="-+">ghij die der dagen lichten <rhyme label="a" type="m" corresp="#s1:l1">stout</rhyme></l>
        <l id="s1:l4" met="4" type="-+">geeft doer veranderingh <rhyme label="b" type="m" corresp="#s1:l2">confoort</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Verleent ons een claer <rhyme label="a" type="m">auondstont</rhyme></l>
        <l id="s2:l2" met="4" type="-+">dat ons leuen ons niet <rhyme label="b" type="m">falgeert</rhyme></l>
        <l id="s2:l3" met="4" type="-+">maer j. heijlich doets verdiensten <rhyme label="a" type="m" corresp="#s2:l1">iont</rhyme></l>
        <l id="s2:l4" met="4" type="-+">bij ons altijt blijft en <rhyme label="b" type="m" corresp="#s2:l2">vermeert</rhyme></l>
      </lg>
    </lg>
  </text>
