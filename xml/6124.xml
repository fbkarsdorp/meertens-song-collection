        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Cur mundus militat</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SEght wat helpt alle <rhyme label="a" type="m">welt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met allen goet en <rhyme label="a" type="m" corresp="#s1:l1">gelt</rhyme> ?</l>
        <l id="s1:l3" met="3" type="-+">Alles verdwijnt <rhyme label="b" type="m">geswindt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gelijck roock inden <rhyme label="b" type="m" corresp="#s1:l3">windt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat helpt den hoogen <rhyme label="a" type="m">throon</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Den Scepter, en den <rhyme label="a" type="m" corresp="#s2:l1">Croon</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Scepter en <rhyme label="b" type="m">Regiment</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Heeft alles haest een <rhyme label="b" type="m" corresp="#s2:l3">endt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat helpt sijn hups en <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Schoon als die Eng'len <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Schoonheyt vergaet int <rhyme label="b" type="m">graf</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die roosen vallen <rhyme label="b" type="m" corresp="#s3:l3">af</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat helpt u gout-geel <rhyme label="a" type="m">hayr</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als Christal oogen <rhyme label="a" type="m" corresp="#s4:l1">claer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Lippen Corallen <rhyme label="b" type="m">root</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Alles vergaet ten <rhyme label="b" type="m" corresp="#s4:l3">doot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wat is dat gulden <rhyme label="a" type="m">stuck</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van gout, steen en <rhyme label="a" type="m" corresp="#s5:l1">gesmuck</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gout is nu roode <rhyme label="b" type="m">aerdt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die aerdt is niet veel <rhyme label="b" type="m" corresp="#s5:l3">waert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat is dat root <rhyme label="a" type="m">gewant</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat purper wert <rhyme label="a" type="m" corresp="#s6:l1">genant</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Van Slecken wt dat <rhyme label="b" type="m">meyr</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Komt alle purper <rhyme label="b" type="m" corresp="#s6:l3">her</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wat is der zijden <rhyme label="a" type="m">pracht</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wie heeft den pracht <rhyme label="a" type="m" corresp="#s7:l1">gewracht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Het hebben worm <rhyme label="b" type="m">gemacht</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Den gantschen zijden <rhyme label="b" type="m" corresp="#s7:l3">pracht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wat zijn dan sulcke <rhyme label="a" type="m">ding</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die ghy acht niet <rhyme label="a" type="m" corresp="#s8:l1">gering</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Aerd, worm-dreck, <rhyme label="b" type="m">slecken-bloet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Is dat ons cieren <rhyme label="b" type="m" corresp="#s8:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Is dat niet <rhyme label="a" type="m">phantasy</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En groote <rhyme label="a" type="m" corresp="#s9:l1">sotterny</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Al is het <rhyme label="b" type="m">geckerny</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En louter <rhyme label="b" type="m" corresp="#s9:l3">phantasy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
