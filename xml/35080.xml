<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35080</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1700</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="dialooglied" translation="dialogue song">dialooglied</tag>
    </tags>
    <melody>Un inconnu pour vos charmes soupire</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Ick voel iets in mijn hert en ziele <rhyme label="a" type="f">zweven</rhyme></l>
        <l id="s1:l2" met="5" type="-+">ick kan niet dencken wat het wesen <rhyme label="b" type="m">magh</rhyme></l>
        <l id="s1:l3" met="4" type="-+">ick queel ick treure van dagh tot <rhyme label="b" type="m" corresp="#s1:l2">dagh</rhyme></l>
        <l id="s1:l4" met="2" type="-+">ah wat <rhyme label="c" type="m">verdriet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">k'besit mijn herte <rhyme label="c" type="m" corresp="#s1:l4">niet</rhyme></l>
        <l id="s1:l6" met="5" type="-+">ghij hebt iris, wilt het mij <rhyme label="a" type="f" corresp="#s1:l1">wedergeven</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">ghij mooght bij mij van uw verlies wel <rhyme label="a" type="f">zwijgen</rhyme></l>
        <l id="s2:l2" met="5" type="-+">k besit geen hert, noch hebb het niet <rhyme label="b" type="m">gewont</rhyme></l>
        <l id="s2:l3" met="4" type="-+">maer weest versekert soo ick het <rhyme label="b" type="m" corresp="#s2:l2">vond</rhyme></l>
        <l id="s2:l4" met="2" type="-+">dat ick het <rhyme label="c" type="m">souw</rhyme></l>
        <l id="s2:l5" met="3" type="-+">bewaeren tot de <rhyme label="c" type="m" corresp="#s2:l4">trouw</rhyme></l>
        <l id="s2:l6" met="5" type="-+">en dat ghij't noijt van mij soud <rhyme label="a" type="f" corresp="#s2:l1">wederkrijgen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">ach weerde maeght mijn hert en ziel <rhyme label="a" type="f">godinne</rhyme></l>
        <l id="s3:l2" met="5" type="-+">mijn hert kan in geen beter handen <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s3:l3" met="4" type="-+">jont mij het uwe bewaert het <rhyme label="b" type="m" corresp="#s3:l2">mijn</rhyme></l>
        <l id="s3:l4" met="2" type="-+">ick ben <rhyme label="c" type="m">voldaen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">soo ghij het wilt <rhyme label="c" type="m" corresp="#s3:l4">ontfaen</rhyme></l>
        <l id="s3:l6" met="5" type="-+">ick sal als u, mijn lief noijt ander <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Dit zijn thirsis de woorden van een <rhyme label="a" type="f">minnaer</rhyme></l>
        <l id="s4:l2" met="5" type="-+">wanneer sijn hert met liefde word <rhyme label="b" type="m">doorgrieft</rhyme></l>
        <l id="s4:l3" met="4" type="-+">soo hadde paris oenone <rhyme label="b" type="m" corresp="#s4:l2">lief</rhyme></l>
        <l id="s4:l4" met="2" type="-+">maer liet haer <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">om naer helen te <rhyme label="c" type="m" corresp="#s4:l4">gaen</rhyme></l>
        <l id="s4:l6" met="5" type="-+">oenone bleef t'slaghoffer der <rhyme label="a" type="f" corresp="#s4:l1">verwinnaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Denckt nimmermeer dat ick het niet en <rhyme label="a" type="f">meene</rhyme></l>
        <l id="s5:l2" met="5" type="-+">ofdat ick oijt als u een ander <rhyme label="b" type="m">min</rhyme></l>
        <l id="s5:l3" met="4" type="-+">kwil u beminnen met hert en <rhyme label="b" type="m" corresp="#s5:l2">sin</rhyme></l>
        <l id="s5:l4" met="2" type="-+">en in mijn <rhyme label="c" type="m">pijn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">magh ick uw paris <rhyme label="c" type="m" corresp="#s5:l4">sijn</rhyme></l>
        <l id="s5:l6" met="5" type="-+">ghij sult altijt iris sijn, mijn <rhyme label="a" type="f" corresp="#s5:l1">heleene</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">soo trouwe min, sou mij op hoop doen <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s6:l2" met="5" type="-+">helena heeft haer minnaer soo <rhyme label="b" type="m">bestiert</rhyme></l>
        <l id="s6:l3" met="4" type="-+">schoon wanckelbaer, dat hij standvastigh <rhyme label="b" type="m" corresp="#s6:l2">wiert</rhyme></l>
        <l id="s6:l4" met="2" type="-+">ick waer <rhyme label="c" type="m">voldaen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">kon ick dien wegh <rhyme label="c" type="m" corresp="#s6:l4">ingaen</rhyme></l>
        <l id="s6:l6" met="5" type="-+">maer wie kan mij daervan versek'ringh <rhyme label="a" type="f" corresp="#s6:l1">geve</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">ghij sult in mij geen valsheijt oijt <rhyme label="a" type="f">ontdelven</rhyme></l>
        <l id="s7:l2" met="5" type="-+">oprechte min niet wel verandren <rhyme label="b" type="m">kan</rhyme></l>
        <l id="s7:l3" met="4" type="-+">jout mij uw trouw bemint mij als uw <rhyme label="b" type="m" corresp="#s7:l2">man</rhyme></l>
        <l id="s7:l4" met="2" type="-+">en vrij <rhyme label="c" type="m">gelooft</rhyme></l>
        <l id="s7:l5" met="3" type="-+">dat sulcke min niet <rhyme label="c" type="m" corresp="#s7:l4">dooft</rhyme></l>
        <l id="s7:l6" met="5" type="-+">van die u mint gelijck sijn eijghen <rhyme label="a" type="f" corresp="#s7:l1">selven</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Thircis ghij weet mij aerdigh te <rhyme label="a" type="f">bekoren</rhyme></l>
        <l id="s8:l2" met="5" type="-+">is t soo met u gelijck uw woorden <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s8:l3" met="4" type="-+">mijn liefd' is meerder als dat <rhyme label="b" type="m" corresp="#s8:l2">schijnt</rhyme></l>
        <l id="s8:l4" met="2" type="-+">dat dan geen <rhyme label="c" type="m">tijdt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">ons beijde min en <rhyme label="c" type="m" corresp="#s8:l4">seijt</rhyme></l>
        <l id="s8:l6" met="5" type="-+">sijt ghij voor mij ick ben voor u <rhyme label="a" type="f" corresp="#s8:l1">geboren</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
