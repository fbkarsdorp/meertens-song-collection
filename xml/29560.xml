      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Heere hooch <rhyme label="a" type="f">gepresen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hoe lange wil dit <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat ick blyf in dit <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dies lyde ick groote <rhyme label="b" type="m" corresp="#s1:l2">pyn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Aenhoort myn stemme <rhyme label="c" type="f">clachtich</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ick roup nacht ende <rhyme label="d" type="m">dach</rhyme></l>
        <l id="s1:l7" met="3" type="-+">En vindt my gants <rhyme label="c" type="f" corresp="#s1:l5">onmachtich</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dus maeck ick groot <rhyme label="d" type="m" corresp="#s1:l6">geclach</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">T'vleys wil den geest <rhyme label="a" type="f">verwinnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Den stryt die valt seer <rhyme label="b" type="m">swaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ick wil den camp <rhyme label="a" type="f" corresp="#s2:l1">beginnen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Verleent myn u licht <rhyme label="b" type="m" corresp="#s2:l2">claer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Soo mach ick <rhyme label="c" type="f">resisteren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">S'viandts begeerte <rhyme label="d" type="m">quaet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Alle boosheyt <rhyme label="c" type="f" corresp="#s2:l5">afweren</rhyme></l>
        <l id="s2:l8" met="3" type="-+">O godt compt my te <rhyme label="d" type="m" corresp="#s2:l6">baet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een reyn hert wilt my <rhyme label="a" type="f">geven</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Soo sal ick vry voort <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Nae u behaegen <rhyme label="a" type="f" corresp="#s3:l1">leven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dan sal ick rust <rhyme label="b" type="m" corresp="#s3:l2">ontfaen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En my tot uwaerts <rhyme label="c" type="f">keeren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ick bid uut herten <rhyme label="d" type="m">grondt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">U wegen wilt my <rhyme label="c" type="f" corresp="#s3:l5">leeren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Nu doch tot deser <rhyme label="d" type="m" corresp="#s3:l6">stont</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Prins is <rhyme label="a" type="f">voorgetreden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Laet wy hem volgen <rhyme label="b" type="m">nae</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hy heeft voor ons <rhyme label="a" type="f" corresp="#s4:l1">geleden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Op dat wy vrouch en <rhyme label="b" type="m" corresp="#s4:l2">spae</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Syn cruys souden helpen <rhyme label="c" type="f">draegen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Inwendich in <rhyme label="d" type="m">t'gemoet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Om t'goede nae te <rhyme label="c" type="f" corresp="#s4:l5">jaegen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Voor t'quaed ons Heer <rhyme label="d" type="m" corresp="#s4:l6">behoet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
