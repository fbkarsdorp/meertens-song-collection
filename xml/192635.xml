    <melody>O salutaris hostia</melody>
  <author>Christianus Vermeulen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">O Salutaris hostia,</l>
        <l id="s1:l2">Quae coeli pandis ostium,</l>
        <l id="s1:l3">Bella premunt hostilia,</l>
        <l id="s1:l4">Da robur fer auxilium,</l>
        <l id="s1:l5">Da robur fer auxilium.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Defensor noster adspice,</l>
        <l id="s2:l2">Infidiantes reprime,</l>
        <l id="s2:l3">Guberna tuos famulos</l>
        <l id="s2:l4">Quos sanguine mercatuses,</l>
        <l id="s2:l5">Quos sanguine mercatuses.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">Uni trinoque Domino,</l>
        <l id="s3:l2">Sit sempiterna gloria,</l>
        <l id="s3:l3">Qui vitam sine termino</l>
        <l id="s3:l4">Nobis donet in patria,</l>
        <l id="s3:l5">Nobis donet in patria.</l>
      </lg>
    </lg>
  </text>
