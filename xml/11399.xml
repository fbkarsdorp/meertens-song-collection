  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'k LOos noch menig <rhyme label="a" type="f">lachje</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Als ik om 't <rhyme label="a" type="f" corresp="#s1:l1">nachje</rhyme></l>
        <l id="s1:l3" met="2" type="+-">Denk, doen <rhyme label="b" type="f">Trijntje</rhyme></l>
        <l id="s1:l4" met="2" type="+-">My by 't <rhyme label="b" type="f" corresp="#s1:l3">lijntje</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Trok, op 't Roze - <rhyme label="a" type="f" corresp="#s1:l1">grachje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met veelderleye <rhyme label="c" type="f">kuurtjes</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s1:l6">gluurtjes</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">De <rhyme label="c" type="f" corresp="#s1:l6">vuurtjes</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Die daar reezen tussche <rhyme label="d" type="m">bey</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Beletten ons <rhyme label="d" type="m" corresp="#s1:l9">geschey</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'k Loos veel droeve <rhyme label="a" type="f">zuchjes</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Nu 'k die <rhyme label="a" type="f" corresp="#s2:l1">genuchjes</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Door 't <rhyme label="b" type="f">vergisze</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Zoo moet <rhyme label="b" type="f" corresp="#s2:l3">misze</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Met Vrouw Venus <rhyme label="a" type="f" corresp="#s2:l1">kluchjes</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Terwijl dat vierig <rhyme label="c" type="f">quantje</rhyme>,</l>
        <l id="s2:l7" met="1" type="-+">Haar <rhyme label="c" type="f" corresp="#s2:l6">Jantje</rhyme>,</l>
        <l id="s2:l8" met="1" type="-+">Zijn <rhyme label="c" type="f" corresp="#s2:l6">brantje</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Na begeeren, dikwils <rhyme label="d" type="m">lest</rhyme>:</l>
        <l id="s2:l10" met="3" type="-+">En blijft by haar te <rhyme label="d" type="m" corresp="#s2:l9">nest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'k Loos die zotte <rhyme label="a" type="f">zinnen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">WeÃªr zoo te <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme></l>
        <l id="s3:l3" met="2" type="+-">'t Darrel <rhyme label="b" type="f">vleisje</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Van een <rhyme label="b" type="f" corresp="#s3:l3">meisje</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Om maar hoon te <rhyme label="a" type="f" corresp="#s3:l1">winnen</rhyme>;</l>
        <l id="s3:l6" met="3" type="-+">'k Ga liever om een <rhyme label="c" type="f">pijntje</rhyme></l>
        <l id="s3:l7" met="1" type="-+">By <rhyme label="c" type="f" corresp="#s3:l6">Lijntje</rhyme>,</l>
        <l id="s3:l8" met="1" type="-+">Of <rhyme label="c" type="f" corresp="#s3:l6">Stijntje</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Dan ik aan een snoo <rhyme label="d" type="m">Moffin</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Verslijt mijn tijdt en <rhyme label="d" type="m" corresp="#s3:l9">Min</rhyme>.</l>
      </lg>
    </lg>
  </text>
