<song>
  <meta>
    <id>20098</id>
    <date>
      <year-start>1654</year-start>
      <year-end>1654</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>La Vignonne</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL hoe wel ramp, op <rhyme label="a" type="m">ramp</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Gedurigh mijn vreugd en glory eyscht ten <rhyme label="a" type="m" corresp="#s1:l1">kamp</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En valt my <rhyme label="b" type="m">aan</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Om neer te <rhyme label="b" type="m" corresp="#s1:l3">slaan</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Nochtans blijf ik vrymoedigh <rhyme label="b" type="m" corresp="#s1:l3">staan</rhyme>;</l>
        <l id="s1:l6" met="1" type="-+">En <rhyme label="c" type="f">handel</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s1:l6">wandel</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+"><rhyme label="d" type="m">Verblijdt</rhyme>,</l>
        <l id="s1:l9" met="1" type="-+">Tot <rhyme label="d" type="m" corresp="#s1:l8">spijt</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Der swarte <rhyme label="d" type="m" corresp="#s1:l8">nijt</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Met u (O <rhyme label="e" type="m">blom</rhyme>)</l>
        <l id="s1:l12" met="2" type="-+">Mijn <rhyme label="e" type="m" corresp="#s1:l11">eygendom</rhyme></l>
        <l id="s1:l13" met="4" type="-+">Van wien ik deze kracht <rhyme label="e" type="m" corresp="#s1:l11">bekom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn eenigh <rhyme label="a" type="m">toeverlaet</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Daar heden mijn Leven, Hoop, en Heul op <rhyme label="a" type="m" corresp="#s2:l1">staat</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">O! lieve <rhyme label="b" type="m">Meyt</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Vol <rhyme label="b" type="m" corresp="#s2:l3">zoetigheyt</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Wel waart van elk te zijn <rhyme label="b" type="m" corresp="#s2:l3">gevleyt</rhyme>.</l>
        <l id="s2:l6" met="1" type="-+">Al <rhyme label="c" type="f">quamen</rhyme></l>
        <l id="s2:l7" met="1" type="-+">Te <rhyme label="c" type="f" corresp="#s2:l6">samen</rhyme></l>
        <l id="s2:l8" met="1" type="-+">’t <rhyme label="d" type="m">Verwoet</rhyme></l>
        <l id="s2:l9" met="1" type="-+">
          <rhyme label="d" type="m" corresp="#s2:l8">Gebroet</rhyme>
        </l>
        <l id="s2:l10" met="2" type="-+">Wt Stix <rhyme label="d" type="m" corresp="#s2:l8">gewroet</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Tot mijn <rhyme label="e" type="m">verdriet</rhyme>;</l>
        <l id="s2:l12" met="2" type="-+">Ik acht het <rhyme label="e" type="m" corresp="#s2:l11">niet</rhyme></l>
        <l id="s2:l13" met="4" type="-+">Zoo langh my troost van u <rhyme label="e" type="m" corresp="#s2:l11">geschiet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">V aangenaam <rhyme label="a" type="m">gelach</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Beglinstert mijn hert, veel klaerder als den <rhyme label="a" type="m" corresp="#s3:l1">dagh</rhyme>.</l>
        <l id="s3:l3" met="2" type="-+">V hel <rhyme label="b" type="m">gezight</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Mijn blyschap <rhyme label="b" type="m" corresp="#s3:l3">sticht</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">V lieve mont mijn druck <rhyme label="b" type="m" corresp="#s3:l3">verlicht</rhyme>;</l>
        <l id="s3:l6" met="1" type="-+">V <rhyme label="c" type="f">leden</rhyme></l>
        <l id="s3:l7" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s3:l6">zeden</rhyme></l>
        <l id="s3:l8" met="1" type="-+">Vol <rhyme label="d" type="m">Deugd</rhyme>’</l>
        <l id="s3:l9" met="1" type="-+">
          <rhyme label="d" type="m" corresp="#s3:l8">Verheughd</rhyme>
        </l>
        <l id="s3:l10" met="2" type="-+">Mijn ziel met <rhyme label="d" type="m" corresp="#s3:l8">vreughd</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Dies ik, <rhyme label="e" type="m">Godin</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Door trouwe <rhyme label="e" type="m" corresp="#s3:l11">Min</rhyme>,</l>
        <l id="s3:l13" met="4" type="-+">My sonder u niet <rhyme label="e" type="m" corresp="#s3:l11">vin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
