<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188768</id>
    <date>
      <year-start>1880</year-start>
      <year-end>1880</year-end>
    </date>
    <location>
      <orginal-name>nijmegen</orginal-name>
      <standard-name>Nijmegen, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8427385</lon>
        <lat>5.8634696</lat>
      </coordinates>
