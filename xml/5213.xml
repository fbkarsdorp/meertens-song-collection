<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5213</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag standard-name="geuzenlied" translation="Geuzen song">geuzenlied</tag>
      <tag standard-name="triomflied" translation="triumph song">triomflied</tag>
    </tags>
    <melody>Met lusten willen wij zingen en loven dat Roomse rijk</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Verheucht u ghy <rhyme label="a" type="f">Nederlanden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Verheucht u tis meer dan <rhyme label="b" type="m">tijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ghy nu siet comen tot <rhyme label="a" type="f" corresp="#s1:l1">schanden</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die u dus seere hebben <rhyme label="b" type="m" corresp="#s1:l2">benijt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Den Spaengiaert blijft int Nederlants <rhyme label="b" type="m" corresp="#s1:l2">crijt</rhyme></l>
        <l id="s1:l6" met="4" type="-+">In spijt van haer moorden en <rhyme label="c" type="f">branden</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Dies sy Godt den Heere <rhyme label="b" type="m" corresp="#s1:l2">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Brabant en Vlaenderen <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Artoys ende Henegouwe <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die hebben als Utrecht <rhyme label="a" type="f" corresp="#s2:l1">ydoone</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Haer verbonden met den Prince <rhyme label="b" type="m" corresp="#s2:l2">ghoet</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Om te verdrijven dat Spaens <rhyme label="b" type="m" corresp="#s2:l2">ghebroet</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dan Groeningherlant spant de <rhyme label="c" type="f">croone</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Want sijt volbrachtent met eenen <rhyme label="b" type="m" corresp="#s2:l2">moet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Caspar de Robles <rhyme label="a" type="f">bloetghierich</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Haren Coronel seer <rhyme label="b" type="m">heet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die heeft doen graven <rhyme label="a" type="f" corresp="#s3:l1">onmanierich</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Eenen put, en een wip ghalghe <rhyme label="b" type="m" corresp="#s3:l2">wreet</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Heeft hy gestelt tot elcx <rhyme label="b" type="m" corresp="#s3:l2">leet</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Om daer met te moorden <rhyme label="c" type="f">vierich</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Want de basten waren oock al <rhyme label="b" type="m" corresp="#s3:l2">ghereet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Voorts heeft hy by hem <rhyme label="a" type="f">ontboden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Al sijn Capiteynen <rhyme label="b" type="m">cloeck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Op dat sy als valsche <rhyme label="a" type="f" corresp="#s4:l1">Joden</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Souden besweeren sijn <rhyme label="b" type="m" corresp="#s4:l2">versoeck</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Dwelc was den Borgher te brenghen om <rhyme label="b" type="m" corresp="#s4:l2">hoeck</rhyme>.</l>
        <l id="s4:l6" met="4" type="-+">Waer voor sy niet en sijn <rhyme label="c" type="f">ghevloden</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Dan Losy en wilde niet aen die <rhyme label="b" type="m" corresp="#s4:l2">vloeck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Doen hy nu wert <rhyme label="a" type="f">gheware</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Losijs mannelijcke <rhyme label="b" type="m">daet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Soo heeft hy terstont <rhyme label="a" type="f" corresp="#s5:l1">openbare</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Ontboden al vander <rhyme label="b" type="m" corresp="#s5:l2">straet</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Losijs Corporael ende sijnen <rhyme label="b" type="m" corresp="#s5:l2">maet</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Om van haer <rhyme label="c" type="f">alleghare</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Te vernemen haren <rhyme label="b" type="m" corresp="#s5:l2">raet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Waerop eenen Sergeant vanden <rhyme label="a" type="f">Baniere</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Antwoorde seer <rhyme label="b" type="m">cloeckelijck</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Smackende die pen int <rhyme label="a" type="f" corresp="#s6:l1">vyere</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Ick sy veel liever eer morghen een <rhyme label="b" type="m" corresp="#s6:l2">lijck</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Dus had den Coronel gheenen <rhyme label="b" type="m" corresp="#s6:l2">wijck</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Dan seyde, ick sal u <rhyme label="c" type="f">schiere</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Doen vallen metten palmen int <rhyme label="b" type="m" corresp="#s6:l2">slijck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daer na sijn sy <rhyme label="a" type="f">gheghangen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">By haer Soldaten aen elcken <rhyme label="b" type="m">oort</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Die alle sonder <rhyme label="a" type="f" corresp="#s7:l1">verlanghen</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Beloefden te wederstaen de <rhyme label="b" type="m" corresp="#s7:l2">moort</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Dit heeft den Coronel seer haest <rhyme label="b" type="m" corresp="#s7:l2">verhoort</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Dies hy met ghroot <rhyme label="c" type="f">verstranghen</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Wert op haerlieden seer <rhyme label="b" type="m" corresp="#s7:l2">ghestoort</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hy heeft den Loupes last <rhyme label="a" type="f">ghegheven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Te vollen veel gheschuts <rhyme label="b" type="m">ghemeen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Met haghel om te doen <rhyme label="a" type="f" corresp="#s8:l1">sneven</rhyme></l>
        <l id="s8:l4" met="4" type="-+">De Soldaten ghroot en <rhyme label="b" type="m" corresp="#s8:l2">cleen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Die daer naer de wacht trocken <rhyme label="b" type="m" corresp="#s8:l2">reen</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Dan zijn opset is <rhyme label="c" type="f">achterghebleven</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Want hy quam selfs int <rhyme label="b" type="m" corresp="#s8:l2">gheween</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Als de Soldaten dit <rhyme label="a" type="f">verstonden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Soo swoeren sy <rhyme label="b" type="m">terstont</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Dat soo sy den Coronel <rhyme label="a" type="f" corresp="#s9:l1">vonden</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Hem souden vanghen vaet mijn <rhyme label="b" type="m" corresp="#s9:l2">oorcont</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Dies trocken sy met sulcken <rhyme label="b" type="m" corresp="#s9:l2">gront</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Tot voor sijn huys daer sy naert <rhyme label="c" type="f">vermonden</rhyme></l>
        <l id="s9:l7" met="4" type="-+">Hem niet en eerden nae doude <rhyme label="b" type="m" corresp="#s9:l2">verbont</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dit heeft den Loupes seer <rhyme label="a" type="f">verdroten</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Dies hijse dreychden <rhyme label="b" type="m">stijf</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Dan sy hebben soo seer naer hem <rhyme label="a" type="f" corresp="#s10:l1">gheschoten</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Dat hy qualijck ontdroech sijn eyghen <rhyme label="b" type="m" corresp="#s10:l2">lijf</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Jae sijn Soldaten noyt schoonder <rhyme label="b" type="m" corresp="#s10:l2">bedrijf</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Hebben met de ander <rhyme label="c" type="f">ghesloten</rhyme></l>
        <l id="s10:l7" met="4" type="-+">Te leven en sterven als man en <rhyme label="b" type="m" corresp="#s10:l2">wijf</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Doen sijn sy met eenen <rhyme label="a" type="f">ghedruyse</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Gheloopen naer den <rhyme label="b" type="m">Coronel</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Dien sy eysten uuten <rhyme label="a" type="f" corresp="#s11:l1">huyse</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Noyt Tyran was in meerder <rhyme label="b" type="m" corresp="#s11:l2">ghequel</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Sy namen hem met den ghoeden <rhyme label="b" type="m" corresp="#s11:l2">ghesel</rhyme></l>
        <l id="s11:l6" met="4" type="-+">En setten hem <rhyme label="c" type="f">confuyse</rhyme></l>
        <l id="s11:l7" met="4" type="-+">Opt raethuys verstaet dit <rhyme label="b" type="m" corresp="#s11:l2">wel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Daer naer sijnder noch <rhyme label="a" type="f">ghegrepen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Vier Capiteynen <rhyme label="b" type="m">bloot</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Met meer ander boeven <rhyme label="a" type="f" corresp="#s12:l1">doornepen</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Die wel eer waren haer <rhyme label="b" type="m" corresp="#s12:l2">overhoot</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Die setten sy ghevanghen in grooten <rhyme label="b" type="m" corresp="#s12:l2">noot</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Al warense vol loosheyts <rhyme label="c" type="f">gheslepen</rhyme></l>
        <l id="s12:l7" met="4" type="-+">Sy en conden niet wederstaen den <rhyme label="b" type="m" corresp="#s12:l2">stoot</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Seigneur Fascus <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Coronel van Sutphen <rhyme label="b" type="m">was</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Die heeft hem terstont <rhyme label="a" type="f" corresp="#s13:l1">begheven</rhyme></l>
        <l id="s13:l4" met="4" type="-+">In een Monicken Clooster <rhyme label="b" type="m" corresp="#s13:l2">ras</rhyme></l>
        <l id="s13:l5" met="4" type="-+">Al waer hy sonder langher <rhyme label="b" type="m" corresp="#s13:l2">gheplas</rhyme></l>
        <l id="s13:l6" met="4" type="-+">Sijnen Baert en Hayr heeft doen <rhyme label="c" type="f">sneven</rhyme></l>
        <l id="s13:l7" met="4" type="-+">Om tontcomen dit vreempt <rhyme label="b" type="m" corresp="#s13:l2">ghebras</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">De Soldaten als Leeuwen <rhyme label="a" type="f">woedich</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Sochten aen elcken <rhyme label="b" type="m">cant</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Om te vinden desen Tyran <rhyme label="a" type="f" corresp="#s14:l1">bloedich</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Dan wat sy deden den lieven <rhyme label="b" type="m" corresp="#s14:l2">sant</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Die was inden Monicken <rhyme label="b" type="m" corresp="#s14:l2">pant</rhyme></l>
        <l id="s14:l6" met="4" type="-+">En socht als ander Monicken <rhyme label="c" type="f">moedich</rhyme></l>
        <l id="s14:l7" met="4" type="-+">Nae hem selfs, met een keersse inde <rhyme label="b" type="m" corresp="#s14:l2">hant</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Den eenen Monick cloeck van <rhyme label="a" type="f">aerde</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Verdroot dit groot <rhyme label="b" type="m">gherucht</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Dies hy den Spaengiaert niet en <rhyme label="a" type="f" corresp="#s15:l1">spaerde</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Mer seyde <rhyme label="b" type="m" corresp="#s15:l2">onbeducht</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Dit is eenen Monick sonder <rhyme label="b" type="m" corresp="#s15:l2">vrucht</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Van onser Oorden als de <rhyme label="c" type="f">vermaerde</rhyme></l>
        <l id="s15:l7" met="4" type="-+">Mer niet van dit convents <rhyme label="b" type="m" corresp="#s15:l2">ghehucht</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Den Monick me <rhyme label="a" type="f">consydereerde</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Datse niet en verstonden zijn <rhyme label="b" type="m">liet</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Dies hy hem weder <rhyme label="a" type="f" corresp="#s16:l1">accuseerde</rhyme></l>
        <l id="s16:l4" met="4" type="-+">En seyde met een soet <rhyme label="b" type="m" corresp="#s16:l2">bediet</rhyme></l>
        <l id="s16:l5" met="4" type="-+">Daer is den Man dus hout hem <rhyme label="b" type="m" corresp="#s16:l2">siet</rhyme></l>
        <l id="s16:l6" met="4" type="-+">Waer door sy hem <rhyme label="c" type="f">apprehendeerde</rhyme></l>
        <l id="s16:l7" met="4" type="-+">Met de Cap die hem bracht in ghroot <rhyme label="b" type="m" corresp="#s16:l2">verdriet</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Corts daer na <rhyme label="a" type="f">onbesweecken</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Vinghen sy noch met groot <rhyme label="b" type="m">Jolijt</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Den Provoost die hem hadt <rhyme label="a" type="f" corresp="#s17:l1">ghesteecken</rhyme></l>
        <l id="s17:l4" met="4" type="-+">In des Pastoors Schoorsteen <rhyme label="b" type="m" corresp="#s17:l2">wijt</rhyme></l>
        <l id="s17:l5" met="4" type="-+">Loupes wert met ghebracht int <rhyme label="b" type="m" corresp="#s17:l2">crijt</rhyme></l>
        <l id="s17:l6" met="4" type="-+">Dien sy soot heeft <rhyme label="c" type="f">ghebleecken</rhyme></l>
        <l id="s17:l7" met="4" type="-+">Noemden den Beul van Groeninghen <rhyme label="b" type="m" corresp="#s17:l2">subijt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Den Wachtmeester socht oock <rhyme label="a" type="f">listen</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Om haer handen te <rhyme label="b" type="m">ontghaen</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Dies hy vol <rhyme label="a" type="f" corresp="#s18:l1">arghelisten</rhyme></l>
        <l id="s18:l4" met="4" type="-+">Een Papenrock heeft <rhyme label="b" type="m" corresp="#s18:l2">anghedaen</rhyme>:</l>
        <l id="s18:l5" met="4" type="-+">Dan sy creghen ehm al mede <rhyme label="b" type="m" corresp="#s18:l2">ghevaen</rhyme>,</l>
        <l id="s18:l6" met="4" type="-+">Ten baten kijven oft <rhyme label="c" type="f">twisten</rhyme></l>
        <l id="s18:l7" met="4" type="-+">Inde diefput most hy springhen <rhyme label="b" type="m" corresp="#s18:l2">saen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Die vanden Dam en Delfsiele <rhyme label="a" type="f">cierich</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Brochten mede als Lantsers <rhyme label="b" type="m">ghoet</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Ghevanghen haer Capiteynen <rhyme label="a" type="f" corresp="#s19:l1">vierich</rhyme></l>
        <l id="s19:l4" met="4" type="-+">Noyt Soldaten waeren bet <rhyme label="b" type="m" corresp="#s19:l2">ghemoet</rhyme>:</l>
        <l id="s19:l5" met="4" type="-+">Sy swoeren tsamen met lijf en <rhyme label="b" type="m" corresp="#s19:l2">ghoet</rhyme></l>
        <l id="s19:l6" met="4" type="-+">Malcanderen by te staen <rhyme label="c" type="f">manierich</rhyme></l>
        <l id="s19:l7" met="4" type="-+">Om te verdrijven de Spaengiaerts <rhyme label="b" type="m" corresp="#s19:l2">verwoet</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Voort hebben sy <rhyme label="a" type="f">ontbonden</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Den Ambassadeur <rhyme label="b" type="m">minioot</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Die vande Staten was <rhyme label="a" type="f" corresp="#s20:l1">ghesonden</rhyme></l>
        <l id="s20:l4" met="4" type="-+">En vanden Coronel ghepijnt ter <rhyme label="b" type="m" corresp="#s20:l2">doot</rhyme>:</l>
        <l id="s20:l5" met="4" type="-+">Die sy terstont met herten <rhyme label="b" type="m" corresp="#s20:l2">bloot</rhyme></l>
        <l id="s20:l6" met="4" type="-+">Eedt deden na sijn <rhyme label="c" type="f">vermonden</rhyme></l>
        <l id="s20:l7" met="4" type="-+">Om dlant te helpen uut allen <rhyme label="b" type="m" corresp="#s20:l2">noot</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Hier na is hy sonder <rhyme label="a" type="f">laeten</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Ghereden na Bruyssel <rhyme label="b" type="m">vaillant</rhyme></l>
        <l id="s21:l3" met="3" type="-+">Om daer te vertellen die <rhyme label="a" type="f" corresp="#s21:l1">Staten</rhyme></l>
        <l id="s21:l4" met="4" type="-+">Hoe hy was ghevaren in <rhyme label="b" type="m" corresp="#s21:l2">Groeningherlant</rhyme>.</l>
        <l id="s21:l5" met="4" type="-+">Dus danckt nu Godt aennote elcken <rhyme label="b" type="m" corresp="#s21:l2">cant</rhyme></l>
        <l id="s21:l6" met="4" type="-+">Dat hy ons dus compt ter <rhyme label="c" type="f">baeten</rhyme></l>
        <l id="s21:l7" met="4" type="-+">Doen wy by na waeren <rhyme label="b" type="m" corresp="#s21:l2">vermant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
