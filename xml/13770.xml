    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
    </tags>
    <melody>Laura zat laatst bij de beek</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>anoniem</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ONlanghs op een morghen <rhyme label="a" type="m">stont</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Coridon hem gingh <rhyme label="b" type="f">vermeyen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Waer hy een schoon Nimphje <rhyme label="a" type="m" corresp="#s1:l1">vont</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die hy soetjens gingh <rhyme label="b" type="f" corresp="#s1:l2">geleyen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Langhs de groene <rhyme label="b" type="f" corresp="#s1:l2">klaver-weyen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Door ‘t gheboomte in den <rhyme label="c" type="m">Hof</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Waerde soete Venus <rhyme label="d" type="f">dieren</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Inden Tempel Venus <rhyme label="d" type="f" corresp="#s1:l7">vieren</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Tot der schoone Maeghde <rhyme label="c" type="m" corresp="#s1:l6">lof</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Haer amoureus wesen <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En lonckjens die daer <rhyme label="b" type="f">stralen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Wt haer hooft als ebbe <rhyme label="a" type="m" corresp="#s2:l1">vloet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Liet sy op hem neder <rhyme label="b" type="f" corresp="#s2:l2">dalen</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Waer deur hy strakx gingh <rhyme label="b" type="f" corresp="#s2:l2">afmalen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Al de schoonheydt vande <rhyme label="c" type="m">Maeght</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Die sijn sinne gants <rhyme label="d" type="f">vervoerde</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">En sijn leden heel <rhyme label="d" type="f" corresp="#s2:l7">ontroerde</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Door de lust der <rhyme label="c" type="m" corresp="#s2:l6">Minne-graeght</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Met verscheen daer Bacches <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Die de vreucht int’t hart gingh <rhyme label="b" type="f">booren</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En ‘t ghespel ‘t musijck <rhyme label="a" type="m" corresp="#s3:l1">accoort</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Klonck heel soet in yders <rhyme label="b" type="f" corresp="#s3:l2">ooren</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Venus liet haer stem mee <rhyme label="b" type="f" corresp="#s3:l2">hooren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Al ter eeren van dit <rhyme label="c" type="m">Paer</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Die door liefde t’saem <rhyme label="d" type="f">verhoolen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En door minne lusjens <rhyme label="d" type="f" corresp="#s3:l7">doolen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Offeren voor ‘t <rhyme label="c" type="m" corresp="#s3:l6">Min-altaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Doen sprack Venus nu wel <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Siet de koets met kransjens <rhyme label="b" type="f">bloemen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Coridon sey nu <rhyme label="a" type="m" corresp="#s4:l1">voortaen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Wil ick van liefs offer <rhyme label="b" type="f" corresp="#s4:l2">roemen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Is op de ledekant <rhyme label="b" type="f" corresp="#s4:l2">gheklommen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Met sijn Nimphjen aen sijn <rhyme label="c" type="m">zy</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">Waer sy met veel soete <rhyme label="d" type="f">lusjens</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En met duysent minne <rhyme label="d" type="f" corresp="#s4:l7">kusjens</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Boeten bey haer minne <rhyme label="c" type="m" corresp="#s4:l6">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
