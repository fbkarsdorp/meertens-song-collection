  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Den dienst Hebe seer selden <rhyme label="a" type="m">saeyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Int hert vreed'lycke <rhyme label="b" type="f">rusten</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Want Venus vier hitten deur <rhyme label="a" type="m" corresp="#s1:l1">laeyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Vntsteeckt therte in <rhyme label="b" type="f" corresp="#s1:l2">lusten</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Quellich vol pyn noeyt arbeyt <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Als die Dedalus paeden <rhyme label="d" type="m">saen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Belaeden <rhyme label="d" type="m" corresp="#s1:l6">gaen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">dit werd ick wel <rhyme label="c" type="m" corresp="#s1:l5">gewaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">My haer dienaer heeft oock <rhyme label="a" type="m">deurwont</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Cupido int hert <rhyme label="b" type="f">vierich</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Want myn lieffs roo lachende <rhyme label="a" type="m" corresp="#s2:l1">mont</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Haer blonghedich haeyrken <rhyme label="b" type="f" corresp="#s2:l2">chierich</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Haer borstjens ront haer soet <rhyme label="c" type="m">aenschyn</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Haer armkens widt handekens <rhyme label="d" type="m">cleen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Tandekens <rhyme label="d" type="m" corresp="#s2:l6">reen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ock haer wangxkens <rhyme label="c" type="m" corresp="#s2:l5">sangwyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer keeltjen widt Lipkens <rhyme label="a" type="m">corael</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gedraeyden hals ront <rhyme label="b" type="f">schoone</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Bruyn ooghxkens claer als scheby <rhyme label="a" type="m" corresp="#s3:l1">strael</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Iae twesen haers <rhyme label="b" type="f" corresp="#s3:l2">perssoone</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Beleeft min stem in my baert <rhyme label="c" type="m">snel</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Een hert om dees goddinne <rhyme label="d" type="m">haer</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Te minnen <rhyme label="d" type="m" corresp="#s3:l6">naer</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Cupido straels <rhyme label="c" type="m" corresp="#s3:l5">bevel</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Iphemena Pero <rhyme label="a" type="m">Leda</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Procris met haeren <rhyme label="b" type="f">schichten</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Printhina Porcia <rhyme label="a" type="m" corresp="#s4:l1">Phedra</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Haer minlyck soet <rhyme label="b" type="f" corresp="#s4:l2">gesichte</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Heeft haer minnaers noeyt <rhyme label="c" type="m">voorsaecht</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Tlyden swaer dwelck my draegen <rhyme label="d" type="m">doet</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Ick claegen <rhyme label="d" type="m" corresp="#s4:l6">moet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Sy die myn thert <rhyme label="c" type="m" corresp="#s4:l5">ontschaekt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Princesselyck schoon maechdelyck <rhyme label="a" type="m">saet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Olimpisch maecht <rhyme label="b" type="f">teere</rhyme></l>
        <l id="s5:l3" met="4" type="-+">V dienaer trouw troostich <rhyme label="a" type="m" corresp="#s5:l1">beraet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Volght Appol Pithium <rhyme label="b" type="f" corresp="#s5:l2">leere</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Eert Hymene gebruyckt u <rhyme label="c" type="m">jeught</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Verkeert Venery stryden <rhyme label="d" type="m">bang</rhyme></l>
        <l id="s5:l7" met="2" type="-+">En Lyden <rhyme label="d" type="m" corresp="#s5:l6">strang</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Constant in reyn <rhyme label="c" type="m" corresp="#s5:l5">gheneucht</rhyme></l>
      </lg>
    </lg>
  </text>
