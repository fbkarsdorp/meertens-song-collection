  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Een meisien leyden een knechie te <rhyme label="a" type="f">vooren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">te trouwen nae de bruelofs <rhyme label="b" type="m">aert</rhyme></l>
        <l id="s1:l3" met="4" type="-+">en hy en wilde daer niet nae <rhyme label="a" type="f" corresp="#s1:l1">hoeren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">maer hy diendese al achter <rhyme label="b" type="m" corresp="#s1:l2">waert</rhyme></l>
        <l id="s1:l5" met="3" type="-+">ey eJ hy en derft niet <rhyme label="c" type="f">koemen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">ey eJ hy Is <rhyme label="b" type="m" corresp="#s1:l2">voervaert</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ten ende soe was hy te <rhyme label="a" type="f">vreede</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Hy sou trouwen <rhyme label="b" type="m">onbeeswaert</rhyme></l>
        <l id="s2:l3" met="4" type="-+">en doen beefde hem al sin <rhyme label="a" type="f" corresp="#s2:l1">leeden</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Hoe heb Ick myn aldus <rhyme label="b" type="m" corresp="#s2:l2">beswaert</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ja Ja. hy en derft nie <rhyme label="c" type="f">koemen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ja Ja hy Is <rhyme label="b" type="m" corresp="#s2:l2">voeruaert</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Alamen de brueloft sou gaen <rhyme label="a" type="f">houwen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">en de vrienden waeren <rhyme label="b" type="m">voergaert</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Het meisie sprack gy moet myn <rhyme label="a" type="f" corresp="#s3:l1">trouwen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">en doen kraude hy syn <rhyme label="b" type="m" corresp="#s3:l2">baert</rhyme></l>
        <l id="s3:l5" met="3" type="-+">ey eJ hy en derft niet <rhyme label="c" type="f">koemen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">ey eJ hy Is <rhyme label="b" type="m" corresp="#s3:l2">voervaert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die bruit die Is te bedde <rhyme label="a" type="f">gegaen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">en sy sprack lief komt <rhyme label="b" type="m">herrewaerts</rhyme></l>
        <l id="s4:l3" met="4" type="-+">doen ginck hy In een houck <rhyme label="a" type="f" corresp="#s4:l1">staen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">en henne toende synen <rhyme label="b" type="m" corresp="#s4:l2">aert</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ja Ja. hy en derft nie <rhyme label="c" type="f">koemen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ja Ja hy Is <rhyme label="b" type="m" corresp="#s4:l2">voeruaert</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Henne soude bedde <rhyme label="a" type="f">gaen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">hy sey vrou bruit licht <rhyme label="b" type="m">achterwaert</rhyme></l>
        <l id="s5:l3" met="4" type="-+">doen werden syn oechkens al <rhyme label="a" type="f" corresp="#s5:l1">traenen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Zyn herteken was soe seer beswaer <rhyme label="b" type="m" corresp="#s5:l2">(t</rhyme>)</l>
        <l id="s5:l5" met="3" type="-+">ey eJ hy en derft niet <rhyme label="c" type="f">koemen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">ey eJ hy Is <rhyme label="b" type="m" corresp="#s5:l2">voervaert</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Prinses hy en wist niet hoe hy sou <rhyme label="a" type="f">lechgen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">omdat hy sou werden <rhyme label="b" type="m">gespaert</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Ick bidde u wiltet de kinderen <rhyme label="a" type="f" corresp="#s6:l1">sechgen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">dat syder meede gaen om <rhyme label="b" type="m" corresp="#s6:l2">mostaer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ja Ja. hy en derft nie <rhyme label="c" type="f">koemen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Ja Ja hy Is <rhyme label="b" type="m" corresp="#s6:l2">voeruaert</rhyme></l>
      </lg>
    </lg>
  </text>
