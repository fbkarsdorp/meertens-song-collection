      <tag>love song</tag>
    </tags>
    <melody>Engelse fortuin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Lieffde int <rhyme label="a" type="m">secreet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Myn jonck hert krencken <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Sonder s'lieffs <rhyme label="a" type="m" corresp="#s1:l1">weet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ick veel oock dencken <rhyme label="b" type="m" corresp="#s1:l2">moet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Hoe ick my <rhyme label="c" type="m">schicke</rhyme></l>
        <l id="s1:l6" met="3" type="-+">My compt te vooren <rhyme label="d" type="m">staech</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Lieffde die <rhyme label="c" type="m" corresp="#s1:l5">ick</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Tot d'uytvercooren <rhyme label="d" type="m" corresp="#s1:l6">draech</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Haer oochskens <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">My somptyts schencken <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Een seer <rhyme label="a" type="m" corresp="#s2:l1">eerbaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Loncxkens en wencxkens <rhyme label="b" type="m" corresp="#s2:l2">zoet</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Dwelck myn <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Int alderdiepste <rhyme label="d" type="m">prickt</rhyme></l>
        <l id="s2:l7" met="2" type="-+">E]n met <rhyme label="c" type="m" corresp="#s2:l5">smart</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ontroert en <rhyme label="d" type="m" corresp="#s2:l6">verschrickt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Een swaer <rhyme label="a" type="m">ghesuchte</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vuyt t'shertsen diepen <rhyme label="b" type="m">grondt</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Volcht seer <rhyme label="a" type="m" corresp="#s3:l1">beducht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer naer treurich <rhyme label="b" type="m" corresp="#s3:l2">terstondt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Alsmen v <rhyme label="c" type="m">naem</rhyme></l>
        <l id="s3:l6" met="3" type="-+">In myn presentie <rhyme label="d" type="m">noempt</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Loopt t'bloet <rhyme label="c" type="m" corresp="#s3:l5">tesamen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Een andere veruwe <rhyme label="d" type="m" corresp="#s3:l6">compt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Nyemandt en <rhyme label="a" type="m">weet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Nochtans wat my <rhyme label="b" type="m">ledt</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Want int <rhyme label="a" type="m" corresp="#s4:l1">secreet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Is de lieffde <rhyme label="b" type="m" corresp="#s4:l2">gesedt</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Ick hebbe mynen <rhyme label="c" type="m">noot</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Noch nyet <rhyme label="d" type="m">geclaechte</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Den medecyn <rhyme label="c" type="m" corresp="#s4:l5">meester</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Noch om geen hulp <rhyme label="d" type="m" corresp="#s4:l6">gevraecht</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Maer ick <rhyme label="a" type="m">bemin</rhyme></l>
        <l id="s5:l2" met="3" type="-+">V bouen goudt <rhyme label="b" type="m">alleen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Dan lachen <rhyme label="a" type="m" corresp="#s5:l1">och</rhyme></l>
        <l id="s5:l4" met="3" type="-+">V hert is nyet wel te <rhyme label="b" type="m" corresp="#s5:l2">vreen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Dat het nyet en <rhyme label="c" type="m">mach</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Door den mondt spreken <rhyme label="d" type="m">luydt</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Syn swaer <rhyme label="c" type="m" corresp="#s5:l5">geclach</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En gebreken <rhyme label="d" type="m" corresp="#s5:l6">vuyt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Dies neem ick <rhyme label="a" type="m">waer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Bequamer vre en <rhyme label="b" type="m">stonde</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Om eens te <rhyme label="a" type="m" corresp="#s6:l1">gaer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Te spreken mondt aen <rhyme label="b" type="m" corresp="#s6:l2">mondt</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Als och en <rhyme label="c" type="m">ooch</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Malcanderen commen <rhyme label="d" type="m">by</rhyme></l>
        <l id="s6:l7" met="2" type="-+">En thertte <rhyme label="c" type="m" corresp="#s6:l5">poocht</rhyme></l>
        <l id="s6:l8" met="3" type="-+">D]atter oock naerder <rhyme label="d" type="m" corresp="#s6:l6">zy</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Compt duyffkens <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Die Venus coetse <rhyme label="b" type="m">treckt</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Nempt dit <rhyme label="a" type="m" corresp="#s7:l1">pampier</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En brenght thet wel <rhyme label="b" type="m" corresp="#s7:l2">pertfeckt</rhyme></l>
        <l id="s7:l5" met="2" type="-+">De jonckvrouwe so <rhyme label="c" type="m">ane</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Die zoo veel deuchden <rhyme label="d" type="m">heeft</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Als een <rhyme label="c" type="m" corresp="#s7:l5">persoon</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Die op der eerden <rhyme label="d" type="m" corresp="#s7:l6">leeft</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Want zy is <rhyme label="a" type="m">myn</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Matresse en ick wil <rhyme label="b" type="m">haer</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Eeuwelyck <rhyme label="a" type="m" corresp="#s8:l1">zyn</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Een oprecht getrouw <rhyme label="b" type="m" corresp="#s8:l2">dienaer</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Naer mynen <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Is zy tschoonste <rhyme label="d" type="m">juweel</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Dat ick <rhyme label="c" type="m" corresp="#s8:l5">benin</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Onder duysend <rhyme label="d" type="m" corresp="#s8:l6">veel</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
