<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>117792</id>
    <date>
      <year-start>1741</year-start>
      <year-end>1741</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Zegt myn eens gy <rhyme label="a" type="f">Jonge-Dogter</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hebt g' ook niet een <rhyme label="b" type="m">Servituur</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Jo Myn Heer voorwoor ik <rhyme label="a" type="f" corresp="#s1:l1">mogter</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wel eyns komen om an de <rhyme label="b" type="m" corresp="#s1:l2">Duur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zovonts wil ik noor hem <rhyme label="a" type="f">wegten</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Om te hebben, het genayt van zyn <rhyme label="b" type="m">vezayt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wel woorom nayt, ter wylewe <rhyme label="a" type="f" corresp="#s2:l1">tregten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Om te zyn uyt ons <rhyme label="b" type="m" corresp="#s2:l2">verdrayt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ik wil myn Froygies op goon <rhyme label="a" type="f">Paleren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Krille moken al van myn <rhyme label="b" type="m">Hair</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dan zal hy myn wel <rhyme label="a" type="f" corresp="#s3:l1">zoluweren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Komen met yen Prootje <rhyme label="b" type="m" corresp="#s3:l2">aan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ik wil nog aan doen main <rhyme label="a" type="f">Pendanten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">I met myn Strikjes <rhyme label="b" type="m">gemonteert</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En main Hooft ain alle <rhyme label="a" type="f" corresp="#s4:l1">kanten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Froy, met Pluymgjes <rhyme label="b" type="m" corresp="#s4:l2">gemonteert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ik heb Kornetjes nog te <rhyme label="a" type="f">moken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Met een Poorle Moye <rhyme label="b" type="m">Snoer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ik zal wel in zyn Grozie <rhyme label="a" type="f" corresp="#s5:l1">roken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Met myn alle Mode <rhyme label="b" type="m" corresp="#s5:l2">Tour</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Gruwe Schoentjes, Witte Kousjes, en een Witte Vorschoot <rhyme label="a" type="f">oon</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Zo kom ik <rhyme label="b" type="m">Stoon</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Zo wil ik hem Tovont <rhyme label="a" type="f" corresp="#s6:l1">wegten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Tegen als hy komt <rhyme label="b" type="m" corresp="#s6:l2">gegoon</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Een Gestreept Kleet wil ik <rhyme label="a" type="f">drogen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">'t Is de Moden zo men zeyt en worom <rhyme label="b" type="m">nayt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Om zo een Vryer te <rhyme label="a" type="f" corresp="#s7:l1">behogen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Want het ister wer meer <rhyme label="b" type="m" corresp="#s7:l2">geschiet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dan zal hy myn <rhyme label="a" type="f">Kurietzeren</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Noemen myn zyn Schoon Godin en <rhyme label="b" type="m">Engelin</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Wel worom nayt, ik horet zo <rhyme label="a" type="f" corresp="#s8:l1">geren</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En dan goot het al noor myn <rhyme label="b" type="m" corresp="#s8:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ziet door komt hy al <rhyme label="a" type="f">oongetreden</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Hy is Netjes gelyk een <rhyme label="b" type="m">Pouw</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Ag hoe beroert stoon hem die <rhyme label="a" type="f" corresp="#s9:l1">Leden</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Ag hay worder von Liefde <rhyme label="b" type="m" corresp="#s9:l2">Flouw</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ik wil myn in Postuur gaan <rhyme label="a" type="f">houden</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">En myn Montje in Fiset, moet zyn <rhyme label="b" type="m">gezet</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Wel worom nayt, hy zal myn <rhyme label="a" type="f" corresp="#s10:l1">Trouwe</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Als 't Papagie moor nayt <rhyme label="b" type="m" corresp="#s10:l2">Belet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Servituur <rhyme label="a" type="f">Madamoselle</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Ag myn over Schoon <rhyme label="b" type="m">Godin</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Dat is 't eerst dat wy dan <rhyme label="a" type="f" corresp="#s11:l1">zelle</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">En dan Goot het al noor myn <rhyme label="b" type="m" corresp="#s11:l2">Zin</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ik wil het myn Mamaagie <rhyme label="a" type="f">vrogen</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Of zy myn nayt ene Man, wil geven <rhyme label="b" type="m">dan</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Wel worom nayt, ik moet het <rhyme label="a" type="f" corresp="#s12:l1">klogen</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Als men nayt langer Wagten <rhyme label="b" type="m" corresp="#s12:l2">kan</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Al ben ik nog Jonk van <rhyme label="a" type="f">Joren</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Nog moor Zestien zo men <rhyme label="b" type="m">ziet</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Zouw yk dan nayt mogen <rhyme label="a" type="f" corresp="#s13:l1">Poren</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Dat woor myn wel eyn groot <rhyme label="b" type="m" corresp="#s13:l2">Verdriet</rhyme>:</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Ik wil nog eens Papogie <rhyme label="a" type="f">vrogen</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Of die myn nayt ene Man, wil geven <rhyme label="b" type="m">dan</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Wel worom nayt, yk wil het <rhyme label="a" type="f" corresp="#s14:l1">klogen</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Als yk nayt langer Wagten <rhyme label="b" type="m" corresp="#s14:l2">kan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
