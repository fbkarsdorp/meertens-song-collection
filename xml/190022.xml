  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TE wenden of te <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En mach nu niet <rhyme label="b" type="m">gheschien</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Mijn overst, moet ick <rhyme label="a" type="f" corresp="#s1:l1">eeren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die my hier in <rhyme label="b" type="m" corresp="#s1:l2">ghebien</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Pallas d’eedel <rhyme label="c" type="f">Godinne</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En Mars mijn <rhyme label="d" type="m">Governeur</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Met Venus soet van <rhyme label="c" type="f" corresp="#s1:l5">sinnen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die gheven mij gheen <rhyme label="d" type="m" corresp="#s1:l6">ceur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe wel ick van u <rhyme label="a" type="f">zeyle</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En schuw u claer <rhyme label="b" type="m">aenschijn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Denckt niet dat mijn hert <rhyme label="a" type="f" corresp="#s2:l1">feyle</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Bedeckt is met <rhyme label="b" type="m" corresp="#s2:l2">fenijn</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Ick soud’ u zijn <rhyme label="c" type="f">ghetrouwe</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Maer nu de woorden <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Ghy niet ghelooft <rhyme label="c" type="f" corresp="#s2:l5">Ionckvrouwe</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Nu salt een ander <rhyme label="d" type="m" corresp="#s2:l6">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
