  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">En wilt ons nyet <rhyme label="a" type="f">versmaeden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Heere wilt ons <rhyme label="b" type="m">aensien</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">Inden schoot uwer <rhyme label="a" type="f" corresp="#s1:l1">genaeden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wy altesaemen <rhyme label="b" type="m" corresp="#s1:l2">vlien</rhyme></l>
        <l id="s1:l5" met="3" type="-+">In desen droeven <rhyme label="c" type="m">tyt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Wilt ons helpen <rhyme label="d" type="f">stryden</rhyme></l>
        <l id="s1:l7" met="1" type="-+">En <rhyme label="d" type="f" corresp="#s1:l6">lyden</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Want ghy den trooster <rhyme label="c" type="m" corresp="#s1:l5">zyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy hebben desen quaeden <rhyme label="a" type="f">tyt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Allegaeder wel <rhyme label="b" type="m">verdient</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Gheestelyck ende <rhyme label="a" type="f" corresp="#s2:l1">werelyck</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wy zyn geweest <rhyme label="b" type="m" corresp="#s2:l2">verblint</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Door ons boos leven <rhyme label="c" type="m">quaet</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">Wy hebben zelve gaen <rhyme label="d" type="f">halen</rhyme></l>
        <l id="s2:l7" met="1" type="-+">Zonder <rhyme label="d" type="f" corresp="#s2:l6">falen</rhyme></l>
        <l id="s2:l8" met="4" type="+-">De Roede daer Godt <rhyme label="c" type="m" corresp="#s2:l5">mede</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vele hebben Godt <rhyme label="a" type="f">uutwendich</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Naerden schyn <rhyme label="b" type="m">gheeert</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Maer het herte <rhyme label="a" type="f" corresp="#s3:l1">inwendich</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat stont al meest <rhyme label="b" type="m" corresp="#s3:l2">gekeert</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Al tot dat aertsche <rhyme label="c" type="m">goet</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">Daerom wy oock nu <rhyme label="d" type="f">draegen</rhyme></l>
        <l id="s3:l7" met="1" type="-+">De <rhyme label="d" type="f" corresp="#s3:l6">plaegen</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Die Godt de Heere nu <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Waeren wy goede <rhyme label="a" type="f">kinderen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">het zoude wel <rhyme label="b" type="m">vergaen</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Godt zoude de plaegen <rhyme label="a" type="f" corresp="#s4:l1">minderen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Jae teenemael laeten <rhyme label="b" type="m" corresp="#s4:l2">staen</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Wilden wy laeten <rhyme label="c" type="m">zaen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Onse loose <rhyme label="d" type="f">treken</rhyme></l>
        <l id="s4:l7" met="1" type="-+">En <rhyme label="d" type="f" corresp="#s4:l6">gebreken</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Die wy al hebben <rhyme label="c" type="m" corresp="#s4:l5">misdaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wy zynt al wel <rhyme label="a" type="f">gedachtich</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Als ons Godt wat <rhyme label="b" type="m">neempt</rhyme> /</l>
        <l id="s5:l3" met="3" type="-+">Maer zyn gaven <rhyme label="a" type="f" corresp="#s5:l1">crachtich</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Waeren in ons hert <rhyme label="b" type="m" corresp="#s5:l2">vervreempt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Die hielen wy al voor <rhyme label="c" type="m">nyet</rhyme> /</l>
        <l id="s5:l6" met="3" type="-+">Wy zijn geweest meer <rhyme label="d" type="f">ghierich</rhyme></l>
        <l id="s5:l7" met="1" type="-+">Dan <rhyme label="d" type="f" corresp="#s5:l6">vierich</rhyme> /</l>
        <l id="s5:l8" met="4" type="+-">Daerom is ons leet <rhyme label="c" type="m" corresp="#s5:l5">geschiet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Liet yegelyck zijn <rhyme label="a" type="f">sonden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Het zoude al wel <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Godt zoude in corter <rhyme label="a" type="f" corresp="#s6:l1">stonden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ons verlossen uutter <rhyme label="b" type="m" corresp="#s6:l2">pyn</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Door zijn genaede <rhyme label="c" type="m">groot</rhyme> /</l>
        <l id="s6:l6" met="3" type="-+">Hy zoude zijn plaegen <rhyme label="d" type="f">keeren</rhyme></l>
        <l id="s6:l7" met="1" type="-+">En <rhyme label="d" type="f" corresp="#s6:l6">weeren</rhyme> /</l>
        <l id="s6:l8" met="4" type="+-">Ons al bystaen inder <rhyme label="c" type="m" corresp="#s6:l5">noot</rhyme> /</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy catholycke <rhyme label="a" type="f">persoonen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Nempt toch eenen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Godt die zal ons <rhyme label="a" type="f" corresp="#s7:l1">toonen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Zyn helpe met <rhyme label="b" type="m" corresp="#s7:l2">voorspoet</rhyme> /</l>
        <l id="s7:l5" met="3" type="-+">Alst hem believen <rhyme label="c" type="m">zal</rhyme> /</l>
        <l id="s7:l6" met="3" type="-+">Doet des Heeren <rhyme label="d" type="f">wille</rhyme></l>
        <l id="s7:l7" met="1" type="-+">Al <rhyme label="d" type="f" corresp="#s7:l6">stille</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Heymelyck zonder <rhyme label="c" type="m" corresp="#s7:l5">geschal</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En wilt de <rhyme label="a" type="f">Calvinisten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Nyet zoe zeer <rhyme label="b" type="m">ontzien</rhyme> /</l>
        <l id="s8:l3" met="3" type="-+">Haer tirannye vol <rhyme label="a" type="f" corresp="#s8:l1">twisten</rhyme></l>
        <l id="s8:l4" met="3" type="-+">En sal niet lange <rhyme label="b" type="m" corresp="#s8:l2">geschien</rhyme> /</l>
        <l id="s8:l5" met="3" type="-+">Godt die Heere die zieget <rhyme label="c" type="m">al</rhyme> /</l>
        <l id="s8:l6" met="3" type="-+">Zij zullen noch al gaen <rhyme label="d" type="f">loopen</rhyme></l>
        <l id="s8:l7" met="1" type="-+">Met <rhyme label="d" type="f" corresp="#s8:l6">hoopen</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Als den tyt eens keeren <rhyme label="c" type="m" corresp="#s8:l5">zal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Laet ons nemen in <rhyme label="a" type="f">dancke</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat Godt den Heere ons <rhyme label="b" type="m">doet</rhyme> /</l>
        <l id="s9:l3" met="3" type="-+">Al zyn wy menschen <rhyme label="a" type="f" corresp="#s9:l1">crancke</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Wy moeten grypen <rhyme label="b" type="m" corresp="#s9:l2">moet</rhyme> /</l>
        <l id="s9:l5" met="3" type="-+">En bekennen onse sonden <rhyme label="c" type="m">groot</rhyme> /</l>
        <l id="s9:l6" met="3" type="-+">Wilt altsaemen <rhyme label="d" type="f">beginnen</rhyme></l>
        <l id="s9:l7" met="1" type="-+">Met <rhyme label="d" type="f" corresp="#s9:l6">zinnen</rhyme></l>
        <l id="s9:l8" met="4" type="+-">Elck te beteren een <rhyme label="c" type="m" corresp="#s9:l5">hoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
