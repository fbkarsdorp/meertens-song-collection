  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HAd ick vloghelen als een arent <rhyme label="a" type="m">grijs</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick soude so hooghe <rhyme label="b" type="f">vlieghen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Daer bouen in dat schoon <rhyme label="a" type="m" corresp="#s1:l1">paradijs</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Tot minen soeten <rhyme label="b" type="f" corresp="#s1:l2">lieue</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dan soude ic segghen vader <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wanneer suldy mi <rhyme label="b" type="f">halen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Wt dese allende op <rhyme label="a" type="m" corresp="#s2:l1">aertrijck</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In uwer hoochster <rhyme label="b" type="f" corresp="#s2:l2">salen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waer is die dochter van <rhyme label="a" type="m">syon</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ick wilse vrolijck <rhyme label="b" type="f">maken</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Ic wil haer een boetscap <rhyme label="a" type="m" corresp="#s3:l1">doen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van also hooghen <rhyme label="b" type="f" corresp="#s3:l2">saken</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die hoochste inder <rhyme label="a" type="m">minnen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat is Iesus Marien <rhyme label="b" type="f">soon</rhyme></l>
        <l id="s4:l3" met="4" type="-+">God laet ons hem <rhyme label="a" type="m" corresp="#s4:l1">ghewinnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer bouen inden <rhyme label="b" type="f" corresp="#s4:l2">throon</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat hemelrijc ende <rhyme label="a" type="m">aertrijck</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Noyt man en conde <rhyme label="b" type="f">ghebruyken</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Dat sal een suyver maget <rhyme label="a" type="m" corresp="#s5:l1">reyn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In haren lichaem <rhyme label="b" type="f" corresp="#s5:l2">besluyten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die duysternisse is ons <rhyme label="a" type="m">verclaert</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat licht is op <rhyme label="b" type="f">gheresen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Een maghet heeft een kint <rhyme label="a" type="m" corresp="#s6:l1">ghebaert</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat mach wel wonder <rhyme label="b" type="f" corresp="#s6:l2">wesen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dat kint was teeder ende <rhyme label="a" type="m">cranc</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Een maghet moestet <rhyme label="b" type="f">voeden</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Tis recht dat wi hem weten <rhyme label="a" type="m" corresp="#s7:l1">danck</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Van sijnder groter <rhyme label="b" type="f" corresp="#s7:l2">armoeden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Heer Iesu christ wt hebt ghi <rhyme label="a" type="m">ghemeent</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Wij soudent v gaerne <rhyme label="b" type="f">vragen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Dat ghi hier dus ligt en <rhyme label="a" type="m" corresp="#s8:l1">weent</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ghi sijt so ionck van <rhyme label="b" type="f" corresp="#s8:l2">daghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat dus viel ende was <rhyme label="a" type="m">ongereet</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat moghen wi nu <rhyme label="b" type="f">ghebruyken</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Dat Adam inden appel <rhyme label="a" type="m" corresp="#s9:l1">beet</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat dede den hemel <rhyme label="b" type="f" corresp="#s9:l2">sluyten</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O moeder Gods van <rhyme label="a" type="m">hemelrijc</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Coninghinne in shemels <rhyme label="b" type="f">throne</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Bidt voer ons allen <rhyme label="a" type="m" corresp="#s10:l1">innichlijc</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Uwen alder liefsten <rhyme label="b" type="f" corresp="#s10:l2">sone</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dat wij daer bouen <rhyme label="a" type="m">moeten</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Daer die engelen vrolijc <rhyme label="b" type="f">singen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">V ende v lief kint <rhyme label="a" type="m" corresp="#s11:l1">groeten</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Dat wil ons God <rhyme label="b" type="f" corresp="#s11:l2">ghehinghen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
