        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mon mari est Ã  Paris</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Mynen Man is na de <rhyme label="a" type="m">Krijgh</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Voor de Heeren <rhyme label="b" type="f">Staten</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Met sijn Degen in de <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hy strijd daer voor sijn <rhyme label="c" type="m" corresp="#s1:l3">Vaderland</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Met vreugde, met vreugde, met <rhyme label="d" type="f">vreugde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hy heeft geschreven my een <rhyme label="a" type="m">Brief</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Dat hy my sal <rhyme label="b" type="f">senden</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Een schoone Westphaelse <rhyme label="c" type="m">Ham</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Als hy ergens die <rhyme label="c" type="m" corresp="#s2:l3">bequam</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Cabassen, cabassen, <rhyme label="d" type="f">cabassen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Wou dat hy sijn woord maer <rhyme label="a" type="m">hiel</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">'k Sou u daer op <rhyme label="b" type="f">nooden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Om te maken goede <rhyme label="c" type="m">cier</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En daer by drincken goed <rhyme label="c" type="m" corresp="#s3:l3">Bier</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Van 't beste, van 't beste, van 't <rhyme label="d" type="f">beste</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy stuurde my eenigh <rhyme label="a" type="m">Geld</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Dat hy had <rhyme label="b" type="f">gekregen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Van een Munsterse <rhyme label="c" type="m">Convoy</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Daer kan ick dees Winter <rhyme label="c" type="m" corresp="#s4:l3">moy</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Af smeeren, Af smeeren, Af <rhyme label="d" type="f">smeeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Wou dat hy dat dickwils <rhyme label="a" type="m">dee</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Soo kost ick weer af <rhyme label="b" type="f">lossen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Uyt den Lombart mijnen <rhyme label="c" type="m">Rock</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Die daer bleef als hy wegh <rhyme label="c" type="m" corresp="#s5:l3">trock</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Marcheren, Marcheren, <rhyme label="d" type="f">Marcheren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Grietje schrijft hy Grietje <rhyme label="a" type="m">Lief</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Wilt gerust maer <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Stelt om my doch geen <rhyme label="c" type="m">verdriet</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Ick sal u vergeten <rhyme label="c" type="m" corresp="#s6:l3">niet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Mijn leven, Mijn leven, Mijn <rhyme label="d" type="f">leven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ick wil voor mijn <rhyme label="a" type="m">Vaderland</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Vromelijck hier <rhyme label="b" type="f">vechten</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Vallen op den Vyand <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En daer dickwils halen <rhyme label="c" type="m" corresp="#s7:l3">buyt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Of slagen, Of slagen, Of <rhyme label="d" type="f">slagen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">So ick daer te blijven <rhyme label="a" type="m">kom</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Ghy sult licht weer <rhyme label="b" type="f">Trouwen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Maer ghy krijght noyt beter <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Wilt my niet vergeten <rhyme label="c" type="m" corresp="#s8:l3">dan</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Mijn hertje, Mijn hertje, Mijn <rhyme label="d" type="f">hertje</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Grietje dats een Man als <rhyme label="a" type="m">speck</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Mijnen lompen <rhyme label="b" type="f">Iorden</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Schreef my onlangs eenen <rhyme label="c" type="m">Brief</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Maer die was my niet heel <rhyme label="c" type="m" corresp="#s9:l3">lief</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Om hooren, Om hooren, Om <rhyme label="d" type="f">hooren</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Hy stuurde my niet een <rhyme label="a" type="m">beet</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Maer hy wou wat <rhyme label="b" type="f">hebben</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Hy schreef tot mijn groot <rhyme label="c" type="m">verdriet</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Dat hy daer kost leven <rhyme label="c" type="m" corresp="#s10:l3">niet</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Van gagie, Van gagie, Van <rhyme label="d" type="f">gagie</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Ick geloof die hontsvot <rhyme label="a" type="m">meent</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Dat ick dese <rhyme label="b" type="f">dingen</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Van mijn vel kan snyden <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">So ick hem dit alles <rhyme label="c" type="m" corresp="#s11:l3">gaf</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">'t Waer sotheyd, 't Waer sotheyd, 't Waer <rhyme label="d" type="f">sotheyd</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Dan mocht ick met Kinders <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Weder Honger <rhyme label="b" type="f">lyden</rhyme>:</l>
        <l id="s12:l3" met="4" type="+-">Neen ick sal niet zijn so <rhyme label="c" type="m">geck</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Dat ick voeden sal syn <rhyme label="c" type="m" corresp="#s12:l3">beck</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Met kasen, Met kasen, Met <rhyme label="d" type="f">kasen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Hy magh doen als uwen <rhyme label="a" type="m">Man</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Gaen oock wat moes <rhyme label="b" type="f">koppen</rhyme>:</l>
        <l id="s13:l3" met="4" type="+-">Soecken ergens vette <rhyme label="c" type="m">buyt</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Of met een bebloede <rhyme label="c" type="m" corresp="#s13:l3">huyt</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Weer komen, Weer komen, Weer <rhyme label="d" type="f">komen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Ick heb selfs genoegh te <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s14:l2" met="3" type="+-">Dat ick met myn <rhyme label="b" type="f">Kinders</rhyme></l>
        <l id="s14:l3" met="4" type="+-">Alle dagen word <rhyme label="c" type="m">gevoed</rhyme>:</l>
        <l id="s14:l4" met="4" type="+-">Dat ick mager eten <rhyme label="c" type="m" corresp="#s14:l3">moet</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Pottagie, Pottagie, <rhyme label="d" type="f">Pottagie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
