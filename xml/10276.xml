    <melody>Amarilli mia bella</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Rosamonde mijn <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Die minne-pijltjes voert in uwe <rhyme label="b" type="f">ooghjes</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Als zijnde <rhyme label="b" type="f" corresp="#s1:l2">minne-booghjes</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">V aesems geur, als lieffelijcke <rhyme label="a" type="f" corresp="#s1:l1">winde</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Doet my verkoelingh <rhyme label="a" type="f" corresp="#s1:l1">vinde</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Als mijn de minne, door 't raken van u <rhyme label="c" type="f">handen</rhyme>,</l>
        <l id="s1:l7" met="7" type="-+">Rosamondi, Rosamondi, Rosamondi, doet <rhyme label="c" type="f" corresp="#s1:l6">branden</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als ick dan kan <rhyme label="a" type="f">genaken</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">V hemel soete mondt, so kan ick <rhyme label="b" type="f">voelen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Mijn heete brandt <rhyme label="b" type="f" corresp="#s2:l2">verkoelen</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Ach! als ick magh aen uwe <rhyme label="a" type="f" corresp="#s2:l1">Roose-kaecken</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En soete lipjes <rhyme label="a" type="f" corresp="#s2:l1">raecken</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Sal ick mijn minne, in 't soete Nectar <rhyme label="c" type="f">zuygen</rhyme>,</l>
        <l id="s2:l7" met="7" type="-+">Rosamondi, Rosamondi, Rosamondi, <rhyme label="c" type="f" corresp="#s2:l6">getuygen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'t Soet omhelsen der <rhyme label="a" type="f">narmen</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">In avondt-uurtjes op hun soetste <rhyme label="b" type="f">groeyen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Doet ziel in liefde <rhyme label="b" type="f" corresp="#s3:l2">bloeyen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Als sigh 't geliefde, beweegen laet door 't <rhyme label="a" type="f" corresp="#s3:l1">karmen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Om over liefd' <rhyme label="a" type="f" corresp="#s3:l1">t'erbarmen</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Laet dat u liefde, door uw liefd' te <rhyme label="c" type="f">keuren</rhyme>,</l>
        <l id="s3:l7" met="7" type="-+">Rosamondi, Rosamondi, Rosamondi, <rhyme label="c" type="f" corresp="#s3:l6">gebeuren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
