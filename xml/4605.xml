<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4605</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatstmaal ging ik ter jacht</melody>
  <author>Joannes Stalpaert van der Wiele</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">T’Geen ik u, sprak den <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Gebie ter goeder <rhyme label="b" type="f">trouwen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat doed noch min noch <rhyme label="a" type="m" corresp="#s1:l1">meer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dus past my <rhyme label="b" type="f" corresp="#s1:l2">t’onderhouwen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">De zuyv’re wet, die ik u <rhyme label="c" type="m">gaf</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Noch doedter toe, noch trekter <rhyme label="c" type="m" corresp="#s1:l5">af</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">doedter toe, noch trekter <rhyme label="c" type="m" corresp="#s1:l5">af</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoord hier Papist! dus <rhyme label="a" type="m">luydt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Den text van <rhyme label="b" type="f">Deutronomen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Fijn Man! wat wiljer <rhyme label="a" type="m" corresp="#s2:l1">uit</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Besluiten, t’uwer <rhyme label="b" type="f" corresp="#s2:l2">vromen</rhyme>?</l>
        <l id="s2:l5" met="4" type="-+">Besluiten? Dat het Pausdom <rhyme label="c" type="m">alst</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Besluiten? Dat het Pausdom <rhyme label="c" type="m" corresp="#s2:l5">alst</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Iet nieus gebiedt, de schrift <rhyme label="c" type="m" corresp="#s2:l5">vervalst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wildt ghy den Text aldus <rhyme label="a" type="m">verstaen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ey goe! zoo doed my <rhyme label="b" type="f">weten</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Of ook dit schennis is <rhyme label="a" type="m" corresp="#s3:l1">begaen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van alle de <rhyme label="b" type="f" corresp="#s3:l2">Propheten</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Schriftuiren, Psalmen, bouken <rhyme label="c" type="m">die</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Schriftuiren, Psalmen, bouken <rhyme label="c" type="m" corresp="#s3:l5">die</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Ick nae dit woord beschreven <rhyme label="c" type="m" corresp="#s3:l5">zie</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hoe kwamen <rhyme label="a" type="m">Mardocheeus</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Zulks wy noch kunnen <rhyme label="b" type="f">lezen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En Iudas <rhyme label="a" type="m" corresp="#s4:l1">Machabeeus</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Te treden boven <rhyme label="b" type="f" corresp="#s4:l2">dezen</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">Hoe kwam dat vierderley <rhyme label="c" type="m">Bodvast</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Hoe kwam dat vierderley <rhyme label="c" type="m" corresp="#s4:l5">Bodvast</rhyme></l>
        <l id="s4:l7" met="4" type="+-">By Zacharias wierd <rhyme label="c" type="m" corresp="#s4:l5">belast</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoe dorsten in ’t <rhyme label="a" type="m">begin</rhyme></l>
        <l id="s5:l2" met="3" type="-+">De eerste twaalf <rhyme label="b" type="f">boden</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Het Christen <rhyme label="a" type="m" corresp="#s5:l1">huisgezin</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Bevelen, als van <rhyme label="b" type="f" corresp="#s5:l2">node</rhyme></l>
        <l id="s5:l5" met="4" type="-+">’t Onhouden zich met nauwe <rhyme label="c" type="m">wacht</rhyme></l>
        <l id="s5:l6" met="4" type="-+">’t Onhouden zich met nauwe <rhyme label="c" type="m" corresp="#s5:l5">wacht</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Van ronnen bloed, en van <rhyme label="c" type="m" corresp="#s5:l5">versmacht</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">’t Is waer dit was wel <rhyme label="a" type="m">eer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Door Moises aenden <rhyme label="b" type="f">Ioden</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Maer nu noch <rhyme label="a" type="m" corresp="#s6:l1">nimmermeer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Aen ’t Heydendom <rhyme label="b" type="f" corresp="#s6:l2">geboden</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Voor dat hen deze keur <rhyme label="c" type="m">gezet</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Voor dat hen deze keur <rhyme label="c" type="m" corresp="#s6:l5">gezet</rhyme></l>
        <l id="s6:l7" met="4" type="+-">Werd deur d’Apostolijke <rhyme label="c" type="m" corresp="#s6:l5">Wet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">En dat deur middel, <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van nieuwe <rhyme label="b" type="f">openbaring</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Gelijk elk een wel <rhyme label="a" type="m" corresp="#s7:l1">ziet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Uit dien, dat by <rhyme label="b" type="f" corresp="#s7:l2">vergaring</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Van raed, als ordinaris <rhyme label="c" type="m">beurdt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Van raed, als ordinaris <rhyme label="c" type="m" corresp="#s7:l5">beurdt</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Dit van d’Apost’len is <rhyme label="c" type="m" corresp="#s7:l5">gekeurdt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wel tot een klaer <rhyme label="a" type="m">bemerk</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat zulks tot allen <rhyme label="b" type="f">tijden</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Goods algemein <rhyme label="a" type="m" corresp="#s8:l1">Kerk</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Was magtig te <rhyme label="b" type="f" corresp="#s8:l2">verlijden</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Die hy tot ’s werelds eynde <rhyme label="c" type="m">toe</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Die hy tot ’s werelds eynde <rhyme label="c" type="m" corresp="#s8:l5">toe</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Belast heeft mette <rhyme label="c" type="m" corresp="#s8:l5">Schepers-roe</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Midsdien werd meer als <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Bevonden, dat van <rhyme label="b" type="f">Gode</rhyme></l>
        <l id="s9:l3" met="3" type="-+">By ’t Deutronooms <rhyme label="a" type="m" corresp="#s9:l1">bevel</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Niet anders werd <rhyme label="b" type="f" corresp="#s9:l2">verboden</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Dan by zijn woord te geender <rhyme label="c" type="m">tijd</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Dan by zijn woord te geender <rhyme label="c" type="m" corresp="#s9:l5">tijd</rhyme></l>
        <l id="s9:l7" met="4" type="+-">Te vougen datter <rhyme label="c" type="m" corresp="#s9:l5">tegenstrijd</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wildt ghy dan, lieve <rhyme label="a" type="m">knecht</rhyme>!</l>
        <l id="s10:l2" met="3" type="-+">De Roomsche keur <rhyme label="b" type="f">afzetten</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Zoo tooondt eerst datze <rhyme label="a" type="m" corresp="#s10:l1">vecht</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Met Goods geschreve <rhyme label="b" type="f" corresp="#s10:l2">Wetten</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">End’ ik zal straks nae d’eerste <rhyme label="c" type="m">proof</rhyme></l>
        <l id="s10:l6" met="4" type="-+">End’ ik zal straks nae d’eerste <rhyme label="c" type="m" corresp="#s10:l5">proof</rhyme></l>
        <l id="s10:l7" met="4" type="+-">Met u aenvaerden ’t nieu <rhyme label="c" type="m" corresp="#s10:l5">geloof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
