    <melody>Psalm 024 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt Aerden-rondt, en alle <rhyme label="a" type="m">ding</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dat in sijn ruymen <rhyme label="a" type="m" corresp="#s1:l1">omme-ring</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Sich off verschuylt off laet <rhyme label="b" type="f">aenschouwen</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">Op woont; (het zy off kleyn off <rhyme label="c" type="m">groot</rhyme>)</l>
        <l id="s1:l5" met="4" type="-+">In is; (’tzy levend’ ofte <rhyme label="c" type="m" corresp="#s1:l4">doodt</rhyme>)</l>
        <l id="s1:l6" met="4" type="-+">Is Godes, die’t wel eer ging <rhyme label="b" type="f" corresp="#s1:l3">bouwen</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die’t konstelijck heeft <rhyme label="a" type="m">toegericht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Gëevenaert in juyst <rhyme label="a" type="m" corresp="#s2:l1">gewicht</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Soo dattet heen noch we’er magh <rhyme label="b" type="f">schricken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Maer vast leyt op’t onvaste <rhyme label="c" type="m">vocht</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Om-togen met d’onvaste <rhyme label="c" type="m" corresp="#s2:l4">locht</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Door sijnes Al-machts wijs <rhyme label="b" type="f" corresp="#s2:l3">beschicken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Aerdt is Godts. en yeder <rhyme label="a" type="m">een</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Maeckt hyse, goedighlijck, <rhyme label="a" type="m" corresp="#s3:l1">gemeen</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Doch eene plaets heeft hy <rhyme label="b" type="f">verkoren</rhyme></l>
        <l id="s3:l4" met="4" type="-+">En sich ter eygen woonst <rhyme label="c" type="m">gemaeckt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Daer yeder een niet in en <rhyme label="c" type="m" corresp="#s3:l4">raeckt</rhyme>.</l>
        <l id="s3:l6" met="4" type="-+">Wien is daer’t borger-recht <rhyme label="b" type="f" corresp="#s3:l3">geboren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hem, die, uyt onvervalscht <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Godt-deuchdelijcke daden <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Oprecht in al sijns levens <rhyme label="b" type="f">wandel</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Die in sijn hert niet is <rhyme label="c" type="m">vergeckt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Op wijsheydt die tot boosheydt <rhyme label="c" type="m" corresp="#s4:l4">streckt</rhyme></l>
        <l id="s4:l6" met="4" type="-+">En kloeck maeckt in verkeerden <rhyme label="b" type="f" corresp="#s4:l3">handel</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die, door geveynsden valschen <rhyme label="a" type="m">eedt</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Sijn tonge niet tot yemandts <rhyme label="a" type="m" corresp="#s5:l1">leet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Off heeft bereydt, off wil <rhyme label="b" type="f">verkoopen</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Dus staet den mensch’ (en anders <rhyme label="c" type="m">niet</rhyme>)</l>
        <l id="s5:l5" met="4" type="-+">Tot zegening en recht <rhyme label="c" type="m" corresp="#s5:l4">geniet</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Van Godes heyl, den ingang <rhyme label="b" type="f" corresp="#s5:l3">open</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dit’s ’tpadt waer langs men God <rhyme label="a" type="m">genaeckt</rhyme>.</l>
        <l id="s6:l2" met="4" type="-+">Dit zijnse die Godt straff-vry <rhyme label="a" type="m" corresp="#s6:l1">maeckt</rhyme></l>
        <l id="s6:l3" met="4" type="-+">En geen van haer voorleden <rhyme label="b" type="f">sonden</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Tot sonde nu meer toe en <rhyme label="c" type="m">telt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Maer heeftse in’t vergeet <rhyme label="c" type="m" corresp="#s6:l4">gestelt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Als in de diepe zee <rhyme label="b" type="f" corresp="#s6:l3">verslonden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dit is het ware <rhyme label="a" type="m">Israel</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Dit zijnse die, na Godts <rhyme label="a" type="m" corresp="#s7:l1">bevel</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ter deuchdt, on-aff-gemat, sich <rhyme label="b" type="f">kloecken</rhyme>:</l>
        <l id="s7:l4" met="4" type="-+">Dit zijnse die voor Godts <rhyme label="c" type="m">aenschijn</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Behaechelijcke vrunden <rhyme label="c" type="m" corresp="#s7:l4">zijn</rhyme>.</l>
        <l id="s7:l6" met="4" type="-+">O! Heyl wel waerdigh om te <rhyme label="b" type="f" corresp="#s7:l3">soecken</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Gaet op, gaet op, en maeckt u <rhyme label="a" type="m">wijdt</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Ghy poorten Godts die eeuwigh <rhyme label="a" type="m" corresp="#s8:l1">zijt</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Daer komt, daer komt een heerlijck <rhyme label="b" type="f">Koning</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Wie is de Koning dieder <rhyme label="c" type="m">naeckt</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En, eeren-rijck, sijn gangen <rhyme label="c" type="m" corresp="#s8:l4">maeckt</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Na Godes uytverkoren <rhyme label="b" type="f" corresp="#s8:l3">wooning</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">De Koning, soo omringt met <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Die is een over-machtich <rhyme label="a" type="m" corresp="#s9:l1">Heer</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">In’t strijden, dapper en <rhyme label="b" type="f">stoutmoedigh</rhyme>;</l>
        <l id="s9:l4" met="4" type="-+">In’t oorlogh, een groot-hertich <rhyme label="c" type="m">heldt</rhyme>;</l>
        <l id="s9:l5" met="4" type="-+">Een man noyt afgeslooft in’t <rhyme label="c" type="m" corresp="#s9:l4">veldt</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">En in’t verwinnen <rhyme label="b" type="f" corresp="#s9:l3">overvloedigh</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Was oyt een sterff’lijck mensche <rhyme label="a" type="m">waerdt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Door deuchdt en Godlijckheydt van <rhyme label="a" type="m" corresp="#s10:l1">aert</rhyme></l>
        <l id="s10:l3" met="4" type="-+">In’t eeuwigh Godes huys te <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s10:l4" met="4" type="-+">Soo is het dese, die op’t <rhyme label="c" type="m">padt</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Ter onverganckelijcker <rhyme label="c" type="m" corresp="#s10:l4">Stadt</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Sich selfs ten voorbeeldt heeft <rhyme label="b" type="f" corresp="#s10:l3">gegeven</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Gaet op dan, (o!) en maeckt u <rhyme label="a" type="m">wijdt</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Ghy poorten Godts die eeuwigh <rhyme label="a" type="m" corresp="#s11:l1">zijt</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">Daer komt, daer komt, een heerlijck <rhyme label="b" type="f">Koning</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Wie is de Koning dieder <rhyme label="c" type="m">naeckt</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">En, eeren-rijck, sijn gangen <rhyme label="c" type="m" corresp="#s11:l4">maeckt</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Na Godes uytverkoren <rhyme label="b" type="f" corresp="#s11:l3">wooning</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">De Koning, soo omringt met <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Die is een over-machtich <rhyme label="a" type="m" corresp="#s12:l1">Heer</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">De groote Heere der <rhyme label="b" type="f">Heyrscharen</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Die duysendt, duysendt, duysendt <rhyme label="c" type="m">telt</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Wanneer hy sijn slach-orden <rhyme label="c" type="m" corresp="#s12:l4">stelt</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Die is’t, die, zeegh-rijck, in komt <rhyme label="b" type="f" corresp="#s12:l3">varen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
