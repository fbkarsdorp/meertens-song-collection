<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>166212</id>
    <date>
      <year-start>1620</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="New Year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Gij heidenen allen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Jaer tyt van <rhyme label="a" type="m">geboort</rhyme></l>
        <l id="s1:l2" met="3" type="-+">wort gehouden <rhyme label="b" type="m">verkeert</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Broot stadt rechte <rhyme label="a" type="m" corresp="#s1:l1">voort</rhyme></l>
        <l id="s1:l4" met="3" type="-+">noodich by stant <rhyme label="b" type="m" corresp="#s1:l2">begeert</rhyme></l>
        <l id="s1:l5" met="2" type="-+">winter ons <rhyme label="b" type="m" corresp="#s1:l2">leert</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Naeckten te nemen <rhyme label="c" type="m">waer</rhyme></l>
        <l id="s1:l7" met="4" type="-+">als wyzen gaet bij arm van <rhyme label="d" type="m">staet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In desen nieuwe <rhyme label="c" type="m" corresp="#s1:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Herodes vol <rhyme label="a" type="m">nydt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Den wyzen heeft <rhyme label="b" type="m">gevraecht</rhyme></l>
        <l id="s2:l3" met="3" type="+-">In hoe corten <rhyme label="a" type="m" corresp="#s2:l1">tyt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">De Star hun heeft <rhyme label="b" type="m" corresp="#s2:l2">behaeght</rhyme></l>
        <l id="s2:l5" met="2" type="-+">En <rhyme label="b" type="m" corresp="#s2:l2">onvertsaecht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Te reysen wegen <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Om sien gods soon ,, met giften <rhyme label="d" type="m">schoon</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Int eerste nieuwe <rhyme label="c" type="m" corresp="#s2:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Herodes <rhyme label="a" type="m">pracktyck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En tyrannye <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s3:l3" met="3" type="+-">vrees voor t'koning <rhyme label="a" type="m" corresp="#s3:l1">ryck</rhyme></l>
        <l id="s3:l4" met="3" type="-+">wenste gods soone <rhyme label="b" type="m" corresp="#s3:l2">Doot</rhyme></l>
        <l id="s3:l5" met="2" type="-+">kinderen In <rhyme label="b" type="m" corresp="#s3:l2">Noot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gode schriftuyr, tuijcht <rhyme label="c" type="m">klaer</rhyme></l>
        <l id="s3:l7" met="4" type="-+">hoe veel vermelt, ende <rhyme label="d" type="m">ontsielt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Int tweede nieuwe <rhyme label="c" type="m" corresp="#s3:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Vrede lange <rhyme label="a" type="m">verwacht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">De werelt niet <rhyme label="b" type="m">bemint</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Bethlem, <rhyme label="a" type="m" corresp="#s4:l1">veracht</rhyme></l>
        <l id="s4:l4" met="3" type="-+">tot hoogheyt, meest <rhyme label="b" type="m" corresp="#s4:l2">gesint</rhyme></l>
        <l id="s4:l5" met="2" type="-+">haet nydt men <rhyme label="b" type="m" corresp="#s4:l2">vindt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">By seckten <rhyme label="c" type="m">openbaer</rhyme></l>
        <l id="s4:l7" met="4" type="-+">werelt neemt merck op syons <rhyme label="d" type="m">kerck</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In desen nieuwen <rhyme label="c" type="m" corresp="#s4:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Een helt wort <rhyme label="a" type="m">gemist</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die klouck was Int <rhyme label="b" type="m">verstant</rhyme></l>
        <l id="s5:l3" met="3" type="+-">heeft den <rhyme label="a" type="m" corresp="#s5:l1">antechrist</rhyme></l>
        <l id="s5:l4" met="3" type="-+">gehouden uyt het <rhyme label="b" type="m" corresp="#s5:l2">lant</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Spaenschen <rhyme label="b" type="m" corresp="#s5:l2">vijant</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Serslagen hier en <rhyme label="c" type="m">Daer</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">verleent o heer, Nassou prys <rhyme label="d" type="m">eer</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In desen nieuwen <rhyme label="c" type="m" corresp="#s5:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Blijf de lieffde <rhyme label="a" type="m">Cranck</rhyme></l>
        <l id="s6:l2" met="3" type="-+">off helpt geen <rhyme label="b" type="m">medecijn</rhyme></l>
        <l id="s6:l3" met="3" type="+-">om der sonden <rhyme label="a" type="m" corresp="#s6:l1">stanck</rhyme></l>
        <l id="s6:l4" met="3" type="-+">en ander quaet <rhyme label="b" type="m" corresp="#s6:l2">Fenijn</rhyme></l>
        <l id="s6:l5" met="2" type="-+">De straffe <rhyme label="b" type="m" corresp="#s6:l2">wijs</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Doet sweeren om een <rhyme label="c" type="m">jaer</rhyme></l>
        <l id="s6:l7" met="4" type="-+">nae overvloet ,, volght <rhyme label="d" type="m">tegenspoet</rhyme></l>
        <l id="s6:l8" met="3" type="-+">In desen nieuwe <rhyme label="c" type="m" corresp="#s6:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Den prins boven <rhyme label="a" type="m">all</rhyme></l>
        <l id="s7:l2" met="3" type="-+">vant begin tot den <rhyme label="b" type="m">ent</rhyme></l>
        <l id="s7:l3" met="3" type="+-">wacht op t'vroom <rhyme label="a" type="m" corresp="#s7:l1">getall</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Daer lieffd es <rhyme label="b" type="m" corresp="#s7:l2">t'Fondament</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Tott twist <rhyme label="b" type="m" corresp="#s7:l2">gewent</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Gaet by t'helsche heyr <rhyme label="c" type="m">schaer</rhyme></l>
        <l id="s7:l7" met="4" type="-+">acoley witt ,, gods rijck <rhyme label="d" type="m">besitt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Int Salich nieuwe <rhyme label="c" type="m" corresp="#s7:l6">Iaer</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
