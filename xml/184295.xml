<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184295</id>
    <date>
      <year-start>1794</year-start>
      <year-end>1794</year-end>
    </date>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ha wat pret komt van het <rhyme label="a" type="f">trouwen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Stijntjen zal 't niet lang meer <rhyme label="a" type="f" corresp="#s1:l1">houên</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s1:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">O ze is reeds zo hups en <rhyme label="a" type="f">gekjes</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Ja ze trekt al scheeve <rhyme label="a" type="f" corresp="#s2:l1">bekjes</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s2:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'K zal wat lustig kraammaal <rhyme label="a" type="f">houên</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Ha wat pret komt van het <rhyme label="a" type="f" corresp="#s3:l1">trouwen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s3:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'K zal mij met kandeel <rhyme label="a" type="f">lardeeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Al de wijfjens fiks <rhyme label="a" type="f" corresp="#s4:l1">tracteeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s4:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'K zal de mans ook niet <rhyme label="a" type="f">vergeeten</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Maar dan schaf ik vaster <rhyme label="a" type="f" corresp="#s5:l1">eeten</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s5:l2">zin</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'K zal een pints bocaaltjen <rhyme label="a" type="f">koopen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s6:l3" met="4" type="+-">'K drink alleen wel twintig <rhyme label="a" type="f" corresp="#s6:l1">stoepen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s6:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'T kind brengt duizend guldens <rhyme label="a" type="f">mede</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s7:l3" met="4" type="+-">En daar ben ik meê te <rhyme label="a" type="f" corresp="#s7:l1">vrede</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s7:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">'K heb nog nooit zo blij <rhyme label="a" type="f">gezongen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s8:l3" met="4" type="+-">Stijntje krijgt ook vast een' <rhyme label="a" type="f" corresp="#s8:l1">jongen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s8:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">'T zal des weêr een Keesjen <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s9:l3" met="4" type="+-">Uit het huwlijk komen <rhyme label="a" type="f" corresp="#s9:l1">Keezen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Vivat de min! 'k heb 't naar mijn' <rhyme label="b" type="m" corresp="#s9:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Och kreeg stijntje 't maar wat <rhyme label="a" type="f">kwaader</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Vivat de min! vivat de <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s10:l3" met="4" type="+-">Werd ik deezen dag nog <rhyme label="a" type="f" corresp="#s10:l1">Vader</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Vivat de min! 't was naar mijn' <rhyme label="b" type="m" corresp="#s10:l2">zin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
