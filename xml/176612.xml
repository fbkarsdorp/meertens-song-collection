    <melody>La marine</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HElp Musa my 't lof <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Al van een Blaeuwe <rhyme label="b" type="m">scheen</rhyme>;</l>
        <l id="s1:l3" met="3" type="+-">Wat nut hy kan voort <rhyme label="a" type="f" corresp="#s1:l1">brengen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Toont nu aan yder <rhyme label="b" type="m" corresp="#s1:l2">een</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Dick komt een jong <rhyme label="c" type="m">gesel</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Nog uyt den dop niet <rhyme label="c" type="m" corresp="#s1:l5">wel</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">die het Trouwen Wis soud <rhyme label="d" type="f">rouwen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Kond hy 't <rhyme label="c" type="m" corresp="#s1:l5">wel</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Die dan door dit <rhyme label="a" type="f">weygeren</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Ook wat langer <rhyme label="b" type="m">wagt</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Of die te hoog wil <rhyme label="a" type="f" corresp="#s2:l1">steygeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Sijn selven te groot <rhyme label="b" type="m" corresp="#s2:l2">agt</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">Krygt hy een blauwe <rhyme label="c" type="m">scheen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Hem leert dit brave <rhyme label="c" type="m" corresp="#s2:l5">Neen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Beter letten, Sijn Pols <rhyme label="d" type="f">Setten</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Na de <rhyme label="c" type="m" corresp="#s2:l5">Reen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Beurt het een reys of <rhyme label="a" type="f">meer</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">dat gy krijge dit <rhyme label="b" type="m">neen</rhyme>?</l>
        <l id="s3:l3" met="3" type="+-">'t Sal u het Vryen <rhyme label="a" type="f" corresp="#s3:l1">leere</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Waar door gy ligt krijgt <rhyme label="b" type="m" corresp="#s3:l2">een</rhyme></l>
        <l id="s3:l5" met="3" type="+-">die in deugt, Eer, of <rhyme label="c" type="m">staat</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">d' Eeerst verr' te boven <rhyme label="c" type="m" corresp="#s3:l5">gaat</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">dus u wagten, en <rhyme label="d" type="f">veragten</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">dijt tot <rhyme label="c" type="m" corresp="#s3:l5">baat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Het scharript ook de <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">En 't maakt u een <rhyme label="b" type="m">PoÃªet</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Schoon dat gy voor het <rhyme label="a" type="f" corresp="#s4:l1">minnen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">daar niemendal van <rhyme label="b" type="m" corresp="#s4:l2">weet</rhyme>;</l>
        <l id="s4:l5" met="3" type="+-">Maar groter <rhyme label="c" type="m">nuttigheyt</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Nog in het blaentje <rhyme label="c" type="m" corresp="#s4:l5">leyt</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">'t Sal u leeren, En <rhyme label="d" type="f">vermeeren</rhyme>.</l>
        <l id="s4:l8" met="2" type="+-"><rhyme label="c" type="m" corresp="#s4:l5">Lijdsaamheyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">dog soud ik 't al <rhyme label="a" type="f">opsoeken</rhyme>:</l>
        <l id="s5:l2" met="3" type="+-">Het blauwe scheen <rhyme label="b" type="m">loft</rhyme>.</l>
        <l id="s5:l3" met="3" type="+-">Een lietje niet Maar <rhyme label="a" type="f" corresp="#s5:l1">boeken</rhyme>.</l>
        <l id="s5:l4" met="3" type="+-">Schreef in van dese <rhyme label="b" type="m" corresp="#s5:l2">stoft</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">dies of het u <rhyme label="c" type="m">geschiet</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Maar daarom geen <rhyme label="c" type="m" corresp="#s5:l5">verdriet</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Want het kijven van qua <rhyme label="d" type="f">Wijven</rhyme></l>
        <l id="s5:l8" met="2" type="+-">Hoorje <rhyme label="c" type="m" corresp="#s5:l5">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
