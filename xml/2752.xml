<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2752</id>
    <date>
      <year-start>1695</year-start>
      <year-end>1695</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody>Schoon liefje jent zeer excellent</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">NOyt soo grooten wonder <rhyme label="a" type="m">daedt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Op aerden is <rhyme label="b" type="f">vernomen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Als dat naer langh besloten <rhyme label="a" type="m" corresp="#s1:l1">raedt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">'t Beloofde Vrouwen <rhyme label="a" type="m" corresp="#s1:l1">zaedt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer all' ons hoop op <rhyme label="a" type="m" corresp="#s1:l1">staet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Is in het vleesch <rhyme label="b" type="f" corresp="#s1:l2">gekomen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">d' Heyl'ge Geest heeft met sijn <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De maeght van Godt <rhyme label="b" type="f">verkooren</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van Davids Konincklijck <rhyme label="a" type="m" corresp="#s2:l1">geslacht</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Omschaduwt <rhyme label="a" type="m" corresp="#s2:l1">onverwacht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Door sijn God'lijcke <rhyme label="a" type="m" corresp="#s2:l1">kracht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Is Jesus ons <rhyme label="b" type="f" corresp="#s2:l2">geboren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dit Wonder-werck verstaen wy <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'t Gaet ons begrijp te <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wat in die schaduw' is <rhyme label="a" type="m" corresp="#s3:l1">geschiet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Geen Menschen oogh <rhyme label="a" type="m" corresp="#s3:l1">doorsiet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Te meerder moet ons <rhyme label="a" type="m" corresp="#s3:l1">Liedt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">'t Werck sijn Meester <rhyme label="b" type="f" corresp="#s3:l2">loven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">De Jonckvrouw' was Maeght <rhyme label="a" type="m">onbevleckt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Maeght bleef sy, en wierdt <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Sy heeft noyt voet by Man <rhyme label="a" type="m" corresp="#s4:l1">gestreckt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Godt heeft in haer <rhyme label="a" type="m" corresp="#s4:l1">verweckt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hem die ons sonden <rhyme label="a" type="m" corresp="#s4:l1">deckt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ons Heylandt en <rhyme label="b" type="f" corresp="#s4:l2">behoeder</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maria gingh van <rhyme label="a" type="m">Nazareth</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met moeyt' en groot <rhyme label="b" type="f">bezwaren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Naer Bethlehem door 's Keysers <rhyme label="a" type="m" corresp="#s5:l1">wet</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Doen was verloopen <rhyme label="a" type="m" corresp="#s5:l1">net</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Haer tijdt van Godt <rhyme label="a" type="m" corresp="#s5:l1">geset</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sy quam in noodt van <rhyme label="b" type="f" corresp="#s5:l2">baren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">In een Stal heeft sy <rhyme label="a" type="m">gebaert</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">By 't Vee is sy <rhyme label="b" type="f">ontbonden</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">s' Heeft haer zwack Lichaem niet <rhyme label="a" type="m" corresp="#s6:l1">gespaert</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Naer Moederlijcken <rhyme label="a" type="m" corresp="#s6:l1">aert</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Haer jongen Soon <rhyme label="a" type="m" corresp="#s6:l1">bewaert</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">'t Best dat sy mocht <rhyme label="b" type="f" corresp="#s6:l2">gewonden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">In een Kribb' niet wel <rhyme label="a" type="m">gespreyt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Most sy hem <rhyme label="b" type="f">nederleggen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hy tot wien 't Vee om voeren <rhyme label="a" type="m" corresp="#s7:l1">schreyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Die alle Beesten <rhyme label="a" type="m" corresp="#s7:l1">weydt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wierdt voor het Vee <rhyme label="a" type="m" corresp="#s7:l1">geleydt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">O wonder vreemt om <rhyme label="b" type="f" corresp="#s7:l2">seggen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Well'kom arm sachtmoedigh <rhyme label="a" type="m">Helt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Weest well'kom hier te <rhyme label="b" type="f">Lande</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Gy hebt ons zwaer rantsoen <rhyme label="a" type="m" corresp="#s8:l1">getelt</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Geen Goudt of Silver <rhyme label="a" type="m" corresp="#s8:l1">gelt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Maer hebt voor ons <rhyme label="a" type="m" corresp="#s8:l1">gestelt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Uw dierbaer Bloedt te <rhyme label="b" type="f" corresp="#s8:l2">pande</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
