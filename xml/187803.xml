    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">O wilt ge met mij gaan,</l>
        <l id="s1:l2">O Tibbie, zoet kind?</l>
        <l id="s1:l3">O wilt ge met mij gaan,</l>
        <l id="s1:l4">O Tibbie, zoet kind?</l>
        <l id="s1:l5">En varen of rijden</l>
        <l id="s1:l6">Zoo snel als de wind,</l>
        <l id="s1:l7">Of traag me ter zij' gaan,</l>
        <l id="s1:l8">O Tibbie, zoet kind?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Wat deert mij uw vader</l>
        <l id="s2:l2">Zijn adellijk bloed!</l>
        <l id="s2:l3">Wat deert mij uw vader</l>
        <l id="s2:l4">Zijn geld en zijn goed!</l>
        <l id="s2:l5">Zeg, dat ge wilt meegaan,</l>
        <l id="s2:l6">Zeg, dat ge mij mint,</l>
        <l id="s2:l7">En kom in uw roksken,</l>
        <l id="s2:l8">O Tibbie, zoet kind!</l>
      </lg>
    </lg>
  </text>
</song>
