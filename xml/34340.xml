      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Partage librement</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">BEweegster van ons <rhyme label="a" type="m">jeught</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Noit uws gelijcken <rhyme label="b" type="m">wert</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van Son op d Aerd <rhyme label="c" type="m">bestraelt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ghy hebt u dienaers <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Door d'afgemaele <rhyme label="b" type="m" corresp="#s1:l2">smert</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In u gewelt <rhyme label="c" type="m" corresp="#s1:l3">behaelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Iuweel van <rhyme label="a" type="m">Zeegbaerheyt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Volmaeckt <rhyme label="b" type="m">Pigmali-belt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">VViens schoonheyt noit men <rhyme label="c" type="m">vant</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In u myn leven <rhyme label="a" type="m" corresp="#s2:l1">leyt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat schier te neer <rhyme label="b" type="m" corresp="#s2:l2">ghevelt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Noch in u Liefde <rhyme label="c" type="m" corresp="#s2:l3">brant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy zyt myn <rhyme label="a" type="m">Morgen-ster</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Myn jeughdens <rhyme label="b" type="m">Dagheraet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En Liefdens gulde <rhyme label="c" type="m">Son</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Ben ick van u wat <rhyme label="a" type="m" corresp="#s3:l1">ver'</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Ke my toch niet <rhyme label="b" type="m" corresp="#s3:l2">versmaet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Myn liefste <rhyme label="c" type="m" corresp="#s3:l3">Helicon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Voedersse van ons <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghy hebt den lof <rhyme label="b" type="m">verdint</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dat ghy hem hebt <rhyme label="c" type="m">ghewont</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Versmaet hem niet <rhyme label="a" type="m" corresp="#s4:l1">Goddin</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Maeckt t'hert van uwen <rhyme label="b" type="m" corresp="#s4:l2">vrint</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Door weder-min <rhyme label="c" type="m" corresp="#s4:l3">ghesont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Soo gy t'gesont weer <rhyme label="a" type="m">maeckt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Princersse die ick <rhyme label="b" type="m">groet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En soo gy heelt mijn <rhyme label="c" type="m">pijn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Mijn hert dat naer u <rhyme label="a" type="m" corresp="#s5:l1">snaeckt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">En leste druppel <rhyme label="b" type="m" corresp="#s5:l2">bloet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Sal u gheschoencken <rhyme label="c" type="m" corresp="#s5:l3">sijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
