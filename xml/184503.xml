    <melody>Droge Kees</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Liefste Kaatje, bolle <rhyme label="a" type="m">mied</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Zult gy dan nooit na na na my <rhyme label="b" type="f">hooren</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">'k Heb u al zo lang <rhyme label="a" type="m" corresp="#s1:l1">gevryd</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Zonder dat ik in die <rhyme label="a" type="m" corresp="#s1:l1">tyd</rhyme></l>
        <l id="s1:l5" met="3" type="+-">U nog kon <rhyme label="b" type="f" corresp="#s1:l2">bekooren</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Zult gy nooit na na na my <rhyme label="b" type="f" corresp="#s1:l2">hooren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Joris of gy klaagt of <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Gy zuit my dog nooit nooit nooit <rhyme label="b" type="f">behaagen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Trouwen baart te veel <rhyme label="a" type="m" corresp="#s2:l1">verdriet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat men dagelyks hoort en <rhyme label="a" type="m" corresp="#s2:l1">ziet</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Veelen die 't <rhyme label="b" type="f" corresp="#s2:l2">beklagen</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Gy zult my dog nooit nooit nooit <rhyme label="b" type="f" corresp="#s2:l2">behaagen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zalig is den egten <rhyme label="a" type="m">staat</rhyme>;</l>
        <l id="s3:l2" met="5" type="-+">Maar men moet die wel wel wel <rhyme label="b" type="f">beleven</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Als men altyd vroeg en <rhyme label="a" type="m" corresp="#s3:l1">laat</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Wil krakeelen dat is <rhyme label="a" type="m" corresp="#s3:l1">kwaat</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Trouw zal blydschap <rhyme label="b" type="f" corresp="#s3:l2">geven</rhyme>;</l>
        <l id="s3:l6" met="5" type="-+">Maar men moet die wel wel wel <rhyme label="b" type="f" corresp="#s3:l2">beleven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Joris ja dat hebt ge <rhyme label="a" type="m">wel</rhyme>!</l>
        <l id="s4:l2" met="5" type="-+">Maar wie weet dat van van van te <rhyme label="b" type="f">vooren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">'t Is beget geen <rhyme label="a" type="m" corresp="#s4:l1">kinderspel</rhyme>;</l>
        <l id="s4:l4" met="4" type="-+">Neen: schoon het niet beuren <rhyme label="a" type="m" corresp="#s4:l1">zel</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">'k Had jou noit <rhyme label="b" type="f" corresp="#s4:l2">verkooren</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Weet men dat van van van te <rhyme label="b" type="f" corresp="#s4:l2">vooren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zoete Engel dat is wel <rhyme label="a" type="m">waar</rhyme>:</l>
        <l id="s5:l2" met="5" type="-+">Schoon 't aan my niet zal zal zal <rhyme label="b" type="f">mankeeren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Maar hoe meenig duizent <rhyme label="a" type="m" corresp="#s5:l1">paar</rhyme>?</l>
        <l id="s5:l4" met="4" type="-+">Leeft gelokkig met <rhyme label="a" type="m" corresp="#s5:l1">malkaar</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Laat ons 't ook <rhyme label="b" type="f" corresp="#s5:l2">probeeren</rhyme>?</l>
        <l id="s5:l6" met="5" type="-+">Lief 't zal niet aan my my my <rhyme label="b" type="f" corresp="#s5:l2">mankeeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'t Meest dat al de Vryers <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Is dat zy ons veel veel veel <rhyme label="b" type="f">belooven</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">'t Zy om een schoonheid of om <rhyme label="a" type="m" corresp="#s6:l1">poen</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Maar hoe gaauw vergaat dat <rhyme label="a" type="m" corresp="#s6:l1">groen</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">'t Wyf zit als <rhyme label="b" type="f" corresp="#s6:l2">verschooven</rhyme>,</l>
        <l id="s6:l6" met="5" type="-+">Jongmans doen niet als als als <rhyme label="b" type="f" corresp="#s6:l2">belooven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Liefste Kaatje zoo 't kon <rhyme label="a" type="m">zyn</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Gy liet myn zeker graag graag graag <rhyme label="b" type="f">probere</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Meisje daar is geen erg by <rhyme label="a" type="m" corresp="#s7:l1">myn</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Wat ik zeg dat meen ik <rhyme label="a" type="m" corresp="#s7:l1">rein</rhyme>?</l>
        <l id="s7:l5" met="3" type="+-">Al seen Borst met <rhyme label="b" type="f" corresp="#s7:l2">eeren</rhyme>,</l>
        <l id="s7:l6" met="5" type="-+">Ik liet zeker graag graag graag <rhyme label="b" type="f" corresp="#s7:l2">probeeren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ik moet laggen om die <rhyme label="a" type="m">praat</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">Eerst te probeeren en en en dan <rhyme label="b" type="f">koopen</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Joris nou dat was niet <rhyme label="a" type="m" corresp="#s8:l1">kwaat</rhyme>;</l>
        <l id="s8:l4" met="4" type="-+">Maar hoe zou de beste <rhyme label="a" type="m" corresp="#s8:l1">maat</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Op het laast nog <rhyme label="b" type="f" corresp="#s8:l2">loopen</rhyme>;</l>
        <l id="s8:l6" met="5" type="-+">Elk zou proeven maar maar maar niet <rhyme label="b" type="f" corresp="#s8:l2">koopen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Kaatje zoete Engel <rhyme label="a" type="m">laat</rhyme>:</l>
        <l id="s9:l2" met="5" type="-+">Ik u hertje dog dog dog <rhyme label="b" type="f">bewegen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Meisje 't Huwelyk is niet <rhyme label="a" type="m" corresp="#s9:l1">kwaat</rhyme>;</l>
        <l id="s9:l4" met="4" type="-+">'k Wacht met u in d'Egten <rhyme label="a" type="m" corresp="#s9:l1">Staat</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Vreede, heil en <rhyme label="b" type="f" corresp="#s9:l2">zeegen</rhyme>,</l>
        <l id="s9:l6" met="5" type="-+">Laat uw hertje dog dog dog <rhyme label="b" type="f" corresp="#s9:l2">beweegen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Och wat zyn wy meisjes <rhyme label="a" type="m">slegt</rhyme>,</l>
        <l id="s10:l2" met="5" type="-+">'k Voel ik weet niet wat wat wat van <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Joris meent gy 't zoo <rhyme label="a" type="m" corresp="#s10:l1">opregt</rhyme>:</l>
        <l id="s10:l4" met="4" type="-+">Is het waarheid dat gy <rhyme label="a" type="m" corresp="#s10:l1">zegt</rhyme>:</l>
        <l id="s10:l5" met="3" type="+-">'k Zal u ook <rhyme label="b" type="f" corresp="#s10:l2">beminnen</rhyme>,</l>
        <l id="s10:l6" met="5" type="-+">'k Voel ik weet niet wat wat wat van <rhyme label="b" type="f" corresp="#s10:l2">binnen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Ach volmaakte <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s11:l2" met="5" type="-+">'k Wil u waarlyk niet niet niet <rhyme label="b" type="f">bedriegen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Al te zuiver is myn <rhyme label="a" type="m" corresp="#s11:l1">min</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Dat ik met een valse <rhyme label="a" type="m" corresp="#s11:l1">zin</rhyme>:</l>
        <l id="s11:l5" met="3" type="+-">U in slap zou <rhyme label="b" type="f" corresp="#s11:l2">wiegen</rhyme>?</l>
        <l id="s11:l6" met="5" type="-+">'k Wil u waarlyk niet niet niet <rhyme label="b" type="f" corresp="#s11:l2">bedriegen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Jongen lui dat gaat jou <rhyme label="a" type="m">voor</rhyme>,</l>
        <l id="s12:l2" met="5" type="-+">'t Is het eerst mee van van van ons <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">'t Gaat zoo met de jeugd al <rhyme label="a" type="m" corresp="#s12:l1">door</rhyme>;</l>
        <l id="s12:l4" met="4" type="-+">Volgt ons al op dat <rhyme label="a" type="m" corresp="#s12:l1">spoor</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Waarom vry <rhyme label="b" type="f" corresp="#s12:l2">gebleven</rhyme>,</l>
        <l id="s12:l6" met="5" type="-+">Trouwen is de ziel van van van 't <rhyme label="b" type="f" corresp="#s12:l2">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
