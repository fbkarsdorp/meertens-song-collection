    <melody>Psalm 100 Datheen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Des grooten Scheppers lof klimm' <rhyme label="a" type="m">staêg</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Omhoog, van al wat d'aard' om <rhyme label="a" type="m" corresp="#s1:l1">laag</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Bewoont, ook worde Jesus <rhyme label="b" type="m">naam</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Verbreid, door alle tongen <rhyme label="b" type="m" corresp="#s1:l3">zaam</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ontferming, zonder eind' of <rhyme label="a" type="m">maat</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Is uw' ontferming, Heer! ook <rhyme label="a" type="m" corresp="#s2:l1">staat</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Uw woord, tot in alle <rhyme label="b" type="m">eeuwigheên</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Onwrikbaar vast, - uw woord <rhyme label="b" type="m" corresp="#s2:l3">alleen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Van uwen lof, ô Vader! <rhyme label="a" type="m">zal</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Eens wedergalmen 't gantsch <rhyme label="a" type="m" corresp="#s3:l1">heelal</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Tot eens die zonnen zullen <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die nimmer weder <rhyme label="b" type="m" corresp="#s3:l3">ondergaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hem, die, van 's hemels hoogen <rhyme label="a" type="m">troon</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Zyn Scepter zwaait, en zynen <rhyme label="a" type="m" corresp="#s4:l1">Zoon</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Het Lam, voor ons, - voor ons <rhyme label="b" type="m">geslacht</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Zy eeuwig eere toegeb <rhyme label="b" type="m" corresp="#s4:l3">racht</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
