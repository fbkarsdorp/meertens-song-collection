<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25096</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Laet ons sinte claren <rhyme label="a" type="f">louen</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Die gloriose <rhyme label="b" type="m">maecht</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Si regneert int rike <rhyme label="a" type="f" corresp="#s1:l1">daerbouen</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Daer si haren lieue <rhyme label="b" type="m" corresp="#s1:l2">behaecht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Si es int licht der <rhyme label="a" type="f">heilicheit</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Ende een lielye <rhyme label="b" type="m">scone</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Si blict in hoger <rhyme label="a" type="f" corresp="#s2:l1">weerdicheit</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Als een vergulden <rhyme label="b" type="m" corresp="#s2:l2">crone</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Eerse haer moeder soude <rhyme label="a" type="f">gebaren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sprac god dat si was een <rhyme label="b" type="m">licht</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Dat die werelt soude <rhyme label="a" type="f" corresp="#s3:l1">verclaren</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">In huer was sijn duecht <rhyme label="b" type="m" corresp="#s3:l2">gesticht</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">God woude haer vroude <rhyme label="a" type="f">meeren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met grooter <rhyme label="b" type="m">weerdicheit</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Si sach die crebbe ons <rhyme label="a" type="f" corresp="#s4:l1">heeren</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">In harer <rhyme label="b" type="m" corresp="#s4:l2">allindicheit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Si was zeer gemint met <rhyme label="a" type="f">trouwen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van alden roemschen <rhyme label="b" type="m">houe</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">En geeert vor alle <rhyme label="a" type="f" corresp="#s5:l1">vrouwen</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Met wonderliken <rhyme label="b" type="m" corresp="#s5:l2">loue</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Het besochse in hare <rhyme label="a" type="f">doot</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">Die vrouwe es vanden <rhyme label="b" type="m">trone</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Met haer quam een geselscap <rhyme label="a" type="f" corresp="#s6:l1">groot</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van meechden alte <rhyme label="b" type="m" corresp="#s6:l2">scone</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
