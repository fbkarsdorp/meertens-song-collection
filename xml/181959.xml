  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zwygt van Vreede blaest <rhyme label="a" type="f">trompette</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Roert de trom, maekt <rhyme label="b" type="m">Krygsgeluyt</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Wild het al in vlammen <rhyme label="a" type="f" corresp="#s1:l1">zetten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wat de drift der Helden <rhyme label="b" type="m" corresp="#s1:l2">stuyt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">In den Oorlog past een <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Nooit verzadigt dan met <rhyme label="c" type="m" corresp="#s1:l5">bloed</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Een waere Held bied <rhyme label="d" type="m">tegenstand</rhyme></l>
        <l id="s1:l8" met="4" type="+-">En word het stael in ‘t hert <rhyme label="d" type="m" corresp="#s1:l7">gepland</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m">stry..d</rhyme>/</l>
        <l id="s1:l10" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m" corresp="#s1:l9">stryd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Liefden kan ons hert niet <rhyme label="a" type="f">raken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Want wy leven in den <rhyme label="b" type="m">stryd</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Wy durven ons roemrugtig <rhyme label="a" type="f" corresp="#s2:l1">maken</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">‘t Is de vrugt der Oorlogs <rhyme label="b" type="m" corresp="#s2:l2">tyd</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">In den Oorlog past een <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Nooit verzadigt dan met <rhyme label="c" type="m" corresp="#s2:l5">bloed</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Een waere Held bied <rhyme label="d" type="m">tegenstand</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">En word het stael in ‘t hert <rhyme label="d" type="m" corresp="#s2:l7">gepland</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m">stry..d</rhyme>.</l>
        <l id="s2:l10" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m" corresp="#s2:l9">stryd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wallen sloopen, Steede <rhyme label="a" type="f">winnen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Is ons leven, is ons <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">‘t Is al bloed wat wy <rhyme label="a" type="f" corresp="#s3:l1">beminnen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ia ‘t Gevegt legt ons de <rhyme label="b" type="m" corresp="#s3:l2">lust</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">In den Oorlog past een <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Nooit verzadigt dan met <rhyme label="c" type="m" corresp="#s3:l5">bloed</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Een waere Held bied <rhyme label="d" type="m">tegenstand</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En word het stael in ‘t hert <rhyme label="d" type="m" corresp="#s3:l7">gepland</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m">stry..d</rhyme>.</l>
        <l id="s3:l10" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m" corresp="#s3:l9">stryd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Heeft de moed ons nu <rhyme label="a" type="f">begeven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ziet wy loopen nu de <rhyme label="b" type="m">Wyn</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Dikwils zien wy ons <rhyme label="a" type="f" corresp="#s4:l1">herleven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Want dat is ons <rhyme label="b" type="m" corresp="#s4:l2">Medecyn</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">In den Oorlog past een <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Nooit verzadigt dan met <rhyme label="c" type="m" corresp="#s4:l5">bloed</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Een waere Held bied <rhyme label="d" type="m">tegenstand</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En word het stael in ‘t hert <rhyme label="d" type="m" corresp="#s4:l7">gepland</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m">stry..d</rhyme>.</l>
        <l id="s4:l10" met="3" type="-+">Wy groeijen in den <rhyme label="e" type="m" corresp="#s4:l9">stryd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
