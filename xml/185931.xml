    <melody>Vervloekte ondankbaarheid die Theseus' hart regeert</melody>
  <author>Hermanus van den Burg</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">VErvloekte ondankbaarheit die Theseus hart <rhyme label="a" type="m">regeert</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">die my den nek <rhyme label="a" type="m" corresp="#s1:l1">toekeert</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Ik red hem uit de <rhyme label="b" type="m">dood</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">omhels hem in myn <rhyme label="b" type="m" corresp="#s1:l3">schoot</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">en noch ontvlugt hy <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">en spot met mynen <rhyme label="b" type="m" corresp="#s1:l3">nood</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Ach Theseus, <rhyme label="d" type="m">ach</rhyme>!</l>
        <l id="s1:l8" met="3" type="-+">myn waarde <rhyme label="b" type="m" corresp="#s1:l3">Bedgenoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
