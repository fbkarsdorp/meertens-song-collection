    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">K'Wil myn sinnen af gaen <rhyme label="a" type="f">trecken</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van die my gheen liefde <rhyme label="b" type="m">draeght</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Terwijl sy met my komt <rhyme label="a" type="f" corresp="#s1:l1">gecken</rhyme></l>
        <l id="s1:l4" met="4" type="+-">En een Spaenjaert haer <rhyme label="b" type="m" corresp="#s1:l2">behaeght</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ick gaen op een ander <rhyme label="c" type="f">preoven</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Hier loop ick een blauwe <rhyme label="d" type="m">scheen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Doch k'en wil my niet <rhyme label="c" type="f" corresp="#s1:l5">bedroeven</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Want men vinter meer dan <rhyme label="d" type="m" corresp="#s1:l6">een</rhyme>,</l>
      </lg>
    </lg>
