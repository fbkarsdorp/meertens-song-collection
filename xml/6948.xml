        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>O Angenietje</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">GEluckighe <rhyme label="a" type="f">hertjes</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Die nae veel <rhyme label="a" type="f" corresp="#s1:l1">smertjes</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Den Hemel met den <rhyme label="b" type="m">Echt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Onbreeckelijcken <rhyme label="b" type="m" corresp="#s1:l3">hecht</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Die nae veel <rhyme label="c" type="f">suchjes</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Verwacht veel <rhyme label="c" type="f" corresp="#s1:l5">kluchjes</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Veel blyschap, veel <rhyme label="d" type="m">gheneugd</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Veel suycker-soete <rhyme label="d" type="m" corresp="#s1:l7">vreugd</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Wy wenschen <rhyme label="e" type="m">dat</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Sijn zeghen-rijcke <rhyme label="e" type="m" corresp="#s1:l9">schat</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Den <rhyme label="f" type="m">opper-Heer</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Van boven <rhyme label="f" type="m" corresp="#s1:l11">neer</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Stort op u meer en <rhyme label="f" type="m" corresp="#s1:l11">meer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Siet <rhyme label="a" type="f">Cupidootje</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Die met een <rhyme label="a" type="f" corresp="#s2:l1">schootje</rhyme></l>
        <l id="s2:l3" met="3" type="-+">V zieltjes had <rhyme label="b" type="m">verplet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die wacht u in het <rhyme label="b" type="m" corresp="#s2:l3">bed</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Het kluchtighe <rhyme label="c" type="f">Boefje</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Het dertele <rhyme label="c" type="f" corresp="#s2:l5">Schroefje</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dat sit wel half en <rhyme label="d" type="m">kijft</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En weet niet waerje <rhyme label="d" type="m" corresp="#s2:l7">blijft</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">En wenscht slechts <rhyme label="e" type="m">om</rhyme></l>
        <l id="s2:l10" met="3" type="-+">De Bruyd en <rhyme label="e" type="m" corresp="#s2:l9">Bruydegom</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Die hy <rhyme label="f" type="m">verwacht</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Om wel en <rhyme label="f" type="m" corresp="#s2:l11">sacht</rhyme></l>
        <l id="s2:l13" met="3" type="-+">Te decken dese <rhyme label="f" type="m" corresp="#s2:l11">nacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Hy heeft een <rhyme label="a" type="f">koocker</rhyme></l>
        <l id="s3:l2" met="2" type="-+">De <rhyme label="a" type="f" corresp="#s3:l1">Minne-stoocker</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Vol pijltjes op sijn <rhyme label="b" type="m">lijf</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tot vriendelijck <rhyme label="b" type="m" corresp="#s3:l3">gherijf</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Dees wecken de <rhyme label="c" type="f">lusjes</rhyme></l>
        <l id="s3:l6" met="2" type="-+">En tenghere <rhyme label="c" type="f" corresp="#s3:l5">kusjes</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En alsjet met het <rhyme label="d" type="m">end</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Sich een in t'hertje <rhyme label="d" type="m" corresp="#s3:l7">prent</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Het springht, het <rhyme label="e" type="m">tocht</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Het hippelt en het <rhyme label="e" type="m" corresp="#s3:l9">klockt</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Het zieltje <rhyme label="f" type="m">teer</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Vlieght gints en <rhyme label="f" type="m" corresp="#s3:l11">weer</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Dan op en dan weer <rhyme label="f" type="m" corresp="#s3:l11">neer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">t'Komt op de <rhyme label="a" type="f">lippen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">En t'schijntje <rhyme label="a" type="f" corresp="#s4:l1">t'ontslippen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En dat het u <rhyme label="b" type="m">verlaet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En ye wers speelen <rhyme label="b" type="m" corresp="#s4:l3">gaet</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">t'Schijnt datje <rhyme label="c" type="f">beswijcken</rhyme></l>
        <l id="s4:l6" met="2" type="-+">En t'vaentje sult <rhyme label="c" type="f" corresp="#s4:l5">strijcken</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Tot het by een weer <rhyme label="d" type="m">kruypt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En in sijn hooltje <rhyme label="d" type="m" corresp="#s4:l7">sluypt</rhyme>:</l>
        <l id="s4:l9" met="2" type="-+">Dees pijltjes <rhyme label="e" type="m">al</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Het wicht verschieten <rhyme label="e" type="m" corresp="#s4:l9">sal</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Waer van met <rhyme label="f" type="m">vreugd</rhyme></l>
        <l id="s4:l12" met="2" type="-+">V hert <rhyme label="f" type="m" corresp="#s4:l11">verheugd</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Sal swemmen in <rhyme label="f" type="m" corresp="#s4:l11">gheneugd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ghy soete <rhyme label="a" type="f">beckjes</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Die met veel <rhyme label="a" type="f" corresp="#s5:l1">treckjes</rhyme></l>
        <l id="s5:l3" met="3" type="-+">V speel-ghenoot soo <rhyme label="b" type="m">stout</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Den Bruydegom <rhyme label="b" type="m" corresp="#s5:l3">onthout</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Wat laetse doch <rhyme label="c" type="f">glijen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Ghy schijnt te <rhyme label="c" type="f" corresp="#s5:l5">benijen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">V speel-noot dese <rhyme label="d" type="m">nacht</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En t'geene sy <rhyme label="d" type="m" corresp="#s5:l7">verwacht</rhyme>.</l>
        <l id="s5:l9" met="2" type="-+">t'Moet voort gaen <rhyme label="e" type="m">t'werck</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Al waert ghy noch soo <rhyme label="e" type="m" corresp="#s5:l9">sterck</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">Ghy zijt te <rhyme label="f" type="m">kranck</rhyme></l>
        <l id="s5:l12" met="2" type="-+">Om haer noch <rhyme label="f" type="m" corresp="#s5:l11">lanck</rhyme></l>
        <l id="s5:l13" met="3" type="-+">Te houden teghen <rhyme label="f" type="m" corresp="#s5:l11">danck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Soo ghy u niet <rhyme label="a" type="f">goedjes</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Wilt draghen en <rhyme label="a" type="f" corresp="#s6:l1">soetjes</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En haer nae t'bed <rhyme label="b" type="m">gheleyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Hoort dan wat Venus <rhyme label="b" type="m" corresp="#s6:l3">seyt</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Sy heeft <rhyme label="c" type="f">gheswooren</rhyme></l>
        <l id="s6:l6" met="2" type="-+">By kerck en <rhyme label="c" type="f" corresp="#s6:l5">tooren</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">By d'ooghjes van haer <rhyme label="d" type="m">Pol</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En sijn ghekruysden <rhyme label="d" type="m" corresp="#s6:l7">bol</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">By heur gheel <rhyme label="e" type="m">hayr</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Dat in een hallif <rhyme label="e" type="m" corresp="#s6:l9">jaer</rhyme></l>
        <l id="s6:l11" met="2" type="+-">Gheen van u <rhyme label="f" type="m">al</rhyme></l>
        <l id="s6:l12" met="2" type="-+">(t'Is jock noch <rhyme label="f" type="m" corresp="#s6:l11">mal</rhyme>)</l>
        <l id="s6:l13" met="3" type="-+">Een vrijer krijghen <rhyme label="f" type="m" corresp="#s6:l11">sal</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wilt ghy u <rhyme label="a" type="f">speel-meysje</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Noch kussen een <rhyme label="a" type="f" corresp="#s7:l1">reysje</rhyme></l>
        <l id="s7:l3" met="3" type="-+">In haren <rhyme label="b" type="m">maegden-staet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Eer dat ghy haer <rhyme label="b" type="m" corresp="#s7:l3">verlaet</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Soo most ghy <rhyme label="c" type="f">beginnen</rhyme></l>
        <l id="s7:l6" met="2" type="-+">En niet langh <rhyme label="c" type="f" corresp="#s7:l5">versinnen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">t'Is nu den lesten <rhyme label="d" type="m">dagh</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Dat het u beuren <rhyme label="d" type="m" corresp="#s7:l7">magh</rhyme>.</l>
        <l id="s7:l9" met="2" type="-+">Gheluckigh <rhyme label="e" type="m">Paer</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Gaet henen met <rhyme label="e" type="m" corresp="#s7:l9">malkaer</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">Den Hemel <rhyme label="f" type="m">goed</rhyme></l>
        <l id="s7:l12" met="2" type="-+">Geev' u veel <rhyme label="f" type="m" corresp="#s7:l11">soet</rhyme></l>
        <l id="s7:l13" met="3" type="-+">En heyl in <rhyme label="f" type="m" corresp="#s7:l11">overvloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
