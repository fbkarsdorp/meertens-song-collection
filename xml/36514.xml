  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs ick g'waer word' de <rhyme label="a" type="f">Tempeesten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van de werelt, en het <rhyme label="b" type="m">Vlees</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Oock 't gewelt der boose <rhyme label="a" type="f" corresp="#s1:l1">Geesten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die my jagen aen de <rhyme label="b" type="m" corresp="#s1:l2">vrées</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Ick roep mynen Engel <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Heer bewaert ons, wy <rhyme label="c" type="m" corresp="#s1:l5">vergaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ick belove my te <rhyme label="a" type="f">voegen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Naer u inspraeck, ende <rhyme label="b" type="m">raet</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">En te doen naer u <rhyme label="a" type="f" corresp="#s2:l1">genoegen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Alsoo langh dit leven <rhyme label="b" type="m" corresp="#s2:l2">staet</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Blyft gy mynen <rhyme label="c" type="m">Pedagoogh</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ende hecht op my u <rhyme label="c" type="m" corresp="#s2:l5">oogh</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Blyft toch aen myn rechte <rhyme label="a" type="f">zyden</rhyme>;</l>
        <l id="s3:l2" met="4" type="+-">Alwaer ick gaen, gaet oock <rhyme label="b" type="m">me</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">En wilt my <rhyme label="a" type="f" corresp="#s3:l1">gebenedyden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Als den Ander Jacob <rhyme label="b" type="m" corresp="#s3:l2">dé</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">De g'heel helle tegen <rhyme label="c" type="m">my</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Opstae: Ick ben wel met <rhyme label="c" type="m" corresp="#s3:l5">dy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als gy met my syt, geen <rhyme label="a" type="f">Beren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Hoe fel oock, en hong'righ <rhyme label="b" type="m">syn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Geen Leeuwen en mogen <rhyme label="a" type="f" corresp="#s4:l1">deren</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Ja self't vier en doet geen <rhyme label="b" type="m" corresp="#s4:l2">pyn</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Exempel 't kindt <rhyme label="c" type="m">Misael</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Met noch twee, en <rhyme label="c" type="m" corresp="#s4:l5">Daniel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gy deckt my gelyck een <rhyme label="a" type="f">hernes</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Tegen alle <rhyme label="b" type="m">tegenspoedt</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Door welck Judith <rhyme label="a" type="f" corresp="#s5:l1">Holofernes</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Ded' versmooren in zyn <rhyme label="b" type="m" corresp="#s5:l2">bloedt</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Ick en vreese, of en <rhyme label="c" type="m">dib</rhyme></l>
        <l id="s5:l6" met="4" type="+-">By u niet voor <rhyme label="c" type="m" corresp="#s5:l5">Sen'cherib</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wie heeft Tobias de <rhyme label="a" type="f">Jongen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Van den gapend' Visch <rhyme label="b" type="m">verricht</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Die op hem quamp <rhyme label="a" type="f" corresp="#s6:l1">aengesprongen</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">En den Ouden het <rhyme label="b" type="m" corresp="#s6:l2">gesicht</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Hergunt? wie, als <rhyme label="c" type="m">Raphaël</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Die oock Sara dede <rhyme label="c" type="m" corresp="#s6:l5">wel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Syt gy om my te <rhyme label="a" type="f">bewaren</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Van den eersten <rhyme label="b" type="m">oogeblick</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die my bystaet tot dees <rhyme label="a" type="f" corresp="#s7:l1">jaren</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Blyft dan tot de leste <rhyme label="b" type="m" corresp="#s7:l2">snick</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">En ontreckt my niet u <rhyme label="c" type="m">handt</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Voor g'en brenght m' in 't <rhyme label="c" type="m" corresp="#s7:l5">vaderlandt</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
