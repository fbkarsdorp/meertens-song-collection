<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8752</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="afscheidslied" translation="farewell song">afscheidslied</tag>
    </tags>
    <melody>Adieu Haarlem Soetendal</melody>
    <categories>
      <category translation="occasions">gelegenheden</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ADieu Haerlem <rhyme label="a" type="m">Soetendal</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met al u schoon <rhyme label="b" type="f">contreyen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Adieu Vrienden en Magen <rhyme label="a" type="m" corresp="#s1:l1">al</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Mijn maers ick niet vergeten en <rhyme label="a" type="m" corresp="#s1:l1">sal</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer ick mee ginck <rhyme label="b" type="f" corresp="#s1:l2">vermeyen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Adieu Andries de Bosscher <rhyme label="a" type="m">net</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Abraham Leenaerts <rhyme label="b" type="f">mede</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Pieter Ioris daer by <rhyme label="a" type="m" corresp="#s2:l1">geset</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ian Vermeers en Gillis de <rhyme label="a" type="m" corresp="#s2:l1">Smet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Often Meuwels soeckt <rhyme label="b" type="f" corresp="#s2:l2">vrede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick bedanck u van harten <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Mijn lieve maets <rhyme label="b" type="f">gepresen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Van goet geselschap 't allen <rhyme label="a" type="m" corresp="#s3:l1">keer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En van u-lieder deught en <rhyme label="a" type="m" corresp="#s3:l1">eer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die ghy my hebt <rhyme label="b" type="f" corresp="#s3:l2">bewesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Adieu Ionckmans en Dochters <rhyme label="a" type="m">mee</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Tot dat wy weer <rhyme label="b" type="f">vergaren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Nu treck ick Godes <rhyme label="a" type="m" corresp="#s4:l1">gelee</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Dat mach my deeren op de <rhyme label="a" type="m" corresp="#s4:l1">zee</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Als Godt my wil <rhyme label="b" type="f" corresp="#s4:l2">bewaren</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Adieu Overveen en <rhyme label="a" type="m">Santvoort</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wijck-op-zee daer <rhyme label="b" type="f">beneden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Heems-kerck, Beverwijck, soo't <rhyme label="a" type="m" corresp="#s5:l1">behoort</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Velsen, Schooten, en de <rhyme label="a" type="m" corresp="#s5:l1">Sant-poort</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Kort-rijck wil ick <rhyme label="b" type="f" corresp="#s5:l2">begeven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Adieu Brero, Blinck-duynen <rhyme label="a" type="m">wijt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Aelberts-bergh, <rhyme label="b" type="f">Bloemendale</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Klever Laen, Passchier <rhyme label="a" type="m" corresp="#s6:l1">Petijt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">'tHuys te Kleef, en 'tHuys te <rhyme label="a" type="m" corresp="#s6:l1">spijt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">'tRoot-huys, Siecken vol <rhyme label="b" type="f" corresp="#s6:l2">qualen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Adieu Sparendam Pan-ael <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Sparwouwer Dijck seer <rhyme label="b" type="f">hooge</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Pennings-veer, Rusten-burg <rhyme label="a" type="m" corresp="#s7:l1">voorwaer</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Adieu Schalckwijck en <rhyme label="a" type="m" corresp="#s7:l1">Schollenaer</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Daermen mach gaen in't <rhyme label="b" type="f" corresp="#s7:l2">drooge</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Adieu Lis, Hillegom, <rhyme label="a" type="m">Heemstee</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">'tSlot, Kraeynest, <rhyme label="b" type="f">Berckenroode</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Nuyssenburgh, Aerdenhout, Baen <rhyme label="a" type="m" corresp="#s8:l1">mee</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En d'Hont daermen spanceert in <rhyme label="a" type="m" corresp="#s8:l1">vree</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Adieu ick laet u <rhyme label="b" type="f" corresp="#s8:l2">noode</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Adieu Liefd' die Trou blycken <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Adieu <rhyme label="b" type="f">Pellicanisten</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Adieu Liefd' boven al weest <rhyme label="a" type="m" corresp="#s9:l1">vroet</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Adieu Edel Vvynranxkens <rhyme label="a" type="m" corresp="#s9:l1">soet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Leeft vrolijck sonder <rhyme label="b" type="f" corresp="#s9:l2">twisten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Oorelof Princelijck <rhyme label="a" type="m">Princier</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Verstroyt uyt u <rhyme label="b" type="f">Landouwe</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Cantica Bruyd reyn van <rhyme label="a" type="m" corresp="#s10:l1">manier</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Gy die schuld onder't VVit <rhyme label="a" type="m" corresp="#s10:l1">Angier</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Blijft in Liefde <rhyme label="b" type="f" corresp="#s10:l2">Getrouwe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
