<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3684</id>
    <date>
      <year-start>1566</year-start>
      <year-end>1566</year-end>
    </date>
    <location>
      <orginal-name>heidelberg</orginal-name>
      <standard-name>Heidelberg, Germany</standard-name>
      <coordinates>
        <lon>49.4093582</lon>
        <lat>8.694724</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 081 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SINGHT den Heere <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Die ons sterckt' is <rhyme label="b" type="f">krachtigh</rhyme>.</l>
        <l id="s1:l3" met="3" type="+-">Verhueght v oock <rhyme label="a" type="m" corresp="#s1:l1">vry</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">In Godt <rhyme label="c" type="m">Israel</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Bewyst hem met <rhyme label="c" type="m" corresp="#s1:l4">spel</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Prys end' eer' <rhyme label="b" type="f" corresp="#s1:l2">aendachtigh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Singht Psalmen seer <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">End lieflick om <rhyme label="b" type="f">hooren</rhyme>.</l>
        <l id="s2:l3" met="3" type="+-">Die Tamboeren <rhyme label="a" type="m" corresp="#s2:l1">soet</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Harpen <rhyme label="c" type="m">bequaeme</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">End Psalters <rhyme label="c" type="m" corresp="#s2:l4">t'saeme</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Wilt met vrueghdt <rhyme label="b" type="f" corresp="#s2:l2">oorbooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Op de nieuwe <rhyme label="a" type="m">Maen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Met basuynen <rhyme label="b" type="f">singhet</rhyme>.</l>
        <l id="s3:l3" met="3" type="+-">End vroylick voort <rhyme label="a" type="m" corresp="#s3:l1">aen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Der <rhyme label="c" type="m">loouertenten</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Iaerlicksche <rhyme label="c" type="m" corresp="#s3:l4">renten</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Betaelt end <rhyme label="b" type="f" corresp="#s3:l2">volbringhet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Dit's in <rhyme label="a" type="m">Israel</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Een ghebruyck <rhyme label="b" type="f">ghewesen</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Godt end niemant <rhyme label="a" type="m" corresp="#s4:l1">el</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Dit beuolen <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">End tot teecken <rhyme label="c" type="m" corresp="#s4:l4">gheeft</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Sijns bondts hoogh' <rhyme label="b" type="f" corresp="#s4:l2">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Sulcks heeft hy <rhyme label="a" type="m">ghedaen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Als t'volck wt den <rhyme label="b" type="f">landen</rhyme></l>
        <l id="s5:l3" met="3" type="+-">AEgypti <rhyme label="a" type="m" corresp="#s5:l1">ghegaen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Is: Daer't die <rhyme label="c" type="m">taelen</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Hoorde <rhyme label="c" type="m" corresp="#s5:l4">verhaelen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Die't niet heeft <rhyme label="b" type="f" corresp="#s5:l2">verstanden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Haer lasten <rhyme label="a" type="m">aldaer</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Heeft hy <rhyme label="b" type="f">weghghenomen</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Van den potten <rhyme label="a" type="m" corresp="#s6:l1">swaer</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Lastigh om <rhyme label="c" type="m">draeghen</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Sijn se <rhyme label="c" type="m" corresp="#s6:l4">ontslaeghen</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">End gantsch vry <rhyme label="b" type="f" corresp="#s6:l2">ghekomen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Ghy riept my daer <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">End ick in v' <rhyme label="b" type="f">lyden</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Hebb' v <rhyme label="a" type="m" corresp="#s7:l1">byghestaen</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">End verhoort in <rhyme label="c" type="m">noot</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Als de donder <rhyme label="c" type="m" corresp="#s7:l4">groot</rhyme></l>
        <l id="s7:l6" met="3" type="+-">My deckte ter <rhyme label="b" type="f" corresp="#s7:l2">zyden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Ick proefd' v <rhyme label="a" type="m">voorwaer</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Aen't twistwater <rhyme label="b" type="f">klachtigh</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">End vandt v <rhyme label="a" type="m" corresp="#s8:l1">daernaer</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Verherdt van <rhyme label="c" type="m">sinnen</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Doch ick wt <rhyme label="c" type="m" corresp="#s8:l4">minnen</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Sprack v aen <rhyme label="b" type="f" corresp="#s8:l2">eendrachtigh</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Myn volck my doch <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Myn bondt sal ick <rhyme label="b" type="f">maeken</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Met v van nu <rhyme label="a" type="m" corresp="#s9:l1">voort</rhyme>.</l>
        <l id="s9:l4" met="3" type="+-">Wilt my doch <rhyme label="c" type="m">hooren</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Opent v' <rhyme label="c" type="m" corresp="#s9:l4">ooren</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">End wilt myn woort <rhyme label="b" type="f" corresp="#s9:l2">smaecken</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Maeckt v myn volck <rhyme label="a" type="m">vroet</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Gheenderly <rhyme label="b" type="f">afgoden</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Gheenen dienst hen <rhyme label="a" type="m" corresp="#s10:l1">doet</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Wiltse niet <rhyme label="c" type="m">eeren</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Noch tot hem <rhyme label="c" type="m" corresp="#s10:l4">keeren</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Want ick heb't <rhyme label="b" type="f" corresp="#s10:l2">verboden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Want ick ben v' <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">Eewigh end <rhyme label="b" type="f">almachtigh</rhyme>:</l>
        <l id="s11:l3" met="3" type="+-">Dit landt tot v' <rhyme label="a" type="m" corresp="#s11:l1">lot</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Ghaf ick v <rhyme label="c" type="m">goedigh</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">End track v <rhyme label="c" type="m" corresp="#s11:l4">spoedigh</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">Wt AEgypten <rhyme label="b" type="f" corresp="#s11:l2">krachtigh</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Opent uwen <rhyme label="a" type="m">mondt</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Seer wydt <rhyme label="b" type="f">onbelaeden</rhyme></l>
        <l id="s12:l3" met="3" type="+-">Ick sal hem <rhyme label="a" type="m" corresp="#s12:l1">terstondt</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">Met goeder <rhyme label="c" type="m">spyse</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Nae myne <rhyme label="c" type="m" corresp="#s12:l4">wyse</rhyme>,</l>
        <l id="s12:l6" met="3" type="+-">Ryckelick <rhyme label="b" type="f" corresp="#s12:l2">versaeden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">T'volck dat ick <rhyme label="a" type="m">verkoos</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Is van t'woort <rhyme label="b" type="f">gheweken</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">Verstockt is't end <rhyme label="a" type="m" corresp="#s13:l1">boos</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">Ick heb't <rhyme label="c" type="m">ghebeden</rhyme>,</l>
        <l id="s13:l5" met="3" type="+-">T'heeft doch myn <rhyme label="c" type="m" corresp="#s13:l4">reden</rhyme>,</l>
        <l id="s13:l6" met="3" type="+-">Gantschelick <rhyme label="b" type="f" corresp="#s13:l2">versteken</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">Ick in <rhyme label="a" type="m">toornicheit</rhyme>,</l>
        <l id="s14:l2" met="3" type="+-">Ghast ouer met <rhyme label="b" type="f">allen</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">Sijner <rhyme label="a" type="m" corresp="#s14:l1">verstocktheit</rhyme>,</l>
        <l id="s14:l4" met="3" type="+-">Om self sijn <rhyme label="c" type="m">saeken</rhyme>,</l>
        <l id="s14:l5" met="3" type="+-">Voortaen te <rhyme label="c" type="m" corresp="#s14:l4">maeken</rhyme>,</l>
        <l id="s14:l6" met="3" type="+-">Nae sijn <rhyme label="b" type="f" corresp="#s14:l2">welgheuallen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="+-">Och of t'volck <rhyme label="a" type="m">rebel</rhyme>,</l>
        <l id="s15:l2" met="3" type="+-">My ghehoorsaem <rhyme label="b" type="f">waere</rhyme>,</l>
        <l id="s15:l3" met="3" type="+-">End dat <rhyme label="a" type="m" corresp="#s15:l1">Israel</rhyme>,</l>
        <l id="s15:l4" met="3" type="+-">Waere <rhyme label="c" type="m">ghebleuen</rhyme>,</l>
        <l id="s15:l5" met="3" type="+-">Op den padt <rhyme label="c" type="m" corresp="#s15:l4">euen</rhyme>,</l>
        <l id="s15:l6" met="3" type="+-">Vast in't <rhyme label="b" type="f" corresp="#s15:l2">openbaere</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="+-">Ick soud' haest <rhyme label="a" type="m">verdaen</rhyme></l>
        <l id="s16:l2" met="3" type="+-">Hebben sijn <rhyme label="b" type="f">vianden</rhyme>,</l>
        <l id="s16:l3" met="3" type="+-">Myn handt sterckt in't <rhyme label="a" type="m" corresp="#s16:l1">slaen</rhyme></l>
        <l id="s16:l4" met="3" type="+-">Hadd' bouen <rhyme label="c" type="m">maeten</rhyme></l>
        <l id="s16:l5" met="3" type="+-">Haest die hem <rhyme label="c" type="m" corresp="#s16:l4">haeten</rhyme>,</l>
        <l id="s16:l6" met="3" type="+-">All' ghebracht ter <rhyme label="b" type="f" corresp="#s16:l2">schande</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="+-">Sijn vianden <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s17:l2" met="3" type="+-">Souden druck <rhyme label="b" type="f">bedryuen</rhyme>,</l>
        <l id="s17:l3" met="3" type="+-">End komen ten <rhyme label="a" type="m" corresp="#s17:l1">val</rhyme>,</l>
        <l id="s17:l4" met="3" type="+-">Dat se al <rhyme label="c" type="m">t'saemen</rhyme>,</l>
        <l id="s17:l5" met="3" type="+-">Souden met <rhyme label="c" type="m" corresp="#s17:l4">blaemen</rhyme>,</l>
        <l id="s17:l6" met="3" type="+-">Eewigh verdruckt <rhyme label="b" type="f" corresp="#s17:l2">blyuen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="+-">Taerw' in <rhyme label="a" type="m">ouervloet</rhyme>,</l>
        <l id="s18:l2" met="3" type="+-">Hadd' ick hen <rhyme label="b" type="f">ghegheuen</rhyme>,</l>
        <l id="s18:l3" met="3" type="+-">End met honigh <rhyme label="a" type="m" corresp="#s18:l1">soet</rhyme>,</l>
        <l id="s18:l4" met="3" type="+-">T'welck vloeyt wt <rhyme label="c" type="m">steenen</rhyme>,</l>
        <l id="s18:l5" met="3" type="+-">Ghespyst met <rhyme label="c" type="m" corresp="#s18:l4">eenen</rhyme>,</l>
        <l id="s18:l6" met="3" type="+-">Hadd' icks' al myn <rhyme label="b" type="f" corresp="#s18:l2">leuen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
