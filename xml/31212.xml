  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wa vrauwen schone. inde vrauwen <rhyme label="a" type="f">guode</rhyme>.</l>
        <l id="s1:l2" met="5" type="+-">suoze inder wol gemuode. minne <rhyme label="b" type="f">leve</rhyme></l>
        <l id="s1:l3" met="4" type="+-">vrauwe. mase da bis bi inde <rhyme label="a" type="f" corresp="#s1:l1">huode</rhyme>.</l>
        <l id="s1:l4" met="5" type="+-">dat si durch lieve ir spil iet over <rhyme label="b" type="f" corresp="#s1:l2">geve</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">wie sere vrauwen schone <rhyme label="c" type="f">tuingit</rhyme></l>
        <l id="s1:l6" met="4" type="-+">ist reyne muot der schone <rhyme label="d" type="m">bi</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">wen ir schone dan in kumber <rhyme label="c" type="f" corresp="#s1:l5">bringit</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">da is die reine schuldin <rhyme label="d" type="m" corresp="#s1:l6">vri</rhyme>.</l>
      </lg>
    </lg>
