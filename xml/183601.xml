  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOe gelukkig is de Lantman <rhyme label="a" type="m">gewis</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Daer de Zeeman een slaef tegen <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Storm, Onweer, naere <rhyme label="b" type="f">Nagte</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Moet een Zeeman al <rhyme label="b" type="f" corresp="#s1:l3">verwagte</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Daer de Lantman lyt stil en <rhyme label="c" type="m">gerust</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Met vreede sijn Fielida <rhyme label="c" type="m" corresp="#s1:l5">kust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Style klippe droogte en <rhyme label="a" type="m">sant</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Verborge laegen agter <rhyme label="a" type="m" corresp="#s2:l1">lant</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Style klippen, swaere <rhyme label="b" type="f">brande</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Bevreest voor meenig <rhyme label="b" type="f" corresp="#s2:l3">strande</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Al ongelukke te wagte <rhyme label="c" type="m">staat</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Gelukkig Lantman in de hoogste <rhyme label="c" type="m" corresp="#s2:l5">graet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Breekter dan een staeg of een <rhyme label="a" type="m">schoot</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dan is de zeeman in groote <rhyme label="a" type="m" corresp="#s3:l1">noot</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">Heemel wat <rhyme label="b" type="f">ongelukke</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Steng en Maste raeke <rhyme label="b" type="f" corresp="#s3:l3">stukke</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Dan gaet het Scheepje te <rhyme label="c" type="m">gront</rhyme></l>
        <l id="s3:l6" met="4" type="-+">dan siet men sijn laetste <rhyme label="c" type="m" corresp="#s3:l5">stond</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wel Lantman hoe dus soo <rhyme label="a" type="m">stout</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Daer de zeeman het lant op <rhyme label="a" type="m" corresp="#s4:l1">hout</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Kon gy lantman wel <rhyme label="b" type="f">bestaene</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Moest het niet te gronde <rhyme label="b" type="f" corresp="#s4:l3">gaene</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Brogt de zeeman geen schatte <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Van Oost en Westen en <rhyme label="c" type="m" corresp="#s4:l5">Noord-zee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Goude Perele <rhyme label="a" type="m">Dyamant</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Komt alles uyt het Injes <rhyme label="a" type="m" corresp="#s5:l1">lant</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Fyne Sisse en <rhyme label="b" type="f">Katoene</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Om te houden sijn <rhyme label="b" type="f" corresp="#s5:l3">Fassoene</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">'s Lands Welvaere tot een <rhyme label="c" type="m">stut</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Den Arrebyder ja selver tot <rhyme label="c" type="m" corresp="#s5:l5">nut</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wel landman en pogt tog geen <rhyme label="a" type="m">meer</rhyme>!</l>
        <l id="s6:l2" met="4" type="-+">Geeft de zeeman alleenig de <rhyme label="a" type="m" corresp="#s6:l1">eer</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Gout, zilver dierbaere <rhyme label="b" type="f">Waere</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Moet een zeeman al om <rhyme label="b" type="f" corresp="#s6:l3">vaere</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Soo lang 't de zeeman gaet <rhyme label="c" type="m">goed</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Hoe meerder gy Lantman werd <rhyme label="c" type="m" corresp="#s6:l5">gevoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
