  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEer Jesus heeft <rhyme label="a" type="m">geseyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tot sommigh van de <rhyme label="b" type="f">scharen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die uyt <rhyme label="a" type="m" corresp="#s1:l1">laetdunkentheyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">By haer rechtvaerdigh <rhyme label="b" type="f" corresp="#s1:l2">waren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dit seer bequaem <rhyme label="c" type="f">exempel</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Daer sijn geweest eens <rhyme label="d" type="m">twee</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die gingen in Goodts <rhyme label="c" type="f" corresp="#s1:l5">tempel</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Om te doen haer <rhyme label="d" type="m" corresp="#s1:l6">gebeë</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">d' Een was een <rhyme label="a" type="m">Publicaen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">d' Ander een <rhyme label="b" type="f">Phariseë</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dees heeft recht op <rhyme label="a" type="m" corresp="#s2:l1">gestaen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En heeft alsoo <rhyme label="b" type="f" corresp="#s2:l2">gebeë</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">O Godt, ik u dank <rhyme label="c" type="f">biede</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ende voor u <rhyme label="d" type="m">beken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat ik de ander <rhyme label="c" type="f" corresp="#s2:l5">liede</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Geensins gelijk en <rhyme label="d" type="m" corresp="#s2:l6">ben</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sommige doen <rhyme label="a" type="m">onrecht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sommige loopen <rhyme label="b" type="f">rooven</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Sommige breeken <rhyme label="a" type="m" corresp="#s3:l1">eght</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ook ben ik daer en <rhyme label="b" type="f" corresp="#s3:l2">booven</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Niet van soo sondigh <rhyme label="c" type="f">leeve</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Als deesen <rhyme label="d" type="m">Publicaen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En ik de tiende <rhyme label="c" type="f" corresp="#s3:l5">geeve</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van al dat mijn gaet <rhyme label="d" type="m" corresp="#s3:l6">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'k Vast twee-mael in de <rhyme label="a" type="m">week</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Den Publicaen dien <rhyme label="b" type="f">tijde</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Stont eenen grooten <rhyme label="a" type="m" corresp="#s4:l1">streek</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Aghterlijk aen een <rhyme label="b" type="f" corresp="#s4:l2">sijde</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ende die niet en <rhyme label="c" type="f">dorste</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sijn oogen opwaerts <rhyme label="d" type="m">slaen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Hy sloegh voor sijne <rhyme label="c" type="f" corresp="#s4:l5">borste</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Met groot ootmoet <rhyme label="d" type="m" corresp="#s4:l6">bevaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hy seyd', Godt doet <rhyme label="a" type="m">genaed'</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Over mijn arm <rhyme label="b" type="f">sondare</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Ik segh u, deesen <rhyme label="a" type="m" corresp="#s5:l1">gaet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Reghtvaerdiger van <rhyme label="b" type="f" corresp="#s5:l2">dare</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Al die wil sijn <rhyme label="c" type="f">verheeven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die worden sal <rhyme label="d" type="m">verneert</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Die hem wil laegh <rhyme label="c" type="f" corresp="#s5:l5">begeeven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Sal worden hoogh <rhyme label="d" type="m" corresp="#s5:l6">ge-eert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
