  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Jch reitt ein mall <rhyme label="a" type="f">spatzerenn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">d rch einenn gr nenn <rhyme label="b" type="m">waltt</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">Jch fandt ein schone J <rhyme label="c" type="f">nckfrawe</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Jren bolenn woltt sei scha <rhyme label="c" type="f" corresp="#s1:l3">wenn</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">nach rosenn so woltt sei <rhyme label="d" type="m">gahn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Jr hendlein brechen <rhyme label="a" type="f">rosen</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">ir m ndtlein dr cket <rhyme label="b" type="m">mein</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">N  sag mir g iter <rhyme label="c" type="f">geselle</rhyme></l>
        <l id="s2:l4" met="3" type="-+">wie stan die rosenn jm <rhyme label="c" type="f" corresp="#s2:l3">felde</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Vnnd wan sein sei brechens <rhyme label="d" type="m">zeitt</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Brech ich  sei an dem <rhyme label="a" type="f">abentt</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">so seindt sei farbenn <rhyme label="b" type="m">loeß</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">brech ich sei dan ahn denn <rhyme label="c" type="f">morgen</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">ein ander hatt sei <rhyme label="c" type="f" corresp="#s3:l3">erworbenn</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Den schaden den moeß ich <rhyme label="d" type="m">han</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die rosen sall man <rhyme label="a" type="f">brechen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Z r mitter halber <rhyme label="b" type="m">nachtt</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">So stahenn die Rosenn / Jm <rhyme label="c" type="f">bladenn</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">mitt kolenn thaw <rhyme label="c" type="f" corresp="#s4:l3">beladenn</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">so sein sei brechens <rhyme label="d" type="m">zeitt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wola ff ir schone <rhyme label="a" type="f">Junckfrawenn</rhyme> /</l>
        <l id="s5:l2" met="3" type="-+">die ir z m dantze <rhyme label="b" type="m">gahett</rhyme> /</l>
        <l id="s5:l3" met="3" type="-+">Jr le chtett also <rhyme label="c" type="f">felle</rhyme> /</l>
        <l id="s5:l4" met="3" type="-+">gleich wie die morgenn <rhyme label="c" type="f" corresp="#s5:l3">sterne</rhyme> /</l>
        <l id="s5:l5" met="3" type="-+">der vor dem tagh a <rhyme label="d" type="m">ffgeitt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Jch sch ß nach einer Da <rhyme label="a" type="f">benn</rhyme> /</l>
        <l id="s6:l2" met="3" type="-+">die mir entflogenn <rhyme label="b" type="m">iß</rhyme> /</l>
        <l id="s6:l3" met="3" type="-+">Sie fla gh a ff ghenem ba <rhyme label="c" type="f">me</rhyme> /</l>
        <l id="s6:l4" met="3" type="-+">Der ba me bloett so <rhyme label="c" type="f" corresp="#s6:l3">schone</rhyme> /</l>
        <l id="s6:l5" met="3" type="-+">bo en allenn  ba <rhyme label="d" type="m">melein</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Eß ist sich nitt ein Da <rhyme label="a" type="f">be</rhyme></l>
        <l id="s7:l2" met="3" type="-+">die mir entpflogen <rhyme label="b" type="m">ist</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ist  mein bolen g <rhyme label="c" type="f">ette</rhyme> /</l>
        <l id="s7:l4" met="3" type="-+">nach ir steitt mein <rhyme label="c" type="f" corresp="#s7:l3">vermogenn</rhyme> /</l>
        <l id="s7:l5" met="3" type="-+">nach ir steitt all mein <rhyme label="d" type="m">Syn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Deiß lyedtt hab ich ges <rhyme label="a" type="f">ngenn</rhyme> /</l>
        <l id="s8:l2" met="3" type="-+">So woll als ich es <rhyme label="b" type="m">kan</rhyme> /</l>
        <l id="s8:l3" met="3" type="-+">So iemantz eß nitt <rhyme label="c" type="f">behagett</rhyme> /</l>
        <l id="s8:l4" met="3" type="-+">Der stahe a ff drei st ndte f r dem <rhyme label="c" type="f" corresp="#s8:l3">taghe</rhyme> /</l>
        <l id="s8:l5" met="3" type="-+">vnnd heb ein bessers <rhyme label="d" type="m">ahn</rhyme>.</l>
      </lg>
