  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Mistroostich <rhyme label="a" type="m">desolaet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Moet ick eylaes <rhyme label="b" type="f">doleren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van sinnen <rhyme label="a" type="m" corresp="#s1:l1">desperaet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Myn jonger hart <rhyme label="b" type="f" corresp="#s1:l2">vexeren</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Om v reyn maecht <rhyme label="c" type="m">eerbaer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Als Tantalus <rhyme label="c" type="m" corresp="#s1:l5">voorwaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Och blyft niet <rhyme label="a" type="m">obstinaet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wilt my doch eens <rhyme label="b" type="f">verhooren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ghy sult coomen te <rhyme label="a" type="m" corresp="#s2:l1">laet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Als ick door pyn moet <rhyme label="b" type="f" corresp="#s2:l2">smooren</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Laet my voor arbeyts <rhyme label="c" type="m">loon</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Vercrygen v <rhyme label="c" type="m" corresp="#s2:l5">persoon</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick sal den helt <rhyme label="a" type="m">onvroom</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Thesea niet <rhyme label="b" type="f">slachten</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die Ariadne <rhyme label="a" type="m" corresp="#s3:l1">schoon</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Syn lieff heeft gaen <rhyme label="b" type="f" corresp="#s3:l2">verachten</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Maer sal sonder aff <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">V trouwelyck by <rhyme label="c" type="m" corresp="#s3:l5">staen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Myn hert ,sin , en <rhyme label="a" type="m">gemoet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hebt ghy geheel <rhyme label="b" type="f">gevangen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">In vrees , hoop ende <rhyme label="a" type="m" corresp="#s4:l1">ootmoet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Naer v staet myn <rhyme label="b" type="f" corresp="#s4:l2">verlangen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Den doot sal soeter <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dan tleuen sonder <rhyme label="c" type="m" corresp="#s4:l5">dyn</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Door Venus starcker <rhyme label="a" type="m">bant</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die my vast heeft <rhyme label="b" type="f">gebonden</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Soo gryp ick aen v <rhyme label="a" type="m" corresp="#s5:l1">hant</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Gelyck den heel <rhyme label="b" type="f" corresp="#s5:l2">duerwonden</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Die sonder v <rhyme label="c" type="m">bystant</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Moet versmooren int <rhyme label="c" type="m" corresp="#s5:l5">sant</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Princesse <rhyme label="a" type="m">goedertier</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ghy alleen moecht myn <rhyme label="b" type="f">helpen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">V lieffelyck <rhyme label="a" type="m" corresp="#s6:l1">bestier</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van myn lyden haest <rhyme label="b" type="f" corresp="#s6:l2">stelpen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Indient tIae woort mach <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Myn druck is haest <rhyme label="c" type="m" corresp="#s6:l5">gedaen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
