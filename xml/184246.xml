  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'s Morgens als hy my kwam <rhyme label="a" type="f">wekken</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met 't krieken van den <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Kwam hy aan myn Vengster <rhyme label="a" type="f" corresp="#s1:l1">trekken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Toen ik in myn rust nog <rhyme label="b" type="m" corresp="#s1:l2">lag</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">En ik zei myn Lief <rhyme label="c" type="f">geprezen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Opent toch uw Deur voor <rhyme label="d" type="m">myn</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Want den dag is reeds <rhyme label="c" type="f" corresp="#s1:l5">gereezen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Spoeit u ook dan maar by <rhyme label="d" type="m" corresp="#s1:l6">myn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Myn lief, myn <rhyme label="a" type="f">uitverkooren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Sprak zy met bevreest <rhyme label="b" type="m">geluid</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Laat 'n van geen ander <rhyme label="a" type="f" corresp="#s2:l1">bekooren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Treed nader tot uw <rhyme label="b" type="m" corresp="#s2:l2">Bruid</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Ach! hoe lieflyk is het <rhyme label="c" type="f">minnen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">In de vroege <rhyme label="d" type="m">Morgenstond</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Daar hoord men 't Gevogelte <rhyme label="c" type="f" corresp="#s2:l5">zingen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Op den Aardbol in het <rhyme label="d" type="m" corresp="#s2:l6">rond</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Myn overschoon <rhyme label="a" type="f">Annette</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sprak myne <rhyme label="b" type="m">Bruidegom</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wilt deez' Roos op myn Boezem <rhyme label="a" type="f" corresp="#s3:l1">zette</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ik omhels u <rhyme label="b" type="m" corresp="#s3:l2">wederom</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En zoo smaake wy met <rhyme label="c" type="f">malkander</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dees Lieffelyke <rhyme label="d" type="m">geur</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Zo trouwt Annette met <rhyme label="c" type="f" corresp="#s3:l5">Leander</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">In 't eelste van haar <rhyme label="d" type="m" corresp="#s3:l6">fleur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Geluk gy <rhyme label="a" type="f">overschoone</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wel leeft te saam in <rhyme label="b" type="m">vreê</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wil beide uw liefde <rhyme label="a" type="f" corresp="#s4:l1">toonen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat is myn wensch en <rhyme label="b" type="m" corresp="#s4:l2">beê</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Wilt Huwelyks-liefde <rhyme label="c" type="f">smaken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Op het zagte Deere <rhyme label="d" type="m">Bed</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Daar schoone Telgen <rhyme label="c" type="f" corresp="#s4:l5">maken</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En leven <rhyme label="d" type="m" corresp="#s4:l6">onbesmet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Speelt nu op Bas en <rhyme label="a" type="f">Fluiten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ter eeren van dit <rhyme label="b" type="m">Paer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En wilt de vreugd <rhyme label="a" type="f" corresp="#s5:l1">besluiten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met 't klinken van de <rhyme label="b" type="m" corresp="#s5:l2">snaar</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Op de Bruiloft van <rhyme label="c" type="f">Annette</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Met Leander haare <rhyme label="d" type="m">Vrind</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Wilt de vreugd geen paale <rhyme label="c" type="f" corresp="#s5:l5">zetten</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">'t Is hier liefde wat men <rhyme label="d" type="m" corresp="#s5:l6">vind</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
