  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">COmt laat ons vermaken met singhen en <rhyme label="a" type="f">spelen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dees Nieuw'-gehoude de Bruygom en <rhyme label="b" type="m">Bruyt</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Met lonkjes en dronkjes wat kusjes om <rhyme label="a" type="f" corresp="#s1:l1">delen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Soo dryvet den waker den slaper <rhyme label="b" type="m" corresp="#s1:l2">nyt</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Want weldoen ende vrolyk <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Dat wort van Davids-Soon <rhyme label="c" type="f" corresp="#s1:l5">ghepresen</rhyme>.</l>
      </lg>
    </lg>
  </text>
