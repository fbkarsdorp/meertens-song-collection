<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188752</id>
    <date>
      <year-start>1690</year-start>
      <year-end>1690</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Barent Pietersz. Kompas</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAt is 'er soeter <rhyme label="a" type="m">soet</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Wat beter voor 't <rhyme label="a" type="m" corresp="#s1:l1">gemoedt</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Als dat men met de <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In min, en vriendtschap <rhyme label="c" type="m">staet</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Ia vroolijck mach <rhyme label="b" type="f" corresp="#s1:l3">verkeeren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hoe dat het hier oock <rhyme label="c" type="m" corresp="#s1:l4">gaet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Geluckigh sulcken <rhyme label="a" type="m">Ziel</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die dit voor hem <rhyme label="a" type="m" corresp="#s2:l1">behiel</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Die by hem mach <rhyme label="b" type="f">bevinden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat hy (t'wijl hy hier <rhyme label="c" type="m">leeft</rhyme>)</l>
        <l id="s2:l5" met="3" type="-+">Is een van Godts <rhyme label="b" type="f" corresp="#s2:l3">beminden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dien hy sijn Trooster <rhyme label="c" type="m" corresp="#s2:l4">geeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En dat die sijn <rhyme label="a" type="m">gemoedt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met troost, en blijdtschap <rhyme label="a" type="m" corresp="#s3:l1">voedt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">In alle <rhyme label="b" type="f">tegenheden</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Al wat hem Godt <rhyme label="c" type="m">toevoeght</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Daer is hy mee te <rhyme label="b" type="f" corresp="#s3:l3">vreeden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ia heel en al <rhyme label="c" type="m" corresp="#s3:l4">vernoeght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Schoon ieder hem <rhyme label="a" type="m">veracht</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En al sijn doen <rhyme label="a" type="m" corresp="#s4:l1">belacht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat hy 't soo nauw wil <rhyme label="b" type="f">siften</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Hy weet wat Godt <rhyme label="c" type="m">gebiedt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Des Menschen yd'le <rhyme label="b" type="f" corresp="#s4:l3">driften</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die reeckent hy gansch <rhyme label="c" type="m" corresp="#s4:l4">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hy houdt hem aen Godts <rhyme label="a" type="m">Woort</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat wijst hem noch al <rhyme label="a" type="m" corresp="#s5:l1">voort</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Noch verder heen te <rhyme label="b" type="f">komen</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Op dat hy meer en <rhyme label="c" type="m">meer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Neffens de heyl'ge <rhyme label="b" type="f" corresp="#s5:l3">vroomen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Mach eens zijn met de <rhyme label="c" type="m" corresp="#s5:l4">Heer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy weet, het kan niet <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat Godt alleen met <rhyme label="a" type="m" corresp="#s6:l1">schijn</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hem laten soud <rhyme label="b" type="f">genoegen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Wie wesen wil Godts <rhyme label="c" type="m">Vrient</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Die moet hem daer na <rhyme label="b" type="f" corresp="#s6:l3">voegen</rhyme>;</l>
        <l id="s6:l6" met="3" type="-+">Dat hy hem suyver <rhyme label="c" type="m" corresp="#s6:l4">dient</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Want in een reyn <rhyme label="a" type="m">gemoet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat Godes wille <rhyme label="a" type="m" corresp="#s7:l1">doet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Daer wil de Heere <rhyme label="b" type="f">woonen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Daer send hy sijnen <rhyme label="c" type="m">Geest</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Waer mee dan sijne <rhyme label="b" type="f" corresp="#s7:l3">Soonen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Sich troosten <rhyme label="c" type="m" corresp="#s7:l4">aldermeest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Al 't geen de Aerd' <rhyme label="a" type="m">omvat</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Is niet soo waerd: als <rhyme label="a" type="m" corresp="#s8:l1">dat</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Godts geest den Mensch komt <rhyme label="b" type="f">stercke</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Met vreughde in 't <rhyme label="c" type="m">gemoedt</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Als 't Geloof, en goe <rhyme label="b" type="f" corresp="#s8:l3">Wercken</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Daer door dan wordt <rhyme label="c" type="m" corresp="#s8:l4">gevoet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">'t Geeft sulcken <rhyme label="a" type="m">seeckerheydt</rhyme>;</l>
        <l id="s9:l2" met="3" type="-+">Als Godt inwendigh <rhyme label="a" type="m" corresp="#s9:l1">seydt</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">j' Hebt een goe wegh <rhyme label="b" type="f">begonnen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Volhert in deught <rhyme label="c" type="m">altijdt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Want als ghy hebt <rhyme label="b" type="f" corresp="#s9:l3">verwonnen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dan sulje zijn <rhyme label="c" type="m" corresp="#s9:l4">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">En soo getuyght die <rhyme label="a" type="m">geest</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Aen die Godt hert'lijck <rhyme label="a" type="m" corresp="#s10:l1">vreest</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Dat hy is van sijn <rhyme label="b" type="f">Kind'ren</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">En <rhyme label="c" type="m">Mede-erfgenaem</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Geen Mensch en kan 't <rhyme label="b" type="f" corresp="#s10:l3">verhind'ren</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Soo Godt hem maeckt <rhyme label="c" type="m" corresp="#s10:l4">bequaem</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O Goddelijcke <rhyme label="a" type="m">weeld'</rhyme>!</l>
        <l id="s11:l2" met="3" type="-+">Als soo het herte <rhyme label="a" type="m" corresp="#s11:l1">queelt</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Inwendigh, vol van <rhyme label="b" type="f">vreughde</rhyme>.</l>
        <l id="s11:l4" met="3" type="-+">Wie soud niet krijgen <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Dat Godt hem soo <rhyme label="b" type="f" corresp="#s11:l3">verheugden</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Met Ziels-vermaeck en <rhyme label="c" type="m" corresp="#s11:l4">lust</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Wanneer ick dit <rhyme label="a" type="m">bevind'</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Dat Godt my soo <rhyme label="a" type="m" corresp="#s12:l1">bemint</rhyme>.</l>
        <l id="s12:l3" met="3" type="-+">En van zijn geest laet <rhyme label="b" type="f">smaecken</rhyme>.</l>
        <l id="s12:l4" met="3" type="-+">Dan denck ick: Lieve <rhyme label="c" type="m">Heer</rhyme>!</l>
        <l id="s12:l5" met="3" type="-+">Laet my tot u <rhyme label="b" type="f" corresp="#s12:l3">genaecken</rhyme>.</l>
        <l id="s12:l6" met="3" type="-+">Die soetheydt smaeckt na <rhyme label="c" type="m" corresp="#s12:l4">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
