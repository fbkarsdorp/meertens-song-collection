  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O! Hemel vol <rhyme label="a" type="f">ghenuchten</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O! boomgaert aller <rhyme label="a" type="f" corresp="#s1:l1">vruchten</rhyme>;</l>
        <l id="s1:l3" met="3" type="+-">Sijt ghy weerde <rhyme label="b" type="f">Venus</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Sijt ghy weerde <rhyme label="b" type="f" corresp="#s1:l3">Venus</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Met u te <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s1:l6" met="2" type="-+">In soetheydt <rhyme label="c" type="f" corresp="#s1:l5">sweven</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Behaeght <rhyme label="b" type="f" corresp="#s1:l3">Silenus</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Minne-voester ô! <rhyme label="d" type="f">lieve</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Vriendinne, vive, ay <rhyme label="d" type="f" corresp="#s1:l8">vive</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'Tsijn gensters hare <rhyme label="a" type="f">loncken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die 't liefdens vier doen <rhyme label="a" type="f" corresp="#s2:l1">voncken</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Van Phoebus voor 't <rhyme label="b" type="f">daghen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Van Phoebus voor 't <rhyme label="b" type="f" corresp="#s2:l3">daghen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Eer dat <rhyme label="c" type="f">Aurora</rhyme></l>
        <l id="s2:l6" met="2" type="-+">De blonde <rhyme label="c" type="f" corresp="#s2:l5">Flora</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Met glans comt <rhyme label="b" type="f" corresp="#s2:l3">behaghen</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Minne-voester ô! <rhyme label="d" type="f">lieve</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Vriendinne, vive, ay <rhyme label="d" type="f" corresp="#s2:l8">vive</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Met macht en <rhyme label="a" type="f">heerschappijen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Komt sy ons jeucht <rhyme label="a" type="f" corresp="#s3:l1">bestrijen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Die hooghe <rhyme label="b" type="f">Goddinne</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Die hooghe <rhyme label="b" type="f" corresp="#s3:l3">Goddinne</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Haer kindt <rhyme label="c" type="f">Cupido</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Heeft de schoon <rhyme label="c" type="f" corresp="#s3:l5">Dido</rhyme></l>
        <l id="s3:l7" met="2" type="-+">AEneam doen <rhyme label="b" type="f" corresp="#s3:l3">minnen</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Minne-voester ô! <rhyme label="d" type="f">lieve</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Vriendinne, vive, ay <rhyme label="d" type="f" corresp="#s3:l8">vive</rhyme>.</l>
      </lg>
    </lg>
  </text>
