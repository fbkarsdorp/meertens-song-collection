<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185138</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Kom o mijn vriendin bemind</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Myn ziel is soo stil en <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Tot Godt die my <rhyme label="b" type="m">bystaet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want hy is mijn trooster <rhyme label="a" type="m" corresp="#s1:l1">groot</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Mijn hulp en <rhyme label="b" type="m" corresp="#s1:l2">toeverlaet</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Daerom sal // geenen <rhyme label="c" type="m">val</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">My doen storten <rhyme label="d" type="m">nimmermeer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En al ist // dat met <rhyme label="e" type="m">list</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Op my wert ghewacht so <rhyme label="d" type="m" corresp="#s1:l6">seer</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Om te helpen van <rhyme label="f" type="m">kant</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Als een hangende <rhyme label="f" type="m" corresp="#s1:l9">want</rhyme>,</l>
        <l id="s1:l11" met="3" type="-+">En muur die hem buyght <rhyme label="d" type="m" corresp="#s1:l6">neer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sy versieren met <rhyme label="a" type="m">voordacht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Leughens om dempen <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Sy geven goe woorden <rhyme label="a" type="m" corresp="#s2:l1">sacht</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">In ‘t herte vloeken <rhyme label="b" type="m" corresp="#s2:l2">sy</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Haren raet // my niet <rhyme label="c" type="m">schaet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Want mijn ziel staet heel en <rhyme label="d" type="m">al</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Op den Heer // die wel <rhyme label="e" type="m">eer</rhyme></l>
        <l id="s2:l8" met="4" type="+-">My behoet heeft voor den <rhyme label="d" type="m" corresp="#s2:l6">val</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Hy is mijn hulp en <rhyme label="f" type="m">troost</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">En mijnen schult <rhyme label="f" type="m" corresp="#s2:l9">altoost</rhyme>,</l>
        <l id="s2:l11" met="3" type="-+">Dat ick niet vallen <rhyme label="d" type="m" corresp="#s2:l6">sal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">By Godt is alleen mijn <rhyme label="a" type="m">heyl</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hy is mijn eer, mijn <rhyme label="b" type="m">steen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Aen zijn hulp en is geen <rhyme label="a" type="m" corresp="#s3:l1">feyl</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Hy ist, en anders <rhyme label="b" type="m" corresp="#s3:l2">geen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Op hem boutnen <rhyme label="c" type="m">vertrout</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Lieve lieden in den <rhyme label="d" type="m">noot</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Schuddet uyt // en <rhyme label="e" type="m">ontsluyt</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Voor hem uwe herten <rhyme label="d" type="m" corresp="#s3:l6">bloot</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Maer op geen menschen <rhyme label="f" type="m">siet</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Want sy en sijn doch <rhyme label="f" type="m" corresp="#s3:l9">niet</rhyme>,</l>
        <l id="s3:l11" met="3" type="-+">Al schijnen sy seer <rhyme label="d" type="m" corresp="#s3:l6">groot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">‘s Menschen doen wel <rhyme label="a" type="m">overleyt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Soo vintmens’ al te <rhyme label="b" type="m">saem</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Lichter dan de <rhyme label="a" type="m" corresp="#s4:l1">ydelheyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Al zijn sy groot van <rhyme label="b" type="m" corresp="#s4:l2">naem</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Op onrecht // niet en <rhyme label="c" type="m">hecht</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Noch op stouten hoogen <rhyme label="d" type="m">moet</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Vliet gheringh // alle <rhyme label="e" type="m">dingh</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Dat van selfs verdwijnen <rhyme label="d" type="m" corresp="#s4:l6">moet</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">So ghy rijckdom <rhyme label="f" type="m">ontfanght</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">V herte niet en <rhyme label="f" type="m" corresp="#s4:l9">hanght</rhyme>,</l>
        <l id="s4:l11" met="3" type="-+">Aen dat tijdelick <rhyme label="d" type="m" corresp="#s4:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Godt heeft voortijts wel <rhyme label="a" type="m">geseyt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Een dierbaer weerdich <rhyme label="b" type="m">woort</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En met groote <rhyme label="a" type="m" corresp="#s5:l1">soeticheyt</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Heb ickt somtijts <rhyme label="b" type="m" corresp="#s5:l2">ghehoort</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Dat de macht // en de <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Om te helpen is <rhyme label="d" type="m">alleen</rhyme></l>
        <l id="s5:l7" met="4" type="+-">By den Heer // Daerom <rhyme label="e" type="m">keer</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Ick tot hem, en anders <rhyme label="d" type="m" corresp="#s5:l6">geen</rhyme>:</l>
        <l id="s5:l9" met="3" type="+-">Ghy Heere zijt soo <rhyme label="f" type="m">goet</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Na dat een yder <rhyme label="f" type="m" corresp="#s5:l9">doet</rhyme>,</l>
        <l id="s5:l11" met="3" type="-+">Betaelt ghy groot en <rhyme label="d" type="m" corresp="#s5:l6">kleen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
