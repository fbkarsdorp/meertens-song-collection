<song>
  <meta>
    <id>12714</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>medemblick</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>C. Rhijnenburgh</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom laat ons met <rhyme label="a" type="m">malkaar</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ter eeren van dit <rhyme label="a" type="m" corresp="#s1:l1">Paar</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een Bruylofts-deuntjen <rhyme label="b" type="f">quelen</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Van hier bedroefde <rhyme label="c" type="m">Geest</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Want in een blijde <rhyme label="c" type="m" corresp="#s1:l4">Feest</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Daar mach men u niet <rhyme label="b" type="f" corresp="#s1:l3">velen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">’t Is Bruyloft, en de <rhyme label="a" type="m">Ieught</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wenscht anders niet dan <rhyme label="a" type="m" corresp="#s2:l1">vreught</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wie kan het haar <rhyme label="b" type="f">verbieden</rhyme>?</l>
        <l id="s2:l4" met="3" type="-+">De tijdt die brenght het <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En ik begin <rhyme label="c" type="m" corresp="#s2:l4">alree</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Volght ghy dan <rhyme label="b" type="f" corresp="#s2:l3">Iongge-lieden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Siet Bruy’gom, siet het <rhyme label="a" type="m">Pandt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Aan u van hooger <rhyme label="a" type="m" corresp="#s3:l1">handt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Geschonken in dit <rhyme label="b" type="f">leven</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Siet Bruyt, siet <rhyme label="c" type="m">wederom</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">V Lief u <rhyme label="c" type="m" corresp="#s3:l4">Bruydegom</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Van boven u <rhyme label="b" type="f" corresp="#s3:l3">gegeven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy Twee ghy zijt nu <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Elk anders vlees en <rhyme label="a" type="m" corresp="#s4:l1">been</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">O wonderlijk <rhyme label="b" type="f">vermenggen</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">Elk heeft wat hy <rhyme label="c" type="m">bemint</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">God laat u voor de <rhyme label="c" type="m" corresp="#s4:l4">wint</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Veel Iaren <rhyme label="b" type="f" corresp="#s4:l3">over-brenggen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Van u doorluchte <rhyme label="a" type="m">Ieugt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Moet ghy tot yeders <rhyme label="a" type="m" corresp="#s5:l1">vreugt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Veel goede Spruyten <rhyme label="b" type="f">winnen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Op dat door nieuwe <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">V weder-zijds <rhyme label="c" type="m" corresp="#s5:l4">geslacht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">’t Weer leven magh <rhyme label="b" type="f" corresp="#s5:l3">beginnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Daar toe wil u de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Geluk van boven <rhyme label="a" type="m" corresp="#s6:l1">neer</rhyme></l>
        <l id="s6:l3" met="3" type="-+">In sijn genade <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En dat u liefde <rhyme label="c" type="m">duyr</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Voor Vredes soet <rhyme label="c" type="m" corresp="#s6:l4">bestuyr</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Aan ’t uyt-eynd’ van u <rhyme label="b" type="f" corresp="#s6:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Soo moet het u <rhyme label="a" type="m">vergaan</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Soo moet het u <rhyme label="a" type="m" corresp="#s7:l1">beslaan</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En op die trou <rhyme label="b" type="f">volharden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Laat God u t’samen <rhyme label="c" type="m">oudt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Laat God u t’samen <rhyme label="c" type="m" corresp="#s7:l4">koudt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En t’samen leelijk <rhyme label="b" type="f" corresp="#s7:l3">werden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Te samen gaan ter <rhyme label="a" type="m">Feest</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Daar niemant minst noch <rhyme label="a" type="m" corresp="#s8:l1">meest</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Komt oyt of oyt te <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En daar men <rhyme label="c" type="m">even-wel</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Vol vreughde, buyten <rhyme label="c" type="m" corresp="#s8:l4">quel</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Sal eeuwigh Bruyloft <rhyme label="b" type="f" corresp="#s8:l3">houwen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
