<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160861</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">ICk noem al dit <rhyme label="a" type="f">singhen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Onnodighe <rhyme label="a" type="f" corresp="#s1:l1">dinghen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Elck doe dat hy best <rhyme label="b" type="m">kan</rhyme>;</l>
        <l id="s1:l4" met="3" type="+-">Maer tijt te <rhyme label="c" type="f">verliesen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Daer roepen mijn <rhyme label="c" type="f" corresp="#s1:l4">kiesen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Te schendigh tegen <rhyme label="b" type="m" corresp="#s1:l3">aen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Doch is het <rhyme label="a" type="f">bedonghen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Dat geen <rhyme label="a" type="f" corresp="#s2:l1">ongesonghen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Mach snijden van de <rhyme label="b" type="m">ham</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Ick sweer by gans <rhyme label="c" type="f">worsten</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Ick songh my te <rhyme label="c" type="f" corresp="#s2:l4">borsten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Op dat ick 'er meed' aen <rhyme label="b" type="m" corresp="#s2:l3">quam</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wat placht ick voor <rhyme label="a" type="f">desen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Een honsvot te <rhyme label="a" type="f" corresp="#s3:l1">wesen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met Archimedes <rhyme label="b" type="m">geslacht</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Doe 'k om haer <rhyme label="c" type="f">verschillen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Met Tuyrtjes en <rhyme label="c" type="f" corresp="#s3:l4">Brillen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Was besigh by dagh, en <rhyme label="b" type="m" corresp="#s3:l3">nacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Ptolomeus die <rhyme label="a" type="f">miste</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Copernicus <rhyme label="a" type="f" corresp="#s4:l1">giste</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ick sagh dat Tycho <rhyme label="b" type="m">Bra</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Soo wel als sijn maet <rhyme label="c" type="f">loogh</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Sijn circkels en <rhyme label="c" type="f" corresp="#s4:l4">graedtboogh</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Bedrogen soo voor als <rhyme label="b" type="m" corresp="#s4:l3">na</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">'t Most al nae haer wil <rhyme label="a" type="f">gaen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Het aerdtrijck most <rhyme label="a" type="f" corresp="#s5:l1">stil-staen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Neen riep den anderen <rhyme label="b" type="m">weer</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Laet draeyen de <rhyme label="c" type="f">aerde</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Maer hoe sy het <rhyme label="c" type="f" corresp="#s5:l4">klaerde</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Haer harss'nen draeyden noch <rhyme label="b" type="m" corresp="#s5:l3">meer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Ick hoord' dees <rhyme label="a" type="f">gesellen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Van Phoebus <rhyme label="a" type="f" corresp="#s6:l1">vertellen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hoe hy de werelt <rhyme label="b" type="m">omliep</rhyme>;</l>
        <l id="s6:l4" met="3" type="+-">Hoe hem voor sijn <rhyme label="c" type="f">loopen</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Twaelf kroegen stonden <rhyme label="c" type="f" corresp="#s6:l4">open</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die Jupiter voor hem <rhyme label="b" type="m" corresp="#s6:l3">schiep</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Swijgh van je <rhyme label="a" type="f">dispuyten</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Of houwt 'er mijn <rhyme label="a" type="f" corresp="#s7:l1">buyten</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Vreed' is my <rhyme label="b" type="m">wellekom</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">En sooj' onderdruckt <rhyme label="c" type="f">wort</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Draeyt datje besuckt <rhyme label="c" type="f" corresp="#s7:l4">wort</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Maer draeyt'er mijn schotel niet <rhyme label="b" type="m" corresp="#s7:l3">om</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">En laet 'er mijn glas <rhyme label="a" type="f">staen</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Daer kan ick op vast <rhyme label="a" type="f" corresp="#s8:l1">gaen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Meer als op u <rhyme label="b" type="m">Almanack</rhyme>;</l>
        <l id="s8:l4" met="3" type="+-">Hier is, of ick dool <rhyme label="c" type="f">ver</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Een seeckerder <rhyme label="c" type="f" corresp="#s8:l4">Poolster</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die nimmer schijnsel <rhyme label="b" type="m" corresp="#s8:l3">ontbrack</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Hier sie ick <rhyme label="a" type="f">Cometen</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Vaste Sterren, <rhyme label="a" type="f" corresp="#s9:l1">Planeten</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Plaisierigh onder <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Soo minnelijck <rhyme label="c" type="f">swieren</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Als een potje met <rhyme label="c" type="f" corresp="#s9:l4">pieren</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Haer huysraet is nu <rhyme label="b" type="m" corresp="#s9:l3">gemeen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Soo gae ick te velt <rhyme label="a" type="f">nu</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Sa Buyrman dat gelt <rhyme label="a" type="f" corresp="#s10:l1">u</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Doet als ick heb <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Soo raeckt al de <rhyme label="c" type="f">luyster</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Van 't nacht-licht in 't <rhyme label="c" type="f" corresp="#s10:l4">duyster</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Mijn dunckt oock den dagh komt <rhyme label="b" type="m" corresp="#s10:l3">aen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
