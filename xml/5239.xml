<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5239</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag translation="Geuzen song">geuzenlied</tag>
      <tag translation="historical song">historielied</tag>
    </tags>
    <melody>Psalm 068 Datheen</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Weest al verblijt, ende <rhyme label="a" type="m">verheucht</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Looft nu met vlijt in rechter <rhyme label="a" type="m" corresp="#s1:l1">deucht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">God onsen Heer <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die ons dit teghenwoordich <rhyme label="c" type="m">jaer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Nu wederom int <rhyme label="c" type="m" corresp="#s1:l4">openbaer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sijn goetheyt heeft <rhyme label="b" type="f" corresp="#s1:l3">bewesen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Voor de stadt van Geertruyden <rhyme label="d" type="m">berch</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Die vande boose boeven <rhyme label="d" type="m" corresp="#s1:l7">erch</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">(Nu gheleden vier <rhyme label="e" type="f">jaren</rhyme>)</l>
        <l id="s1:l10" met="4" type="-+">Worde vercocht aenden <rhyme label="f" type="m">vyant</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Maer sy zijn meest comen te <rhyme label="f" type="m" corresp="#s1:l10">schandt</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Tot hunlieder <rhyme label="e" type="f" corresp="#s1:l9">beswaren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Graef Maurits des Lants <rhyme label="a" type="m">Gouverneur</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Die isser weer ghecomen <rhyme label="a" type="m" corresp="#s2:l1">veur</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Drie daghen in <rhyme label="b" type="f">Aprille</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Tnegentich drie, tzy u <rhyme label="c" type="m">verclaert</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Heeft de stadt vry en <rhyme label="c" type="m" corresp="#s2:l4">onbeswaert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Beleyt na sijnen <rhyme label="b" type="f" corresp="#s2:l3">wille</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Hy heefter om ghemaeckt een <rhyme label="d" type="m">perck</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Om daer in sijn vyanden <rhyme label="d" type="m" corresp="#s2:l7">sterck</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Vry wederstant te <rhyme label="e" type="f">bieden</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Als sy quamen met al hun <rhyme label="f" type="m">macht</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Om de stadt tontsetten met <rhyme label="f" type="m" corresp="#s2:l10">cracht</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Twelck niet conde <rhyme label="e" type="f" corresp="#s2:l9">gheschieden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Graef Mansvelt was oock op de <rhyme label="a" type="m">baen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Met sijn volck om te comen <rhyme label="a" type="m" corresp="#s3:l1">aen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Tquartier van <rhyme label="b" type="f">d'Excelentie</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maer doen hy sach, dat hy geen <rhyme label="c" type="m">cans</rhyme></l>
        <l id="s3:l5" met="4" type="-+">En had aen d'Excellenti <rhyme label="c" type="m" corresp="#s3:l4">schans</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Maeckte hy schier geen <rhyme label="b" type="f" corresp="#s3:l3">mencie</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Als of hy daer ghecomen <rhyme label="d" type="m">waer</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">De stadt tontsetten, maer van <rhyme label="d" type="m" corresp="#s3:l7">daer</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Is hy weder <rhyme label="e" type="f">ghetoghen</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Na t'quartier van Graef <rhyme label="f" type="m">Hohenlo</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Alwaer hy oock bleef ligghen <rhyme label="f" type="m" corresp="#s3:l10">so</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Sonder t'ontset te <rhyme label="e" type="f" corresp="#s3:l9">pooghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Doe ons volck eerst quam voor de <rhyme label="a" type="m">stadt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Heeft den vyant een schans <rhyme label="a" type="m" corresp="#s4:l1">ghehadt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Buyten de stadt <rhyme label="b" type="f">gheleghen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Daerom ons volck hen heeft <rhyme label="c" type="m">ghestelt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Om dit te winnen met <rhyme label="c" type="m" corresp="#s4:l4">ghewelt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">End hebbens oock <rhyme label="b" type="f" corresp="#s4:l3">ghecreghen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Doen den vyant de schans was <rhyme label="d" type="m">quijt</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Doen is ons volck al metter <rhyme label="d" type="m" corresp="#s4:l7">tijt</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Naerder de stadt <rhyme label="e" type="f">ghecomen</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Daer lach noch een Fort tusschen <rhyme label="f" type="m">tween</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Dat heeft ons volck oock <rhyme label="f" type="m" corresp="#s4:l10">afghescheen</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Vande stadt sonder <rhyme label="e" type="f" corresp="#s4:l9">schromen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Doen heeft die vyant noch <rhyme label="a" type="m">ghehadt</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Een schans teghen over de <rhyme label="a" type="m" corresp="#s5:l1">Kat</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Contrepunt by <rhyme label="b" type="f">namen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Waer naer ons volck oock heeft <rhyme label="c" type="m">ghetracht</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Om die te winnen metter <rhyme label="c" type="m" corresp="#s5:l4">macht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Derwaert dat sy oock <rhyme label="b" type="f" corresp="#s5:l3">quamen</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Op S. Jans dach of daer <rhyme label="d" type="m">ontrent</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Dat sy de Contrepunt <rhyme label="d" type="m" corresp="#s5:l7">present</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Te bestormen <rhyme label="e" type="f">begonnen</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Capiteyn Haen, end <rhyme label="f" type="m">Buberi</rhyme></l>
        <l id="s5:l11" met="4" type="+-">Elck van een Zeeusche <rhyme label="f" type="m" corresp="#s5:l10">Compaigni</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Hebben die punt <rhyme label="e" type="f" corresp="#s5:l9">ghewonnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als den vyant sach op dat <rhyme label="a" type="m">pas</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Dat hy de Contrepunt quijt <rhyme label="a" type="m" corresp="#s6:l1">was</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Socht hy om <rhyme label="b" type="f">taccordeeren</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dees mare wert al metter <rhyme label="c" type="m">spoet</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Ghedraghen aen Graef Maurits <rhyme label="c" type="m" corresp="#s6:l4">goet</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Diet niet wou <rhyme label="b" type="f" corresp="#s6:l3">refuseren</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">T'accoort dat wert besloten <rhyme label="d" type="m">saen</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Het Garnisoen soude uyt <rhyme label="d" type="m" corresp="#s6:l7">gaen</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Met haer lien zijd' <rhyme label="e" type="f">gheweyren</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Maer sestien Vendelen seer <rhyme label="f" type="m">schoon</rhyme></l>
        <l id="s6:l11" met="4" type="+-">Die moesten sy (voor haren <rhyme label="f" type="m" corresp="#s6:l10">loon</rhyme>)</l>
        <l id="s6:l12" met="3" type="-+">Oock altemael <rhyme label="e" type="f" corresp="#s6:l9">ontbeyren</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">In Junio twintich en <rhyme label="a" type="m">vier</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Zijnde den dach ten avont <rhyme label="a" type="m" corresp="#s7:l1">schier</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Is de stadt <rhyme label="b" type="f">opghegheven</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Maer eerst des anderendaechs daer <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Zijn de soldaten <rhyme label="c" type="m" corresp="#s7:l4">uytghegaen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Maer drie zijnder <rhyme label="b" type="f" corresp="#s7:l3">ghebleven</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Die den Berch meed hadden <rhyme label="d" type="m">vercocht</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Twelck hun heeft aen een galg <rhyme label="d" type="m" corresp="#s7:l7">ghebrocht</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Ten Berch zijnse <rhyme label="e" type="f">ghehanghen</rhyme>,</l>
        <l id="s7:l10" met="4" type="-+">Dus sietmen dat sulcken volck <rhyme label="f" type="m">fel</rhyme>,</l>
        <l id="s7:l11" met="4" type="+-">Die hun overheyt zijn <rhyme label="f" type="m" corresp="#s7:l10">rebel</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">Noch al hun loon <rhyme label="e" type="f" corresp="#s7:l9">ontfanghen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Binnen t'belegh ist oock <rhyme label="a" type="m">gheschiet</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Dat Graef van Hohenloo <rhyme label="a" type="m" corresp="#s8:l1">subijt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Maeckte een loos <rhyme label="b" type="f">Alarme</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Des stadts Gouverneur coen en <rhyme label="c" type="m">fier</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Ghenaemt de Heer van <rhyme label="c" type="m" corresp="#s8:l4">Masier</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Hy was doen also <rhyme label="b" type="f" corresp="#s8:l3">warme</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Om tontset te sien comen <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Is hy op den Thooren <rhyme label="d" type="m" corresp="#s8:l7">ghegaen</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Zijn Cappelaen oock <rhyme label="e" type="f">mede</rhyme></l>
        <l id="s8:l10" met="4" type="-+">Alwaer sy quamen inden <rhyme label="f" type="m">noot</rhyme>,</l>
        <l id="s8:l11" met="4" type="+-">Want sy zijn daer gheschoten <rhyme label="f" type="m" corresp="#s8:l10">doot</rhyme>,</l>
        <l id="s8:l12" met="3" type="-+">De schiltwacht oock ter <rhyme label="e" type="f" corresp="#s8:l9">stede</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ons volck heeft oock cloeck ende <rhyme label="a" type="m">stout</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Uutghevallen soo menich <rhyme label="a" type="m" corresp="#s9:l1">fout</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Teghent heyr der <rhyme label="b" type="f">vyanden</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Hebben malcanderen <rhyme label="c" type="m">gheslaen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Ons volck brachter dickwils <rhyme label="c" type="m" corresp="#s9:l4">ghevaen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Den vyant tsijnder <rhyme label="b" type="f" corresp="#s9:l3">schanden</rhyme>:</l>
        <l id="s9:l7" met="4" type="-+">Tot Turnhout ende elders <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Dat den vyant quam int <rhyme label="d" type="m" corresp="#s9:l7">verdriet</rhyme></l>
        <l id="s9:l9" met="3" type="-+">Heefter veel volcx <rhyme label="e" type="f">ghelaten</rhyme></l>
        <l id="s9:l10" met="4" type="-+">Ghelooft zy God <rhyme label="f" type="m">ghebenedijt</rhyme>,</l>
        <l id="s9:l11" met="4" type="+-">Die ons nu langhe heeft <rhyme label="f" type="m" corresp="#s9:l10">bevryt</rhyme></l>
        <l id="s9:l12" met="3" type="-+">Voor de gheen die ons <rhyme label="e" type="f" corresp="#s9:l9">haten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Oorlof ghy inwoonders al <rhyme label="a" type="m">tsaem</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Van Hollant ende Zeelant <rhyme label="a" type="m" corresp="#s10:l1">bequaem</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Laet ons doch den Heer <rhyme label="b" type="f">vreesen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Die ons nu wel int seste <rhyme label="c" type="m">jaer</rhyme></l>
        <l id="s10:l5" met="4" type="-+">In al onse benautheyt <rhyme label="c" type="m" corresp="#s10:l4">swaer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Sijn bystant heeft <rhyme label="b" type="f" corresp="#s10:l3">bewesen</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Soo wy leven nae sijn <rhyme label="d" type="m">ghebodt</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">Hy sal ons vry (dit is het <rhyme label="d" type="m" corresp="#s10:l7">slot</rhyme>)</l>
        <l id="s10:l9" met="3" type="-+">Sijn ghenade <rhyme label="e" type="f">bewijsen</rhyme>,</l>
        <l id="s10:l10" met="4" type="-+">Gheduerichlijck tot alder <rhyme label="f" type="m">stont</rhyme>,</l>
        <l id="s10:l11" met="4" type="+-">Dus laet ons hem uyt s'herten <rhyme label="f" type="m" corresp="#s10:l10">gront</rhyme>,</l>
        <l id="s10:l12" met="3" type="-+">Voor sijn weldaden <rhyme label="e" type="f" corresp="#s10:l9">prijsen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
