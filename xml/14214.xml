    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Rijke God wie zal ik klagen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O God wilt my <rhyme label="a" type="f">verhooren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Jck roepe van herten <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die werelt wilt my <rhyme label="a" type="f" corresp="#s1:l1">verstooren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sy werckt druck ende groot <rhyme label="b" type="m" corresp="#s1:l2">verdriet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daerom tot v moet ick <rhyme label="c" type="f">claghen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ghy sijt mijn troost <rhyme label="d" type="m">alleen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Laet my doch v <rhyme label="c" type="f" corresp="#s1:l5">behaghen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Maeckt my van sonden <rhyme label="d" type="m" corresp="#s1:l6">reen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die Duyuel wilt my <rhyme label="a" type="f">omsetten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met ghewelt seer <rhyme label="b" type="m">onghestilt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Wye can hem dat <rhyme label="a" type="f" corresp="#s2:l1">beletten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Als ghy niet helpen en <rhyme label="b" type="m" corresp="#s2:l2">wilt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Jn sonden hout hy my <rhyme label="c" type="f">geuanghen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Bedeckt met vleesschelijcke <rhyme label="d" type="m">list</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Met hoouerdije der weerelt <rhyme label="c" type="f" corresp="#s2:l5">behanghen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Sijn bedrog ick niet en <rhyme label="d" type="m" corresp="#s2:l6">wist</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die wet my nv oock <rhyme label="a" type="f">dreyghet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die helle voor ooghen <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Tot sonden ben ick <rhyme label="a" type="f" corresp="#s3:l1">gheneyghet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Bekenne ick eert wort te <rhyme label="b" type="m" corresp="#s3:l2">laet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Noch moet ick Heere <rhyme label="c" type="f">versaghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Jn uwer <rhyme label="d" type="m">gherechticheyt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Jst dat ghy niet wt blooter <rhyme label="c" type="f" corresp="#s3:l5">ghenaden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Bedeckt mijn <rhyme label="d" type="m" corresp="#s3:l6">swackicheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wel hem die op v <rhyme label="a" type="f">bouwet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">O Christe der ghenaden <rhyme label="b" type="m">troon</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ende v woordt vast <rhyme label="a" type="f" corresp="#s4:l1">gheloouet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die is ghereynicht <rhyme label="b" type="m" corresp="#s4:l2">schoon</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Coemt hier ghy die sijt <rhyme label="c" type="f">beladen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Bekennet uwer sonden <rhyme label="d" type="m">noot</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Jck wil v ontfanghen in <rhyme label="c" type="f" corresp="#s4:l5">ghenaden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat ghy niet en smaeckt den eewighen <rhyme label="d" type="m" corresp="#s4:l6">doot</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijnen gheest wyl ick v <rhyme label="a" type="f">schencken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die v regeren <rhyme label="b" type="m">sal</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Den ouden Adam te <rhyme label="a" type="f" corresp="#s5:l1">crencken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat ick frisch ouer <rhyme label="b" type="m" corresp="#s5:l2">al</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Hert, sin, moet en <rhyme label="c" type="f">wille</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Regeere nae mijnder <rhyme label="d" type="m">leer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Hout hier int ghelooue <rhyme label="c" type="f" corresp="#s5:l5">stille</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Soo sijt ghy gheboren <rhyme label="d" type="m" corresp="#s5:l6">weer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die warachtighe trouwe te <rhyme label="a" type="f">ghedencken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Daer ick mede lief hebbe <rhyme label="b" type="m">dy</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Wilt mijn woort in v herte <rhyme label="a" type="f" corresp="#s6:l1">prenten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat ghy te rechte betrout op <rhyme label="b" type="m" corresp="#s6:l2">my</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Die sonde leert af <rhyme label="c" type="f">steruen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Neemt toe in <rhyme label="d" type="m">gherechticheyt</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Soo suldy mijn ghenade <rhyme label="c" type="f" corresp="#s6:l5">verweruen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Dat ghy sult leuen inder <rhyme label="d" type="m" corresp="#s6:l6">eewicheyt</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn siele v hier voor <rhyme label="a" type="f">prijset</rhyme></l>
        <l id="s7:l2" met="3" type="-+">God vader der <rhyme label="b" type="m">bermherticheyt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">God Sone ghy die my <rhyme label="a" type="f" corresp="#s7:l1">wijset</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Den wech totter <rhyme label="b" type="m" corresp="#s7:l2">salicheyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Uwen heylighen Gheest my <rhyme label="c" type="f">behouwe</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Jnden rechten woorde <rhyme label="d" type="m">dijn</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat ick niet inder liefden en <rhyme label="c" type="f" corresp="#s7:l5">vercouwe</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Teghen v en den naesten <rhyme label="d" type="m" corresp="#s7:l6">mijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
