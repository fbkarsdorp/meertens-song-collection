<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24566</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ONs genaket die auent <rhyme label="a" type="m">steerne</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die ons verlichtet alsoe <rhyme label="a" type="m" corresp="#s1:l1">claer</rhyme></l>
        <l id="s1:l3" met="2" type="-+">wal was hoer <rhyme label="b" type="m">doe</rhyme></l>
        <l id="s1:l4" met="4" type="+-">susa nynna susa <rhyme label="b" type="m" corresp="#s1:l3">noe</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Jhesus mynne sprack maria <rhyme label="b" type="m" corresp="#s1:l3">toe</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat huys dat hadde soe mennich <rhyme label="a" type="m">gat</rhyme></l>
        <l id="s2:l2" met="4" type="-+">daer christus in gebaren <rhyme label="a" type="m" corresp="#s2:l1">was</rhyme></l>
        <l id="s2:l3" met="2" type="-+">wal was hoer <rhyme label="b" type="m">doe</rhyme></l>
        <l id="s2:l4" met="4" type="+-">susa nynna susa <rhyme label="b" type="m" corresp="#s2:l3">noe</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Jhesus mynne sprack maria <rhyme label="b" type="m" corresp="#s2:l3">toe</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy setten dat kynt op hoeren <rhyme label="a" type="m">scoet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">sy cussedet voer syn mondekyn <rhyme label="a" type="m" corresp="#s3:l1">roet</rhyme></l>
        <l id="s3:l3" met="2" type="-+">het was soe <rhyme label="b" type="m">suet</rhyme></l>
        <l id="s3:l4" met="4" type="+-">susa nynna susa <rhyme label="b" type="m" corresp="#s3:l3">noe</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Jhesus mynne sprack maria <rhyme label="b" type="m" corresp="#s3:l3">toe</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy setten dat kynt op hoeren <rhyme label="a" type="m">knyen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">sy sprack groet eer moet v <rhyme label="a" type="m" corresp="#s4:l1">geschien</rhyme></l>
        <l id="s4:l3" met="2" type="-+">wal was hoer <rhyme label="b" type="m">doe</rhyme></l>
        <l id="s4:l4" met="4" type="+-">susa nynna susa <rhyme label="b" type="m" corresp="#s4:l3">noe</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Jhesus mynne sprack maria <rhyme label="b" type="m" corresp="#s4:l3">toe</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy setten dat kynt op oren <rhyme label="a" type="m">narm</rhyme></l>
        <l id="s5:l2" met="4" type="-+">mit groter vrouden sach syt <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">het was soe <rhyme label="b" type="m">suet</rhyme></l>
        <l id="s5:l4" met="4" type="+-">susa nynna susa <rhyme label="b" type="m" corresp="#s5:l3">noe</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Jhesus mynne sprack maria <rhyme label="b" type="m" corresp="#s5:l3">toe</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die moder die makeden den kynde een <rhyme label="a" type="m">bat</rhyme></l>
        <l id="s6:l2" met="4" type="-+">hoe lieflic dattet  daer ynne <rhyme label="a" type="m" corresp="#s6:l1">sat</rhyme></l>
        <l id="s6:l3" met="2" type="-+">wal was hoer <rhyme label="b" type="m">doe</rhyme></l>
        <l id="s6:l4" met="4" type="+-">susa nynna susa <rhyme label="b" type="m" corresp="#s6:l3">noe</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Jhesus mynne sprack maria <rhyme label="b" type="m" corresp="#s6:l3">toe</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dat kyndekyn pleterden mitter <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s7:l2" met="4" type="-+">dattet water wtten becken <rhyme label="a" type="m" corresp="#s7:l1">spranc</rhyme></l>
        <l id="s7:l3" met="2" type="-+">wal was hoer <rhyme label="b" type="m">doe</rhyme></l>
        <l id="s7:l4" met="4" type="+-">susa nynna susa <rhyme label="b" type="m" corresp="#s7:l3">noe</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Jhesus mynne sprack maria <rhyme label="b" type="m" corresp="#s7:l3">toe</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die os ende oec dat <rhyme label="a" type="m">eselkyn</rhyme></l>
        <l id="s8:l2" met="4" type="-+">die aenbeden dat suete <rhyme label="a" type="m" corresp="#s8:l1">kyndekyn</rhyme></l>
        <l id="s8:l3" met="2" type="-+">wal was hoer <rhyme label="b" type="m">doe</rhyme></l>
        <l id="s8:l4" met="4" type="+-">susa nyna susa <rhyme label="b" type="m" corresp="#s8:l3">no</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Jhesus mynne sprack marien <rhyme label="b" type="m" corresp="#s8:l3">toe</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
