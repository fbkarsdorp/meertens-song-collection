        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Puer natus in Bethleem</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EEn kint geboor'n tot <rhyme label="a" type="m">Bethlehem</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">tot <rhyme label="a" type="m" corresp="#s1:l1">Bethlehem</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dies hem verheucht <rhyme label="a" type="m" corresp="#s1:l1">Jerusalem</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Alle, Alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hiet leyt hy in dat <rhyme label="a" type="m">Cribbekijn</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+"><rhyme label="a" type="m" corresp="#s2:l1">Cribbekijn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sonder eint is die heerschap <rhyme label="a" type="m" corresp="#s2:l1">zijn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">alle, alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat Oskijn, en dat <rhyme label="a" type="m">Eselkijn</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+"><rhyme label="a" type="m" corresp="#s3:l1">Eselkijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Bekenden Godt den Heeren <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Alle, Alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">D'wijsen van t' Oosten quamen <rhyme label="a" type="m">daer</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">quamen <rhyme label="a" type="m" corresp="#s4:l1">daer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Gout, Wierooc, Myrrh' sy brachten <rhyme label="a" type="m" corresp="#s4:l1">daer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Alle, Alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sijn Moeder is die reyne <rhyme label="a" type="m">Maecht</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">reyne <rhyme label="a" type="m" corresp="#s5:l1">Maecht</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die sonder Man gebaret <rhyme label="a" type="m" corresp="#s5:l1">hadt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Alle, Alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dien die Slang niet vergiften <rhyme label="a" type="m">kondt</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">vergiften <rhyme label="a" type="m" corresp="#s6:l1">kondt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Is worden ons bloet sonder <rhyme label="a" type="m" corresp="#s6:l1">sont</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Alle, alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy is ons gants g'lijck na den <rhyme label="a" type="m">vleysch</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">na den <rhyme label="a" type="m" corresp="#s7:l1">vleysch</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Der sonden na ons niet <rhyme label="a" type="m" corresp="#s7:l1">gelijck</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Alle, Alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer met hy hem ons maeckt <rhyme label="a" type="m">gelijc</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">maeckt <rhyme label="a" type="m" corresp="#s8:l1">ghelijck</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En weder bracht tot Godes <rhyme label="a" type="m" corresp="#s8:l1">rijc</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Alle, alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Voor sulck ghenaden-rijcken <rhyme label="a" type="m">tijdt</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">rijcken <rhyme label="a" type="m" corresp="#s9:l1">tijdt</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Sy Godt gelooft in <rhyme label="a" type="m" corresp="#s9:l1">eewicheyt</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Alle, Alle, <rhyme label="b" type="m">Alleluia</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
