<song>
  <meta>
    <id>23189</id>
    <date>
      <year-start>1525</year-start>
      <year-end>1525</year-end>
    </date>
    <location>
      <orginal-name>zuid-holland (dordrecht?)</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Laat ons met harten rein</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ons is een Kijnt <rhyme label="a" type="f">gheboren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Een Soen <rhyme label="b" type="m">ghepresenteert</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Hi coemt ter helle <rhyme label="a" type="f" corresp="#s1:l1">storen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Als mensche <rhyme label="c" type="f">ghefygureert</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Hi wil ons, al <rhyme label="d" type="m">ghemeyne</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Verlossen uutter <rhyme label="c" type="f" corresp="#s1:l4">pine</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Mit Sine bloede <rhyme label="d" type="m" corresp="#s1:l5">alleyne</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s1:l1">reyn</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s1:l2">Kindekijn</rhyme>:</l>
        <l id="s1:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s1:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die Vader van hier <rhyme label="a" type="f">boven</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Sprac Sinen enghel <rhyme label="b" type="m">an</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">'Ic coem die hel <rhyme label="a" type="f" corresp="#s2:l1">verstoren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En verlossen wijf ende <rhyme label="c" type="f">man</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Gaet neder tot die <rhyme label="d" type="m">Fonteyne</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Maria, claer <rhyme label="c" type="f" corresp="#s2:l4">Aenschijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ende segt Haer wat Ic <rhyme label="d" type="m" corresp="#s2:l5">meyne</rhyme>.'</l>
        <l id="s2:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s2:l1">reyn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s2:l2">Kindekijn</rhyme>:</l>
        <l id="s2:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s2:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'Groetse My <rhyme label="a" type="f">vriendelike</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die suver jonge <rhyme label="b" type="m">Joecht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ende segt Haer <rhyme label="a" type="f" corresp="#s3:l1">blidelike</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Si mach wel sijn <rhyme label="c" type="f">verhoecht</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Want Sy, ende anders <rhyme label="d" type="m">gheyne</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Gods Moeder moet Sy <rhyme label="c" type="f" corresp="#s3:l4">sijn</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">God is mit Haer <rhyme label="d" type="m" corresp="#s3:l5">ghemeyne</rhyme>.'</l>
        <l id="s3:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s3:l1">reyn</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s3:l2">Kindekijn</rhyme>:</l>
        <l id="s3:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s3:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al binnen <rhyme label="a" type="f">Nazaretten</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Quam die enghel <rhyme label="b" type="m">Gabriel</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Hi groete der <rhyme label="a" type="f" corresp="#s4:l1">Fyoletten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met soete woerden <rhyme label="c" type="f">snel</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">'God groet U, Maghet <rhyme label="d" type="m">reyne</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Vol gracien is U <rhyme label="c" type="f" corresp="#s4:l4">aenschijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">God is mit U <rhyme label="d" type="m" corresp="#s4:l5">alleyne</rhyme>.'</l>
        <l id="s4:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s4:l1">reyn</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s4:l2">Kindekijn</rhyme>:</l>
        <l id="s4:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s4:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'Bi U sel noch <rhyme label="a" type="f">becliven</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Dat Adam heeft <rhyme label="b" type="m">ontfrijt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Want boven alle <rhyme label="a" type="f" corresp="#s5:l1">wiven</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Si Di <rhyme label="c" type="f">ghebenedit</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Want Ghi, ende anders <rhyme label="d" type="m">gheyne</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Gods Moeder moet Ghi <rhyme label="c" type="f" corresp="#s5:l4">sijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">God is mit U <rhyme label="d" type="m" corresp="#s5:l5">alleyne</rhyme>.'</l>
        <l id="s5:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s5:l1">reyn</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s5:l2">Kindekijn</rhyme>:</l>
        <l id="s5:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s5:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Si sprac <rhyme label="a" type="f">oetmoedeliken</rhyme>:</l>
        <l id="s6:l2" met="4" type="-+">'Hoe soude dat <rhyme label="b" type="m">gheschen</rhyme>?</l>
        <l id="s6:l3" met="3" type="-+">Ic en kende noeyt, <rhyme label="a" type="f" corresp="#s6:l1">sekerlike</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Man; des verwondert <rhyme label="c" type="f">my</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Hoe soude Ic, ende anders <rhyme label="d" type="m">gheyne</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Gods Moeder moghen <rhyme label="c" type="f" corresp="#s6:l4">sijn</rhyme>?</l>
        <l id="s6:l7" met="3" type="-+">Mijn haert is cout als eenen <rhyme label="d" type="m" corresp="#s6:l5">steyne</rhyme>!'</l>
        <l id="s6:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s6:l1">reyn</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s6:l2">Kindekijn</rhyme>:</l>
        <l id="s6:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s6:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die enghel sprac tot <rhyme label="a" type="f">Haer</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">'O, waerde suver <rhyme label="b" type="m">Maecht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En weest in gheenen <rhyme label="a" type="f" corresp="#s7:l1">vare</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">Hets wonder dat Ghi <rhyme label="c" type="f">claecht</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Want Ghi selt sonder <rhyme label="d" type="m">pinen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Baren een <rhyme label="c" type="f" corresp="#s7:l4">kijndekijn</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Ende bliven maghet <rhyme label="d" type="m" corresp="#s7:l5">fine</rhyme>.'</l>
        <l id="s7:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s7:l1">reyn</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s7:l2">Kindekijn</rhyme>:</l>
        <l id="s7:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s7:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Die enghel schiet van <rhyme label="a" type="f">Hare</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Die Maecht, die bleef <rhyme label="b" type="m">bevrucht</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Al sonder wee of <rhyme label="a" type="f" corresp="#s8:l1">swaere</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ende in herten <rhyme label="c" type="f">versucht</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Soe baerde die suver <rhyme label="d" type="m">Reyne</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Dat soete <rhyme label="c" type="f" corresp="#s8:l4">Kijndekijn</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Het brenct ons uutter <rhyme label="d" type="m" corresp="#s8:l5">weyne</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s8:l1">reyn</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s8:l2">Kindekijn</rhyme>:</l>
        <l id="s8:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s8:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Die Maecht doen <rhyme label="a" type="f">consenterde</rhyme></l>
        <l id="s9:l2" met="4" type="-+">In dat Haer die enghel <rhyme label="b" type="m">seide</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ende Si doe <rhyme label="a" type="f" corresp="#s9:l1">respondeerde</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">'Siet, Ic bin <rhyme label="c" type="f">bereit</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Na Dinen woerden <rhyme label="d" type="m">gheemeyne</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Stel Ic die woerden <rhyme label="c" type="f" corresp="#s9:l4">Mijn</rhyme>;</l>
        <l id="s9:l7" met="3" type="-+">Siet hier, Gods Deern <rhyme label="d" type="m" corresp="#s9:l5">alleyne</rhyme>.'</l>
        <l id="s9:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s9:l1">reyn</rhyme></l>
        <l id="s9:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s9:l2">Kindekijn</rhyme>:</l>
        <l id="s9:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s9:l1">weynen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Des moeghen wi wel <rhyme label="a" type="f">dancken</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Die Moeder, die Hem <rhyme label="b" type="m">droech</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">In Haer suver <rhyme label="a" type="f" corresp="#s10:l1">lancken</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Die vrucht Haer niet en <rhyme label="c" type="f">verwuech</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Wes vrolic, groot ende <rhyme label="d" type="m">cleyne</rhyme>!</l>
        <l id="s10:l6" met="3" type="-+">Dat soete <rhyme label="c" type="f" corresp="#s10:l4">Kijndekijn</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Het brenct ons uutter <rhyme label="d" type="m" corresp="#s10:l5">weyne</rhyme>.</l>
        <l id="s10:l8" met="3" type="-+">Laet ons mit herten <rhyme label="a" type="f" corresp="#s10:l1">reyn</rhyme></l>
        <l id="s10:l9" met="3" type="-+">Love dat soete <rhyme label="b" type="m" corresp="#s10:l2">Kindekijn</rhyme>:</l>
        <l id="s10:l10" met="3" type="-+">Het brenct ons uut <rhyme label="a" type="f" corresp="#s10:l1">weynen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
