  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Jeught laet vreught nu <rhyme label="a" type="f">blijcken</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Phoebus heeft met <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Boream doen <rhyme label="a" type="f" corresp="#s1:l1">wijcken</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Dies de Lenten <rhyme label="b" type="m" corresp="#s1:l2">lacht</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Siende ooc des <rhyme label="c" type="f">couden</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Winters fel bedwangh,, te <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Daerom in de <rhyme label="c" type="f" corresp="#s1:l5">wouden</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Den Victory-sangh,, <rhyme label="d" type="m" corresp="#s1:l6">gheschiet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Iuno heeft den <rhyme label="a" type="f">gordel</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Van Venus <rhyme label="b" type="m">ontleent</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Iupiter tot <rhyme label="a" type="f" corresp="#s2:l1">vordel</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Js met haer <rhyme label="b" type="m" corresp="#s2:l2">vereent</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Vesta na t' <rhyme label="c" type="f">betamen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">heeft nat haren wyden,, <rhyme label="d" type="m">schoot</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Dies de Nimphen t' <rhyme label="c" type="f" corresp="#s2:l5">samen</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Leven in verblyden,, <rhyme label="d" type="m" corresp="#s2:l6">groot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Driades <rhyme label="a" type="f">Goddinnen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Worden nieuw' <rhyme label="b" type="m">becroont</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Jn t'bewijs van <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Tot Adonis <rhyme label="b" type="m" corresp="#s3:l2">toont</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Venus soet van <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Haer bloet aende Roosen,, <rhyme label="d" type="m">root</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Al dede Mars <rhyme label="c" type="f" corresp="#s3:l5">desen</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Lyden een soo boosen,, <rhyme label="d" type="m" corresp="#s3:l6">doot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Faunen en <rhyme label="a" type="f">Silvanen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Hebben wilde <rhyme label="b" type="m">vrueght</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Herse coele <rhyme label="a" type="f" corresp="#s4:l1">tranen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Lieflijck end' vol <rhyme label="b" type="m" corresp="#s4:l2">deught</rhyme></l>
        <l id="s4:l5" met="3" type="+-">D'alder zoetste <rhyme label="c" type="f">Flora</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Des morghens ontfanghen,, <rhyme label="d" type="m">doet</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Als de schoon' <rhyme label="c" type="f" corresp="#s4:l5">Aurora</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Comt met roode wanghen,, <rhyme label="d" type="m" corresp="#s4:l6">zoet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Zephyr, Aura <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Blasen coeltgens <rhyme label="b" type="m">uyt</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Napen hebben <rhyme label="a" type="f" corresp="#s5:l1">Vrede</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Met gheblomt' end' <rhyme label="b" type="m" corresp="#s5:l2">cruydt</rhyme></l>
        <l id="s5:l5" met="3" type="+-">End' de Borne <rhyme label="c" type="f">morren</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Loopende de steenen,, <rhyme label="d" type="m">langst</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Voor Phaetons <rhyme label="c" type="f" corresp="#s5:l5">verdorren</rhyme></l>
        <l id="s5:l8" met="4" type="-+">En is nu oock gheenen,, <rhyme label="d" type="m" corresp="#s5:l6">angst</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Philomeles <rhyme label="a" type="f">kele</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Hoortmen nacht end' <rhyme label="b" type="m">dagh</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Echo volght zoo <rhyme label="a" type="f" corresp="#s6:l1">vele</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Als sy immers <rhyme label="b" type="m" corresp="#s6:l2">magh</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Jn bosschen end' <rhyme label="c" type="f">rotsen</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Men haer doen haer beste,, <rhyme label="d" type="m">hoort</rhyme></l>
        <l id="s6:l7" met="3" type="+-">Stemmen na te <rhyme label="c" type="f" corresp="#s6:l5">botsen</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Zy hout gheerne t'leste,, <rhyme label="d" type="m" corresp="#s6:l6">woort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Ceres dryft en <rhyme label="a" type="f">baret</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Met haer groen <rhyme label="b" type="m">ghewas</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Op het velt al <rhyme label="a" type="f" corresp="#s7:l1">waret</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Een Neptunes <rhyme label="b" type="m" corresp="#s7:l2">plas</rhyme></l>
        <l id="s7:l5" met="3" type="+-">End' Aeolus <rhyme label="c" type="f">knapen</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Als broot-droncken wichten, <rhyme label="d" type="m">snel</rhyme></l>
        <l id="s7:l7" met="3" type="+-">Schynen vreught te <rhyme label="c" type="f" corresp="#s7:l5">rapen</rhyme></l>
        <l id="s7:l8" met="4" type="-+">Hier in veel te stichten,, <rhyme label="d" type="m" corresp="#s7:l6">spel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Jn desen tijdt <rhyme label="a" type="f">lustigh</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Door cypris <rhyme label="b" type="m">bestier</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Elck ghediert' <rhyme label="a" type="f" corresp="#s8:l1">onrustigh</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Voelt der liefden <rhyme label="b" type="m" corresp="#s8:l2">vyer</rhyme></l>
        <l id="s8:l5" met="3" type="+-">Maer begaeft met <rhyme label="c" type="f">reden</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Mensch' v wel te letten,, <rhyme label="d" type="m">staet</rhyme></l>
        <l id="s8:l7" met="3" type="+-">Jn oprechte <rhyme label="c" type="f" corresp="#s8:l5">seden</rhyme></l>
        <l id="s8:l8" met="4" type="-+">t' Houden vast aen Wet end',, <rhyme label="d" type="m" corresp="#s8:l6">maet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Denckt hoe dat veel <rhyme label="a" type="f">blommen</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Door verliefde <rhyme label="b" type="m">jeught</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Sijn te voorschijn <rhyme label="a" type="f" corresp="#s9:l1">commen</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Dus ghy leeren <rhyme label="b" type="m" corresp="#s9:l2">meught</rhyme></l>
        <l id="s9:l5" met="3" type="+-">V ghedachten <rhyme label="c" type="f">wennen</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Want u oock ghegheven,, <rhyme label="d" type="m">wort</rhyme></l>
        <l id="s9:l7" met="3" type="+-">Door t' ghebloemt te <rhyme label="c" type="f" corresp="#s9:l5">kenne</rhyme></l>
        <l id="s9:l8" met="4" type="-+">t' Voorspel van u leven,, <rhyme label="d" type="m" corresp="#s9:l6">cort</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Soo Poeten <rhyme label="a" type="f">singhen</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Liefd' is blindt end' <rhyme label="b" type="m">mal</rhyme></l>
        <l id="s10:l3" met="3" type="+-">Sy is yeders <rhyme label="a" type="f" corresp="#s10:l1">dinghen</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Sy verwint het <rhyme label="b" type="m" corresp="#s10:l2">al</rhyme></l>
        <l id="s10:l5" met="3" type="+-">Maer is <rhyme label="c" type="f">onghestadigh</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Van tranen end' swaer, <rhyme label="d" type="m">ghesucht</rhyme></l>
        <l id="s10:l7" met="3" type="+-">Met veel smerten <rhyme label="c" type="f" corresp="#s10:l5">schadigh</rhyme></l>
        <l id="s10:l8" met="4" type="-+">Zijnde meest eenpaer,, <rhyme label="d" type="m" corresp="#s10:l6">bevrucht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Het mochten <rhyme label="a" type="f">cupido</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Wel vervloecken <rhyme label="b" type="m">seer</rhyme></l>
        <l id="s11:l3" met="3" type="+-">Ariadne <rhyme label="a" type="f" corresp="#s11:l1">Dydo</rhyme></l>
        <l id="s11:l4" met="3" type="+-">End' veel duysent <rhyme label="b" type="m" corresp="#s11:l2">meer</rhyme></l>
        <l id="s11:l5" met="3" type="+-">Die door <rhyme label="c" type="f">onghetrouwe</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Minnaers vol bedroch,, end' <rhyme label="d" type="m">list</rhyme></l>
        <l id="s11:l7" met="3" type="+-">Comen zijn in <rhyme label="c" type="f" corresp="#s11:l5">rouwe</rhyme></l>
        <l id="s11:l8" met="4" type="-+">t' Welck oock al noch niet,, en <rhyme label="d" type="m" corresp="#s11:l6">mist</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Edel reyn <rhyme label="a" type="f">Princessen</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Vry van hert' end' <rhyme label="b" type="m">mondt</rhyme></l>
        <l id="s12:l3" met="3" type="+-">d' Alderbeste <rhyme label="a" type="f" corresp="#s12:l1">lessen</rhyme></l>
        <l id="s12:l4" met="3" type="+-">Die ghy leeren <rhyme label="b" type="m" corresp="#s12:l2">condt</rhyme>:</l>
        <l id="s12:l5" met="3" type="+-">Weest altijt <rhyme label="c" type="f">voorsichtich</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Blijft van Hymens veter,, <rhyme label="d" type="m">vry</rhyme></l>
        <l id="s12:l7" met="3" type="+-">Zonder trouwen <rhyme label="c" type="f" corresp="#s12:l5">lichtigh</rhyme></l>
        <l id="s12:l8" met="4" type="-+">Als het laten beter,, <rhyme label="d" type="m" corresp="#s12:l6">sy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
