<song>
  <meta>
    <id>20834</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Aanzie o zondige ziel</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">UYt 't diepste van mijn <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Heb ick U, Heer der <rhyme label="b" type="f">Heeren</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Geropen om <rhyme label="c" type="m">gena</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Ey! verhoort // toch mijn <rhyme label="d" type="m">woord</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Gelieft U oogh te <rhyme label="b" type="f" corresp="#s1:l2">keeren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Tot uwen dienaer <rhyme label="c" type="m" corresp="#s1:l3">dra</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Laet ne'er geboogen <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Op mijn gebed uw' <rhyme label="b" type="f">ooren</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">O goedertieren <rhyme label="c" type="m">Heer</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">Mijn gebe'en // mijn <rhyme label="d" type="m">gesteen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wilt Jesu lief! <rhyme label="b" type="f" corresp="#s2:l2">Verhooren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Bewijst my dese <rhyme label="c" type="m" corresp="#s2:l3">eer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Is 't saecke dat Ghy <rhyme label="a" type="m">hebt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gelet op mijn <rhyme label="b" type="f">misdaden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Waer sal ick blijven <rhyme label="c" type="m">dan</rhyme>?</l>
        <l id="s3:l4" met="4" type="+-">Ach! mijn Heer! // 'k neem mijn <rhyme label="d" type="m">keer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Tot U, wilt my <rhyme label="b" type="f" corresp="#s3:l2">ontladen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Neemt voor u kind my <rhyme label="c" type="m" corresp="#s3:l3">an</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daer is gena by <rhyme label="a" type="m">U</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghy sult mijn ziel <rhyme label="b" type="f">ontfermen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghy sult my wasschen <rhyme label="c" type="m">af</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">'k Heb gewacht // dagh en <rhyme label="d" type="m">nacht</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dat Ghy my soudt <rhyme label="b" type="f" corresp="#s4:l2">erbermen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En niet soud wesen <rhyme label="c" type="m" corresp="#s4:l3">straf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">5.Ick heb na uwe <rhyme label="a" type="m">Wet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Na d' Wet van uw' <rhyme label="b" type="f">genade</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Gewacht ten allen <rhyme label="c" type="m">tijd</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Geeft dat ick // stof en <rhyme label="d" type="m">slick</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">My in u bloed mach <rhyme label="b" type="f" corresp="#s5:l2">baden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van sonden my <rhyme label="c" type="m" corresp="#s5:l3">bevrijdt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al was mijn ziel <rhyme label="a" type="m">beswaert</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met schroomelijcke <rhyme label="b" type="f">sonden</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Nochtans behiel ick <rhyme label="c" type="m">hoop</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Als mijn hart // was <rhyme label="d" type="m">verwart</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Met doodelijcke <rhyme label="b" type="f" corresp="#s6:l2">wonden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Naem ick tot U mijn <rhyme label="c" type="m" corresp="#s6:l3">loop</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Soo moet oock Isr'el <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hy moet in God staechs <rhyme label="b" type="f">hoopen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Van 's morgens tot het <rhyme label="c" type="m">endt</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">In sijn noodt // in sijn <rhyme label="d" type="m">dood</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Moet hy tot Christum <rhyme label="b" type="f" corresp="#s7:l2">loopen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Van wanhoop <rhyme label="c" type="m" corresp="#s7:l3">ongeschent</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Want daer <rhyme label="a" type="m">bermhartigheyd</rhyme></l>
        <l id="s8:l2" met="3" type="-+">By God is staechs te <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En lossinghs <rhyme label="c" type="m">overvloed</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">En Hy sal // van haer <rhyme label="d" type="m">val</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Verlossen sijne <rhyme label="b" type="f" corresp="#s8:l2">vrinden</rhyme>:</l>
        <l id="s8:l6" met="3" type="-+">Daerom weest wel <rhyme label="c" type="m" corresp="#s8:l3">gemoedt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Heer Jesu! geeft de <rhyme label="a" type="m">rust</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Aen all' die zijn hier <rhyme label="b" type="f">onder</rhyme></l>
        <l id="s9:l3" met="3" type="-+">In 't Vagevyers <rhyme label="c" type="m">torment</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Reyckt uw' hand // blust haer <rhyme label="d" type="m">brand</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Schort op des gramschaps <rhyme label="b" type="f" corresp="#s9:l2">Donder</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Uw' oogen tot Haer <rhyme label="c" type="m" corresp="#s9:l3">wendt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
