  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Het eerste <rhyme label="a" type="m">gebodt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Eer Adam was <rhyme label="b" type="f">beschuldicht</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Beual hem <rhyme label="a" type="m" corresp="#s1:l1">Godt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wast en <rhyme label="b" type="f" corresp="#s1:l2">vermenichuuldicht</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Dit is v lot / dit is v <rhyme label="a" type="m" corresp="#s1:l1">lot</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dus elck een volch <rhyme label="b" type="f" corresp="#s1:l2">gheduldicht</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Het eerste <rhyme label="a" type="m" corresp="#s1:l1">gebodt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Gods eyghen <rhyme label="a" type="m">aert</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Streck[..] om te <rhyme label="b" type="f">genereren</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Die ons <rhyme label="a" type="m" corresp="#s2:l1">verclaert</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat wy souden <rhyme label="b" type="f" corresp="#s2:l2">vermeren</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Dus v vergaert / dus v <rhyme label="a" type="m" corresp="#s2:l1">vergaert</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En volcht met reyn <rhyme label="b" type="f" corresp="#s2:l2">begeeren</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Gods eyghen <rhyme label="a" type="m" corresp="#s2:l1">aert</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Gheen meerder <rhyme label="a" type="m">vreucht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dan twe te saemen <rhyme label="b" type="f">paeren</rhyme></l>
        <l id="s3:l3" met="2" type="-+">In eer en <rhyme label="a" type="m" corresp="#s3:l1">deucht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Als siel en lyf <rhyme label="b" type="f" corresp="#s3:l2">vergaeren</rhyme></l>
        <l id="s3:l5" met="4" type="-+">By een geueucht / by een <rhyme label="a" type="m" corresp="#s3:l1">geueucht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Als sy eendracht <rhyme label="b" type="f" corresp="#s3:l2">beuaeren</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Gheen meerder <rhyme label="a" type="m" corresp="#s3:l1">vreucht</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ter werelt <rhyme label="a" type="m">wyt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Vintmen geen meerder <rhyme label="b" type="f">lyen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Als twist en <rhyme label="a" type="m" corresp="#s4:l1">spyt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Man en wyf doen <rhyme label="b" type="f" corresp="#s4:l2">pertyen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Dus v vermyt / dus v <rhyme label="a" type="m" corresp="#s4:l1">vermyt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Steltse al omme <rhyme label="b" type="f" corresp="#s4:l2">besyen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Ter werelt <rhyme label="a" type="m" corresp="#s4:l1">wyt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Reyn liefde <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Moet door liefd syn <rhyme label="b" type="f">getoghen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Des menschen <rhyme label="a" type="m" corresp="#s5:l1">gemoet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Can gheen gewelt <rhyme label="b" type="f" corresp="#s5:l2">gedoghen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Alst lichaem doet / alst lichaem <rhyme label="a" type="m" corresp="#s5:l1">doet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dus draecht nae v <rhyme label="b" type="f" corresp="#s5:l2">vermoghen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Reyn liefde <rhyme label="a" type="m" corresp="#s5:l1">soet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Het <rhyme label="a" type="m">fondament</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Des huwelyckx <rhyme label="b" type="f">gepresen</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Is goet en <rhyme label="a" type="m" corresp="#s6:l1">jent</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dats liefde <rhyme label="b" type="f" corresp="#s6:l2">wtgeleesen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Tsy v bekent / tsy v <rhyme label="a" type="m" corresp="#s6:l1">bekent</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Als liefd door liefd mach <rhyme label="b" type="f" corresp="#s6:l2">wesen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Het <rhyme label="a" type="m" corresp="#s6:l1">fondament</rhyme></l>
      </lg>
    </lg>
  </text>
