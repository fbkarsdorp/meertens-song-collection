  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Saligh ist der <rhyme label="a" type="m">tagh</rhyme></l>
        <l id="s1:l2" met="4" type="-+">der mir das gl ck verlehenndtt <rhyme label="a" type="m" corresp="#s1:l1">hat</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">vnnd hatt geholffenn <rhyme label="b" type="m">mir</rhyme>  /</l>
        <l id="s1:l4" met="4" type="-+">das ich hertzlieb wieder mb bin khommen z  <rhyme label="b" type="m" corresp="#s1:l3">dir</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Jch danckenn der st <rhyme label="a" type="m">ndt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">darz  hertzlieb dienenn roter m <rhyme label="a" type="m" corresp="#s2:l1">ndt</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">die antwortt gab sei mir <rhyme label="b" type="m">baldtt</rhyme> /</l>
        <l id="s2:l4" met="4" type="-+">die dir vnnd mir ist woll <rhyme label="b" type="m" corresp="#s2:l3">bekantt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die frag mich eich nitt en <rhyme label="a" type="m">moth</rhyme>  /</l>
        <l id="s3:l2" met="4" type="-+">Es seindt a ch vill der bösenn le <rhyme label="a" type="m" corresp="#s3:l1">th</rhyme> /</l>
        <l id="s3:l3" met="4" type="-+">Die s lch gl ck verg nnett <rhyme label="b" type="m">han</rhyme>  /</l>
        <l id="s3:l4" met="4" type="-+">noch m ssen selbes scha wenn <rhyme label="b" type="m" corresp="#s3:l3">ahn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ob ich nitt sei bei <rhyme label="a" type="m">dir</rhyme> /</l>
        <l id="s4:l2" met="4" type="-+">Vnnd d  hertzlieb bei <rhyme label="a" type="m" corresp="#s4:l1">mir</rhyme> /</l>
        <l id="s4:l3" met="4" type="-+">so gib deinenn willenn <rhyme label="b" type="m">darjn</rhyme> /</l>
        <l id="s4:l4" met="4" type="-+">Laß mich dir hertzlieb befolhenn <rhyme label="b" type="m" corresp="#s4:l3">sein</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sei fr mb vnnd <rhyme label="a" type="m">frei</rhyme> /</l>
        <l id="s5:l2" met="4" type="-+">F rchtt Gott bewar dein ehr <rhyme label="a" type="m" corresp="#s5:l1">darbei</rhyme> /</l>
        <l id="s5:l3" met="4" type="-+">bis daß Gott schickett die <rhyme label="b" type="m">zeit</rhyme> /</l>
        <l id="s5:l4" met="4" type="-+">Die dich vnnd mich hertzlieb <rhyme label="b" type="m" corresp="#s5:l3">erfrewett</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Haltt fest an gottes <rhyme label="a" type="m">wortt</rhyme> /</l>
        <l id="s6:l2" met="4" type="-+">Das bitt ich dich mein hochster <rhyme label="a" type="m" corresp="#s6:l1">hortt</rhyme></l>
        <l id="s6:l3" met="4" type="-+">bleiff fromb vnnd trew bei <rhyme label="b" type="m">myr</rhyme> /</l>
        <l id="s6:l4" met="4" type="-+">Jch will baldtt wieder mb khommen z  <rhyme label="b" type="m" corresp="#s6:l3">dir</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">A ch d  mein Edler <rhyme label="a" type="m">schatz</rhyme> /</l>
        <l id="s7:l2" met="4" type="-+">an dich gedenck ich tagh vnnd <rhyme label="a" type="m" corresp="#s7:l1">nachtt</rhyme> /</l>
        <l id="s7:l3" met="4" type="-+">vnnd kan vergessen deiner <rhyme label="b" type="m">nitt</rhyme> /</l>
        <l id="s7:l4" met="4" type="-+">Mein hertz hatt sich z  dir <rhyme label="b" type="m" corresp="#s7:l3">gerichtt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">An dir  vertzagh ich <rhyme label="a" type="m">nichtt</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Dar mb iß dieß liedlein <rhyme label="a" type="m" corresp="#s8:l1">gedichtt</rhyme> /</l>
        <l id="s8:l3" met="4" type="-+">vmb dienenttwillenn <rhyme label="b" type="m">erdacht</rhyme> /</l>
        <l id="s8:l4" met="4" type="-+">Ade hertzlieb z  ill g etter <rhyme label="b" type="m" corresp="#s8:l3">nachtt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
