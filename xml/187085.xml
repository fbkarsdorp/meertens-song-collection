  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O	heldere <rhyme label="a" type="m">Morgen-son</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">Verwarmt de koele <rhyme label="a" type="m" corresp="#s1:l1">bron</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">U Lieffelyke <rhyme label="b" type="f">straalen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Die gy doet <rhyme label="b" type="f" corresp="#s1:l3">daalen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Op 't weeldrig aardrijk <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Voldoen mijn lust en <rhyme label="c" type="m" corresp="#s1:l5">begeer</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Natte bedoude <rhyme label="a" type="m">Groen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Moet gy haar wasdom <rhyme label="a" type="m" corresp="#s2:l1">voldoen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">U koesterende <rhyme label="b" type="f">Vuure</rhyme>;</l>
        <l id="s2:l4" met="2" type="-+">Dwingt de <rhyme label="b" type="f" corresp="#s2:l3">Natuure</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">'t Bedruppelde telg en <rhyme label="c" type="m">blad</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Word door u kragtig <rhyme label="c" type="m" corresp="#s2:l5">omvat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Bloempjes buigen haar <rhyme label="a" type="m">neer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Verbryden en meld u <rhyme label="a" type="m" corresp="#s3:l1">eer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Elk kruytje op sijn <rhyme label="b" type="f">wyse</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">U bloeijende <rhyme label="b" type="f" corresp="#s3:l3">pryse</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zelfs het klaverryke <rhyme label="c" type="m">veld</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gestadig u lof <rhyme label="c" type="m" corresp="#s3:l5">vermeld</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'t Kweelende <rhyme label="a" type="m">Pluimgediert</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat op haar Veder <rhyme label="a" type="m" corresp="#s4:l1">zwiert</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Des 's morgens u <rhyme label="b" type="f">prysen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">En u eer <rhyme label="b" type="f" corresp="#s4:l3">bewysen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zelfs het redeloose <rhyme label="c" type="m">Vee</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die roemt u luister met <rhyme label="c" type="m" corresp="#s4:l5">vreÃª</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O son gy rijst in 't <rhyme label="a" type="m">Oost</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">U glans nimmer <rhyme label="a" type="m" corresp="#s5:l1">verbloost</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gy daalt in het <rhyme label="b" type="f">Westen</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Dan duykt gy in u <rhyme label="b" type="f" corresp="#s5:l3">vesten</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Tot aan de <rhyme label="c" type="m">dageraat</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Als ge uit u kameren <rhyme label="c" type="m" corresp="#s5:l5">gaat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
