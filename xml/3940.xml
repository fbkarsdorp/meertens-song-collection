    <melody>Aanhoor toch mijn geklaag gij ruiters</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">REyn maechdelijc <rhyme label="a" type="m">aenscijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghi hemelsce <rhyme label="b" type="f">coninghinne</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ghi staet int herte <rhyme label="a" type="m" corresp="#s1:l1">mijn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Altijt sonder eenich <rhyme label="a" type="m" corresp="#s1:l1">termijn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">V eygen wil ick <rhyme label="a" type="m" corresp="#s1:l1">sijn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Bedwingt dat herte <rhyme label="a" type="m" corresp="#s1:l1">mijn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">En schenct mi doch v <rhyme label="b" type="f" corresp="#s1:l2">minne</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O vruchtbaer honich <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Der maechden <rhyme label="b" type="f">keyserinne</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Neemt mi in v <rhyme label="a" type="m" corresp="#s2:l1">behoet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Al in des werelts <rhyme label="a" type="m" corresp="#s2:l1">vloet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In minen <rhyme label="a" type="m" corresp="#s2:l1">teghenspoet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Maect mi der dinghen <rhyme label="a" type="m" corresp="#s2:l1">vroet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En schenct mi doch v <rhyme label="b" type="f" corresp="#s2:l2">minne</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">In v rust mijn <rhyme label="a" type="m">verlaet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">O wijste vrou van <rhyme label="b" type="f">sinne</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Daer alle mijn hoep aen <rhyme label="a" type="m" corresp="#s3:l1">staet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">O schoonste <rhyme label="a" type="m" corresp="#s3:l1">dagheraet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Weest doch mijn <rhyme label="a" type="m" corresp="#s3:l1">aduocaet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Al in des hoochsten <rhyme label="a" type="m" corresp="#s3:l1">raet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Ende schect mi doch v <rhyme label="b" type="f" corresp="#s3:l2">minne</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Schoon bloemken <rhyme label="a" type="m">excellent</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wiens duecht ic seer <rhyme label="b" type="f">beminne</rhyme></l>
        <l id="s4:l3" met="3" type="-+">O edelste Maria <rhyme label="a" type="m" corresp="#s4:l1">yent</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Int liefste <rhyme label="a" type="m" corresp="#s4:l1">parlement</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Als ick sal sijn <rhyme label="a" type="m" corresp="#s4:l1">present</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Solueert mijn <rhyme label="a" type="m" corresp="#s4:l1">argument</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">En schenct doch mi v <rhyme label="b" type="f" corresp="#s4:l2">minne</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maria roose <rhyme label="a" type="m">root</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Helpt mi ten hemel <rhyme label="b" type="f">binnen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Maect mi van sonden <rhyme label="a" type="m" corresp="#s5:l1">bloot</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Helpt mi in alder <rhyme label="a" type="m" corresp="#s5:l1">noot</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Wacht mi voer svyants <rhyme label="a" type="m" corresp="#s5:l1">stoot</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Och coemt doch in mijn <rhyme label="a" type="m" corresp="#s5:l1">doot</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Ende schenct mi doch v <rhyme label="b" type="f" corresp="#s5:l2">minne</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die dit liedeken heeft <rhyme label="a" type="m">gemaect</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Eerstmael int <rhyme label="b" type="f">beghinne</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Is in die cap <rhyme label="a" type="m" corresp="#s6:l1">gheraect</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ende heeft die werelt <rhyme label="a" type="m" corresp="#s6:l1">versaect</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Van eenen predikaer <rhyme label="a" type="m" corresp="#s6:l1">ontscaect</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Princersse doch op mi <rhyme label="a" type="m" corresp="#s6:l1">waect</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ende schenct mi doch v <rhyme label="b" type="f" corresp="#s6:l2">minne</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
