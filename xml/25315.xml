<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25315</id>
    <date>
      <year-start>1487</year-start>
      <year-end>1487</year-end>
    </date>
    <tags>
      <tag>christmas song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Die coutste nacht die oit <rhyme label="a" type="m">gewaert</rhyme></l>
        <l id="s1:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dat was doen jhesus geboren <rhyme label="a" type="m" corresp="#s1:l1">was</rhyme></l>
        <l id="s1:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s1:l2">gracia</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hij waert geleit in einre <rhyme label="a" type="m">cribben</rhyme></l>
        <l id="s2:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Ein luttel hoys dat was sijn <rhyme label="a" type="m" corresp="#s2:l1">bedde</rhyme></l>
        <l id="s2:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s2:l2">gracia</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Jhesus is een suete <rhyme label="a" type="m">kernne</rhyme></l>
        <l id="s3:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Daer omme craken wi dese noetkens <rhyme label="a" type="m" corresp="#s3:l1">geern</rhyme></l>
        <l id="s3:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s3:l2">gracia</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Jhesus is eine suete <rhyme label="a" type="m">mosschaet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Daer omme sullen wij die sunden <rhyme label="a" type="m" corresp="#s4:l1">laten</rhyme></l>
        <l id="s4:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s4:l2">gracia</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maria sette Jhesus op haeren <rhyme label="a" type="m">scoet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Sij gaff hem einen appel <rhyme label="a" type="m" corresp="#s5:l1">roet</rhyme></l>
        <l id="s5:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s5:l2">gracia</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maria soude jhesus ein papke <rhyme label="a" type="m">maken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Her joseph ginck die stexkens <rhyme label="a" type="m" corresp="#s6:l1">rapen</rhyme></l>
        <l id="s6:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s6:l2">gracia</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maria nayde en sij <rhyme label="a" type="m">span</rhyme></l>
        <l id="s7:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Daer sij her jhesus sbroet mede <rhyme label="a" type="m" corresp="#s7:l1">wan</rhyme></l>
        <l id="s7:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s7:l2">gracia</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maeria naeyde einen <rhyme label="a" type="m">naet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Her jhesus draeide haer den <rhyme label="a" type="m" corresp="#s8:l1">draet</rhyme></l>
        <l id="s8:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s8:l2">gracia</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maeria roecte einen <rhyme label="a" type="m">rock</rhyme></l>
        <l id="s9:l2" met="3" type="-+">O virgo <rhyme label="b" type="m">Maria</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Her jhesus voer zo hemel <rhyme label="a" type="m" corresp="#s9:l1">op</rhyme></l>
        <l id="s9:l4" met="8" type="+-">Domine fulgencia ave plena <rhyme label="b" type="m" corresp="#s9:l2">gracia</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
