  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ALs by <rhyme label="a" type="m">gevall</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Onlangs, mijn <rhyme label="b" type="f">Beminde</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Ging door het <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Bloemp jes t'samen <rhyme label="b" type="f" corresp="#s1:l2">binden</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Siet, onder 't <rhyme label="c" type="f">lesen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">VVoud't juyst soo <rhyme label="c" type="f" corresp="#s1:l5">vvesen</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Dat den <rhyme label="d" type="m">Minne-Guyt</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Syn vlammig <rhyme label="c" type="f" corresp="#s1:l5">vvesen</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Schuylden onder 't <rhyme label="d" type="m" corresp="#s1:l7">kruyd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hy, die een <rhyme label="a" type="m">lust</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Schept in vreemde <rhyme label="b" type="f">grillen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Nam daer syn <rhyme label="a" type="m" corresp="#s2:l1">rust</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Om een pijl te <rhyme label="b" type="f" corresp="#s2:l2">spillen</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Maer d'arme <rhyme label="c" type="f">Iongen</rhyme></l>
        <l id="s2:l6" met="2" type="-+">VVierd selfs <rhyme label="c" type="f" corresp="#s2:l5">besprongen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Van mijn <rhyme label="d" type="m">ROSELIIN</rhyme>;</l>
        <l id="s2:l8" met="2" type="-+">En most, <rhyme label="c" type="f" corresp="#s2:l5">gedvvongen</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Haer gevangen <rhyme label="d" type="m" corresp="#s2:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Want sy dien <rhyme label="a" type="m">Gast</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Met haer blommen <rhyme label="b" type="f">boeyde</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">En krÃªegh hem <rhyme label="a" type="m" corresp="#s3:l1">vast</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Schoon hy angstigh <rhyme label="b" type="f" corresp="#s3:l2">spoeyde</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Met syne <rhyme label="c" type="f">pennen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Het quaet <rhyme label="c" type="f" corresp="#s3:l5">t'ontrennen</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Daer hy, <rhyme label="d" type="m">naderhand</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Niet kost <rhyme label="c" type="f" corresp="#s3:l5">ontvvennen</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Van dien soeten <rhyme label="d" type="m" corresp="#s3:l7">band</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">VVant als hy <rhyme label="a" type="m">sag</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Hare lodder <rhyme label="b" type="f">OOGEN</rhyme>;</l>
        <l id="s4:l3" met="2" type="-+">Haer soeten <rhyme label="a" type="m" corresp="#s4:l1">lach</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">En syn vreemt <rhyme label="b" type="f" corresp="#s4:l2">vermogen</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">O! <rhyme label="c" type="f">Overschoone</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Hier vvill ick <rhyme label="c" type="f" corresp="#s4:l5">vvonen</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Riep dien kleynen <rhyme label="d" type="m">Guyt</rhyme>;</l>
        <l id="s4:l8" met="2" type="-+">Mijns Moeders <rhyme label="c" type="f" corresp="#s4:l5">Throone</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Heeft nu met my <rhyme label="d" type="m" corresp="#s4:l7">uyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
