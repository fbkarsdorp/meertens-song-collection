  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ACh Vader vol Genaed' end <rhyme label="a" type="m">Eer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Aff wasschen wilt ons altoos <rhyme label="a" type="m" corresp="#s1:l1">meer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die sondig' Aert des <rhyme label="b" type="f">Vleesches</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Om dijns Heylandes Bloet so <rhyme label="c" type="m">puer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Maeckt ons een Nieuwe <rhyme label="c" type="m" corresp="#s1:l4">Creatuer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door Wercking' dijnes <rhyme label="b" type="f" corresp="#s1:l3">Gheestes</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Dat wy recht als die Doop <rhyme label="d" type="m">beduyd</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Daedich bevonden Christen <rhyme label="d" type="m" corresp="#s1:l7">Luyd</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">In dijnder Huld' <rhyme label="e" type="f">beclijven</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Als ons benaut des Vleesches <rhyme label="f" type="m">Last</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Aen dijn Verbondt ons houden <rhyme label="f" type="m" corresp="#s1:l10">vast</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Eewich dijn kinder <rhyme label="e" type="f" corresp="#s1:l9">blijven</rhyme>.</l>
      </lg>
    </lg>
  </text>
