      <year-start>1783</year-start>
      <year-end>1783</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Aangename dagen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AEngename <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die men in Alkmaer <rhyme label="b" type="m">slyt</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Gy kunt een ieder <rhyme label="a" type="f" corresp="#s1:l1">behagen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Door uwe <rhyme label="b" type="m" corresp="#s1:l2">vrolykheyt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Daer 't Roosje by de <rhyme label="c" type="f">doorne</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">Een yder zyn Liefje <rhyme label="d" type="m">zie</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Maer gy kunt my niet <rhyme label="c" type="f" corresp="#s1:l5">bekoren</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Want myn Phielis is hier <rhyme label="d" type="m" corresp="#s1:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vermakelyk avond <rhyme label="a" type="f">Uur</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Daer de Koffie en <rhyme label="b" type="m">Thee-kan</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Banket en <rhyme label="a" type="f" corresp="#s2:l1">Confituur</rhyme>?</l>
        <l id="s2:l4" met="4" type="-+">Werd gebruikt van meenig <rhyme label="b" type="m" corresp="#s2:l2">Man</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daer 't Roosje by de <rhyme label="c" type="f">doorne</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Een yder zyn Liefje <rhyme label="d" type="m">ziet</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Maer gy kunt my niet <rhyme label="c" type="f" corresp="#s2:l5">bekoren</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Want myn Phielis is hier <rhyme label="d" type="m" corresp="#s2:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Steygert op 't <rhyme label="a" type="f">Water</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Langs de vesten van de <rhyme label="b" type="m">Stad</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daer de Visjes in 't <rhyme label="a" type="f" corresp="#s3:l1">Water</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Langes de Golven <rhyme label="b" type="m" corresp="#s3:l2">trat</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Niets kan de Vreugde <rhyme label="c" type="f">stooren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">In 't vermakelyk <rhyme label="d" type="m">gebiet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Maer gy kunt my niet <rhyme label="c" type="f" corresp="#s3:l5">bekoren</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Want myn Phielis is hier <rhyme label="d" type="m" corresp="#s3:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De teedere witte <rhyme label="a" type="f">Schapen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die daer in de weiden <rhyme label="b" type="m">gaen</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">En droog staen te <rhyme label="a" type="f" corresp="#s4:l1">gapen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Terwyl men uw ziet <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Daer 't Lammetje zig laet <rhyme label="c" type="f">horen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Met een blyen <rhyme label="d" type="m">stem</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Maer gy kunt my niet <rhyme label="c" type="f" corresp="#s4:l5">bekoren</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Want myn Phielis is hier <rhyme label="d" type="m" corresp="#s4:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Helaes nu moet ik <rhyme label="a" type="f">scheide</rhyme>!</l>
        <l id="s5:l2" met="4" type="-+">Van die schone <rhyme label="b" type="m">Stad</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Daer ik in korte <rhyme label="a" type="f" corresp="#s5:l1">tyden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Veel vreugd in heb <rhyme label="b" type="m" corresp="#s5:l2">gehad</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Daer het Roozie by de <rhyme label="c" type="f">doorne</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Een yder zijn Liefje <rhyme label="d" type="m">ziet</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Daerom so moet ik <rhyme label="c" type="f" corresp="#s5:l5">scheiden</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Want myn Phielis is hier <rhyme label="d" type="m" corresp="#s5:l6">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
