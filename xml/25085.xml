  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Maria es eene sterre <rhyme label="a" type="m">scone</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">Die ons verlicht aenden <rhyme label="a" type="m" corresp="#s1:l1">trone</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Wie haer aenbeet hi heeft te <rhyme label="a" type="m" corresp="#s1:l1">lone</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Ihesus christus marien <rhyme label="a" type="m" corresp="#s1:l1">sone</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s1:l5">prophecie</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s1:l5">marie</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s1:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maria es eene <rhyme label="a" type="m">dageraert</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Die vor der middernacht op <rhyme label="a" type="m" corresp="#s2:l1">staet</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Wie haer aenbeedt hi heeft <rhyme label="a" type="m" corresp="#s2:l1">aflaet</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Van alder sunderliker <rhyme label="a" type="m" corresp="#s2:l1">daet</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s2:l5">prophecie</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s2:l5">marie</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s2:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maria es eene <rhyme label="a" type="m">triniteyt</rhyme>.</l>
        <l id="s3:l2" met="4" type="-+">Moeder van alder <rhyme label="a" type="m" corresp="#s3:l1">ontfermicheyt</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Si es soe vol alder <rhyme label="a" type="m" corresp="#s3:l1">heilicheyt</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Dats ons een groote <rhyme label="a" type="m" corresp="#s3:l1">salicheyt</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s3:l5">prophecie</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s3:l5">marie</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s3:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maria es eene volle <rhyme label="a" type="m">vloet</rhyme>.</l>
        <l id="s4:l2" met="4" type="-+">Daer god aen nam vleesch ende <rhyme label="a" type="m" corresp="#s4:l1">bloet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Dat sal den genen wesen <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Die dienen om dat eewege <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s4:l5">prophecie</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s4:l5">marie</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s4:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maria es eene rose sonder <rhyme label="a" type="m">doren</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Die god tot eender moeder heeft <rhyme label="a" type="m" corresp="#s5:l1">vercoren</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Eer ihesus christus was <rhyme label="a" type="m" corresp="#s5:l1">geboren</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Was dese werelt al <rhyme label="a" type="m" corresp="#s5:l1">verloren</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s5:l5">prophecie</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s5:l5">marie</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s5:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Her ioseph viel neder op sijn <rhyme label="a" type="m">knien</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Hine conste dat wonder niet <rhyme label="a" type="m" corresp="#s6:l1">aensien</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Dat kint dat inde crebbe <rhyme label="a" type="m" corresp="#s6:l1">lach</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Hi wiste wel dat die gods sone <rhyme label="a" type="m" corresp="#s6:l1">was</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s6:l5">prophecie</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s6:l5">marie</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s6:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die ingelen die saten daer alle <rhyme label="a" type="m">bi</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Met grooter vrouden wiechden <rhyme label="a" type="m" corresp="#s7:l1">si</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Met wonder waren si <rhyme label="a" type="m" corresp="#s7:l1">beuaen</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Dat haren scepper soe was <rhyme label="a" type="m" corresp="#s7:l1">gedaen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s7:l5">prophecie</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s7:l5">marie</rhyme></l>
        <l id="s7:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s7:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Die ingelen songen alle <rhyme label="a" type="m">sanctus</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Die heilege geest es hier int <rhyme label="a" type="m" corresp="#s8:l1">huys</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Geboren van eender ionger <rhyme label="a" type="m" corresp="#s8:l1">maecht</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Die onsen vader soe wel <rhyme label="a" type="m" corresp="#s8:l1">behaecht</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s8:l5">prophecie</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s8:l5">marie</rhyme></l>
        <l id="s8:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s8:l5">nie</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Sijt willecome her <rhyme label="a" type="m">iongelinc</rhyme>.</l>
        <l id="s9:l2" met="4" type="-+">Die ons van bouen es <rhyme label="a" type="m" corresp="#s9:l1">gesint</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Der reynder maget marien <rhyme label="a" type="m" corresp="#s9:l1">kint</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Ende alder yngelen <rhyme label="a" type="m" corresp="#s9:l1">coninc</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Kinder nv loeft die maecht <rhyme label="b" type="m">marie</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Si heeft vervult die <rhyme label="b" type="m" corresp="#s9:l5">prophecie</rhyme></l>
        <l id="s9:l7" met="4" type="-+">Si droech een kint die maecht <rhyme label="b" type="m" corresp="#s9:l5">marie</rhyme></l>
        <l id="s9:l8" met="4" type="-+">Dat wonder en gesciet <rhyme label="b" type="m" corresp="#s9:l5">nie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
