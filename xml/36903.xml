  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">d'AEnghename <rhyme label="a" type="f">tyden</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Van den koelen <rhyme label="b" type="m">Mey</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Die de Jeught <rhyme label="a" type="f" corresp="#s1:l1">verblyden</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Komen ons nu <rhyme label="b" type="m" corresp="#s1:l2">by</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">'t Vogheltjen gaet <rhyme label="c" type="f">singhen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Het Lammeken gaet <rhyme label="c" type="f" corresp="#s1:l5">springhen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">'t Groen en jeughdigh kruyt komt <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Siet hoe alle <rhyme label="c" type="f" corresp="#s1:l5">dinghen</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Uuytter aerden <rhyme label="d" type="m" corresp="#s1:l7">spruyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Het suer winters <rhyme label="a" type="f">weder</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Is voor-by <rhyme label="b" type="m">ghegaen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En den Mey komt <rhyme label="a" type="f" corresp="#s2:l1">weder</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Met den Somer <rhyme label="b" type="m" corresp="#s2:l2">aen</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">d'Incarnate <rhyme label="c" type="f">Rosen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Staen nu soo schoon en <rhyme label="c" type="f" corresp="#s2:l5">blosen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Heel den Hof die groeyt en <rhyme label="d" type="m">bloeyt</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Daer ghy blyft <rhyme label="c" type="f" corresp="#s2:l5">bevrosen</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">En gheen' liefd' en <rhyme label="d" type="m" corresp="#s2:l7">voeyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Hoort de lieve <rhyme label="a" type="f">Dieren</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Heel verheught van <rhyme label="b" type="m">gheest</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Soetjens <rhyme label="a" type="f" corresp="#s3:l1">tierelieren</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Op hun <rhyme label="b" type="m" corresp="#s3:l2">Bruyloft-feest</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Hoort het <rhyme label="c" type="f">Nachtegaeltjen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat vriendelyck <rhyme label="c" type="f" corresp="#s3:l5">Ćoraeltjen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">'t Singht met blyden moet soet, <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">'t Singht een <rhyme label="c" type="f" corresp="#s3:l5">Madrigaeltjen</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Dat ons vreught <rhyme label="d" type="m" corresp="#s3:l7">aen-doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">In ghen' Wey sit <rhyme label="a" type="f">Tyter</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Met syn' <rhyme label="b" type="m">Herderin</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Hy speelt op syn' <rhyme label="a" type="f" corresp="#s4:l1">Cyther</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Sy singht van de <rhyme label="b" type="m" corresp="#s4:l2">min</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Laet ons met ons <rhyme label="c" type="f">beyen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ons oock soo gaen <rhyme label="c" type="f" corresp="#s4:l5">vermeyen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Spelend' op een Riet een <rhyme label="d" type="m">Liedt</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">In de groene <rhyme label="c" type="f" corresp="#s4:l5">Weyen</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Die ghy ginder <rhyme label="d" type="m" corresp="#s4:l7">siet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Uwe groene <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s5:l2" met="3" type="+-">En den koelen <rhyme label="b" type="m">Mey</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Zyn nu goet om <rhyme label="a" type="f" corresp="#s5:l1">paren</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Schenckt uw' Trouw aen <rhyme label="b" type="m" corresp="#s5:l2">my</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Laet my u ô <rhyme label="c" type="f">Schoone</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met d'eerste Rooskens <rhyme label="c" type="f" corresp="#s5:l5">kroonen</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Die de soete Lent ons <rhyme label="d" type="m">sendt</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Om u soo te <rhyme label="c" type="f" corresp="#s5:l5">thoonen</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Wat Bloem dat ghy <rhyme label="d" type="m" corresp="#s5:l7">bent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Vader Adam <rhyme label="a" type="f">paerden</rhyme></l>
        <l id="s6:l2" met="3" type="+-">In de soete <rhyme label="b" type="m">Lent</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Als de vruchtbaer <rhyme label="a" type="f" corresp="#s6:l1">Aerden</rhyme></l>
        <l id="s6:l4" met="3" type="+-">d'Eerste Bloemen <rhyme label="b" type="m" corresp="#s6:l2">sendt</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Wildy Bruyloft <rhyme label="c" type="f">houwen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">O Bloem van schoone <rhyme label="c" type="f" corresp="#s6:l5">Vrouwen</rhyme></l>
        <l id="s6:l7" met="4" type="-+">In den koelen Mey met <rhyme label="d" type="m">my</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Schenckt my nu uw' <rhyme label="c" type="f" corresp="#s6:l5">Trouwe</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Maeckt myn hertjen <rhyme label="d" type="m" corresp="#s6:l7">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
