<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8737</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Laatst was ik met mijn Rozemond</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">LEst was ick met mijn <rhyme label="a" type="m">Rosemondt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ghetreden in een <rhyme label="a" type="m" corresp="#s1:l1">Soen-verbondt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Van duysendt Kusjes, min noch <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Te gheven en te krijghen <rhyme label="b" type="m" corresp="#s1:l3">weer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'t Is waer mijn Lief ghy hebt <rhyme label="a" type="m">voldaen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">'t Ghetal heb ick van u <rhyme label="a" type="m" corresp="#s2:l1">ontfaen</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Maer waer is erghens een <rhyme label="b" type="m">ghetal</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Daer Liefd' sich mee vernoeghen <rhyme label="b" type="m" corresp="#s2:l3">sal</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waer saghmen oyt een <rhyme label="a" type="m">Kooren-velt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Daer Aer, en Stoppel was <rhyme label="a" type="m" corresp="#s3:l1">ghetelt</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">Waer sagh men kenlijck tal van <rhyme label="b" type="m">Graen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Zijn leven op den Acker <rhyme label="b" type="m" corresp="#s3:l3">staen</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Waer vondtmen oyt soo slechten <rhyme label="a" type="m">Sot</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Die immermeer den Wijngaert <rhyme label="a" type="m" corresp="#s4:l1">Godt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Den ghever van het Eele <rhyme label="b" type="m">Nat</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Om een ghetal van Druyven <rhyme label="b" type="m" corresp="#s4:l3">badt</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Waer saghmen sulcken plompen <rhyme label="a" type="m">Boer</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Soo groven Kinckel, sulcken <rhyme label="a" type="m" corresp="#s5:l1">Loer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die opt zijn handen hief tot <rhyme label="b" type="m">Pan</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En riep om duysendt Byen <rhyme label="b" type="m" corresp="#s5:l3">an</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wie isser, die de druppels <rhyme label="a" type="m">telt</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Wanneer het Reghent op het <rhyme label="a" type="m" corresp="#s6:l1">Velt</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">En die 't ghetal van 't goedtje <rhyme label="b" type="m">kendt</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Dat van den Haghel werdt <rhyme label="b" type="m" corresp="#s6:l3">gheschendt</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Het zy of goedt, of <rhyme label="a" type="m">ongheval</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">De Goden senden 't sonder <rhyme label="a" type="m" corresp="#s7:l1">tal</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Iupijn die seydt, dat past my <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Dat ick mijn selfs gheen maeten <rhyme label="b" type="m" corresp="#s7:l3">stel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy dan, die oock zijt een <rhyme label="a" type="m">Goddin</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Hoe kan u komen in de <rhyme label="a" type="m" corresp="#s8:l1">sin</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dat ghy my maet in't kussen <rhyme label="b" type="m">stelt</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Daer ghy mijn suchten niet en <rhyme label="b" type="m" corresp="#s8:l3">telt</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">De bracke Traenen die ick <rhyme label="a" type="m">giet</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Mijn Rosemondt, die telje <rhyme label="a" type="m" corresp="#s9:l1">niet</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">De Kusjes (tusschen ons <rhyme label="b" type="m">ghestelt</rhyme>)</l>
        <l id="s9:l4" met="4" type="-+">Die hebje fijntjes nae <rhyme label="b" type="m" corresp="#s9:l3">ghetelt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Soo ghy de Traentjes telt in <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Soo telt oock uwe Kusjes <rhyme label="a" type="m" corresp="#s10:l1">vry</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">Maer soo ghy 't eerst wilt <rhyme label="b" type="m">overslaen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Soo laet oock 't ander tellen <rhyme label="b" type="m" corresp="#s10:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">En gheeft my Kusjes sonder <rhyme label="a" type="m">endt</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">En seght: Ick ben nu <rhyme label="a" type="m" corresp="#s11:l1">omghewent</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">V Tranen waren sonder <rhyme label="b" type="m">tal</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Mijn kussen oock soo wesen <rhyme label="b" type="m" corresp="#s11:l3">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
