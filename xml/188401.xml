<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188401</id>
    <date>
      <year-start>1801</year-start>
      <year-end>1900</year-end>
    </date>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Ten besluit, ten besluit,</l>
        <l id="s1:l2">Waarde Bruidegom en Bruid!</l>
        <l id="s1:l3">Blijven wij ons aanbevelen,</l>
        <l id="s1:l4">Om steeds in den roem te deelen</l>
        <l id="s1:l5">Uwer vriendschap; bij deze eer</l>
        <l id="s1:l6">Hebben wij geen wenschen meer.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Ten besluit, ten besluit,</l>
        <l id="s2:l2">Vrienden drinkt uw glaasjes uit,</l>
        <l id="s2:l3">Nog eens voor het laatst gedronken.</l>
        <l id="s2:l4">En vriendschapp'lijk t' zaam geklonken</l>
        <l id="s2:l5">Dat de vriendschap groei en bloei,</l>
        <l id="s2:l6">En ons aan elkandren boei.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">Ten besluit, ten besluit,</l>
        <l id="s3:l2">Waarde Bruidegom en Brnid!</l>
        <l id="s3:l3">Roept elk onzer, daar wij scheiden,</l>
        <l id="s3:l4">En ge elkand'ren zult geleiden</l>
        <l id="s3:l5">Naar het Bruidsbed, dat u wacht.</l>
        <l id="s3:l6">Goeden nacht! goeden nacht!</l>
      </lg>
    </lg>
  </text>
</song>
