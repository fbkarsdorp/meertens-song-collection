<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>163864</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hoor Kees mijn vrijer</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Wat heeft de <rhyme label="a" type="f">moode</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hier al geen gelt van <rhyme label="b" type="m">doen</rhyme>?</l>
        <l id="s1:l3" met="2" type="-+">En ook van <rhyme label="a" type="f" corresp="#s1:l1">noode</rhyme></l>
        <l id="s1:l4" met="3" type="-+">By Lien, die zonder <rhyme label="b" type="m" corresp="#s1:l2">schoen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Staeg loopen langs de <rhyme label="c" type="m">straet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En schyne <rhyme label="c" type="m" corresp="#s1:l5">inderdaed</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Net als de grootste <rhyme label="d" type="f">Heeren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En Dames vroeg en <rhyme label="c" type="m" corresp="#s1:l5">laet</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Staeg <rhyme label="d" type="f" corresp="#s1:l7">triumpheeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wilt eens <rhyme label="a" type="f">beschouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De Pruykemaekers, <rhyme label="b" type="m">hier</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Met hunne <rhyme label="a" type="f" corresp="#s2:l1">Vrouwen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe zy gaen na de <rhyme label="b" type="m" corresp="#s2:l2">zwier</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy draegd een groote <rhyme label="c" type="m">Hoed</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Twee gespes aen zyn <rhyme label="c" type="m" corresp="#s2:l5">Voed</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">'t Moet Henriquatre <rhyme label="d" type="f">weeze</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Of anders is 't niet <rhyme label="c" type="m" corresp="#s2:l5">goet</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">En meer van <rhyme label="d" type="f" corresp="#s2:l7">deze</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">'t Hair met <rhyme label="a" type="f">pomaede</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gepoeijerd na de <rhyme label="b" type="m">smaeck</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">'t Ruykt na <rhyme label="a" type="f" corresp="#s3:l1">muskaede</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dit is een schoone <rhyme label="b" type="m" corresp="#s3:l2">zaek</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Twee Tikkers, in de <rhyme label="c" type="m">zak</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Een mooije france <rhyme label="c" type="m" corresp="#s3:l5">Pak</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met fyne staele <rhyme label="d" type="f">knoope</rhyme></l>
        <l id="s3:l8" met="3" type="-+">'t Is tog maer france <rhyme label="c" type="m" corresp="#s3:l5">kak</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Zoo zy hier <rhyme label="d" type="f" corresp="#s3:l7">loopen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ziet eens haer <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik wet dog <rhyme label="b" type="m">inderdaed</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Men zalze <rhyme label="a" type="f" corresp="#s4:l1">houwen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Voor Daemes, van veel <rhyme label="b" type="m" corresp="#s4:l2">Staet</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Haer kleeding is zeer <rhyme label="c" type="m">net</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Na France smaek <rhyme label="c" type="m" corresp="#s4:l5">gezet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Het is zeer <rhyme label="d" type="f">aengenaeme</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En alles tout a <rhyme label="c" type="m" corresp="#s4:l5">fet</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Al by die <rhyme label="d" type="f" corresp="#s4:l7">Daeme</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ziet eens haer <rhyme label="a" type="f">tette</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met linte <rhyme label="b" type="m">gepalleerd</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Zoo vraey en <rhyme label="a" type="f" corresp="#s5:l1">nette</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Het hair <rhyme label="b" type="m" corresp="#s5:l2">g'acommodeerd</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Eens a le <rhyme label="c" type="m">h√©rizon</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dan weer een ander <rhyme label="c" type="m" corresp="#s5:l5">ton</rhyme></l>
        <l id="s5:l7" met="3" type="-+">De chien couche wilt <rhyme label="d" type="f">weeten</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die is ook al te <rhyme label="c" type="m" corresp="#s5:l5">bon</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Om te <rhyme label="d" type="f" corresp="#s5:l7">vergeete</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Men ziet haer <rhyme label="a" type="f">bloozen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Gestaeg aen haer <rhyme label="b" type="m">Gelaet</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Als zy met <rhyme label="a" type="f" corresp="#s6:l1">doozen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Hier loope langs de <rhyme label="b" type="m" corresp="#s6:l2">Straet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">'t Is echter maer <rhyme label="c" type="m">blanket</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Op 't aengezigt <rhyme label="c" type="m" corresp="#s6:l5">gezet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Zoo zyn zy vraey van <rhyme label="d" type="f">weezen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Maer ziet ze eens uyt 't <rhyme label="c" type="m" corresp="#s6:l5">bed</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Dan zult gy <rhyme label="d" type="f" corresp="#s6:l7">vreezen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wilt gy eens <rhyme label="a" type="f">weeten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hoe dat haer leeven <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">En wat zy <rhyme label="a" type="f" corresp="#s7:l1">Eeten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dats zeeker en <rhyme label="b" type="m" corresp="#s7:l2">gewis</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Somtyds een pot <rhyme label="c" type="m">Buljon</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Met Oolie en <rhyme label="c" type="m" corresp="#s7:l5">Unjon</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Geen vlees raeckt aen haer <rhyme label="d" type="f">tande</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Van Beuf nog van <rhyme label="c" type="m" corresp="#s7:l5">Cuchon</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Is dat geen <rhyme label="d" type="f" corresp="#s7:l7">Schande</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Zoo zyn die <rhyme label="a" type="f">France</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Paruikemaekers <rhyme label="b" type="m">hier</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Met zang en <rhyme label="a" type="f" corresp="#s8:l1">danze</rhyme></l>
        <l id="s8:l4" met="3" type="-+">En staeg met veel <rhyme label="b" type="m" corresp="#s8:l2">plyzier</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Doch met een leege <rhyme label="c" type="m">Maeg</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Och wat oen zware <rhyme label="c" type="m" corresp="#s8:l5">plaeg</rhyme>!</l>
        <l id="s8:l7" met="3" type="-+">Ik schey nu uyt met <rhyme label="d" type="f">Schryven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Ik werd van 't digten <rhyme label="c" type="m" corresp="#s8:l5">flauw</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Ik laet het <rhyme label="d" type="f" corresp="#s8:l7">blyven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
