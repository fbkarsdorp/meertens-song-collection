<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185456</id>
    <date>
      <year-start>1824</year-start>
      <year-end>1824</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Laat ons, ô gasten! vrolijk <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Op 't feest dier éénsgezinde <rhyme label="b" type="m">twee</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Het vreugdgevoel dier <rhyme label="a" type="f" corresp="#s1:l1">echtelingen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Deelt ook ons hart zijn' invloed <rhyme label="b" type="m" corresp="#s1:l2">meê</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Men wensch dat <rhyme label="c" type="f">zegen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Hunn' echt <rhyme label="c" type="f" corresp="#s1:l5">bejegen'</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Bij liefde en <rhyme label="b" type="m" corresp="#s1:l2">vreê</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wanneer, in 't bloeijen hunner <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Een minnend tweetal zich <rhyme label="b" type="m">verbindt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dan smaakt het vrucht van 't jeugdig <rhyme label="a" type="f" corresp="#s2:l1">paren</rhyme></l>
        <l id="s2:l4" met="4" type="-+">'t Geluk van 't hart dat rein <rhyme label="b" type="m" corresp="#s2:l2">bemint</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">'t Kuscht eens min <rhyme label="c" type="f">vurig</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Maar blijft <rhyme label="c" type="f" corresp="#s2:l5">gedurig</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Vrindin en <rhyme label="b" type="m" corresp="#s2:l2">vrind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zoo heeft dit paar zijn lot <rhyme label="a" type="f">verbonden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En plukt de bloemen zijner <rhyme label="b" type="m">jeugd</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Doch hoopt in later <rhyme label="a" type="f" corresp="#s3:l1">levenstonden</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Nog zaam' veréénd te zijn in <rhyme label="b" type="m" corresp="#s3:l2">vreugd</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">'t Zie gantsch zijn <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Met heil <rhyme label="c" type="f" corresp="#s3:l5">doorweven</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Gegrond op <rhyme label="b" type="m" corresp="#s3:l2">deugd</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Geniet, ô trouwe <rhyme label="a" type="f">lotgenoten</rhyme>!</l>
        <l id="s4:l2" met="4" type="-+">Al 't geen uw minnend hart <rhyme label="b" type="m">verblijdt</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Viert eens, met kroost, uit u <rhyme label="a" type="f" corresp="#s4:l1">gesproten</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Den dag toen gij veréénigd <rhyme label="b" type="m" corresp="#s4:l2">zijt</rhyme>!</l>
        <l id="s4:l5" met="2" type="-+">Oogst <rhyme label="c" type="f">allerwegen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Den hoogsten <rhyme label="c" type="f" corresp="#s4:l5">zegen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ja, bloeit <rhyme label="b" type="m" corresp="#s4:l2">altijd</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
