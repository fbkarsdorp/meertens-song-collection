    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Fortune que tu m'importunes</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Petrus Suetmans, Eustachius de Meyer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+">Sters <rhyme label="a" type="f">glinsters</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Raecken deur, en <rhyme label="a" type="f" corresp="#s1:l1">vinsters</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">En de bleecke <rhyme label="b" type="m">Maen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Hun glans sal soo lanck blyven <rhyme label="b" type="m" corresp="#s1:l3">staen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Tot Phebus <rhyme label="c" type="f">straelen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die op hun <rhyme label="c" type="f" corresp="#s1:l5">daelen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Sullen neer <rhyme label="c" type="f" corresp="#s1:l5">draelen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">En 'theel licht <rhyme label="b" type="m" corresp="#s1:l3">vergaen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+">Myn <rhyme label="a" type="f">suchten</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Clacht, en <rhyme label="a" type="f" corresp="#s2:l1">onghenuchten</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Zyn vol druck en <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die Son waer van ick wacht den <rhyme label="b" type="m" corresp="#s2:l3">scheyn</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">En wilt niet <rhyme label="c" type="f">achten</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Myn droeve <rhyme label="c" type="f" corresp="#s2:l5">klachten</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Verleyt myn <rhyme label="c" type="f" corresp="#s2:l5">trachten</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En scheynt bly te <rhyme label="b" type="m" corresp="#s2:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+"><rhyme label="a" type="f">Goddinne</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Voester van myn <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">'kBid zyt niet <rhyme label="b" type="m">verbleyt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Worden u straelen my <rhyme label="b" type="m" corresp="#s3:l3">ont-seyt</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">'kSal soo lanck <rhyme label="c" type="f">klaghen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Tot dat de <rhyme label="c" type="f" corresp="#s3:l5">plaghen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Die ick moet <rhyme label="c" type="f" corresp="#s3:l5">draghen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Worden <rhyme label="b" type="m" corresp="#s3:l3">af-gheleyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="1" type="-+">Myn <rhyme label="a" type="f">lusten</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Zyn staegh sonder <rhyme label="a" type="f" corresp="#s4:l1">rusten</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">U hert sonder <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">In lust, ick self gheen lust en <rhyme label="b" type="m" corresp="#s4:l3">vin</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Myn teere <rhyme label="c" type="f">sinnen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">U schoonheyt <rhyme label="c" type="f" corresp="#s4:l5">minnen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Laet my troost <rhyme label="c" type="f" corresp="#s4:l5">winnen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Myn schoone <rhyme label="b" type="m" corresp="#s4:l3">Vrindin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="1" type="-+">Wat <rhyme label="a" type="f">tyden</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Braght oyt sulck <rhyme label="a" type="f" corresp="#s5:l1">verbleyden</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Als die voor den <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Als die, wanneer ick liefste <rhyme label="b" type="m" corresp="#s5:l3">sagh</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Dat door myn <rhyme label="c" type="f">suchten</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Den druck sou <rhyme label="c" type="f" corresp="#s5:l5">vluchten</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">En brenghen <rhyme label="c" type="f" corresp="#s5:l5">vruchten</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Daer noyt vruchten <rhyme label="b" type="m" corresp="#s5:l3">lagh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
