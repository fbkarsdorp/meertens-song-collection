<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179648</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody>Wel wat heeft de min vermogen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ZOu men nu van Vreugt niet <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zou men niet daok toonen <rhyme label="b" type="m">slaen</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Zou men nu niet op de <rhyme label="b" type="m" corresp="#s1:l2">baen</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Met ons Amstels <rhyme label="a" type="f" corresp="#s1:l1">Lievelingen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Vrolyk nu ten ryen <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schoon Heer Stenis door <rhyme label="a" type="f">ongelukken</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Kiel en Mast, en Rae <rhyme label="b" type="m">verloor</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">’t Geen ons droevig klonk in ’t <rhyme label="b" type="m" corresp="#s2:l2">oor</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Wyl ’s Lands Schip brak heel aen <rhyme label="a" type="f" corresp="#s2:l1">stukken</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Stiet, en borst heel midde <rhyme label="b" type="m" corresp="#s2:l2">door</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">’t Was op de Kust van <rhyme label="a" type="f">Barbereyen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Daer den Turk voerd het <rhyme label="b" type="m">gebied</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Daer men nooyt verlossing <rhyme label="b" type="m" corresp="#s3:l2">ziet</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Uyt zyn Wrede <rhyme label="a" type="f" corresp="#s3:l1">slavernyen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Daer troost, hoop en bystand <rhyme label="b" type="m" corresp="#s3:l2">vliet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Van een Turk heel slaefs <rhyme label="a" type="f">gekregen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Kwam ’t Grootmagtig <rhyme label="b" type="m">Engeland</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">De Conzul van dat magtig <rhyme label="b" type="m" corresp="#s4:l2">strand</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Tot zyn hulp en dienst <rhyme label="a" type="f" corresp="#s4:l1">genegen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Voed haer in die droeve <rhyme label="b" type="m" corresp="#s4:l2">stand</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ons Grootmagtig lieve <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Die voor dierbare Vryheid <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Konde nooyt beschouwen <rhyme label="b" type="m" corresp="#s5:l2">aen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Waerde Vaderen in ’t <rhyme label="a" type="f" corresp="#s5:l1">Regeeren</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Dat haer volk zou <rhyme label="b" type="m" corresp="#s5:l2">ondergaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zy besteden Neerlands <rhyme label="a" type="f">gaven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Maken vry by dat <rhyme label="b" type="m">Geslagt</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Slaverny is hier <rhyme label="b" type="m" corresp="#s6:l2">veragt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Dit is werk voor alle <rhyme label="a" type="f" corresp="#s6:l1">Braven</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Soo is ’t Volks heyl <rhyme label="b" type="m" corresp="#s6:l2">volbragt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Lieve Land en <rhyme label="a" type="f">Stadgenooten</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Welkom in uw <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Denkt een nooyt volprezen <rhyme label="b" type="m" corresp="#s7:l2">hand</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Heeft uw van uw boey <rhyme label="a" type="f" corresp="#s7:l1">ontsloten</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Eerd den zeven <rhyme label="b" type="m" corresp="#s7:l2">Pylenband</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Laten wy u <rhyme label="a" type="f">Echtvrindinnen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Kinders die zyn veel in <rhyme label="b" type="m">tal</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Lof verkonden <rhyme label="b" type="m" corresp="#s8:l2">overal</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Dat zy Neerlands heil <rhyme label="a" type="f" corresp="#s8:l1">beminnen</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">’t Geen voor eerst niet eyndige <rhyme label="b" type="m" corresp="#s8:l2">zal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">So Oranje blyft in ’t <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Groeid de vreugt voor land en <rhyme label="b" type="m">Kerk</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Dankt hem die ook in dit <rhyme label="b" type="m" corresp="#s9:l2">werk</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">’t Roer hem heeft in de hand <rhyme label="a" type="f" corresp="#s9:l1">gegeven</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Dat ’s Lands Vaderen magt <rhyme label="b" type="m" corresp="#s9:l2">versterkt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Gy weet wat het is te <rhyme label="a" type="f">zeggen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Als men dierbaer Vryheid <rhyme label="b" type="m">mist</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Veelen zyn daer in <rhyme label="b" type="m" corresp="#s10:l2">vergist</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Dog gy kund het haer <rhyme label="a" type="f" corresp="#s10:l1">uitleggen</rhyme>:</l>
        <l id="s10:l5" met="4" type="+-">Het is buyten ’t net <rhyme label="b" type="m" corresp="#s10:l2">gevist</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Nog eens welkom lieve <rhyme label="a" type="f">Vrinde</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Die ’k met Tranen heb <rhyme label="b" type="m">beschryd</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Dankt de Eeuwige <rhyme label="b" type="m" corresp="#s11:l2">Majesteid</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Met opregten <rhyme label="a" type="f" corresp="#s11:l1">Prinsgezinden</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Leeft en sterft vol <rhyme label="b" type="m" corresp="#s11:l2">Zaligheid</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Dat ’s myne bee myn hoop en <rhyme label="a" type="f">smeken</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Ongelukkig en nu <rhyme label="b" type="m">hersteld</rhyme></l>
        <l id="s12:l3" met="4" type="+-">Slaverny toond zyn <rhyme label="b" type="m" corresp="#s12:l2">geweld</rhyme>;</l>
        <l id="s12:l4" met="4" type="+-">Maer die Leeuw op ’t eerste <rhyme label="a" type="f" corresp="#s12:l1">teken</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Band die dwingland uit het <rhyme label="b" type="m" corresp="#s12:l2">Veld</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
