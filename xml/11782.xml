<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11782</id>
    <date>
      <year-start>1648</year-start>
      <year-end>1648</year-end>
    </date>
    <tags/>
    <melody>J'aimerai en ce village</melody>
  <author>Dirck Pietersz Pers</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">LOf, Lof, zy den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Die dus komt <rhyme label="b" type="m">geciert</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Die met lichte <rhyme label="a" type="f" corresp="#s1:l1">kleeren</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Schoon en heerlijck <rhyme label="b" type="m" corresp="#s1:l2">swiert</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Die des Hemels <rhyme label="c" type="f">wolcken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als met een tapijt <rhyme label="d" type="m">bedeckt</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">En sijn heyl'ge <rhyme label="c" type="f" corresp="#s1:l5">Tolcken</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Tot sijn winden heeft <rhyme label="d" type="m" corresp="#s1:l6">gestreckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Die des Aerdrijx <rhyme label="a" type="f">gronden</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Heeft soo vast <rhyme label="b" type="m">gestelt</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">En de Zee <rhyme label="a" type="f" corresp="#s2:l1">gebonden</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Die niet hooger <rhyme label="b" type="m" corresp="#s2:l2">swelt</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Als ghy in u <rhyme label="c" type="f">palen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Haer doet rennen aen haer <rhyme label="d" type="m">oort</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Datse snel moet <rhyme label="c" type="f" corresp="#s2:l5">dalen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Op het wencken van u <rhyme label="d" type="m" corresp="#s2:l6">woordt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Die der Bronnen <rhyme label="a" type="f">wellen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Van de bergen <rhyme label="b" type="m">swaeyt</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">En soo langhs <rhyme label="a" type="f" corresp="#s3:l1">struyvellen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Door de Landen <rhyme label="b" type="m" corresp="#s3:l2">draeyt</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Daer de Diertjes <rhyme label="c" type="f">queelen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Als sy laven haren <rhyme label="d" type="m">dorst</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Daer de Herders <rhyme label="c" type="f" corresp="#s3:l5">speelen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Vrolijck uyt een vrye <rhyme label="d" type="m" corresp="#s3:l6">bortst</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Daer het Landt vol <rhyme label="a" type="f">vruchten</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">En het grasjen <rhyme label="b" type="m">wast</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Daer elck met <rhyme label="a" type="f" corresp="#s4:l1">genuchten</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Op het kooren <rhyme label="b" type="m" corresp="#s4:l2">past</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Daer de Wijn en <rhyme label="c" type="f">Oly</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die des menschen geest <rhyme label="d" type="m">verheughd</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Daer het kruyt en <rhyme label="c" type="f" corresp="#s4:l5">Foly</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Tierigh zijn der velden <rhyme label="d" type="m" corresp="#s4:l6">vreughd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Daer de Vogels <rhyme label="a" type="f">broeden</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Daer de Reygers <rhyme label="b" type="m">zijn</rhyme>.</l>
        <l id="s5:l3" met="3" type="+-">Daer de Gemsen <rhyme label="a" type="f" corresp="#s5:l1">hoeden</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">En het vet <rhyme label="b" type="m" corresp="#s5:l2">Konijn</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Daer des Hemels <rhyme label="c" type="f">lampen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Nu weer op, dan onder <rhyme label="d" type="m">gaen</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Daer de Leeuwen <rhyme label="c" type="f" corresp="#s5:l5">kampen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Zijnde met den roof <rhyme label="d" type="m" corresp="#s5:l6">gela'en</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Die het jaer doet <rhyme label="a" type="f">kroonen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Met genaed' en <rhyme label="b" type="m">goedt</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Dat soo <rhyme label="a" type="f" corresp="#s6:l1">overschoone</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Pronckt in <rhyme label="b" type="m" corresp="#s6:l2">overvloet</rhyme>:</l>
        <l id="s6:l5" met="3" type="+-">Daer de kudden <rhyme label="c" type="f">swieren</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En de schaepkens vet en <rhyme label="d" type="m">mals</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">In de weelde <rhyme label="c" type="f" corresp="#s6:l5">tieren</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Hebbende genoegh van <rhyme label="d" type="m" corresp="#s6:l6">als</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Heere Godt u <rhyme label="a" type="f">wercken</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Die zijn <rhyme label="b" type="m">wonderbaer</rhyme>:</l>
        <l id="s7:l3" met="3" type="+-">Mensch wilt hier op <rhyme label="a" type="f" corresp="#s7:l1">mercken</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Neemt sijn zegen <rhyme label="b" type="m" corresp="#s7:l2">waer</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Want sijn eer sal <rhyme label="c" type="f">groeyen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Nu en in der <rhyme label="d" type="m">eeuwigheydt</rhyme>:</l>
        <l id="s7:l7" met="3" type="+-">Wilt in deughde <rhyme label="c" type="f" corresp="#s7:l5">bloeyen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En Godts Lof alsins <rhyme label="d" type="m" corresp="#s7:l6">verbreydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
