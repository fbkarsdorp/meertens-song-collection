    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Is het altemael reyn <rhyme label="a" type="m">op</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Isser niet een schotel <rhyme label="a" type="m" corresp="#s1:l1">sop</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Voor n'en armen ambachts <rhyme label="b" type="f">jonghen</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Isser niet tot my <rhyme label="c" type="m">gherief</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Keure goedt herteken <rhyme label="c" type="m" corresp="#s1:l4">lief</rhyme>?</l>
      </lg>
    </lg>
  </text>
