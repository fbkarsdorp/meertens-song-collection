<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3946</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>reflective song</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Ik weet nog een maagd</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ten was noyt mensche van <rhyme label="a" type="f">sonden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">van sonden so <rhyme label="b" type="m">versaecht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Had hijs groten <rhyme label="c" type="f">rouwe</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En Gode sijn liden <rhyme label="b" type="m" corresp="#s1:l2">claecht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ende voort op hope <rhyme label="d" type="m">leuet</rhyme></l>
        <l id="s1:l6" met="4" type="-+">God die heere is al so <rhyme label="e" type="m">goet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Dat hijt hem al <rhyme label="d" type="m" corresp="#s1:l5">vergheuet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wij willen God gaen <rhyme label="a" type="f">dienen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hi salt ons loonen <rhyme label="b" type="m">bat</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Hi wil ons gaerne <rhyme label="c" type="f">gheuen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sinen eeweliken <rhyme label="b" type="m" corresp="#s2:l2">schat</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Die niet en mach <rhyme label="d" type="m">vergaen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Daer eenen dach meer vruechden <rhyme label="e" type="m">is</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dan hier vijfduysent <rhyme label="d" type="m" corresp="#s2:l5">iaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Aen God van <rhyme label="a" type="f">hemelrike</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Staet alle mijn <rhyme label="b" type="m">toeuerlaet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Want alle dese <rhyme label="c" type="f">werelt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In sijnder hulden <rhyme label="b" type="m" corresp="#s3:l2">staet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Daer sonder niet en mach <rhyme label="d" type="m">sijn</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Eenen corten <rhyme label="e" type="m">ooghenblick</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dat is ons allen aen <rhyme label="d" type="m" corresp="#s3:l5">schijn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hi wacht hem also <rhyme label="a" type="f">wale</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die hem te wachten <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Men vint in niemant <rhyme label="c" type="f">trouwe</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In gheenen mensche die <rhyme label="b" type="m" corresp="#s4:l2">leeft</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dan aen den rijcken <rhyme label="d" type="m">God</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Wouden wij dat wel <rhyme label="e" type="m">verstaen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wij onderhielen sijn <rhyme label="d" type="m" corresp="#s4:l5">ghebot</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dese boose <rhyme label="a" type="f">werelt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En is anders <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dan loos / quaet / ende <rhyme label="c" type="f">vals</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Onghestadich als dat <rhyme label="b" type="m" corresp="#s5:l2">riet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dat voer alle winden <rhyme label="d" type="m">helt</rhyme></l>
        <l id="s5:l6" met="4" type="-+">So wie der werelt volgen <rhyme label="e" type="m">wil</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Sijn herte blijft <rhyme label="d" type="m" corresp="#s5:l5">onghestelt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dese valsche werelt <rhyme label="a" type="f">is</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Broosscher dan een <rhyme label="b" type="m">ghelas</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ten was noyt eenich <rhyme label="c" type="f">mensche</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hoe hooch hi gheboren <rhyme label="b" type="m" corresp="#s6:l2">was</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Hi en sterf / ende wert <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Met alle sinen groten <rhyme label="e" type="m">goede</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Daer hi hem toe <rhyme label="d" type="m" corresp="#s6:l5">verliet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
