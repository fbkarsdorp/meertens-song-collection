    <melody>Margrietje gij zijt zo hups dier</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ick lech voorsmoert in druck <rhyme label="a" type="m">gequel</rhyme></l>
        <l id="s1:l2" met="4" type="-+">met pijne voordwine als Ick noch <rhyme label="a" type="m" corresp="#s1:l1">sel</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Door liefde <rhyme label="a" type="m" corresp="#s1:l1">rebel</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Zij en hout geen akkoord Ick wout soe <rhyme label="a" type="m" corresp="#s1:l1">wel</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Dus roup Ick mort mom fors wiet <rhyme label="b" type="m">hoort</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Door liefde <rhyme label="a" type="m" corresp="#s1:l1">fel</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gij weet seekreet mijn tsersen <rhyme label="a" type="m">gront</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Dus ben Ick geheel duer druck <rhyme label="a" type="m" corresp="#s2:l1">gewont</rhyme></l>
        <l id="s2:l3" met="2" type="-+">heel <rhyme label="a" type="m" corresp="#s2:l1">ongesont</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Gij steeck den Draeck met mijn goedt <rhyme label="a" type="m" corresp="#s2:l1">ront</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Tis sonder saeck dies roup Ick <rhyme label="b" type="m">wraeck</rhyme></l>
        <l id="s2:l6" met="2" type="-+">tot alder <rhyme label="a" type="m" corresp="#s2:l1">stont</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Eylaes hoe dwaes soe mocht Ick <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s3:l2" met="4" type="-+">In suchten en kluchten soe bracht sij <rhyme label="a" type="m" corresp="#s3:l1">mijn</rhyme></l>
        <l id="s3:l3" met="2" type="-+">tschint <rhyme label="a" type="m" corresp="#s3:l1">meedoesijn</rhyme></l>
        <l id="s3:l4" met="4" type="-+">nu smaeck Ick van dit vals <rhyme label="a" type="m" corresp="#s3:l1">fen(in</rhyme>)</l>
        <l id="s3:l5" met="4" type="+-">mijn vruecht moet vlien <rhyme label="b" type="m">Ick</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Druck ende <rhyme label="a" type="m" corresp="#s3:l1">pijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ae mij dat sij mij oeit <rhyme label="a" type="m">aensach</rhyme></l>
        <l id="s4:l2" met="4" type="-+">fantaesy perty nacht ende <rhyme label="a" type="m" corresp="#s4:l1">dach</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Ick roup oe <rhyme label="a" type="m" corresp="#s4:l1">wacht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Ick wou sij wist mijn swaer <rhyme label="a" type="m" corresp="#s4:l1">geklaech</rhyme></l>
        <l id="s4:l5" met="4" type="+-">sertein sij Is die Ick meyn <rhyme label="b" type="m">allein</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Die mijn helpen <rhyme label="a" type="m" corresp="#s4:l1">mach</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick quam vrouch genouch al hadt Ick <rhyme label="a" type="m">haest</rhyme></l>
        <l id="s5:l2" met="4" type="-+">nu loop Ick en roup geheel <rhyme label="a" type="m" corresp="#s5:l1">voorbaest</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Als een die <rhyme label="a" type="m" corresp="#s5:l1">raest</rhyme></l>
        <l id="s5:l4" met="4" type="-+">hart sin en moet Is mij <rhyme label="a" type="m" corresp="#s5:l1">soelaes</rhyme></l>
        <l id="s5:l5" met="4" type="+-">sertein sij doet mijn sinnen <rhyme label="b" type="m">voorwoet</rhyme></l>
        <l id="s5:l6" met="2" type="-+">al heel <rhyme label="a" type="m" corresp="#s5:l1">voerdwaest</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Veenes prinses hoe hebdy mij <rhyme label="a" type="m">voeraen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">begeckt Int net <rhyme label="a" type="m" corresp="#s6:l1">geJaecht</rhyme></l>
        <l id="s6:l3" met="2" type="-+">ten baet geen <rhyme label="a" type="m" corresp="#s6:l1">kracht</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Tis liefde groedt diet hert <rhyme label="a" type="m" corresp="#s6:l1">duerknaecht</rhyme></l>
        <l id="s6:l5" met="4" type="+-">In deesen noet <rhyme label="b" type="m">roup</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Ick o doet leeven <rhyme label="a" type="m" corresp="#s6:l1">plaecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
