  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ach! myn vryheid waard te <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Ach wat zyt gy <rhyme label="b" type="m">zoet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">'k Voel de min niet meer van <rhyme label="a" type="f" corresp="#s1:l1">binnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Z'is al voort uit myn <rhyme label="b" type="m" corresp="#s1:l2">gemoed</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Ach myn vryheid waart te <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Ach wat zyt gy <rhyme label="b" type="m" corresp="#s1:l2">zoet</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Wech min, 'k ban u uit myn <rhyme label="a" type="f" corresp="#s1:l1">zinnen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Gy verteert het vlees en <rhyme label="b" type="m" corresp="#s1:l2">bloed</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Ach myn vryheid waard te <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Ach wat zyt gy <rhyme label="b" type="m" corresp="#s1:l2">zoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Is veel beter vry te <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Als in <rhyme label="b" type="m">slaverny</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Dan behoeftmen niet te <rhyme label="a" type="f" corresp="#s2:l1">vreezen</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">'t Stelt de droefheid aan een <rhyme label="b" type="m" corresp="#s2:l2">zy</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">'t Is veel beter vry te <rhyme label="a" type="f" corresp="#s2:l1">wezen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Als in <rhyme label="b" type="m" corresp="#s2:l2">slaverny</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Laat u niet te licht <rhyme label="a" type="f" corresp="#s2:l1">belezen</rhyme>;</l>
        <l id="s2:l8" met="4" type="+-">'t Vryen maakt ons gants <rhyme label="b" type="m" corresp="#s2:l2">onvry</rhyme>.</l>
        <l id="s2:l9" met="4" type="+-">'t Is veel beter vry te <rhyme label="a" type="f" corresp="#s2:l1">wezen</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Als in <rhyme label="b" type="m" corresp="#s2:l2">slaverny</rhyme>.</l>
      </lg>
    </lg>
