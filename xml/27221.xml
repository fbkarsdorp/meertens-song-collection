    <melody>Met een minnelijke zin</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Met een minnelycke <rhyme label="a" type="m">sinne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Coom ick u schoon lieff <rhyme label="b" type="m">voort</rhyme></l>
        <l id="s1:l3" met="4" type="+-">V onmatelyck ick <rhyme label="a" type="m" corresp="#s1:l1">minne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hierom geeft mijn <rhyme label="b" type="m" corresp="#s1:l2">gehoor</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Want ick geheel <rhyme label="c" type="m">versmoor</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Soo ick u lieff <rhyme label="c" type="m" corresp="#s1:l5">verloor</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Schoon goddin / Int <rhyme label="d" type="m">beghin</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Ick gewin / laet my <rhyme label="d" type="m" corresp="#s1:l7">in</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Liefflyck cloppen voor u <rhyme label="c" type="m" corresp="#s1:l5">door</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">D]at ick haestelycke <rhyme label="a" type="m">my</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als nu met u <rhyme label="b" type="m">versaem</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Segge vryelyck tot <rhyme label="a" type="m" corresp="#s2:l1">dy</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat laestemael was v <rhyme label="b" type="m" corresp="#s2:l2">faem</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Een persoon sonder <rhyme label="c" type="m">blaem</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Want venus is haer <rhyme label="c" type="m" corresp="#s2:l5">naem</rhyme></l>
        <l id="s2:l7" met="4" type="+-">S]oet gety / ende <rhyme label="d" type="m">bly</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Oock so vry / seyde <rhyme label="d" type="m" corresp="#s2:l7">sy</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Off sulcken goddinne by u <rhyme label="c" type="m" corresp="#s2:l5">quaem</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Met haer kinneken <rhyme label="a" type="m">cruweel</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Cupido als een <rhyme label="b" type="m">dier</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Dit tirannelyck <rhyme label="a" type="m" corresp="#s3:l1">morteel</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Myn Vur syn stout <rhyme label="b" type="m" corresp="#s3:l2">bestier</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Heel queste dat ick <rhyme label="c" type="m">schier</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Sonder v storue ick <rhyme label="c" type="m" corresp="#s3:l5">hier</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Soo rebel / ende <rhyme label="d" type="m">fel</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Met gequel Toonde <rhyme label="d" type="m" corresp="#s3:l7">wel</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Dat ick most branden in syn <rhyme label="c" type="m" corresp="#s3:l5">vier</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">B]lust hieromme myn <rhyme label="a" type="m">torment</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Princesse <rhyme label="b" type="m">madgeleyn</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Oft ick blyff in u <rhyme label="a" type="m" corresp="#s4:l1">geschent</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Om v myn liefte <rhyme label="b" type="m" corresp="#s4:l2">greyn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Want ghy syt die <rhyme label="c" type="m">fonteyn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Daer ick by leef <rhyme label="c" type="m" corresp="#s4:l5">alleyn</rhyme></l>
        <l id="s4:l7" met="4" type="+-">S]y Content / <rhyme label="d" type="m">Exelent</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Tfondament / es <rhyme label="d" type="m" corresp="#s4:l7">bekent</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Van u myn lieff[.] niet te <rhyme label="c" type="m" corresp="#s4:l5">cleyn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
