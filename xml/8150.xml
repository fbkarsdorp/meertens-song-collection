<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8150</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody>Psalm 068 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MIjn Gheest voel ick my dringhen <rhyme label="a" type="m">sterck</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Om singhen van een heughlijck <rhyme label="a" type="m" corresp="#s1:l1">werck</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Hoe dat een <rhyme label="b" type="f">Opper-Koningh</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En Heer der Heeren, sijn <rhyme label="c" type="m">Intree</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sachtmoedigh, doch seer heerlijck, <rhyme label="c" type="m" corresp="#s1:l4">dee</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In Ierusalems <rhyme label="b" type="f" corresp="#s1:l3">wooningh</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Waerom Zach'rias de <rhyme label="d" type="m">Propheet</rhyme></l>
        <l id="s1:l8" met="4" type="-+">De Doch'tren Zions juychen <rhyme label="d" type="m" corresp="#s1:l7">heet</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">En noodt tot blijd <rhyme label="e" type="f">uytkijcken</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Om dat haer Koningh komt tot <rhyme label="f" type="m">haer</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Sittend' op 't Veulen van 't <rhyme label="f" type="m" corresp="#s1:l10">lastbaer</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Diertjen seer <rhyme label="e" type="f" corresp="#s1:l9">armelijcken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dees Siender, langh voorheen <rhyme label="a" type="m">gheweest</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Wist dit, ghedreven door Gods <rhyme label="a" type="m" corresp="#s2:l1">Gheest</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Soo duydtlijck uyt te <rhyme label="b" type="f">spreecken</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Verblijdt u dan ghy Christen <rhyme label="c" type="m">schaer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Nu de gheschieder daedt soo <rhyme label="c" type="m" corresp="#s2:l4">klaer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En waerlijck is <rhyme label="b" type="f" corresp="#s2:l3">ghebleecken</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Doen Iesus tot Bethphaghe <rhyme label="d" type="m">quam</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">En na d'Olijfbergh sijn wegh <rhyme label="d" type="m" corresp="#s2:l7">nam</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Sandt hy twee Iongh'ren <rhyme label="e" type="f">henen</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">En sprack: In 't Vleck, hier over, <rhyme label="f" type="m">gaet</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">d'Ees'lin die daer gebonden <rhyme label="f" type="m" corresp="#s2:l10">staet</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Brenght hier en 't Veul met <rhyme label="e" type="f" corresp="#s2:l9">eenen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Vraeght yemandt waerom ghy <rhyme label="a" type="m">s'ontbindt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dien seght tot antwoordt wel <rhyme label="a" type="m" corresp="#s3:l1">besint</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">De Heer heeft dees van <rhyme label="b" type="f">noode</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Terstondt hy 't volghen laten <rhyme label="c" type="m">sal</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Sy vonden 't soo. En deden 't <rhyme label="c" type="m" corresp="#s3:l4">al</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Alsoo haer was <rhyme label="b" type="f" corresp="#s3:l3">gheboden</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">En brachten d'Ezelinne <rhyme label="d" type="m">meed'</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Met 't Veul, daer noyt een Mensch op <rhyme label="d" type="m" corresp="#s3:l7">reed</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Waer op s'haer kleed'ren <rhyme label="e" type="f">leyden</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">En Christum setten boven <rhyme label="f" type="m">op</rhyme>!</l>
        <l id="s3:l11" met="4" type="+-">Veel volcks zijnd' op d' Olijfberghs <rhyme label="f" type="m" corresp="#s3:l10">top</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Op d'wegh haer kleed'ren <rhyme label="e" type="f" corresp="#s3:l9">spreyden</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">And're Palm-tacken hieuwen <rhyme label="a" type="m">of</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Van 't groen geboomt, en stroyden <rhyme label="a" type="m" corresp="#s4:l1">'tlof</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Langhs henen op den <rhyme label="b" type="f">weghe</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">De schare Volleckx die <rhyme label="c" type="m">voorgingh</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En die navolghd', en hem <rhyme label="c" type="m" corresp="#s4:l4">ontvingh</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Oock die hem liepen <rhyme label="b" type="f" corresp="#s4:l3">teghen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Riepen altsamen seer <rhyme label="d" type="m">verblijdt</rhyme>:</l>
        <l id="s4:l8" met="4" type="-+">Hosanna, o <rhyme label="d" type="m" corresp="#s4:l7">ghebenedijt</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Zijt ghy, o Davids <rhyme label="e" type="f">Soone</rhyme>!</l>
        <l id="s4:l10" met="4" type="-+">O! Coningh Zions <rhyme label="f" type="m">hoogh-beroemt</rhyme>;</l>
        <l id="s4:l11" met="4" type="+-">Die inden naem des Heeren <rhyme label="f" type="m" corresp="#s4:l10">komt</rhyme>.</l>
        <l id="s4:l12" met="3" type="-+">Hosann' in d'hooghste <rhyme label="e" type="f" corresp="#s4:l9">Throonen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Met sulck een statie noyt <rhyme label="a" type="m">ghehoort</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Quam Christus tot aen Salems <rhyme label="a" type="m" corresp="#s5:l1">Poort</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Van d'Olijfbergh <rhyme label="b" type="f">afrijden</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Waer door de Stadt wierdt heel <rhyme label="c" type="m">beroert</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Vraghend' om wien men soo <rhyme label="c" type="m" corresp="#s5:l4">rumoert</rhyme>?</l>
        <l id="s5:l6" met="3" type="-+">'t Volck antwoordt met <rhyme label="b" type="f" corresp="#s5:l3">verblijden</rhyme>:</l>
        <l id="s5:l7" met="4" type="-+">Dit is Iesus de <rhyme label="d" type="m">Nazareen</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Een groot Propheet uyt <rhyme label="d" type="m" corresp="#s5:l7">Galileen</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">d'Heer gingh voort in Godts <rhyme label="e" type="f">Tempel</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">En dreef de Verkoopers van <rhyme label="f" type="m">daer</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Met de Koopers en <rhyme label="f" type="m" corresp="#s5:l10">wisselaer</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Wt 's Beed-huys Heyl'ghe <rhyme label="e" type="f" corresp="#s5:l9">drempel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De blind' en Lammen hy <rhyme label="a" type="m">ghenas</rhyme>.</l>
        <l id="s6:l2" met="4" type="-+">Waerdoor 't Priester-rot toornigh <rhyme label="a" type="m" corresp="#s6:l1">was</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">(Als sy dees wond'ren <rhyme label="b" type="f">saghen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En 't roepen der Kind'ren veel <rhyme label="c" type="m">meer</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Hosanna, Davids Soon zy <rhyme label="c" type="m" corresp="#s6:l4">eer</rhyme>)</l>
        <l id="s6:l6" met="3" type="-+">Soo toondens' haer <rhyme label="b" type="f" corresp="#s6:l3">mishaghen</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">En seyden tot Hem, Hoort ghy <rhyme label="d" type="m">wel</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Al dit gheroep, en vreemde <rhyme label="d" type="m" corresp="#s6:l7">spel</rhyme>?</l>
        <l id="s6:l9" met="3" type="-+">Ia, sprack de Heer der <rhyme label="e" type="f">Heeren</rhyme>:</l>
        <l id="s6:l10" met="4" type="-+">En laest ghy noyt? Der Kind'ren <rhyme label="f" type="m">mondt</rhyme></l>
        <l id="s6:l11" met="4" type="+-">En Suygh'linghen u lof doet <rhyme label="f" type="m" corresp="#s6:l10">kont</rhyme>.</l>
        <l id="s6:l12" met="3" type="-+">Doen gingh hy van daer <rhyme label="e" type="f" corresp="#s6:l9">keeren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daer hebt ghy de vervulde <rhyme label="a" type="m">daedt</rhyme>;</l>
        <l id="s7:l2" met="4" type="-+">Waer van in Zacharias <rhyme label="a" type="m" corresp="#s7:l1">staet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">En Davids Psalm <rhyme label="b" type="f">gheschreven</rhyme>:</l>
        <l id="s7:l4" met="4" type="-+">Verciert aen d'Hoornen van 't <rhyme label="c" type="m">Altaer</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Dees Feest met Meyen, gantsch <rhyme label="c" type="m" corresp="#s7:l4">vruchtbaer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Van een Godtsaligh <rhyme label="b" type="f" corresp="#s7:l3">leven</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Help, Heer! het wel ghelucken <rhyme label="d" type="m">laet</rhyme></l>
        <l id="s7:l8" met="4" type="-+">Gheef dat wy Christ'nen in ons <rhyme label="d" type="m" corresp="#s7:l7">staet</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Christus ons hooft <rhyme label="e" type="f">ghelijcken</rhyme></l>
        <l id="s7:l10" met="4" type="-+">In sachtmoedighe <rhyme label="f" type="m">nedrigheydt</rhyme>.</l>
        <l id="s7:l11" met="4" type="+-">Op dat d'ingangh ons werd' <rhyme label="f" type="m" corresp="#s7:l10">bereydt</rhyme>.</l>
        <l id="s7:l12" met="3" type="-+">In Salems Hemelsch <rhyme label="e" type="f" corresp="#s7:l9">Rijcke</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
