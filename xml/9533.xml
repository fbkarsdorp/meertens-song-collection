<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9533</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Venus gij en uw kind</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NIet tegen-staend' de <rhyme label="a" type="m">vleck</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van uw beweend' <rhyme label="a" type="m" corresp="#s1:l1">gebreck</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">So blijfje mijn <rhyme label="b" type="f">Vriendinne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dien ick van herten <rhyme label="b" type="f" corresp="#s1:l3">minne</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Gelijck de schoone <rhyme label="c" type="f">Steden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van Koninghs <rhyme label="c" type="f" corresp="#s1:l5">heerlijckheden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Thirza, <rhyme label="a" type="m">Ierusalem</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Des werelts eer en <rhyme label="a" type="m" corresp="#s2:l1">klem</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Als heyren met <rhyme label="b" type="f">banieren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Vw' schoonigheden <rhyme label="b" type="f" corresp="#s2:l3">swieren</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">En schrick'lijck sich <rhyme label="c" type="f">vertoogen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In uwe vyandts <rhyme label="c" type="f" corresp="#s2:l5">oogen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wend' af van my het <rhyme label="a" type="m">oogh</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En keer het naer om <rhyme label="a" type="m" corresp="#s3:l1">hoogh</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Want ick ghewelt moet <rhyme label="b" type="f">lijden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van uw' gedurigh <rhyme label="b" type="f" corresp="#s3:l3">strijden</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Des wil ick we'er u <rhyme label="c" type="f">prijsen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En alle schoonte <rhyme label="c" type="f" corresp="#s3:l5">wijsen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vw' hangend' hayren <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Gelijck der Geyten <rhyme label="a" type="m" corresp="#s4:l1">zijn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die Gilead <rhyme label="b" type="f">af-scheeren</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Sy trecken mijn <rhyme label="b" type="f" corresp="#s4:l3">begeeren</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Ick spreeck van uw' <rhyme label="c" type="f">belijden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En sonden <rhyme label="c" type="f" corresp="#s4:l5">tegen-strijden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Vw' tanden als een <rhyme label="a" type="m">kud</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die 't vuyl d' er hene <rhyme label="a" type="m" corresp="#s5:l1">schud</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Van 't water <rhyme label="b" type="f">op-gekomen</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Vw' Leeraers <rhyme label="b" type="f" corresp="#s5:l3">uyt-genomen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En eygen <rhyme label="c" type="f">herts-gepeynsen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Herkauwen sonder <rhyme label="c" type="f" corresp="#s5:l5">veynsen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gelijck de schapen <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Om hunne lijf te <rhyme label="a" type="m" corresp="#s6:l1">voen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo dese met'et <rhyme label="b" type="f">hooren</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Noch brengen sy te <rhyme label="b" type="f" corresp="#s6:l3">vooren</rhyme></l>
        <l id="s6:l5" met="3" type="-+">De vruchten van de <rhyme label="c" type="f">Zielen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die in de Kercke <rhyme label="c" type="f" corresp="#s6:l5">krielen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Vw' wangen, uw' <rhyme label="a" type="m">gelaet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die zijn als een <rhyme label="a" type="m" corresp="#s7:l1">Granaet</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Van tusschen uwe <rhyme label="b" type="f">vlechten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Gantsch nederigh, en <rhyme label="b" type="f" corresp="#s7:l3">slechte</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Van schaemte nimmer <rhyme label="c" type="f">ledigh</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Kuysch, minnelijck, en <rhyme label="c" type="f" corresp="#s7:l5">zedigh</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En schoon d'er vele <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met een gheroemde <rhyme label="a" type="m" corresp="#s8:l1">schijn</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Van Kercken of <rhyme label="b" type="f">Gemeynten</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Met haer ghemeynde <rhyme label="b" type="f" corresp="#s8:l3">reynten</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Ia tsestigh <rhyme label="c" type="f">Koninginnen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Die my in schijn <rhyme label="c" type="f" corresp="#s8:l5">beminnen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">By-wijlen tachtigh <rhyme label="a" type="m">stelt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En Maeghden <rhyme label="a" type="m" corresp="#s9:l1">ongetelt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Nochtans een eenigh <rhyme label="b" type="f">eene</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Is mijne Duyf <rhyme label="b" type="f" corresp="#s9:l3">alleene</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Sy is haer Moeders <rhyme label="c" type="f">eenigh</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En weet niet van soo <rhyme label="c" type="f" corresp="#s9:l5">menigh</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Als haer de Dochters <rhyme label="a" type="m">sien</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">De vreemde, 't sal <rhyme label="a" type="m" corresp="#s10:l1">geschien</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Dat sy haer saligh <rhyme label="b" type="f">noemen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En voor geluckigh <rhyme label="b" type="f" corresp="#s10:l3">roemen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Dat sy h'er eer <rhyme label="c" type="f">op-dragen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En na h'er schoonte <rhyme label="c" type="f" corresp="#s10:l5">vragen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wie isse die d'er <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Gelijck de <rhyme label="a" type="m" corresp="#s11:l1">dagheraet</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Gelijck de Maen soo <rhyme label="b" type="f">schoone</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Gelijck de Son ten <rhyme label="b" type="f" corresp="#s11:l3">toone</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Gelijck de <rhyme label="c" type="f">Krijghs-banieren</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Met tuchtelijck <rhyme label="c" type="f" corresp="#s11:l5">regieren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
