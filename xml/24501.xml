<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24501</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
      <tag standard-name="mystiek lied" translation="mystical song">mystiek lied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Gij die Jezus' wijngaard plant</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O ghi die Jhesus wyngart <rhyme label="a" type="m">plant</rhyme></l>
        <l id="s1:l2" met="4" type="-+">verblyt v in dat suete <rhyme label="a" type="m" corresp="#s1:l1">lant</rhyme></l>
        <l id="s1:l3" met="3" type="-+">daer gy syt toe <rhyme label="b" type="f">vercaren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die claerheit is <rhyme label="a" type="m">onsprekelick</rhyme></l>
        <l id="s2:l2" met="4" type="-+">die blyscap <rhyme label="a" type="m" corresp="#s2:l1">onbegrypelic</rhyme></l>
        <l id="s2:l3" met="3" type="-+">dien daer sal <rhyme label="b" type="f">gebueren</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die cierheit vroude vrede <rhyme label="a" type="m">suuerheit</rhyme></l>
        <l id="s3:l2" met="4" type="-+">die Jhesus v daer heeft <rhyme label="a" type="m" corresp="#s3:l1">bereyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">en is v niet te <rhyme label="b" type="f">noemen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Jherusalem is dat lant syn <rhyme label="a" type="m">naem</rhyme></l>
        <l id="s4:l2" met="4" type="-+">vol hilicheiden seer <rhyme label="a" type="m" corresp="#s4:l1">bequaem</rhyme></l>
        <l id="s4:l3" met="3" type="-+">veruullet mit alre <rhyme label="b" type="f">vrouden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Van hier en comet daer nyemant <rhyme label="a" type="m">in</rhyme></l>
        <l id="s5:l2" met="4" type="-+">dan die int herte draget Jhesus <rhyme label="a" type="m" corresp="#s5:l1">mynne</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ende is verciert mit <rhyme label="b" type="f">doegden</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dus is hi die sinen wyngart <rhyme label="a" type="m">plant</rhyme></l>
        <l id="s6:l2" met="4" type="-+">ende neernstich dient hem seer <rhyme label="a" type="m" corresp="#s6:l1">verlanget</rhyme></l>
        <l id="s6:l3" met="3" type="-+">om cortes by hem te <rhyme label="b" type="f">comen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Tsal ewich dueren sonder <rhyme label="a" type="m">verganc</rhyme></l>
        <l id="s7:l2" met="4" type="-+">het is voel claere dan claer <rhyme label="a" type="m" corresp="#s7:l1">golt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">verciert mit gulden <rhyme label="b" type="f">bloemen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die claere blencken dan die <rhyme label="a" type="m">maene</rhyme></l>
        <l id="s8:l2" met="4" type="-+">die sonne sy oec bauen <rhyme label="a" type="m" corresp="#s8:l1">gaen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">mit hoeren sueten <rhyme label="b" type="f">roeke</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Van claren golde ist schoen <rhyme label="a" type="m">paueyt</rhyme></l>
        <l id="s9:l2" met="4" type="-+">mit sueten rosen <rhyme label="a" type="m" corresp="#s9:l1">auerleyt</rhyme></l>
        <l id="s9:l3" met="3" type="-+">die sullen ewich <rhyme label="b" type="f">dueren</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Och bauen al marien <rhyme label="a" type="m">soen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">is daer soe wttermaten <rhyme label="a" type="m" corresp="#s10:l1">schoen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">diet lant v heeft <rhyme label="b" type="f">gewonnen</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hi is soe suet <rhyme label="a" type="m">genoechgelic</rhyme></l>
        <l id="s11:l2" met="4" type="-+">voel claere schoenre <rhyme label="a" type="m" corresp="#s11:l1">ongelic</rhyme></l>
        <l id="s11:l3" met="3" type="-+">dan hondert dusent <rhyme label="b" type="f">sonnen</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Hi syt daer in dat suete <rhyme label="a" type="m">lant</rhyme></l>
        <l id="s12:l2" met="4" type="-+">syn lieue moeder ter <rhyme label="a" type="m" corresp="#s12:l1">rechterhant</rhyme></l>
        <l id="s12:l3" met="3" type="-+">voer alle <rhyme label="b" type="f">creatueren</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Die hem anschouwet is soe <rhyme label="a" type="m">verblyt</rhyme></l>
        <l id="s13:l2" met="4" type="-+">tien dusent jaer doncket hem geen <rhyme label="a" type="m" corresp="#s13:l1">tyt</rhyme></l>
        <l id="s13:l3" met="3" type="-+">tys waer diet kan <rhyme label="b" type="f">versynnen</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Want reden die tyt en is daer <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s14:l2" met="4" type="-+">dat daer ommermeer is <rhyme label="a" type="m" corresp="#s14:l1">verdriet</rhyme></l>
        <l id="s14:l3" met="3" type="-+">van buten noch van <rhyme label="b" type="f">bynnen</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Men is daer ewelic vrolic ende <rhyme label="a" type="m">blyde</rhyme></l>
        <l id="s15:l2" met="4" type="-+">van allen sorgen is men <rhyme label="a" type="m" corresp="#s15:l1">vry</rhyme></l>
        <l id="s15:l3" met="3" type="-+">daer en mach geen weelde <rhyme label="b" type="f">gebreken</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Maria is daer die <rhyme label="a" type="m">conynginne</rhyme></l>
        <l id="s16:l2" met="4" type="-+">alle reyne maegden hoer <rhyme label="a" type="m" corresp="#s16:l1">gesynne</rhyme></l>
        <l id="s16:l3" met="3" type="-+">die Jhesus syn <rhyme label="b" type="f">vercaren</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Mer si is bauen al <rhyme label="a" type="m">gemynt</rhyme></l>
        <l id="s17:l2" met="4" type="-+">van des conynges soen hoer lieue <rhyme label="a" type="m" corresp="#s17:l1">kynt</rhyme></l>
        <l id="s17:l3" met="3" type="-+">als balsem onder den <rhyme label="b" type="f">doernen</rhyme></l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Hoe claer schoen ende <rhyme label="a" type="m">suuerlick</rhyme></l>
        <l id="s18:l2" met="4" type="-+">dat si daer is in hoers kyndes <rhyme label="a" type="m" corresp="#s18:l1">ryke</rhyme></l>
        <l id="s18:l3" met="3" type="-+">dat en kan geen mensche <rhyme label="b" type="f">geweten</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Die borgers dragen daer alle <rhyme label="a" type="m">cronen</rhyme></l>
        <l id="s19:l2" met="4" type="-+">ende syngen dat clyncket inden <rhyme label="a" type="m" corresp="#s19:l1">throen</rhyme></l>
        <l id="s19:l3" met="3" type="-+">Osanna <rhyme label="b" type="f">alleluya</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Mer och der jonferen sanck is soe <rhyme label="a" type="m">schoen</rhyme></l>
        <l id="s20:l2" met="4" type="-+">sy syngen mit des conynges <rhyme label="a" type="m" corresp="#s20:l1">soen</rhyme></l>
        <l id="s20:l3" met="3" type="-+">ende mit synre moeder <rhyme label="b" type="f">marien</rhyme></l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Hoe dat clyncket auert <rhyme label="a" type="m">hemelryke</rhyme></l>
        <l id="s21:l2" met="4" type="-+">hoe claer schoen ende <rhyme label="a" type="m" corresp="#s21:l1">wonderlic</rhyme></l>
        <l id="s21:l3" met="3" type="-+">en is niet wt te <rhyme label="b" type="f">spreken</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Hoer croen heeft om enen rosen <rhyme label="a" type="m">crans</rhyme></l>
        <l id="s22:l2" met="4" type="-+">sy syngen mit Jhesum anden <rhyme label="a" type="m" corresp="#s22:l1">dans</rhyme></l>
        <l id="s22:l3" met="3" type="-+">ende lauen sonder <rhyme label="b" type="f">merren</rhyme></l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Dat krensken heeft soe groet <rhyme label="a" type="m">vertuut</rhyme></l>
        <l id="s23:l2" met="4" type="-+">elc roesken geuet meer claerheit <rhyme label="a" type="m" corresp="#s23:l1">wt</rhyme></l>
        <l id="s23:l3" met="3" type="-+">dan sonne ende mane ende <rhyme label="b" type="f">sternen</rhyme></l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Gheen cruyt op eertryke en is soe <rhyme label="a" type="m">guet</rhyme></l>
        <l id="s24:l2" met="4" type="-+">dat rucken mach als dat krensken <rhyme label="a" type="m" corresp="#s24:l1">doet</rhyme></l>
        <l id="s24:l3" met="3" type="-+">al waer oncruyt <rhyme label="b" type="f">fyolen</rhyme></l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Sy hoeren oec dat suete <rhyme label="a" type="m">geluyt</rhyme></l>
        <l id="s25:l2" met="4" type="-+">comet myne gemynde suete <rhyme label="a" type="m" corresp="#s25:l1">bruyt</rhyme></l>
        <l id="s25:l3" met="3" type="-+">ende rust in mynen <rhyme label="b" type="f">armen</rhyme></l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Sy syn beuaen mit Jhesus <rhyme label="a" type="m">mynne</rhyme></l>
        <l id="s26:l2" met="4" type="-+">als cherubyn ende <rhyme label="a" type="m" corresp="#s26:l1">seraphyn</rhyme></l>
        <l id="s26:l3" met="3" type="-+">van groter mynnen sy <rhyme label="b" type="f">bernen</rhyme></l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Ghelic die sonne is hoer <rhyme label="a" type="m">aenschyn</rhyme></l>
        <l id="s27:l2" met="4" type="-+">sy volgen dat suete <rhyme label="a" type="m" corresp="#s27:l1">lammekyn</rhyme></l>
        <l id="s27:l3" met="3" type="-+">gecledet mit wytten <rhyme label="b" type="f">stoelen</rhyme></l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Die engelen ende zielen syn <rhyme label="a" type="m">gemeyn</rhyme></l>
        <l id="s28:l2" met="4" type="-+">ende syngen blydelic onder <rhyme label="a" type="m" corresp="#s28:l1">een</rhyme></l>
        <l id="s28:l3" met="3" type="-+">den conynck van den <rhyme label="b" type="f">throne</rhyme></l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Lof eer sy v here <rhyme label="a" type="m">sabaoth</rhyme></l>
        <l id="s29:l2" met="4" type="-+">almechtich hilich ewich <rhyme label="a" type="m" corresp="#s29:l1">god</rhyme></l>
        <l id="s29:l3" met="3" type="-+">een wesen drie <rhyme label="b" type="f">persoenen</rhyme></l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="4" type="-+">Ghi syt die ewich heeft <rhyme label="a" type="m">geweest</rhyme></l>
        <l id="s30:l2" met="4" type="-+">ende hemel ende eertryke mit v <rhyme label="a" type="m" corresp="#s30:l1">geest</rhyme></l>
        <l id="s30:l3" met="3" type="-+">dat vrolike <rhyme label="b" type="f">jubelieren</rhyme></l>
      </lg>
      <lg id="s31" type="stanza">
        <l id="s31:l1" met="4" type="-+">Och tys hem seer <rhyme label="a" type="m">behagelic</rhyme></l>
        <l id="s31:l2" met="4" type="-+">te hoeren van dat suete <rhyme label="a" type="m" corresp="#s31:l1">ryck</rhyme></l>
        <l id="s31:l3" met="3" type="-+">die eerberlike <rhyme label="b" type="f">leuen</rhyme></l>
      </lg>
      <lg id="s32" type="stanza">
        <l id="s32:l1" met="4" type="-+">Die Jhesum dienen <rhyme label="a" type="m">trouwelic</rhyme></l>
        <l id="s32:l2" met="4" type="-+">synen wyngart plant hi <rhyme label="a" type="m" corresp="#s32:l1">boldelic</rhyme></l>
        <l id="s32:l3" met="3" type="-+">die werlt sy <rhyme label="b" type="f">begeuen</rhyme></l>
      </lg>
      <lg id="s33" type="stanza">
        <l id="s33:l1" met="4" type="-+">Want als hoer tyt hier is <rhyme label="a" type="m">gedaen</rhyme></l>
        <l id="s33:l2" met="4" type="-+">sal hise lieflic daer <rhyme label="a" type="m" corresp="#s33:l1">ontfaen</rhyme></l>
        <l id="s33:l3" met="3" type="-+">ende gelic der sonnen <rhyme label="b" type="f">vercieren</rhyme></l>
      </lg>
      <lg id="s34" type="stanza">
        <l id="s34:l1" met="4" type="-+">Ick en hebt soe wal oec niet <rhyme label="a" type="m">ghesecht</rhyme></l>
        <l id="s34:l2" met="4" type="-+">dat lant en is noch bet <rhyme label="a" type="m" corresp="#s34:l1">bereyt</rhyme></l>
        <l id="s34:l3" met="3" type="-+">dan enich mensche kan <rhyme label="b" type="f">gedencken</rhyme></l>
      </lg>
      <lg id="s35" type="stanza">
        <l id="s35:l1" met="4" type="-+">Plant vrylic ghi sult daer syn <rhyme label="a" type="m">verblyt</rhyme></l>
        <l id="s35:l2" met="4" type="-+">mer siet dat gy gestaedich <rhyme label="a" type="m" corresp="#s35:l1">syt</rhyme></l>
        <l id="s35:l3" met="3" type="-+">v tyt die naket <rhyme label="b" type="f">alleynsken</rhyme></l>
      </lg>
      <lg id="s36" type="stanza">
        <l id="s36:l1" met="4" type="-+">Dat Jhesus v daer sal <rhyme label="a" type="m">ontbieden</rhyme></l>
        <l id="s36:l2" met="4" type="-+">v werken sal hi <rhyme label="a" type="m" corresp="#s36:l1">auersien</rhyme></l>
        <l id="s36:l3" met="3" type="-+">ende v nae  doegden <rhyme label="b" type="f">loenen</rhyme></l>
      </lg>
      <lg id="s37" type="stanza">
        <l id="s37:l1" met="4" type="-+">Och waerlic hi is seer <rhyme label="a" type="m">verblynt</rhyme></l>
        <l id="s37:l2" met="4" type="-+">die tytlic voer ewich <rhyme label="a" type="m" corresp="#s37:l1">mynt</rhyme></l>
        <l id="s37:l3" met="3" type="-+">ende wil dat lant <rhyme label="b" type="f">vergeten</rhyme></l>
      </lg>
      <lg id="s38" type="stanza">
        <l id="s38:l1" met="4" type="-+">Nochtant wal siet syn <rhyme label="a" type="m">fondament</rhyme></l>
        <l id="s38:l2" met="4" type="-+">dat schoen vercierde <rhyme label="a" type="m" corresp="#s38:l1">fyrmament</rhyme></l>
        <l id="s38:l3" met="3" type="-+">mit steernen ende <rhyme label="b" type="f">planeten</rhyme></l>
      </lg>
      <lg id="s39" type="stanza">
        <l id="s39:l1" met="4" type="-+">Och god verleen een <rhyme label="a" type="m">yegelic</rhyme></l>
        <l id="s39:l2" met="4" type="-+">omt lant te dencken op <rhyme label="a" type="m" corresp="#s39:l1">eertryke</rhyme></l>
        <l id="s39:l3" met="3" type="-+">ende daer nae te <rhyme label="b" type="f">bewoenen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
