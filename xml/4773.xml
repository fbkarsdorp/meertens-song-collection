<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4773</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Herderin mijn zinnen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">FY zonden, uw’ <rhyme label="a" type="f">vonden</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Zijn gronden van <rhyme label="b" type="m">hel</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Zy smeken, Maer <rhyme label="c" type="f">steken</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Op ’t lesten te <rhyme label="b" type="m" corresp="#s1:l2">fel</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">’t Begin// is <rhyme label="d" type="m">min</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Maer ’t end, <rhyme label="e" type="m">ellend</rhyme>//</l>
        <l id="s1:l7" met="2" type="-+">Des mijden// wy ’t <rhyme label="f" type="f">lijden</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En spoen// na <rhyme label="g" type="m">zoen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Niet zonder// <rhyme label="a" type="f">verwonder</rhyme>//</l>
        <l id="s2:l2" met="2" type="-+">Dat w’onder de <rhyme label="b" type="m">lien</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Van kennis// noch <rhyme label="c" type="f">schennis</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Van grouwel en <rhyme label="b" type="m" corresp="#s2:l2">zien</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Want al// te <rhyme label="d" type="m">mal</rhyme>//</l>
        <l id="s2:l6" met="2" type="-+">En dwaes// <rhyme label="e" type="m">helaes</rhyme>//</l>
        <l id="s2:l7" met="2" type="-+">De wenschen der <rhyme label="f" type="f">menschen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Sijn tot dit <rhyme label="g" type="m">aes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Als smoken// van <rhyme label="a" type="f">roken</rhyme>//</l>
        <l id="s3:l2" met="2" type="-+">Verdwijnen, zoo <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s3:l3" met="2" type="-+">De beelden// der <rhyme label="c" type="f">weelden</rhyme>//</l>
        <l id="s3:l4" met="2" type="-+">En scheyden met <rhyme label="b" type="m" corresp="#s3:l2">pijn</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Het zoet// <rhyme label="d" type="m">vervloedt</rhyme>//</l>
        <l id="s3:l6" met="2" type="-+">Maer ’t zuyr// heeft <rhyme label="e" type="m">duir</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">O zielen// hoe <rhyme label="f" type="f">ielen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Vw’ paen// <rhyme label="g" type="m">voordaen</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wild vrezen// het <rhyme label="a" type="f">wezen</rhyme>//</l>
        <l id="s4:l2" met="2" type="-+">Des Heren <rhyme label="b" type="m">gewis</rhyme>;</l>
        <l id="s4:l3" met="2" type="-+">Die allen// uw’ <rhyme label="c" type="f">vallen</rhyme>//</l>
        <l id="s4:l4" met="2" type="-+">Aenschouwende <rhyme label="b" type="m" corresp="#s4:l2">is</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Zijn oog// zit <rhyme label="d" type="m">hoog</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Maer ziet om <rhyme label="e" type="m">laeg</rhyme></l>
        <l id="s4:l7" met="2" type="-+">De minnen// der <rhyme label="f" type="f">zinnen</rhyme>//</l>
        <l id="s4:l8" met="2" type="-+">By nacht by <rhyme label="g" type="m">daeg</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Geen meerder// geen <rhyme label="a" type="f">teerder</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Liefhebber als <rhyme label="b" type="m">God</rhyme>:</l>
        <l id="s5:l3" met="2" type="-+">Geen vader// geen <rhyme label="c" type="f">dader</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Van meerder <rhyme label="b" type="m" corresp="#s5:l2">genot</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Ontziet// en <rhyme label="d" type="m">bied</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Dan vrucht// en <rhyme label="e" type="m">ducht</rhyme>//</l>
        <l id="s5:l7" met="2" type="-+">Te feilen// aen <rhyme label="f" type="f">heilen</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Van zulken <rhyme label="g" type="m">zucht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Zijn bloeden// van <rhyme label="a" type="f">goeden</rhyme>//</l>
        <l id="s6:l2" met="2" type="-+">En hebben geen <rhyme label="b" type="m">maet</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Maer tegen// zijn <rhyme label="c" type="f">zegen</rhyme>//</l>
        <l id="s6:l4" met="2" type="-+">Te jammeren <rhyme label="b" type="m" corresp="#s6:l2">staet</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Voor ’t kaf// een <rhyme label="d" type="m">straf</rhyme>//</l>
        <l id="s6:l6" met="2" type="-+">Van vier// van <rhyme label="e" type="m">pier</rhyme>//</l>
        <l id="s6:l7" met="2" type="-+">Die knagen// en <rhyme label="f" type="f">plagen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Zal ’t zondig <rhyme label="g" type="m">dier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">d’Ellenden// geen <rhyme label="a" type="f">ende</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Verwachten, wild <rhyme label="b" type="m">dan</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Goods woorden// als <rhyme label="c" type="f">koorden</rhyme>//</l>
        <l id="s7:l4" met="2" type="-+">Gebruyken, om <rhyme label="b" type="m" corresp="#s7:l2">van</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Het quaed// mis <rhyme label="d" type="m">daed</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Gevlucht// u <rhyme label="e" type="m">zucht</rhyme></l>
        <l id="s7:l7" met="2" type="-+">En zinnen// te <rhyme label="f" type="f">binnen</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Aen God, en <rhyme label="g" type="m">tucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
