    <melody>Wat is er groter vreugde op aarde</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wat isser grooter vreugt op   <rhyme label="a" type="m">aart</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Als <rhyme label="a" type="m" corresp="#s1:l1">gepaart</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Met malkaar in vreugt te <rhyme label="b" type="f">wesen</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Daar twe samen, eens van <rhyme label="a" type="m" corresp="#s1:l1">zin</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Vloeijen uyt een suivre <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dan hoeft men nergens voor te <rhyme label="b" type="f" corresp="#s1:l3">vreesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Min ontsteekt een hert vol <rhyme label="a" type="m">gloed</rhyme></l>
        <l id="s2:l2" met="2" type="+-">En geeft <rhyme label="a" type="m" corresp="#s2:l1">moet</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Aan de bloodste van de <rhyme label="b" type="f">mensen</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Jonk en Oud gevoelt de <rhyme label="a" type="m" corresp="#s2:l1">vlam</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Die eerst van de Goden <rhyme label="a" type="m" corresp="#s2:l1">quam</rhyme>:</l>
        <l id="s2:l6" met="4" type="+-">Wie sou dan de Min <rhyme label="b" type="f" corresp="#s2:l3">verwensen</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Venus die ontvonkte <rhyme label="a" type="m">Mars</rhyme>:</l>
        <l id="s3:l2" met="2" type="+-">Self hoe <rhyme label="a" type="m" corresp="#s3:l1">bars</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hy moest voor de Liefde <rhyme label="b" type="f">buygen</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Sonder ‘t soete Venus <rhyme label="a" type="m" corresp="#s3:l1">kind</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Die de koelste herten <rhyme label="a" type="m" corresp="#s3:l1">bind</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Viel de waereld gansch in <rhyme label="b" type="f" corresp="#s3:l3">duygen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Waar is nu myn <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Die ik <rhyme label="a" type="m" corresp="#s4:l1">min</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Waar mag ‘t poesele meysje <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die op Venus <rhyme label="a" type="m" corresp="#s4:l1">ledikant</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Blussen sal myn <rhyme label="a" type="m" corresp="#s4:l1">minnebrant</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Als die schoonste plag voor <rhyme label="b" type="f" corresp="#s4:l3">desen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ach, daar komt dat <rhyme label="a" type="m">troeteldier</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">‘t Oog vol <rhyme label="a" type="m" corresp="#s5:l1">vier</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">‘k Voel mijn koude boesem <rhyme label="b" type="f">blaken</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Gy sult desen nagt <rhyme label="a" type="m" corresp="#s5:l1">gerust</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Smaken soete <rhyme label="a" type="m" corresp="#s5:l1">minnelust</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Als wy aan het streelen <rhyme label="b" type="f" corresp="#s5:l3">raken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dartel Boeltje, gaat te <rhyme label="a" type="m">rust</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Want myn <rhyme label="a" type="m" corresp="#s6:l1">lust</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Werd   geprest door groot <rhyme label="b" type="f">verlangen</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Gy sult my als een <rhyme label="a" type="m" corresp="#s6:l1">piloot</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Haast   sien varen in uw <rhyme label="a" type="m" corresp="#s6:l1">schoot</rhyme></l>
        <l id="s6:l6" met="4" type="+-">En daar volle vreugd <rhyme label="b" type="f" corresp="#s6:l3">ontvangen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
