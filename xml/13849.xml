    <melody>Florida zo het wezen mag</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">TErwijl veel Maeghden van de <rhyme label="a" type="m">Stadt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gants trots, hooghoedigh, preuts en <rhyme label="a" type="m" corresp="#s1:l1">prat</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Teghen ons Harders <rhyme label="b" type="f">stellen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Tot loon, der deughden, haer <rhyme label="c" type="m">gheschiedt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Maer seggen, ‘k machme <rhyme label="c" type="m" corresp="#s1:l4">niet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met sulcke Kinckels <rhyme label="b" type="f" corresp="#s1:l3">quellen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daerom verkoos ick tot mijn <rhyme label="a" type="m">stee</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Het veldt, by ‘t selschap van mijn <rhyme label="a" type="m" corresp="#s2:l1">Vee</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En Boerse <rhyme label="b" type="f">Harderinnen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En heb de Hoofs’ en Steedtse <rhyme label="c" type="m">pracht</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Met haer gevolgh <rhyme label="c" type="m" corresp="#s2:l4">veracht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Vyt afgekeerde <rhyme label="b" type="f" corresp="#s2:l3">sinnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maer ghy, mijn lieve <rhyme label="a" type="m">Roosemond</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ghy hebt mijn hart ter doodt <rhyme label="a" type="m" corresp="#s3:l1">gewond</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En voort gevoerd <rhyme label="b" type="f">gevangen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Door ‘t soet van u bedouwde <rhyme label="c" type="m">Tongh</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die door mijn nieren <rhyme label="c" type="m" corresp="#s3:l4">drongh</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ly ick dees zware <rhyme label="b" type="f" corresp="#s3:l3">prangen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick roem u aldervroomst <rhyme label="a" type="m">ghemoedt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hart-aer, en Borne van mijn <rhyme label="a" type="m" corresp="#s4:l1">soet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Vyt alle soete <rhyme label="b" type="f">dinghen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En t’wijl u over-braef <rhyme label="c" type="m">ghelaet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">My door ‘et harte <rhyme label="c" type="m" corresp="#s4:l4">gaet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Treckt ghy my <rhyme label="b" type="f" corresp="#s4:l3">sonderlinghe</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Denckt dan het overstout <rhyme label="a" type="m">bestaen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Van ‘t vry en moedigh spreecken <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">In u <rhyme label="b" type="f">goedtdadigheden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">T’wijl ghy my sluymer-lustigh <rhyme label="c" type="m">weckt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En vyerigh tot u <rhyme label="c" type="m" corresp="#s5:l4">treckt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Soo staet mijn doen in <rhyme label="b" type="f" corresp="#s5:l3">reden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
