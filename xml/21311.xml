    <melody>De vogeltjes in de muiten</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">C Oemt al van Suyden ende <rhyme label="a" type="f">Oosten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die met sonden zijt <rhyme label="b" type="m">belaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick sal v wel <rhyme label="a" type="f" corresp="#s1:l1">vertroosten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met mijn vleesch sal ick v <rhyme label="b" type="m" corresp="#s1:l2">versaen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ick ben die fonteyne <rhyme label="c" type="f">verheuen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die dorst heeft die come <rhyme label="d" type="m">naer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die daer wt drinct sal eewich <rhyme label="c" type="f" corresp="#s1:l5">leuen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Spreect Christus die Heer int <rhyme label="d" type="m" corresp="#s1:l6">openbaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick en ben hier niet <rhyme label="a" type="f">ghecomen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">O menschen zijt dies wel <rhyme label="b" type="m">vroet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat ick v soude <rhyme label="a" type="f" corresp="#s2:l1">verdomen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Maer verlossen al met mijn <rhyme label="b" type="m" corresp="#s2:l2">bloet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ick begheer in dijn herte te <rhyme label="c" type="f">woonen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daer en moet gheen ander <rhyme label="d" type="m">in</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Alleen moet ghy op my <rhyme label="c" type="f" corresp="#s2:l5">stoonen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Oft ghy en coemt niet daer ick <rhyme label="d" type="m" corresp="#s2:l6">bin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick ben den wijnstock wt <rhyme label="a" type="f">ghesproten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Spreect Christus verstaet mijn <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Blijft vast in my <rhyme label="a" type="f" corresp="#s3:l1">ghesloten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ghy sult bringhen goede vruchten <rhyme label="b" type="m" corresp="#s3:l2">voort</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Maer laet ghy v <rhyme label="c" type="f">verleyden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat ghy menschen troost <rhyme label="d" type="m">aensiet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">So suldy vanden wijnstock worden <rhyme label="c" type="f" corresp="#s3:l5">gescheyden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Alle v vruchten en sullen dooghen <rhyme label="d" type="m" corresp="#s3:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hebt lief en <rhyme label="a" type="f">wtuercoren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die v ter werelt doen <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Die v boosheyt legghen te <rhyme label="a" type="f" corresp="#s4:l1">voren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Bidt daer voor, tis Christus <rhyme label="b" type="m" corresp="#s4:l2">raet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Want God heeft selue <rhyme label="c" type="f">ghesproken</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Aenden cruyce met luyder <rhyme label="d" type="m">stem</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Och vader en latet niet zijn <rhyme label="c" type="f" corresp="#s4:l5">ghewroken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ick bid v vergheuet <rhyme label="d" type="m" corresp="#s4:l6">hem</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En vreest niet al te <rhyme label="a" type="f">samen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Mijn woort voor die werelt <rhyme label="b" type="m">belijt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Oft ick sal v <rhyme label="a" type="f" corresp="#s5:l1">beschamen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Uoor mijnen Uader <rhyme label="b" type="m" corresp="#s5:l2">ghebenedijt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Aldus en wilt niet <rhyme label="c" type="f">vreesen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Maer strijt met herten <rhyme label="d" type="m">vro</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Laet den knecht ghelijck den meester <rhyme label="c" type="f" corresp="#s5:l5">wesen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Het belieft den Uader <rhyme label="d" type="m" corresp="#s5:l6">also</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ic ben een goet herder wter <rhyme label="a" type="f">maten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ick gae mijn schaepkens <rhyme label="b" type="m">voor</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ick begheer mijn leuen daer voor te <rhyme label="a" type="f" corresp="#s6:l1">laten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ick ben die rechte <rhyme label="b" type="m" corresp="#s6:l2">door</rhyme></l>
        <l id="s6:l5" met="3" type="-+">En laet v niet <rhyme label="c" type="f">verleyden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Uan die daer buyten der doren <rhyme label="d" type="m">passeert</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Tzijn dieuen en moordenaers <rhyme label="c" type="f" corresp="#s6:l5">beyden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Spreect Christus so hy ons <rhyme label="d" type="m" corresp="#s6:l6">leert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Niemant en coemt totten <rhyme label="a" type="f">Uader</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dan door den sone Gods <rhyme label="b" type="m">alleen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Hy bidt voor ons <rhyme label="a" type="f" corresp="#s7:l1">allegader</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Weest toch met Christum te <rhyme label="b" type="m" corresp="#s7:l2">vreen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Wilt vast op hem <rhyme label="c" type="f">betrouwen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Hy is die verworpenen <rhyme label="d" type="m">steen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Doet ghijt niet het sal v <rhyme label="c" type="f" corresp="#s7:l5">berouwen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Als ghy van hier sult <rhyme label="d" type="m" corresp="#s7:l6">scheen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Prince God aensiet ons <rhyme label="a" type="f">ellende</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Hier in dit eertsche <rhyme label="b" type="m">dal</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Wilt uwen gheest hier neder <rhyme label="a" type="f" corresp="#s8:l1">senden</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Die ons vertroosten <rhyme label="b" type="m" corresp="#s8:l2">sal</rhyme></l>
        <l id="s8:l5" met="3" type="-+">So moghen wy v die eer <rhyme label="c" type="f">gheuen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Al inden hemelschen <rhyme label="d" type="m">throon</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Door Christum, hy is ons <rhyme label="c" type="f" corresp="#s8:l5">leuen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Wy verwachten van hem die <rhyme label="d" type="m" corresp="#s8:l6">croon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
