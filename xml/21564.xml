  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VVilt onderhouden Gods <rhyme label="a" type="f">gheboden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Siet aen oock elck <rhyme label="b" type="m">sije</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En kleeft aen gheen vreemde <rhyme label="a" type="f" corresp="#s1:l1">goden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Tis Baruchs <rhyme label="b" type="m" corresp="#s1:l2">Prophecie</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Laet alle die <rhyme label="b" type="m" corresp="#s1:l2">afgoderie</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Heel van v zijn <rhyme label="c" type="f">verdreuen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Dagon seer fel, en zijn <rhyme label="d" type="m">opstel</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En wilt dat niet <rhyme label="c" type="f" corresp="#s1:l6">aenkleuen</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Sijn dienaers hier, sy spouwen <rhyme label="e" type="m">vier</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Op den onnooselen <rhyme label="c" type="f" corresp="#s1:l6">onbeseuen</rhyme></l>
        <l id="s1:l11" met="3" type="-+">En brenghense in swaer <rhyme label="e" type="m" corresp="#s1:l9">dangier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Men siet nv veel <rhyme label="a" type="f">gheseten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Meer dan men noeyt en <rhyme label="b" type="m">de</rhyme></l>
        <l id="s2:l3" met="3" type="-+">God is so seer <rhyme label="a" type="f" corresp="#s2:l1">vergheten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat een kalf staet in zijn <rhyme label="b" type="m" corresp="#s2:l2">ste</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Daeromme leet Daniel <rhyme label="b" type="m" corresp="#s2:l2">onvre</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Het mach God wel <rhyme label="c" type="f">verdrieten</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Want sy seer qaaet en <rhyme label="d" type="m">opstinaet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Fenijnighe pijlen <rhyme label="c" type="f" corresp="#s2:l6">schieten</rhyme></l>
        <l id="s2:l9" met="4" type="+-">In uwen wijn, klaer ende <rhyme label="e" type="m">fijn</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Daer sietmen vuyl water in <rhyme label="c" type="f" corresp="#s2:l6">ghieten</rhyme></l>
        <l id="s2:l11" met="3" type="-+">Ende menghen al met <rhyme label="e" type="m" corresp="#s2:l9">fenijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Men siet nv sonder <rhyme label="a" type="f">versaghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In Babylonien <rhyme label="b" type="m">stout</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Al op die schouderen <rhyme label="a" type="f" corresp="#s3:l1">draghen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Steenen ende daer toe <rhyme label="b" type="m" corresp="#s3:l2">hout</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Ia Godekens van siluer en <rhyme label="b" type="m" corresp="#s3:l2">hout</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Bieden sy lof en <rhyme label="c" type="f">eere</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Maer als ghijt sult sien ten seluen <rhyme label="d" type="m">tien</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En versaghet daer af niet <rhyme label="c" type="f" corresp="#s3:l6">seere</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Stout onbeureest, smaect wel den <rhyme label="e" type="m">keest</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Wilt aenbeden God den <rhyme label="c" type="f" corresp="#s3:l6">Heere</rhyme></l>
        <l id="s3:l11" met="3" type="-+">En inwendich al in den <rhyme label="e" type="m" corresp="#s3:l9">gheest</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Pharao koemt hem <rhyme label="a" type="f">storen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Teghen Gods gheest inden lesten <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Antechrist is nv <rhyme label="a" type="f" corresp="#s4:l1">gheboren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die de waerheyt seer <rhyme label="b" type="m" corresp="#s4:l2">benijt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Want hy veruolchtse breet ende <rhyme label="b" type="m" corresp="#s4:l2">wijt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Die vander waerheyt <rhyme label="c" type="f">spreken</rhyme></l>
        <l id="s4:l7" met="4" type="-+">En brenghtse ter doot met loghenen <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Al door zijn valsche <rhyme label="c" type="f" corresp="#s4:l6">treken</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Tfenijnich Serpent, heeft nv <rhyme label="e" type="m">gheschent</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Den onnooselen <rhyme label="c" type="f" corresp="#s4:l6">onbesweken</rhyme></l>
        <l id="s4:l11" met="3" type="-+">En ghebrocht in swaer <rhyme label="e" type="m" corresp="#s4:l9">torment</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt nv den Heere <rhyme label="a" type="f">louen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat Princelicke <rhyme label="b" type="m">greyn</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En laet v niet <rhyme label="a" type="f" corresp="#s5:l1">verdouen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Van valsche leeraers <rhyme label="b" type="m" corresp="#s5:l2">vileyn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Al saecht ghy eenen Enghel <rhyme label="b" type="m" corresp="#s5:l2">reyn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Al wt den Hemel <rhyme label="c" type="f">sincken</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Die het ghebot, gaue anders dan <rhyme label="d" type="m">God</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En wilt hem niet <rhyme label="c" type="f" corresp="#s5:l6">gedincken</rhyme></l>
        <l id="s5:l9" met="4" type="+-">Die dorstende zijt in deser <rhyme label="e" type="m">tijt</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Coemt tot my om te <rhyme label="c" type="f" corresp="#s5:l6">drincken</rhyme></l>
        <l id="s5:l11" met="3" type="-+">Spreect Christus <rhyme label="e" type="m" corresp="#s5:l9">ghebenedijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
