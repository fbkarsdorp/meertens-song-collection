<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>32769</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="paaslied" translation="Easter song">paaslied</tag>
    </tags>
    <melody>Apollo's stralen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">COmpt neder <rhyme label="a" type="f">daelen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Met uwe <rhyme label="a" type="f" corresp="#s1:l1">straelen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">O drymael heyligh <rhyme label="b" type="m">Geest</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ontsteeckt nu minst en <rhyme label="b" type="m" corresp="#s1:l3">meest</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ons hert, ziel en <rhyme label="c" type="m">ghemoet</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Met Liefdens <rhyme label="c" type="m" corresp="#s1:l5">soet</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Wilt keeren <rhyme label="d" type="m">t'quaedt</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Door u <rhyme label="d" type="m" corresp="#s1:l7">ghenaedt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Neempt ons in u <rhyme label="c" type="m" corresp="#s1:l5">behoedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Naer u <rhyme label="a" type="f">begheeren</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Wilt van ons <rhyme label="a" type="f" corresp="#s2:l1">weeren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">De quaey <rhyme label="b" type="m">genegenheydt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die stadigh ons <rhyme label="b" type="m" corresp="#s2:l3">verleydt</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">De sonde vleyt schoon <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Met valsch <rhyme label="c" type="m" corresp="#s2:l5">vermaen</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Gheeft ons de <rhyme label="d" type="m">kracht</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Door uwe <rhyme label="d" type="m" corresp="#s2:l7">macht</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Om haer te <rhyme label="c" type="m" corresp="#s2:l5">weder-staen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Fonteyn der <rhyme label="a" type="f">deughden</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Vol soete <rhyme label="a" type="f" corresp="#s3:l1">vreughden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Waer uyt ghenade <rhyme label="b" type="m">vloeyt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die onse ziel <rhyme label="b" type="m" corresp="#s3:l3">besproeyt</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Comt voester, voeyt en <rhyme label="c" type="m">laeft</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Die tot u <rhyme label="c" type="m" corresp="#s3:l5">draeft</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Den Aertschen <rhyme label="d" type="m">mensch</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Die ghy naer <rhyme label="d" type="m" corresp="#s3:l7">wensch</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Met wijsheydt hebt <rhyme label="c" type="m" corresp="#s3:l5">begaeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Laet ons <rhyme label="a" type="f">ghebeden</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">En klaecht-baer <rhyme label="a" type="f" corresp="#s4:l1">reden</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Van u doch zijn <rhyme label="b" type="m">verhoort</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Weest nu niet meer <rhyme label="b" type="m" corresp="#s4:l3">verstoort</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Spaert ons ghenadigh <rhyme label="c" type="m">heer</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Weerdt alder <rhyme label="c" type="m" corresp="#s4:l5">eer</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">Wy roepen <rhyme label="d" type="m">steedts</rhyme>:</l>
        <l id="s4:l8" met="2" type="-+">O Gheest des <rhyme label="d" type="m" corresp="#s4:l7">Vreedts</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Neemt tot ons uwen <rhyme label="c" type="m" corresp="#s4:l5">keer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
