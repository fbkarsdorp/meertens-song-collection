<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25319</id>
    <date>
      <year-start>1582</year-start>
      <year-end>1582</year-end>
    </date>
    <location>
      <orginal-name>bisdom utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="praise song">loflied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody>Lauda Sion salvatorem</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Loeft sijon v <rhyme label="a" type="f">salichmaeker</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Loeft v leijdsman loeft v <rhyme label="a" type="f" corresp="#s1:l1">waker</rhyme></l>
        <l id="s1:l3" met="4" type="+-">In hijmnen met zoet <rhyme label="b" type="m">gesang</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Soe veel ghij moecht soe veel ghij <rhyme label="c" type="f">doet</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Want hij is alle loff te <rhyme label="c" type="f" corresp="#s1:l4">groet</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Vol te louen valt v <rhyme label="b" type="m" corresp="#s1:l3">prang</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Broot dat leeft, ende geeft <rhyme label="a" type="f">t'leuen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Om te louen voort <rhyme label="a" type="f" corresp="#s2:l1">gegeuen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Ende heden voor <rhyme label="b" type="m">gestelt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Welck int leste <rhyme label="c" type="f">auondmael</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Christus geeft sijn broeders te <rhyme label="c" type="f" corresp="#s2:l4">mael</rhyme></l>
        <l id="s2:l6" met="4" type="+-">ongetuijfelt, <rhyme label="b" type="m" corresp="#s2:l3">ongescelt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laet t'loff vol zijn met geluijt <rhyme label="a" type="f">claer</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Oeck genoechlick oeck scoen <rhyme label="a" type="f" corresp="#s3:l1">voerwaer</rhyme></l>
        <l id="s3:l3" met="4" type="+-">des herten vermake <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Want heden soo houdmen <rhyme label="c" type="f">feeste</rhyme></l>
        <l id="s3:l5" met="4" type="+-">dat is ingestelt nu <rhyme label="c" type="f" corresp="#s3:l4">eeste</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Insettinge van dit <rhyme label="b" type="m" corresp="#s3:l3">broet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">In dees taefel des nieu <rhyme label="a" type="f">Conincks</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Een nieu paeslam nieu <rhyme label="a" type="f" corresp="#s4:l1">versonincks</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Het ouwets paeschlam neemt <rhyme label="b" type="m">ent</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Het nieu verdrijfft den <rhyme label="c" type="f">ouderdom</rhyme></l>
        <l id="s4:l5" met="4" type="+-">de waerheijt maeckt die figuer <rhyme label="c" type="f" corresp="#s4:l4">stom</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Tlicht veriaecht den nacht <rhyme label="b" type="m" corresp="#s4:l3">present</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dat Christus heeft nae tmael <rhyme label="a" type="f">gedaen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Heeft betoent wat men sal doen <rhyme label="a" type="f" corresp="#s5:l1">tsaen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">In sijnder <rhyme label="b" type="m">gedenckenis</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Geleert door sijn <rhyme label="c" type="f">insettinge</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Broet ende wijn door <rhyme label="c" type="f" corresp="#s5:l4">wijdinge</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Hostij des <rhyme label="b" type="m" corresp="#s5:l3">behoudenis</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Den Christen mens geeftmen <rhyme label="a" type="f">lering</rhyme></l>
        <l id="s6:l2" met="4" type="+-">dat broet vleijsch wijn bloet <rhyme label="a" type="f" corresp="#s6:l1">gering</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Wort verandert <rhyme label="b" type="m">machtelick</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Dat ghij niet siet ofte <rhyme label="c" type="f">begrijpt</rhyme></l>
        <l id="s6:l5" met="4" type="+-">starck tgeloeff neem wech s'harten <rhyme label="c" type="f" corresp="#s6:l4">strijt</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Bouen natuer <rhyme label="b" type="m" corresp="#s6:l3">crachtelick</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Onder versceijden <rhyme label="a" type="f">gedaenten</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Alleen teijcken ende <rhyme label="a" type="f" corresp="#s7:l1">waenten</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Is verburgen <rhyme label="b" type="m">t'costelick</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Vleijs tot spijse, Broet dranck <rhyme label="c" type="f">ele</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Blijft christus nochtans <rhyme label="c" type="f" corresp="#s7:l4">gehele</rhyme></l>
        <l id="s7:l6" met="4" type="+-">onder gedaent <rhyme label="b" type="m" corresp="#s7:l3">weselick</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Van diet nutten niet <rhyme label="a" type="f">doersneden</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Niet doerbreken, off <rhyme label="a" type="f" corresp="#s8:l1">ontleeden</rhyme></l>
        <l id="s8:l3" met="4" type="+-">maer wort omfangen <rhyme label="b" type="m">geheel</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Nutttent duijsent, nuttet <rhyme label="c" type="f">ene</rhyme></l>
        <l id="s8:l5" met="4" type="+-">soo veel deese alst <rhyme label="c" type="f" corresp="#s8:l4">gemeene</rhyme></l>
        <l id="s8:l6" met="4" type="+-">en wort niet minder doer <rhyme label="b" type="m" corresp="#s8:l3">veel</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Off die goet, dan off die quaet <rhyme label="a" type="f">sijn</rhyme></l>
        <l id="s9:l2" met="4" type="+-">die een ten leuen den ander tot <rhyme label="a" type="f" corresp="#s9:l1">pijn</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Nutten haer <rhyme label="b" type="m">verdoemenis</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Der quaden doet, den goe <rhyme label="c" type="f">tleuen</rhyme></l>
        <l id="s9:l5" met="4" type="+-">siet versceijden loen het <rhyme label="c" type="f" corresp="#s9:l4">geue</rhyme></l>
        <l id="s9:l6" met="4" type="+-">eender <rhyme label="b" type="m" corresp="#s9:l3">omfanckenis</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
