  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NU ben ik out van <rhyme label="a" type="f">jaaren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Annetten is myn <rhyme label="b" type="m">ontrukt</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Eerst poogden ik te <rhyme label="a" type="f" corresp="#s1:l1">paaren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Door dertele <rhyme label="b" type="m" corresp="#s1:l2">minnedrift</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De dood kan niemant <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">De mensch leeft maar in <rhyme label="b" type="m">schyn</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Wie zou daar niet om <rhyme label="a" type="f" corresp="#s2:l1">treuren</rhyme>?</l>
        <l id="s2:l4" met="3" type="-+">Om altoos alleen te <rhyme label="b" type="m" corresp="#s2:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy kon my ligt <rhyme label="a" type="f">bestieren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">In rampspoet en in <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met haar heusen <rhyme label="a" type="f" corresp="#s3:l1">manieren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daar moest een scheiding <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nu zal ik myn <rhyme label="a" type="f">verbeyden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">In eenzaamheid van <rhyme label="b" type="m">geest</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Tot de dood my komt <rhyme label="a" type="f" corresp="#s4:l1">verblyden</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">Leef ik maar <rhyme label="b" type="m" corresp="#s4:l2">onbevreest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
