    <melody>Psalm 068 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIe Godt heeft tot sijn schilt en <rhyme label="a" type="m">broght</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wat cost die mensch te zijn <rhyme label="a" type="m" corresp="#s1:l1">besorght</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat hem niets quaets sou <rhyme label="b" type="f">hind'ren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Want Godt die is tot haer <rhyme label="c" type="m">gesindt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Die hy voorheenen heeft <rhyme label="c" type="m" corresp="#s1:l4">bemint</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En noemt haer sijne <rhyme label="b" type="f" corresp="#s1:l3">kind'ren</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Hy trecktse uyt d'eeuwige <rhyme label="d" type="m">doodt</rhyme>:</l>
        <l id="s1:l8" met="4" type="-+">En staet haer by in anghst of <rhyme label="d" type="m" corresp="#s1:l7">noot</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Met sijn geest vol <rhyme label="e" type="f">genaden</rhyme>:</l>
        <l id="s1:l10" met="4" type="-+">Hy leydt haer op de rechte <rhyme label="f" type="m">baen</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Die yder Christen hier moet <rhyme label="f" type="m" corresp="#s1:l10">gaen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Geen dingh en mach haer <rhyme label="e" type="f" corresp="#s1:l9">schaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De vroome vreest geen <rhyme label="a" type="m">ongeval</rhyme>:</l>
        <l id="s2:l2" met="4" type="-+">Al wandelt hy in des doodts <rhyme label="a" type="m" corresp="#s2:l1">dal</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Geen haer en kan hem <rhyme label="b" type="f">krencken</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">De Dagh des Heeren daer op <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dus is de macht des vyandts <rhyme label="c" type="m" corresp="#s2:l4">niet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wat listen sy <rhyme label="b" type="f" corresp="#s2:l3">bedencken</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">Al werpt hy Ioseph in den <rhyme label="d" type="m">kuyl</rhyme>:</l>
        <l id="s2:l8" met="4" type="-+">Daniel voor der Leeuwen <rhyme label="d" type="m" corresp="#s2:l7">muyl</rhyme>:</l>
        <l id="s2:l9" met="3" type="-+">Al socht hy oock te <rhyme label="e" type="f">braden</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Sadrach, Mesach, <rhyme label="f" type="m">Abednego</rhyme>:</l>
        <l id="s2:l11" met="4" type="+-">Maer daer Godt toont sijn hulp <rhyme label="f" type="m" corresp="#s2:l10">alsoo</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Geen dingh en mach haer <rhyme label="e" type="f" corresp="#s2:l9">schaden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Godt toonden hem een hulp te <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">By Israel in de <rhyme label="a" type="m" corresp="#s3:l1">woestijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En sloegh al haer <rhyme label="b" type="f">vyanden</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Daer Gidion en <rhyme label="c" type="m">Iosua</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Door Barac, Simson en <rhyme label="c" type="m" corresp="#s3:l4">Iephtha</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Oock veel door Davids <rhyme label="b" type="f" corresp="#s3:l3">handen</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Daer om steund niet op paert, of <rhyme label="d" type="m">macht</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Maer op des Heeren hulp, en <rhyme label="d" type="m" corresp="#s3:l7">kracht</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Wanneer ghy sijt <rhyme label="e" type="f">beladen</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">Den vyandt vlucht en is <rhyme label="f" type="m">verbaest</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Soo haest den vroomen Tromp maer <rhyme label="f" type="m" corresp="#s3:l10">blaest</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Geen dingh mach haer <rhyme label="e" type="f" corresp="#s3:l9">schaden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Eng'len hebben (tot <rhyme label="a" type="m">behoudt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Der vrome) haer Leger <rhyme label="a" type="m" corresp="#s4:l1">gebout</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gestelt rondom sijn <rhyme label="b" type="f">Hutten</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Dus hy geen Iagers pijlen <rhyme label="c" type="m">acht</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Noch pest, noch grouwel, in der <rhyme label="c" type="m" corresp="#s4:l4">nacht</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Want Godt wil hun <rhyme label="b" type="f" corresp="#s4:l3">beschutten</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Al viel de werelt over <rhyme label="d" type="m">hoop</rhyme></l>
        <l id="s4:l8" met="4" type="-+">De vrome neemt tot Godt sijn <rhyme label="d" type="m" corresp="#s4:l7">loop</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Want hy is vol <rhyme label="e" type="f">weldaden</rhyme>:</l>
        <l id="s4:l10" met="4" type="-+">Al komt daer diert, of honger <rhyme label="f" type="m">groot</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Gezegent is, en blijft, sijn <rhyme label="f" type="m" corresp="#s4:l10">broodt</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Geen dingh en mach haer <rhyme label="e" type="f" corresp="#s4:l9">schaden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Nochtans de vrome oock wel <rhyme label="a" type="m">lijt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Maer dan wordt hy slechs <rhyme label="a" type="m" corresp="#s5:l1">gekastijt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">'t Welck voor geluck is 't <rhyme label="b" type="f">achten</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Om niet van 't quaedt te zijn <rhyme label="c" type="m">verleydt</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Dies streckt hem 't kruys tot <rhyme label="c" type="m" corresp="#s5:l4">saligheydt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wie kan doch meer <rhyme label="b" type="f" corresp="#s5:l3">verwachten</rhyme>:</l>
        <l id="s5:l7" met="4" type="-+">Hierom ghy Akoleyen <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Wiens geest nu ondersoeckt het <rhyme label="d" type="m" corresp="#s5:l7">goet</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Ey! Vreest niet voor den <rhyme label="e" type="f">quaden</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Want als Godt voor liefd' boven <rhyme label="f" type="m">Al</rhyme></l>
        <l id="s5:l11" met="4" type="+-">Sijn Wijngaert-rancken helpen <rhyme label="f" type="m" corresp="#s5:l10">sal</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Geen dingh en mach haer <rhyme label="e" type="f" corresp="#s5:l9">schaden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
