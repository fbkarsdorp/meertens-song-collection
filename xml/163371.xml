<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>163371</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Luz de mi alma</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WAer toe o <rhyme label="a" type="f">minne</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Prickelt ghy mijn <rhyme label="b" type="m">bloet</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Wegh uyt mijn <rhyme label="a" type="f" corresp="#s1:l1">sinne</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Wegh uyt mijn <rhyme label="b" type="m" corresp="#s1:l2">gemoet</rhyme></l>
        <l id="s1:l5" met="7" type="-+">Wegh min ick dan u uyt mijn harte, gae soete wreetheyt <rhyme label="c" type="m">gae</rhyme></l>
        <l id="s1:l6" met="5" type="-+">V lieve quellingh doet mijn leven <rhyme label="c" type="m" corresp="#s1:l5">schae</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hoe kan ick <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Daer ick ziel en <rhyme label="b" type="m">sin</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Haer heb <rhyme label="a" type="f" corresp="#s2:l1">gegeven</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Dien ick trou <rhyme label="b" type="m" corresp="#s2:l2">bemin</rhyme></l>
        <l id="s2:l5" met="7" type="-+">O min die my uyt lust bekoorde, tot wellust van een <rhyme label="c" type="m">Vrouw</rhyme></l>
        <l id="s2:l6" met="5" type="-+">Wegh soete minne, wegh ick heb <rhyme label="c" type="m" corresp="#s2:l5">berou</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">'t Scheen dat u <rhyme label="a" type="f">sinne</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Lieve <rhyme label="b" type="m">Rosemond</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Mijn wilde <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme>:</l>
        <l id="s3:l4" met="3" type="+-">Maer geveynsde <rhyme label="b" type="m" corresp="#s3:l2">gront</rhyme></l>
        <l id="s3:l5" met="7" type="-+">Heeft myn door schijn van gunst bedrogen, 't vertrouden op het <rhyme label="c" type="m">geen</rhyme></l>
        <l id="s3:l6" met="5" type="-+">Daer ghy mijn Lusjes stelde me te <rhyme label="c" type="m" corresp="#s3:l5">vreen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Dat waren <rhyme label="a" type="f">kusjes</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Ja het was noch <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Gaeft ghy myn <rhyme label="a" type="f" corresp="#s4:l1">lusjes</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Niet al haer <rhyme label="b" type="m" corresp="#s4:l2">begeer</rhyme>,</l>
        <l id="s4:l5" met="7" type="-+">Mocht ick niet na mijn wil gemeten, al t geen de minne <rhyme label="c" type="m">wou</rhyme></l>
        <l id="s4:l6" met="5" type="-+">Hoe Rosimonde? hoe my dus <rhyme label="c" type="m" corresp="#s4:l5">ontrou</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Wat denckt u <rhyme label="a" type="f">herte</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Als de min tot <rhyme label="b" type="m">spijt</rhyme></l>
        <l id="s5:l3" met="2" type="-+">V dese <rhyme label="a" type="f" corresp="#s5:l1">perte</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Rosimond <rhyme label="b" type="m" corresp="#s5:l2">verwijt</rhyme>,</l>
        <l id="s5:l5" met="7" type="-+">Wat denckt ghy doch als u gedachten op onse vryheyt <rhyme label="c" type="m">voen</rhyme>?</l>
        <l id="s5:l6" met="5" type="-+">Ach! Rosimonde, ach! hoe kund ghy 't <rhyme label="c" type="m" corresp="#s5:l5">doen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
