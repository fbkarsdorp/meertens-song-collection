  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HOort eens onze Dochters <rhyme label="a" type="f">klagen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Datze niet getrouwt en <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat zy in haar jonge <rhyme label="a" type="f" corresp="#s1:l1">dagen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Moeten lijden zo veel <rhyme label="b" type="m" corresp="#s1:l2">pijn</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">'t Is slechten tijdt’ niemandt en <rhyme label="c" type="m">vrijt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">'t Schijnt Kupido gebonden <rhyme label="c" type="m" corresp="#s1:l5">leidt</rhyme>.</l>
        <l id="s1:l7" met="5" type="+-">Zalder ons dan niemandt komen <rhyme label="d" type="f">troosten</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Eer ons jonge Jeugt <rhyme label="c" type="m" corresp="#s1:l5">verslijt</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Jakomain staat in haar <rhyme label="a" type="f">deure</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En verwacht den heelen <rhyme label="b" type="m">dag</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Daar en zijn geen <rhyme label="a" type="f" corresp="#s2:l1">Serviteuren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Daar zy tegens praaten <rhyme label="b" type="m" corresp="#s2:l2">mag</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">'t Is slechten tijdt’ niemandt en <rhyme label="c" type="m">vrijt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">'t Schijnt Kupido gebonden <rhyme label="c" type="m" corresp="#s2:l5">leidt</rhyme>.</l>
        <l id="s2:l7" met="5" type="+-">Zalder ons dan niemandt komen <rhyme label="d" type="f">troosten</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Eer ons jonge Jeugt <rhyme label="c" type="m" corresp="#s2:l5">verslijt</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Antones die gaat al <rhyme label="a" type="f">pronkken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Als een Pauwken over <rhyme label="b" type="m">straat</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Lizabeth doet niet als <rhyme label="a" type="f" corresp="#s3:l1">lonkken</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Waar daar eenen Jongman <rhyme label="b" type="m" corresp="#s3:l2">gaat</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">'t Is slechten tijdt’ niemandt en <rhyme label="c" type="m">vrijt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">'t Schijnt Kupido gebonden <rhyme label="c" type="m" corresp="#s3:l5">leidt</rhyme>.</l>
        <l id="s3:l7" met="5" type="+-">Zalder ons dan niemandt komen <rhyme label="d" type="f">troosten</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Eer ons jonge Jeugt <rhyme label="c" type="m" corresp="#s3:l5">verslijt</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Angenees en wilt niet <rhyme label="a" type="f">treuren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Al om eenen <rhyme label="b" type="m">Serviteur</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Gaat niet drillen lang de <rhyme label="a" type="f" corresp="#s4:l1">deuren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Om te vinden uwen <rhyme label="b" type="m" corresp="#s4:l2">keur</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">'t Is slechten tijdt’ niemandt en <rhyme label="c" type="m">vrijt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">'t Schijnt Kupido gebonden <rhyme label="c" type="m" corresp="#s4:l5">leidt</rhyme>.</l>
        <l id="s4:l7" met="5" type="+-">Zalder ons dan niemandt komen <rhyme label="d" type="f">troosten</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Eer ons jonge Jeugt <rhyme label="c" type="m" corresp="#s4:l5">verslijt</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Anneken is vijfthien <rhyme label="a" type="f">Jaren</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Zeventhien heeft <rhyme label="b" type="m">Jakomain</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Zy en doen daar niet dan <rhyme label="a" type="f" corresp="#s5:l1">klagen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dat zy niet getrouwt en <rhyme label="b" type="m" corresp="#s5:l2">zijn</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">'t Is slechten tijdt’ niemandt en <rhyme label="c" type="m">vrijt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">'t Schijnt Kupido gebonden <rhyme label="c" type="m" corresp="#s5:l5">leidt</rhyme>.</l>
        <l id="s5:l7" met="5" type="+-">Zalder ons dan niemandt komen <rhyme label="d" type="f">troosten</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Eer ons jonge Jeugt <rhyme label="c" type="m" corresp="#s5:l5">verslijt</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Katalain sprak zonder <rhyme label="a" type="f">schame</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Moeder geeft eenen man aan <rhyme label="b" type="m">may</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Want ik ben zoo wel <rhyme label="a" type="f" corresp="#s6:l1">bequame</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Moeder, om dat te doen als <rhyme label="b" type="m" corresp="#s6:l2">ghy</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">'t Is slechten tijdt’ niemandt en <rhyme label="c" type="m">vrijt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">'t Schijnt Kupido gebonden <rhyme label="c" type="m" corresp="#s6:l5">leidt</rhyme>.</l>
        <l id="s6:l7" met="5" type="+-">Zalder ons dan niemandt komen <rhyme label="d" type="f">troosten</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Eer ons jonge Jeugt <rhyme label="c" type="m" corresp="#s6:l5">verflijt</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
