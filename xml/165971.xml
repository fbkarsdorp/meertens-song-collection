    </tags>
    <melody>Lief in het begin tot reine min</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="groups">groepen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Lieff int beginne,, uijt reynder <rhyme label="a" type="f">minne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">quelt myn van binnen,, <rhyme label="b" type="f">'lacij</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ic zou myn vinnen,, gerust van <rhyme label="a" type="f" corresp="#s1:l1">zinnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">mocht Ic gewinnen,, <rhyme label="b" type="f" corresp="#s1:l2">graty</rhyme></l>
        <l id="s1:l5" met="3" type="-+">van u myn Lieff <rhyme label="c" type="m">plaijsant</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Diet hart doet <rhyme label="d" type="f">breecken</rhyme></l>
        <l id="s1:l7" met="3" type="+-">wiens schoonheyt <rhyme label="c" type="m" corresp="#s1:l5">trijumphant</rhyme></l>
        <l id="s1:l8" met="3" type="-+">een weselijck <rhyme label="c" type="m" corresp="#s1:l5">semblant</rhyme></l>
        <l id="s1:l9" met="3" type="-+">passeert al mijn <rhyme label="c" type="m" corresp="#s1:l5">verstant</rhyme></l>
        <l id="s1:l10" met="2" type="-+">door venus <rhyme label="d" type="f" corresp="#s1:l6">treecken</rhyme></l>
        <l id="s1:l11" met="2" type="-+">twelck mijn doet <rhyme label="d" type="f" corresp="#s1:l6">smeecken</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Ooch mocht comende <rhyme label="e" type="m">uijr</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Dat Ic uyt lieffden <rhyme label="e" type="m" corresp="#s1:l12">puijr</rhyme></l>
        <l id="s1:l14" met="3" type="-+">voor al myn groot <rhyme label="e" type="m" corresp="#s1:l12">besuyr</rhyme></l>
        <l id="s1:l15" met="2" type="-+">haer eens mocht <rhyme label="d" type="f" corresp="#s1:l6">spreecken</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O, zuijver Jeuchden, off u <rhyme label="a" type="f">geneuchden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat myn u deuchden <rhyme label="b" type="f">vloeyden</rhyme></l>
        <l id="s2:l3" met="4" type="-+">en ghy u veuchden, met myn <rhyme label="a" type="f" corresp="#s2:l1">verheuchden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">den geest In vreuchden <rhyme label="b" type="f" corresp="#s2:l2">groeyden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">door U mondeken <rhyme label="c" type="m">root</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">zoet van <rhyme label="d" type="f">Faconden</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Cupido my door <rhyme label="c" type="m" corresp="#s2:l5">schoot</rhyme></l>
        <l id="s2:l8" met="3" type="-+">al dunckt het u geen <rhyme label="c" type="m" corresp="#s2:l5">noot</rhyme></l>
        <l id="s2:l9" met="3" type="-+">het quest my totter <rhyme label="c" type="m" corresp="#s2:l5">Doot</rhyme></l>
        <l id="s2:l10" met="2" type="-+">een diepe <rhyme label="d" type="f" corresp="#s2:l6">wonde</rhyme></l>
        <l id="s2:l11" met="2" type="-+">Ic <rhyme label="d" type="f" corresp="#s2:l6">ongesonde</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Och mocht comende <rhyme label="e" type="m">uijr</rhyme></l>
        <l id="s2:l13" met="3" type="-+">Dat ghij muyt lieffden <rhyme label="e" type="m" corresp="#s2:l12">puijr</rhyme></l>
        <l id="s2:l14" met="3" type="-+">voor al myn goot <rhyme label="e" type="m" corresp="#s2:l12">besuijr</rhyme></l>
        <l id="s2:l15" met="2" type="-+">u lieff myn <rhyme label="d" type="f" corresp="#s2:l6">Jonden</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">veel Fantasyen In myn <rhyme label="a" type="f">gedyen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">nu vreucht dan lijen <rhyme label="b" type="f">wercken</rhyme></l>
        <l id="s3:l3" met="4" type="-+">vrees vol envijen, comt myn <rhyme label="a" type="f" corresp="#s3:l1">bestryen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">men macht by tyen, <rhyme label="b" type="f" corresp="#s3:l2">mercken</rhyme></l>
        <l id="s3:l5" met="3" type="-+">waer door den geest <rhyme label="c" type="m">perpleckt</rhyme></l>
        <l id="s3:l6" met="2" type="-+">moet <rhyme label="d" type="f">lamenteren</rhyme></l>
        <l id="s3:l7" met="3" type="+-">maer hoop myn heel <rhyme label="c" type="m" corresp="#s3:l5">verweckt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">door deedel Jonste <rhyme label="c" type="m" corresp="#s3:l5">streckt</rhyme></l>
        <l id="s3:l9" met="3" type="-+">tot u lieff <rhyme label="c" type="m" corresp="#s3:l5">onbevleckt</rhyme></l>
        <l id="s3:l10" met="2" type="-+">zonder <rhyme label="d" type="f" corresp="#s3:l6">cesseren</rhyme></l>
        <l id="s3:l11" met="2" type="-+">met groot <rhyme label="d" type="f" corresp="#s3:l6">begeren</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Och mocht comende <rhyme label="e" type="m">uijr</rhyme></l>
        <l id="s3:l13" met="3" type="-+">dat ghy uijt lieffden <rhyme label="e" type="m" corresp="#s3:l12">puijr</rhyme></l>
        <l id="s3:l14" met="3" type="-+">voor al myn groot <rhyme label="e" type="m" corresp="#s3:l12">besuijr</rhyme></l>
        <l id="s3:l15" met="2" type="-+">u liet <rhyme label="d" type="f" corresp="#s3:l6">moveren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Princesse schoon vrouwe,, eedele <rhyme label="a" type="f">kersouwe</rhyme></l>
        <l id="s4:l2" met="3" type="-+">aenmerckt myn trouwe <rhyme label="b" type="f">harte</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Op aer Ic bouwe, neemt toch <rhyme label="a" type="f" corresp="#s4:l1">aenschouwe</rhyme></l>
        <l id="s4:l4" met="3" type="-+">dat mij geen rouwe <rhyme label="b" type="f" corresp="#s4:l2">smerte</rhyme></l>
        <l id="s4:l5" met="3" type="-+">teffeckt van myn <rhyme label="c" type="m">devoor</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">blyckt uij myn <rhyme label="d" type="f">kermen</rhyme></l>
        <l id="s4:l7" met="3" type="+-">geeft ghij mijn geen <rhyme label="c" type="m" corresp="#s4:l5">gehoor</rhyme></l>
        <l id="s4:l8" met="3" type="-+">myn leven gaet te <rhyme label="c" type="m" corresp="#s4:l5">loor</rhyme></l>
        <l id="s4:l9" met="3" type="-+">van druck Ic heel <rhyme label="c" type="m" corresp="#s4:l5">versmoor</rhyme></l>
        <l id="s4:l10" met="2" type="-+">eylacij och <rhyme label="d" type="f" corresp="#s4:l6">armen</rhyme></l>
        <l id="s4:l11" met="2" type="-+">wilt myns <rhyme label="d" type="f" corresp="#s4:l6">ontfarmen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">och mocht comende <rhyme label="e" type="m">uijr</rhyme></l>
        <l id="s4:l13" met="3" type="-+">dat Ic uijt lieffden <rhyme label="e" type="m" corresp="#s4:l12">Puijr</rhyme></l>
        <l id="s4:l14" met="3" type="-+">voor al dit groot <rhyme label="e" type="m" corresp="#s4:l12">besuijr</rhyme></l>
        <l id="s4:l15" met="2" type="-+">Laech In u <rhyme label="d" type="f" corresp="#s4:l6">armen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
