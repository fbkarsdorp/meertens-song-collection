  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHY Princen end ghy <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Begaeft met grooter <rhyme label="a" type="f" corresp="#s1:l1">eeren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Schryft Godt toe alle <rhyme label="b" type="f">t'saemen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Syn kracht end lof <rhyme label="b" type="f" corresp="#s1:l3">bequaeme</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Wilt hem sulcken prys <rhyme label="c" type="f">bewysen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als sijn macht niet om vol <rhyme label="c" type="f" corresp="#s1:l5">prysen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Toestaet, end' in sijn woningh' <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Buyghet hem die knyen met <rhyme label="d" type="m" corresp="#s1:l7">ootmoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Godts stem werdt gehoort <rhyme label="a" type="f">krachtigh</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Op die wateren <rhyme label="a" type="f" corresp="#s2:l1">machtigh</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">In s'hemels wolcken seer <rhyme label="b" type="f">klaer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Werdt oock Godts kracht <rhyme label="b" type="f" corresp="#s2:l3">openbaer</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Sijn stemme niet om <rhyme label="c" type="f">verstercken</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ghetuyght van sijn groote <rhyme label="c" type="f" corresp="#s2:l5">wercken</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Sijn stemm' is vol t'allen <rhyme label="d" type="m">stonden</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Van heerlicke kracht <rhyme label="d" type="m" corresp="#s2:l7">beuonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Godts stem slaet met <rhyme label="a" type="f">onweder</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die Cederboomen <rhyme label="a" type="f" corresp="#s3:l1">neder</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Op Liban den bergh <rhyme label="b" type="f">gheplantt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">End' ander berghen in't <rhyme label="b" type="f" corresp="#s3:l3">landt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Hy doet die springhen te <rhyme label="c" type="f">deghen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">So die jonghe kaluers <rhyme label="c" type="f" corresp="#s3:l5">pleghen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">End' als die jongh' Eenhoornen <rhyme label="d" type="m">doen</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Die opspringen in bosschen <rhyme label="d" type="m" corresp="#s3:l7">groen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Godts stem doersnijdt end <rhyme label="a" type="f">scheydet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">T'vuer end sijn vlamm' <rhyme label="a" type="f" corresp="#s4:l1">wtbreydet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy doet t'dal Cades <rhyme label="b" type="f">beuen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met den daelen daer <rhyme label="b" type="f" corresp="#s4:l3">neuen</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Hy schrickt die hinden <rhyme label="c" type="f">kleenmoedigh</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">End doet se misuallen <rhyme label="c" type="f" corresp="#s4:l5">spoedigh</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Hy doet die bosschen groen end <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Doer sijn kracht werden dorr' end <rhyme label="d" type="m" corresp="#s4:l7">bloot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maer jeghlick gaet bin <rhyme label="a" type="f">desen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">In Godts tempel <rhyme label="a" type="f" corresp="#s5:l1">ghepresen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">End daer hy plach te <rhyme label="b" type="f">beuen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hy loeft hem al sijn <rhyme label="b" type="f" corresp="#s5:l3">leuen</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Des waters dat men vreest <rhyme label="c" type="f">seere</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Des Sendtvloedts is Godt een <rhyme label="c" type="f" corresp="#s5:l5">Heere</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Ende sijn Koninghrycke <rhyme label="d" type="m">voortaen</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Sal eewighlick vast blyuen <rhyme label="d" type="m" corresp="#s5:l7">staen</rhyme>.</l>
      </lg>
    </lg>
  </text>
