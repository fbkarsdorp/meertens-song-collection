<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>158691</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Marijn de Brauwer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe menich duysent <rhyme label="a" type="f">mensche</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Sijnder voor onsen tijt <rhyme label="b" type="m">geweest</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die noch wille noch <rhyme label="a" type="f" corresp="#s1:l1">wenschen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En hadden alsoo men <rhyme label="b" type="m" corresp="#s1:l2">leest</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Om te soeken den <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En oock te dienen <rhyme label="d" type="m">hem</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Maer gingen met <rhyme label="c" type="f" corresp="#s1:l5">begeere</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Liever den Wegh <rhyme label="d" type="m" corresp="#s1:l6">Caeyem</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Caeyem heeft doot <rhyme label="a" type="f">gesmeten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zijn vroomen broeder <rhyme label="b" type="m">Abel</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Wt boos Herten <rhyme label="a" type="f" corresp="#s2:l1">vermeten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Gewaerschout van den Heere <rhyme label="b" type="m" corresp="#s2:l2">wel</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Maer Cayem achtet <rhyme label="c" type="f">lichte</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Des Heeren goeden <rhyme label="d" type="m">raet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Gingh wegh van den <rhyme label="c" type="f" corresp="#s2:l5">aensichte</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Des Heeren, en bleef <rhyme label="d" type="m" corresp="#s2:l6">quaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Caeyems gheheel <rhyme label="a" type="f">geslachte</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die van hem gecomen <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Zy hadden kleen <rhyme label="a" type="f" corresp="#s3:l1">gedachte</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tot haren Schepper <rhyme label="b" type="m" corresp="#s3:l2">devijn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zy woonden tegent <rhyme label="c" type="f">Ooste</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Binnen den Lande <rhyme label="d" type="m">Nodts</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Niemant van haer en <rhyme label="c" type="f" corresp="#s3:l5">gloofte</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Op dat bevel van <rhyme label="d" type="m" corresp="#s3:l6">Godt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dees boose <rhyme label="a" type="f">generati</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Is soo seer <rhyme label="b" type="m">geprospereert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Meest al buyten Godts <rhyme label="a" type="f" corresp="#s4:l1">grati</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Want zy leefden gantsch <rhyme label="b" type="m" corresp="#s4:l2">verkeert</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Want Caeyem niet <rhyme label="c" type="f">alleene</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Timmerde daer een <rhyme label="d" type="m">stadt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Van Gods kinder niet <rhyme label="c" type="f" corresp="#s4:l5">eene</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En heeft hy hulpe <rhyme label="d" type="m" corresp="#s4:l6">gehat</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gods kinders daer en <rhyme label="a" type="f">tegen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die gekommen sijn van <rhyme label="b" type="m">Seth</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Sochten des Heeren <rhyme label="a" type="f" corresp="#s5:l1">wegen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En zy leefden naer Gods <rhyme label="b" type="m" corresp="#s5:l2">Wet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Twee volcken hier op <rhyme label="c" type="f">aerden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Waren van dat <rhyme label="d" type="m">begin</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die God hielden in <rhyme label="c" type="f" corresp="#s5:l5">waerden</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die hadden tbeste <rhyme label="d" type="m" corresp="#s5:l6">gewin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Seth heeft Enos <rhyme label="a" type="f">gewonnen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Kints kidneren voort en <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Van doen heeft men <rhyme label="a" type="f" corresp="#s6:l1">begonnen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Onder haer met goet <rhyme label="b" type="m" corresp="#s6:l2">accoort</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Te Predicken <rhyme label="c" type="f">bequame</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Al op de selve <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Al van des Heeren <rhyme label="c" type="f" corresp="#s6:l5">Name</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Soo ons Moyses <rhyme label="d" type="m" corresp="#s6:l6">belijt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Kenan van Enos <rhyme label="a" type="f">zade</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Brachte voort <rhyme label="b" type="m">Mahalalel</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En die seer wijs van <rhyme label="a" type="f" corresp="#s7:l1">rade</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Wan Jaret, sonder <rhyme label="b" type="m" corresp="#s7:l2">appel</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Jaredt Enoch <rhyme label="c" type="f">beneven</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dat was een vroom <rhyme label="d" type="m">pylaer</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Leyd' een Goddelijck <rhyme label="c" type="f" corresp="#s7:l5">leven</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Meer dan drie hondert <rhyme label="d" type="m" corresp="#s7:l6">jaer</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Godt heeft hem wegh <rhyme label="a" type="f">genomen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Datmen hem niet meer en <rhyme label="b" type="m">sach</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Van hem is eenen <rhyme label="a" type="f" corresp="#s8:l1">comen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Die hietmen <rhyme label="b" type="m" corresp="#s8:l2">Mathusalach</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Mathusalach <rhyme label="c" type="f">ydone</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Die brachte voort <rhyme label="d" type="m">Lamech</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Lamech wan eenen <rhyme label="c" type="f" corresp="#s8:l5">Sone</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Dats Noe sonder <rhyme label="d" type="m" corresp="#s8:l6">weerlech</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Als die menschen <rhyme label="a" type="f">vermeerden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Op der aerden doen ten <rhyme label="b" type="m">tien</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Gods kinderen <rhyme label="a" type="f" corresp="#s9:l1">verkeerden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">En gingen stout <rhyme label="b" type="m" corresp="#s9:l2">ommesien</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Naer smenschens Dochters <rhyme label="c" type="f">schoone</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Namen ten Wijve <rhyme label="d" type="m">haer</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Het welck zy niet <rhyme label="c" type="f" corresp="#s9:l5">gwoone</rhyme>:</l>
        <l id="s9:l8" met="3" type="-+">En waren te doene <rhyme label="d" type="m" corresp="#s9:l6">daer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Godt ging over haer <rhyme label="a" type="f">klagen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Om dat zy na zijnen <rhyme label="b" type="m">heesch</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Niet meer en wilden <rhyme label="a" type="f" corresp="#s10:l1">vraghen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Zy waren geworden <rhyme label="b" type="m" corresp="#s10:l2">vleesch</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Die oec tot haerder <rhyme label="c" type="f">baten</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Den Heer hadden <rhyme label="d" type="m">gevreest</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Die wilden haer niet <rhyme label="c" type="f" corresp="#s10:l5">laten</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">straffen van zynen <rhyme label="d" type="m" corresp="#s10:l6">geest</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De Heere sprack dat <rhyme label="a" type="f">ende</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Van alle vleesch dat is voor <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Gecommen want elck <rhyme label="a" type="f" corresp="#s11:l1">wende</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Hem tot boosheyt en daer <rhyme label="b" type="m" corresp="#s11:l2">by</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Zy deden buyten <rhyme label="c" type="f">Trouwen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Dat elck hem soo <rhyme label="d" type="m">verliep</rhyme></l>
        <l id="s11:l7" met="3" type="-+">En brachten God tot <rhyme label="c" type="f" corresp="#s11:l5">rouwen</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Dat hy den mensche <rhyme label="d" type="m" corresp="#s11:l6">schiep</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Haer doen dat deed hem <rhyme label="a" type="f">smerte</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Want twas al voor hem <rhyme label="b" type="m">naect</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Tbecommerde hem int <rhyme label="a" type="f" corresp="#s12:l1">herte</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Dat hy haer had <rhyme label="b" type="m" corresp="#s12:l2">gemaect</rhyme></l>
        <l id="s12:l5" met="3" type="-+">En sprack met groot <rhyme label="c" type="f">verdriete</rhyme></l>
        <l id="s12:l6" met="3" type="-+">(Want haer doen de hem <rhyme label="d" type="m">wee</rhyme>)</l>
        <l id="s12:l7" met="3" type="-+">Ick salt al doen te <rhyme label="c" type="f" corresp="#s12:l5">niete</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Van den mensche tot den <rhyme label="d" type="m" corresp="#s12:l6">Vee</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">De boosheyt <rhyme label="a" type="f">menichfuldich</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Nam soo seere de <rhyme label="b" type="m">overhant</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Maer Noe was <rhyme label="a" type="f" corresp="#s13:l1">ontschuldich</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Die voor Godt genade <rhyme label="b" type="m" corresp="#s13:l2">vant</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Een Arke moste hy <rhyme label="c" type="f">maken</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Tot zalicheyt zijns <rhyme label="d" type="m">huys</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Dat Volck dat was <rhyme label="c" type="f" corresp="#s13:l5">eylaken</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Vol moetwillich <rhyme label="d" type="m" corresp="#s13:l6">confuys</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Hondert twintich Jaer <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Gaff haer die Heere <rhyme label="b" type="m">respijt</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Op dat elck boete <rhyme label="a" type="f" corresp="#s14:l1">dede</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Voor zijne Oogen met <rhyme label="b" type="m" corresp="#s14:l2">vlijt</rhyme></l>
        <l id="s14:l5" met="3" type="-+">In plaetse van <rhyme label="c" type="f">bekeeren</rhyme></l>
        <l id="s14:l6" met="3" type="-+">bleven zy vleesch <rhyme label="d" type="m">gesint</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">al ging haer Noe <rhyme label="c" type="f" corresp="#s14:l5">leeren</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Zy sloegent inde <rhyme label="d" type="m" corresp="#s14:l6">wint</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Als desen tijt <rhyme label="a" type="f">genaecte</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Niemant en dede daer <rhyme label="b" type="m">afstant</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Noe d' Arke <rhyme label="a" type="f" corresp="#s15:l1">volmaeckte</rhyme></l>
        <l id="s15:l4" met="3" type="-+">God die Deluvie <rhyme label="b" type="m" corresp="#s15:l2">sant</rhyme></l>
        <l id="s15:l5" met="3" type="-+">tWater hoort dit <rhyme label="c" type="f">vertellen</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Dat wies dat niet en <rhyme label="d" type="m">looch</rhyme></l>
        <l id="s15:l7" met="3" type="-+">Ontrent de vijftich <rhyme label="c" type="f" corresp="#s15:l5">Ellen</rhyme></l>
        <l id="s15:l8" met="3" type="-+">Boven die Bergen <rhyme label="d" type="m" corresp="#s15:l6">hoogh</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Niemant en cost <rhyme label="a" type="f">ontvluchten</rhyme></l>
        <l id="s16:l2" met="3" type="-+">In dees grooten <rhyme label="b" type="m">onderganck</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">Maer wel weenen en <rhyme label="a" type="f" corresp="#s16:l1">suchten</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Wat Adam hadde <rhyme label="b" type="m" corresp="#s16:l2">verdranck</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Tginck daer al te <rhyme label="c" type="f">ruijne</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Geen vleesch en was <rhyme label="d" type="m">gespaert</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Maer Noe met de <rhyme label="c" type="f" corresp="#s16:l5">zijne</rhyme></l>
        <l id="s16:l8" met="3" type="-+">Waren in d' Arke <rhyme label="d" type="m" corresp="#s16:l6">bewaert</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Dese en al dander <rhyme label="a" type="f">scharen</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Sullen in den jongsten <rhyme label="b" type="m">dagh</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Al voor Christum <rhyme label="a" type="f" corresp="#s17:l1">vergaren</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Daer niemant voor by en <rhyme label="b" type="m" corresp="#s17:l2">mach</rhyme></l>
        <l id="s17:l5" met="3" type="-+">Om tontfangen <rhyme label="c" type="f">tbeloonen</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Al voor den rechter <rhyme label="d" type="m">stoel</rhyme>,</l>
        <l id="s17:l7" met="3" type="-+">De vroome sal hy <rhyme label="c" type="f" corresp="#s17:l5">kroonen</rhyme></l>
        <l id="s17:l8" met="3" type="-+">Maer de quade inden <rhyme label="d" type="m" corresp="#s17:l6">Poel</rhyme></l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Prince laet ons <rhyme label="a" type="f">volheerdich</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Blyven in s'Heeren <rhyme label="b" type="m">gebien</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Dat wy mogen sijn <rhyme label="a" type="f" corresp="#s18:l1">weerdich</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Al die droefheyt te <rhyme label="b" type="m" corresp="#s18:l2">ontvlien</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">En onsen wandel <rhyme label="c" type="f">leyden</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Op aerden met <rhyme label="d" type="m">ootmoet</rhyme>,</l>
        <l id="s18:l7" met="3" type="-+">Daer wert groot <rhyme label="c" type="f" corresp="#s18:l5">onderscheyden</rhyme></l>
        <l id="s18:l8" met="3" type="-+">Al tusschen quaet en <rhyme label="d" type="m" corresp="#s18:l6">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
