<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182086</id>
    <date>
      <year-start>1791</year-start>
      <year-end>1791</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Soete Lief ik kom u <rhyme label="a" type="f">groeten</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ik valder voor u ter Aerde <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En buig voor u mijn <rhyme label="a" type="f" corresp="#s1:l1">Voeten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om u o Maget <rhyme label="b" type="m" corresp="#s1:l2">teer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Om uwent wil zo moet ik <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">dalen in het duister <rhyme label="d" type="m">Graf</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Kan ik dan geen troost <rhyme label="c" type="f" corresp="#s1:l5">verwerven</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Ach zoete Liefje aenhoord mijn <rhyme label="d" type="m" corresp="#s1:l6">klagt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wie hoor ik hier zoo <rhyme label="a" type="f">treuren</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">die der mij daegelijks zoo <rhyme label="b" type="m">kweld</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Het zal dog niet <rhyme label="a" type="f" corresp="#s2:l1">gebeuren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">U Zinne niet vast en <rhyme label="b" type="m" corresp="#s2:l2">steld</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Ik roep den Hemel tot <rhyme label="c" type="f">getuijge</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ik zweer bij Sterre Zon en <rhyme label="d" type="m">Maen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">dat ik zal voor u neder <rhyme label="c" type="f" corresp="#s2:l5">buijgen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Liefje wild mij ten Antwoord <rhyme label="d" type="m" corresp="#s2:l6">staen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Liefje hoe kund gij <rhyme label="a" type="f">gedogen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">dat ik ga dolen agter <rhyme label="b" type="m">land</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ik bid u zijt dog <rhyme label="a" type="f" corresp="#s3:l1">bewogen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Geeft mij op trouw uw <rhyme label="b" type="m" corresp="#s3:l2">hand</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">daer is een Ring van <rhyme label="c" type="f">diamant</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Steekze vrij aen uw <rhyme label="d" type="m">Regterhand</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Liefje mogt ik u gunst <rhyme label="c" type="f" corresp="#s3:l5">genieten</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Ik gafze jou op Trouw tot <rhyme label="d" type="m" corresp="#s3:l6">onderpand</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ionkman uw <rhyme label="a" type="f">lammenteren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Heeft 'er bewogen myn <rhyme label="b" type="m">Ionkhert</rhyme>!</l>
        <l id="s4:l3" met="3" type="-+">U liefde is mijn <rhyme label="a" type="f" corresp="#s4:l1">begeeren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Laet dog u druk en <rhyme label="b" type="m" corresp="#s4:l2">smert</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">daerom zal ik uw Trouwtje <rhyme label="c" type="f">ontfangen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Neem jou voor mijn zoete Lief <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Komt dan Ionkman en wagt niet <rhyme label="c" type="f" corresp="#s4:l5">langer</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Want al u klagen dat is <rhyme label="d" type="m" corresp="#s4:l6">gedaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ik dank u waerde <rhyme label="a" type="f">Engelin</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Al voor u goed <rhyme label="b" type="m">bescheid</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Nu gij ons te zamen <rhyme label="a" type="f" corresp="#s5:l1">mengel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Alderzoetste <rhyme label="b" type="m" corresp="#s5:l2">Meid</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Nu is mijn druk <rhyme label="c" type="f">genezen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">die ik voelde in mijn <rhyme label="d" type="m">Ionkhert</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">die door u mijn <rhyme label="c" type="f" corresp="#s5:l5">Uitgelezen</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Nu in vreugd verandert <rhyme label="d" type="m" corresp="#s5:l6">werd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Komt nu gij <rhyme label="a" type="f">musikanten</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Speeld op Bas, Fiool en <rhyme label="b" type="m">Fluit</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Nu ik met <rhyme label="a" type="f" corresp="#s6:l1">diamanten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vercier mijn waerde <rhyme label="b" type="m" corresp="#s6:l2">Bruid</rhyme>!</l>
        <l id="s6:l5" met="4" type="-+">Nu dat ik ga mijn min <rhyme label="c" type="f">besteden</rhyme>?</l>
        <l id="s6:l6" met="4" type="-+">Aen mijn liefste tot <rhyme label="d" type="m">besluit</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">En met haer in den egt ga <rhyme label="c" type="f" corresp="#s6:l5">treden</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Ik als Bruidegom en zij als <rhyme label="d" type="m" corresp="#s6:l6">Bruid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
