  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlt u, 's hemels <rhyme label="a" type="f">Koninginne</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Nu verheughen, zyt <rhyme label="b" type="m">ver-blyd</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die ghy droeght uyt suyver <rhyme label="a" type="f" corresp="#s1:l1">minne</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Gh'lyck hy sprack voor desen <rhyme label="b" type="m" corresp="#s1:l2">tyd</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Is ver-resen; reyn <rhyme label="a" type="f" corresp="#s1:l1">Vriendinne</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Bid dat God ons al <rhyme label="b" type="m" corresp="#s1:l2">be-vryd</rhyme>.</l>
      </lg>
    </lg>
  </text>
