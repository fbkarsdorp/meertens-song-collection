<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22396</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Dies est laetitiae in ortu regali</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Dit is den dach van <rhyme label="a" type="m">vrolijcheyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Voor allen <rhyme label="b" type="f">Creatueren</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Want Godes Soon van <rhyme label="a" type="m" corresp="#s1:l1">eewicheyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ouer alle <rhyme label="b" type="f" corresp="#s1:l2">natueren</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Van eender Maghet is <rhyme label="c" type="m">gheboren</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Maria ghy zijt <rhyme label="c" type="m" corresp="#s1:l5">wtvercoren</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat ghy Moeder <rhyme label="d" type="f">wordet</rhyme>:</l>
        <l id="s1:l8" met="4" type="-+">Dit gheschiet soo <rhyme label="e" type="m">wonderlijck</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Godes Soon van <rhyme label="e" type="m" corresp="#s1:l8">Hemelrijck</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Die is nu Mensch <rhyme label="d" type="f" corresp="#s1:l7">gheworden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghelijck die Sonne dat Glas <rhyme label="a" type="m">doorsteeckt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met haren glansse <rhyme label="b" type="f">reyne</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Twelck sy nochtans niet af en <rhyme label="a" type="m" corresp="#s2:l1">breeckt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Soo merckt hier al <rhyme label="b" type="f" corresp="#s2:l2">ghemeyne</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Also wort oock gheboren <rhyme label="c" type="m">hier</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Van eener Maghet reyn en <rhyme label="c" type="m" corresp="#s2:l5">fier</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Godes Sone vol <rhyme label="d" type="f">weerden</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Sijn bedde wert in een cribbe <rhyme label="e" type="m">ghespreet</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Grooter iammer hy voor ons <rhyme label="e" type="m" corresp="#s2:l8">leet</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Hier op deser <rhyme label="d" type="f" corresp="#s2:l7">eerden</rhyme>. /</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die Herderkens op dat selue <rhyme label="a" type="m">pas</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vernamen nieuwe <rhyme label="b" type="f">mare</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat Jhesus Christus gheboren <rhyme label="a" type="m" corresp="#s3:l1">was</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sprack die Enghelsche <rhyme label="b" type="f" corresp="#s3:l2">schare</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Een Coninck ouer die Coninghen <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Twelck Herodes seer <rhyme label="c" type="m" corresp="#s3:l5">verdroot</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En sant wt zijne <rhyme label="d" type="f">boden</rhyme>:</l>
        <l id="s3:l8" met="4" type="-+">O wat grooten valschen <rhyme label="e" type="m">list</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Dacht hy teghen Jhesum <rhyme label="e" type="m" corresp="#s3:l8">Christ</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Die Kinderkens liet hy <rhyme label="d" type="f" corresp="#s3:l7">doden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
