    <tags/>
    <melody>Het marmotje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MEisje, wilje mijn Beesje eens <rhyme label="a" type="m">zien</rhyme>?</l>
        <l id="s1:l2" met="3" type="+-">'t Is een kley <rhyme label="b" type="f">Marmotje</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Het kan Danssen voor alle de <rhyme label="a" type="m" corresp="#s1:l1">Lien</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als hy komt uyt zijn <rhyme label="b" type="f" corresp="#s1:l2">Kotje</rhyme>:</l>
        <l id="s1:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Donnimo, elk wat in 't <rhyme label="a" type="m">rond</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Voor mijn en 't <rhyme label="b" type="f">Marmotje</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Het zal Danssen zo <rhyme label="a" type="m" corresp="#s2:l1">terstond</rhyme>:</l>
        <l id="s2:l4" met="3" type="+-">Daar komt hy uyt zijn <rhyme label="b" type="f" corresp="#s2:l2">Kotje</rhyme>:</l>
        <l id="s2:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Za alon Beesje uyt je <rhyme label="a" type="m">Hol</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Danss√© avik <rhyme label="b" type="f">Marmotje</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Foute Bouger worje <rhyme label="a" type="m" corresp="#s3:l1">dol</rhyme>?</l>
        <l id="s3:l4" met="3" type="+-">Kruypje weer in jou <rhyme label="b" type="f" corresp="#s3:l2">Kotje</rhyme>?</l>
        <l id="s3:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Nog een keer <rhyme label="a" type="m">Marmotje</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Dan zult gy weer zo <rhyme label="b" type="f">terstond</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Kruypen in jou <rhyme label="a" type="m" corresp="#s4:l1">Kotje</rhyme>:</l>
        <l id="s4:l4" met="3" type="+-">Toujour, toujour, in 't <rhyme label="b" type="f" corresp="#s4:l2">rond</rhyme>;</l>
        <l id="s4:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Mamoisel en zijt niet <rhyme label="a" type="m">peur</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Voor mijn kleyn <rhyme label="b" type="f">Marmotje</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Het sluypt in geen <rhyme label="a" type="m" corresp="#s5:l1">Iuffrouws-deur</rhyme>;</l>
        <l id="s5:l4" met="3" type="+-">Niet anders als in 't <rhyme label="b" type="f" corresp="#s5:l2">Kotje</rhyme>:</l>
        <l id="s5:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Keyk, die Teef, zy loop, al <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Geen Duet voor mijn <rhyme label="b" type="f">Marmotje</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Maer zy denkt meer als ik <rhyme label="a" type="m" corresp="#s6:l1">meen</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Op een ander <rhyme label="b" type="f" corresp="#s6:l2">Rotje</rhyme>:</l>
        <l id="s6:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Komt mijn Beestjen, 't is <rhyme label="a" type="m">gedaen</rhyme>;</l>
        <l id="s7:l2" met="3" type="+-">Rust nou wat <rhyme label="b" type="f">Marmotje</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Wy zullen eens na een ander <rhyme label="a" type="m" corresp="#s7:l1">kaen</rhyme>;</l>
        <l id="s7:l4" met="3" type="+-">Kruypt maar in je <rhyme label="b" type="f" corresp="#s7:l2">Kotje</rhyme>:</l>
        <l id="s7:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Wel dat is eene rare <rhyme label="a" type="m">kwant</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Die Ionge met zijn <rhyme label="b" type="f">Marmotje</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En komt hy uyt Savojers <rhyme label="a" type="m" corresp="#s8:l1">Land</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Met zijn Beesje in 't <rhyme label="b" type="f" corresp="#s8:l2">Kotje</rhyme>?</l>
        <l id="s8:l5" met="7" type="-+">A vikkie moy, A vikkie moy, A vikkie moy, <rhyme label="c" type="f">Marmotto</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
