<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5249</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag translation="Geuzen song">geuzenlied</tag>
      <tag translation="historical song">historielied</tag>
      <tag translation="mocking song">spotlied</tag>
    </tags>
    <melody>Een meisje op een riviertje zat</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Jan Broerman heeft een Bierken <rhyme label="a" type="f">ghebrouwen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Van Rommelken <rhyme label="b" type="m">Rom</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ten deucht te drincken noch te <rhyme label="a" type="f" corresp="#s1:l1">houwen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s1:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hansken van spangien tsal u <rhyme label="a" type="f">rouwen</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Voor bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat ghy meynt de Waerd in te <rhyme label="a" type="f" corresp="#s2:l1">houwen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s2:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Bommel en Tielschen Weert <rhyme label="a" type="f">vercooren</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Tot,  Bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hebben malcanderen eedt <rhyme label="a" type="f" corresp="#s3:l1">gheswooren</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s3:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy en sullen malcanderen niet <rhyme label="a" type="f">verlaten</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Tot bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Sy hebben daer toe te cloecke <rhyme label="a" type="f" corresp="#s4:l1">soldaten</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s4:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Geldersman is op u <rhyme label="a" type="f">ghebeten</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Van bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Uyt haer Schapray suldy niet <rhyme label="a" type="f" corresp="#s5:l1">eten</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s5:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Geldersche zijn alsoo van <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Meest bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Sy willen doch geen Tyrannen <rhyme label="a" type="f" corresp="#s6:l1">beminnen</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s6:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sy willen aen u haer kraghen, noch <rhyme label="a" type="f">waghen</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Voor bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En u lieden met slaghen, <rhyme label="a" type="f" corresp="#s7:l1">verjaghen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Oft keert <rhyme label="b" type="m" corresp="#s7:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Sy sullen noch voor haer stercke <rhyme label="a" type="f">schanssen</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Voor bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Hun Switzers doen pijpen en u doen <rhyme label="a" type="f" corresp="#s8:l1">danssen</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s8:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">U Hoeren en hebben daer gheen goe <rhyme label="a" type="f">daghen</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Voor bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Kleef, Munster, Westphalen sy liever <rhyme label="a" type="f" corresp="#s9:l1">saghen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s9:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Wilt weder trecken na t'landt van <rhyme label="a" type="f">Kleef</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Van Bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Dees willen u niet een goet woort <rhyme label="a" type="f" corresp="#s10:l1">gheven</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s10:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wat leten u voor de halve <rhyme label="a" type="f">Mane</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Bij bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Dat onse palasaden daer <rhyme label="a" type="f" corresp="#s11:l1">staene</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s11:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Den vierden Julij, t staet u te <rhyme label="a" type="f">ghedinken</rhyme></l>
        <l id="s12:l2" met="2" type="-+">By bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Lieten de Geusen haer wapenen, <rhyme label="a" type="f" corresp="#s12:l1">blincken</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s12:l2">wederom</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Sy hebben u flucx van daer <rhyme label="a" type="f">ghedreven</rhyme></l>
        <l id="s13:l2" met="2" type="-+">Van bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Daer isser doen seven hondert doot <rhyme label="a" type="f" corresp="#s13:l1">ghebleven</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s13:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">U Papen die quamen met cruycen en <rhyme label="a" type="f">vanen</rhyme>,</l>
        <l id="s14:l2" met="2" type="-+">Bij bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">U Krijchsvolck seghenen voor de halve <rhyme label="a" type="f" corresp="#s14:l1">mane</rhyme>,</l>
        <l id="s14:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s14:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Ten heeft u al gheen victorie <rhyme label="a" type="f">ghegeven</rhyme></l>
        <l id="s15:l2" met="2" type="-+">By bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Twee Papen die moesten daer oock om <rhyme label="a" type="f" corresp="#s15:l1">sneven</rhyme>,</l>
        <l id="s15:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s15:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">God helpt altoos de vereenichde <rhyme label="a" type="f">Landen</rhyme></l>
        <l id="s16:l2" met="2" type="-+">Vreest Bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Al door de Leeuw met scherpe <rhyme label="a" type="f" corresp="#s16:l1">tanden</rhyme>,</l>
        <l id="s16:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s16:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Wy hebben s'Prins van Orangien <rhyme label="a" type="f">sone</rhyme></l>
        <l id="s17:l2" met="2" type="-+">Tot Bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">By alle vrome spant hy de <rhyme label="a" type="f" corresp="#s17:l1">croone</rhyme>,</l>
        <l id="s17:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s17:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Sijn gasten,, verrasten, u volck int <rhyme label="a" type="f">vanghen</rhyme></l>
        <l id="s18:l2" met="2" type="-+">By Bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Aen masten, met basten sy souden u <rhyme label="a" type="f" corresp="#s18:l1">hanghen</rhyme>,</l>
        <l id="s18:l4" met="2" type="-+">Oft keert <rhyme label="b" type="m" corresp="#s18:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Eylacen ocharmen,, is dan al <rhyme label="a" type="f">t'karmen</rhyme></l>
        <l id="s19:l2" met="2" type="-+">By bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Wie wil u bescharmen oft oock <rhyme label="a" type="f" corresp="#s19:l1">ontfarmen</rhyme>,</l>
        <l id="s19:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s19:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Ghy meucht sien met u valsche <rhyme label="a" type="f">listen</rhyme></l>
        <l id="s20:l2" met="2" type="-+">Van bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">Om loosselijck binnen den Bosch te <rhyme label="a" type="f" corresp="#s20:l1">twisten</rhyme>,</l>
        <l id="s20:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s20:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Ghy moet ras loopen al soudy wat <rhyme label="a" type="f">sweten</rhyme></l>
        <l id="s21:l2" met="2" type="-+">Van bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">De bosschenaers sullen u wellecom <rhyme label="a" type="f" corresp="#s21:l1">heeten</rhyme>,</l>
        <l id="s21:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s21:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Soo ghy cond binnen den Bosch <rhyme label="a" type="f">gheraken</rhyme>,</l>
        <l id="s22:l2" met="2" type="-+">Van bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s22:l3" met="4" type="-+">Ghy mocht een Antwerps Casteel <rhyme label="a" type="f" corresp="#s22:l1">maken</rhyme>,</l>
        <l id="s22:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s22:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Alwaerdy hier over een maent <rhyme label="a" type="f">gheweken</rhyme>,</l>
        <l id="s23:l2" met="2" type="-+">Van bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">Daer souder soo vele van bloede niet <rhyme label="a" type="f" corresp="#s23:l1">leken</rhyme>,</l>
        <l id="s23:l4" met="2" type="-+">Keert <rhyme label="b" type="m" corresp="#s23:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Ghy hebt de weerd in, hoe comder nu <rhyme label="a" type="f">buyten</rhyme></l>
        <l id="s24:l2" met="2" type="-+">Van bommelken <rhyme label="b" type="m">bom</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Ontbiedt veel schepen Galeyen en <rhyme label="a" type="f" corresp="#s24:l1">schuyten</rhyme>,</l>
        <l id="s24:l4" met="2" type="-+">En keert <rhyme label="b" type="m" corresp="#s24:l2">wederom</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">God wil Nassouwe ghenade <rhyme label="a" type="f">verleenen</rhyme>,</l>
        <l id="s25:l2" met="2" type="-+">Hier ende <rhyme label="b" type="m">alom</rhyme>,</l>
        <l id="s25:l3" met="4" type="-+">Dat hyse mach helpen die treuren en <rhyme label="a" type="f" corresp="#s25:l1">weenen</rhyme>,</l>
        <l id="s25:l4" met="2" type="-+">Dit wenscht <rhyme label="b" type="m" corresp="#s25:l2">Bisdom</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
