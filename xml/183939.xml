  <author>Jan Steven van Esveldt Holtrop, Willem Holtrop</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ce n'est que dans ces <rhyme label="a" type="m">lieux</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Qu'on goûte un sort <rhyme label="b" type="f">tranquille</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Ce sejour est <rhyme label="b" type="f" corresp="#s1:l2">l'azyle</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Des mortels et des <rhyme label="a" type="m" corresp="#s1:l1">dieux</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Astrée y tient sa <rhyme label="c" type="m">cour</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Et la brillante <rhyme label="d" type="f">Flore</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Sur les pas de <rhyme label="d" type="f" corresp="#s1:l6">l'aurore</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En écarte <rhyme label="c" type="m" corresp="#s1:l5">l'amour</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">L'aimable <rhyme label="a" type="m">liberté</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Qui pour nous <rhyme label="b" type="f">s'interesle</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dans notre Ordre sans <rhyme label="b" type="f" corresp="#s2:l2">cesse</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Conduit la <rhyme label="a" type="m" corresp="#s2:l1">volupté</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Les jeux et les <rhyme label="c" type="m">plaisirs</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">A nos voeux tout <rhyme label="d" type="f">suceède</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Tout s'y soumet, tout <rhyme label="d" type="f" corresp="#s2:l6">cède</rhyme></l>
        <l id="s2:l8" met="3" type="-+">A nos ardents <rhyme label="c" type="m" corresp="#s2:l5">désirs</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Des plus vives <rhyme label="a" type="m">ardeurs</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Le dieu de la <rhyme label="b" type="f">tendresse</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Par la <rhyme label="b" type="f" corresp="#s3:l2">délicatesse</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sait enflammer nos <rhyme label="a" type="m" corresp="#s3:l1">coeurs</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">C'est lui qui nous <rhyme label="c" type="m">conduit</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Son slambeau nous <rhyme label="d" type="f">éclaire</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Tout est pour nous <rhyme label="d" type="f" corresp="#s3:l6">Cythère</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Où sa clarté nous <rhyme label="c" type="m" corresp="#s3:l5">luit</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Si nous saisons un <rhyme label="a" type="m">choix</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">C'est d'après la <rhyme label="b" type="f">sagesse</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Cette aimable <rhyme label="b" type="f" corresp="#s4:l2">déesse</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dicte seule nos <rhyme label="a" type="m" corresp="#s4:l1">loix</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Des plus purs <rhyme label="c" type="m">sentimens</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Elle remplit nos <rhyme label="d" type="f">ames</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Elle allume nos <rhyme label="d" type="f" corresp="#s4:l6">slammes</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Et gouverne nos <rhyme label="c" type="m" corresp="#s4:l5">sens</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Envain à nos <rhyme label="a" type="m">regards</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dans ce lieu <rhyme label="b" type="f">folitaire</rhyme></l>
        <l id="s5:l3" met="3" type="-+">La reine de <rhyme label="b" type="f" corresp="#s5:l2">Cythère</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Et l'illustre dieu <rhyme label="a" type="m" corresp="#s5:l1">Mars</rhyme></l>
        <l id="s5:l5" met="3" type="-+">S'offriroient à la <rhyme label="c" type="m">fois</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">La paix a nos <rhyme label="d" type="f">hommages</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">La vertu nos <rhyme label="d" type="f" corresp="#s5:l6">suffrages</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Et l'amitie nos <rhyme label="c" type="m" corresp="#s5:l5">voix</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
