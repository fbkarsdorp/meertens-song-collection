  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Maria kuniginne <rhyme label="a" type="m">her</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">laz mich geniezen <rhyme label="b" type="f">vrowe</rhyme></l>
        <l id="s1:l3" met="4" type="-+">daz der wiselosen <rhyme label="a" type="m" corresp="#s1:l1">cher</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">gahet gegen diner <rhyme label="c" type="f">gute</rhyme></l>
        <l id="s1:l5" met="5" type="-+">vnde hilf ouch mir durch die vroude <rhyme label="d" type="m">din</rhyme></l>
        <l id="s1:l6" met="4" type="-+">der ich dir vuonfe nenne <rhyme label="e" type="m">hie</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">die erste di din heiliger <rhyme label="f" type="f">lip</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">den herren grouwz <rhyme label="e" type="m" corresp="#s1:l6">enphihe</rhyme></l>
        <l id="s1:l9" met="3" type="-+">du dir der engel <rhyme label="g" type="f">kunde</rhyme></l>
        <l id="s1:l10" met="5" type="-+">daz du gotes muter soldes <rhyme label="d" type="m" corresp="#s1:l5">sin</rhyme></l>
        <l id="s1:l11" met="5" type="+-">die ander vroude daz du maget <rhyme label="h" type="f">gebere</rhyme>.</l>
        <l id="s1:l12" met="5" type="-+">Die dritde. Do du in grozen leide <rhyme label="h" type="f" corresp="#s1:l11">were</rhyme></l>
        <l id="s1:l13" met="4" type="-+">Do man dir sagete sine <rhyme label="i" type="f">vrstende</rhyme>.</l>
        <l id="s1:l14" met="4" type="-+">Die vierte was sin <rhyme label="j" type="m">hemelvart</rhyme></l>
        <l id="s1:l15" met="4" type="-+">Die vumfte dir erzeiget <rhyme label="j" type="m" corresp="#s1:l14">wart</rhyme></l>
        <l id="s1:l16" met="5" type="-+">Do dich heim wolte ane <rhyme label="i" type="f" corresp="#s1:l13">missewende</rhyme></l>
      </lg>
    </lg>
  </text>
