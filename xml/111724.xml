  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SUjo, Sujo, Kintje <rhyme label="a" type="m">slaap</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Leg uw kopje rasjes <rhyme label="b" type="f">neder</rhyme>;</l>
        <l id="s1:l3" met="5" type="-+">Schrey zo niet, myn teere schaap, <rhyme label="b" type="f" corresp="#s1:l2">Alweder</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Want de boogjes Van uw <rhyme label="c" type="f">oogjes</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Zyn noch veel te <rhyme label="b" type="f" corresp="#s1:l2">teder</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Lieve poetje, slaap toch <rhyme label="a" type="m">ras</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Sluit toch vaardig toe uw <rhyme label="b" type="f">oogen</rhyme>:</l>
        <l id="s2:l3" met="5" type="-+">Gy hebt immers wel te pas <rhyme label="b" type="f" corresp="#s2:l2">Gezoogen</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">En geen speltje, Liefje, <rhyme label="c" type="f">queltje</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Of ik ben <rhyme label="b" type="f" corresp="#s2:l2">bedrogen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Zie dat zy haar oogjes <rhyme label="a" type="m">sluyt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Mogt zy nu wat ruste <rhyme label="b" type="f">rapen</rhyme>!</l>
        <l id="s3:l3" met="5" type="-+">Maak in't minste geen geluyt Laat <rhyme label="b" type="f" corresp="#s3:l2">slapen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">'t Lieve Zoetje, Myn teer <rhyme label="c" type="f">bloetje</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Tot myn vreugd <rhyme label="b" type="f" corresp="#s3:l2">geschapen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Dagt eerst, dat zo vast geen <rhyme label="a" type="m">bant</rhyme></l>
        <l id="s4:l2" met="4" type="+-">In natuure wiert <rhyme label="b" type="f">gevonden</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Als van't Paar, dat hand aan hand <rhyme label="b" type="f" corresp="#s4:l2">Verbonden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Eens van zinnen, In haar <rhyme label="c" type="f">Minnen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Alle wellust <rhyme label="b" type="f" corresp="#s4:l2">vonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maar, een Moeder <rhyme label="a" type="m">ondervind</rhyme></l>
        <l id="s5:l2" met="4" type="+-">In den grooten kinder <rhyme label="b" type="f">zeegen</rhyme></l>
        <l id="s5:l3" met="5" type="-+">Noch een band, die nau verbind, <rhyme label="b" type="f" corresp="#s5:l2">Gelegen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Die verbint'nis, Die aan 't Kint <rhyme label="c" type="f">is</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Is niet op te <rhyme label="b" type="f" corresp="#s5:l2">wegen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Geeft de Hemel die <rhyme label="a" type="m">genâ</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat ik u leer goede <rhyme label="b" type="f">Zeden</rhyme>.</l>
        <l id="s6:l3" met="5" type="-+">Zo vergeet ik ras de schâ <rhyme label="b" type="f" corresp="#s6:l2">Geleden</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">'k Zal myn rust en Al myn <rhyme label="c" type="f">lusten</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Tot uw heil <rhyme label="b" type="f" corresp="#s6:l2">besteden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
