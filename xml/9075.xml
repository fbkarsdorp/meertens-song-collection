<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9075</id>
    <date>
      <year-start>1676</year-start>
      <year-end>1676</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
    </tags>
    <melody>Het vinnige stralen van de zon</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O minnelijckst' <rhyme label="a" type="m">Emanuel</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Emanuel mijn <rhyme label="b" type="f">Leven</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Com set u by my neder <rhyme label="a" type="m" corresp="#s1:l1">wel</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Maar! wat sal ick u <rhyme label="b" type="f" corresp="#s1:l2">geven</rhyme>?</l>
        <l id="s1:l5" met="2" type="-+">Ick sal een <rhyme label="c" type="m">croon</rhyme></l>
        <l id="s1:l6" met="3" type="-+">van Lauwerieren <rhyme label="c" type="m" corresp="#s1:l5">schoon</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Uw dierbaar hooft <rhyme label="b" type="f" corresp="#s1:l2">omweven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O onbedagte <rhyme label="a" type="m">Sulamith</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Sulamith soet van <rhyme label="b" type="f">wesen</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">Waar cont gy bloemen rood en <rhyme label="a" type="m" corresp="#s2:l1">wit</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Waar soud gy 't cruydje <rhyme label="b" type="f" corresp="#s2:l2">lesen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Dat my het <rhyme label="c" type="m">breyn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat uwe minne <rhyme label="c" type="m" corresp="#s2:l5">reyn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Gevoegelijck soud <rhyme label="b" type="f" corresp="#s2:l2">wesen</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De blaad'ren van mijn hert, den <rhyme label="a" type="m">mond</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door lipp' en tong <rhyme label="b" type="f">ontschoten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">De bloemen uyt den selven <rhyme label="a" type="m" corresp="#s3:l1">grond</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Met uw dauw <rhyme label="b" type="f" corresp="#s3:l2">overgoten</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Die <rhyme label="c" type="m">ongemeen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Geweven digt in <rhyme label="c" type="m" corresp="#s3:l5">een</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hebben u noyt <rhyme label="b" type="f" corresp="#s3:l2">verdroten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laat door uw blad en bloemen <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Door cleur en geur, van <rhyme label="b" type="f">verren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Granaat en druyf-tros onder <rhyme label="a" type="m" corresp="#s4:l1">een</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Door-flonckeren als <rhyme label="b" type="f" corresp="#s4:l2">sterren</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">So sal ick <rhyme label="c" type="m">in</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Uw liefelijcke <rhyme label="c" type="m" corresp="#s4:l5">min</rhyme></l>
        <l id="s4:l7" met="3" type="-+">O Sulamith <rhyme label="b" type="f" corresp="#s4:l2">verwerren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O! Sulamith! gy hebt mijn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Verovert met uw <rhyme label="b" type="f">oogen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Emanuel! gy door uw <rhyme label="a" type="m" corresp="#s5:l1">smert</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Hebt my mijn siel <rhyme label="b" type="f" corresp="#s5:l2">ontogen</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Dus <rhyme label="c" type="m">Sulamith</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Emanuel <rhyme label="c" type="m" corresp="#s5:l5">besit</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Haar eygen siel <rhyme label="b" type="f" corresp="#s5:l2">ontvlogen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Seg Sulamith! wat was u <rhyme label="a" type="m">lest</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat gy my liet voor deur <rhyme label="b" type="f">staan</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">Ick clopt', ick riep, ick sprak om <rhyme label="a" type="m" corresp="#s6:l1">best</rhyme>;</l>
        <l id="s6:l4" met="4" type="-+">Tot my den dauw dee <rhyme label="b" type="f" corresp="#s6:l2">deur-gaan</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Of ick u <rhyme label="c" type="m">schuw</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dan of ick ben by <rhyme label="c" type="m" corresp="#s6:l5">uw</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Voor dees' wist gy daar keur <rhyme label="b" type="f" corresp="#s6:l2">aan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ey! swijgt mijn schaamt, <rhyme label="a" type="m">Emanuel</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Gy keerde u nauwlijcx <rhyme label="b" type="f">henen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Of 't lijf sig rept ter bedd' uyt <rhyme label="a" type="m" corresp="#s7:l1">snel</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En 't hert besweeck met <rhyme label="b" type="f" corresp="#s7:l2">eenen</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">En deed een <rhyme label="c" type="m">vloed</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Van Myrrhe druppen <rhyme label="c" type="m" corresp="#s7:l5">soet</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Op grendel, slot, en <rhyme label="b" type="f" corresp="#s7:l2">stenen</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Den oly deed het slot met <rhyme label="a" type="m">lust</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Opspringen, en ick <rhyme label="b" type="f">ruckte</rhyme></l>
        <l id="s8:l3" met="4" type="-+">My stratewaart (sy dat de <rhyme label="a" type="m" corresp="#s8:l1">rust</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Mijn wakend hert soo <rhyme label="b" type="f" corresp="#s8:l2">druckte</rhyme>!)</l>
        <l id="s8:l5" met="2" type="-+">Of (dagt ick) <rhyme label="c" type="m">daar</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Emanuel nog <rhyme label="c" type="m" corresp="#s8:l5">waar</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Maar laas! het my <rhyme label="b" type="f" corresp="#s8:l2">misluckte</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Lust Sulamith den slaap so <rhyme label="a" type="m">(dagt</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Ick) dat sy my mag <rhyme label="b" type="f">derven</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Sy slape dan vry voort. (Sul.) Bey; <rhyme label="a" type="m" corresp="#s9:l1">sagt</rhyme>!</l>
        <l id="s9:l4" met="4" type="-+">Bey! liever woud ick <rhyme label="b" type="f" corresp="#s9:l2">sterven</rhyme>:</l>
        <l id="s9:l5" met="2" type="-+">Van nu af <rhyme label="c" type="m">aan</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Wilt 't herte wacker <rhyme label="c" type="m" corresp="#s9:l5">staan</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Laat ick uw gunst maar <rhyme label="b" type="f" corresp="#s9:l2">werven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Vol op is u mijn gunst <rhyme label="a" type="m">bereyd</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Die 't eyndeloos <rhyme label="b" type="f">behagen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Van 's Vaders <rhyme label="a" type="m" corresp="#s10:l1">Algenoegsaamheyd</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Heb aan den mensch doen <rhyme label="b" type="f" corresp="#s10:l2">dagen</rhyme>.</l>
        <l id="s10:l5" met="2" type="-+">Mijn siele <rhyme label="c" type="m">smelt</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Als gy my dat <rhyme label="c" type="m" corresp="#s10:l5">vermelt</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Mijn hert en can 't niet <rhyme label="b" type="f" corresp="#s10:l2">dragen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ia 's Hemels glansch mijn <rhyme label="a" type="m">errifdeel</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Heb ick om u <rhyme label="b" type="f">gelaten</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Gaf ymand my dees aard <rhyme label="a" type="m" corresp="#s11:l1">geheel</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Vol rijckdom, lust en <rhyme label="b" type="f" corresp="#s11:l2">staten</rhyme>;</l>
        <l id="s11:l5" met="2" type="-+">'K reuyldese <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Voor Lauwren u ter <rhyme label="c" type="m" corresp="#s11:l5">eer</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Te vlegten met <rhyme label="b" type="f" corresp="#s11:l2">Granaten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Waar heen, mijn Coning? Ah en <rhyme label="a" type="m">scheydt</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En laat die son niet <rhyme label="b" type="f">dalen</rhyme>!</l>
        <l id="s12:l3" met="4" type="-+">Ia duyv mijn bysijn u <rhyme label="a" type="m" corresp="#s12:l1">verleydt</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Gy laat uw schaapjens <rhyme label="b" type="f" corresp="#s12:l2">dwalen</rhyme>.</l>
        <l id="s12:l5" met="2" type="-+">Dwalense? <rhyme label="c" type="m">vliedt</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Emanuel dan <rhyme label="c" type="m" corresp="#s12:l5">niet</rhyme>:</l>
        <l id="s12:l7" met="3" type="-+">Maar helptse kop-waarts <rhyme label="b" type="f" corresp="#s12:l2">halen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
