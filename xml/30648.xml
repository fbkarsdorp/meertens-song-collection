<song>
  <meta>
    <id>30648</id>
    <date>
      <year-start>1576</year-start>
      <year-end>1600</year-end>
    </date>
    <location>
      <orginal-name>gent?</orginal-name>
      <standard-name>Gent, Belgium</standard-name>
      <coordinates>
        <lon>51.0538286</lon>
        <lat>3.7250121</lat>
      </coordinates>
