    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Myn Siel maeckt groot den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Myn geest verhueght hem <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In mynen Godt <rhyme label="b" type="f">genadich</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Die met nieuw <rhyme label="c" type="m">segeningh</rhyme></l>
        <l id="s1:l5" met="3" type="-+">My syn dienstmaecht <rhyme label="c" type="m" corresp="#s1:l4">geringh</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Heeft besocht seer <rhyme label="b" type="f" corresp="#s1:l3">weldadich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gy hebt, o Heer, <rhyme label="a" type="m">verhooght</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Myn HORN, end u <rhyme label="a" type="m" corresp="#s2:l1">getooght</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Mynen heylandt te <rhyme label="b" type="f">wesen</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Hierom soo wenschen <rhyme label="c" type="m">my</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Al rechtgesinde <rhyme label="c" type="m" corresp="#s2:l4">bly</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Veel g'luck end heyl <rhyme label="b" type="f" corresp="#s2:l3">mitsdesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gy hebt, Heer, van jongs <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">My soo veel goets <rhyme label="a" type="m" corresp="#s3:l1">gedaen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Meer als ick wel can <rhyme label="b" type="f">loven</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Ja, soud' ickt noemen <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">My soude t'groot <rhyme label="c" type="m" corresp="#s3:l4">getal</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Seer verre gaen te <rhyme label="b" type="f" corresp="#s3:l3">boven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maer t'welck is <rhyme label="a" type="m">aldermeest</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hoogh t'achten, uwen <rhyme label="a" type="m" corresp="#s4:l1">Geest</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hebt gy my, Heer, <rhyme label="b" type="f">gegeven</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Oock hebt gy <rhyme label="c" type="m">doorgeboort</rhyme></l>
        <l id="s4:l5" met="3" type="-+">My d'ooren, om <rhyme label="c" type="m" corresp="#s4:l4">u'woort</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Rechtsinnich te <rhyme label="b" type="f" corresp="#s4:l3">beleven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy, als een herder <rhyme label="a" type="m">trauw</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hebt my op groen <rhyme label="a" type="m" corresp="#s5:l1">landauw</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Steeds lieffelyck <rhyme label="b" type="f">geweydet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">End aen dat water <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Van uwe kennis <rhyme label="c" type="m" corresp="#s5:l4">goet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met uwen staf <rhyme label="b" type="f" corresp="#s5:l3">geleydet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">(Wie ben ick doch, o <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat gy gedenckt soo <rhyme label="a" type="m" corresp="#s6:l1">seer</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Aen u' dienstmaecht <rhyme label="b" type="f">eenvoudich</rhyme>?</l>
        <l id="s6:l4" met="3" type="-+">Ick ben ja te <rhyme label="c" type="m">geringh</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Voor al de <rhyme label="c" type="m" corresp="#s6:l4">segeningh</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die gy my schenckt <rhyme label="b" type="f" corresp="#s6:l3">veelvoudich</rhyme>.)</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Al dit U docht te <rhyme label="a" type="m">syn</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Noch weynich. O Godt <rhyme label="a" type="m" corresp="#s7:l1">myn</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Gy stelt my heel in <rhyme label="b" type="f">rusten</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Gy neempt my by der <rhyme label="c" type="m">handt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">End voert my aen den <rhyme label="c" type="m" corresp="#s7:l4">cant</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Eener BEK' om <rhyme label="b" type="f" corresp="#s7:l3">verlusten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Op dat dien <rhyme label="a" type="m">PALMEBOOM</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Aen desen <rhyme label="a" type="m" corresp="#s8:l1">waterstroom</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Mocht weeldrich staen ten <rhyme label="b" type="f">thoone</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Dien boom, die met u' <rhyme label="c" type="m">handt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Van jongs op was <rhyme label="c" type="m" corresp="#s8:l4">geplant</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In u' voorhoven <rhyme label="b" type="f" corresp="#s8:l3">schoone</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">(Och Heer, hoe costlyck <rhyme label="a" type="m">is</rhyme></l>
        <l id="s9:l2" met="3" type="-+">U' gunst voor die <rhyme label="a" type="m" corresp="#s9:l1">gewis</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Onder u' vleugels <rhyme label="b" type="f">rusten</rhyme>!</l>
        <l id="s9:l4" met="3" type="-+">Gy laeft se met het <rhyme label="c" type="m">vet</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Dyns huys, en drencktse <rhyme label="c" type="m" corresp="#s9:l4">met</rhyme></l>
        <l id="s9:l6" met="3" type="-+">De bek' uwer <rhyme label="b" type="f" corresp="#s9:l3">wellusten</rhyme>.)</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Gepresen sy myn <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die my dit beste <rhyme label="a" type="m" corresp="#s10:l1">lot</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Heeft gunstich <rhyme label="b" type="f">toegevoeget</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Hy wil my tselve <rhyme label="c" type="m">deel</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Langh houden int <rhyme label="c" type="m" corresp="#s10:l4">geheel</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Dat my soo wel <rhyme label="b" type="f" corresp="#s10:l3">genoeget</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Geeft, Heer, dat dit <rhyme label="a" type="m">plantsoen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Vrucht brengh' in recht <rhyme label="a" type="m" corresp="#s11:l1">saisoen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Tot aengename <rhyme label="b" type="f">spyse</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">Geeft dat syn bladers <rhyme label="c" type="m">fyn</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Tot heylsaem <rhyme label="c" type="m" corresp="#s11:l4">medecyn</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Dienen tot uwen <rhyme label="b" type="f" corresp="#s11:l3">pryse</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Geeft dat ons' tacken <rhyme label="a" type="m">wyt</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Sich mogen breyden <rhyme label="a" type="m" corresp="#s12:l1">uut</rhyme></l>
        <l id="s12:l3" met="3" type="-+">G'lyck spruyten van <rhyme label="b" type="f">olyven</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Te dienen tot <rhyme label="c" type="m">cieraet</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Van Sion, om te <rhyme label="c" type="m" corresp="#s12:l4">baet</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Uwes huys te <rhyme label="b" type="f" corresp="#s12:l3">verblyven</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Eyndlyck, maeckt ons <rhyme label="a" type="m">bequaem</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Te heyl'gen uwen <rhyme label="a" type="m" corresp="#s13:l1">naem</rhyme></l>
        <l id="s13:l3" met="3" type="-+">End in u' vrees' te <rhyme label="b" type="f">blyven</rhyme>:</l>
        <l id="s13:l4" met="3" type="-+">Want uwe gunst <rhyme label="c" type="m">voortaen</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Aen all' die oprecht <rhyme label="c" type="m" corresp="#s13:l4">ghaen</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Eeuwichlyck sal <rhyme label="b" type="f" corresp="#s13:l3">beclyven</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Gy all' die vreest den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Verhueght u met my <rhyme label="a" type="m" corresp="#s14:l1">seer</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">End wilt sonder <rhyme label="b" type="f">berouwen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Voortgaen van duecht tot <rhyme label="c" type="m">duecht</rhyme>:</l>
        <l id="s14:l5" met="3" type="-+">Hy sal ons t' onser <rhyme label="c" type="m" corresp="#s14:l4">vruecht</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Veel segening <rhyme label="b" type="f" corresp="#s14:l3">ontvauwen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Voorts, gy myn Siele <rhyme label="a" type="m">wilt</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Den Heer uw' Son end <rhyme label="a" type="m" corresp="#s15:l1">Schilt</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Gestadich dancbaer <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Voor al dit groote <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Dat hy u senden <rhyme label="c" type="m" corresp="#s15:l4">doet</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Uut Sion <rhyme label="b" type="f" corresp="#s15:l3">hooch-gepresen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
