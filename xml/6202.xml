<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6202</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het daagt in de oosten</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">BEdroefde <rhyme label="a" type="f">Harderinne</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die 't schrale strandt <rhyme label="b" type="m">betreet</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Siet dat ghy dijne <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Aen 't aerdigh groen <rhyme label="b" type="m" corresp="#s1:l2">besteet</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">En wilt u Schaepjes <rhyme label="c" type="f">leyden</rhyme></l>
        <l id="s1:l6" met="2" type="+-">In goe <rhyme label="c" type="f" corresp="#s1:l5">Weyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mocht ick nae wensch <rhyme label="a" type="f">verkiesen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Een wel-gheseghent <rhyme label="b" type="m">Landt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ick sou wel graegh <rhyme label="a" type="f" corresp="#s2:l1">verliesen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Het Peeckel-schuymich <rhyme label="b" type="m" corresp="#s2:l2">strandt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Maer wie soud' my <rhyme label="c" type="f">begheeren</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Te <rhyme label="c" type="f" corresp="#s2:l5">vereeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Lof-waerde <rhyme label="a" type="f">Harderinne</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">Ick presenteer mijn <rhyme label="b" type="m">Wey</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Aen u, uyt lieve <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">En wil u doen <rhyme label="b" type="m" corresp="#s3:l2">gheley</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Daer ghy u Vee meucht <rhyme label="c" type="f">asen</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">In mijn <rhyme label="c" type="f" corresp="#s3:l5">Grasen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Danck heb u <rhyme label="a" type="f">presentacy</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">V woorden schijnen <rhyme label="b" type="m">trou</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Maer ach! u hart <rhyme label="a" type="f" corresp="#s4:l1">(eylacy</rhyme>)</l>
        <l id="s4:l4" met="3" type="-+">Wel anders meynen <rhyme label="b" type="m" corresp="#s4:l2">sou</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En ick kan 't harts <rhyme label="c" type="f">secreten</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Qualijck <rhyme label="c" type="f" corresp="#s4:l5">weten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick sweer u by de <rhyme label="a" type="f">Goden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Mijn lieve <rhyme label="b" type="m">Galathe</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Mijn Vader sal u <rhyme label="a" type="f" corresp="#s5:l1">noden</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Daer toe u hong'rich <rhyme label="b" type="m" corresp="#s5:l2">Vee</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Op wel-ghemeste <rhyme label="c" type="f">Fruyten</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Vol <rhyme label="c" type="f" corresp="#s5:l5">virtuyten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Adieu dan dorre <rhyme label="a" type="f">Stranden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Daer 't altijdt guerigh <rhyme label="b" type="m">waeyt</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Adieu, ghy bracke <rhyme label="a" type="f" corresp="#s6:l1">Sanden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Met Schelpen hart <rhyme label="b" type="m" corresp="#s6:l2">besaeyt</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Ick wil (voor groene <rhyme label="c" type="f">straten</rhyme>)</l>
        <l id="s6:l6" met="2" type="+-">V <rhyme label="c" type="f" corresp="#s6:l5">verlaten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Treed aen, mijn <rhyme label="a" type="f">uytghelesen</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Treed aen, mijn Beckjen <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick sal u Leydsman <rhyme label="a" type="f" corresp="#s7:l1">wesen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Daerom hebt goeden <rhyme label="b" type="m" corresp="#s7:l2">moet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Tot aen de soete <rhyme label="c" type="f">Stroomtjens</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Met haer <rhyme label="c" type="f" corresp="#s7:l5">Boomtjens</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu sullen dijne <rhyme label="a" type="f">Wanghen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Soo bleeck, en wit als <rhyme label="b" type="m">snee</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Haest purper-root <rhyme label="a" type="f" corresp="#s8:l1">ontfanghen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Mijn lieve <rhyme label="b" type="m" corresp="#s8:l2">Galathe</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Daer Ph≈ìbus gulde <rhyme label="c" type="f">Stralen</rhyme></l>
        <l id="s8:l6" met="2" type="+-">Schitt'righ <rhyme label="c" type="f" corresp="#s8:l5">dalen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Hier zijn wy aen de <rhyme label="a" type="f">Acker</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Die tot mijn Wooningh <rhyme label="b" type="m">reckt</rhyme>;</l>
        <l id="s9:l3" met="3" type="-+">Nu Galatheetje, <rhyme label="a" type="f" corresp="#s9:l1">wacker</rhyme></l>
        <l id="s9:l4" met="3" type="-+">V hackjes nae u <rhyme label="b" type="m" corresp="#s9:l2">treckt</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Hier ist daer Vog'len <rhyme label="c" type="f">singhen</rhyme>,</l>
        <l id="s9:l6" met="2" type="+-">Dieren <rhyme label="c" type="f" corresp="#s9:l5">springhen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Treedt in dees gulde <rhyme label="a" type="f">Salen</rhyme>;</l>
        <l id="s10:l2" met="3" type="-+">Dit is nu mijn <rhyme label="b" type="m">Ghebouw</rhyme>;</l>
        <l id="s10:l3" met="3" type="-+">Hier wil ick u <rhyme label="a" type="f" corresp="#s10:l1">onthalen</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Hier sult ghy zijn mijn <rhyme label="b" type="m" corresp="#s10:l2">Vrouw</rhyme>;</l>
        <l id="s10:l5" met="3" type="-+">Wilt hier voor 't Vee te <rhyme label="c" type="f">drijven</rhyme></l>
        <l id="s10:l6" met="2" type="+-">Vreuchdigh <rhyme label="c" type="f" corresp="#s10:l5">blijven</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O! wel-gheluckigh <rhyme label="a" type="f">leven</rhyme>;</l>
        <l id="s11:l2" met="3" type="-+">Dat my dus soet toe <rhyme label="b" type="m">lacht</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Die my dus heeft <rhyme label="a" type="f" corresp="#s11:l1">verheven</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En hier toe heeft <rhyme label="b" type="m" corresp="#s11:l2">ghebracht</rhyme>:</l>
        <l id="s11:l5" met="3" type="-+">Wensch ick het hooghste <rhyme label="c" type="f">wenschen</rhyme></l>
        <l id="s11:l6" met="2" type="+-">Aller <rhyme label="c" type="f" corresp="#s11:l5">menschen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
