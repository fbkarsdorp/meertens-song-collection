    <tags>
      <tag translation="mystical song">mystiek lied</tag>
    </tags>
    <melody>Verlangen verlangen doet mijn hart pijn</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Min hartge, dat wil ic Gode <rhyme label="a" type="f">opgheven</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ende doen my alle menschen <rhyme label="b" type="m">of</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ende soeken troest in alle minen <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Daer icse ghestadich vinden <rhyme label="b" type="m" corresp="#s1:l2">mach</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m">laes</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m">ghelucx</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">So lijdt dat herte <rhyme label="d" type="m" corresp="#s1:l6">mijn</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m" corresp="#s1:l5">laes</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m" corresp="#s1:l6">mijn</rhyme>,</l>
        <l id="s1:l11" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m" corresp="#s1:l7">ghelucx</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">So lijdt dat herte <rhyme label="d" type="m" corresp="#s1:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Och, harte ende sin wilic Hem <rhyme label="a" type="f">gheven</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Maer boven alledien ic <rhyme label="b" type="m">weet</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Dat ic dus spade heb <rhyme label="a" type="f" corresp="#s2:l1">begonnen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat is my van gueder herten <rhyme label="b" type="m" corresp="#s2:l2">leet</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m">laes</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m">ghelucx</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">So lijdt dat herte <rhyme label="d" type="m" corresp="#s2:l6">mijn</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m" corresp="#s2:l5">laes</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m" corresp="#s2:l6">mijn</rhyme>,</l>
        <l id="s2:l11" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m" corresp="#s2:l7">ghelucx</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">So lijdt dat herte <rhyme label="d" type="m" corresp="#s2:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Conde ic dat weesen Gods <rhyme label="a" type="f">ghebruken</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Alsoe vriendelijc inder sielen <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Soe soude ic alle dinck buten <rhyme label="a" type="f" corresp="#s3:l1">sluten</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat my een hinder mochten <rhyme label="b" type="m" corresp="#s3:l2">sijn</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m">laes</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m">ghelucx</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">So lijdt dat herte <rhyme label="d" type="m" corresp="#s3:l6">mijn</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m" corresp="#s3:l5">laes</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m" corresp="#s3:l6">mijn</rhyme>,</l>
        <l id="s3:l11" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m" corresp="#s3:l7">ghelucx</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">So lijdt dat herte <rhyme label="d" type="m" corresp="#s3:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Och, daer en kan ic niet toe <rhyme label="a" type="f">gheraken</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hy en wort in my een stil <rhyme label="b" type="m">afgront</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat ic mach sien ende leeren <rhyme label="a" type="f" corresp="#s4:l1">smaken</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Hoe lief tot lief spreect sonder <rhyme label="b" type="m" corresp="#s4:l2">mont</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m">laes</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m">ghelucx</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">So lijdt dat herte <rhyme label="d" type="m" corresp="#s4:l6">mijn</rhyme>.</l>
        <l id="s4:l9" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m" corresp="#s4:l5">laes</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m" corresp="#s4:l6">mijn</rhyme>,</l>
        <l id="s4:l11" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m" corresp="#s4:l7">ghelucx</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">So lijdt dat herte <rhyme label="d" type="m" corresp="#s4:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Al en soude ic nymmermeer Sijn hulde <rhyme label="a" type="f">vercrighen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Noch troest tot ghenre tijt <rhyme label="b" type="m">ontfaen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Nochtans wilic Sijn eyghen <rhyme label="a" type="f" corresp="#s5:l1">bliven</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ghestadich vast al inder <rhyme label="b" type="m" corresp="#s5:l2">doet</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m">laes</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m">ghelucx</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">So lijdt dat herte <rhyme label="d" type="m" corresp="#s5:l6">mijn</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m" corresp="#s5:l5">laes</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m" corresp="#s5:l6">mijn</rhyme>,</l>
        <l id="s5:l11" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m" corresp="#s5:l7">ghelucx</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">So lijdt dat herte <rhyme label="d" type="m" corresp="#s5:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ic gheef my op, tis meer dan <rhyme label="a" type="f">tijt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ic legghe mijn hooft in Uwen <rhyme label="b" type="m">scoet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Want Ghij temael mijn eyghen <rhyme label="a" type="f" corresp="#s6:l1">sijt</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Nu helpt ons, God, uut alre <rhyme label="b" type="m" corresp="#s6:l2">noot</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m">laes</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m">ghelucx</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">So lijdt dat herte <rhyme label="d" type="m" corresp="#s6:l6">mijn</rhyme>.</l>
        <l id="s6:l9" met="3" type="-+">Ay laes, Ay laes, Ay <rhyme label="c" type="m" corresp="#s6:l5">laes</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">Heer Jhesus <rhyme label="d" type="m" corresp="#s6:l6">mijn</rhyme>,</l>
        <l id="s6:l11" met="3" type="-+">Meer druck, meer dan <rhyme label="e" type="m" corresp="#s6:l7">ghelucx</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">So lijdt dat herte <rhyme label="d" type="m" corresp="#s6:l6">mijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
