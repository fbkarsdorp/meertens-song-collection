<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188365</id>
    <date>
      <year-start>1651</year-start>
      <year-end>1651</year-end>
    </date>
    <location>
      <orginal-name>brugge</orginal-name>
      <standard-name>Brugge, Belgium</standard-name>
      <coordinates>
        <lon>51.2315785</lon>
        <lat>3.20809951603418</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Quis hic resultat clamor</melody>
  <author>Philippus Jennyn</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">QUis hic resultat <rhyme label="a" type="m">clamor</rhyme> ?</l>
        <l id="s1:l2" met="4" type="-+">Tuusne ? tuusne ? ô noster <rhyme label="a" type="m" corresp="#s1:l1">Amor</rhyme> !</l>
        <l id="s1:l3" met="2" type="-+">Quis gemit <rhyme label="b" type="m">hic</rhyme> ?</l>
        <l id="s1:l4" met="2" type="-+">Quis tremit <rhyme label="b" type="m" corresp="#s1:l3">sic</rhyme> ?</l>
        <l id="s1:l5" met="5" type="-+">Iesus est, vagiens ins <rhyme label="c" type="m">stabulo</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Super bestiarum <rhyme label="c" type="m" corresp="#s1:l5">pabulo</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Gemit aula, gemit <rhyme label="d" type="m">Rex</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Plaude caula, plaude <rhyme label="d" type="m" corresp="#s1:l7">grex</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">Caula gregis, aula Regis <rhyme label="e" type="m">est</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In Bethleëm <rhyme label="a" type="m">eamus</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Verbum hoc caro factum <rhyme label="a" type="m" corresp="#s2:l1">videamus</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Ah Deus <rhyme label="b" type="m">est</rhyme> !</l>
        <l id="s2:l4" met="2" type="-+">Nec reus <rhyme label="b" type="m" corresp="#s2:l3">est</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Iesus est, qui permisit se <rhyme label="c" type="m">pio</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Reclinari in <rhyme label="c" type="m" corresp="#s2:l5">praesepio</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">Nostra est <rhyme label="d" type="m">laudatio</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Nostra <rhyme label="d" type="m" corresp="#s2:l7">gloriatio</rhyme>,</l>
        <l id="s2:l9" met="5" type="+-">Nostrae mentis <rhyme label="e" type="m">Iubilatio</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Cum media sit jam <rhyme label="a" type="m">nox</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Protinus, Afini Bovisque sile <rhyme label="a" type="m" corresp="#s3:l1">vox</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Ut <rhyme label="b" type="m">dormiat</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ut <rhyme label="b" type="m" corresp="#s3:l3">dormiat</rhyme></l>
        <l id="s3:l5" met="5" type="-+">Iesulus, puer <rhyme label="c" type="m">benignissimus</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Infans millies <rhyme label="c" type="m" corresp="#s3:l5">gratissimus</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Dormi mea <rhyme label="d" type="m">requies</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Dormi, dormi mea <rhyme label="d" type="m" corresp="#s3:l7">spes</rhyme>,</l>
        <l id="s3:l9" met="5" type="+-">Dormi, dormi mea <rhyme label="e" type="m">saties</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O dulcis nate <rhyme label="a" type="m">Dei</rhyme> !</l>
        <l id="s4:l2" met="4" type="-+">Huccine descendisti causa <rhyme label="a" type="m" corresp="#s4:l1">mei</rhyme> ?</l>
        <l id="s4:l3" met="2" type="-+">O <rhyme label="b" type="m">charitas</rhyme> ?</l>
        <l id="s4:l4" met="2" type="-+">Quae <rhyme label="b" type="m" corresp="#s4:l3">paritas</rhyme> ?</l>
        <l id="s4:l5" met="5" type="-+">Deus es; vilis sum <rhyme label="c" type="m">homuncio</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Non aequivalens <rhyme label="c" type="m" corresp="#s4:l5">teruncio</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">Tibi sum amara <rhyme label="d" type="m">crux</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Tu es bonus Iudea <rhyme label="d" type="m" corresp="#s4:l7">Dux</rhyme>,</l>
        <l id="s4:l9" met="5" type="+-">Ego nox; &amp; tu es mea <rhyme label="e" type="m">lux</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Quid ploras Dei <rhyme label="a" type="m">Gnate</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Iesule nobis nate, nobis <rhyme label="a" type="m" corresp="#s5:l1">date</rhyme> ?</l>
        <l id="s5:l3" met="2" type="-+">Si <rhyme label="b" type="m">frigeas</rhyme>;</l>
        <l id="s5:l4" met="2" type="-+">Ne <rhyme label="b" type="m" corresp="#s5:l3">rigeas</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Ardeo in interno <rhyme label="c" type="m">homine</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Stabulare ibi <rhyme label="c" type="m" corresp="#s5:l5">Domine</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Veni nectar <rhyme label="d" type="m">coelicum</rhyme>.</l>
        <l id="s5:l8" met="4" type="+-">Veni mel <rhyme label="d" type="m" corresp="#s5:l7">Angelicum</rhyme>,</l>
        <l id="s5:l9" met="5" type="+-">Saturabis me <rhyme label="e" type="m">famelicum</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">In aure dulce <rhyme label="a" type="m">melos</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Iesule, trahe meum cor ad <rhyme label="a" type="m" corresp="#s6:l1">caelos</rhyme>:</l>
        <l id="s6:l3" met="2" type="-+">O cordis <rhyme label="b" type="m">mel</rhyme> !</l>
        <l id="s6:l4" met="2" type="-+"><rhyme label="b" type="m" corresp="#s6:l3">Emmanuel</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Casulà jacens in <rhyme label="c" type="m">pauperculâ</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Vt des nobis coeli <rhyme label="c" type="m" corresp="#s6:l5">fercula</rhyme>:</l>
        <l id="s6:l7" met="4" type="+-">Ad amoris <rhyme label="d" type="m">satiem</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Phebe mihi <rhyme label="d" type="m" corresp="#s6:l7">faciem</rhyme>;</l>
        <l id="s6:l9" met="5" type="+-">Solve cordis mei <rhyme label="e" type="m">glaciem</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Sit tibi laus, <rhyme label="a" type="m">decusque</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Iesule, ad extremum terrae <rhyme label="a" type="m" corresp="#s7:l1">usque</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+"><rhyme label="b" type="m">Coelestia</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+"><rhyme label="b" type="m" corresp="#s7:l3">Terrestria</rhyme>,</l>
        <l id="s7:l5" met="5" type="-+">Iesu mi, &amp; inferni <rhyme label="c" type="m">condita</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Tibi genu flectant <rhyme label="c" type="m" corresp="#s7:l5">subdita</rhyme>:</l>
        <l id="s7:l7" met="4" type="+-">Gloria, &amp; laus <rhyme label="d" type="m">Deo</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Redemptori sit <rhyme label="d" type="m" corresp="#s7:l7">meo</rhyme>,</l>
        <l id="s7:l9" met="5" type="+-">Quem laudare satis <rhyme label="e" type="m">nequeo</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
