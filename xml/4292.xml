<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4292</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Zoet Robbertje</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AErdich reyn // <rhyme label="a" type="f">gelaet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Int wesen <rhyme label="b" type="m">amoreus</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In geen pleyn // en <rhyme label="a" type="f" corresp="#s1:l1">staet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sulck bloemken <rhyme label="b" type="m" corresp="#s1:l2">pretieus</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Schoon maget <rhyme label="c" type="m">smal</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die wesen <rhyme label="c" type="m" corresp="#s1:l5">sal</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Geheel, en al // mijn <rhyme label="d" type="m">vreught</rhyme>:</l>
        <l id="s1:l8" met="3" type="+-">Laet my (by uwer <rhyme label="d" type="m" corresp="#s1:l7">deught</rhyme>)</l>
        <l id="s1:l9" met="3" type="-+">Met u eens zijn <rhyme label="d" type="m" corresp="#s1:l7">verheught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een eerbare // <rhyme label="a" type="f">bruyt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Zijt ghy, die wonder <rhyme label="b" type="m">doet</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Als int clare // <rhyme label="a" type="f" corresp="#s2:l1">luydt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">V voysken <rhyme label="b" type="m" corresp="#s2:l2">honigh-soet</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Mijn droufheyt <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s2:l6" met="2" type="-+">(Soo swaer als <rhyme label="c" type="m" corresp="#s2:l5">loot</rhyme>)</l>
        <l id="s2:l7" met="3" type="-+">Als vander doot // <rhyme label="d" type="m">verrijst</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">T'is recht datmen u <rhyme label="d" type="m" corresp="#s2:l7">prijst</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">T'bevinden sulcx <rhyme label="d" type="m" corresp="#s2:l7">bewijst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laet dan henen // <rhyme label="a" type="f">gaen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vileyne quade <rhyme label="b" type="m">becx</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Houdt verdwenen // <rhyme label="a" type="f" corresp="#s3:l1">staen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">T'gepeyns vol van <rhyme label="b" type="m" corresp="#s3:l2">gebrecx</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Met u <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">(En anders <rhyme label="c" type="m" corresp="#s3:l5">geen</rhyme>)</l>
        <l id="s3:l7" met="3" type="-+">Salt algemeen // doch <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Cause hier van, is <rhyme label="d" type="m" corresp="#s3:l7">dijn</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Goddinnelick <rhyme label="d" type="m" corresp="#s3:l7">aenschijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Kiest (O vrouwe // <rhyme label="a" type="f">reen</rhyme>)</l>
        <l id="s4:l2" met="3" type="-+">Geen ander lief <rhyme label="b" type="m">oprecht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Want in trouwe // <rhyme label="a" type="f" corresp="#s4:l1">geen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Soo vast, als uwen <rhyme label="b" type="m" corresp="#s4:l2">knecht</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Mijn hert <rhyme label="c" type="m">geheel</rhyme></l>
        <l id="s4:l6" met="2" type="-+">(Als een <rhyme label="c" type="m" corresp="#s4:l5">iuweel</rhyme>)</l>
        <l id="s4:l7" met="3" type="-+">Sal u tot deel // <rhyme label="d" type="m">geschien</rhyme>:</l>
        <l id="s4:l8" met="3" type="+-">Dus lief, wilt my by <rhyme label="d" type="m" corresp="#s4:l7">dien</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Genadelick <rhyme label="d" type="m" corresp="#s4:l7">aensien</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Est dat heden // <rhyme label="a" type="f">mijn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Versouck, u eens <rhyme label="b" type="m">behaeght</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Geen voorleden // <rhyme label="a" type="f" corresp="#s5:l1">pijn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En wert van my <rhyme label="b" type="m" corresp="#s5:l2">beclaeght</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Noyt heb ick <rhyme label="c" type="m">schat</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Soo lief <rhyme label="c" type="m" corresp="#s5:l5">gehadt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Als u reyn vat // ghy <rhyme label="d" type="m">zijt</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Van God (tot aller <rhyme label="d" type="m" corresp="#s5:l7">tijt</rhyme>)</l>
        <l id="s5:l9" met="3" type="-+">Meer dan <rhyme label="d" type="m" corresp="#s5:l7">gebenedijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Neemt (Princesse // <rhyme label="a" type="f">vry</rhyme>)</l>
        <l id="s6:l2" met="3" type="-+">Dit liedt, in u <rhyme label="b" type="m">gedult</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Mijn Meestresse // <rhyme label="a" type="f" corresp="#s6:l1">ghy</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Voort altijt wesen <rhyme label="b" type="m" corresp="#s6:l2">sult</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Des herten <rhyme label="c" type="m">gront</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Getuyght den <rhyme label="c" type="m" corresp="#s6:l5">mont</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Waer by ghy cont // <rhyme label="d" type="m">verstaen</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Dat dit simpel <rhyme label="d" type="m" corresp="#s6:l7">vermaen</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Wt liefde is <rhyme label="d" type="m" corresp="#s6:l7">gedaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
