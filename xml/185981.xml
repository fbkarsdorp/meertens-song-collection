    <melody>Zeldzaam is de zoete min</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ZEldzaam is de zoete <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die betovering van <rhyme label="a" type="m" corresp="#s1:l1">zin</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Die my haat moet ik <rhyme label="b" type="f">beminnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">kan noch wil, die drift <rhyme label="b" type="f" corresp="#s1:l3">verwinnen</rhyme>;</l>
        <l id="s1:l5" met="2" type="+-">En ik <rhyme label="c" type="m">weet</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">tot myn <rhyme label="c" type="m" corresp="#s1:l5">leet</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dat zy my niet doet dan <rhyme label="d" type="f">plaagen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Nochtans kan zy zy my <rhyme label="d" type="f" corresp="#s1:l7">behaagen</rhyme>.</l>
      </lg>
    </lg>
  </text>
