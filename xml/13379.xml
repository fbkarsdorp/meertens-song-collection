<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13379</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
    </tags>
    <melody>Belle Iris</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">BRalt nu uyt mijn helle <rhyme label="a" type="m">keel</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dat het klinckt in yeders <rhyme label="b" type="f">ooren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Om Schellingers lof te <rhyme label="b" type="f" corresp="#s1:l2">hooren</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Wantdie Helt die is te <rhyme label="a" type="m" corresp="#s1:l1">eel</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat men hem geen eer sou <rhyme label="c" type="f">dichten</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En uyt galmen met <rhyme label="d" type="m">geluyt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die een yders oor <rhyme label="c" type="f" corresp="#s1:l5">verlichten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Van het Noorden tot het <rhyme label="d" type="m" corresp="#s1:l6">Zuyd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Komt dan ras ghy snelle <rhyme label="a" type="m">faem</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Doet sijn lof in 't Oosten <rhyme label="b" type="f">draagen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Keer dan weder met u <rhyme label="b" type="f" corresp="#s2:l2">Waagen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Naer het Westen toe <rhyme label="a" type="m" corresp="#s2:l1">bequaem</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Om dees Helt sijn roem te <rhyme label="c" type="f">geven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Met der haest de Werelt <rhyme label="d" type="m">door</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">'k Wensch dat hy noch langh mag <rhyme label="c" type="f" corresp="#s2:l5">leven</rhyme>:</l>
        <l id="s2:l8" met="4" type="+-">Want hy wijst ons 't rechte <rhyme label="d" type="m" corresp="#s2:l6">spoor</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Door den hollen <rhyme label="a" type="m">Oceaen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En de wilde woeste <rhyme label="b" type="f">baaren</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Leert ghy de Stuurluyden <rhyme label="b" type="f" corresp="#s3:l2">vaaren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Datse zeylen <rhyme label="a" type="m" corresp="#s3:l1">onbelaen</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Om door Klippen ende <rhyme label="c" type="f">Stranden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Niet in haest te zijn <rhyme label="d" type="m">verrast</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Maer op 't spoedighst te <rhyme label="c" type="f" corresp="#s3:l5">belanden</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Daer het haer te reyse <rhyme label="d" type="m" corresp="#s3:l6">past</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die dan eens u Boeck door <rhyme label="a" type="m">leest</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Vint veel stof om u te <rhyme label="b" type="f">prijsen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want de wercken 't vonnis <rhyme label="b" type="f" corresp="#s4:l2">wijsen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Datter noyt voor u <rhyme label="a" type="m" corresp="#s4:l1">geweest</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Noch oyt komt, die een <rhyme label="c" type="f">bequamer</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Schip sal maaken, na de <rhyme label="d" type="m">maet</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">'t Welck de Zeevaert <rhyme label="c" type="f" corresp="#s4:l5">aengenamer</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Uyt druckt, in den hooghsten <rhyme label="d" type="m" corresp="#s4:l6">graet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wy kunnen Schellinger <rhyme label="a" type="m">fris</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Na sijn waerde niet <rhyme label="b" type="f">waerdeeren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Noch hem na sijn daet niet <rhyme label="b" type="f" corresp="#s5:l2">eeren</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Ja al quammer noch een <rhyme label="a" type="m" corresp="#s5:l1">tris</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Die hen al tot Dichten <rhyme label="c" type="f">stelde</rhyme>.</l>
        <l id="s5:l6" met="4" type="+-">Daerom segh ick tot <rhyme label="d" type="m">besluyt</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Het sou by dit Boeck niet <rhyme label="c" type="f" corresp="#s5:l5">gelden</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Daerom ick het Dichten <rhyme label="d" type="m" corresp="#s5:l6">stuyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
