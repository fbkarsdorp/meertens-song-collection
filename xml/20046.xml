  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">JOnghmans ‘tza, Toont eens dra, Alle jou <rhyme label="a" type="f">kunsjes</rhyme>,</l>
        <l id="s1:l2" met="6" type="+-">Maeck ruymbaen, Vangt dan aen, ’t Waer ander <rhyme label="a" type="f" corresp="#s1:l1">vunsjes</rhyme></l>
        <l id="s1:l3" met="6" type="+-">Datmen u, nu sou sien, dutten, en <rhyme label="b" type="f">druylen</rhyme>.</l>
        <l id="s1:l4" met="5" type="+-">Als een deel lompige weet-niets, <rhyme label="b" type="f" corresp="#s1:l3">guylen</rhyme>.</l>
      </lg>
    </lg>
  </text>
