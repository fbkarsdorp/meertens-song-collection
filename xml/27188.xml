  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Achtbare maecht seer hooch <rhyme label="a" type="f">verheuen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Ghy die syt myn troost <rhyme label="b" type="m">alleyn</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Voor v woude ick eynden myn <rhyme label="a" type="f" corresp="#s1:l1">leuen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Als pyramus dee <rhyme label="b" type="m" corresp="#s1:l2">certeyn</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Om Tisbe verstaet wel <rhyme label="c" type="m">dat</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Doen hy meenden alsoo <rhyme label="c" type="m" corresp="#s1:l5">radt</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Dat sy <rhyme label="d" type="m">was</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Op dat <rhyme label="d" type="m" corresp="#s1:l7">pas</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Daer <rhyme label="e" type="f">verslonden</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Van die leuwin <rhyme label="f" type="m">wredelyck</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ghelyck Eneas met <rhyme label="a" type="f">beswarren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Most Troyen syn <rhyme label="b" type="m">latende</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Disghelycks sal nu myn <rhyme label="a" type="f" corresp="#s2:l1">varren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van v myn syn <rhyme label="b" type="m" corresp="#s2:l2">hatende</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Want op v nu staet <rhyme label="c" type="m">certeyn</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Mynen hoop o eedel <rhyme label="c" type="m" corresp="#s2:l5">greyn</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Want om <rhyme label="d" type="m">v</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Moet ick <rhyme label="d" type="m" corresp="#s2:l7">nu</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Myn <rhyme label="e" type="f">beswaren</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Vadt die reden nu <rhyme label="f" type="m">alleyn</rhyme></l>
      </lg>
    </lg>
  </text>
