    <melody>Ik dank God u almachtig</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O Syon delicaet // <rhyme label="a" type="m">zaet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Doet aen al u <rhyme label="a" type="m" corresp="#s1:l1">chieraet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die hier in smenschen haet // <rhyme label="a" type="m" corresp="#s1:l1">staet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Godt is u <rhyme label="a" type="m" corresp="#s1:l1">toeverlaet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dus slaet ten Hemelwaert <rhyme label="b" type="f">eendrachtich</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Crachtich, u ooghen <rhyme label="c" type="m">al</rhyme>.</l>
        <l id="s1:l7" met="3" type="+-">Want hem seer haest vertooghen <rhyme label="c" type="m" corresp="#s1:l6">sal</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Uwen Bruydegom <rhyme label="b" type="f" corresp="#s1:l5">Almachtich</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Neerstich weest hem <rhyme label="b" type="f" corresp="#s1:l5">verwachtich</rhyme>:</l>
        <l id="s1:l10" met="3" type="+-">Voordachtich, bidt en <rhyme label="d" type="m">waeckt</rhyme>,</l>
        <l id="s1:l11" met="3" type="-+">Opdat ghy niet <rhyme label="b" type="f" corresp="#s1:l5">onsachtich</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">Klachtich/ Bevonden <rhyme label="d" type="m" corresp="#s1:l10">naeckt</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">En smaeckt, tot uwer <rhyme label="e" type="f">schanden</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">Den kelck die vierich <rhyme label="d" type="m" corresp="#s1:l10">blaeckt</rhyme>,</l>
        <l id="s1:l15" met="3" type="-+">So maeckt, als vol <rhyme label="e" type="f" corresp="#s1:l13">verstanden</rhyme>,</l>
        <l id="s1:l16" met="3" type="-+">Dat u Lampe blijft <rhyme label="e" type="f" corresp="#s1:l13">branden</rhyme>,</l>
        <l id="s1:l17" met="4" type="-+">En wijckt niet vanden ploeghe <rhyme label="f" type="m">snel</rhyme>,</l>
        <l id="s1:l18" met="3" type="-+">Want siet door traghe <rhyme label="e" type="f" corresp="#s1:l13">handen</rhyme></l>
        <l id="s1:l19" met="3" type="-+">Vallen balcken en <rhyme label="e" type="f" corresp="#s1:l13">wanden</rhyme>,</l>
        <l id="s1:l20" met="5" type="+-">Doet offeranden die Godt ghenoeghen <rhyme label="f" type="m" corresp="#s1:l17">wel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Niet al te seer sucht // <rhyme label="a" type="m">ducht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Niet voor eenich <rhyme label="a" type="m" corresp="#s2:l1">gherucht</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als men hooch in de lucht // <rhyme label="a" type="m" corresp="#s2:l1">vlucht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al uyt dit aertsch <rhyme label="a" type="m" corresp="#s2:l1">ghehucht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Laet vrucht, aen u doch <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Al in den Somer <rhyme label="c" type="m">tijt</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Van vroom noch vromer <rhyme label="c" type="m" corresp="#s2:l6">sijt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Waeyt niet met alle <rhyme label="b" type="f" corresp="#s2:l5">winden</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Snel sal uwen <rhyme label="b" type="f" corresp="#s2:l5">versinden</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Doen binden t' oncruyt <rhyme label="d" type="m">snoot</rhyme>,</l>
        <l id="s2:l11" met="3" type="-+">Dat sal het vyer <rhyme label="b" type="f" corresp="#s2:l5">verslinden</rhyme>,</l>
        <l id="s2:l12" met="3" type="+-">Niet min den Acker <rhyme label="d" type="m" corresp="#s2:l10">bloot</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Die niet ter noot, <rhyme label="e" type="f">voortbringhet</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Goet cruyt den Bouwer <rhyme label="d" type="m" corresp="#s2:l10">groot</rhyme>,</l>
        <l id="s2:l15" met="3" type="-+">Ter doot, werdt hy <rhyme label="e" type="f" corresp="#s2:l13">ghesinghet</rhyme>,</l>
        <l id="s2:l16" met="3" type="-+">Hierom niet en <rhyme label="e" type="f" corresp="#s2:l13">ghehinghet</rhyme>,</l>
        <l id="s2:l17" met="4" type="-+">Dat ghy u minghet, met t' ghemeyne <rhyme label="f" type="m">cruyt</rhyme>,</l>
        <l id="s2:l18" met="3" type="-+">Oft ghy daer me <rhyme label="e" type="f" corresp="#s2:l13">verginghet</rhyme>,</l>
        <l id="s2:l19" met="3" type="-+">Wie quaet is van u <rhyme label="e" type="f" corresp="#s2:l13">dwinghet</rhyme>,</l>
        <l id="s2:l20" met="5" type="+-">Vruchtbaer ontspringhet, Als reyne <rhyme label="f" type="m" corresp="#s2:l17">spruyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Een weynich heve zwaer // <rhyme label="a" type="m">maer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Versuert dat deech <rhyme label="a" type="m" corresp="#s3:l1">algaer</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Suyvert reyn uyt eenpaer // <rhyme label="a" type="m" corresp="#s3:l1">claer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat suere deech wel <rhyme label="a" type="m" corresp="#s3:l1">naer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Neemt waer, d' onreyne <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Die heden, souden <rhyme label="c" type="m">coen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">U in den Gheest vercouden <rhyme label="c" type="m" corresp="#s3:l6">doen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Weert die van u <rhyme label="b" type="f" corresp="#s3:l5">gesneden</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Wilt in u reyne <rhyme label="b" type="f" corresp="#s3:l5">zeden</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Voort treden Maghet <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s3:l11" met="3" type="-+">Uwen roep hier <rhyme label="b" type="f" corresp="#s3:l5">beneden</rhyme>,</l>
        <l id="s3:l12" met="3" type="+-">Is in vreden <rhyme label="d" type="m" corresp="#s3:l10">ydoon</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">U Croon, claer <rhyme label="e" type="f">uytghelesen</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">Wilt houden t' uwen <rhyme label="d" type="m" corresp="#s3:l10">loon</rhyme>,</l>
        <l id="s3:l15" met="3" type="-+">Persoon, aensien <rhyme label="e" type="f" corresp="#s3:l13">mispresen</rhyme>,</l>
        <l id="s3:l16" met="3" type="-+">Werpt wech van u <rhyme label="e" type="f" corresp="#s3:l13">verknesen</rhyme>,</l>
        <l id="s3:l17" met="4" type="-+">Maeckt u in desen, svleesch jonste <rhyme label="f" type="m">vry</rhyme>:</l>
        <l id="s3:l18" met="3" type="-+">Na dien ghy sijt <rhyme label="e" type="f" corresp="#s3:l13">ghenesen</rhyme>,</l>
        <l id="s3:l19" met="3" type="-+">En van der doot <rhyme label="e" type="f" corresp="#s3:l13">verresen</rhyme>,</l>
        <l id="s3:l20" met="5" type="+-">In 't Hemelsche wesen, u wonste <rhyme label="f" type="m" corresp="#s3:l17">sy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Princesse excellent // <rhyme label="a" type="m">jent</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wijckt niet van t' <rhyme label="a" type="m" corresp="#s4:l1">Fondament</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Nae t' vleesch niemant als blent // <rhyme label="a" type="m" corresp="#s4:l1">kent</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Wacht u voor 't oudt <rhyme label="a" type="m" corresp="#s4:l1">Serpent</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Het sendt, Wolven voor <rhyme label="b" type="f">Knapen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">In Schapen, cleeders <rhyme label="c" type="m">loos</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Valsche verleyders <rhyme label="c" type="m" corresp="#s4:l6">boos</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die om verscheuren <rhyme label="b" type="f" corresp="#s4:l5">gapen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Hierom en wilt niet <rhyme label="b" type="f" corresp="#s4:l5">slapen</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Maer rapen, eenen <rhyme label="d" type="m">moet</rhyme>,</l>
        <l id="s4:l11" met="3" type="-+">En laet u niet <rhyme label="b" type="f" corresp="#s4:l5">betrapen</rhyme>,</l>
        <l id="s4:l12" met="3" type="+-">Sonder wapen, <rhyme label="d" type="m" corresp="#s4:l10">onvroet</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Maer om een goet <rhyme label="e" type="f">verweeren</rhyme>,</l>
        <l id="s4:l14" met="3" type="-+">Dat Harnas Godts <rhyme label="d" type="m" corresp="#s4:l10">aendoet</rhyme>,</l>
        <l id="s4:l15" met="3" type="-+">T' ghebroet boos in 't <rhyme label="e" type="f" corresp="#s4:l13">gheneeren</rhyme>,</l>
        <l id="s4:l16" met="3" type="-+">Leeuwen, Wolven noch <rhyme label="e" type="f" corresp="#s4:l13">Beeren</rhyme>,</l>
        <l id="s4:l17" met="4" type="-+">Schaden noch deeren, en sullen <rhyme label="f" type="m">dan</rhyme>,</l>
        <l id="s4:l18" met="3" type="-+">Doet ghy na Godts <rhyme label="e" type="f" corresp="#s4:l13">begheeren</rhyme>,</l>
        <l id="s4:l19" met="3" type="-+">Beveelt hem u <rhyme label="e" type="f" corresp="#s4:l13">affeeren</rhyme>,</l>
        <l id="s4:l20" met="5" type="+-">Die 't al verteeren, en vervullen <rhyme label="f" type="m" corresp="#s4:l17">can</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
