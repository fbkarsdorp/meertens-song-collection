<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160061</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik zucht ik klaag ik ween</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HY, die de Scepter <rhyme label="a" type="m">swaeydt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In Babelse <rhyme label="b" type="f">Paleysen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Van welckers pracht staegh Fama <rhyme label="a" type="m" corresp="#s1:l1">kraydt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Waer oyt een Mensch kon <rhyme label="b" type="f" corresp="#s1:l2">reysen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die worde door Godts <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In Beest'lijckheydt <rhyme label="b" type="f">verschapen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Soo dat hy at het Gras van 't <rhyme label="a" type="m" corresp="#s2:l1">Landt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En in de dauw lagh t' <rhyme label="b" type="f" corresp="#s2:l2">slapen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sijn ruyghe Hayr hingh <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En sleepte langhs de <rhyme label="b" type="f">gronden</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Sijn Nag'len wiessen, als of <rhyme label="a" type="m" corresp="#s3:l1">zy</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Aen Arents voeten <rhyme label="b" type="f" corresp="#s3:l2">stonden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hier blijft mijn weet-lust <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En siet nu in dit <rhyme label="b" type="f">wonder</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De stant van Aerdtsche Menschen <rhyme label="a" type="m" corresp="#s4:l1">aen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En al haer doen <rhyme label="b" type="f" corresp="#s4:l2">besonder</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Seght ghy: het gras is <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">De Spijs der <rhyme label="b" type="f">Wereldt-linghen</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Wat hy nochtans met lust <rhyme label="a" type="m" corresp="#s5:l1">gheniet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat sijn maer Aerdtsche <rhyme label="b" type="f" corresp="#s5:l2">dinghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al leydt hy niet, en <rhyme label="a" type="m">rust</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Des Nachts op nare <rhyme label="b" type="f">velden</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">'t Is nochtans in des Wereldts <rhyme label="a" type="m" corresp="#s6:l1">lust</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Daer hy sijn rust oyt <rhyme label="b" type="f" corresp="#s6:l2">stelde</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Draeght hy gheen ruyghe <rhyme label="a" type="m">vacht</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hy draeght wel ruyghe <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En woelt om Rijckdom, Eer, en <rhyme label="a" type="m" corresp="#s7:l1">Pracht</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Het Doel-wit van sijn <rhyme label="b" type="f" corresp="#s7:l2">Minne</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Of hy gheen klauwen <rhyme label="a" type="m">draeght</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Hy is nochtans te <rhyme label="b" type="f">vreesen</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Door Roof-lust die sijn hert op <rhyme label="a" type="m" corresp="#s8:l1">knaeght</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Verdruckt hy Weeuw, en <rhyme label="b" type="f" corresp="#s8:l2">Weesen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O Aerdtschen, Beesten <rhyme label="a" type="m">Aerdt</rhyme>!</l>
        <l id="s9:l2" met="3" type="-+">Ontwaeckt uyt Aerdtsche <rhyme label="b" type="f">Droomen</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">En tracht dat ghy een Schat <rhyme label="a" type="m" corresp="#s9:l1">vergaert</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Daer Mot, noch Dief kan <rhyme label="b" type="f" corresp="#s9:l2">komen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
