      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag>shepherd's song</tag>
      <tag>dialog song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EEuwigh groote Godt <rhyme label="a" type="f">Almachtigh</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Eeuwigh al-voort-brengend' <rhyme label="b" type="m">Woordt</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Noyt van yemandts oor dus <rhyme label="a" type="f" corresp="#s1:l1">klachtigh</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Bitter schreyende <rhyme label="b" type="m" corresp="#s1:l2">ghehoort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Noyt van yemandt dus <rhyme label="a" type="f">ghevonden</rhyme></l>
        <l id="s2:l2" met="4" type="+-">In een open koude <rhyme label="b" type="m">schuur</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Noyt van yemandt dus <rhyme label="a" type="f" corresp="#s2:l1">ghewonden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">In een stuck versleten <rhyme label="b" type="m" corresp="#s2:l2">huur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Als van uwe teere <rhyme label="a" type="f">moeder</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Die met ons hier voor u <rhyme label="b" type="m">leydt</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">En kust u, als kindt, en <rhyme label="a" type="f" corresp="#s3:l1">broeder</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En als Godt van <rhyme label="b" type="m" corresp="#s3:l2">ewicheydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Moyses heeft u eens <rhyme label="a" type="f">ghesproken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Op den Berrigh <rhyme label="b" type="m">Sinaij</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Maer hem heeft dees troost <rhyme label="a" type="f" corresp="#s4:l1">ontbroocken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Menscheyt was u noch niet <rhyme label="b" type="m" corresp="#s4:l2">by</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Noyt sijt ghy dan dus <rhyme label="a" type="f">bekeecken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">In een aerdtsche naeckte <rhyme label="b" type="m">schijn</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als van ons die vol <rhyme label="a" type="f" corresp="#s5:l1">ghebreecken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ende slechte Herders <rhyme label="b" type="m" corresp="#s5:l2">sijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Heer wat sullen wy u <rhyme label="a" type="f">gheven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Voor die eer, die ghy ons <rhyme label="b" type="m">doet</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Dan den tijdt die wy noch <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">V staech dancken met <rhyme label="b" type="m" corresp="#s6:l2">ootmoedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Soo als ick dit Lam <rhyme label="a" type="f">op-offer</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Aen u kleyn en grooten <rhyme label="b" type="m">Godt</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Schenck ick oock mijn ziele <rhyme label="a" type="f" corresp="#s7:l1">coffer</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Hanght daer aen, u wille <rhyme label="b" type="m" corresp="#s7:l2">slot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Op dat wy in <rhyme label="a" type="f">eeuwichheden</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Als dit leven is <rhyme label="b" type="m">ghedaen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">V gesternden zael <rhyme label="a" type="f" corresp="#s8:l1">betreden</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Daer de uren niet en <rhyme label="b" type="m" corresp="#s8:l2">slaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
