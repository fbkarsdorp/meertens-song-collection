  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SAns cesse les <rhyme label="a" type="m">Zephirs</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sont errants dans nos <rhyme label="b" type="m">bois</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Il n'aime pas <rhyme label="c" type="m">longtems</rhyme></l>
        <l id="s1:l4" met="3" type="-+">La rose la plus <rhyme label="d" type="f">belle</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">L'onde suit le <rhyme label="c" type="m" corresp="#s1:l3">penchant</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Qu'elle a pris une <rhyme label="b" type="m" corresp="#s1:l2">fois</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Sans chercher chaque <rhyme label="e" type="m">jour</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Une route <rhyme label="d" type="f" corresp="#s1:l4">nouvelle</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">Nous imitons <rhyme label="f" type="m">Bergers</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Vous les Zephirs <rhyme label="f" type="m" corresp="#s1:l9">legers</rhyme>;</l>
        <l id="s1:l11" met="3" type="+-">Moi, le ruisseau <rhyme label="d" type="f" corresp="#s1:l4">fidelle</rhyme>.</l>
      </lg>
    </lg>
  </text>
