<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25081</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Alleen mijn troost mijn toeverlaat</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Alder werelt <rhyme label="a" type="m">heylant</rhyme>.</l>
        <l id="s1:l2" met="2" type="-+">
          <rhyme label="b" type="m">Kyrieleison</rhyme>
        </l>
        <l id="s1:l3" met="3" type="-+">heeft ons sinen bode <rhyme label="a" type="m" corresp="#s1:l1">gesant</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die bode hi was een <rhyme label="a" type="m">yngel</rhyme>.</l>
        <l id="s2:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Geheeten was hi <rhyme label="a" type="m" corresp="#s2:l1">gabriel</rhyme>.</l>
        <l id="s2:l4" met="2" type="-+">
          <rhyme label="c" type="m">Alleluia</rhyme>
        </l>
        <l id="s2:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hi quam tonser lieuer <rhyme label="a" type="m">vrouwen</rhyme>.</l>
        <l id="s3:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Men machse gerne <rhyme label="a" type="m" corresp="#s3:l1">aenscouwen</rhyme>.</l>
        <l id="s3:l4" met="2" type="-+">
          <rhyme label="c" type="m">Alleluia</rhyme>
        </l>
        <l id="s3:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hi quam daer hise besloten <rhyme label="a" type="m">vant</rhyme>.</l>
        <l id="s4:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Hi versceen haer alte <rhyme label="a" type="m" corresp="#s4:l1">hant</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die maget haer seere <rhyme label="a" type="m">ontsach</rhyme>.</l>
        <l id="s5:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Doen si den ingel bi haer <rhyme label="a" type="m" corresp="#s5:l1">sach</rhyme>.</l>
        <l id="s5:l4" met="2" type="-+">
          <rhyme label="c" type="m">Alleluia</rhyme>
        </l>
        <l id="s5:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gegruet bestu maget <rhyme label="a" type="m">fijn</rhyme>.</l>
        <l id="s6:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Du sult baren een <rhyme label="a" type="m" corresp="#s6:l1">kindekijn</rhyme>.</l>
        <l id="s6:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dies nam si doen groot wonder <rhyme label="a" type="m">an</rhyme>.</l>
        <l id="s7:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Want sine kinde genen <rhyme label="a" type="m" corresp="#s7:l1">man</rhyme>.</l>
        <l id="s7:l4" met="2" type="-+">
          <rhyme label="c" type="m">Alleluia</rhyme>
        </l>
        <l id="s7:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dat moet een groot wonder <rhyme label="a" type="m">sijn</rhyme>.</l>
        <l id="s8:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">God heeft die reynichede in <rhyme label="a" type="m" corresp="#s8:l1">my</rhyme>.</l>
        <l id="s8:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Doen sprac die ingel daer <rhyme label="a" type="m">na</rhyme>.</l>
        <l id="s9:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s9:l3" met="3" type="-+">Aue plena <rhyme label="a" type="m" corresp="#s9:l1">gracia</rhyme>.</l>
        <l id="s9:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Nv saltu di houden <rhyme label="a" type="m">wel</rhyme>.</l>
        <l id="s10:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">Met v blijft genade ende <rhyme label="a" type="m" corresp="#s10:l1">spel</rhyme>.</l>
        <l id="s10:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Na dine woerden moet my <rhyme label="a" type="m">gescien</rhyme>.</l>
        <l id="s11:l2" met="2" type="-+">
          <rhyme label="b" type="m">Kyrieleison</rhyme>
        </l>
        <l id="s11:l3" met="3" type="-+">Heuet god ane my <rhyme label="a" type="m" corresp="#s11:l1">versien</rhyme>.</l>
        <l id="s11:l4" met="2" type="-+">
          <rhyme label="c" type="m">Alleluia</rhyme>
        </l>
        <l id="s11:l5" met="3" type="-+">God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Gods deerne willic gerne <rhyme label="a" type="m">sijn</rhyme>.</l>
        <l id="s12:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s12:l3" met="3" type="-+">Gescie my na den woerden <rhyme label="a" type="m" corresp="#s12:l1">dijn</rhyme>.</l>
        <l id="s12:l4" met="2" type="-+">
          <rhyme label="c" type="m">Alleluia</rhyme>
        </l>
        <l id="s12:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Si knielde neder op eenen <rhyme label="a" type="m">steen</rhyme>.</l>
        <l id="s13:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s13:l3" met="3" type="-+">Hare gebet was suuer ende <rhyme label="a" type="m" corresp="#s13:l1">reen</rhyme>.</l>
        <l id="s13:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s13:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Si ginc te hare <rhyme label="a" type="m">stedelijn</rhyme>.</l>
        <l id="s14:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s14:l3" met="3" type="-+">Ihesus iij. namen soete ende <rhyme label="a" type="m" corresp="#s14:l1">fijn</rhyme>.</l>
        <l id="s14:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">o God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Si droegen machmen <rhyme label="a" type="m">spreken</rhyme>.</l>
        <l id="s15:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s15:l3" met="3" type="-+">lxxxix. <rhyme label="a" type="m" corresp="#s15:l1">Weken</rhyme>.</l>
        <l id="s15:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s15:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Si droegene te hare <rhyme label="a" type="m">herte</rhyme>.</l>
        <l id="s16:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">Al sonder eenege <rhyme label="a" type="m" corresp="#s16:l1">smerte</rhyme>.</l>
        <l id="s16:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s16:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Si droegene onder hare <rhyme label="a" type="m">gere</rhyme>.</l>
        <l id="s17:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>. :</l>
        <l id="s17:l3" met="3" type="-+">God alder weerelt <rhyme label="a" type="m" corresp="#s17:l1">here</rhyme>.</l>
        <l id="s17:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s17:l5" met="3" type="-+">God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Dat was wel een salich <rhyme label="a" type="m">nacht</rhyme>.</l>
        <l id="s18:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s18:l3" met="3" type="-+">Dat geboren was die <rhyme label="a" type="m" corresp="#s18:l1">dracht</rhyme>.</l>
        <l id="s18:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s18:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Doen si des kindekijns <rhyme label="a" type="m">genas</rhyme>.</l>
        <l id="s19:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s19:l3" met="3" type="-+">Maget bleef si alsi <rhyme label="a" type="m" corresp="#s19:l1">was</rhyme>.</l>
        <l id="s19:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s19:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Si want in haer <rhyme label="a" type="m">doekelkijn</rhyme>.</l>
        <l id="s20:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s20:l3" met="3" type="-+">Ihesus dat soete <rhyme label="a" type="m" corresp="#s20:l1">kindekijn</rhyme>.</l>
        <l id="s20:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s20:l5" met="3" type="-+">God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Sijn wiege was een <rhyme label="a" type="m">crebbekijn</rhyme>.</l>
        <l id="s21:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s21:l3" met="3" type="-+">Hine woude geen beter <rhyme label="a" type="m" corresp="#s21:l1">hebben</rhyme>.</l>
        <l id="s21:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s21:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Si soechden met haren <rhyme label="a" type="m">borsten</rhyme>.</l>
        <l id="s22:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s22:l3" met="3" type="-+">God alder weerelt <rhyme label="a" type="m" corresp="#s22:l1">voerste</rhyme>.</l>
        <l id="s22:l4" met="2" type="-+">
          <rhyme label="c" type="m">Alleluia</rhyme>
        </l>
        <l id="s22:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Si sette tkint op haren <rhyme label="a" type="m">scoet</rhyme>.</l>
        <l id="s23:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s23:l3" met="3" type="-+">Die ons sondaren halp vter <rhyme label="a" type="m" corresp="#s23:l1">noet</rhyme>.</l>
        <l id="s23:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s23:l5" met="3" type="-+">God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Wie sal dat kint behoeden <rhyme label="a" type="m">dan</rhyme>.</l>
        <l id="s24:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s24:l3" met="3" type="-+">Joseph die goede gerechte <rhyme label="a" type="m" corresp="#s24:l1">man</rhyme>.</l>
        <l id="s24:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s24:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Wie sal des kindekens pleger <rhyme label="a" type="m">sijn</rhyme>.</l>
        <l id="s25:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s25:l3" met="3" type="-+">Dat sal maria die maget <rhyme label="a" type="m" corresp="#s25:l1">sijn</rhyme>.</l>
        <l id="s25:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s25:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Wie sal doepen dat <rhyme label="a" type="m">kindekijn</rhyme>.</l>
        <l id="s26:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s26:l3" met="3" type="-+">Dien sal des conincs rike <rhyme label="a" type="m" corresp="#s26:l1">sijn</rhyme>.</l>
        <l id="s26:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s26:l5" met="3" type="-+">God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="3" type="-+">Dat sal sijn baptiste sint <rhyme label="a" type="m">ian</rhyme>.</l>
        <l id="s27:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s27:l3" met="3" type="-+">Al in die fluine van <rhyme label="a" type="m" corresp="#s27:l1">iordan</rhyme>.</l>
        <l id="s27:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s27:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="3" type="-+">Het scheen een lichte <rhyme label="a" type="m">sterre</rhyme>.</l>
        <l id="s28:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s28:l3" met="3" type="-+">In orienten herde <rhyme label="a" type="m" corresp="#s28:l1">verre</rhyme>.</l>
        <l id="s28:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s28:l5" met="3" type="-+">Geloeft sidi <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="3" type="-+">Si wijsde die coningen op die <rhyme label="a" type="m">vaert</rhyme>.</l>
        <l id="s29:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s29:l3" met="3" type="-+">Doen dat kint geboren <rhyme label="a" type="m" corresp="#s29:l1">waert</rhyme>.</l>
        <l id="s29:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s29:l5" met="3" type="-+">God gruete v <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="3" type="-+">Si offerden hem een rike <rhyme label="a" type="m">scout</rhyme>.</l>
        <l id="s30:l2" met="2" type="-+"><rhyme label="b" type="m">Kyrieleison</rhyme>.</l>
        <l id="s30:l3" met="3" type="-+">Wieroec mirre ende <rhyme label="a" type="m" corresp="#s30:l1">gout</rhyme>.</l>
        <l id="s30:l4" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
        <l id="s30:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="d" type="m">maria</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
