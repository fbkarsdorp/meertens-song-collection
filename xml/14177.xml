    <melody>Het nachtegaaltje klein</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk verheuge my <rhyme label="a" type="f">seere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat wy sullen <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Int huys Gods <rhyme label="c" type="m">excelent</rhyme>,,</l>
        <l id="s1:l4" met="3" type="-+">Ende dat ons weten <rhyme label="a" type="f" corresp="#s1:l1">teere</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">In die Poorten sullen <rhyme label="b" type="m" corresp="#s1:l2">staen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van Jerusalem <rhyme label="c" type="m" corresp="#s1:l3">bekent</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Jerusalem is <rhyme label="c" type="m" corresp="#s1:l3">present</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Een Stadt ghebouwet <rhyme label="d" type="f">reyne</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Daer men te samen <rhyme label="e" type="m">compt</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Daer de stammen opgaen <rhyme label="d" type="f" corresp="#s1:l8">gemeyne</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Israels Godts stam <rhyme label="e" type="m" corresp="#s1:l9">ghenoempt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Om te predicken den <rhyme label="a" type="f">volcke</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van Israel zijn <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">End' dancken s'Heeren <rhyme label="c" type="m">naem</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Want aldaer sitten om <rhyme label="a" type="f" corresp="#s2:l1">wolcken</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">De sterren een gherechter <rhyme label="b" type="m" corresp="#s2:l2">of</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Stoelen Davids huys <rhyme label="c" type="m" corresp="#s2:l3">bequaem</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wenschet Jerusalem t <rhyme label="c" type="m" corresp="#s2:l3">saem</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Geluck en t'moet <rhyme label="d" type="f">welgane</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Die u beminnen <rhyme label="e" type="m">soet</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Den vreede <rhyme label="d" type="f" corresp="#s2:l8">aengename</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">By binnen u meuren <rhyme label="e" type="m" corresp="#s2:l9">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En gheluck in u <rhyme label="a" type="f">palaysen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Moet altijt by u <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om mijn broeders <rhyme label="c" type="m">excelent</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">End vrienden die u soude <rhyme label="a" type="f" corresp="#s3:l1">weysen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Wensch den vreede <rhyme label="b" type="m" corresp="#s3:l2">bekent</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In Gods huys sonder <rhyme label="c" type="m" corresp="#s3:l3">ent</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Op dat wy wel en <rhyme label="c" type="m" corresp="#s3:l3">jent</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Om des huys wil en <rhyme label="d" type="f">name</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Des Heeren onses Godts <rhyme label="e" type="m">goet</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Sochten het best <rhyme label="d" type="f" corresp="#s3:l8">altesamen</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Tot Jerusalems <rhyme label="e" type="m" corresp="#s3:l9">voorspoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
