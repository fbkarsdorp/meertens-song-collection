<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8890</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Durch Adams Fall ist ganz verderbt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIe Heere sprack in synen <rhyme label="a" type="m">Throon</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tot Christum mynen <rhyme label="b" type="f">HEEre</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghy zijt myn Eengheboren <rhyme label="a" type="m" corresp="#s1:l1">Soon</rhyme></l>
        <l id="s1:l4" met="3" type="-+">T 'behoort dy Godtlijck <rhyme label="b" type="f" corresp="#s1:l2">eere</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Set dy playsant t'myn Rechter <rhyme label="c" type="m">hant</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Bet dat ick dijn <rhyme label="d" type="f">Vyanden</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Legg' metter spoet, onder dijn <rhyme label="c" type="m" corresp="#s1:l5">Voet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Als een Schabell' met <rhyme label="d" type="f" corresp="#s1:l6">schanden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De HEER die sal wt Zyon <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dijns Rijckes Scepter <rhyme label="b" type="f">seynden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dijn Woort sal daer beginnen <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En gaen tot s'werelts <rhyme label="b" type="f" corresp="#s2:l2">Eynden</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Gaet heyrschen over al <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dijn Vyanden <rhyme label="d" type="f">verheven</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Dat sy doch haer van Doots <rhyme label="c" type="m" corresp="#s2:l5">ghevaer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Bekeeren ende <rhyme label="d" type="f" corresp="#s2:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wanneer ghy door dijn lijden <rhyme label="a" type="m">swaer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hebt Sond en Doot <rhyme label="b" type="f">bestreden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sal dijn Volck dy willich al <rhyme label="a" type="m" corresp="#s3:l1">daer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Off'ren in <rhyme label="b" type="f" corresp="#s3:l2">Heylicheden</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Dijn Kind'ren werden dy <rhyme label="c" type="m">Geboor'n</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">(Alleen door dijn <rhyme label="d" type="f">Ghenade</rhyme>)</l>
        <l id="s3:l7" met="4" type="-+">Gelijck den Douw' Coel <rhyme label="c" type="m" corresp="#s3:l5">wtvercoor'n</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Vroech voor den <rhyme label="d" type="f" corresp="#s3:l6">Dagherade</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De HEEr gesworen heeft <rhyme label="a" type="m">Certeyn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">En t'sal hem niet <rhyme label="b" type="f">Berouwen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ghy zijt een Eewich Priester <rhyme label="a" type="m" corresp="#s4:l1">reyn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om GOdes Dienst te <rhyme label="b" type="f" corresp="#s4:l2">bouwen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Nae d'Ordening' <rhyme label="c" type="m">Melchisedech</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die oude <rhyme label="d" type="f">Offerhande</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Moet na dijn Overwinning' <rhyme label="c" type="m" corresp="#s4:l5">wech</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">End heelijcx wt den <rhyme label="d" type="f" corresp="#s4:l6">Lande</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Altijdt sal de HEER by dy <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En staen tot dijner <rhyme label="b" type="f">Rechten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En straffen eewelijck met <rhyme label="a" type="m" corresp="#s5:l1">pijn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Al die dy <rhyme label="b" type="f" corresp="#s5:l2">Wedervechten</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ter tijdt als synen Thoorn gaet <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">So doet de Heer wech <rhyme label="d" type="f">rucken</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Die Coninghen subijt daer <rhyme label="c" type="m" corresp="#s5:l5">van</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En smijtse gantsch in <rhyme label="d" type="f" corresp="#s5:l6">stucken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Met groote Heerlickheyt en <rhyme label="a" type="m">Macht</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Sal hy die Heyden <rhyme label="b" type="f">Richten</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Hy sal doen men'ghen grooten <rhyme label="a" type="m" corresp="#s6:l1">Slacht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Op die, die hem <rhyme label="b" type="f" corresp="#s6:l2">Vernichten</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Hy sal den Prins van deser <rhyme label="c" type="m">Werlt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die hem altijt gaet <rhyme label="d" type="f">steecken</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">En teghen hem soo wreetlijck <rhyme label="c" type="m" corresp="#s6:l5">stelt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Syn Helsche Rijck <rhyme label="d" type="f" corresp="#s6:l6">verbreecken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">In swacheyt, lijden' Cruys en <rhyme label="a" type="m">Doot</rhyme></l>
        <l id="s7:l2" met="3" type="-+">(Al gaet hy tijdtlijck <rhyme label="b" type="f">sterven</rhyme>)</l>
        <l id="s7:l3" met="4" type="-+">Sal hy verwinnen alle <rhyme label="a" type="m" corresp="#s7:l1">Noot</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Al Godes goeden <rhyme label="b" type="f" corresp="#s7:l2">erven</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Ten derden Daegh' Verrijsen <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Een Eewich Coninck <rhyme label="d" type="f">Crachtich</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Die Hemel ende Aerd' <rhyme label="c" type="m" corresp="#s7:l5">regeer</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Een Heyrschend' Heer <rhyme label="d" type="f" corresp="#s7:l6">Almachtich</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer voor wy seggen Loff end <rhyme label="a" type="m">Eer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat wy dien Heylandt <rhyme label="b" type="f">haven</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Loff singhen end verblijdet <rhyme label="a" type="m" corresp="#s8:l1">seer</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hem dancken sulcker <rhyme label="b" type="f" corresp="#s8:l2">gaven</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Want even als hijt ons <rhyme label="c" type="m">bediet</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Hier door syn Woort op <rhyme label="d" type="f">Eerden</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">End ons gheloove twijffelt <rhyme label="c" type="m" corresp="#s8:l5">niet</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Wy Salich sullen <rhyme label="d" type="f" corresp="#s8:l6">weerden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
