      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Godt almachtigh Vader <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghy die zijt hoogh in ‘t <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wy die nu hier vergadert <rhyme label="a" type="m" corresp="#s1:l1">zijn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In uwen naem <rhyme label="b" type="f" corresp="#s1:l2">ghepresen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Vallen te voet, Heer met <rhyme label="c" type="m">ootmoet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Voor u, biet ons u <rhyme label="d" type="f">Ooren</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Wy bidden nu, Heere <rhyme label="e" type="m">ghetrouw</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wilt ons ghebedt <rhyme label="d" type="f" corresp="#s1:l6">verhooren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Heer die alle herten <rhyme label="a" type="m">kent</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ghy weet yders <rhyme label="b" type="f">betrachten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Uwen Gheest goedigh in ons <rhyme label="a" type="m" corresp="#s2:l1">sent</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met alle sijne <rhyme label="b" type="f" corresp="#s2:l2">krachten</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Dat hy ons leyt, en maeckt <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tot uwen dienst <rhyme label="d" type="f">bequame</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Op dese stee, in uwen <rhyme label="e" type="m">vree</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Tot prijs van uwen <rhyme label="d" type="f" corresp="#s2:l6">name</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Opent den mont van uwen <rhyme label="a" type="m">knecht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gheeft hem wijsheyt daer <rhyme label="b" type="f">neven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat hy u woort mach spreken <rhyme label="a" type="m" corresp="#s3:l1">recht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat ghy hem hebt <rhyme label="b" type="f" corresp="#s3:l2">ghegeven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Sterckt hem doch meest, door uwen <rhyme label="c" type="m">Gheest</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat hy ‘t ons mach <rhyme label="d" type="f">voor-draghen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Heel onbevreest, doch <rhyme label="e" type="m">aldermeest</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Nae u goet <rhyme label="d" type="f" corresp="#s3:l6">wel-behaghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat is ons beden in ‘t <rhyme label="a" type="m">ghemeyn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">O Godt, Vader vol <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wilt onse herten maken <rhyme label="a" type="m" corresp="#s4:l1">reyn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat is al ons <rhyme label="b" type="f" corresp="#s4:l2">begheeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Een recht verstant, in ons hert <rhyme label="c" type="m">plant</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wilt al onkruyt <rhyme label="d" type="f">uyt-roeyen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Op dat u woort, van ons <rhyme label="e" type="m">ghehoort</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Mach wassen ende <rhyme label="d" type="f" corresp="#s4:l6">groeyen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Heer ghy die zijt een eeuwigh <rhyme label="a" type="m">licht</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Klaer boven allen <rhyme label="b" type="f">maten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Gheeft onse Ooghen oock <rhyme label="a" type="m" corresp="#s5:l1">ghesicht</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat wy ‘t recht moghen <rhyme label="b" type="f" corresp="#s5:l2">vaten</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">De blintheyt groot, Heer van ons <rhyme label="c" type="m">sloot</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daer in wy voormaels <rhyme label="d" type="f">saten</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dat wy ghewis, de <rhyme label="e" type="m">duysternis</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met haer wercken <rhyme label="d" type="f" corresp="#s5:l6">verlaten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Gheeft ons oock hongher ende <rhyme label="a" type="m">dorst</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Om u woort te <rhyme label="b" type="f">volbringhen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">U willen wy dan stercke <rhyme label="a" type="m" corresp="#s6:l1">Vorst</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Stadighen lof toe <rhyme label="b" type="f" corresp="#s6:l2">singhen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Van herten gront, tot aller <rhyme label="c" type="m">stont</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dat sy u <rhyme label="d" type="f">aen-ghename</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Op dat het kom in ‘t <rhyme label="e" type="m">heylichdom</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Voor u ons Godt <rhyme label="d" type="f" corresp="#s6:l6">eersame</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Lof, eer en prijs, en <rhyme label="a" type="m">danckbaerheyt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Zy onsen Godt hier <rhyme label="b" type="f">bovenL</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Hemelen weest met ons <rhyme label="a" type="m" corresp="#s7:l1">bereyt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En ghy aerd’ om te <rhyme label="b" type="f" corresp="#s7:l2">loven</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Ghy Sterren al, sonder <rhyme label="c" type="m">ghetal</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ghy Son en Maen met <rhyme label="d" type="f">namen</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">En wat ghewis, op Aerden <rhyme label="e" type="m">is</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Looft Godt met vreughden, <rhyme label="d" type="f" corresp="#s7:l6">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
