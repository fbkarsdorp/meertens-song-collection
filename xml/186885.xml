<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186885</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Con dolce forza</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Holland <rhyme label="a" type="m">vry</rhyme>!</l>
        <l id="s1:l2" met="2" type="-+">'k moet u <rhyme label="b" type="f">verkonden</rhyme>;</l>
        <l id="s1:l3" met="2" type="-+">Uw eygen <rhyme label="b" type="f" corresp="#s1:l2">Wonden</rhyme>;</l>
        <l id="s1:l4" met="2" type="-+">Die bewerkt <rhyme label="a" type="m" corresp="#s1:l1">gy</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Gy zyt een <rhyme label="c" type="f">snoode</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Want uw <rhyme label="d" type="f">behaegen</rhyme>!</l>
        <l id="s1:l7" met="2" type="-+">Is om te <rhyme label="d" type="f" corresp="#s1:l6">draegen</rhyme>;</l>
        <l id="s1:l8" met="2" type="-+">Engelsche <rhyme label="c" type="f" corresp="#s1:l5">Moode</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Komt maer een <rhyme label="a" type="m">veer</rhyme>!</l>
        <l id="s2:l2" met="2" type="-+">Uit Eng'land <rhyme label="b" type="f">vliegen</rhyme>!</l>
        <l id="s2:l3" met="2" type="-+">Ge laat u <rhyme label="b" type="f" corresp="#s2:l2">bedriegen</rhyme>!</l>
        <l id="s2:l4" met="2" type="-+">Op eene <rhyme label="a" type="m" corresp="#s2:l1">keer</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Al uw <rhyme label="c" type="f">Fabrieken</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">Moeten <rhyme label="d" type="f">bezwyken</rhyme>!</l>
        <l id="s2:l7" met="2" type="-+">Koopmanschap <rhyme label="d" type="f" corresp="#s2:l6">Wyken</rhyme>.</l>
        <l id="s2:l8" met="2" type="-+">Der <rhyme label="c" type="f" corresp="#s2:l5">Republieken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Laeken, Zey <rhyme label="a" type="m">Stoff</rhyme>!</l>
        <l id="s3:l2" met="2" type="-+">Koussen of <rhyme label="b" type="f">Hoeden</rhyme>!</l>
        <l id="s3:l3" met="2" type="-+">Sitz fijn en <rhyme label="b" type="f" corresp="#s3:l2">groff</rhyme>!</l>
        <l id="s3:l4" met="2" type="-+">Knoopen, geen <rhyme label="a" type="m" corresp="#s3:l1">goeden</rhyme>!</l>
        <l id="s3:l5" met="2" type="-+">Zoo ze niet <rhyme label="c" type="f">koomen</rhyme>!</l>
        <l id="s3:l6" met="2" type="-+">Om Hollands <rhyme label="d" type="f">vinken</rhyme>;</l>
        <l id="s3:l7" met="2" type="-+">Pons te doen <rhyme label="d" type="f" corresp="#s3:l6">drinken</rhyme>!</l>
        <l id="s3:l8" met="2" type="-+">Uit Britze <rhyme label="c" type="f" corresp="#s3:l5">Stroomen</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ã´ Batoos <rhyme label="a" type="m">Land</rhyme>!</l>
        <l id="s4:l2" met="2" type="-+">Moedige <rhyme label="b" type="f">Helden</rhyme>!</l>
        <l id="s4:l3" met="2" type="-+">Voed geen <rhyme label="b" type="f" corresp="#s4:l2">Vyand</rhyme>!</l>
        <l id="s4:l4" met="2" type="-+">Nu hy u <rhyme label="a" type="m" corresp="#s4:l1">kwelden</rhyme>!</l>
        <l id="s4:l5" met="2" type="-+">Engelsche <rhyme label="c" type="f">pleister</rhyme>!</l>
        <l id="s4:l6" met="2" type="-+">Plakt aen uw <rhyme label="d" type="f">zoolen</rhyme>!</l>
        <l id="s4:l7" met="2" type="-+">Schoensmeer <rhyme label="d" type="f" corresp="#s4:l6">bevoolen</rhyme>!</l>
        <l id="s4:l8" met="2" type="-+">t' Is al Spoor <rhyme label="c" type="f" corresp="#s4:l5">beister</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Drink, Hollands <rhyme label="a" type="m">Bier</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Bergs, Jochems, <rhyme label="b" type="f">Vinkel</rhyme>?</l>
        <l id="s5:l3" met="2" type="-+">Want Onze <rhyme label="b" type="f" corresp="#s5:l2">Bannier</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Heeft waer in <rhyme label="a" type="m" corresp="#s5:l1">Winkel</rhyme>!</l>
        <l id="s5:l5" met="2" type="-+">Eygen <rhyme label="c" type="f">belangen</rhyme>!</l>
        <l id="s5:l6" met="2" type="-+">Hollands <rhyme label="d" type="f">welweezen</rhyme>!</l>
        <l id="s5:l7" met="2" type="-+">Zullen <rhyme label="d" type="f" corresp="#s5:l6">volpreezen</rhyme>!</l>
        <l id="s5:l8" met="2" type="-+">Luyster <rhyme label="c" type="f" corresp="#s5:l5">Ontfangen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
