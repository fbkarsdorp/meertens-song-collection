      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Polyphemus aan de stranden</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>anoniem</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SOete maeghden, dartle <rhyme label="a" type="f">dieren</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die graech <rhyme label="a" type="f" corresp="#s1:l1">swieren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Somers in het lieve <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Langhs de groene else <rhyme label="c" type="f">boompjes</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Daer de <rhyme label="c" type="f" corresp="#s1:l4">stroompjes</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Christalijn-beeckjens <rhyme label="b" type="m" corresp="#s1:l3">voen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Daer de hoogh-getopte <rhyme label="a" type="f">eycken</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Schier <rhyme label="a" type="f" corresp="#s2:l1">bereycken</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">‘tBlaeuw-gewulf van ‘s Hemels <rhyme label="b" type="m">zael</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">In wiens schaeuw ghy gaet <rhyme label="c" type="f">vermaeken</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Daer uw <rhyme label="c" type="f" corresp="#s2:l4">kaken</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Strijden teghens roose <rhyme label="b" type="m" corresp="#s2:l3">prael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Daer de beesjes <rhyme label="a" type="f">tierelieren</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">En steets <rhyme label="a" type="f" corresp="#s3:l1">swieren</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Van de eene tack op <rhyme label="b" type="m">d’aer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Schenkt ghy Venus <rhyme label="c" type="f">offerhanden</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">En doet <rhyme label="c" type="f" corresp="#s3:l4">branden</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Vwe lust op ‘t <rhyme label="b" type="m" corresp="#s3:l3">hoogh-altaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Doch geen beesjes sullent <rhyme label="a" type="f">klappen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Soo het <rhyme label="a" type="f" corresp="#s4:l1">snappen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Van die geen, die ghy u <rhyme label="b" type="m">schoot</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Laet gebruyken; selfs kan <rhyme label="c" type="f">swijgen</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Nae ‘t <rhyme label="c" type="f" corresp="#s4:l4">verkrijghen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Van het maeghdelijck <rhyme label="b" type="m" corresp="#s4:l3">genoot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">‘t Schijnt dees eeuwen zijn <rhyme label="a" type="f">bedurven</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s5:l1">versturven</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Want men nu roemt van het <rhyme label="b" type="m">geen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Selfs Natura ons leert <rhyme label="c" type="f">heelen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">‘t Steels-wijs <rhyme label="c" type="f" corresp="#s5:l4">steelen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dat maecktmen nu straet <rhyme label="b" type="m" corresp="#s5:l3">ghemeen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dus ghy maeghden wilt <rhyme label="a" type="f">uytbannen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Die niet <rhyme label="a" type="f" corresp="#s6:l1">kannen</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Snoeren ‘t <rhyme label="b" type="m">losse-tonghe-bladt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Die u soete keurtjes <rhyme label="c" type="f">melden</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">En met <rhyme label="c" type="f" corresp="#s6:l4">schelden</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Draegen door de heele <rhyme label="b" type="m" corresp="#s6:l3">Stadt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
