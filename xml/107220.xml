<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>107220</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Bobbing Joe</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'t SA 't Sa, weest wacker ghy vroolijcke <rhyme label="a" type="m">Luy</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Schroomt niet voor Kannen, en <rhyme label="b" type="f">Glaesen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Sta vast in dese vogtige <rhyme label="a" type="m" corresp="#s1:l1">Buy</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ick moet u al 't saem eens <rhyme label="b" type="f" corresp="#s1:l2">aesen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Eerst met dese <rhyme label="c" type="m">Fluyt</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Heel suyvertjes <rhyme label="c" type="m" corresp="#s1:l5">uyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Tot welkomst van dees <rhyme label="b" type="f" corresp="#s1:l2">Baesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Schencker schenckt het wederom <rhyme label="a" type="m">vol</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Soo mach het dees Drogert <rhyme label="b" type="f">ontfangen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Sijn Buyck is hem soo vreeselijck <rhyme label="a" type="m" corresp="#s2:l1">hol</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ey laet hem niet langer <rhyme label="b" type="f" corresp="#s2:l2">verlangen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Een ander die <rhyme label="c" type="m">lacht</rhyme></l>
        <l id="s2:l6" met="2" type="-+">En haeckt na de <rhyme label="c" type="m" corresp="#s2:l5">slach</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">De Buyck meyndt de Keel is <rhyme label="b" type="f" corresp="#s2:l2">ghehangen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'t Sa volt ons weer een lustige <rhyme label="a" type="m">Kop</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vry overvloedich <rhyme label="b" type="f">geschoncken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Elck toont hem van dit lustige <rhyme label="a" type="m" corresp="#s3:l1">sop</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat hy nu is <rhyme label="b" type="f" corresp="#s3:l2">beschoncken</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Van ons <rhyme label="c" type="m">Vaderlandt</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dus bied my de <rhyme label="c" type="m" corresp="#s3:l5">handt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wy Blussen de Oorlogs <rhyme label="b" type="f" corresp="#s3:l2">voncken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">In sulcken storm verliestmen geen <rhyme label="a" type="m">Hooft</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">'t Sa daer ist lustich te <rhyme label="b" type="f">Vechten</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Men wert wel van sijn sinnen <rhyme label="a" type="m" corresp="#s4:l1">berooft</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maer komen noch weder te <rhyme label="b" type="f" corresp="#s4:l2">rechten</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Al roltmen eens <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Men raeckt op de <rhyme label="c" type="m" corresp="#s4:l5">been</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En blijven noch vrome <rhyme label="b" type="f" corresp="#s4:l2">Krijchs-knechten</rhyme>:</l>
      </lg>
    </lg>
  </text>
</song>
