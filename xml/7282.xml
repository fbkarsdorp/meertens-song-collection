<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>7282</id>
    <date>
      <year-start>1630</year-start>
      <year-end>1640</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Als alle akers rijpen</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als de Eeckelen <rhyme label="a" type="f">rijpen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Soo mest den Boer zijn <rhyme label="b" type="m">Swijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick hebber een wilt <rhyme label="c" type="f">gheschoten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Het heeft my soo langhe <rhyme label="c" type="f" corresp="#s1:l3">verdroten</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">By de liefste en mach ick niet <rhyme label="b" type="m" corresp="#s1:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Het soud’ een <rhyme label="a" type="f">suyverlijcke</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Des avondts gaen om <rhyme label="b" type="m">wijn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">’s Avondt wel alsoo <rhyme label="c" type="f">late</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Den Ruyter quam haer te <rhyme label="c" type="f" corresp="#s2:l3">sprake</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wn hy hietese willekom <rhyme label="b" type="m" corresp="#s2:l2">zijn</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy nam de <rhyme label="a" type="f">suyverlijcke</rhyme></l>
        <l id="s3:l2" met="3" type="-+">By haer sneeu witte <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hy settese al aen zijn <rhyme label="c" type="f">zijde</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wat hy dede zy moester <rhyme label="c" type="f" corresp="#s3:l3">lijden</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Haer eerken en duurde niet <rhyme label="b" type="m" corresp="#s3:l2">langh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maer doen die stoute <rhyme label="a" type="f">Ruyter</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Zijn wille hadde <rhyme label="b" type="m">ghedaen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy seyde wel schoon <rhyme label="c" type="f">Ionckvrouwe</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hadt ghy u eecken <rhyme label="c" type="f" corresp="#s4:l3">behouwen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Gy meucht wel t’huys waert <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Waer sal ick heenen <rhyme label="a" type="f">rijden</rhyme>?</l>
        <l id="s5:l2" met="3" type="-+">Waer sal ick heenen <rhyme label="b" type="m">gaen</rhyme>?</l>
        <l id="s5:l3" met="3" type="-+">Hadt ghy my maghet <rhyme label="c" type="f">ghelaten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Een maechdeken van der <rhyme label="c" type="f" corresp="#s5:l3">straten</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ghy had veel beter <rhyme label="b" type="m" corresp="#s5:l2">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Had ick u maeght <rhyme label="a" type="f">ghelaten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Een maeght soo ick u <rhyme label="b" type="m">vandt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ghy sout soo fier zijn boven <rhyme label="c" type="f">schreven</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ghy en waert geen maecht <rhyme label="c" type="f" corresp="#s6:l3">gebleven</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Want den Somer die is noch <rhyme label="b" type="m" corresp="#s6:l2">lanck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De maegt die keerden daer <rhyme label="a" type="f">omme</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Eenen ganck is zy <rhyme label="b" type="m">ghegaen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zy ginck haer moeder <rhyme label="c" type="f">vraghen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Of zy over de Ruyter wou <rhyme label="c" type="f" corresp="#s7:l3">klaghen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Want hy had haer groot leyt <rhyme label="b" type="m" corresp="#s7:l2">gedaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Men nam den stouten <rhyme label="a" type="f">Ruyter</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ghevangen ghelijck een <rhyme label="b" type="m">Dief</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Al tusschen twee Heeren <rhyme label="c" type="f">Knapen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Schoon spreken en mocht hem niet <rhyme label="c" type="f" corresp="#s8:l3">baten</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Ghevanghen soo moest hy <rhyme label="b" type="m" corresp="#s8:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Men dede den Ruyter <rhyme label="a" type="f">legghen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Al op een pijne <rhyme label="b" type="m">banck</rhyme></l>
        <l id="s9:l3" met="3" type="-+">De pijnbanck was soo <rhyme label="c" type="f">kleyne</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat dede den Ruyter <rhyme label="c" type="f" corresp="#s9:l3">weyne</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Dat hy lag onder des heeren <rhyme label="b" type="m" corresp="#s9:l2">bedwang</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Men dede den Ruyter <rhyme label="a" type="f">knielen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Al voor dat blancke <rhyme label="b" type="m">Sweert</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Ghesellen wilt dit <rhyme label="c" type="f">onthouwen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Dit komt van schoone jonge <rhyme label="c" type="f" corresp="#s10:l3">vrouwen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Laetse varen diese niet en <rhyme label="b" type="m" corresp="#s10:l2">begheert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
