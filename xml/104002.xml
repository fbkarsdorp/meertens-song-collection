<song>
  <meta>
    <id>104002</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Alcander is het met u gedaan</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALcander ist met u <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wilt ghy u trouheyt breecken <rhyme label="a" type="m" corresp="#s1:l1">gaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Kont ghy de vlam <rhyme label="b" type="f">ontvluchten</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Wat schout ghy ons gewest soo <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En hoort niet naer mijn <rhyme label="b" type="f" corresp="#s1:l3">suchten</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Alcander, <rhyme label="d" type="f">Alcander</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Ach ick sterf en ghy vaert <rhyme label="c" type="m" corresp="#s1:l4">wel</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Acht lief hoe syt gy soo <rhyme label="a" type="m">versteent</rhyme>?</l>
        <l id="s2:l2" met="4" type="-+">Daer ic u altijt heb <rhyme label="a" type="m" corresp="#s2:l1">verleent</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Het soete trecke <rhyme label="b" type="f">becken</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">En t'geen ic noch niet seggen <rhyme label="c" type="m">derf</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En derf gy laes <rhyme label="b" type="f" corresp="#s2:l3">vertrecken</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Alcander, <rhyme label="d" type="f">Alcander</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ach keert weer eer dat ic <rhyme label="c" type="m" corresp="#s2:l4">sterf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Keert weder om O lieven <rhyme label="a" type="m">Iongelinck</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Die ic altijt uyt min <rhyme label="a" type="m" corresp="#s3:l1">ontfinck</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Vol doet noch eens u <rhyme label="b" type="f">lustens</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En peyst eens op voorleden <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hoe gym y d'eerstmael <rhyme label="b" type="f" corresp="#s3:l3">custen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Alcander, <rhyme label="d" type="f">Alcander</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Cust my doot soo heb ic <rhyme label="c" type="m" corresp="#s3:l4">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
