      <year-end>1695</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="danklied" translation="gratitude song">danklied</tag>
    </tags>
    <melody>Psalm 009 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEer die verzadight al wat <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Van wien al 't Vee sijn voedsel <rhyme label="a" type="m" corresp="#s1:l1">heeft</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ja selfs de groote stercke <rhyme label="b" type="f">Leeuwen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Tot wien de Jonge Ravens <rhyme label="b" type="f" corresp="#s1:l3">schreeuwen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy bidden die ons Vader <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Uw' gaven ons <rhyme label="a" type="m" corresp="#s2:l1">gebenedijt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want sy en konnen niet <rhyme label="b" type="f">verstercken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ten zy uw' handt daer door wil <rhyme label="b" type="f" corresp="#s2:l3">wercken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In 't broodt bestaet ons leven <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Maer in u Woordt en u <rhyme label="a" type="m" corresp="#s3:l1">gebiedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Gy kondt den staf des Broodts <rhyme label="b" type="f">verbreecken</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Soo is de kracht daer van <rhyme label="b" type="f" corresp="#s3:l3">geweecken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gy straft de Sondaers <rhyme label="a" type="m">opstinaet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dats' eten en niet zijn <rhyme label="a" type="m" corresp="#s4:l1">versaet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Heer wilt ons voor dien vloeck <rhyme label="b" type="f">behoeden</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En ons met uwen zegen <rhyme label="b" type="f" corresp="#s4:l3">voeden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ons' Lichaem voet in <rhyme label="a" type="m">soberheydt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ons' hert vervult met <rhyme label="a" type="m" corresp="#s5:l1">danckbaerheydt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Voor overdaet ons doch <rhyme label="b" type="f">bewaret</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die hert en ziel soo seer <rhyme label="b" type="f" corresp="#s5:l3">beswaret</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Geeft ons Ã´ Godt ons daeg'lijcks <rhyme label="a" type="m">broodt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Behoedt ons voor des hongers <rhyme label="a" type="m" corresp="#s6:l1">noodt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Heer maeckt ons vry van 't ydel <rhyme label="b" type="f">sorgen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Voor d'ongeboren dagh van <rhyme label="b" type="f" corresp="#s6:l3">morgen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Doch voor de ziel bidden wy <rhyme label="a" type="m">meest</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Versorght die door uw' Woort en <rhyme label="a" type="m" corresp="#s7:l1">Geest</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">'t Broodt van uw' woort kan niet <rhyme label="b" type="f">verderven</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Wie daer van eet die kan niet <rhyme label="b" type="f" corresp="#s7:l3">sterven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Versorght ons voort met spijs en <rhyme label="a" type="m">dranck</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Voor ziel en lijf ons leven <rhyme label="a" type="m" corresp="#s8:l1">lanck</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En als ons dagen zijn <rhyme label="b" type="f">versleten</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Soo laet-ons aen u Tafel <rhyme label="b" type="f" corresp="#s8:l3">eten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
