    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Gelijk als de witte zwaan</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ist niet een groet wonder <rhyme label="a" type="f">heede</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dat het goedt dus wert <rhyme label="b" type="m">beegeert</rhyme></l>
        <l id="s1:l3" met="4" type="-+">In wat dorprn ofte <rhyme label="a" type="f" corresp="#s1:l1">steede</rhyme></l>
        <l id="s1:l4" met="4" type="-+">waer men komt of <rhyme label="b" type="m" corresp="#s1:l2">komferseert</rhyme></l>
        <l id="s1:l5" met="4" type="-+">tis al vant tgoet dat men hoort <rhyme label="c" type="f">spreeken</rhyme></l>
        <l id="s1:l6" met="2" type="-+">ofte nae <rhyme label="d" type="m">vraeght</rhyme></l>
        <l id="s1:l7" met="4" type="-+">maer de Duecht die wert <rhyme label="c" type="f" corresp="#s1:l5">vergeeten</rhyme></l>
        <l id="s1:l8" met="2" type="-+">tsy godt <rhyme label="d" type="m" corresp="#s1:l6">geklaecht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dus gaet het met de Jonckmans <rhyme label="a" type="f">meede</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Die een dochter minnen omt <rhyme label="b" type="m">goedt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">sy en vraegen niet nae de <rhyme label="a" type="f" corresp="#s2:l1">eerbaerheede</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Die een dochter hebben <rhyme label="b" type="m" corresp="#s2:l2">moet</rhyme></l>
        <l id="s2:l5" met="4" type="-+">maer heeft sy goedt Ick wil myn <rhyme label="c" type="f">voerneeren</rhyme></l>
        <l id="s2:l6" met="2" type="-+">en draegen <rhyme label="d" type="m">faefuer</rhyme></l>
        <l id="s2:l7" met="4" type="-+">maedaemosselle wilt al <rhyme label="c" type="f" corresp="#s2:l5">salueeren</rhyme></l>
        <l id="s2:l8" met="2" type="-+">u <rhyme label="d" type="m" corresp="#s2:l6">faefuer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Is Daer een dochter <rhyme label="a" type="f">Idoene</rhyme></l>
        <l id="s3:l2" met="4" type="-+">van goeder huis of <rhyme label="b" type="m">quaelyteit</rhyme></l>
        <l id="s3:l3" met="4" type="-+">wort sy gesien van een <rhyme label="a" type="f" corresp="#s3:l1">persoene</rhyme></l>
        <l id="s3:l4" met="4" type="-+">men vraecht of sy geen goedt en <rhyme label="b" type="m" corresp="#s3:l2">heit</rhyme></l>
        <l id="s3:l5" met="4" type="-+">men vraecht niet of sy duechsaem <rhyme label="c" type="f">Is</rhyme></l>
        <l id="s3:l6" met="2" type="-+">van goedt <rhyme label="d" type="m">geslacht</rhyme></l>
        <l id="s3:l7" met="4" type="-+">maer als sy geen goedt en <rhyme label="c" type="f" corresp="#s3:l5">heit</rhyme></l>
        <l id="s3:l8" met="2" type="-+">soo en worse niet <rhyme label="d" type="m" corresp="#s3:l6">geacht</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Is daer aen dander <rhyme label="a" type="f">syde</rhyme></l>
        <l id="s4:l2" met="4" type="-+">een dochter machtich van <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">De Joncmans gaen haestich <rhyme label="a" type="f" corresp="#s4:l1">vryen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">sy denken tgoet maeck <rhyme label="b" type="m" corresp="#s4:l2">moet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Dan siet mense by de <rhyme label="c" type="f">rycke</rhyme></l>
        <l id="s4:l6" met="2" type="-+">
          <rhyme label="d" type="m">pracktyseeren</rhyme>
        </l>
        <l id="s4:l7" met="4" type="-+">al syn sy soo seedich <rhyme label="c" type="f" corresp="#s4:l5">niet</rhyme></l>
        <l id="s4:l8" met="2" type="-+">tmach wel <rhyme label="d" type="m" corresp="#s4:l6">passeeren</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Prinse prinsesse lief <rhyme label="a" type="f">Idoene</rhyme></l>
        <l id="s5:l2" met="4" type="-+">versint eer gy <rhyme label="b" type="m">begint</rhyme></l>
        <l id="s5:l3" met="4" type="-+">wert gy geesien van een <rhyme label="a" type="f" corresp="#s5:l1">persoene</rhyme></l>
        <l id="s5:l4" met="4" type="-+">let op die duecht eer gy <rhyme label="b" type="m" corresp="#s5:l2">begint</rhyme></l>
        <l id="s5:l5" met="4" type="-+">en wilt u niet <rhyme label="c" type="f">voornebuseeren</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Int aerschee <rhyme label="d" type="m">dal</rhyme></l>
        <l id="s5:l7" met="4" type="-+">want als u die persoen <rhyme label="c" type="f" corresp="#s5:l5">faelieert</rhyme></l>
        <l id="s5:l8" met="2" type="-+">faelieert het <rhyme label="d" type="m" corresp="#s5:l6">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
