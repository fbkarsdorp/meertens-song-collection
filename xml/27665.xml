    <melody>Schoon jonkheertje heus en trouw</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Lest ginck ick inde mey <rhyme label="a" type="f">spanceren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Wandelen met Lusten <rhyme label="b" type="m">fyn</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Waer door het hert most <rhyme label="a" type="f" corresp="#s1:l1">conforteren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Jae geheel in Rusten <rhyme label="b" type="m" corresp="#s1:l2">syn</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Vrou venus kaeren <rhyme label="c" type="m">excelent</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Sach ick by paeren daer <rhyme label="c" type="m" corresp="#s1:l5">ontrent</rhyme></l>
        <l id="s1:l7" met="6" type="+-">Hant aen hant // seer plaisant // dansen en <rhyme label="d" type="f">springen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Wentelen int groene // <rhyme label="e" type="m">gras</rhyme></l>
        <l id="s1:l9" met="6" type="-+">Soudt een quandt // syn verstandt // connen <rhyme label="d" type="f" corresp="#s1:l7">dwingen</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Die gewoon te doene // <rhyme label="e" type="m" corresp="#s1:l8">was</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Terstondt quaemen sy voorts <rhyme label="a" type="f">gedrongen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Herpen veelen Luyten <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Sy dansten speelden ende <rhyme label="a" type="f" corresp="#s2:l1">songen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Sulckx tgehoor ontsluyten <rhyme label="b" type="m" corresp="#s2:l2">doet</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Musyka schoon was daer <rhyme label="c" type="m">ontrent</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Elck hielt syn thoon opt <rhyme label="c" type="m" corresp="#s2:l5">instrument</rhyme></l>
        <l id="s2:l7" met="6" type="+-">Delykaet op syn maet // soo <rhyme label="d" type="f">manierich</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Pas a pas voorts <rhyme label="e" type="m">dansende</rhyme></l>
        <l id="s2:l9" met="6" type="-+">Nyders Raet // wert versmaet // <rhyme label="d" type="f" corresp="#s2:l7">puttertierich</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Met vreucht bloomkens <rhyme label="e" type="m" corresp="#s2:l8">schansende</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dees machdekens Lustich van <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Stelden my die Rosen <rhyme label="b" type="m">by</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Waerom soud'ick geen vreucht <rhyme label="a" type="f" corresp="#s3:l1">aencleuen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Die tot sulckx verkosen <rhyme label="b" type="m" corresp="#s3:l2">sy</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Veel bloemkens oock suyuer ende <rhyme label="c" type="m">Claer</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Liefflick van Reuck brochten sy <rhyme label="c" type="m" corresp="#s3:l5">daer</rhyme></l>
        <l id="s3:l7" met="6" type="+-">Sulcken vreucht // my verheucht // bouen <rhyme label="d" type="f">maetten</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Dacht ick hier end most hooren <rhyme label="e" type="m">daer</rhyme></l>
        <l id="s3:l9" met="6" type="-+">Tza ghy Ieucht // maeckt geneucht // met soet <rhyme label="d" type="f" corresp="#s3:l7">praeten</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Tis corage voor ende <rhyme label="e" type="m" corresp="#s3:l8">naer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Terstont quaemen sy voorts <rhyme label="a" type="f">gelaeden</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Omden Lust te boeten <rhyme label="b" type="m">meere</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Met vlaeden taerten ende <rhyme label="a" type="f" corresp="#s4:l1">gebraeden</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Wyn om druckx versoeten <rhyme label="b" type="m" corresp="#s4:l2">seer</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Myn docht ick haeck naer sulcken <rhyme label="c" type="m">geur</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Doen wert de smaeck noch <rhyme label="c" type="m" corresp="#s4:l5">gouuerneur</rhyme></l>
        <l id="s4:l7" met="6" type="+-">Heus en bly // willen wy // <rhyme label="d" type="f">bancketteren</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Sprack een van dees maechden <rhyme label="e" type="m">fier</rhyme></l>
        <l id="s4:l9" met="6" type="-+">Hout v vry // wie soudy // moghen <rhyme label="d" type="f" corresp="#s4:l7">deeren</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Spyt al diet mishaechden <rhyme label="e" type="m" corresp="#s4:l8">hier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dus siende alle dees vreucht voor <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Therten bloet verwermden <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Terstondt syn sy toe <rhyme label="a" type="f" corresp="#s5:l1">gevlogen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Helsden custen narmden <rhyme label="b" type="m" corresp="#s5:l2">my</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Ick hiel my coele op dit <rhyme label="c" type="m">termyn</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Maer het gevoele deede my <rhyme label="c" type="m" corresp="#s5:l5">pyn</rhyme></l>
        <l id="s5:l7" met="6" type="+-">Dye nu mocht als hy docht nae syn <rhyme label="d" type="f">wille</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Venus hoff omgraeuen <rhyme label="e" type="m">siet</rhyme></l>
        <l id="s5:l9" met="6" type="-+">Sulcken tocht moet gewrocht syn al <rhyme label="d" type="f" corresp="#s5:l7">stille</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Maer elck heeft de gaeuen <rhyme label="e" type="m" corresp="#s5:l8">niet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
