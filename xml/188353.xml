    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">J'Avois dans ce <rhyme label="a" type="f">voisinage</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Un fort aimable <rhyme label="b" type="m">Berger</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Mais d'un humeur si <rhyme label="a" type="f" corresp="#s1:l1">volage</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Quil ne se plait qu'à <rhyme label="b" type="m" corresp="#s1:l2">Changer</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Lorsquil surprit ma <rhyme label="c" type="f">tendresse</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Jeluy Croyois plus <rhyme label="d" type="m">constant</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Mais helas! il change sans <rhyme label="c" type="f" corresp="#s1:l5">cesse</rhyme>:</l>
        <l id="s1:l8" met="4" type="+-">Faut il que je l'aime <rhyme label="d" type="m" corresp="#s1:l6">tant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Il me dit quil ne peut <rhyme label="a" type="f">vivre</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Sans m'aimer ni sans me <rhyme label="b" type="m">voir</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Cependant je le vis <rhyme label="a" type="f" corresp="#s2:l1">suivre</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Par Jeannetton hier au <rhyme label="b" type="m" corresp="#s2:l2">soir</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Il entra chez sa <rhyme label="c" type="f">Climene</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Il en sortit fort <rhyme label="d" type="m">content</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">Mais tout cecy fait ma <rhyme label="c" type="f" corresp="#s2:l5">peine</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Faut il que je l'aime <rhyme label="d" type="m" corresp="#s2:l6">tant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">De la Brune &amp; de la <rhyme label="a" type="f">Blonde</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Le fripon charme le <rhyme label="b" type="m">coeur</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Il enchante tout le <rhyme label="a" type="f" corresp="#s3:l1">monde</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Par son aimable <rhyme label="b" type="m" corresp="#s3:l2">douceur</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Pour pousser son <rhyme label="c" type="f">avanture</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Il n'Epargne aucun <rhyme label="d" type="m">serment</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Ses sermens sont des <rhyme label="c" type="f" corresp="#s3:l5">parjures</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Faut il que je l'aime <rhyme label="d" type="m" corresp="#s3:l6">tant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">L'autre jour sur la <rhyme label="a" type="f">Fougere</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ce Berger <rhyme label="b" type="m">audacieux</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Carrossoit une <rhyme label="a" type="f" corresp="#s4:l1">bergere</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Et lá Baisoit à mes <rhyme label="b" type="m" corresp="#s4:l2">jeux</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Je le vis &amp; je fus <rhyme label="c" type="f">prette</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">De le bien battre à <rhyme label="d" type="m">l'instant</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">Mais j'abaissai ma <rhyme label="c" type="f" corresp="#s4:l5">houlette</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Faut il que je l'aime <rhyme label="d" type="m" corresp="#s4:l6">tant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Adieu donc belle <rhyme label="a" type="f">bergere</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Puisque vous ne m'aimes <rhyme label="b" type="m">pas</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Je m'en vais sur la <rhyme label="a" type="f" corresp="#s5:l1">fougere</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">M'abandonner au <rhyme label="b" type="m" corresp="#s5:l2">trepas</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Mais Helas! ee me dit <rhyme label="c" type="f">elle</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Si vous m'aimes <rhyme label="d" type="m">tendrement</rhyme>:</l>
        <l id="s5:l7" met="4" type="+-">Soyez moi toujours <rhyme label="c" type="f" corresp="#s5:l5">fidelle</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Et puis vous seres <rhyme label="d" type="m" corresp="#s5:l6">content</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
