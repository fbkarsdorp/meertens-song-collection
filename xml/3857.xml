        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
      <tag>spiritual song</tag>
      <tag>devout song</tag>
    </tags>
    <melody>Solaas wil ik hanteren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Solaes wil ic <rhyme label="a" type="f">hanteren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ende daer toe vrolijck <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En blijdelijck <rhyme label="a" type="f" corresp="#s1:l1">houeren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met Iesus mijn <rhyme label="b" type="m" corresp="#s1:l2">minnekijn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Waer om so willen wi <rhyme label="c" type="f">trueren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ten mach anders nyet <rhyme label="d" type="m">sijn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ten sal niet langhe <rhyme label="c" type="f" corresp="#s1:l5">dueren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dat ick bedruct sal <rhyme label="d" type="m" corresp="#s1:l6">sijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In alle die werelt <rhyme label="a" type="f">wide</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En vinde ick niet so <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat mi mach maken <rhyme label="a" type="f" corresp="#s2:l1">blide</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dan Iesus mijn minneken <rhyme label="b" type="m" corresp="#s2:l2">soet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Hier om wil ick mi <rhyme label="c" type="f">gheuen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Hem te dienen tot alder <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">So wordic van herten <rhyme label="c" type="f" corresp="#s2:l5">verheuen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ende eewelijck <rhyme label="d" type="m" corresp="#s2:l6">verblijt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy is een heer des <rhyme label="a" type="f">heeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die schoonste diemen <rhyme label="b" type="m">vint</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ick wil mi tot hem <rhyme label="a" type="f" corresp="#s3:l1">keeren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Want hi ons so seere <rhyme label="b" type="m" corresp="#s3:l2">bemint</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Gheen hert en mach <rhyme label="c" type="f">versinnen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die bliscap die hi heeft <rhyme label="d" type="m">bereyt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Den ghenen die hem <rhyme label="c" type="f" corresp="#s3:l5">minnen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">So ons die scriftuere dat <rhyme label="d" type="m" corresp="#s3:l6">seyt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy is een bloem <rhyme label="a" type="f">gheheeten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Seer suyuerlijck ende <rhyme label="b" type="m">claer</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hy is so hooghe <rhyme label="a" type="f" corresp="#s4:l1">gheseten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Al bouen der enghelen <rhyme label="b" type="m" corresp="#s4:l2">schaer</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Gheboren tot onser <rhyme label="c" type="f">baten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van eender maget <rhyme label="d" type="m">bequaem</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Als bouen <rhyme label="c" type="f" corresp="#s4:l5">honichraten</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Is soeter Ihesus <rhyme label="d" type="m" corresp="#s4:l6">naem</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wanneer sal ick <rhyme label="a" type="f">anscouwen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Sijn ouer claer <rhyme label="b" type="m">aenschijn</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Och mocht ick hem <rhyme label="a" type="f" corresp="#s5:l1">behouwen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Al in dat herte <rhyme label="b" type="m" corresp="#s5:l2">mijn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Wat mi mocht ouer <rhyme label="c" type="f">comen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En soude ick achten <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Waert soe het soude mi <rhyme label="c" type="f" corresp="#s5:l5">vromen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">So en had ick gheen <rhyme label="d" type="m" corresp="#s5:l6">verdriet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O god wilt mijns <rhyme label="a" type="f">ontfermen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Doer uwen name <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ende wilt ons doch <rhyme label="a" type="f" corresp="#s6:l1">bescermen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Al vander hellen <rhyme label="b" type="m" corresp="#s6:l2">pijn</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ende wilt ons doch <rhyme label="c" type="f">gheleyden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Ende bringhen int eewich <rhyme label="d" type="m">rijck</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Als wij van hier al <rhyme label="c" type="f" corresp="#s6:l5">scheyden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Dat bidden wij al <rhyme label="d" type="m" corresp="#s6:l6">ghelijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
