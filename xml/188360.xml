<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188360</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Branle de Metz</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">POurquoi charmante <rhyme label="a" type="f">Bergere</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Voulez vous n'aimer <rhyme label="b" type="m">jamais</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">L'Amour m'offre un de fes <rhyme label="a" type="f" corresp="#s1:l1">traits</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Si digne helas de vous <rhyme label="b" type="m" corresp="#s1:l2">plaire</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Je n'en dirai pas le <rhyme label="c" type="m">nom</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Il a sû charmer sa <rhyme label="d" type="f">mere</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Je n'en dirai pas le <rhyme label="c" type="m" corresp="#s1:l5">nom</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s1:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Chaque Amant, pour son <rhyme label="a" type="f">partage</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Porte ce trait avec <rhyme label="b" type="m">soy</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Mais il n'a pas comme <rhyme label="a" type="f" corresp="#s2:l1">moy</rhyme></l>
        <l id="s2:l4" met="4" type="+-">L'art d'en faire un bon <rhyme label="b" type="m" corresp="#s2:l2">usage</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">On le sçait bien à vôtre <rhyme label="d" type="f">âge</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s2:l5">nom</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s2:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Il flatte au moment qu'il <rhyme label="a" type="f">blesse</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Et ne blesse qu'une <rhyme label="b" type="m">fois</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">On en trouve le <rhyme label="a" type="f" corresp="#s3:l1">carquois</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Prés du coeur de sa <rhyme label="b" type="m" corresp="#s3:l2">Maîtresse</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Il pleure quand on le <rhyme label="d" type="f">caresse</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s3:l5">nom</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s3:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sa figure est peu <rhyme label="a" type="f">jolie</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Quand il n'est point <rhyme label="b" type="m">animé</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Mais bien-tôt l'objet <rhyme label="a" type="f" corresp="#s4:l1">aimé</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Fait cesser sa <rhyme label="b" type="m" corresp="#s4:l2">léthargie</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">J'en ay pourtant bien <rhyme label="d" type="f">envie</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s4:l5">nom</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s4:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">La Bergere la plus <rhyme label="a" type="f">sage</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Ne peut souffrir un <rhyme label="b" type="m">Amant</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">S'il na ce trait si <rhyme label="a" type="f" corresp="#s5:l1">charmant</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Et s'il n'en sçait faire <rhyme label="b" type="m" corresp="#s5:l2">usage</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Vous le devinez, je <rhyme label="d" type="f">gage</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s5:l5">nom</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Or écoutez, ma <rhyme label="c" type="m" corresp="#s5:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Lorsqu'une main <rhyme label="a" type="f">favorite</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Vient doucement le <rhyme label="b" type="m">flatter</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Cela ne fait <rhyme label="a" type="f" corresp="#s6:l1">qu'iriter</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Le doux transport qui <rhyme label="b" type="m" corresp="#s6:l2">l'excite</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Jugéz en par son <rhyme label="d" type="f">merite</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s6:l5">nom</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s6:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Alcide le <rhyme label="a" type="f">redoutable</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Parmy ses travaux <rhyme label="b" type="m">vantez</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Brilla plus chez les <rhyme label="a" type="f" corresp="#s7:l1">Beautez</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Par ce trait <rhyme label="b" type="m" corresp="#s7:l2">infatigable</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Quoiqu'il n'ait rien que <rhyme label="d" type="f">d'aimable</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s7:l5">nom</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s7:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">C'est pour l'Amour qu'il <rhyme label="a" type="f">s'anime</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Et plein d'un si noble <rhyme label="b" type="m">feu</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Il rend fier, il rend <rhyme label="a" type="f" corresp="#s8:l1">heureux</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Il enfonce, &amp; il <rhyme label="b" type="m" corresp="#s8:l2">abîme</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Le respect en fait un <rhyme label="d" type="f">crime</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s8:l5">nom</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s8:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Le mien a l'heureux <rhyme label="a" type="f">mérite</rhyme></l>
        <l id="s9:l2" met="4" type="+-">De ne point craindre les <rhyme label="b" type="m">coups</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Belle, il ne tiendra qu'à <rhyme label="a" type="f" corresp="#s9:l1">vous</rhyme></l>
        <l id="s9:l4" met="4" type="+-">D'en être bien-tôt <rhyme label="b" type="m" corresp="#s9:l2">instruite</rhyme>;</l>
        <l id="s9:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Je répond de sa <rhyme label="d" type="f">conduite</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s9:l5">nom</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s9:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Si vôtre tendre <rhyme label="a" type="f">Bergere</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Paroît en craindre <rhyme label="b" type="m">l'effet</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Il faut retenir ce <rhyme label="a" type="f" corresp="#s10:l1">trait</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Vers la fin de sa <rhyme label="b" type="m" corresp="#s10:l2">carriére</rhyme>;</l>
        <l id="s10:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">La prudence est <rhyme label="d" type="f">necessaire</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s10:l5">nom</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s10:l5">chanson</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">J'oses vous en faire <rhyme label="a" type="f">hommage</rhyme>;</l>
        <l id="s11:l2" met="4" type="+-">N'allez pas le <rhyme label="b" type="m">refuser</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Rien que pour vous <rhyme label="a" type="f" corresp="#s11:l1">amuser</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Acceptez son <rhyme label="b" type="m" corresp="#s11:l2">badinage</rhyme>;</l>
        <l id="s11:l5" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m">nom</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">C'est un meuble de <rhyme label="d" type="f">ménage</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Je n'en diray pas le <rhyme label="c" type="m" corresp="#s11:l5">nom</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Or écoutez ma <rhyme label="c" type="m" corresp="#s11:l5">chanson</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
