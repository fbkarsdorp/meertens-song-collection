<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13375</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="sailor's song">zeemanslied</tag>
      <tag translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Als Phoebus vertoog</melody>
    <categories>
      <category translation="work">werk</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">KOmt nu Gasten <rhyme label="a" type="m">tsa</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">En repje oock <rhyme label="a" type="m" corresp="#s1:l1">dra</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Verhuert jou met een <rhyme label="b" type="m">beraet</rhyme>:</l>
        <l id="s1:l4" met="3" type="+-">Ons Schip leyt nu <rhyme label="c" type="m">ree</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">En wil voort na <rhyme label="c" type="m" corresp="#s1:l4">Zee</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Na Groenlandt my wel <rhyme label="b" type="m" corresp="#s1:l3">verstaet</rhyme>;</l>
        <l id="s1:l7" met="3" type="+-">Komt voort met een <rhyme label="d" type="m">vlijt</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Het is nu hoogh <rhyme label="d" type="m" corresp="#s1:l7">tijdt</rhyme>:</l>
        <l id="s1:l9" met="4" type="+-">Seght eens wat je winnen <rhyme label="e" type="m">wilt</rhyme>;</l>
        <l id="s1:l10" met="3" type="+-">Op morgen <rhyme label="f" type="m">aenhoort</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Soo vaaren wy <rhyme label="f" type="m" corresp="#s1:l10">voort</rhyme>;</l>
        <l id="s1:l12" met="4" type="+-">Siet datje geen tijdt en <rhyme label="e" type="m" corresp="#s1:l9">verspilt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Vier Sloepen seer <rhyme label="a" type="m">jent</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">En heel <rhyme label="a" type="m" corresp="#s2:l1">pertinent</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Soo elck een nu wel kan <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Die voeren wy <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">En seer wel <rhyme label="c" type="m" corresp="#s2:l4">bemant</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Van alles oock wel <rhyme label="b" type="m" corresp="#s2:l3">versien</rhyme>.</l>
        <l id="s2:l7" met="3" type="+-">Ziet ons <rhyme label="d" type="m">Commandeur</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Is meede geen <rhyme label="d" type="m" corresp="#s2:l7">leur</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Maer een bevaaren braef <rhyme label="e" type="m">Man</rhyme>;</l>
        <l id="s2:l10" met="3" type="+-">En al d' <rhyme label="f" type="m">Officiers</rhyme></l>
        <l id="s2:l11" met="3" type="+-">En de <rhyme label="f" type="m" corresp="#s2:l10">Harpeniers</rhyme></l>
        <l id="s2:l12" met="4" type="+-">Gebreeckt in 't minste niet <rhyme label="e" type="m" corresp="#s2:l9">an</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Komt nu dan met <rhyme label="a" type="m">vree</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Hier gaene wy <rhyme label="a" type="m" corresp="#s3:l1">t'Zee</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Nae Groenlant al om de <rhyme label="b" type="m">Noort</rhyme>;</l>
        <l id="s3:l4" met="3" type="+-">Om 't Schip wel te <rhyme label="c" type="m">laen</rhyme>;</l>
        <l id="s3:l5" met="3" type="+-">Met Speck ofte <rhyme label="c" type="m" corresp="#s3:l4">Traen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">So is de meenigh <rhyme label="b" type="m" corresp="#s3:l3">hoort</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">Wy zeylen om <rhyme label="d" type="m">prijs</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">'t Schip lustigh in 't <rhyme label="d" type="m" corresp="#s3:l7">Ys</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Al nae de Walvis <rhyme label="e" type="m">stout</rhyme>:</l>
        <l id="s3:l10" met="3" type="+-">Maer meenigh die <rhyme label="f" type="m">t'proeft</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Heeft laes seer <rhyme label="f" type="m" corresp="#s3:l10">bedroeft</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Te laet dan sijn kop <rhyme label="e" type="m" corresp="#s3:l9">gekrout</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wel siet men een <rhyme label="a" type="m">Vis</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">'t Is tsae mannen <rhyme label="a" type="m" corresp="#s4:l1">fris</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wie 't eerst daer by komen <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">En wie dan seer <rhyme label="c" type="m">koen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Eerst schiet sijn <rhyme label="c" type="m" corresp="#s4:l4">Harpoen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Voorwaer die isser best <rhyme label="b" type="m" corresp="#s4:l3">an</rhyme>;</l>
        <l id="s4:l7" met="3" type="+-">Want slaet de Vis <rhyme label="d" type="m">seer</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">En rijst daer nae <rhyme label="d" type="m" corresp="#s4:l7">weer</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">De andere Sloepen <rhyme label="e" type="m">voort</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Die lensen hem <rhyme label="f" type="m">doodt</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">De Wallevis <rhyme label="f" type="m" corresp="#s4:l10">groot</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">En boeghseeren hem nae <rhyme label="e" type="m" corresp="#s4:l9">Boort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Daer snijt men dan 't <rhyme label="a" type="m">speck</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Al sonder <rhyme label="a" type="m" corresp="#s5:l1">vertreck</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En wint het met Taeckels in 't <rhyme label="b" type="m">Schip</rhyme>:</l>
        <l id="s5:l4" met="3" type="+-">Van daer na <rhyme label="c" type="m">beneen</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Aen stuckjes <rhyme label="c" type="m" corresp="#s5:l4">gesneen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">In Vaten al met een <rhyme label="b" type="m" corresp="#s5:l3">wip</rhyme>.</l>
        <l id="s5:l7" met="3" type="+-">De Baerden seer <rhyme label="d" type="m">raer</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Die halen wy <rhyme label="d" type="m" corresp="#s5:l7">daer</rhyme></l>
        <l id="s5:l9" met="4" type="+-">De Vis uyt sijn kaecken <rhyme label="e" type="m">koen</rhyme>:</l>
        <l id="s5:l10" met="3" type="+-">Dats 't Wallevis <rhyme label="f" type="m">bien</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Verstaet wel goe <rhyme label="f" type="m" corresp="#s5:l10">Lien</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">Daer wy soo veel werck om <rhyme label="e" type="m" corresp="#s5:l9">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Maer is de vanghst <rhyme label="a" type="m">slecht</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Dan gaen wy weer <rhyme label="a" type="m" corresp="#s6:l1">recht</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Na 't Lant, of na het <rhyme label="b" type="m">Ys</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">En slaen dan ter <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">'t Zy Walrus of <rhyme label="c" type="m" corresp="#s6:l4">Beer</rhyme>:</l>
        <l id="s6:l6" met="4" type="+-">Elck waeght daer sijn Schip om <rhyme label="b" type="m" corresp="#s6:l3">prijs</rhyme>:</l>
        <l id="s6:l7" met="3" type="+-">Het moetter op <rhyme label="d" type="m">staen</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Men waechter 't Schip <rhyme label="d" type="m" corresp="#s6:l7">aen</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Lustigh in 't Ys om goe <rhyme label="e" type="m">buyt</rhyme>;</l>
        <l id="s6:l10" met="3" type="+-">Want komt men <rhyme label="f" type="m">gelooft</rhyme></l>
        <l id="s6:l11" met="3" type="+-">Mette kous op het <rhyme label="f" type="m" corresp="#s6:l10">hooft</rhyme>,</l>
        <l id="s6:l12" met="4" type="+-">Soo is de reys heel <rhyme label="e" type="m" corresp="#s6:l9">verbruyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Maer heeft men goe <rhyme label="a" type="m">vanghst</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Dan is men een <rhyme label="a" type="m" corresp="#s7:l1">langhst</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die waert om te prijsen <rhyme label="b" type="m">is</rhyme>:</l>
        <l id="s7:l4" met="3" type="+-">Is 't Schip maer <rhyme label="c" type="m">gelaen</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Met Speck of met <rhyme label="c" type="m" corresp="#s7:l4">Traen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Die man is welkom <rhyme label="b" type="m" corresp="#s7:l3">gewis</rhyme>;</l>
        <l id="s7:l7" met="3" type="+-">Sijn gelt is <rhyme label="d" type="m">verdient</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">'t Is alle mans <rhyme label="d" type="m" corresp="#s7:l7">vrient</rhyme>:</l>
        <l id="s7:l9" met="4" type="+-">Want siet, die man heeft <rhyme label="e" type="m">verstant</rhyme>:</l>
        <l id="s7:l10" met="3" type="+-">Ja dit is de <rhyme label="f" type="m">Man</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Die Vis vangen <rhyme label="f" type="m" corresp="#s7:l10">kan</rhyme>:</l>
        <l id="s7:l12" met="4" type="+-">Trots yemant van 't heele <rhyme label="e" type="m" corresp="#s7:l9">Lant</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Siet, dus is de <rhyme label="a" type="m">vaert</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Van Groenlant <rhyme label="a" type="m" corresp="#s8:l1">vermaert</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Al was de Commandeur <rhyme label="b" type="m">geck</rhyme>:</l>
        <l id="s8:l4" met="3" type="+-">Soo 't luck hem <rhyme label="c" type="m">bemint</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Dat hy de Vis <rhyme label="c" type="m" corresp="#s8:l4">vint</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Te laden het Schip met <rhyme label="b" type="m" corresp="#s8:l3">Speck</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Hy raeckt strackx weer <rhyme label="d" type="m">voort</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Daer meenigh man, <rhyme label="d" type="m" corresp="#s8:l7">hoort</rhyme>,</l>
        <l id="s8:l9" met="4" type="+-">Het tienmael beter is <rhyme label="e" type="m">waert</rhyme>,</l>
        <l id="s8:l10" met="3" type="+-">Want dees het <rhyme label="f" type="m">geluckt</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Maer d' ander <rhyme label="f" type="m" corresp="#s8:l10">bedruckt</rhyme>,</l>
        <l id="s8:l12" met="4" type="+-">Staet treurigh, als gans <rhyme label="e" type="m" corresp="#s8:l9">ontaert</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">O Reeders ghy <rhyme label="a" type="m">dwaelt</rhyme></l>
        <l id="s9:l2" met="3" type="+-">In 't Oordeel, en <rhyme label="a" type="m" corresp="#s9:l1">faelt</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Van meenigh vroom eerlijck <rhyme label="b" type="m">helt</rhyme>;</l>
        <l id="s9:l4" met="3" type="+-">Om dat het <rhyme label="c" type="m">verstaet</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Hem niet wel en <rhyme label="c" type="m" corresp="#s9:l4">gaet</rhyme></l>
        <l id="s9:l6" met="4" type="+-">In 't winnen van 't Duyvelse <rhyme label="b" type="m" corresp="#s9:l3">gelt</rhyme>.</l>
        <l id="s9:l7" met="3" type="+-">Maer Godt de Heer <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">Verlaet hem noch <rhyme label="d" type="m" corresp="#s9:l7">niet</rhyme>;</l>
        <l id="s9:l9" met="4" type="+-">Al stoot ghy hem met de <rhyme label="e" type="m">voet</rhyme>,</l>
        <l id="s9:l10" met="3" type="+-">Hy sal noch hier <rhyme label="f" type="m">naer</rhyme></l>
        <l id="s9:l11" met="3" type="+-">Uyt blincken seer <rhyme label="f" type="m" corresp="#s9:l10">klaer</rhyme>,</l>
        <l id="s9:l12" met="4" type="+-">Gesegent in groot <rhyme label="e" type="m" corresp="#s9:l9">overvloet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Nu Groenlanders, <rhyme label="a" type="m">hoort</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Voor 't laest noch een <rhyme label="a" type="m" corresp="#s10:l1">woort</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Ick wens u al t' saem nae den <rhyme label="b" type="m">eys</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Veel Speck en veel <rhyme label="c" type="m">Traen</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">U Scheepen <rhyme label="c" type="m" corresp="#s10:l4">gelaen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">'t Huys met een behouden <rhyme label="b" type="m" corresp="#s10:l3">Reys</rhyme>;</l>
        <l id="s10:l7" met="3" type="+-">En dat ghy dan <rhyme label="d" type="m">weer</rhyme></l>
        <l id="s10:l8" met="3" type="+-">Mooght nemen u <rhyme label="d" type="m" corresp="#s10:l7">keer</rhyme>,</l>
        <l id="s10:l9" met="4" type="+-">Wanneer het Saysoen weer <rhyme label="e" type="m">genaeckt</rhyme>.</l>
        <l id="s10:l10" met="3" type="+-">En tot een <rhyme label="f" type="m">besluyt</rhyme>,</l>
        <l id="s10:l11" met="3" type="+-">Vaer wel in en <rhyme label="f" type="m" corresp="#s10:l10">uyt</rhyme>;</l>
        <l id="s10:l12" met="4" type="+-">Dit wens ick tot slot van de <rhyme label="e" type="m" corresp="#s10:l9">saeck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
