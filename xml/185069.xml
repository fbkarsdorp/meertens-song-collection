    <tags/>
    <melody>Psalm 079 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">V Liet wegh van <rhyme label="a" type="m">my</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sorghvuldighe <rhyme label="b" type="f">ghedachten</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Wijckt aen d’een <rhyme label="a" type="m" corresp="#s1:l1">sy</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die mijn ziele <rhyme label="b" type="f" corresp="#s1:l2">versmachten</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">2   Maer comt hier <rhyme label="a" type="m" corresp="#s1:l1">ghy</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Hemelsche soete <rhyme label="b" type="f" corresp="#s1:l2">crachten</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Siet ick wil <rhyme label="a" type="m" corresp="#s1:l1">dy</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In my alleen <rhyme label="b" type="f" corresp="#s1:l2">verwachten</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Want soo ghy my <rhyme label="c" type="m">vervult</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Ick weet dat ghy my <rhyme label="c" type="m" corresp="#s1:l9">sult</rhyme></l>
        <l id="s1:l11" met="3" type="+-">3 Med’ deylen die <rhyme label="d" type="f">nature</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Van ‘t God’lijck wesen <rhyme label="e" type="m">soet</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Waer door ick werden <rhyme label="e" type="m" corresp="#s1:l12">moet</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">Een nieuwe <rhyme label="d" type="f" corresp="#s1:l11">Creature</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">* Dus van my <rhyme label="a" type="m">scheyt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">O werelt ghy moet <rhyme label="b" type="f">blaken</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">V <rhyme label="a" type="m" corresp="#s2:l1">heerlijckheyt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En can my niet <rhyme label="b" type="f" corresp="#s2:l2">vermaken</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ick ben <rhyme label="a" type="m" corresp="#s2:l1">bereyt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Gheheel u te <rhyme label="b" type="f" corresp="#s2:l2">versaken</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+"><rhyme label="a" type="m" corresp="#s2:l1">D’inwendicheyt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">5 Moet nu wat beters <rhyme label="b" type="f" corresp="#s2:l2">smaken</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">V heerlijckheut, u <rhyme label="c" type="m">jeught</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">V glory, en u <rhyme label="c" type="m" corresp="#s2:l9">vreught</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">‘T en is maer een <rhyme label="d" type="f">figure</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Van ‘t waerachtighe <rhyme label="e" type="m">goet</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Het welck my werden <rhyme label="e" type="m" corresp="#s2:l12">doet</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Een nieuwe <rhyme label="d" type="f" corresp="#s2:l11">Creature</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">* Dat eertijts <rhyme label="a" type="m">was</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In hoogher acht en <rhyme label="b" type="f">weerde</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Dat moet seer <rhyme label="a" type="m" corresp="#s3:l1">ras</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Vergaen tot stof en <rhyme label="b" type="f" corresp="#s3:l2">eerde</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Ten gheeft gheen <rhyme label="a" type="m" corresp="#s3:l1">pas</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat de oude <rhyme label="b" type="f" corresp="#s3:l2">bejaerde</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">6 In hoy en <rhyme label="a" type="m" corresp="#s3:l1">gras</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Zijn sinnen noch <rhyme label="b" type="f" corresp="#s3:l2">vergaerde</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">7 ‘T eerste heeft zijn <rhyme label="c" type="m">adieu</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">‘T moet altemale <rhyme label="c" type="m" corresp="#s3:l9">nieu</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Ghemaeckt zijn, reyn en <rhyme label="d" type="f">pure</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Want hoement heft of <rhyme label="e" type="m">stelt</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">8 Voor Godt alleene <rhyme label="e" type="m" corresp="#s3:l12">gelt</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">Een nieuwe <rhyme label="d" type="f" corresp="#s3:l11">Creature</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">* ‘T moet al <rhyme label="a" type="m">vergaen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wat daer noch is voor <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Ten mach niet <rhyme label="a" type="m" corresp="#s4:l1">staen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">‘T moet met den vyere <rhyme label="b" type="f" corresp="#s4:l2">branden</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Ia Zon en <rhyme label="a" type="m" corresp="#s4:l1">Maen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die moeten oock tot <rhyme label="b" type="f" corresp="#s4:l2">schanden</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">Godts licht <rhyme label="a" type="m" corresp="#s4:l1">voortaen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Lichtet in zyn <rhyme label="b" type="f" corresp="#s4:l2">waranden</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Want daer en is gheen <rhyme label="c" type="m">Zee</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Droefheyt, gheclagh, noch <rhyme label="c" type="m" corresp="#s4:l9">wee</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Noch gheen tempeesten <rhyme label="d" type="f">sture</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">‘T is al lieflijck en <rhyme label="e" type="m">stil</rhyme>,</l>
        <l id="s4:l13" met="3" type="-+">En sonder eyghen <rhyme label="e" type="m" corresp="#s4:l12">wil</rhyme>,</l>
        <l id="s4:l14" met="3" type="-+">10 Een nieuwe <rhyme label="d" type="f" corresp="#s4:l11">Creature</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
