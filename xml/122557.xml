  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laat Jupijn 't Heeläl <rhyme label="a" type="f">verbazen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Als hij met zijn donder <rhyme label="b" type="m">loeit</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Mars in 't blinkend harnas <rhyme label="a" type="f" corresp="#s1:l1">jagen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als hij 't veld met bloed <rhyme label="b" type="m" corresp="#s1:l2">besproeit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Rozàlien heeft meer <rhyme label="a" type="f">vermogen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Op mijn hart, dan deze <rhyme label="b" type="m">twee</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'t Vuur van haar bekoorlijke <rhyme label="a" type="f" corresp="#s2:l1">oogen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maakt mij met een lonk <rhyme label="b" type="m" corresp="#s2:l2">gedwee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Als ik in haar schoot mag <rhyme label="a" type="f">spart'len</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Baadde ik in een zee vol <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Als ik om haar hals mag <rhyme label="a" type="f" corresp="#s3:l1">dart'len</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Is mijn mond nooit moê <rhyme label="b" type="m" corresp="#s3:l2">gekust</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik zie de violet en <rhyme label="a" type="f">roose</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Elk om strijd op haar <rhyme label="b" type="m">gelaat</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Met een eerbre schaamte <rhyme label="a" type="f" corresp="#s4:l1">blooze</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">In haar lente <rhyme label="b" type="m" corresp="#s4:l2">dageraad</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ik laat wie wil den krijgsgod <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Laat hem beven voor <rhyme label="b" type="m">Jupijn</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ik laat mij graâg <rhyme label="a" type="f" corresp="#s5:l1">overwinnen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Door een lonk van <rhyme label="b" type="m" corresp="#s5:l2">Rozalijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
