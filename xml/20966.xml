<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20966</id>
    <date>
      <year-start>1655</year-start>
      <year-end>1655</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="May song">meilied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody>Een meisje had een ruiter lief</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Als ick was laetstmael <rhyme label="a" type="m">uytgegaen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Heb ick een soete reuck <rhyme label="a" type="m" corresp="#s1:l1">ontfaen</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Ick sagh met douw <rhyme label="b" type="f">beladen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">De open-gaende <rhyme label="b" type="f" corresp="#s1:l3">bladen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick sagh de kruyden spruyten <rhyme label="a" type="m">uyt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ick hoorden-aen een soet <rhyme label="a" type="m" corresp="#s2:l1">geluyt</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">De vogelkens die <rhyme label="b" type="f">songen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In 't veldt de beesjes <rhyme label="b" type="f" corresp="#s2:l3">sprongen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick sagh den gulden <rhyme label="a" type="m">Butter-blom</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die in de velden droegh haer <rhyme label="a" type="m" corresp="#s3:l1">rom</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Ick sagh de Door-bes <rhyme label="b" type="f">bloeyen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ick sagh de boomtjes <rhyme label="b" type="f" corresp="#s3:l3">groeyen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick sagh de Imp'riaelsche <rhyme label="a" type="m">kroon</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die haer vertoonden wonder <rhyme label="a" type="m" corresp="#s4:l1">schoon</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Ick kon voorwaer niet <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Haer peereltjes van <rhyme label="b" type="f" corresp="#s4:l3">binnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick sagh de Wey bekleedt met <rhyme label="a" type="m">gras</rhyme>;</l>
        <l id="s5:l2" met="4" type="-+">Ick rusten, ick hiel stil mijn <rhyme label="a" type="m" corresp="#s5:l1">pas</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Ia ick gingh <rhyme label="b" type="f">nederleggen</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Ick docht, wat wilt dit <rhyme label="b" type="f" corresp="#s5:l3">seggen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ick docht, hoe goedt is Godt den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die ons de Lenten nu brengt <rhyme label="a" type="m" corresp="#s6:l1">we'er</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wien met haer <rhyme label="b" type="f">quinckeleren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">De vogelkens <rhyme label="b" type="f" corresp="#s6:l3">vereeren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ick docht, hoe fraey is nu de <rhyme label="a" type="m">Wey</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En ick verweckten soet <rhyme label="a" type="m" corresp="#s7:l1">geschrey</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Om dat ick Godt <rhyme label="b" type="f">Almachtich</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Soo weynich was <rhyme label="b" type="f" corresp="#s7:l3">gedachtich</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ick docht, hoe helder is de <rhyme label="a" type="m">lucht</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Ick trock uyt 't hart een diep <rhyme label="a" type="m" corresp="#s8:l1">ghesucht</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Ick docht, och of mijn <rhyme label="b" type="f">wooningh</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Was hy des Hemels <rhyme label="b" type="f" corresp="#s8:l3">Coningh</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ick docht hoe schoon moet wesen <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Die dus de boomen ciert met <rhyme label="a" type="m" corresp="#s9:l1">lot</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Och of ick eens mocht <rhyme label="b" type="f">komen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">By die dus ciert de <rhyme label="b" type="f" corresp="#s9:l3">boomen</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ick docht, en sey; Och of mijn <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s10:l2" met="4" type="-+">De glans der deughden soo <rhyme label="a" type="m" corresp="#s10:l1">behiel</rhyme>!</l>
        <l id="s10:l3" met="3" type="-+">G'lijck als haer glans <rhyme label="b" type="f">behouden</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Die blommen, die 'k <rhyme label="b" type="f" corresp="#s10:l3">aenschouden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ick docht, 't is wonder dat ick <rhyme label="a" type="m">ben</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Soo slecht, dat ick mijn Godt niet <rhyme label="a" type="m" corresp="#s11:l1">ken</rhyme>:</l>
        <l id="s11:l3" met="3" type="-+">Ick docht, en gingh <rhyme label="b" type="f">oprijsen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Om dat ick Godt sou <rhyme label="b" type="f" corresp="#s11:l3">prijsen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ick sey; Godt moet ten allen <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Gelooft zijn, en <rhyme label="a" type="m" corresp="#s12:l1">gebenedijdt</rhyme>:</l>
        <l id="s12:l3" met="3" type="-+">Wat seght ghy hier van <rhyme label="b" type="f">menschen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Wilt ghy dit oock niet <rhyme label="b" type="f" corresp="#s12:l3">wenschen</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
