  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Frolich so willen mir <rhyme label="a" type="f">singen</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">schla dein weib / vmb denn <rhyme label="b" type="m">kop</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">mitt kl ppelenn solst  sei <rhyme label="a" type="f" corresp="#s1:l1">schmerenn</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">verdrinckenn ihr mantell vnnd <rhyme label="b" type="m" corresp="#s1:l2">rock</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">vnnd treitt sei mitt den f <rhyme label="c" type="f">ssenn</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">vnnd z gh sei bei dem <rhyme label="d" type="m">har</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">th itt ihr das <rhyme label="c" type="f" corresp="#s1:l5">verdriessenn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">ich h ir ein stim so s <rhyme label="c" type="f" corresp="#s1:l5">sse</rhyme> /</l>
        <l id="s1:l9" met="3" type="-+">vnnd gib jr einenn <rhyme label="d" type="m" corresp="#s1:l6">schlagh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vnnd schmer sei vmb die <rhyme label="a" type="f">lendenn</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">mitt einem haselenn <rhyme label="b" type="m">stock</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">stos ir denn kap woll vmb die <rhyme label="a" type="f" corresp="#s2:l1">wende</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">vnnd trett sei jn den <rhyme label="b" type="m" corresp="#s2:l2">sack</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">der schleg salst  dich <rhyme label="c" type="f">fleittenn</rhyme>/</l>
        <l id="s2:l6" met="3" type="-+">erschla sei mir zu <rhyme label="d" type="m">gar</rhyme>  /</l>
        <l id="s2:l7" met="3" type="-+">so dir woltt <rhyme label="c" type="f" corresp="#s2:l5">entrynnenn</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">der wechter auff der <rhyme label="c" type="f" corresp="#s2:l5">zinnenn</rhyme> /</l>
        <l id="s2:l9" met="3" type="-+">der nimbtt jr ebenn <rhyme label="d" type="m" corresp="#s2:l6">whair</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Also solst  sei <rhyme label="a" type="f">straffenn</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">wolst  sei haben <rhyme label="b" type="m">zam</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">Befliß dich bei deiner <rhyme label="a" type="f" corresp="#s3:l1">ehre</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Das d  sei schlagest <rhyme label="b" type="m" corresp="#s3:l2">lam</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">an henden vnd an f <rhyme label="c" type="f">ssenn</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">das sei nitt la ff dar <rhyme label="d" type="m">on</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Also salst  sei <rhyme label="c" type="f" corresp="#s3:l5">straffenn</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">so le fft sei z  keinem <rhyme label="c" type="f" corresp="#s3:l5">pfaffen</rhyme> /</l>
        <l id="s3:l9" met="3" type="-+">Dan wirtt sei dir <rhyme label="d" type="m" corresp="#s3:l6">vnderthan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Jch singenn vonn bosen <rhyme label="a" type="f">weibenn</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">Denn g thenn gahetz nitt <rhyme label="b" type="m">ahn</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Die altzeitt Buberei  <rhyme label="a" type="f" corresp="#s4:l1">dreiben</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">vnnd haben guette <rhyme label="b" type="m" corresp="#s4:l2">man</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Die sall men also <rhyme label="c" type="f">straffen</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">das sei werden <rhyme label="d" type="m">vnderthann</rhyme> /</l>
        <l id="s4:l7" met="3" type="-+">Man sall sei trewlich <rhyme label="c" type="f" corresp="#s4:l5">erhenn</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">Die stobenn mitt jn <rhyme label="c" type="f" corresp="#s4:l5">keren</rhyme> /</l>
        <l id="s4:l9" met="3" type="-+">nicht trotz vnnd laß <rhyme label="d" type="m" corresp="#s4:l6">darvon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">H ett e ch ir frome  <rhyme label="a" type="f">alle</rhyme> /</l>
        <l id="s5:l2" met="3" type="-+">f r diese <rhyme label="b" type="m">tageweis</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ob es emantz nitt <rhyme label="a" type="f" corresp="#s5:l1">gefelle</rhyme> /</l>
        <l id="s5:l4" met="3" type="-+">So habentt g etten <rhyme label="b" type="m" corresp="#s5:l2">fleiß</rhyme> /</l>
        <l id="s5:l5" met="3" type="-+">gegen ewer menner <rhyme label="c" type="f">alle</rhyme> /</l>
        <l id="s5:l6" met="3" type="-+">vnnd seitt jr <rhyme label="d" type="m">vnderthan</rhyme> /</l>
        <l id="s5:l7" met="3" type="-+">Das ir das mogenn <rhyme label="c" type="f" corresp="#s5:l5">verdragenn</rhyme> /</l>
        <l id="s5:l8" met="3" type="-+">also vbell nitt werden <rhyme label="c" type="f" corresp="#s5:l5">geschlagenn</rhyme> /</l>
        <l id="s5:l9" met="3" type="-+">Das iß altzeitt mein <rhyme label="d" type="m" corresp="#s5:l6">rath</rhyme>.</l>
      </lg>
    </lg>
  </text>
