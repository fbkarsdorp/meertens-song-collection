    <tags>
      <tag translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Zie hoe gelukkig zijn ze dan</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Overschone Maagden <rhyme label="a" type="m">beelt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die my so veele <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hebt in mijn sin <rhyme label="a" type="m" corresp="#s1:l1">gespeelt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Men vint noyt uws <rhyme label="c" type="m">gelijk</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Op het gansche <rhyme label="c" type="m" corresp="#s1:l4">aartrijk</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van Liefde ik <rhyme label="d" type="m">beswijk</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Van liefde ik <rhyme label="d" type="m" corresp="#s1:l6">beswijk</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als ik aanschouw u lieve <rhyme label="a" type="m">Mond</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">U roo Coralen <rhyme label="b" type="f">Lipjes</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En Aansigt appel <rhyme label="a" type="m" corresp="#s2:l1">rond</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">My dunkt dat gy <rhyme label="c" type="m">Diaan</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Niet behoeft te <rhyme label="c" type="m" corresp="#s2:l4">ontgaan</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Maar voor haar kan <rhyme label="d" type="m">bestaen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maar voor haar kan <rhyme label="d" type="m" corresp="#s2:l6">bestaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maar boven al prijs ik in <rhyme label="a" type="m">uw</rhyme></l>
        <l id="s3:l2" met="3" type="-+">U vrindelijk <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Weest dog voor my niet <rhyme label="a" type="m" corresp="#s3:l1">schuw</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Verhoort een reys mijn <rhyme label="c" type="m">klagt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die ik so menig <rhyme label="c" type="m" corresp="#s3:l4">Nagt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Heb uyt mijn hert <rhyme label="d" type="m">gebragt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Heb uyt mijn hert <rhyme label="d" type="m" corresp="#s3:l6">gebragt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maakt dat ik my na veel <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Nog eyndelijk <rhyme label="b" type="f">verblyde</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Laet dog noyt zijn voor <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Al 't geene dat ik <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">O Liefste Maget <rhyme label="c" type="m" corresp="#s4:l4">koen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Helpt my en wilt u <rhyme label="d" type="m">spoen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Helpt my en wilt u <rhyme label="d" type="m" corresp="#s4:l6">spoen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Of anders ik verga van <rhyme label="a" type="m">smert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Als gy my niet wil <rhyme label="b" type="f">troosten</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dan moet bersten mijn <rhyme label="a" type="m" corresp="#s5:l1">hert</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Want gy zijt daar vast <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Geplant, door soete <rhyme label="c" type="m" corresp="#s5:l4">min</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Mijn waarde Aers <rhyme label="d" type="m">Godin</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Mijn waarde Aers <rhyme label="d" type="m" corresp="#s5:l6">Godin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
