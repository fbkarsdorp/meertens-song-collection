<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184645</id>
    <date>
      <year-start>1634</year-start>
      <year-end>1634</year-end>
    </date>
    <location>
      <orginal-name>den haag</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat mag Goossen toch gebreken</melody>
  <author>Pieter Nootmans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Waer sal ick mijn Laura <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dick bekranst met <rhyme label="b" type="m">lauwer-blaen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Om met haer ten dans te <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daer sy nimmer <rhyme label="c" type="m">Coridon</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Soo verlaet, noch <rhyme label="d" type="m">af-gaet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Maer hem dickmaels kussen <rhyme label="c" type="m" corresp="#s1:l4">kon</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ach! hoe suycker-soet moet <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Als ghy lipjens-spijse <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En mijn Ziel door d'adem <rhyme label="b" type="m" corresp="#s2:l2">leeft</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Ws Nectarelijcke <rhyme label="c" type="m">mondt</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Dies Laura, segh my <rhyme label="d" type="m">dra</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Waerom ghy mijn Ziele <rhyme label="c" type="m" corresp="#s2:l4">wondt</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Of tracht ghy my <rhyme label="a" type="f">t'over-vallen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Als die wreede Vrou <rhyme label="b" type="m">Diaen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Die Actaeon dee <rhyme label="b" type="m" corresp="#s3:l2">verslaen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Om dat hy de <rhyme label="c" type="m">Sonne-schijn</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Van den dagh, naeckt <rhyme label="d" type="m">aen-sagh</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Met soo menigh <rhyme label="c" type="m" corresp="#s3:l4">Nymphelijn</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ach! hoe lieflijck tracht u <rhyme label="a" type="f">vleyen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Mijne vryheydt af te <rhyme label="b" type="m">raen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om in't klaver-bosch te <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En de kransjes <rhyme label="c" type="m">blader-groen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Die men draegt, voor een <rhyme label="d" type="m">Maegt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Sonder min-lust aen te <rhyme label="c" type="m" corresp="#s4:l4">doen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Of ghy my schoon door u <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En u mal gestreel, <rhyme label="b" type="m">gevley</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ruckte van dees <rhyme label="b" type="m" corresp="#s5:l2">Maegden-Rey</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En ick Hymens Wet <rhyme label="c" type="m">vol-dee</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Soud' ghy niet mijn <rhyme label="d" type="m">gebiedt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Van u Laura volgen <rhyme label="c" type="m" corresp="#s5:l4">mee</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Al wat Laura wil <rhyme label="a" type="f">gebieden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Sal mijn grage graegt <rhyme label="b" type="m">vol-doen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Om my nae u wil te <rhyme label="b" type="m" corresp="#s6:l2">spoen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En u teder <rhyme label="c" type="m">Lammer-vlock</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Tot mijn graff duyn-waerts <rhyme label="d" type="m">aff</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Drijven met mijn <rhyme label="c" type="m" corresp="#s6:l4">Harders-stock</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dat is, dat ick u sal <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Met een reyne <rhyme label="b" type="m">weder-min</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Soo langh als u Zieltjen <rhyme label="b" type="m" corresp="#s7:l2">in</rhyme></l>
        <l id="s7:l4" met="4" type="+-">'tSchoone lichaem is <rhyme label="c" type="m">gesteldt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">En ick jou tot mijn <rhyme label="d" type="m">Vrou</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Krijten sal doo al het <rhyme label="c" type="m" corresp="#s7:l4">veldt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">'tSa wel an, u lieve <rhyme label="a" type="f">lusjes</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Sal ick Coridon <rhyme label="b" type="m">vol-doen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En u Zieltje weder <rhyme label="b" type="m" corresp="#s8:l2">voen</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Met u Lauraes lieve <rhyme label="c" type="m">lust</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Coridon, lieve <rhyme label="d" type="m">Son</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Hier op werdt ghy dan <rhyme label="c" type="m" corresp="#s8:l4">gekust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
