    <melody>Guillot dit partout ma fille</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wilt Dominicum doch <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Singht met vreughden sijnen <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die soo grooten vriendt des <rhyme label="a" type="f" corresp="#s1:l1">Heeren</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Is bekent in 't eeuwigh <rhyme label="b" type="m" corresp="#s1:l2">hof</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s1:l5">verdwenen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s1:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die de zielen verr' <rhyme label="a" type="f">gheweken</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Van Godts liefd' aen alle <rhyme label="b" type="m">kant</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als een fackel heeft <rhyme label="a" type="f" corresp="#s2:l1">ontsteken</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Met den Goddelijcken <rhyme label="b" type="m" corresp="#s2:l2">brandt</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s2:l5">verdwenen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s2:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die Mariam seer <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En haer liefd' alom <rhyme label="b" type="m">ontstack</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Nacht en dagh haer vierigh <rhyme label="a" type="f" corresp="#s3:l1">diende</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dus haer jonst hem noot <rhyme label="b" type="m" corresp="#s3:l2">ontbrack</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s3:l5">verdwenen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s3:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'t Roose-cransken en de <rhyme label="a" type="f">vruchten</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Heeft de Maeght hem gh' <rhyme label="b" type="m">openbaert</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Daer hy me de sond' de <rhyme label="a" type="f" corresp="#s4:l1">vluchten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Soo veel menschen heeft <rhyme label="b" type="m" corresp="#s4:l2">bewaert</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s4:l5">verdwenen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s4:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Broederschap oock op doen <rhyme label="a" type="f">stellen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Van den Crans heeft hem de <rhyme label="b" type="m">Maeght</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Waer hy me 't ghespuys der <rhyme label="a" type="f" corresp="#s5:l1">hellen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Heeft veel zielen <rhyme label="b" type="m" corresp="#s5:l2">afghejaeght</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s5:l5">verdwenen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s5:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die van zielen te <rhyme label="a" type="f">bekeeren</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Maeckte nach en dagh sijn <rhyme label="b" type="m">werck</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Die versien van <rhyme label="a" type="f" corresp="#s6:l1">Predick-heeren</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Heeft allom de Christen <rhyme label="b" type="m" corresp="#s6:l2">Kerck</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s6:l5">verdwenen</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s6:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die de Kettery <rhyme label="a" type="f">verdreven</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Heeft 'tgheloof allom <rhyme label="b" type="m">gheplant</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En de deughden doen <rhyme label="a" type="f" corresp="#s7:l1">herleven</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Groeien seer aen alle <rhyme label="b" type="m" corresp="#s7:l2">kant</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s7:l5">verdwenen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s7:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Die veel sieken heeft <rhyme label="a" type="f">ghenesen</rhyme></l>
        <l id="s8:l2" met="4" type="+-">En soo verr' sijn macht <rhyme label="b" type="m">verstrect</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dat dry dooden zijn <rhyme label="a" type="f" corresp="#s8:l1">verresen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Door 't ghebedt van hem <rhyme label="b" type="m" corresp="#s8:l2">verweckt</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s8:l5">verdwenen</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s8:l6">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wilt door hem nu oock <rhyme label="a" type="f">verlichten</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Onse herten, soetsten <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Door sijn heylich leven <rhyme label="a" type="f" corresp="#s9:l1">stichten</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dat u heeft behaeght soo <rhyme label="b" type="m" corresp="#s9:l2">seer</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Als een baeck die heeft <rhyme label="c" type="f">gheschenen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">En de wereldt seer <rhyme label="d" type="m">verlicht</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Dat de dolingh is <rhyme label="c" type="f" corresp="#s9:l5">verdwenen</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Godt ghecomen in 't <rhyme label="d" type="m" corresp="#s9:l6">ghesicht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
