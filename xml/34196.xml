  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Lest sagh ick Gallathé in't <rhyme label="a" type="m">veldt</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Met haeren vrindt Leander 'tsamen <rhyme label="b" type="f">kouten</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ick docht, ick moght my wel <rhyme label="b" type="f" corresp="#s1:l2">verstouten</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Om aen te hooren wat hun reden <rhyme label="a" type="m" corresp="#s1:l1">meldt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ick hoorde die schoon <rhyme label="c" type="m">Herderin</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Leander vraghen met een soeten <rhyme label="c" type="m" corresp="#s1:l5">sin</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Magh ick u <rhyme label="d" type="f">Herder</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Eer ghy gaet <rhyme label="d" type="f" corresp="#s1:l7">verder</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Wenschen veel <rhyme label="e" type="m">gheluck</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Al syn vreught <rhyme label="d" type="f" corresp="#s1:l7">verwerder</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Daer terstont in <rhyme label="e" type="m" corresp="#s1:l9">druck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Leander die was heel <rhyme label="a" type="m">beroert</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Als hy sy lief dees woorden hoorden <rhyme label="b" type="f">spreken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En vraeghde wat zyn dat voor <rhyme label="b" type="f" corresp="#s2:l2">treken</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Myn hert dat wordt schier van my wegh <rhyme label="a" type="m" corresp="#s2:l1">ghevoert</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Door 'tgheen ghy Gallathé daer <rhyme label="c" type="m">seght</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Soudt ghy bedroeven uwen trouwen <rhyme label="c" type="m" corresp="#s2:l5">knecht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Want ick <rhyme label="d" type="f">Leander</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Bemin gheen <rhyme label="d" type="f" corresp="#s2:l7">ander</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Dan u <rhyme label="e" type="m">Gallathé</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Laet ons met <rhyme label="d" type="f" corresp="#s2:l7">malkander</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">'tSaemen zyn in <rhyme label="e" type="m" corresp="#s2:l9">vré</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Neen, ghy mint noch een ander <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Dat weet ghy 'tbest, wat wil ick voorder <rhyme label="b" type="f">segghen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Vrindin wat wilt ghy my <rhyme label="b" type="f" corresp="#s3:l2">op-legghen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Ghy zyt alleen die myn jongh hert <rhyme label="a" type="m" corresp="#s3:l1">behaeght</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Herder hoe spreeckt ghy soo <rhyme label="c" type="m">gheveynst</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Eer ghy dat seght u reden <rhyme label="c" type="m" corresp="#s3:l5">over-peynst</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Ghy hebt haer <rhyme label="d" type="f">vonden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Onder die <rhyme label="d" type="f" corresp="#s3:l7">ronde</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Daer soo menigh <rhyme label="e" type="m">vee</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Ghedeelt wordt, en <rhyme label="d" type="f" corresp="#s3:l7">verslonden</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Kunt ghy segghen <rhyme label="e" type="m" corresp="#s3:l9">neen</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Jupeyn sendt my u straffen <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">En plaeght my hier, sprack heel bedroeft <rhyme label="b" type="f">Leander</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Bemin ick elders noch een <rhyme label="b" type="f" corresp="#s4:l2">ander</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Laet my te niet naer Carons Schuyt toe <rhyme label="a" type="m" corresp="#s4:l1">gaen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Bemin ick meer dan <rhyme label="c" type="m">Gallathé</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Vol-brenght u straf, ick ben seer wel te <rhyme label="c" type="m" corresp="#s4:l5">vré</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Want al myn <rhyme label="d" type="f">klachten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Helaes! maer <rhyme label="d" type="f" corresp="#s4:l7">trachten</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Maeght naer u schoon <rhyme label="e" type="m">beldt</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">d'Welck door minne <rhyme label="d" type="f" corresp="#s4:l7">krachten</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">My heeft 'thert <rhyme label="e" type="m" corresp="#s4:l9">ontstelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick was beweeght door syn <rhyme label="a" type="m">vermaen</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Maer doen begonst Leander voorts te <rhyme label="b" type="f">spreken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">O nyders wat zyn dat voor <rhyme label="b" type="f" corresp="#s5:l2">treken</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Ach! helsche tonghen wat heb ick <rhyme label="a" type="m" corresp="#s5:l1">mis-daen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dat ghy wilt door u boos <rhyme label="c" type="m">venyn</rhyme>,</l>
        <l id="s5:l6" met="5" type="-+">Twee vrinden 'tsamen sien ghescheyden <rhyme label="c" type="m" corresp="#s5:l5">zyn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wat sou u <rhyme label="d" type="f">baeten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Als ick sou <rhyme label="d" type="f" corresp="#s5:l7">laeten</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Myn schoon <rhyme label="e" type="m">Herderin</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Dat ghy komt <rhyme label="d" type="f" corresp="#s5:l7">behaeten</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Onse soete <rhyme label="e" type="m" corresp="#s5:l9">min</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Princes ghelooft gheen Nyders <rhyme label="a" type="m">noyt</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Laet ons in vreucht ons vee naer huys <rhyme label="b" type="f">toe-leyden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ick blyft u dienaer t'alder <rhyme label="b" type="f" corresp="#s6:l2">tyden</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Tot dat ick sterven sal de bitter <rhyme label="a" type="m" corresp="#s6:l1">doot</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">'kBedanck Dorinde u <rhyme label="c" type="m">Vrindin</rhyme>,</l>
        <l id="s6:l6" met="5" type="-+">Die niet ghelooft heeft dat ick myne <rhyme label="c" type="m" corresp="#s6:l5">min</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Van u sou <rhyme label="d" type="f">scheyden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Ach! tot gheender <rhyme label="d" type="f" corresp="#s6:l7">tyden</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Myn soete <rhyme label="e" type="m">party</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Wilt my nu <rhyme label="d" type="f" corresp="#s6:l7">verblyden</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Gunt u gonst aen <rhyme label="e" type="m" corresp="#s6:l9">my</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
