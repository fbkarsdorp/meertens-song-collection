<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100188</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Jacques</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALdersoetsten <rhyme label="a" type="f">Honichraetjen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Tu es mon Petit coeur <rhyme label="b" type="m">doux</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ben ick niet u beste <rhyme label="a" type="f" corresp="#s1:l1">maetjen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Ach! je voudroy bien pour <rhyme label="b" type="m" corresp="#s1:l2">vous</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Eens te troetlen met genucht <rhyme label="c" type="f">verhoetlen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Vne piece de fix <rhyme label="b" type="m" corresp="#s1:l2">soulz</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">VVaerom wiltge my niet <rhyme label="a" type="f">hebben</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Belle, scais tu bien <rhyme label="b" type="m">Pourquoy</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Dunck ick't u te slecht te <rhyme label="a" type="f" corresp="#s2:l1">hebben</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">N'ay je point assez de <rhyme label="b" type="m" corresp="#s2:l2">quoy</rhyme>?</l>
        <l id="s2:l5" met="5" type="+-">Ba! myn schatten, zyn wel twintich <rhyme label="c" type="f">matten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Tous pesants et bons d' <rhyme label="b" type="m" corresp="#s2:l2">alloy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">VVenstge niet te zyn <rhyme label="a" type="f">gekoppelt</rhyme></l>
        <l id="s3:l2" met="4" type="+-">A un tout-fidelle <rhyme label="b" type="m">amant</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die wel dichtjens is <rhyme label="a" type="f" corresp="#s3:l1">bestoppelt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Et fournu d' argent <rhyme label="b" type="m" corresp="#s3:l2">contant</rhyme>,</l>
        <l id="s3:l5" met="5" type="+-">Om met schyven, U de Beurs te <rhyme label="c" type="f">styven</rhyme></l>
        <l id="s3:l6" met="4" type="+-">A jouër Rogier bon <rhyme label="b" type="m" corresp="#s3:l2">temps</rhyme>;</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Mint me vry dan ; neemt den wil <rhyme label="a" type="f">aen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">De vostre humble <rhyme label="b" type="m">serviteur</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ick sal op myn rechter bil <rhyme label="a" type="f" corresp="#s4:l1">slaen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Et diray sautant del' <rhyme label="b" type="m" corresp="#s4:l2">heur</rhyme></l>
        <l id="s4:l5" met="5" type="+-">Vast een reysjen, wel-geminde <rhyme label="c" type="f">vleysje</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Et 't appelleray mon <rhyme label="b" type="m" corresp="#s4:l2">coeur</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ach hoe wou ick u dan <rhyme label="a" type="f">streelen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Jour et nuict Pour <rhyme label="b" type="m">tesmoigner</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Dat ick kan het Scheur-buyck <rhyme label="a" type="f" corresp="#s5:l1">heelen</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Et au bois d'amour <rhyme label="b" type="m" corresp="#s5:l2">chaffer</rhyme>,</l>
        <l id="s5:l5" met="5" type="+-">Daer geen Ruyntjen, of gelukt <rhyme label="c" type="f">Capuyntjen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Peut auec sa Lance <rhyme label="b" type="m" corresp="#s5:l2">entrer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">A Dieu Liefste noch tot <rhyme label="a" type="f">morghen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Je vous donne à <rhyme label="b" type="m">Consulter</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Langher kan ick u niet <rhyme label="a" type="f" corresp="#s6:l1">borghen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Car la froide <rhyme label="b" type="m" corresp="#s6:l2">My-hiuer</rhyme></l>
        <l id="s6:l5" met="5" type="+-">Doetme loopen, om een wijf te <rhyme label="c" type="f">koopen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Chou, j'ay peur de'm <rhyme label="b" type="m" corresp="#s6:l2">engeler</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
