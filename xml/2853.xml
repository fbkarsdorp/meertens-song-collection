  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Schoen lief <rhyme label="a" type="f">gepreesen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Ick gae vol <rhyme label="a" type="f" corresp="#s1:l1">vreesen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">als de voerweesen <rhyme label="b" type="f">naesy</rhyme></l>
        <l id="s1:l4" met="2" type="-+">rein uit <rhyme label="a" type="f" corresp="#s1:l1">geleesen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">wilt midn <rhyme label="a" type="f" corresp="#s1:l1">geneesen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">verleent doer deesen <rhyme label="b" type="f" corresp="#s1:l3">graecy</rhyme></l>
        <l id="s1:l7" met="3" type="-+">U lief die is <rhyme label="c" type="m">belaen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">met swaer <rhyme label="d" type="f">doelperen</rhyme></l>
        <l id="s1:l9" met="3" type="+-">heeft mijn Jonckheit <rhyme label="c" type="m" corresp="#s1:l7">bevaen</rhyme></l>
        <l id="s1:l10" met="3" type="-+">u schoenheit wijt <rhyme label="c" type="m" corresp="#s1:l7">ondaen</rhyme></l>
        <l id="s1:l11" met="3" type="-+">Ick kandt niet weeder <rhyme label="c" type="m" corresp="#s1:l7">staen</rhyme></l>
        <l id="s1:l12" met="2" type="-+">u venes <rhyme label="d" type="f" corresp="#s1:l8">kueren</rhyme></l>
        <l id="s1:l13" met="2" type="-+">waer sal Ick <rhyme label="d" type="f" corresp="#s1:l8">dueren</rhyme></l>
        <l id="s1:l14" met="3" type="-+">schoen liefken mijn <rhyme label="e" type="m">torment</rhyme></l>
        <l id="s1:l15" met="3" type="-+">verlichten maeckt <rhyme label="e" type="m" corresp="#s1:l14">bekent</rhyme></l>
        <l id="s1:l16" met="3" type="-+">u goetheit sonder <rhyme label="e" type="m" corresp="#s1:l14">ent</rhyme></l>
        <l id="s1:l17" met="2" type="-+">tot deeser <rhyme label="d" type="f" corresp="#s1:l8">ueren</rhyme>.</l>
      </lg>
