    <tags>
      <tag translation="farewell song">afscheidslied</tag>
      <tag translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Ik zat en fantaseerde</melody>
    <categories>
      <category translation="occasions">gelegenheden</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Och hoe benaut <rhyme label="a" type="f">bevinde</rhyme></l>
        <l id="s1:l2" met="3" type="-+">ick my aldus <rhyme label="b" type="m">subyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">van smeten en <rhyme label="a" type="f" corresp="#s1:l1">ellinde</rhyme></l>
        <l id="s1:l4" met="3" type="-+">myn hert inwendich <rhyme label="b" type="m" corresp="#s1:l2">splyt</rhyme></l>
        <l id="s1:l5" met="5" type="-+">want ick helaes nu moet / tis <rhyme label="c" type="m">onsoet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">voor een minnaer <rhyme label="d" type="m">getrouw</rhyme></l>
        <l id="s1:l7" met="5" type="-+">scheyden dwelck my met spoet / treuren <rhyme label="c" type="m" corresp="#s1:l5">doet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">van u liefste <rhyme label="d" type="m" corresp="#s1:l6">jonckurouw</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nu comt den tyt <rhyme label="a" type="f">voorleden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">my eest in het <rhyme label="b" type="m">gedacht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">hoe lieffelyck ick met <rhyme label="a" type="f" corresp="#s2:l1">reden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">u aensprack dach en <rhyme label="b" type="m" corresp="#s2:l2">nacht</rhyme></l>
        <l id="s2:l5" met="5" type="-+">en v mondeken root / kusten <rhyme label="c" type="m">bloot</rhyme></l>
        <l id="s2:l6" met="3" type="-+">in amoereusheyt <rhyme label="d" type="m">reyn</rhyme></l>
        <l id="s2:l7" met="5" type="-+">ghy toonde v minioot / in myn <rhyme label="c" type="m" corresp="#s2:l5">noot</rhyme></l>
        <l id="s2:l8" met="3" type="-+">inde vreucht o edel <rhyme label="d" type="m" corresp="#s2:l6">greyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Myn liefde seer <rhyme label="a" type="f">ghestadich</rhyme></l>
        <l id="s3:l2" met="3" type="-+">is u oock wel <rhyme label="b" type="m">bekent</rhyme></l>
        <l id="s3:l3" met="3" type="-+">hoe ick altyt <rhyme label="a" type="f" corresp="#s3:l1">weldadich</rhyme></l>
        <l id="s3:l4" met="3" type="-+">my tuwaerts heb <rhyme label="b" type="m" corresp="#s3:l2">gewent</rhyme></l>
        <l id="s3:l5" met="5" type="-+">seer dickwils oock gegaen / seer <rhyme label="c" type="m">belaen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">deur reghen en deur <rhyme label="d" type="m">wint</rhyme></l>
        <l id="s3:l7" met="5" type="-+">om v te doen verstaen / wel <rhyme label="c" type="m" corresp="#s3:l5">beraen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">hoe myn hert was <rhyme label="d" type="m" corresp="#s3:l6">gesint</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gheen moeyte ofte <rhyme label="a" type="f">pyne</rhyme></l>
        <l id="s4:l2" met="3" type="-+">en heb ick oock <rhyme label="b" type="m">gespaert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">om v tallen <rhyme label="a" type="f" corresp="#s4:l1">termyne</rhyme></l>
        <l id="s4:l4" met="3" type="-+">hoe ick oock was <rhyme label="b" type="m" corresp="#s4:l2">beswaert</rhyme></l>
        <l id="s4:l5" met="5" type="-+">te complaceren bly / want tdocht / <rhyme label="c" type="m">my</rhyme></l>
        <l id="s4:l6" met="3" type="-+">te syn een groot <rhyme label="d" type="m">plaisier</rhyme></l>
        <l id="s4:l7" met="5" type="-+">als ick met melody / iet brocht / <rhyme label="c" type="m" corresp="#s4:l5">by</rhyme></l>
        <l id="s4:l8" met="3" type="-+">dat v verheuchden <rhyme label="d" type="m" corresp="#s4:l6">sier</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Al hebben clappaerts <rhyme label="a" type="f">tonghen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">somtyts haer saet <rhyme label="b" type="m">gesaeyt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ick heb u <rhyme label="a" type="f" corresp="#s5:l1">onbedwongen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">met reden weer <rhyme label="b" type="m" corresp="#s5:l2">gepaeyt</rhyme></l>
        <l id="s5:l5" met="5" type="-+">dat haer fenynich soch / vol <rhyme label="c" type="m">bedroch</rhyme></l>
        <l id="s5:l6" met="3" type="-+">ons niet en heeft <rhyme label="d" type="m">geschaet</rhyme></l>
        <l id="s5:l7" met="5" type="-+">al was tbegin quaet och / int lest <rhyme label="c" type="m" corresp="#s5:l5">doch</rhyme></l>
        <l id="s5:l8" met="3" type="-+">wert bespot haeren <rhyme label="d" type="m" corresp="#s5:l6">praet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Daerom blyft doch <rhyme label="a" type="f">volherdich</rhyme></l>
        <l id="s6:l2" met="3" type="-+">en in u herte <rhyme label="b" type="m">prent</rhyme></l>
        <l id="s6:l3" met="3" type="-+">myn trouwicheyt wel <rhyme label="a" type="f" corresp="#s6:l1">weerdich</rhyme></l>
        <l id="s6:l4" met="3" type="-+">te syn geloont int <rhyme label="b" type="m" corresp="#s6:l2">ent</rhyme></l>
        <l id="s6:l5" met="5" type="-+">al moet ick lieffste nu / laeten <rhyme label="c" type="m">v</rhyme></l>
        <l id="s6:l6" met="3" type="-+">den tyt noch comen <rhyme label="d" type="m">mach</rhyme></l>
        <l id="s6:l7" met="5" type="-+">dat ghy my snel u trouw / sonder <rhyme label="c" type="m" corresp="#s6:l5">rouw</rhyme></l>
        <l id="s6:l8" met="3" type="-+">noch schencken met <rhyme label="d" type="m" corresp="#s6:l6">verdrach</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Adieu ick gae nu <rhyme label="a" type="f">reysen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">maer thert blyft u te <rhyme label="b" type="m">pant</rhyme></l>
        <l id="s7:l3" met="3" type="-+">princesse wilt eens <rhyme label="a" type="f" corresp="#s7:l1">peysen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">als ick sy uyt het <rhyme label="b" type="m" corresp="#s7:l2">lant</rhyme></l>
        <l id="s7:l5" met="5" type="-+">op my uwen dienaer / die <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">v vryt ter eeren <rhyme label="d" type="m">fyn</rhyme></l>
        <l id="s7:l7" met="5" type="-+">oorloff neem ick eenpaer / tvalt my <rhyme label="c" type="m" corresp="#s7:l5">swaer</rhyme></l>
        <l id="s7:l8" met="3" type="-+">maer tmoet nu alsoo <rhyme label="d" type="m" corresp="#s7:l6">syn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
