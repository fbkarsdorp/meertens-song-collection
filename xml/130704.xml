    <melody>Daar waren twee wijfjes die klopten malkaar</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Te Gent daer staet â€˜er een Klooster <rhyme label="a" type="m">klein</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Daer alle de fijne nonnen in <rhyme label="a" type="m" corresp="#s1:l1">zijn</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Van voren, Van   <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Men Heer de Rammelaer   komt t' avond   <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Hy brengt 'er wel seventien Ramelaers <rhyme label="a" type="m" corresp="#s2:l1">mee</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">Van voren, Van <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy lei het Nonnetje al op een <rhyme label="a" type="m">ton</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hy hakte dat 'er de boom   uit <rhyme label="a" type="m" corresp="#s3:l1">sprong</rhyme>.</l>
        <l id="s3:l3" met="2" type="-+">Van voren, Van <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy lei het Nonnetje al op een <rhyme label="a" type="m">Plank</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hy soende wel sesmael om een <rhyme label="a" type="m" corresp="#s4:l1">blank</rhyme>.</l>
        <l id="s4:l3" met="2" type="-+">Van voren, Van <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy lei het Nonnetje al op een <rhyme label="a" type="m">sluis</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Hy gaf 'er wat in haer <rhyme label="a" type="m" corresp="#s5:l1">kombuis</rhyme>.</l>
        <l id="s5:l3" met="2" type="-+">Van voren, Van <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Het Nonnetje klom 'er de trappen <rhyme label="a" type="m">op</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">De Paep die keek 'er van onderen <rhyme label="a" type="m" corresp="#s6:l1">op</rhyme>.</l>
        <l id="s6:l3" met="2" type="-+">Van voren, Van <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy lei het Nonnetje op een blaeuw <rhyme label="a" type="m">steen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Al was se maget, se bleef <rhyme label="a" type="m" corresp="#s7:l1">alleen</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Van voren, Van <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die ons dat nieuw Lied heeft <rhyme label="a" type="m">gedigt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Het was een kwantje, zijn hartje was <rhyme label="a" type="m" corresp="#s8:l1">ligt</rhyme>.</l>
        <l id="s8:l3" met="2" type="-+">Van voren, Van <rhyme label="b" type="f">voren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
