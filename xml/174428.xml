  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Adieu voorleden <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Inden Cronijck <rhyme label="b" type="m">gestelt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Godt heeft hollant <rhyme label="a" type="m" corresp="#s1:l1">bevrijt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">al van het spaensch <rhyme label="b" type="m" corresp="#s1:l2">gewelt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">verlost zyn volck te <rhyme label="b" type="m" corresp="#s1:l2">velt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In noot blijckt <rhyme label="c" type="m">openbaer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">In noot blyckt <rhyme label="c" type="m" corresp="#s1:l6">openbaer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">de victorie <rhyme label="b" type="m" corresp="#s1:l2">telt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">van zestien hondert <rhyme label="c" type="m" corresp="#s1:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Uijt den huijs van <rhyme label="a" type="m">Nassou</rhyme></l>
        <l id="s2:l2" met="3" type="-+">heeft god verweckt een <rhyme label="b" type="m">heer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">het vaderlant <rhyme label="a" type="m" corresp="#s2:l1">getrou</rhyme></l>
        <l id="s2:l4" met="3" type="-+">bewesen altoos <rhyme label="b" type="m" corresp="#s2:l2">meer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">tot Gods loff prijs en <rhyme label="b" type="m" corresp="#s2:l2">eer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">veel steden hier en <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">veel steden hier en <rhyme label="c" type="m" corresp="#s2:l6">daer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">geplant de rechte <rhyme label="b" type="m" corresp="#s2:l2">leer</rhyme></l>
        <l id="s2:l9" met="3" type="-+">van zestien hondert <rhyme label="c" type="m" corresp="#s2:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Aensiet de cleijne <rhyme label="a" type="m">Macht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">tegen veel landen <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s3:l3" met="3" type="-+">zult mercken dat gods <rhyme label="a" type="m" corresp="#s3:l1">cracht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">den zynen helpt In <rhyme label="b" type="m" corresp="#s3:l2">noot</rhyme></l>
        <l id="s3:l5" met="3" type="-+">tbleeck aen de spaensche <rhyme label="b" type="m" corresp="#s3:l2">vloot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">die verstroeyden te <rhyme label="c" type="m">gaer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">die verstroeyden te <rhyme label="c" type="m" corresp="#s3:l6">gaer</rhyme></l>
        <l id="s3:l8" met="3" type="-+">veel tyrannen <rhyme label="b" type="m" corresp="#s3:l2">gedoot</rhyme></l>
        <l id="s3:l9" met="3" type="-+">In zestien hondert <rhyme label="c" type="m" corresp="#s3:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Veracht als <rhyme label="a" type="m">Bethliem</rhyme></l>
        <l id="s4:l2" met="3" type="-+">zoo was Int eerste <rhyme label="b" type="m">hollant</rhyme></l>
        <l id="s4:l3" met="3" type="-+">mer gods roepende <rhyme label="a" type="m" corresp="#s4:l1">stem</rhyme></l>
        <l id="s4:l4" met="3" type="-+">zyn woord alhier <rhyme label="b" type="m" corresp="#s4:l2">geplant</rhyme></l>
        <l id="s4:l5" met="3" type="-+">op dat der lieffden <rhyme label="b" type="m" corresp="#s4:l2">brant</rhyme></l>
        <l id="s4:l6" met="3" type="-+">ontsteecken zou voor <rhyme label="c" type="m">waer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">ontsteecken zou voor <rhyme label="c" type="m" corresp="#s4:l6">waer</rhyme></l>
        <l id="s4:l8" met="3" type="-+">verwonnen den <rhyme label="b" type="m" corresp="#s4:l2">vijant</rhyme></l>
        <l id="s4:l9" met="3" type="-+">In zestien hondert <rhyme label="c" type="m" corresp="#s4:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Tis noch het selve <rhyme label="a" type="m">licht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">gesien bij gods <rhyme label="b" type="m">geboort</rhyme></l>
        <l id="s5:l3" met="3" type="-+">geen Fabulen meer <rhyme label="a" type="m" corresp="#s5:l1">dicht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">die stryden tegen <rhyme label="b" type="m" corresp="#s5:l2">dwoort</rhyme></l>
        <l id="s5:l5" met="3" type="-+">den armen besouckt <rhyme label="b" type="m" corresp="#s5:l2">voort</rhyme></l>
        <l id="s5:l6" met="3" type="-+">in davits stal zeer <rhyme label="c" type="m">naer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">in Davits stal zeer <rhyme label="c" type="m" corresp="#s5:l6">naer</rhyme></l>
        <l id="s5:l8" met="3" type="-+">blyft In lieffden <rhyme label="b" type="m" corresp="#s5:l2">accoort</rhyme></l>
        <l id="s5:l9" met="3" type="-+">van zestien hondert <rhyme label="c" type="m" corresp="#s5:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Eendracht heeft <rhyme label="a" type="m">principael</rhyme></l>
        <l id="s6:l2" met="3" type="-+">hollant gemaeckt zo <rhyme label="b" type="m">sterck</rhyme></l>
        <l id="s6:l3" met="3" type="-+">voorspoet Int <rhyme label="a" type="m" corresp="#s6:l1">generael</rhyme></l>
        <l id="s6:l4" met="3" type="-+">den segen is gods <rhyme label="b" type="m" corresp="#s6:l2">werck</rhyme></l>
        <l id="s6:l5" met="3" type="-+">bevryt altoos zijn <rhyme label="b" type="m" corresp="#s6:l2">kerck</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Om crincken nyet een <rhyme label="c" type="m">haer</rhyme></l>
        <l id="s6:l7" met="3" type="-+">om crincken nyet een <rhyme label="c" type="m" corresp="#s6:l6">haer</rhyme></l>
        <l id="s6:l8" met="3" type="-+">verblint, neemt doch eens <rhyme label="b" type="m" corresp="#s6:l2">merck</rhyme></l>
        <l id="s6:l9" met="3" type="-+">van zestien hondert <rhyme label="c" type="m" corresp="#s6:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Adieu Iaer van <rhyme label="a" type="m">wonder</rhyme></l>
        <l id="s7:l2" met="3" type="-+">ghy hebt den roep <rhyme label="b" type="m">alleen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">vanden Paus <rhyme label="a" type="m" corresp="#s7:l1">bijsonder</rhyme></l>
        <l id="s7:l4" met="3" type="-+">mer voor nieupoort van <rhyme label="b" type="m" corresp="#s7:l2">geen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">menige blaeuwe <rhyme label="b" type="m" corresp="#s7:l2">scheen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">stiet elck paus <rhyme label="c" type="m">pilaer</rhyme></l>
        <l id="s7:l7" met="3" type="-+">stiet elck paus <rhyme label="c" type="m" corresp="#s7:l6">pilaer</rhyme></l>
        <l id="s7:l8" met="3" type="-+">met peerden over <rhyme label="b" type="m" corresp="#s7:l2">een</rhyme></l>
        <l id="s7:l9" met="3" type="-+">zulck loon, gaff t'gulden <rhyme label="c" type="m" corresp="#s7:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Den Prince, die hooch <rhyme label="a" type="m">sit</rhyme></l>
        <l id="s8:l2" met="3" type="-+">es Nassou wel <rhyme label="b" type="m">bekent</rhyme></l>
        <l id="s8:l3" met="3" type="-+">die oprecht schiet nae <rhyme label="a" type="m" corresp="#s8:l1">twit</rhyme></l>
        <l id="s8:l4" met="3" type="-+">want lieffd es t <rhyme label="b" type="m" corresp="#s8:l2">Fondament</rhyme></l>
        <l id="s8:l5" met="3" type="-+">zijn vijanden <rhyme label="b" type="m" corresp="#s8:l2">ontrent</rhyme></l>
        <l id="s8:l6" met="3" type="-+">doet hy versuchten <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s8:l7" met="3" type="-+">doet hy versuchten <rhyme label="c" type="m" corresp="#s8:l6">swaer</rhyme></l>
        <l id="s8:l8" met="3" type="-+">geen accoleijen <rhyme label="b" type="m" corresp="#s8:l2">schent</rhyme></l>
        <l id="s8:l9" met="3" type="-+">nae sestien hondert <rhyme label="c" type="m" corresp="#s8:l6">Iaer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
