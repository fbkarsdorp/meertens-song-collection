  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAer wasser een dolle <rhyme label="a" type="f">Begijntje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Smorgens vroeg <rhyme label="b" type="m">opgestaen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Drie uertjes voor den <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Ia dagen ja <rhyme label="c" type="f" corresp="#s1:l3">dagen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Drie ueren voor den <rhyme label="c" type="f" corresp="#s1:l3">dagen</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Naer Meggelen soude sy <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Godt groet u sey sy mijn <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met uwen <rhyme label="b" type="m">Kappelaen</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Ik soude u so gaeren <rhyme label="c" type="f">Biegten</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Ia biegten, ja <rhyme label="c" type="f" corresp="#s2:l3">biegten</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ick souder soo gaeren <rhyme label="c" type="f" corresp="#s2:l3">Biegten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met sonden ben ick <rhyme label="b" type="m" corresp="#s2:l2">belaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soudt gy so gaeren <rhyme label="a" type="f">biechten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zijt gy met sonden <rhyme label="b" type="m">belaen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Kom knielt hier voor mijn <rhyme label="c" type="f">neder</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ia neder, ja <rhyme label="c" type="f" corresp="#s3:l3">neder</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Komt knielt hier voor mijn <rhyme label="c" type="f" corresp="#s3:l3">neder</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En segt wat gy hebt <rhyme label="b" type="m" corresp="#s3:l2">gedaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick hebber niet te <rhyme label="a" type="f">biegten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Niet als van een <rhyme label="b" type="m">Iongman</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die my noch gisteren <rhyme label="c" type="f">avont</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Ia avont, ja <rhyme label="c" type="f" corresp="#s4:l3">avont</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die my noch gisteren <rhyme label="c" type="f" corresp="#s4:l3">avont</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In beyde sijn armen <rhyme label="b" type="m" corresp="#s4:l2">nam</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sy knielden daer voor hem <rhyme label="a" type="f">neder</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Al in dat groene <rhyme label="b" type="m">gras</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En hy gaf haer de <rhyme label="c" type="f">benedictje</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Ia dictje, ja <rhyme label="c" type="f" corresp="#s5:l3">dictje</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hy gafse de <rhyme label="c" type="f" corresp="#s5:l3">benedictie</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daer sy van gekomen <rhyme label="b" type="m" corresp="#s5:l2">was</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al heeft hy by mijn <rhyme label="a" type="f">geslapen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hy heeft mijn niet <rhyme label="b" type="m">misdaen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hy genas my van die <rhyme label="c" type="f">Tantpijn</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Buyckpijn en <rhyme label="c" type="f" corresp="#s6:l3">Scheurmont</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Hy genas my van de <rhyme label="c" type="f" corresp="#s6:l3">Scheurmont</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Daer ick so sieck of <rhyme label="b" type="m" corresp="#s6:l2">was</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
