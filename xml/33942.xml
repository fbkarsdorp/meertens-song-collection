      <tag>biblical history song</tag>
    </tags>
    <melody>Zoet Robbertje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">IEsus <rhyme label="a" type="f">passeerde</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Door Jericho, daer <rhyme label="b" type="m">quam</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Een die <rhyme label="a" type="f" corresp="#s1:l1">begeerde</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hem eens te sien, die <rhyme label="b" type="m" corresp="#s1:l2">klam</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Op eenen Boom die welk daer <rhyme label="c" type="m">stond'</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Want anders hy niet sien en <rhyme label="c" type="m" corresp="#s1:l5">kond'</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Vermids den grooten <rhyme label="d" type="m">treyn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Want sijn statuer was <rhyme label="d" type="m" corresp="#s1:l7">kleyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hy was seer <rhyme label="a" type="f">rijke</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hy was een <rhyme label="b" type="m">Tollenaer</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">En in 's <rhyme label="a" type="f" corresp="#s2:l1">gelijke</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Was hy een Prins van <rhyme label="b" type="m" corresp="#s2:l2">haer</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Sijn rechte naem Zacheus <rhyme label="c" type="m">was</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Hy was voor uyt geloopen <rhyme label="c" type="m" corresp="#s2:l5">ras</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">In eenen boom sat <rhyme label="d" type="m">hy</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Daer Iesus sou <rhyme label="d" type="m" corresp="#s2:l7">voor-by</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Iesus sijn <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Opslaende, wierdt <rhyme label="b" type="m">gewaer</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Hem daer om <rhyme label="a" type="f" corresp="#s3:l1">hoogen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hy seyde, komt van <rhyme label="b" type="m" corresp="#s3:l2">daer</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">(ZacheÃ« klimt haest van den <rhyme label="c" type="m">boom</rhyme>)</l>
        <l id="s3:l6" met="4" type="-+">Want ik van daegh tot uwent <rhyme label="c" type="m" corresp="#s3:l5">koom</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En blijve daer een <rhyme label="d" type="m">tijdt</rhyme>;</l>
        <l id="s3:l8" met="3" type="-+">Zacheus werdt <rhyme label="d" type="m" corresp="#s3:l7">verblijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Hy van den <rhyme label="a" type="f">boome</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met groote haest klom <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Heeft <rhyme label="a" type="f" corresp="#s4:l1">in-genoome</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In sijnen huys den <rhyme label="b" type="m" corresp="#s4:l2">Heer</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En hierom wordt <rhyme label="c" type="m">gemurmureert</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Van yder, om dat hy <rhyme label="c" type="m" corresp="#s4:l5">verkeert</rhyme></l>
        <l id="s4:l7" met="3" type="-+">By een, die <rhyme label="d" type="m">openbaer</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Bekent was voor <rhyme label="d" type="m" corresp="#s4:l7">Sondaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Zacheus <rhyme label="a" type="f">staende</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Voor Iesum onsen <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Van sijn <rhyme label="a" type="f" corresp="#s5:l1">voorgaende</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Quaet leeven dee' <rhyme label="b" type="m" corresp="#s5:l2">bekeer</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Mijn halve goet ik d'Armen <rhyme label="c" type="m">geef</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">En soo ik ooyt onrecht <rhyme label="c" type="m" corresp="#s5:l5">bedreef</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Soo wil ik dat <rhyme label="d" type="m">viermael</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Weer-geeven tot <rhyme label="d" type="m" corresp="#s5:l7">betael</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Na deese <rhyme label="a" type="f">reeden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Heeft Iesus weer <rhyme label="b" type="m">geseydt</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Dit huys is <rhyme label="a" type="f" corresp="#s6:l1">heden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Geschiet de <rhyme label="b" type="m" corresp="#s6:l2">Saligheydt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Want hy is Abrams Soon ook <rhyme label="c" type="m">wis</rhyme>;</l>
        <l id="s6:l6" met="4" type="-+">'s Mensch Soone komen soeken <rhyme label="c" type="m" corresp="#s6:l5">is</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Het geen dat was <rhyme label="d" type="m">vergaen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Ter saligheydt <rhyme label="d" type="m" corresp="#s6:l7">t'ontfaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+"><rhyme label="a" type="f">Experienti</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Soo menighmael <rhyme label="b" type="m">verklaert</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Dat uw <rhyme label="a" type="f" corresp="#s7:l1">presenti</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">O Jesu, is seer <rhyme label="b" type="m" corresp="#s7:l2">waert</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Geluckigh is soodanigh <rhyme label="c" type="m">huys</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Waer binnen gy plant uwe <rhyme label="c" type="m" corresp="#s7:l5">kruys</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Soo waer gy binnen <rhyme label="d" type="m">woont</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Gy daer uw goetheydt <rhyme label="d" type="m" corresp="#s7:l7">toont</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
