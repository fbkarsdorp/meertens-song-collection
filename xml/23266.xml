    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">`In eenen boemgaert quam ic <rhyme label="a" type="m">ghegaen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Daer vant ic scoene vrouwen <rhyme label="a" type="m" corresp="#s1:l1">staen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sy plucten alle <rhyme label="b" type="f">roesen</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Sy plucten alle <rhyme label="b" type="f" corresp="#s1:l3">roesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">My dochte, dat my den hemel <rhyme label="a" type="m">ontsloet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Doe my die scoene een cransselijn <rhyme label="a" type="m" corresp="#s2:l1">boet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Mit hoer snee wytseer <rhyme label="b" type="f">handen</rhyme>.'</l>
        <l id="s2:l4" met="3" type="-+">Mit hoer snee wytseer <rhyme label="b" type="f" corresp="#s2:l3">handen</rhyme>.'</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een roede ridder heeft dat <rhyme label="a" type="m">vernoemen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Tot synen heer is hy <rhyme label="a" type="m" corresp="#s3:l1">ghecoemen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hy brochte soe leider <rhyme label="b" type="f">maeren</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Hy brochte soe leider <rhyme label="b" type="f" corresp="#s3:l3">maeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">`Heere,' seide hy, `heere <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dats Bruneburch draecht hoeghe <rhyme label="a" type="m" corresp="#s4:l1">moet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy slaept by dinre <rhyme label="b" type="f">vrouwen</rhyme>.'</l>
        <l id="s4:l4" met="3" type="-+">Hy slaept by dinre <rhyme label="b" type="f" corresp="#s4:l3">vrouwen</rhyme>.'</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">`Dat en gheloef ic waerlick <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dat Bruneburch my ontrou <rhyme label="a" type="m" corresp="#s5:l1">doet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hy staet my by in <rhyme label="b" type="f">noeden</rhyme>.'</l>
        <l id="s5:l4" met="3" type="-+">Hy staet my by in <rhyme label="b" type="f" corresp="#s5:l3">noeden</rhyme>.'</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die ridder die maecte syn clacht soe <rhyme label="a" type="m">groet</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat Brunenburch ghevangen <rhyme label="a" type="m" corresp="#s6:l1">wort</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Gheleit op eenen <rhyme label="b" type="f">toernen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Gheleit op eenen <rhyme label="b" type="f" corresp="#s6:l3">toernen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dat verhoerde dat vrouken <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Sy dede sadelen hoer <rhyme label="a" type="m" corresp="#s7:l1">telderkijn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sy volchde hem totter <rhyme label="b" type="f">toernen</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Sy volchde hem totter <rhyme label="b" type="f" corresp="#s7:l3">toernen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">`Bruneburch, waerom hebdy my <rhyme label="a" type="m">lief</rhyme>?</l>
        <l id="s8:l2" met="4" type="-+">Want u van my niet warden en <rhyme label="a" type="m" corresp="#s8:l1">mach</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dan reine cuusche <rhyme label="b" type="f">liefde</rhyme>.'</l>
        <l id="s8:l4" met="3" type="-+">Dan reine cuusche <rhyme label="b" type="f" corresp="#s8:l3">liefde</rhyme>.'</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">`Mocht ic hier noch ligghen seven <rhyme label="a" type="m">jaer</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Aenschouwen ju met oghens <rhyme label="a" type="m" corresp="#s9:l1">claer</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Eens daechs een woert te <rhyme label="b" type="f">spreken</rhyme>!'</l>
        <l id="s9:l4" met="3" type="-+">Eens daechs een woert te <rhyme label="b" type="f" corresp="#s9:l3">spreken</rhyme>!'</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">`Bruneburch, ic heb een <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Die my ter eeren wel houden <rhyme label="a" type="m" corresp="#s10:l1">sal</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Ic wil daer mede <rhyme label="b" type="f">ghenoeghen</rhyme>.'</l>
        <l id="s10:l4" met="3" type="-+">Ic wil daer mede <rhyme label="b" type="f" corresp="#s10:l3">ghenoeghen</rhyme>.'</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">`Mocht ic hier leggen myn leven <rhyme label="a" type="m">lanc</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Omvanghen u mit armkens <rhyme label="a" type="m" corresp="#s11:l1">blanck</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Een vriendelick kusgen <rhyme label="b" type="f">mede</rhyme>.'</l>
        <l id="s11:l4" met="3" type="-+">Een vriendelick kusgen <rhyme label="b" type="f" corresp="#s11:l3">mede</rhyme>.'</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Die ridder die maecte sijn clacht soe <rhyme label="a" type="m">groet</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Dat Bruneburch ghehangen <rhyme label="a" type="m" corresp="#s12:l1">wort</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Gheleit al totter <rhyme label="b" type="f">galgen</rhyme>.</l>
        <l id="s12:l4" met="3" type="-+">Gheleit al totter <rhyme label="b" type="f" corresp="#s12:l3">galgen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ende dat verhoerde dat vrouken <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Sy dede sadelen hoeren roes was <rhyme label="a" type="m" corresp="#s13:l1">fijn</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Sy volchde hem totten <rhyme label="b" type="f">galghen</rhyme>.</l>
        <l id="s13:l4" met="3" type="-+">Sy volchde hem totten <rhyme label="b" type="f" corresp="#s13:l3">galghen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">`Sy comt gheronnen op eenen <rhyme label="a" type="m">roes</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Daer ic den doet om sterven <rhyme label="a" type="m" corresp="#s14:l1">moet</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Al buten mynre <rhyme label="b" type="f">sculden</rhyme>.'</l>
        <l id="s14:l4" met="3" type="-+">Al buten mynre <rhyme label="b" type="f" corresp="#s14:l3">sculden</rhyme>.'</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">`Ic heb noch soeven broeders <rhyme label="a" type="m">stout</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Die sellen wel wreken uwen <rhyme label="a" type="m" corresp="#s15:l1">doot</rhyme>;</l>
        <l id="s15:l3" met="3" type="-+">Daer en sel gheen wrekens <rhyme label="b" type="f">ontbreken</rhyme>.'</l>
        <l id="s15:l4" met="3" type="-+">Daer en sel gheen wrekens <rhyme label="b" type="f" corresp="#s15:l3">ontbreken</rhyme>.'</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">`Dat nemic op mijn <rhyme label="a" type="m">heenevaert</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Dat ic hoer lijf niet sculdich en <rhyme label="a" type="m" corresp="#s16:l1">ben</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Daer ic den doot om <rhyme label="b" type="f">sterve</rhyme>.'</l>
        <l id="s16:l4" met="3" type="-+">Daer ic den doot om <rhyme label="b" type="f" corresp="#s16:l3">sterve</rhyme>.'</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">`Mijn haer sel ongevluchten <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Mijn oghens en sellen niet meer spoellen <rhyme label="a" type="m" corresp="#s17:l1">gaen</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Mijn mont en sel niet meer <rhyme label="b" type="f">lachen</rhyme>.'</l>
        <l id="s17:l4" met="3" type="-+">Mijn mont en sel niet meer <rhyme label="b" type="f" corresp="#s17:l3">lachen</rhyme>.'</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Brunenburch die gaf den <rhyme label="a" type="m">gheest</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Oft hadde gheweest eenen stommen <rhyme label="a" type="m" corresp="#s18:l1">beest</rhyme>;</l>
        <l id="s18:l3" met="3" type="-+">Het mochte een man <rhyme label="b" type="f">ontfermen</rhyme>.</l>
        <l id="s18:l4" met="3" type="-+">Het mochte een man <rhyme label="b" type="f" corresp="#s18:l3">ontfermen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
