<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>176610</id>
    <date>
      <year-start>1697</year-start>
      <year-end>1697</year-end>
    </date>
    <location>
      <orginal-name>monnikendam</orginal-name>
      <standard-name>Monnickendam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4570766</lon>
        <lat>5.0373214</lat>
      </coordinates>
    </location>
    <tags>
      <tag>shepherd's song</tag>
    </tags>
    <melody>Rozemond die lag gedoken</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SYlvia ginck Water <rhyme label="a" type="f">halen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Aen de blinde Liefs <rhyme label="b" type="m">Fonteyn</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Met so quam haar Herder <rhyme label="a" type="f" corresp="#s1:l1">dwalen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Met sijn Schaepjes in het <rhyme label="b" type="m" corresp="#s1:l2">pleyn</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat hyse aansprak <rhyme label="c" type="m">terstont</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">En Kustese voor haar roder <rhyme label="c" type="m" corresp="#s1:l5">mont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sy was stuers en wou weg <rhyme label="a" type="f">vlieden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Eer sy 't water had <rhyme label="b" type="m">gehaalt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Maar hy gink sijn gunste <rhyme label="a" type="f" corresp="#s2:l1">bieden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">En bad haar niet en <rhyme label="b" type="m" corresp="#s2:l2">smaelt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Op mijn Boere kleetje <rhyme label="c" type="m">kleen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Want het is ons dragt <rhyme label="c" type="m" corresp="#s2:l5">gemeen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Nog seyd sy 'k begeer geen <rhyme label="a" type="f">Herder</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Maar ik wil aan jonger <rhyme label="b" type="m">helt</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Darom wijkt wat <rhyme label="a" type="f" corresp="#s3:l1">verder</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En gaat dwalen in het <rhyme label="b" type="m" corresp="#s3:l2">velt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Of aan 't naest gelegen <rhyme label="c" type="m">strant</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Want ik min een ander <rhyme label="c" type="m" corresp="#s3:l5">quant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Gy sult evenwel niet <rhyme label="a" type="f">vlugten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sprack hy tot de <rhyme label="b" type="m">Herderin</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Of ik blijf in duyster <rhyme label="a" type="f" corresp="#s4:l1">sugten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Stervende in uwe <rhyme label="b" type="m" corresp="#s4:l2">min</rhyme>?</l>
        <l id="s4:l5" met="4" type="+-">Doen vermurfde hy haer <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s4:l6" met="4" type="+-">En breeg weer liefde voor sijn <rhyme label="c" type="m" corresp="#s4:l5">smart</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ik mag wel geluckig <rhyme label="a" type="f">noemen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Gy hebt my mijn Ziel <rhyme label="b" type="m">ontrooft</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ontfangt desen krans van <rhyme label="a" type="f" corresp="#s5:l1">bloemmen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Set hem op u <rhyme label="b" type="m" corresp="#s5:l2">kralend'</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Leeft met my in eer en <rhyme label="c" type="m">deugt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En hanteert slegts reyne <rhyme label="c" type="m" corresp="#s5:l5">vreugt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Sy bedankt hem voor de <rhyme label="a" type="f">gaven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">die hy haar uyt goetheyt <rhyme label="b" type="m">dee</rhyme></l>
        <l id="s6:l3" met="4" type="+-">En sy gink sijn hitte <rhyme label="a" type="f" corresp="#s6:l1">laven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En hy dreef met haar <rhyme label="b" type="m" corresp="#s6:l2">Vee</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Nog wat verder 't Bosch waart <rhyme label="c" type="m">in</rhyme></l>
        <l id="s6:l6" met="4" type="+-">daar koelt hy sijn heete <rhyme label="c" type="m" corresp="#s6:l5">min</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
