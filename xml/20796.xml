<song>
  <meta>
    <id>20796</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Dimmi che del mio cuore</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">MARIA <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Die Gode <rhyme label="a" type="m" corresp="#s1:l1">behaeght</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Hebt <rhyme label="b" type="m">al-dermeest</rhyme>;</l>
        <l id="s1:l4" met="2" type="-+">Mijn hartje <rhyme label="c" type="m">tracht</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">By dagh en by <rhyme label="c" type="m" corresp="#s1:l4">nacht</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Na u-we <rhyme label="b" type="m" corresp="#s1:l3">Feest</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Op welcke Ghy <rhyme label="d" type="f">gevaren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Zijt na des hemels <rhyme label="e" type="m">hof</rhyme>;</l>
        <l id="s1:l9" met="3" type="-+">Waer d' Eng'len, en Throonen, en <rhyme label="f" type="f">Machten</rhyme>,</l>
        <l id="s1:l10" met="5" type="-+">En d' Heerschappy, U lo-ven, met de <rhyme label="f" type="f" corresp="#s1:l9">Krach-ten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Op dese <rhyme label="a" type="m">aerd'</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Hebt Gh' eertijds <rhyme label="a" type="m" corresp="#s2:l1">gebaert</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Goods een'gen <rhyme label="b" type="m">Soon</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Dan Ghy nu <rhyme label="c" type="m">vaert</rhyme></l>
        <l id="s2:l5" met="2" type="-+">(Gelijck Ghy zijt <rhyme label="c" type="m" corresp="#s2:l4">waerd</rhyme>)</l>
        <l id="s2:l6" met="2" type="-+">Nae 's Hemels <rhyme label="b" type="m" corresp="#s2:l3">throon</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Waer hoogh Ghy wordt <rhyme label="d" type="f">verheven</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Op een bysond're <rhyme label="e" type="m">stee</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">U bidden, u vasten, uw' <rhyme label="f" type="f">deughden</rhyme>,</l>
        <l id="s2:l10" met="5" type="-+">Uw' ned'righeyd, verdienden d' hooghste <rhyme label="f" type="f" corresp="#s2:l9">vreughden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Uw' waerde <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s3:l2" met="2" type="-+">By God haer <rhyme label="a" type="m" corresp="#s3:l1">onthiel</rhyme></l>
        <l id="s3:l3" met="2" type="-+">In 's Hemels <rhyme label="b" type="m">hof</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">U Maeghd'lijck <rhyme label="c" type="m">vleysch</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Wierdt na redens <rhyme label="c" type="m" corresp="#s3:l4">eysch</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Bevrijdt van 't <rhyme label="b" type="m" corresp="#s3:l3">stof</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Gheensins en is 't <rhyme label="d" type="f">verandert</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Het bleef in 't graf <rhyme label="e" type="m">geheel</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">De wormen en hebben uw' <rhyme label="f" type="f">leden</rhyme></l>
        <l id="s3:l10" met="5" type="-+">Niet in het minst' geraeckt, 't is meer als <rhyme label="f" type="f" corresp="#s3:l9">reden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">U quam dees <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Die eertijds den <rhyme label="a" type="m" corresp="#s4:l1">Heer</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Hebt Maeght <rhyme label="b" type="m">gebaert</rhyme>;</l>
        <l id="s4:l4" met="2" type="-+">Dees grooten <rhyme label="c" type="m">God</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Heeft U <rhyme label="c" type="m" corresp="#s4:l4">onverrot</rhyme></l>
        <l id="s4:l6" met="2" type="-+">En gaef <rhyme label="b" type="m" corresp="#s4:l3">bewaert</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Tot dat Hy uyt den <rhyme label="d" type="f">Hemel</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Met uwe ziel quam <rhyme label="e" type="m">af</rhyme>;</l>
        <l id="s4:l9" met="3" type="-+">Welck weder aen 't lichaem <rhyme label="f" type="f">gegeven</rhyme></l>
        <l id="s4:l10" met="5" type="-+">Heeft uwen Soon; die U weÃªr schonck het <rhyme label="f" type="f" corresp="#s4:l9">leven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Uyt 's Hemels <rhyme label="a" type="m">zael</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Met d' Eng'len te <rhyme label="a" type="m" corresp="#s5:l1">mael</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Quam uwen <rhyme label="b" type="m">Soon</rhyme>;</l>
        <l id="s5:l4" met="2" type="-+">Met <rhyme label="c" type="m">Majesteyt</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Heeft Hy U <rhyme label="c" type="m" corresp="#s5:l4">geleydt</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Tot uwen <rhyme label="b" type="m" corresp="#s5:l3">kroon</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Ghy rusten op sijn <rhyme label="d" type="f">schouwd'ren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met wonderlijcke <rhyme label="e" type="m">lust</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Op onsen Verlosser Ghy <rhyme label="f" type="f">steunden</rhyme>,</l>
        <l id="s5:l10" met="5" type="-+">'t Was een vermaeck te sien hoe Gh' op Hem <rhyme label="f" type="f" corresp="#s5:l9">leunden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">My dunckt ick <rhyme label="a" type="m">hoor</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Dat d' Engelsche <rhyme label="a" type="m" corresp="#s6:l1">Choor</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Singht <rhyme label="b" type="m">over-luydt</rhyme>;</l>
        <l id="s6:l4" met="2" type="-+">Verduystert <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s6:l5" met="2" type="-+">De Sonn' en de <rhyme label="c" type="m" corresp="#s6:l4">Maen</rhyme></l>
        <l id="s6:l6" met="2" type="-+">By Christi <rhyme label="b" type="m" corresp="#s6:l3">Bruyd</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Daer en is niet te <rhyme label="d" type="f">g'lijcken</rhyme></l>
        <l id="s6:l8" met="3" type="-+">By hare schoone <rhyme label="e" type="m">glans</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Haer voeten, haer oogen, haer <rhyme label="f" type="f">wesen</rhyme>,</l>
        <l id="s6:l10" met="5" type="-+">Kan noyt genoegh van yemand zijn <rhyme label="f" type="f" corresp="#s6:l9">gepresen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Hoort aen noch <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">En siet wat een <rhyme label="a" type="m" corresp="#s7:l1">eer</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Aen Haer <rhyme label="b" type="m">geschiet</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">D' Engelsche <rhyme label="c" type="m">schaer</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Singht helder en <rhyme label="c" type="m" corresp="#s7:l4">klaer</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Dit Hemels <rhyme label="b" type="m" corresp="#s7:l3">Lied</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Zijt well'kom Godes <rhyme label="d" type="f">Moeder</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Des Hemels groot <rhyme label="e" type="m">Princes</rhyme>:</l>
        <l id="s7:l9" met="3" type="-+">Wy hieten U welkom Goods <rhyme label="f" type="f">Moeder</rhyme>,</l>
        <l id="s7:l10" met="5" type="-+">Gaet, neemt uw' plaets beneffens Ons' <rhyme label="f" type="f" corresp="#s7:l9">Behoeder</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Sy is <rhyme label="a" type="m">gegaen</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">En wierdt daer <rhyme label="a" type="m" corresp="#s8:l1">ontfaen</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Voor <rhyme label="b" type="m">Coningin</rhyme>.</l>
        <l id="s8:l4" met="2" type="-+">God Vader <rhyme label="c" type="m">groet</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">En komt te <rhyme label="c" type="m" corresp="#s8:l4">gemoet</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Dees <rhyme label="b" type="m" corresp="#s8:l3">Keyserin</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">En Hy heeft Haer <rhyme label="d" type="f">ontfangen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Met groot' <rhyme label="e" type="m">eerbiedigheyd</rhyme>.</l>
        <l id="s8:l9" met="3" type="-+">De Eng'len dit siende, die <rhyme label="f" type="f">boogen</rhyme></l>
        <l id="s8:l10" met="5" type="-+">Met ned'righeyd haer vleugels, en haer <rhyme label="f" type="f" corresp="#s8:l9">oogen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Haer lieve <rhyme label="a" type="m">Kind</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Heeft soo Haer <rhyme label="a" type="m" corresp="#s9:l1">bemindt</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Dat Hy Haer <rhyme label="b" type="m">stelt</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">'s Hemels <rhyme label="c" type="m">Princes</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">End' een'ge <rhyme label="c" type="m" corresp="#s9:l4">Vooghdes</rhyme></l>
        <l id="s9:l6" met="2" type="-+">Van 's Hemels <rhyme label="b" type="m" corresp="#s9:l3">veld</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">En siet! Hy gingh Haer <rhyme label="d" type="f">setten</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Aen sijne <rhyme label="e" type="m">rechter-hand</rhyme>.</l>
        <l id="s9:l9" met="3" type="-+">Daer is nu Maria <rhyme label="f" type="f">geseten</rhyme>,</l>
        <l id="s9:l10" met="5" type="-+">En Sy sal ons ten gheender tijd <rhyme label="f" type="f" corresp="#s9:l9">vergeten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Laet dan ons <rhyme label="a" type="m">oogh</rhyme></l>
        <l id="s10:l2" met="2" type="-+">Wy heffen <rhyme label="a" type="m" corresp="#s10:l1">om-hoogh</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Na dese <rhyme label="b" type="m">Vrouw</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Die van den <rhyme label="c" type="m">val</rhyme></l>
        <l id="s10:l5" met="2" type="-+">Bewaren ons <rhyme label="c" type="m" corresp="#s10:l4">sal</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">En van den <rhyme label="b" type="m" corresp="#s10:l3">rouw</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Die eeuwelijck sal <rhyme label="d" type="f">dueren</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">In d' afgrond van de <rhyme label="e" type="m">Hell'</rhyme>.</l>
        <l id="s10:l9" met="3" type="-+">Laet spreken, ons allen, dees' <rhyme label="f" type="f">woorden</rhyme>,</l>
        <l id="s10:l10" met="5" type="-+">Och of Ghy, O Maria! ons <rhyme label="f" type="f" corresp="#s10:l9">verhoorden</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
