<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>17423</id>
    <date>
      <year-start>1903</year-start>
      <year-end>1908</year-end>
    </date>
    <tags/>
    <melody>Benedicamus Domino alleluja</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Loeft God den Heer,</l>
        <l id="s1:l2">ghi heydens seer,</l>
        <l id="s1:l3">groot ende cleyn;</l>
        <l id="s1:l4">int aertsche pleyn,</l>
        <l id="s1:l5">ghi alle ghemeyn,</l>
        <l id="s1:l6">met herten reyn,</l>
        <l id="s1:l7">loeft hem alleyn.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Want vast en goet</l>
        <l id="s2:l2">sijn gracy soet</l>
        <l id="s2:l3">heeft ons bevaen;</l>
        <l id="s2:l4">sijn trou vermaen</l>
        <l id="s2:l5">tot ons ghedaen,</l>
        <l id="s2:l6">sal niet vergaen,</l>
        <l id="s2:l7">maer eewich bliven staen.</l>
      </lg>
    </lg>
  </text>
</song>
