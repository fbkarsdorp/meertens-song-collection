    <location>
      <orginal-name>noordelijke nederlanden (holland?)</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>devout song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Jhesu, uutvercoren <rhyme label="a" type="m">heer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Na u verlanghet my so <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Mijn hert end al mijn <rhyme label="b" type="f">synnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Begheren u <rhyme label="c" type="m">tontfaen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">End al van groter <rhyme label="b" type="f" corresp="#s1:l3">mynnen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die gi my hebt <rhyme label="c" type="m" corresp="#s1:l4">gedaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Jhesu lief, alst u <rhyme label="a" type="m">behaecht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Begheer ic tsijn mit vruechd <rhyme label="a" type="m" corresp="#s2:l1">ghedaecht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Bi u, mijn troest van <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Mijn blijsscap, vreed te <rhyme label="c" type="m">gaer</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Mijn hert dat quelt van <rhyme label="b" type="f" corresp="#s2:l3">mynnen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ic woud ic bi ju <rhyme label="c" type="m" corresp="#s2:l4">waer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gy sijt mijn hoep, al mijn <rhyme label="a" type="m">solaes</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Deed ghi in my en waer gheen <rhyme label="a" type="m" corresp="#s3:l1">paes</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Want had gi nijt <rhyme label="b" type="f">gheleden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Den doot hier op <rhyme label="c" type="m">eertrijc</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En vond in my gheen <rhyme label="b" type="f" corresp="#s3:l3">reden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Te comen in u <rhyme label="c" type="m" corresp="#s3:l4">rijc</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Jhesu, hoe soud ic ymmer <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Vergheten u, mijn lieve <rhyme label="a" type="m" corresp="#s4:l1">heer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die sijt om my <rhyme label="b" type="f">bespoghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">End yamerlic <rhyme label="c" type="m">bespot</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Opt hoeft ghedut, <rhyme label="b" type="f" corresp="#s4:l3">ghetoghen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Gehouden voer een <rhyme label="c" type="m" corresp="#s4:l4">sot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Och ghi wort valschelic <rhyme label="a" type="m">befaemt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gheghecelt, deerlic naect <rhyme label="a" type="m" corresp="#s5:l1">bescaemt</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Ic denck eer gi te <rhyme label="b" type="f">deghen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">U cleer had weder <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Gi most dat zwaer cruys <rhyme label="b" type="f" corresp="#s5:l3">dreghen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ter bitter doot werts <rhyme label="c" type="m" corresp="#s5:l4">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Onnosel lam, gi ghinct <rhyme label="a" type="m">bervoet</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Bloet hoefs, ghewont end zeer <rhyme label="a" type="m" corresp="#s6:l1">bebloet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ic denck mit bloten <rhyme label="b" type="f">armen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Men stiet u hier en <rhyme label="c" type="m">daer</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Ghi zeecht dic neer, <rhyme label="b" type="f" corresp="#s6:l3">ocharmen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">U cruys was veels te <rhyme label="c" type="m" corresp="#s6:l4">zwaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Om minen wil liet gi u <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Voeten, handen dat cruys <rhyme label="a" type="m" corresp="#s7:l1">doersclaen</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Ju syyd wert <rhyme label="b" type="f">doerghesteken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Van doornen was ju <rhyme label="c" type="m">croen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ghi liet u leedkijns <rhyme label="b" type="f" corresp="#s7:l3">breken</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Seer bitterlicke, <rhyme label="c" type="m" corresp="#s7:l4">doen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wel stenen waerlic is sijn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Die hoert of denct u doot, u <rhyme label="a" type="m" corresp="#s8:l1">smert</rhyme></l>
        <l id="s8:l3" met="3" type="-+">End nijt en weent uut <rhyme label="b" type="f">mynnen</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Tsi weynich, veel of <rhyme label="c" type="m">zeer</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Of nijt en lijdt van <rhyme label="b" type="f" corresp="#s8:l3">binnen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Mit u, vercoren <rhyme label="c" type="m" corresp="#s8:l4">heer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die u hoert nomen in der <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Mit recht mach hi wel sijn <rhyme label="a" type="m" corresp="#s9:l1">verblijt</rhyme>.</l>
        <l id="s9:l3" met="3" type="-+">Want tis hem suet te <rhyme label="b" type="f">horen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Jhesum, die zuete <rhyme label="c" type="m">naem</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Die denct ic waer <rhyme label="b" type="f" corresp="#s9:l3">verloren</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Had Jhesus doot <rhyme label="c" type="m" corresp="#s9:l4">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O Jhesu lief, u doot, u <rhyme label="a" type="m">naem</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Is my int hert also <rhyme label="a" type="m" corresp="#s10:l1">bequaem</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Dat ic en weet te <rhyme label="b" type="f">vijnden</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Gheen beter <rhyme label="c" type="m">medecijn</rhyme></l>
        <l id="s10:l5" met="3" type="-+">In my om te <rhyme label="b" type="f" corresp="#s10:l3">verwinnen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Des viants fel <rhyme label="c" type="m" corresp="#s10:l4">venijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hier om bid ic u, waer ic <rhyme label="a" type="m">ga</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Of waer ic bin, sit, leg of <rhyme label="a" type="m" corresp="#s11:l1">sta</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Dat gi tot allen <rhyme label="b" type="f">tiden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">U naem Jhesus, u <rhyme label="c" type="m">myn</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">U doot end al u <rhyme label="b" type="f" corresp="#s11:l3">liden</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Laet comen in mijn <rhyme label="c" type="m" corresp="#s11:l4">syn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
