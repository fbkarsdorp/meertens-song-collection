      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EEdel Maeghden, <rhyme label="a" type="f">Zangheresjes</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die de <rhyme label="a" type="f" corresp="#s1:l1">lesjes</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van de soete <rhyme label="b" type="m">Minne-plicht</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Garen hoort, en tracht te <rhyme label="c" type="f">singen</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Nieuwe <rhyme label="c" type="f" corresp="#s1:l4">dinghen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Brenghen wy nu we’er in’t <rhyme label="b" type="m" corresp="#s1:l3">licht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">‘kSagh dat ghy quaemt vyerig <rhyme label="a" type="f">loopen</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Om te <rhyme label="a" type="f" corresp="#s2:l1">koopen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van mijn eerste <rhyme label="b" type="m">Vreughden-Bron</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">(V te dancken is my ‘t <rhyme label="c" type="f">naeste</rhyme>)</l>
        <l id="s2:l5" met="4" type="+-">Dies ick <rhyme label="c" type="f" corresp="#s2:l4">haeste</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En dit Tweede Deel <rhyme label="b" type="m" corresp="#s2:l3">begon</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dat niet min als ‘t eerst sal <rhyme label="a" type="f">vloeyen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s3:l1">besproeyen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Vwe hartjes met <rhyme label="b" type="m">gheneught</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Stroompjes van verscheyden <rhyme label="c" type="f">krachten</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Wilt <rhyme label="c" type="f" corresp="#s3:l4">verwachten</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Stelt u Keeltjes ree tot <rhyme label="b" type="m" corresp="#s3:l3">vreught</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zuyght uyt varsche springend’ <rhyme label="a" type="f">Aren</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Van het <rhyme label="a" type="f" corresp="#s4:l1">Sparen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">‘t Aengenaem en lieff’lyck <rhyme label="b" type="m">soet</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Daer geen vuyl, noch geyle <rhyme label="c" type="f">stromen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Vyt en <rhyme label="c" type="f" corresp="#s4:l4">komen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">‘t Welck d’onkuysche lusten <rhyme label="b" type="m" corresp="#s4:l3">voedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Koopt dan weder als te <rhyme label="a" type="f">voren</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Ghy sult <rhyme label="a" type="f" corresp="#s5:l1">hooren</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Soete Deuntjes, van den <rhyme label="b" type="m">Mey</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Gaet aen Sparens Oever <rhyme label="c" type="f">rusten</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Singht nae <rhyme label="c" type="f" corresp="#s5:l4">lusten</rhyme></l>
        <l id="s5:l6" met="4" type="+-">In een Klaver-rijcke <rhyme label="b" type="m" corresp="#s5:l3">Wey</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">En ghy kreeght by <rhyme label="a" type="f">ongheluckje</rhyme></l>
        <l id="s6:l2" met="2" type="+-">Dan een <rhyme label="a" type="f" corresp="#s6:l1">stuckje</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat wat koddighs uyt wou <rhyme label="b" type="m">slaen</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Van de Vrijsters <rhyme label="c" type="f">openbaringh</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Die <rhyme label="c" type="f" corresp="#s6:l4">vergaringh</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Laet vry voor de Jonghmans <rhyme label="b" type="m" corresp="#s6:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Vryers moeten me wat <rhyme label="a" type="f">queelen</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Of wat <rhyme label="a" type="f" corresp="#s7:l1">speelen</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Alsse zijn by Bier of <rhyme label="b" type="m">Wijn</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Alsse wand’len met haer <rhyme label="c" type="f">Liefjes</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Soete <rhyme label="c" type="f" corresp="#s7:l4">Diefjes</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En in rust wat vrolyck <rhyme label="b" type="m" corresp="#s7:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dan soo steecktmen in syn <rhyme label="a" type="f">sackje</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Met <rhyme label="a" type="f" corresp="#s8:l1">gemackje</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Wel een Boeckje, ‘t is niet <rhyme label="b" type="m">groot</rhyme>;</l>
        <l id="s8:l4" met="4" type="+-">Evenwel vol vreemde <rhyme label="c" type="f">kluchjes</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">En <rhyme label="c" type="f" corresp="#s8:l4">ghenuchjes</rhyme>;</l>
        <l id="s8:l6" met="4" type="+-">Ey, syn kleynheydt niet <rhyme label="b" type="m" corresp="#s8:l3">verstoot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">En of ghy het hadt <rhyme label="a" type="f">verloren</rhyme></l>
        <l id="s9:l2" met="2" type="+-">In het <rhyme label="a" type="f" corresp="#s9:l1">Koren</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Of op ‘t kantje van een <rhyme label="b" type="m">Beeck</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Dat u ‘t Boeckjen was <rhyme label="c" type="f">ontvallen</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Door het <rhyme label="c" type="f" corresp="#s9:l4">mallen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Daer ‘t dan met de stroom heen <rhyme label="b" type="m" corresp="#s9:l3">streeck</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Komt by Segerman weer <rhyme label="a" type="f">soecken</rhyme></l>
        <l id="s10:l2" met="2" type="+-">In syn <rhyme label="a" type="f" corresp="#s10:l1">Boecken</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Want ick elck tot koopen <rhyme label="b" type="m">noo</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Ghy sult daer u slagh wel <rhyme label="c" type="f">vinnen</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-"><rhyme label="c" type="f" corresp="#s10:l4">Sangherinnen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Vaert dan wel, en doet <rhyme label="b" type="m" corresp="#s10:l3">alsoo</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
