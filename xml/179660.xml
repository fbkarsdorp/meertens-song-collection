<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179660</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WIlt vreugde <rhyme label="a" type="f">hanteren</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">In deze <rhyme label="b" type="m">Kersnagt</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Den heer der <rhyme label="a" type="f" corresp="#s1:l1">heeren</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Is ons <rhyme label="b" type="m" corresp="#s1:l2">voorgebragt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Wilt vreugde <rhyme label="a" type="f" corresp="#s1:l1">hanteeren</rhyme>;</l>
        <l id="s1:l6" met="2" type="-+">Den heer der <rhyme label="a" type="f" corresp="#s1:l1">heeren</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Is ons voort <rhyme label="b" type="m" corresp="#s1:l2">gebragt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hoord d’Engeltjes <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">In ’t midden der <rhyme label="b" type="m">nagt</rhyme>;</l>
        <l id="s2:l3" met="2" type="-+">En tyding <rhyme label="a" type="f" corresp="#s2:l1">bringen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Aen de herders <rhyme label="b" type="m" corresp="#s2:l2">wagt</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Hoord d’Engeltjes <rhyme label="a" type="f" corresp="#s2:l1">zingen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Die tyding <rhyme label="a" type="f" corresp="#s2:l1">bringen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Aen de herders <rhyme label="b" type="m" corresp="#s2:l2">wagt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ras herder wilt <rhyme label="a" type="f">zoeken</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">de Schepper van <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Hy leid in <rhyme label="a" type="f" corresp="#s3:l1">doeken</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">die ons verlossen <rhyme label="b" type="m" corresp="#s3:l2">zal</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Kom laet ons gaen <rhyme label="a" type="f" corresp="#s3:l1">zoeken</rhyme>:</l>
        <l id="s3:l6" met="2" type="-+">hy leid in <rhyme label="a" type="f" corresp="#s3:l1">doeken</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">die ons verlossen <rhyme label="b" type="m" corresp="#s3:l2">zal</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">De herders te <rhyme label="a" type="f">zamen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Syn heenen <rhyme label="b" type="m">gegaen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">En met haer <rhyme label="a" type="f" corresp="#s4:l1">namen</rhyme>:</l>
        <l id="s4:l4" met="2" type="-+">Melk Room en <rhyme label="b" type="m" corresp="#s4:l2">zaen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">de herders te <rhyme label="a" type="f" corresp="#s4:l1">zamen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">In de stal <rhyme label="a" type="f" corresp="#s4:l1">quamen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Sagen ’t Kindje <rhyme label="b" type="m" corresp="#s4:l2">aen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Maria heeft <rhyme label="a" type="f">geboren</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">’t Zaem Godt en <rhyme label="b" type="m">Mensch</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Die ons <rhyme label="a" type="f" corresp="#s5:l1">verkooren</rhyme></l>
        <l id="s5:l4" met="2" type="-+">heeft na Siel en <rhyme label="b" type="m" corresp="#s5:l2">wensch</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Tot Bethlehem is <rhyme label="a" type="f" corresp="#s5:l1">geboren</rhyme>:</l>
        <l id="s5:l6" met="2" type="-+">Die ons <rhyme label="a" type="f" corresp="#s5:l1">verkooren</rhyme></l>
        <l id="s5:l7" met="2" type="-+">heeft na ziel en <rhyme label="b" type="m" corresp="#s5:l2">wensch</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Sa komt nu <rhyme label="a" type="f">geloopen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">hier is u Siels <rhyme label="b" type="m">lust</rhyme>:</l>
        <l id="s6:l3" met="2" type="-+">Soo ras met <rhyme label="a" type="f" corresp="#s6:l1">hopen</rhyme>:</l>
        <l id="s6:l4" met="2" type="-+">En het Kindje <rhyme label="b" type="m" corresp="#s6:l2">kust</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Sa komt nu <rhyme label="a" type="f" corresp="#s6:l1">gelopen</rhyme>;</l>
        <l id="s6:l6" met="2" type="-+">Met groote <rhyme label="a" type="f" corresp="#s6:l1">hoopen</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">En het Kindje <rhyme label="b" type="m" corresp="#s6:l2">zust</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Treet al minst en <rhyme label="a" type="f">meeste</rhyme>:</l>
        <l id="s7:l2" met="2" type="-+">Na ’t arme beeste <rhyme label="b" type="m">kot</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Tusschen de <rhyme label="a" type="f" corresp="#s7:l1">beesten</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Leid den Grooten <rhyme label="b" type="m" corresp="#s7:l2">God</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Treed al minst en <rhyme label="a" type="f" corresp="#s7:l1">meeste</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Tusschen de <rhyme label="a" type="f" corresp="#s7:l1">Beesten</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Leyd dien grooten <rhyme label="b" type="m" corresp="#s7:l2">God</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">In doeken <rhyme label="a" type="f">omwonden</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">In de krebbe <rhyme label="b" type="m">gelyd</rhyme></l>
        <l id="s8:l3" met="2" type="-+">’t Is om ons <rhyme label="a" type="f" corresp="#s8:l1">zonden</rhyme>;</l>
        <l id="s8:l4" met="2" type="-+">Dat het Schaepje <rhyme label="b" type="m" corresp="#s8:l2">schreid</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">In doeken <rhyme label="a" type="f" corresp="#s8:l1">omwonden</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Om ons van <rhyme label="a" type="f" corresp="#s8:l1">zonden</rhyme>;</l>
        <l id="s8:l7" met="2" type="-+">Te maken <rhyme label="b" type="m" corresp="#s8:l2">bevryt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Dat is het Saed der <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Van ons lang <rhyme label="b" type="m">verwagt</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">hy wil <rhyme label="a" type="f" corresp="#s9:l1">behouwen</rhyme>;</l>
        <l id="s9:l4" met="2" type="-+">heel het Mensch <rhyme label="b" type="m" corresp="#s9:l2">geslagt</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">dit is het zaet der <rhyme label="a" type="f" corresp="#s9:l1">Vrouwen</rhyme></l>
        <l id="s9:l6" met="2" type="-+">hy wil <rhyme label="a" type="f" corresp="#s9:l1">behouwen</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">heel het Mensch <rhyme label="b" type="m" corresp="#s9:l2">geslagt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Hy daeld tot ons <rhyme label="a" type="f">beneden</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Uit zyn Vaders <rhyme label="b" type="m">Troon</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Laet ons gaen <rhyme label="a" type="f" corresp="#s10:l1">treden</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Soeken Godes <rhyme label="b" type="m" corresp="#s10:l2">Soon</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">die tot ons komt <rhyme label="a" type="f" corresp="#s10:l1">beneden</rhyme></l>
        <l id="s10:l6" met="2" type="-+">En brengt ons <rhyme label="a" type="f" corresp="#s10:l1">vreden</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">’t Welk is Gods <rhyme label="b" type="m" corresp="#s10:l2">Soon</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Agt dagen <rhyme label="a" type="f">geleden</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Na Mozes Wet <rhyme label="b" type="m">klaer</rhyme>;</l>
        <l id="s11:l3" met="2" type="-+">Wierd hy <rhyme label="a" type="f" corresp="#s11:l1">besneden</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">In den Tempel <rhyme label="b" type="m" corresp="#s11:l2">daer</rhyme>,</l>
        <l id="s11:l5" met="2" type="-+">Agt dagen <rhyme label="a" type="f" corresp="#s11:l1">geleden</rhyme>;</l>
        <l id="s11:l6" met="2" type="-+">Wierd hy <rhyme label="a" type="f" corresp="#s11:l1">besneden</rhyme></l>
        <l id="s11:l7" met="2" type="-+">In den Tempel <rhyme label="b" type="m" corresp="#s11:l2">daer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Stort ziels <rhyme label="a" type="f">offerhande</rhyme>;</l>
        <l id="s12:l2" met="2" type="-+">Looft den <rhyme label="b" type="m">Opperheer</rhyme></l>
        <l id="s12:l3" met="2" type="-+">Die Satans <rhyme label="a" type="f" corresp="#s12:l1">banden</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Heeft geslagen <rhyme label="b" type="m" corresp="#s12:l2">neer</rhyme></l>
        <l id="s12:l5" met="2" type="-+">Stort ziels <rhyme label="a" type="f" corresp="#s12:l1">offerhande</rhyme>,</l>
        <l id="s12:l6" met="2" type="-+">En wilt die <rhyme label="a" type="f" corresp="#s12:l1">branden</rhyme></l>
        <l id="s12:l7" met="2" type="-+">Altyd meer en <rhyme label="b" type="m" corresp="#s12:l2">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
