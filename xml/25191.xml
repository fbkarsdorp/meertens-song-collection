  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ich sach eine vrauwe <rhyme label="a" type="m">minnenclich</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">mit rosen wol <rhyme label="b" type="f">gecronit</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">die hette umbe gurtet <rhyme label="a" type="m" corresp="#s1:l1">sich</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">mit lilien wol <rhyme label="b" type="f" corresp="#s1:l2">geschonit</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">was si mit blomen <rhyme label="c" type="m">mangerlei</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">an irme libe uber <rhyme label="d" type="m">al</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">van laube ein dach gaf ir der <rhyme label="c" type="m" corresp="#s1:l5">mei</rhyme>.</l>
        <l id="s1:l8" met="6" type="-+">de vogil sungin umbe si so suoissen <rhyme label="d" type="m" corresp="#s1:l6">schal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Der vrauwen ich uch bescheiden <rhyme label="a" type="m">wil</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">De da sas in der <rhyme label="b" type="f">wnne</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">de kuosche hait der dogende <rhyme label="a" type="m" corresp="#s2:l1">vil</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">si gelienschet sam die <rhyme label="b" type="f" corresp="#s2:l2">sunne</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Ir gurtil wijs dat is der <rhyme label="c" type="m">schaim</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">die ander blomen wol <rhyme label="d" type="m">gestalt</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">ist manige tuogent. dan von ir <rhyme label="c" type="m" corresp="#s2:l5">name</rhyme></l>
        <l id="s2:l8" met="6" type="-+">gepriset wirt. inde uch ir ere manich <rhyme label="d" type="m" corresp="#s2:l6">valt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die voglin de da sungen <rhyme label="a" type="m">ir</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">in suoisser stimmen <rhyme label="b" type="f">tone</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">ist manige zunge geloubit <rhyme label="a" type="m" corresp="#s3:l1">mir</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">die na ir richeme <rhyme label="b" type="f" corresp="#s3:l2">lone</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">ringit inde ir <rhyme label="c" type="m">werdicheit</rhyme>.</l>
        <l id="s3:l6" met="4" type="-+">das ir ere werde <rhyme label="d" type="m">lanc</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">in sielden sal er sin <rhyme label="c" type="m" corresp="#s3:l5">gemeit</rhyme>.</l>
        <l id="s3:l8" met="6" type="-+">deme van ir libe wirt ein steder umbe <rhyme label="d" type="m" corresp="#s3:l6">vanc</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
