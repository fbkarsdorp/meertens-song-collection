    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Belle Phyllis pleine d'amour</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Livinus van der Minnen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">SOete <rhyme label="a" type="m">Jolie</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Kleyn <rhyme label="a" type="m" corresp="#s1:l1">honingh-bie</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Die <rhyme label="b" type="m">allen-tom</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Soeckt naer een soete <rhyme label="b" type="m" corresp="#s1:l3">blom</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Soet <rhyme label="c" type="f">Honingh-Diefken</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Suyght aen myn Lief <rhyme label="c" type="f" corresp="#s1:l5">ken</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Haer aen-sicht bloost <rhyme label="d" type="m">altoos</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Ghelyck een <rhyme label="d" type="m" corresp="#s1:l7">Roos</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Heel het jaer <rhyme label="a" type="m">deur</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Staet sy in <rhyme label="a" type="m" corresp="#s2:l1">fleur</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Sy en <rhyme label="b" type="m">verliest</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Haer schoonheyt niet als 't <rhyme label="b" type="m" corresp="#s2:l3">vriest</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Haer soete <rhyme label="c" type="f">geuren</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Haer schoon <rhyme label="c" type="f" corresp="#s2:l5">colleuren</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En hebben hier gheen <rhyme label="d" type="m">endt</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Al endt de <rhyme label="d" type="m" corresp="#s2:l7">Lent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Soeckt <rhyme label="a" type="m">over-al</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Op bergh en <rhyme label="a" type="m" corresp="#s3:l1">dal</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Een Bloem, waer <rhyme label="b" type="m">uyt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Soo soeten aessem <rhyme label="b" type="m" corresp="#s3:l3">spruyt</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">G'en sult gheen <rhyme label="c" type="f">vinde</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Als myn <rhyme label="c" type="f" corresp="#s3:l5">Beminde</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Soo soet, soo schoon, en <rhyme label="d" type="m">fris</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Ghelyck sy <rhyme label="d" type="m" corresp="#s3:l7">is</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Sy is <rhyme label="a" type="m">habiel</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Sy is <rhyme label="a" type="m" corresp="#s4:l1">subtiel</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Sy is <rhyme label="b" type="m">gentiel</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En soet van lyf, en <rhyme label="b" type="m" corresp="#s4:l3">ziel</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Vol soet <rhyme label="c" type="f">humeuren</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Vol schoon <rhyme label="c" type="f" corresp="#s4:l5">colleuren</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Lieffelyck, <rhyme label="d" type="m">vriendelyck</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En <rhyme label="d" type="m" corresp="#s4:l7">minnelyck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">O Bloem vol <rhyme label="a" type="m">vreught</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Vol eer', en <rhyme label="a" type="m" corresp="#s5:l1">deught</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Maeckt my <rhyme label="b" type="m">verheught</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Door uwe soete <rhyme label="b" type="m" corresp="#s5:l3">jeught</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Soete <rhyme label="c" type="f">Vriendinne</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Die ick <rhyme label="c" type="f" corresp="#s5:l5">beminne</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Confyt myn bitter <rhyme label="d" type="m">smert</rhyme></l>
        <l id="s5:l8" met="2" type="-+">In uw soet <rhyme label="d" type="m" corresp="#s5:l7">hert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
