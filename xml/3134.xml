<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3134</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Phyllis kwam Philander tegen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALyda; o soetste <rhyme label="a" type="f">Diertje</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Dien ick op de Werelt <rhyme label="b" type="m">weet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Stoocksteres van 't <rhyme label="a" type="f" corresp="#s1:l1">Minne-viertje</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat sijn vlamme-straeltjes <rhyme label="b" type="m" corresp="#s1:l2">heet</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Brandend' in mijn Boesem <rhyme label="c" type="m">schiet</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Sonder dat ick lust <rhyme label="c" type="m" corresp="#s1:l5">geniet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Waerom moet ick sijn <rhyme label="a" type="f">verscheyen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Van u soete <rhyme label="b" type="m">Engelin</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Waerom moet mijn lieflijck <rhyme label="a" type="f" corresp="#s2:l1">vleyen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">(Segh, mijn schoone <rhyme label="b" type="m" corresp="#s2:l2">Dianin</rhyme>)</l>
        <l id="s2:l5" met="4" type="+-">Aen dees stomme Beeck <rhyme label="c" type="m">geschien</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Sonder u mijn lief te <rhyme label="c" type="m" corresp="#s2:l5">sien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer ick weet mijn <rhyme label="a" type="f">overschoone</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Wie ons dees af-gonsten <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s3:l3" met="4" type="+-">'t Is de bloedige <rhyme label="a" type="f" corresp="#s3:l1">Bellone</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Wagen-meester, die haer <rhyme label="b" type="m" corresp="#s3:l2">voet</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Op haer broeders schouders <rhyme label="c" type="m">plant</rhyme></l>
        <l id="s3:l6" met="4" type="+-">en vernietight landt op <rhyme label="c" type="m" corresp="#s3:l5">landt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer, ach Venus; wilt <rhyme label="a" type="f">ophitsen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Cupido met al zijn <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Met zijn Boogh, en met zijn <rhyme label="a" type="f" corresp="#s4:l1">flitsen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Met syn gantsche <rhyme label="b" type="m" corresp="#s4:l2">Oorloghs-kracht</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dat hy Mars zijn vleugels <rhyme label="c" type="m">bint</rhyme>.</l>
        <l id="s4:l6" met="4" type="+-">Want hy u niet meer <rhyme label="c" type="m" corresp="#s4:l5">bemint</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Siet, hy neemt den Oorlog <rhyme label="a" type="f">machtigh</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Niet om u, o Venus, <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Maer om Juno preuts en <rhyme label="a" type="f" corresp="#s5:l1">prachtigh</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Die haer voor-hooft (als de <rhyme label="b" type="m" corresp="#s5:l2">Maen</rhyme>)</l>
        <l id="s5:l5" met="4" type="+-">In de duyster nachten <rhyme label="c" type="m">blinckt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Steedts den Oorlog' t' oogh <rhyme label="c" type="m" corresp="#s5:l5">toe-winckt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dan sal ick een Tempel <rhyme label="a" type="f">maken</rhyme></l>
        <l id="s6:l2" met="4" type="+-">(U ter eeren) O <rhyme label="b" type="m">Godin</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Meyrth' en Wieroock sal ick <rhyme label="a" type="f" corresp="#s6:l1">blaken</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Op het altaer van de <rhyme label="b" type="m" corresp="#s6:l2">Min</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">En ick sal uwen jongen <rhyme label="c" type="m">Soon</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Schencken een Cypressen <rhyme label="c" type="m" corresp="#s6:l5">Croon</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Tot getuygen zijt ghy <rhyme label="a" type="f">Boompjes</rhyme></l>
        <l id="s7:l2" met="4" type="+-">En ghy Beeck of <rhyme label="b" type="m">Wester-bron</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die in 't eerst u klare <rhyme label="a" type="f" corresp="#s7:l1">Stroompjes</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Wt het Duyn in 't Bosje <rhyme label="b" type="m" corresp="#s7:l2">von</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Daerom Eyck en <rhyme label="c" type="m">Else-blaen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Hoort doch dese woortjens <rhyme label="c" type="m" corresp="#s7:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hoe sal ick Nimphje <rhyme label="a" type="f">strelen</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Als ick by haer wesen <rhyme label="b" type="m">magh</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Hoe wil dan met haer <rhyme label="a" type="f" corresp="#s8:l1">spelen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Hemel gunt dien blijden <rhyme label="b" type="m" corresp="#s8:l2">dagh</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Als ick mont en lipjens <rhyme label="c" type="m">al</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Met een vreught genaken <rhyme label="c" type="m" corresp="#s8:l5">sal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Maer my dunckt de vogels <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Toomt u lusjes toomt u <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Wilt u soete gedacht <rhyme label="a" type="f" corresp="#s9:l1">bedwingen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Want die hem te vroegh <rhyme label="b" type="m" corresp="#s9:l2">verheugt</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Eer dat het de tijt <rhyme label="c" type="m">gebiedt</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Krijght voor vreughde vaeck <rhyme label="c" type="m" corresp="#s9:l5">verdriet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
