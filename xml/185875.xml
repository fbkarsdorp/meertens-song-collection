    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Verfoeide <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Gestrenge <rhyme label="b" type="f">reede</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Wie van u <rhyme label="c" type="m">twee</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Moet ik volgen <rhyme label="d" type="m">na</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Wys mijn de <rhyme label="e" type="m">Weg</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Dien ik zal <rhyme label="b" type="f" corresp="#s1:l2">betreeden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Vereenigt met <rhyme label="f" type="f">elkaer</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">d'Eeene roept my <rhyme label="g" type="m">toe</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Het zal U <rhyme label="h" type="f">rouwen</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Walging en spyt volgen het <rhyme label="h" type="f" corresp="#s1:l9">trouwen</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">d'Anderen op haer <rhyme label="i" type="m">beurt</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Spot met dees <rhyme label="b" type="f" corresp="#s1:l2">Reeden</rhyme>,</l>
        <l id="s1:l13" met="2" type="-+">En voert myn <rhyme label="j" type="m">hart</rhyme>,</l>
        <l id="s1:l14" met="2" type="-+">Gekluistert <rhyme label="k" type="m">heen</rhyme>,</l>
