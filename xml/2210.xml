  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">GRoote <rhyme label="a" type="f">Gooden</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Hoe treft het <rhyme label="b" type="m">Minnewicht</rhyme></l>
        <l id="s1:l3" met="5" type="-+">My on-meedoogend met sijn <rhyme label="b" type="m" corresp="#s1:l2">minne-schicht</rhyme>!</l>
        <l id="s1:l4" met="5" type="-+">My on-meedoogend met sijn <rhyme label="b" type="m" corresp="#s1:l2">minne-schicht</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Moet ik dan steeds mijn <rhyme label="c" type="f">daagen</rhyme></l>
        <l id="s1:l6" met="1" type="-+">In <rhyme label="c" type="f" corresp="#s1:l5">klaagen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">En soet <rhyme label="d" type="f">verlangen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">so verslijten <rhyme label="e" type="m">doen</rhyme>?</l>
        <l id="s1:l9" met="2" type="-+">En soet <rhyme label="d" type="f" corresp="#s1:l7">verlangen</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">En soet <rhyme label="d" type="f" corresp="#s1:l7">verlangen</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">So verslijten <rhyme label="e" type="m" corresp="#s1:l8">doen</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Ik moet <rhyme label="a" type="f">sterven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">So ik geen middel <rhyme label="b" type="m">vin</rhyme></l>
        <l id="s2:l3" met="5" type="-+">Door de belooning van uw <rhyme label="b" type="m" corresp="#s2:l2">weedermin</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Door de belooning van uw <rhyme label="b" type="m" corresp="#s2:l2">weedermin</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">O lieve Nymf, mijn <rhyme label="c" type="f">aaren</rhyme></l>
        <l id="s2:l6" met="1" type="-+">
          <rhyme label="c" type="f" corresp="#s2:l5">Verklaaren</rhyme>
        </l>
        <l id="s2:l7" met="2" type="-+">Hoe groote <rhyme label="d" type="f">liefde</rhyme></l>
        <l id="s2:l8" met="3" type="+-">ik uw schoonheid <rhyme label="e" type="m">draag</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">Hoe groote <rhyme label="d" type="f" corresp="#s2:l7">liefde</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Hoe groote <rhyme label="d" type="f" corresp="#s2:l7">liefde</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Ik uw schoonheid <rhyme label="e" type="m" corresp="#s2:l8">draag</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Sietse <rhyme label="a" type="f">swellen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door een stantvaste <rhyme label="b" type="m">vlam</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Die uit uw oogjes in mijn boesem <rhyme label="b" type="m" corresp="#s3:l2">quam</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Die uit uw oog jes in mijn boesem <rhyme label="b" type="m" corresp="#s3:l2">quam</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Mijn hartje draagt veel <rhyme label="c" type="f">wonde'</rhyme>,</l>
        <l id="s3:l6" met="1" type="-+">O <rhyme label="c" type="f" corresp="#s3:l5">Blonde</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Die ik door <rhyme label="d" type="f">diensten</rhyme></l>
        <l id="s3:l8" met="3" type="+-">om uw deugden <rhyme label="e" type="m">kreeg</rhyme>.</l>
        <l id="s3:l9" met="2" type="-+">Die ik door <rhyme label="d" type="f" corresp="#s3:l7">diensten</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Die ik door <rhyme label="d" type="f" corresp="#s3:l7">diensten</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Om uw deugden <rhyme label="e" type="m" corresp="#s3:l8">kreeg</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Niet behaagt <rhyme label="a" type="f">my</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Nu ik uw weermin <rhyme label="b" type="m">mis</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Dan eensaamheid die in de nachten <rhyme label="b" type="m" corresp="#s4:l2">is</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Dan eensaamheid die in de nachten <rhyme label="b" type="m" corresp="#s4:l2">is</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Mijn leevens dag sal <rhyme label="c" type="f">enden</rhyme>,</l>
        <l id="s4:l6" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s4:l5">wenden</rhyme></l>
        <l id="s4:l7" met="2" type="-+">So <rhyme label="d" type="f">Celestyne</rhyme></l>
        <l id="s4:l8" met="3" type="+-">gy geen liefde <rhyme label="e" type="m">toont</rhyme>.</l>
        <l id="s4:l9" met="2" type="-+">So <rhyme label="d" type="f" corresp="#s4:l7">Celestyne</rhyme></l>
        <l id="s4:l10" met="2" type="-+">So <rhyme label="d" type="f" corresp="#s4:l7">Celestyne</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Gy geen liefde <rhyme label="e" type="m" corresp="#s4:l8">toont</rhyme>.</l>
      </lg>
    </lg>
  </text>
