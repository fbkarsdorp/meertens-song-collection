  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SOnne van mijn <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Floncker-sterre van mijn <rhyme label="b" type="m">jeught</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Waarde Engel weest <rhyme label="a" type="f" corresp="#s1:l1">meedoogen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">In u oogen eenigh <rhyme label="b" type="m" corresp="#s1:l2">deught</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Ach volmaakte! die <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Onder soo veel <rhyme label="c" type="m" corresp="#s1:l5">godlijckheen</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Souder noch ontbreken <rhyme label="a" type="f" corresp="#s1:l1">mogen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Andersints ick weet'er <rhyme label="c" type="m" corresp="#s1:l5">geen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ongevoel'ge <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Hoe kan u aanminningh <rhyme label="b" type="m">beeld</rhyme></l>
        <l id="s2:l3" met="4" type="+-">By soo wreeden hertjen <rhyme label="a" type="f" corresp="#s2:l1">woone</rhyme>?</l>
        <l id="s2:l4" met="4" type="+-">Ach! 't is ongelijck <rhyme label="b" type="m" corresp="#s2:l2">verdeelt</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Of gy most soo schoon niet <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Of soo wreed niet <rhyme label="c" type="m" corresp="#s2:l5">Roselyn</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Maar met wedermin <rhyme label="a" type="f" corresp="#s2:l1">beloone</rhyme></l>
        <l id="s2:l8" met="4" type="+-">My, die om uw liefde <rhyme label="c" type="m" corresp="#s2:l5">quyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Moeder der <rhyme label="a" type="f">minne</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Treft haar ongevoeligh <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Treft myn liefs verstaalde <rhyme label="a" type="f" corresp="#s3:l1">sinnen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Doch met een gelijcken <rhyme label="b" type="m" corresp="#s3:l2">smert</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Op dat haar verbert <rhyme label="c" type="m">gemoed</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">'t Geen myn nu dees pijn <rhyme label="c" type="m" corresp="#s3:l5">aandoet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Eyndelick sich laat <rhyme label="a" type="f" corresp="#s3:l1">verwinnen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En het zuur verkeerd in 't <rhyme label="c" type="m" corresp="#s3:l5">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
