<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35094</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1700</year-end>
    </date>
    <tags>
      <tag translation="reflective song">bespiegelend lied</tag>
      <tag translation="">wereldlijk</tag>
    </tags>
    <melody>Les dieux nous ont formé</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">heeft hij t'niet wel <rhyme label="a" type="m">gegist</rhyme></l>
        <l id="s1:l2" met="3" type="-+">die eertijts heeft de <rhyme label="b" type="f">trouwe</rhyme></l>
        <l id="s1:l3" met="3" type="-+">geleecken bij een <rhyme label="c" type="m">zee</rhyme></l>
        <l id="s1:l4" met="3" type="-+">hij heeft niet veel <rhyme label="a" type="m" corresp="#s1:l1">gemist</rhyme></l>
        <l id="s1:l5" met="4" type="-+">want hoe gedwee, want hoe <rhyme label="c" type="m" corresp="#s1:l3">gedwee</rhyme></l>
        <l id="s1:l6" met="4" type="-+">en stil de zee sigh toont, sij <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme></l>
        <l id="s1:l7" met="3" type="-+">vol achterdocht en <rhyme label="b" type="f" corresp="#s1:l2">rauwe</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">elck en die kent het <rhyme label="a" type="m">nut</rhyme></l>
        <l id="s2:l2" met="3" type="-+">en voordeel van het <rhyme label="b" type="f">vaeren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">elck een een zeeman <rhyme label="c" type="m">waer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">weert, sijn drift niet <rhyme label="a" type="m" corresp="#s2:l1">gestat</rhyme></l>
        <l id="s2:l5" met="4" type="-+">door tgroot gevaer, door tgroot <rhyme label="c" type="m" corresp="#s2:l3">gevaer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">dat hem op t land houd in sijn <rhyme label="a" type="m" corresp="#s2:l1">hudt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">en vluchten doet de <rhyme label="b" type="f" corresp="#s2:l2">baeren</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">een blix een <rhyme label="a" type="m">ongeweert</rhyme></l>
        <l id="s3:l2" met="3" type="-+">een draeij-klock quaede <rhyme label="b" type="f">winden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">en meenigh <rhyme label="c" type="m">ongheval</rhyme></l>
        <l id="s3:l4" met="3" type="-+">dat schip en schipman <rhyme label="a" type="m" corresp="#s3:l1">deert</rhyme></l>
        <l id="s3:l5" met="4" type="-+">doet menighmael, doet <rhyme label="c" type="m" corresp="#s3:l3">menighmael</rhyme></l>
        <l id="s3:l6" met="4" type="-+">verwonderen hoe sigh een <rhyme label="a" type="m" corresp="#s3:l1">begeert</rhyme></l>
        <l id="s3:l7" met="3" type="-+">op zee te laten <rhyme label="b" type="f" corresp="#s3:l2">vinden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Soo gaet het inde <rhyme label="a" type="m">trouw</rhyme></l>
        <l id="s4:l2" met="3" type="-+">elck een wint de <rhyme label="b" type="f">genuchten</rhyme></l>
        <l id="s4:l3" met="3" type="-+">maer niet het last en <rhyme label="c" type="m">pijn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">gemeen aen man en <rhyme label="a" type="m" corresp="#s4:l1">vrouw</rhyme></l>
        <l id="s4:l5" met="4" type="-+">vol sorgh te sijn, vol sorgh te <rhyme label="c" type="m" corresp="#s4:l3">sijn</rhyme></l>
        <l id="s4:l6" met="4" type="-+">en in gevaer van rang en <rhyme label="a" type="m" corresp="#s4:l1">rouw</rhyme></l>
        <l id="s4:l7" met="3" type="-+">doet veel de trouwe <rhyme label="b" type="f" corresp="#s4:l2">vluchten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">het insight van <rhyme label="a" type="m">gewin</rhyme></l>
        <l id="s5:l2" met="3" type="-+">van lust of ander <rhyme label="b" type="f">voordeel</rhyme></l>
        <l id="s5:l3" met="3" type="-+">behoort er somtijts <rhyme label="c" type="m">veel</rhyme></l>
        <l id="s5:l4" met="3" type="-+">te luijstren na de <rhyme label="a" type="m" corresp="#s5:l1">min</rhyme></l>
        <l id="s5:l5" met="4" type="-+">maer t'meesten deel, maer t'meesten <rhyme label="c" type="m" corresp="#s5:l3">deel</rhyme></l>
        <l id="s5:l6" met="4" type="-+">sagh elck het last en sorg wel <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme></l>
        <l id="s5:l7" met="3" type="-+">streeck haest een ander <rhyme label="b" type="f" corresp="#s5:l2">oordeel</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">een ijder is <rhyme label="a" type="m">gereet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">een geurge roos te <rhyme label="b" type="f">plucken</rhyme></l>
        <l id="s6:l3" met="3" type="-+">maer soeckt de doorens <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">waer mêe die word <rhyme label="a" type="m" corresp="#s6:l1">bekleet</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">alsoo soo vlied, alsoo soo <rhyme label="c" type="m" corresp="#s6:l3">vlied</rhyme></l>
        <l id="s6:l6" met="4" type="-+">men tgoed uijt vrees dat eenigh <rhyme label="a" type="m" corresp="#s6:l1">leed</rhyme></l>
        <l id="s6:l7" met="3" type="-+">het goed sou kommen <rhyme label="b" type="f" corresp="#s6:l2">drucken</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dat niemant het <rhyme label="a" type="m">profijt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">des zeemans en <rhyme label="b" type="f">benijde</rhyme></l>
        <l id="s7:l3" met="3" type="-+">haeckt hij na groot <rhyme label="c" type="m">gewin</rhyme></l>
        <l id="s7:l4" met="3" type="-+">hij toon de selve <rhyme label="a" type="m" corresp="#s7:l1">vleijd</rhyme></l>
        <l id="s7:l5" met="4" type="-+">dus inde min, dus inde <rhyme label="c" type="m" corresp="#s7:l3">min</rhyme></l>
        <l id="s7:l6" met="4" type="-+">die tsoet uijt vrees van't seur <rhyme label="a" type="m" corresp="#s7:l1">verwij</rhyme></l>
        <l id="s7:l7" met="3" type="-+">moet lijden dat hij <rhyme label="b" type="f" corresp="#s7:l2">lijde</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">die t'leven en sijn <rhyme label="a" type="m">rust</rhyme></l>
        <l id="s8:l2" met="3" type="-+">bemint die vlucht de <rhyme label="b" type="f">baeren</rhyme></l>
        <l id="s8:l3" met="3" type="-+">met t'geen hij heeft de <rhyme label="c" type="m">vreen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">bewaert sijn eijgen <rhyme label="a" type="m" corresp="#s8:l1">kust</rhyme></l>
        <l id="s8:l5" met="4" type="-+">soo oock als een, soo oock als <rhyme label="c" type="m" corresp="#s8:l3">een</rhyme></l>
        <l id="s8:l6" met="4" type="-+">de trouw vreest die beswoeij sijn <rhyme label="a" type="m" corresp="#s8:l1">lust</rhyme></l>
        <l id="s8:l7" met="3" type="-+">en houd sigh van het <rhyme label="b" type="f" corresp="#s8:l2">paeren</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
