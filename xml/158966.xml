<song>
  <meta>
    <id>158966</id>
    <date>
      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Diei solemnia</melody>
  <author>Augustinus van Teylingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O koningin’ des hemels <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s1:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s1:l3" met="4" type="-+">Comt doch af van ‘shemels <rhyme label="a" type="m" corresp="#s1:l1">troon</rhyme></l>
        <l id="s1:l4" met="3" type="-+">O koningin <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O moeder van <rhyme label="a" type="m">bermherticheyt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s2:l3" met="4" type="-+">Bidt voor de gantsche <rhyme label="a" type="m" corresp="#s2:l1">Christenheydt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">O koningin’ <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Bidt dat Godt genadigh <rhyme label="a" type="m">zy</rhyme></l>
        <l id="s3:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s3:l3" met="4" type="-+">Dat hy ons maeckt van sonden <rhyme label="a" type="m" corresp="#s3:l1">vry</rhyme></l>
        <l id="s3:l4" met="3" type="-+">O koningin’ <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Bidt Godt ons geeft kracht en <rhyme label="a" type="m">ghenae</rhyme></l>
        <l id="s4:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s4:l3" met="4" type="-+">Dat an ziel noch lichaem yet <rhyme label="a" type="m" corresp="#s4:l1">scha</rhyme></l>
        <l id="s4:l4" met="3" type="-+">O koningin’ <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Bidt voor het heyligh Roomsche <rhyme label="a" type="m">Rijck</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s5:l3" met="4" type="-+">Dat den Keyser niet <rhyme label="a" type="m" corresp="#s5:l1">beswijck</rhyme></l>
        <l id="s5:l4" met="3" type="-+">O koningin’ <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doet dat hem geschiedt <rhyme label="a" type="m">by-stant</rhyme></l>
        <l id="s6:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s6:l3" met="4" type="-+">Verjaeght Godts vyandt uyt het <rhyme label="a" type="m" corresp="#s6:l1">landt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">O koningin’ <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Keert quaet aenstoot en duerten <rhyme label="a" type="m">af</rhyme></l>
        <l id="s7:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s7:l3" met="4" type="-+">Oock Pest, Brandt, en ander <rhyme label="a" type="m" corresp="#s7:l1">straf</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">O koningin’ <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Staet ons by in alle <rhyme label="a" type="m">noodt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">O koningin’, ô <rhyme label="b" type="m">koningin</rhyme>’</l>
        <l id="s8:l3" met="4" type="-+">En blijft by ons tot inder <rhyme label="a" type="m" corresp="#s8:l1">doodt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">O koningin’ <rhyme label="c" type="f">Maria</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
