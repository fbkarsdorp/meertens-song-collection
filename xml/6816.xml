<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6816</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>emmerick</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Grâce au bon petit Jésus</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Lof Iesu die tot ons <rhyme label="a" type="m">profijt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Op desen <rhyme label="a" type="m" corresp="#s1:l1">tijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghedaelt zijt uyt ws vaders <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Om te <rhyme label="c" type="f">verdragen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Nae weynich <rhyme label="c" type="f" corresp="#s1:l4">dagen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Die bitter <rhyme label="b" type="m" corresp="#s1:l3">doot</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beginnen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s1:l8">beminnen</rhyme></l>
        <l id="s1:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s1:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy cmt ter werelt naeckt en <rhyme label="a" type="m">bloot</rhyme></l>
        <l id="s2:l2" met="2" type="-+">In groter <rhyme label="a" type="m" corresp="#s2:l1">noot</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En achterlaet s’hemels <rhyme label="b" type="m">laysier</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Om door u <rhyme label="c" type="f">lijden</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ons te <rhyme label="c" type="f" corresp="#s2:l4">bevrijden</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Vant helsche <rhyme label="b" type="m" corresp="#s2:l3">vier</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beginnen</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s2:l8">beminnen</rhyme></l>
        <l id="s2:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s2:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Aensiet o mensche hoe dit <rhyme label="a" type="m">kint</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Hem selven <rhyme label="a" type="m" corresp="#s3:l1">pijnt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En maeckt hem cleyn in menschen <rhyme label="b" type="m">schijn</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Om haer te <rhyme label="c" type="f">trecken</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">En te <rhyme label="c" type="f" corresp="#s3:l4">verwecken</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Om cleyn te <rhyme label="b" type="m" corresp="#s3:l3">sijn</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beginnen</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s3:l8">beminnen</rhyme></l>
        <l id="s3:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s3:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wie heeft sulck wonder oyt <rhyme label="a" type="m">ghehoort</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Vleys wert het <rhyme label="a" type="m" corresp="#s4:l1">woort</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En onsen vader op dat <rhyme label="b" type="m">pas</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Wordt onsen <rhyme label="c" type="f">broeder</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Een maegt sijn <rhyme label="c" type="f" corresp="#s4:l4">moeder</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Wiens kint sy <rhyme label="b" type="m" corresp="#s4:l3">was</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beginnen</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s4:l8">beminnen</rhyme></l>
        <l id="s4:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s4:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Int kribbeken wort hy <rhyme label="a" type="m">gheleyt</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Wiens <rhyme label="a" type="m" corresp="#s5:l1">Majesteyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die koningen te boven <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Hy wert <rhyme label="c" type="f">ghewonden</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Die heeft <rhyme label="c" type="f" corresp="#s5:l4">ontbonden</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Al ons <rhyme label="b" type="m" corresp="#s5:l3">misdaet</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beginnen</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s5:l8">beminnen</rhyme></l>
        <l id="s5:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s5:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Met woorden tonen sijn <rhyme label="a" type="m">verdriet</rhyme></l>
        <l id="s6:l2" met="2" type="-+">En kan hy <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maer sijn cleyn oogskens sijn <rhyme label="b" type="m">vervult</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Met vele <rhyme label="c" type="f">tranen</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Om te <rhyme label="c" type="f" corresp="#s6:l4">vermanen</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Der menschen <rhyme label="b" type="m" corresp="#s6:l3">schult</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beginnen</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s6:l8">beminnen</rhyme></l>
        <l id="s6:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s6:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Iesu die tot ons nu ter <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Ghecomen <rhyme label="a" type="m" corresp="#s7:l1">sijt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Wy bidden u geeft ons den <rhyme label="b" type="m">loon</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Dat nae dit <rhyme label="c" type="f">lijden</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Wy ons <rhyme label="c" type="f" corresp="#s7:l4">verblijden</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">In uwen <rhyme label="b" type="m" corresp="#s7:l3">throon</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beginnen</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s7:l8">beminnen</rhyme></l>
        <l id="s7:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s7:l7">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
