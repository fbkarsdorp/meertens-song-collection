    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Psalm 100 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy al die zijt tot vreuchd' <rhyme label="a" type="m">gesint</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Bemerckt, en op u herte <rhyme label="a" type="m" corresp="#s1:l1">bint</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Des grooten Gods <rhyme label="b" type="m">Genaden-troon</rhyme></l>
        <l id="s1:l4" met="4" type="-+">In 't senden van sijn lieve <rhyme label="b" type="m" corresp="#s1:l3">Soon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laet uwe sanghen zijn <rhyme label="a" type="m">bereyt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Tot lof van sijne <rhyme label="a" type="m" corresp="#s2:l1">Majesteyt</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Wilt in u lof, en nu, en <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ghedachtigh zijn aen sijn <rhyme label="b" type="m" corresp="#s2:l3">Geboort'</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Siet op de vijf-mael vijfde <rhyme label="a" type="m">dagh</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Der leste maendt, verschoven <rhyme label="a" type="m" corresp="#s3:l1">lagh</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Te Bethlehem, 't beloofde <rhyme label="b" type="m">Kindt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Van Godt gezeghent en <rhyme label="b" type="m" corresp="#s3:l3">bemindt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Maeght, in dese goede <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Met haren Soon <rhyme label="a" type="m" corresp="#s4:l1">ghebenedijt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gingh soecken eene goedt <rhyme label="b" type="m">logijs</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Naer alle Reysenaren <rhyme label="b" type="m" corresp="#s4:l3">wijs</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer siet, hoe dat het quam te <rhyme label="a" type="m">pas</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Voor haer niet eene plaetse <rhyme label="a" type="m" corresp="#s5:l1">was</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Daer was voor haer geen ruymt' met <rhyme label="b" type="m">al</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Dan in een vuyle <rhyme label="b" type="m" corresp="#s5:l3">Beesten-stal</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Daer heeft de Maget, in der <rhyme label="a" type="m">nacht</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Een Soon ter Werelt <rhyme label="a" type="m" corresp="#s6:l1">voort-gebracht</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Die na Godts eeuwigh <rhyme label="b" type="m">Hemel-raet</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">In onser aller plaetse <rhyme label="b" type="m" corresp="#s6:l3">staet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">By Bethlehem, in 't open <rhyme label="a" type="m">velt</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Daer waren Herderen <rhyme label="a" type="m" corresp="#s7:l1">ghestelt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die door den Engel, <rhyme label="b" type="m">Hemelsch-Geest</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Den blijden Bode, zijn <rhyme label="b" type="m" corresp="#s7:l3">bevreest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Gaet henen sonder vrees, hy <rhyme label="a" type="m">seyde</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Na Bethlehem, en niet <rhyme label="a" type="m" corresp="#s8:l1">verbeydt</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Daer sult ghy vinden zijn <rhyme label="b" type="m">gebaart</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Den Iesus, 't kindt van soeter <rhyme label="b" type="m" corresp="#s8:l3">aert</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Sy gaen dan, vrolijck van <rhyme label="a" type="m">gemoet</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Te soecken 't kleyne Kindtjen <rhyme label="a" type="m" corresp="#s9:l1">soet</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En vinden, na des Engels <rhyme label="b" type="m">woort</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Ons Salighmakers <rhyme label="b" type="m" corresp="#s9:l3">nieu-gheboort'</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Daer was hy in een krib <rhyme label="a" type="m">gheleyt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">En oock Maria gantsch <rhyme label="a" type="m" corresp="#s10:l1">bereyt</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Om wel te passen op sijn <rhyme label="b" type="m">lijf</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Nu zijnde Moeder, Maeght, en <rhyme label="b" type="m" corresp="#s10:l3">Wijf</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Drie wijsen quamen verd' van 't <rhyme label="a" type="m">Oost</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Door eenen Star geleyt, <rhyme label="a" type="m" corresp="#s11:l1">getroost</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Sy rusten niet eenen <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Tot dat' se waren daer hy <rhyme label="b" type="m" corresp="#s11:l3">lagh</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Gekomen zijnde tot de <rhyme label="a" type="m">plaets</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Sy wierpen, sonder veel <rhyme label="a" type="m" corresp="#s12:l1">ghelaets</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">In ned'righeyt voor sijnen <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Haer Gouden gift, en reucken <rhyme label="b" type="m" corresp="#s12:l3">soet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Sy sien den grooten Heer van <rhyme label="a" type="m">Al</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Vernedert in een <rhyme label="a" type="m" corresp="#s13:l1">Beesten-stal</rhyme>;</l>
        <l id="s13:l3" met="4" type="-+">Een soet exempel voor de <rhyme label="b" type="m">mensch</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Die niet en heeft sijn wil, of <rhyme label="b" type="m" corresp="#s13:l3">wensch</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Geen kostelijck gewaet, of <rhyme label="a" type="m">kleedt</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Was voor ons' aller Heer <rhyme label="a" type="m" corresp="#s14:l1">gereedt</rhyme>;</l>
        <l id="s14:l3" met="4" type="-+">Geen melody, gheen <rhyme label="b" type="m">harmony</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Tot dat de Engels singen <rhyme label="b" type="m" corresp="#s14:l3">bly</rhyme>:</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Eer zy den Heer in 't <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">En vreed' op Aerden al <rhyme label="a" type="m" corresp="#s15:l1">gelijck</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Den menschen, Godes <rhyme label="b" type="m">wel-geval</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">En Aerd', en Hemel, voegen <rhyme label="b" type="m" corresp="#s15:l3">sal</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Het hooren van dit Engels <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Gaf allen menschen kind'ren <rhyme label="a" type="m" corresp="#s16:l1">stof</rhyme></l>
        <l id="s16:l3" met="4" type="-+">Tot prijs, en eer, van haren <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Dien hy sijn Sone geeft ten <rhyme label="b" type="m" corresp="#s16:l3">lodt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Dies laet een yeder vrouw, oft <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Gedencken, die ghedencken <rhyme label="a" type="m" corresp="#s17:l1">kan</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Sijn heylighleven, en <rhyme label="b" type="m">gedoen</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Sijn doodt, ons arme sondaers <rhyme label="b" type="m" corresp="#s17:l3">soen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Neemt eens, o mensche! dat ghy <rhyme label="a" type="m">laeght</rhyme></l>
        <l id="s18:l2" met="4" type="-+">Seer vast gevanghen, <rhyme label="a" type="m" corresp="#s18:l1">aengeklaeght</rhyme></l>
        <l id="s18:l3" met="4" type="-+">Ter doot, en dat geen <rhyme label="b" type="m">menschen-waerd'</rhyme></l>
        <l id="s18:l4" met="4" type="-+">V kost randt-soenen op der <rhyme label="b" type="m" corresp="#s18:l3">aerd'</rhyme>:</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Ten zy by uw' party, ghy <rhyme label="a" type="m">vondt</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Die u te lossen noch <rhyme label="a" type="m" corresp="#s19:l1">bestondt</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Met storten van sijns <rhyme label="b" type="m">herten-bloedt</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">En leven laten t'uwen <rhyme label="b" type="m" corresp="#s19:l3">goedt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Dit heeft ons Christi liefd' <rhyme label="a" type="m">gedaen</rhyme>.</l>
        <l id="s20:l2" met="4" type="-+">Wel waerdigh om ter Hel te <rhyme label="a" type="m" corresp="#s20:l1">gaen</rhyme>.</l>
        <l id="s20:l3" met="4" type="-+">Geschreven doch in 's <rhyme label="b" type="m">Levens-boeck</rhyme>.</l>
        <l id="s20:l4" met="4" type="-+">Hy is geworden onsen <rhyme label="b" type="m" corresp="#s20:l3">vloeck</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Terwijl hy op der Aerden <rhyme label="a" type="m">gaet</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">Hy niet een uyr verlooren <rhyme label="a" type="m" corresp="#s21:l1">laet</rhyme></l>
        <l id="s21:l3" met="4" type="-+">In vasten, bidden, al den <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s21:l4" met="4" type="-+">Van sijnen leven hy <rhyme label="b" type="m" corresp="#s21:l3">verslijt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">By daegh hy in den Tempel <rhyme label="a" type="m">leerd'</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">En ons met wonderen <rhyme label="a" type="m" corresp="#s22:l1">vereerd'</rhyme>:</l>
        <l id="s22:l3" met="4" type="-+">De Blinde gaf hy het <rhyme label="b" type="m">ghesicht</rhyme>,</l>
        <l id="s22:l4" met="4" type="-+">En maeckt' der Lamme voeten <rhyme label="b" type="m" corresp="#s22:l3">licht</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Hy heelde der Melaetsen <rhyme label="a" type="m">huydt</rhyme>,</l>
        <l id="s23:l2" met="4" type="-+">En dreef met macht de Duyvels <rhyme label="a" type="m" corresp="#s23:l1">uyt</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">Hy weckte Lazrum uyt het <rhyme label="b" type="m">graf</rhyme>,</l>
        <l id="s23:l4" met="4" type="-+">Hy 't siecke volck gesontheyt <rhyme label="b" type="m" corresp="#s23:l3">gaf</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">En schoon hy dese daden <rhyme label="a" type="m">wrocht</rhyme>,</l>
        <l id="s24:l2" met="4" type="-+">Van Iudas is hy noch <rhyme label="a" type="m" corresp="#s24:l1">gesocht</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Die hem een groet en kusse <rhyme label="b" type="m">biedt</rhyme>,</l>
        <l id="s24:l4" met="4" type="-+">Op dat hy onsen Heer <rhyme label="b" type="m" corresp="#s24:l3">verriedt</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Men leydt hem in den <rhyme label="a" type="m">Rechters-hal</rhyme>,</l>
        <l id="s25:l2" met="4" type="-+">Als d'aldersnoodtse mensch van <rhyme label="a" type="m" corresp="#s25:l1">al</rhyme>;</l>
        <l id="s25:l3" met="4" type="-+">Daer gaf men vonnis voor het <rhyme label="b" type="m">huys</rhyme>,</l>
        <l id="s25:l4" met="4" type="-+">Dat hy soud' sterven aen het <rhyme label="b" type="m" corresp="#s25:l3">kruys</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Men bracht hem op het <rhyme label="a" type="m">galghe-velt</rhyme>,</l>
        <l id="s26:l2" met="4" type="-+">Met groote smaedtheyt en <rhyme label="a" type="m" corresp="#s26:l1">gewelt</rhyme>,</l>
        <l id="s26:l3" met="4" type="-+">Met bitterheyt, en allen <rhyme label="b" type="m">hoon</rhyme>,</l>
        <l id="s26:l4" met="4" type="-+">Gekroont met een <rhyme label="b" type="m" corresp="#s26:l3">Doornen-kroon</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Men maeckt hem handen, voeten <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s27:l2" met="4" type="-+">Sijn lichaem tot een eyghen <rhyme label="a" type="m" corresp="#s27:l1">last</rhyme>:</l>
        <l id="s27:l3" met="4" type="-+">En tot vermeerderingh van <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s27:l4" met="4" type="-+">Men stack hem met een speer in 't <rhyme label="b" type="m" corresp="#s27:l3">hert</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Sijn liefde sien en hooren <rhyme label="a" type="m">wy</rhyme>,</l>
        <l id="s28:l2" met="4" type="-+">Om ons van sond' te maken <rhyme label="a" type="m" corresp="#s28:l1">vry</rhyme>,</l>
        <l id="s28:l3" met="4" type="-+">Door 't storten van sijns <rhyme label="b" type="m">herten-bloedt</rhyme>,</l>
        <l id="s28:l4" met="4" type="-+">Tot ons' gheluck en hooghste <rhyme label="b" type="m" corresp="#s28:l3">goedt</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Hy is na Godes <rhyme label="a" type="m">wel-geval</rhyme>,</l>
        <l id="s29:l2" met="4" type="-+">Een Salighmaker van ons <rhyme label="a" type="m" corresp="#s29:l1">al</rhyme>.</l>
        <l id="s29:l3" met="4" type="-+">Al' eer, en lof, en <rhyme label="b" type="m">heerlijckheyt</rhyme>,</l>
        <l id="s29:l4" met="4" type="-+">Zy hem tot in der <rhyme label="b" type="m" corresp="#s29:l3">eeuwigheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
