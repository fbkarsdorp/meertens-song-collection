        <l id="s1:l4" met="4" type="+-">Die an reynen vrouwen <rhyme label="b" type="m" corresp="#s1:l2">leyt</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Denke an minnentliche <rhyme label="c" type="m">wijp</rhyme>!</l>
        <l id="s1:l6" met="2" type="+-">Kan der <rhyme label="d" type="f">lachen</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Doch nicht <rhyme label="d" type="f" corresp="#s1:l6">machen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Vro, verzaget ist din <rhyme label="c" type="m" corresp="#s1:l5">lijp</rhyme>.</l>
