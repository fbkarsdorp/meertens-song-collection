  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SA, sa, wie is een <rhyme label="a" type="m">Helt</rhyme>?</l>
        <l id="s1:l2" met="4" type="-+">SA, sa, wie is een <rhyme label="a" type="m" corresp="#s1:l1">Helt</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Het moet nu overhandt te <rhyme label="a" type="m" corresp="#s1:l1">Velt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Het moet nu overhandt te <rhyme label="a" type="m" corresp="#s1:l1">Velt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Drie Glaesjes wert u <rhyme label="a" type="m" corresp="#s1:l1">toe-getelt</rhyme></l>
        <l id="s1:l6" met="2" type="-+">'t Sal u <rhyme label="b" type="f">vermaken</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">'t Moet u wel <rhyme label="b" type="f" corresp="#s1:l6">smaken</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Waerom <rhyme label="c" type="m">verdriet</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Want wy sijn voor Caloyen <rhyme label="c" type="m" corresp="#s1:l8">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ay siet wat een noble <rhyme label="a" type="m">vocht</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ay siet wat een noble <rhyme label="a" type="m" corresp="#s2:l1">vocht</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Het heugt my dat ons niet beuren en <rhyme label="a" type="m" corresp="#s2:l1">mocht</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Het heugt my dat ons niet beuren en <rhyme label="a" type="m" corresp="#s2:l1">mocht</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Maer wy hebben 't hans weer een and're <rhyme label="a" type="m" corresp="#s2:l1">Locht</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Soet en <rhyme label="b" type="f">geurigh</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Waerom dan <rhyme label="b" type="f" corresp="#s2:l6">treurigh</rhyme>?</l>
        <l id="s2:l8" met="2" type="-+">Waerom dan <rhyme label="c" type="m">kranck</rhyme>?</l>
        <l id="s2:l9" met="4" type="-+">Want dit marcheren en valt niet <rhyme label="c" type="m" corresp="#s2:l8">lanck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den douw die Bloempjes wasdom <rhyme label="a" type="m">geeft</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Den douw die Bloempjes wasdom <rhyme label="a" type="m" corresp="#s3:l1">geeft</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Van sulcken douw mijn hertje <rhyme label="a" type="m" corresp="#s3:l1">leeft</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Van sulcken douw mijn hertje <rhyme label="a" type="m" corresp="#s3:l1">leeft</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Dus niemant voor een Troytje <rhyme label="a" type="m" corresp="#s3:l1">beeft</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Maer wilt met <rhyme label="b" type="f">verlangen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Het blijlick <rhyme label="b" type="f" corresp="#s3:l6">ontfangen</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Want van dit <rhyme label="c" type="m">nat</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Broutmen te Roterdam in de <rhyme label="c" type="m" corresp="#s3:l8">Stadt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Men wist niet waerom Bacchus <rhyme label="a" type="m">weent</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Men wist niet waerom Bacchus <rhyme label="a" type="m" corresp="#s4:l1">weent</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Als hy om 't Jaer eens wert <rhyme label="a" type="m" corresp="#s4:l1">gespeent</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Als hy om 't Jaer eens wert <rhyme label="a" type="m" corresp="#s4:l1">gespeent</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Maer al waer een Mensch sijn hert <rhyme label="a" type="m" corresp="#s4:l1">versteent</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Het most hem <rhyme label="b" type="f">bewegen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En schreyen als <rhyme label="b" type="f" corresp="#s4:l6">regen</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Door dit <rhyme label="c" type="m">gewelt</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Want de Kan t'hans geen schelling en <rhyme label="c" type="m" corresp="#s4:l8">gelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dit is den Oly diemen <rhyme label="a" type="m">seyt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dit is den Oly diemen <rhyme label="a" type="m" corresp="#s5:l1">seyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Vol vreugden, en vol <rhyme label="a" type="m" corresp="#s5:l1">geestigheyt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Vol vreugden, en vol <rhyme label="a" type="m" corresp="#s5:l1">geestigheyt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Waer in de sorgh begraven <rhyme label="a" type="m" corresp="#s5:l1">leyt</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">s' Is lustigh tot <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Vrolijck van <rhyme label="b" type="f" corresp="#s5:l6">sinnen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Want men wel <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s5:l9" met="4" type="-+">Dat wy zijn t'hans voor Gelder <rhyme label="c" type="m" corresp="#s5:l8">niet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Een Glaesje Bier of Romer <rhyme label="a" type="m">Wijn</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Een Glaesje Bier of Romer <rhyme label="a" type="m" corresp="#s6:l1">Wijn</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Is beter als uyt Maes, of <rhyme label="a" type="m" corresp="#s6:l1">Rhijn</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Is beter als uyt Maes, of <rhyme label="a" type="m" corresp="#s6:l1">Rhijn</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Dit moeten ons af-wassers <rhyme label="a" type="m" corresp="#s6:l1">zijn</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">En sonder <rhyme label="b" type="f">verkoelen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Het water <rhyme label="b" type="f" corresp="#s6:l6">af-spoelen</rhyme>:</l>
        <l id="s6:l8" met="2" type="-+">Datmen <rhyme label="c" type="m">ontfing</rhyme></l>
        <l id="s6:l9" met="4" type="-+">Doe men lest langs der Heyde <rhyme label="c" type="m" corresp="#s6:l8">ging</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Siet daerom dese drie <rhyme label="a" type="m">Santeen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Siet daerom dese drie <rhyme label="a" type="m" corresp="#s7:l1">Santeen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die breng ick als te Lit wy <rhyme label="a" type="m" corresp="#s7:l1">deen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Die breng ick als te Lit wy <rhyme label="a" type="m" corresp="#s7:l1">deen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Op ons welcomst, en yder <rhyme label="a" type="m" corresp="#s7:l1">een</rhyme>;</l>
        <l id="s7:l6" met="2" type="-+">Ick sal het <rhyme label="b" type="f">aenvaerden</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Ter eeren de <rhyme label="b" type="f" corresp="#s7:l6">Gaerden</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Nobel en <rhyme label="c" type="m">braef</rhyme>,</l>
        <l id="s7:l9" met="4" type="-+">Want 't is hier beter als voor de <rhyme label="c" type="m" corresp="#s7:l8">Graef</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
