<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25099</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Barbara die reyne <rhyme label="a" type="m">maecht</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Een edel bruyt ons <rhyme label="b" type="f">heeren</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Haer leuen dat heeft gode <rhyme label="a" type="m" corresp="#s1:l1">behaecht</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hets recht dat wise <rhyme label="b" type="f" corresp="#s1:l2">eeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Baptista die heilege <rhyme label="a" type="m">man</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Bouen al si <rhyme label="b" type="f">minde</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Wiens leuen si ane <rhyme label="a" type="m" corresp="#s2:l1">nam</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Want si hem groot <rhyme label="b" type="f" corresp="#s2:l2">bekinde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Goet ende hoge <rhyme label="a" type="m">edelheit</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Dat wou si al <rhyme label="b" type="f">versmaden</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">En haer scone <rhyme label="a" type="m" corresp="#s3:l1">minlijcheit</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Keerde si in <rhyme label="b" type="f" corresp="#s3:l2">genaden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gods wijsheit had si diep <rhyme label="a" type="m">vercleert</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Inder hoechster <rhyme label="b" type="f">eenicheit</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Dat haer was <rhyme label="a" type="m" corresp="#s4:l1">geopenbaert</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Dat wesen der <rhyme label="b" type="f" corresp="#s4:l2">driewldicheit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Si es een reyn rose <rhyme label="a" type="m">root</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Want god sprac in haer <rhyme label="b" type="f">liden</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Dat van harer pinen <rhyme label="a" type="m" corresp="#s5:l1">groot</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Die ingelen souden <rhyme label="b" type="f" corresp="#s5:l2">verbliden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">God sant haer al <rhyme label="a" type="m">ouerluut</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">Een cleet was <rhyme label="b" type="f">minnelike</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Hi sprac nv comt mijn scone <rhyme label="a" type="m" corresp="#s6:l1">bruut</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hier bouen in mijn <rhyme label="b" type="f" corresp="#s6:l2">rike</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Si heet der menschen <rhyme label="a" type="m">toeuerlaet</rhyme>.</l>
        <l id="s7:l2" met="3" type="-+">Van gode heeft sijt <rhyme label="b" type="f">vercregen</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Die tot haren dienste <rhyme label="a" type="m" corresp="#s7:l1">staet</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Haer sonden sal god <rhyme label="b" type="f" corresp="#s7:l2">vergeuen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
