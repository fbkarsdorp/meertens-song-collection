<song>
  <meta>
    <id>160331</id>
    <date>
      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Adieu nymphe du bois</melody>
  <author>Augustinus van Teylingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Maria, Maghet <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Schoon, Suyver, klaer <rhyme label="a" type="m" corresp="#s1:l1">fonteyn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Godts Tabernakel <rhyme label="b" type="f">schoone</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Wy menschen, groot en <rhyme label="a" type="m" corresp="#s1:l1">kleyn</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Loven u al <rhyme label="a" type="m" corresp="#s1:l1">ghemeyn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met sangen soet van <rhyme label="b" type="f" corresp="#s1:l3">thoone</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon als den <rhyme label="a" type="m">dagheraet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met Goddelijck <rhyme label="a" type="m" corresp="#s2:l1">cieraet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">V licht is klaer in’t <rhyme label="b" type="f">blincken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Want ghy altijdt <rhyme label="a" type="m" corresp="#s2:l1">by-staet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Met troost, vroegh ende <rhyme label="a" type="m" corresp="#s2:l1">laet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die uwen lof doen <rhyme label="b" type="f" corresp="#s2:l3">klincken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy zijt ons levens <rhyme label="a" type="m">vreught</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Daer Godt in heeft <rhyme label="a" type="m" corresp="#s3:l1">verheught</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Want, boven alle <rhyme label="b" type="f">Vrouwen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sijt ghy vol aller <rhyme label="a" type="m" corresp="#s3:l1">deught</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Naest Godt ghy veel <rhyme label="a" type="m" corresp="#s3:l1">vermeught</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ghy helpt ons uyt <rhyme label="b" type="f" corresp="#s3:l3">‘tbenouwen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vol van ghenaden <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghy, en <rhyme label="a" type="m" corresp="#s4:l1">ghebenedijt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Maria hoogh <rhyme label="b" type="f">verheven</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Dus seggh’ ick <rhyme label="a" type="m" corresp="#s4:l1">onvermijt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Den ketterem tot <rhyme label="a" type="m" corresp="#s4:l1">spijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Gunt ons het eeuwigh <rhyme label="b" type="f" corresp="#s4:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Princesse Maghet <rhyme label="a" type="m">wijs</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Aarons bloeyend’ <rhyme label="a" type="m" corresp="#s5:l1">rijs</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Lof moet u altijdt <rhyme label="b" type="f">wesen</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">V zy eer ende <rhyme label="a" type="m" corresp="#s5:l1">prijs</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Met Lof-sangen <rhyme label="a" type="m" corresp="#s5:l1">propijs</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Reyn Maghet <rhyme label="b" type="f" corresp="#s5:l3">uyt-ghelesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
