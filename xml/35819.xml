    <melody>O rad van avontuur</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">BEwaerders van mijn <rhyme label="a" type="f">ziele</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Engl'en van d' eerste <rhyme label="b" type="m">Choor</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Voor u ick <rhyme label="a" type="f" corresp="#s1:l1">nederkniele</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Lof en prijs u daer <rhyme label="b" type="m" corresp="#s1:l2">voor</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Drievuldigheyt wilt <rhyme label="c" type="f">prijsen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Om de weldaden <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die al uyt haer <rhyme label="c" type="f" corresp="#s1:l5">verrijsen</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">En wilt haer eer <rhyme label="c" type="f" corresp="#s1:l5">bewijsen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Voor mijne Sondaer <rhyme label="d" type="m" corresp="#s1:l6">snoodt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn Jesus is <rhyme label="a" type="f">Besneden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat op den achtsten <rhyme label="b" type="m">dach</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">'t Bloedt vloeyde uyt sijn <rhyme label="a" type="f" corresp="#s2:l1">leden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gelijck men doe klaer <rhyme label="b" type="m" corresp="#s2:l2">sach</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Aen u Vader, 't bloedt 'k <rhyme label="c" type="f">toone</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En u heylige <rhyme label="d" type="m">Geest</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Door 't bloedt van uwe <rhyme label="c" type="f" corresp="#s2:l5">Soone</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Van sonden maeckt nu <rhyme label="c" type="f" corresp="#s2:l5">schoone</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Mijn ziel en die <rhyme label="d" type="m" corresp="#s2:l6">geneest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Voor ons thoont <rhyme label="a" type="f">danckbaerheden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een Godt, Aerdsch-Eng'len <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om dat met Mensche <rhyme label="a" type="f" corresp="#s3:l1">leden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Jesus lach in een <rhyme label="b" type="m" corresp="#s3:l2">Stal</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Broeders wilt niet <rhyme label="c" type="f">vergeeten</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">'t Bloedt veel dat Jesus <rhyme label="d" type="m">sweet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In 't Hof van <rhyme label="c" type="f" corresp="#s3:l5">Oliveeten</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Geeft Heer, die Christ'nen <rhyme label="c" type="f" corresp="#s3:l5">heeten</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Dat elck sijn wil <rhyme label="d" type="m" corresp="#s3:l6">vergeet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy van den derden <rhyme label="a" type="f">Choore</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Overheden <rhyme label="b" type="m">genaemt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Drievuldicheyt laet <rhyme label="a" type="f" corresp="#s4:l1">hooren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Lof, voor my soo 't <rhyme label="b" type="m" corresp="#s4:l2">betaemt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Door 't geesselen krijght <rhyme label="c" type="f">wonden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ghy mijnen Jesu <rhyme label="d" type="m">veel</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Vergeeft hier door mijn <rhyme label="c" type="f" corresp="#s4:l5">sonden</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Want suyver alle <rhyme label="c" type="f" corresp="#s4:l5">stonden</rhyme></l>
        <l id="s4:l9" met="3" type="-+">'k Soeck u te dienen <rhyme label="d" type="m" corresp="#s4:l6">heel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Met my looft <rhyme label="a" type="f">Mogentheden</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Drievuldigheyt seer <rhyme label="b" type="m">sterck</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Dat ick ben van de <rhyme label="a" type="f" corresp="#s5:l1">Leden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van d'ware Roomsche <rhyme label="b" type="m" corresp="#s5:l2">Kerck</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Jesu voor u men <rhyme label="c" type="f">maeckte</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Een scherpe doornen <rhyme label="d" type="m">Kroon</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die uw' Harsenen <rhyme label="c" type="f" corresp="#s5:l5">raeckte</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Geeft mijn, waer na ick <rhyme label="c" type="f" corresp="#s5:l5">haeckte</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">U vreught in 's Hemels <rhyme label="d" type="m" corresp="#s5:l6">Throon</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Met mijne Hemelsche <rhyme label="a" type="f">krachten</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">Drievuldigheyt oock <rhyme label="b" type="m">looft</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Versoeckt, dat Sathans <rhyme label="a" type="f" corresp="#s6:l1">wachten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Mijn Ziel niet en <rhyme label="b" type="m" corresp="#s6:l2">berooft</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Kruyst hem, kruyst hem, ick <rhyme label="c" type="f">hoore</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ende, aensiet den <rhyme label="d" type="m">Mensch</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Geeft dat 'k niet gae <rhyme label="c" type="f" corresp="#s6:l5">verloren</rhyme>!</l>
        <l id="s6:l8" met="3" type="-+">Maer met u <rhyme label="c" type="f" corresp="#s6:l5">uyt-verkoren</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Te staen, na mijnen <rhyme label="d" type="m" corresp="#s6:l6">wensch</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Looft Godt met <rhyme label="a" type="f">d'Heerschappy</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat heel en <rhyme label="b" type="m">ongeschent</rhyme></l>
        <l id="s7:l3" met="3" type="-+">d' Heer ons spijst dese <rhyme label="a" type="f" corresp="#s7:l1">ty'en</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In 't Heyligh <rhyme label="b" type="m" corresp="#s7:l2">Sacrament</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Iesu, ghy hebt <rhyme label="c" type="f">gedragen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">'t Kruys voor mijn sonden <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">'t Bloedt over al men <rhyme label="c" type="f" corresp="#s7:l5">saghen</rhyme>:</l>
        <l id="s7:l8" met="3" type="-+">Ey! door 't bloedt wilt af <rhyme label="c" type="f" corresp="#s7:l5">vaghen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Mijn sonden voor mijn <rhyme label="d" type="m" corresp="#s7:l6">doodt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Laet ons, laet ons nu <rhyme label="a" type="f">Throonen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Loven <rhyme label="b" type="m">Drievuldigheyt</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">Ghy wilt u niet <rhyme label="a" type="f" corresp="#s8:l1">verschoonen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hier toe zijt ghy <rhyme label="b" type="m" corresp="#s8:l2">bereyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Kleederen sy <rhyme label="c" type="f">uyttrecken</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Van u Iesu mijn <rhyme label="d" type="m">vrindt</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Uw' leden sy <rhyme label="c" type="f" corresp="#s8:l5">uytrecken</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Aen 't Kruys sy u <rhyme label="c" type="f" corresp="#s8:l5">begecken</rhyme>:</l>
        <l id="s8:l9" met="3" type="-+">Maeckt mijn hier door u <rhyme label="d" type="m" corresp="#s8:l6">Kint</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Niet met u <rhyme label="a" type="f">Cherubijnen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Te singen Godes <rhyme label="b" type="m">lof</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">'t Souw my zijn groote <rhyme label="a" type="f" corresp="#s9:l1">pijnen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Al ben ick as en <rhyme label="b" type="m" corresp="#s9:l2">stof</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Ghy hangt aen 't Kruys in 't <rhyme label="c" type="f">lijden</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Jesu, een langen <rhyme label="d" type="m">tijdt</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Geeft my oock vroom te <rhyme label="c" type="f" corresp="#s9:l5">strijden</rhyme>.</l>
        <l id="s9:l8" met="3" type="-+">Met den Moorder <rhyme label="c" type="f" corresp="#s9:l5">verblijden</rhyme></l>
        <l id="s9:l9" met="3" type="-+">Wilt, my na mijnen <rhyme label="d" type="m" corresp="#s9:l6">strijdt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Mijn hart, en al mijn <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Met Seraphijnen <rhyme label="b" type="m">al</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Drievuldigheyt <rhyme label="a" type="f" corresp="#s10:l1">beminnen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Ende beminnen <rhyme label="b" type="m" corresp="#s10:l2">sal</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Jesu door al u <rhyme label="c" type="f">leven</rhyme>;</l>
        <l id="s10:l6" met="3" type="-+">Door u doodt ende <rhyme label="d" type="m">Kruys</rhyme>:</l>
        <l id="s10:l7" met="3" type="-+">Ey! wilt ons alle <rhyme label="c" type="f" corresp="#s10:l5">geven</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Ey! wilt ons al <rhyme label="c" type="f" corresp="#s10:l5">verheven</rhyme></l>
        <l id="s10:l9" met="3" type="-+">By u in 's Hemels <rhyme label="d" type="m" corresp="#s10:l6">Huys</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Knielende wy <rhyme label="a" type="f">neervallen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Voor u <rhyme label="b" type="m">Drievuldigheyt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Niet eenen, maer wy <rhyme label="a" type="f" corresp="#s11:l1">allen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Met groot <rhyme label="b" type="m" corresp="#s11:l2">ootmoedigheyt</rhyme>:</l>
        <l id="s11:l5" met="3" type="-+">Off'ren van u <rhyme label="c" type="f">Lof-sangen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Aen u nu eene <rhyme label="d" type="m">Kroon</rhyme>;</l>
        <l id="s11:l7" met="3" type="-+">Wilt dese doch <rhyme label="c" type="f" corresp="#s11:l5">ontfangen</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Na ons wensch, en <rhyme label="c" type="f" corresp="#s11:l5">verlangen</rhyme>:</l>
        <l id="s11:l9" met="3" type="-+">Och! dat hy maer was <rhyme label="d" type="m" corresp="#s11:l6">schoon</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
