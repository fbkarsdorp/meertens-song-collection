<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191579</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Op u betrouw ik Heer</melody>
  <author>Marijn de Brauwer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VRiendekens wilt <rhyme label="a" type="f">aenmerken</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Op dat ghy niet en <rhyme label="b" type="m">faelt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Want nu door Satans <rhyme label="a" type="f" corresp="#s1:l1">werken</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Soo menich mensche <rhyme label="b" type="m" corresp="#s1:l2">dwaelt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Alsoo Christus <rhyme label="b" type="m" corresp="#s1:l2">verhaelt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Int Evangelium al <rhyme label="c" type="f">vooren</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Zy sullen seggen <rhyme label="d" type="m">voorwaer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Christus is hier en <rhyme label="d" type="m" corresp="#s1:l7">daer</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Maer en wiltse niet <rhyme label="c" type="f" corresp="#s1:l6">hooren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Satan vanden <rhyme label="a" type="f">beginne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die heeft altijdt <rhyme label="b" type="m">benijdt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat wy t' onsen <rhyme label="a" type="f" corresp="#s2:l1">gewinne</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van God worden <rhyme label="b" type="m" corresp="#s2:l2">bevrijt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Daer omme voert hy <rhyme label="b" type="m" corresp="#s2:l2">strijdt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met vele boven <rhyme label="c" type="f">maten</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Als Pharao met moede <rhyme label="d" type="m">fel</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ginck vervolgen <rhyme label="d" type="m" corresp="#s2:l7">Israel</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Als zy hem wilden <rhyme label="c" type="f" corresp="#s2:l6">verlaten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo lange als zy hem <rhyme label="a" type="f">dienden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En was Pharao niet <rhyme label="b" type="m">gram</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Maer waren soo zijn <rhyme label="a" type="f" corresp="#s3:l1">Vrienden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">tot der tijdt dat Moyses <rhyme label="b" type="m" corresp="#s3:l2">quam</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Als Pharao dat <rhyme label="b" type="m" corresp="#s3:l2">vernam</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ginck hyse seer <rhyme label="c" type="f">benyden</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die nu van desen wil <rhyme label="d" type="m">scheen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Na Gods woort zijn leven <rhyme label="d" type="m" corresp="#s3:l7">leen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Die moeten altijdt <rhyme label="c" type="f" corresp="#s3:l6">strijden</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy hebben meer te <rhyme label="a" type="f">strijden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dan tegen vlees en <rhyme label="b" type="m">bloet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Alle die ons <rhyme label="a" type="f" corresp="#s4:l1">benyden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">maeckt ons Paulus <rhyme label="b" type="m" corresp="#s4:l2">vroet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Tegen den hoogen <rhyme label="b" type="m" corresp="#s4:l2">moet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van dese Werelt <rhyme label="c" type="f">machtich</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Tegen Vorsten en <rhyme label="d" type="m">gewelt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Daer sijn wy tegen <rhyme label="d" type="m" corresp="#s4:l7">ghestelt</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Om altijdt te strijden <rhyme label="c" type="f" corresp="#s4:l6">krachtich</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Neemt Gods woort daer en <rhyme label="a" type="f">tussen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Als een Sweert aen uwer <rhyme label="b" type="m">sije</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Daer me condt ghy uyt <rhyme label="a" type="f" corresp="#s5:l1">blusschen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">De pijlen van u <rhyme label="b" type="m" corresp="#s5:l2">partije</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Treck aen met moede <rhyme label="b" type="m" corresp="#s5:l2">blye</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat Harnas Gods <rhyme label="c" type="f">eersame</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dat ghy t' uwer baten // <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Meught van zijn Soldaten // <rhyme label="d" type="m" corresp="#s5:l7">sijn</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">En voeren zynen <rhyme label="c" type="f" corresp="#s5:l6">Name</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Als vroome Christen <rhyme label="a" type="f">knechten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Weest altijdt <rhyme label="b" type="m">bereyt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Neemt op u hooft om <rhyme label="a" type="f" corresp="#s6:l1">vechten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Den Helm der <rhyme label="b" type="m" corresp="#s6:l2">zaligheyt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Van den Krist, oock niet <rhyme label="b" type="m" corresp="#s6:l2">scheydt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Des geloofs schilt, wilt <rhyme label="c" type="f">vaten</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En alsoo ten strijde <rhyme label="d" type="m">gaen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Om u vijant <rhyme label="d" type="m" corresp="#s6:l7">wederstaen</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Ghelijck Christus <rhyme label="c" type="f" corresp="#s6:l6">Soldaten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hout altijdt goede <rhyme label="a" type="f">wachte</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Voor de Stadt <rhyme label="b" type="m">delicaet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Den vyandt dach en <rhyme label="a" type="f" corresp="#s7:l1">nachte</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Ons als een Leeu om <rhyme label="b" type="m" corresp="#s7:l2">gaet</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Nijdich en <rhyme label="b" type="m" corresp="#s7:l2">opstinaer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Oft hy koste <rhyme label="c" type="f">verwerven</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Door eenich listigh <rhyme label="d" type="m">feyt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Op den wegh die daer <rhyme label="d" type="m" corresp="#s7:l7">afleyt</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Om ons Zielen te <rhyme label="c" type="f" corresp="#s7:l6">verderven</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Vele valsche <rhyme label="a" type="f">Propheten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Sullender <rhyme label="b" type="m">opstaen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dit laet ons Christus <rhyme label="a" type="f" corresp="#s8:l1">weten</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Al deur zijn goet <rhyme label="b" type="m" corresp="#s8:l2">vermaen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Waert mogelijck <rhyme label="b" type="m" corresp="#s8:l2">gedaen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Sonden ons maken <rhyme label="c" type="f">dwalich</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Maer wie volstandich <rhyme label="d" type="m">blijft</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Soo ons Mathei <rhyme label="d" type="m" corresp="#s8:l7">schrijft</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Sullen hier na worden <rhyme label="c" type="f" corresp="#s8:l6">zalich</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
