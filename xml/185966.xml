      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SIngt en weest vrolyk, nu Fillis <rhyme label="a" type="m">Verjaart</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Wens haar lang <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Ten hemel te <rhyme label="b" type="f" corresp="#s1:l2">streeven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Want byde is zy <rhyme label="a" type="m" corresp="#s1:l1">waard</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Zingt en weest vrolyk nu Fillis <rhyme label="a" type="m" corresp="#s1:l1">verjaart</rhyme>.</l>
