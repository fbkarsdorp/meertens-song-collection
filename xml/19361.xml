    <date>
      <year-start>1552</year-start>
      <year-end>1554</year-end>
    </date>
    <location>
      <orginal-name>antwerpen]</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Gevaar gevaar hoe is het met uw vlas</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ONs weghen//ter <rhyme label="a" type="f">deghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Heer recht voor v <rhyme label="b" type="m">bereyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Uwen goddelijcken <rhyme label="a" type="f" corresp="#s1:l1">seghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gheeft ons Heer sonder <rhyme label="b" type="m" corresp="#s1:l2">verbeyt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Dat wy recht <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Uast op v <rhyme label="c" type="m" corresp="#s1:l5">baen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ende houden v <rhyme label="d" type="m">sermoon</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Want sonder Heer v <rhyme label="e" type="f">helpe</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Soo en connen wy niet <rhyme label="d" type="m" corresp="#s1:l7">ghedoen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Te <rhyme label="a" type="f">voren//verloren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soo waren wy alle <rhyme label="b" type="m">ghelijck</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Door Adams <rhyme label="a" type="f" corresp="#s2:l1">wederhooren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ghestooten wt Gods <rhyme label="b" type="m" corresp="#s2:l2">rijck</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Maer doen Christus <rhyme label="c" type="m">quam</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Ende hy wech <rhyme label="c" type="m" corresp="#s2:l5">nam</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Al onse sonden <rhyme label="d" type="m">swaer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Alleen door zijn <rhyme label="e" type="f">verdiensten</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Heeft hy ons ghereynicht <rhyme label="d" type="m" corresp="#s2:l7">claer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Onse sonden//die <rhyme label="a" type="f">vonden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Den throon van vruechden <rhyme label="b" type="m">vast</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Nv is hy ons van God <rhyme label="a" type="f" corresp="#s3:l1">ghesonden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die draecht al onse sonden <rhyme label="b" type="m" corresp="#s3:l2">last</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Dat beloofde <rhyme label="c" type="m">saet</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Ons <rhyme label="c" type="m" corresp="#s3:l5">toeuerlaet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hy maect ons met hem <rhyme label="d" type="m">ghemeen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Hy es alleen ons <rhyme label="e" type="f">middelaer</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Byden Uader en anders <rhyme label="d" type="m" corresp="#s3:l7">gheen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">
          <rhyme label="a" type="f">Prince//beminse</rhyme>
        </l>
        <l id="s4:l2" met="3" type="-+">U aerden vaetkens <rhyme label="b" type="m">broos</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Uoor v wtuercoren <rhyme label="a" type="f" corresp="#s4:l1">kinse</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Al is haer leuen <rhyme label="b" type="m" corresp="#s4:l2">boos</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Wy cranck als <rhyme label="c" type="m">riet</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Wy en vermoghen <rhyme label="c" type="m" corresp="#s4:l5">niet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Tot v Heer willen wy <rhyme label="d" type="m">vlien</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Sijn wy in liefden <rhyme label="e" type="f">vierich</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Het moet al door v <rhyme label="d" type="m" corresp="#s4:l7">gheschien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
