<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185084</id>
    <date>
      <year-start>1805</year-start>
      <year-end>1805</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het meisje van Amsterdam</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Eerst was de loop by <rhyme label="a" type="m">Willemyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Nu is hy weer by <rhyme label="a" type="m" corresp="#s1:l1">Suverein</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Nu schuilt het agter 't <rhyme label="b" type="f">Gordyntje</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Nu is hy glad, en op het <rhyme label="c" type="m">pad</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Nu loopt hy op het <rhyme label="b" type="f" corresp="#s1:l3">eintje</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dirk reed naar de Stad <rhyme label="a" type="m">terstond</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Om te zien of hy zyn Vader <rhyme label="a" type="m" corresp="#s2:l1">vond</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hy zogt daar als de <rhyme label="b" type="f">blinden</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Het geld was weg, zo ieder <rhyme label="c" type="m">zegt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy kon zyn Vader niet <rhyme label="b" type="f" corresp="#s2:l3">vinden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zy maakte daar een ieder <rhyme label="a" type="m">wys</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dat het geld was gezonken door 't <rhyme label="a" type="m" corresp="#s3:l1">ys</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Het zal wel weer <rhyme label="b" type="f">verdoooven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Men zegt dat hy een Kampen <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat zou ik eer <rhyme label="b" type="f" corresp="#s3:l3">gelooven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Eerst was hy een groene <rhyme label="a" type="m">Baas</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hy nam vier Knegten met 'er <rhyme label="a" type="m" corresp="#s4:l1">haast</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Eerst reed hy met Paart en <rhyme label="b" type="f">Waagen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Nu is hy weg met al het <rhyme label="c" type="m">geld</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat hy zo bloedig moest <rhyme label="b" type="f" corresp="#s4:l3">draagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wie had het gedagt van <rhyme label="a" type="m">Suverein</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dat hy was een groot man in <rhyme label="a" type="m" corresp="#s5:l1">schyn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wilt geen hoop op menschen <rhyme label="b" type="f">bouwen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ment zietze op de kop, maar niet in de <rhyme label="c" type="m">krop</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Men kan op hem niet <rhyme label="b" type="f" corresp="#s5:l3">vertrouwen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Nu komt Suverein weer <rhyme label="a" type="m">thuis</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Zonder te maaken een <rhyme label="a" type="m" corresp="#s6:l1">gedruis</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En heeft nog wat geld <rhyme label="b" type="f">ontfangen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Nu is hy weer na Kampen <rhyme label="c" type="m">gegaan</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En dat was zyn <rhyme label="b" type="f" corresp="#s6:l3">verlangen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Nu is hy weg voor de laatste <rhyme label="a" type="m">keer</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Zyn leeven ziet men hem niet <rhyme label="a" type="m" corresp="#s7:l1">meer</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Hy heeft zyn Vrouw <rhyme label="b" type="f">verlaaten</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En ook zyn Kinderen met <rhyme label="c" type="m">haar</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En wie zou daar niet van <rhyme label="b" type="f" corresp="#s7:l3">praaten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Die dat Liedje heeft <rhyme label="a" type="m">gedicht</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Het was voor Suverein <rhyme label="a" type="m" corresp="#s8:l1">verricht</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Hy was niet beter <rhyme label="b" type="f">waardig</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Want die schand die was te <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Daarom was hy dat wel <rhyme label="b" type="f" corresp="#s8:l3">waardig</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
