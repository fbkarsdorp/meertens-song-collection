<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24822</id>
    <date>
      <year-start>1425</year-start>
      <year-end>1430</year-end>
    </date>
    <tags>
      <tag>new year song</tag>
      <tag>secular song</tag>
      <tag>love song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Tsu eyme nyen <rhyme label="a" type="m">jaer</rhyme></l>
        <l id="s1:l2" met="4" type="-+">gheÜe ich mir mit willen <rhyme label="b" type="m">dyr</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">ghehure, zelighe <rhyme label="a" type="m" corresp="#s1:l1">claer</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Wes des aen <rhyme label="a" type="m" corresp="#s1:l1">vaer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">des du wankes icht an <rhyme label="b" type="m" corresp="#s1:l2">myr</rhyme></l>
        <l id="s1:l6" met="3" type="-+">groes vindes tsÜ eyme <rhyme label="a" type="m" corresp="#s1:l1">haer</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Ich un was men nement <rhyme label="c" type="m">mijn</rhyme> -</l>
        <l id="s1:l8" met="4" type="-+">nicht zonders icht us <rhyme label="d" type="f">sceyde</rhyme> -</l>
        <l id="s1:l9" met="4" type="-+">das ist eyghentlijchen <rhyme label="c" type="m" corresp="#s1:l7">diin</rhyme>:</l>
        <l id="s1:l10" met="2" type="-+">des zeldu <rhyme label="c" type="m" corresp="#s1:l7">ziin</rhyme></l>
        <l id="s1:l11" met="4" type="+-">gaer zicher, by mime <rhyme label="d" type="f" corresp="#s1:l8">eyde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vrouwe, us al der werelt <rhyme label="a" type="m">ghemeyne</rhyme></l>
        <l id="s2:l2" met="4" type="-+">han ich dyr tsu lieve <rhyme label="b" type="m">erkoren</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Vyl minnentliiche <rhyme label="a" type="m" corresp="#s2:l1">reyne</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">gheweldich bystu <rhyme label="a" type="m" corresp="#s2:l1">eyne</rhyme></l>
        <l id="s2:l5" met="4" type="-+">mijns gheleistens, tzwaer, des <rhyme label="b" type="m" corresp="#s2:l2">tzorn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">das acht ich <rhyme label="a" type="m" corresp="#s2:l1">cleyne</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Dijn bin ich unde niemens <rhyme label="c" type="m">me</rhyme>.</l>
        <l id="s2:l8" met="4" type="-+">Och das bedenche, <rhyme label="d" type="f">goyte</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">unde wes mer nicht <rhyme label="c" type="m" corresp="#s2:l7">gheÜe</rhyme>.</l>
        <l id="s2:l10" met="2" type="-+">Miin wol, miin <rhyme label="c" type="m" corresp="#s2:l7">wee</rhyme></l>
        <l id="s2:l11" met="4" type="+-">haystÜ in diin <rhyme label="d" type="f" corresp="#s2:l8">hoyte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vrouwe, mijnder zalden <rhyme label="a" type="m">gaerte</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">holt van minen worden <rhyme label="b" type="m">nicht</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">daer werke miin dou <rhyme label="a" type="m" corresp="#s3:l1">warte</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">verzuchte mich gaer <rhyme label="a" type="m" corresp="#s3:l1">harte</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Vindes du dan wankes <rhyme label="b" type="m" corresp="#s3:l2">icht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">an <rhyme label="a" type="m" corresp="#s3:l1">mir</rhyme> [...........],</l>
        <l id="s3:l7" met="4" type="-+">[.....] zo laes mir <rhyme label="c" type="m">verterbin</rhyme> [..]</l>
        <l id="s3:l8" met="4" type="-+">aen alre leye <rhyme label="d" type="f">erbarmen</rhyme>.</l>
        <l id="s3:l9" met="4" type="-+">Bin ich aÜer untrÜwen <rhyme label="c" type="m" corresp="#s3:l7">baer</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">des neme oech <rhyme label="c" type="m" corresp="#s3:l7">waer</rhyme></l>
        <l id="s3:l11" met="4" type="+-">unde troeste mir vyl <rhyme label="d" type="f" corresp="#s3:l8">armen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
