<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3155</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Hei hoe helder schijnt het maantje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALyda, als mijn <rhyme label="a" type="f">gedachjes</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Spelen op u <rhyme label="b" type="m">aerdigheen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Weet het soetste van mijn <rhyme label="a" type="f" corresp="#s1:l1">krachjes</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Smelt, en vloeyt door al mijn <rhyme label="b" type="m" corresp="#s1:l2">leen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">O schoon, Ha <rhyme label="c" type="m">Goon</rhyme>!</l>
        <l id="s1:l6" met="3" type="+-">Heeft Natuer wel <rhyme label="d" type="m">oyt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Soo een Maeght in als <rhyme label="d" type="m" corresp="#s1:l6">voltoyt</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Ha Goon, heeft Natuer wel <rhyme label="d" type="m" corresp="#s1:l6">oyt</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Soo haer milde gonst <rhyme label="d" type="m" corresp="#s1:l6">gestroyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sien ick Alyda u <rhyme label="a" type="f">Oogjes</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Die soo soet en lodder <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">O verschaut met ronde <rhyme label="a" type="f" corresp="#s2:l1">booghjes</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Yder als een halve <rhyme label="b" type="m" corresp="#s2:l2">Maen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">'k Verblindt. En <rhyme label="c" type="m">vint</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Sulcken <rhyme label="d" type="m">aerdigheyt</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Die daer in beschaduwt <rhyme label="d" type="m" corresp="#s2:l6">leyt</rhyme>;</l>
        <l id="s2:l8" met="4" type="+-">En vint Suclken <rhyme label="d" type="m" corresp="#s2:l6">aerdigheyt</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Ja den Hemel daer <rhyme label="d" type="m" corresp="#s2:l6">bereydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Schynt u Kaeckjes Oorlog <rhyme label="a" type="f">voeren</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Tegen uwen <rhyme label="b" type="m">roose-mont</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Om de Bloosjes af te <rhyme label="a" type="f" corresp="#s3:l1">loeren</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Die haer Lipjens zijn <rhyme label="b" type="m" corresp="#s3:l2">gejont</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">En 't wit, Tgeen <rhyme label="c" type="m">sit</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Op het <rhyme label="d" type="m">silver-vel</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Ick by 't blanck Albaster <rhyme label="d" type="m" corresp="#s3:l6">stel</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Tgeen sit Op het <rhyme label="d" type="m" corresp="#s3:l6">silver-vel</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Evenaert met Venus <rhyme label="d" type="m" corresp="#s3:l6">wel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laet Jupijn vry <rhyme label="a" type="f">domineren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">In gedaente van een <rhyme label="b" type="m">Swaen</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">En met Leda schoon <rhyme label="a" type="f" corresp="#s4:l1">boeleren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En Euroop' ontschaken <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Wt lust. soo <rhyme label="c" type="m">rust</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Noch mijn hart, en <rhyme label="d" type="m">sin</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Op mijn soete <rhyme label="d" type="m" corresp="#s4:l6">Engelin</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Soo rust Noch mijn hart, en <rhyme label="d" type="m" corresp="#s4:l6">sin</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Op die geen die ick <rhyme label="d" type="m" corresp="#s4:l6">bemin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Waren dees soo schoon van <rhyme label="a" type="f">Leden</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Als wy waren <rhyme label="b" type="m">moeder-naeckt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">'k Meen mijn Liefjes <rhyme label="a" type="f" corresp="#s5:l1">poeselheden</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Dienen niet te zijn <rhyme label="b" type="m" corresp="#s5:l2">gelaeckt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Want 't moet Soo <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Zijn als 't <rhyme label="d" type="m">aen-gesicht</rhyme></l>
        <l id="s5:l7" met="4" type="+-">'t Geen de Son, en Maen <rhyme label="d" type="m" corresp="#s5:l6">verlicht</rhyme>.</l>
        <l id="s5:l8" met="4" type="+-">Soo soet Is haer <rhyme label="d" type="m" corresp="#s5:l6">aengesicht</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Dat het Mensch en Goon <rhyme label="d" type="m" corresp="#s5:l6">verplicht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
