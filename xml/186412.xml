<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186412</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Gij mannen van verstand</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAt my Jupijn quam <rhyme label="a" type="f">noden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In 't midden van de <rhyme label="a" type="f" corresp="#s1:l1">Goden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ick wedd' ick dronck noch <rhyme label="b" type="m">at</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Veel min soud' ik om 't <rhyme label="c" type="f">sommen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Schoon ik by Venus <rhyme label="b" type="m" corresp="#s1:l3">sat</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn oude rugge eens <rhyme label="c" type="f" corresp="#s1:l4">krommen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al waeren 's <rhyme label="a" type="f">Hemels-saelen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Vervult met gulde <rhyme label="a" type="f" corresp="#s2:l1">schaelen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met blyschap, en <rhyme label="b" type="m">gelach</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al sag ik 'er Nectar <rhyme label="c" type="f">stromen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">'k Souw seggen, Jupijn, ik <rhyme label="b" type="m" corresp="#s2:l3">mag</rhyme></l>
        <l id="s2:l6" met="3" type="-+">By dat canaille niet <rhyme label="c" type="f" corresp="#s2:l4">komen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Je bent een <rhyme label="a" type="f">hoere-jaeger</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Je hebt soo meen'gen <rhyme label="a" type="f" corresp="#s3:l1">swaeger</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Als hayren op je <rhyme label="b" type="m">hooft</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Om een nacht by te <rhyme label="c" type="f">slaepen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hebje de eer <rhyme label="b" type="m" corresp="#s3:l3">ontrooft</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Aen't geenje selfs hadt <rhyme label="c" type="f" corresp="#s3:l4">geschaepen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Cupido is een <rhyme label="a" type="f">jongen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De roê noch naeuw <rhyme label="a" type="f" corresp="#s4:l1">ontsprongen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En wat is toch sijn <rhyme label="b" type="m">Moêr</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Een schoon wijf? mais au <rhyme label="c" type="f">reste</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">Een uytgebruyde <rhyme label="b" type="m" corresp="#s4:l3">hoer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die 't alles geeft ten <rhyme label="c" type="f" corresp="#s4:l4">besten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Siet Bachus bolle <rhyme label="a" type="f">wangen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Eens op sijn schouders <rhyme label="a" type="f" corresp="#s5:l1">hangen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En wat geeft ons Godt <rhyme label="b" type="m">Mars</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Met sijn verroesten <rhyme label="c" type="f">degen</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">Al sagh hy noch soo <rhyme label="b" type="m" corresp="#s5:l3">bars</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ick setter een besem-stock <rhyme label="c" type="f" corresp="#s5:l4">tegen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Apoll met negen <rhyme label="a" type="f">Musen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die 't backhuys van <rhyme label="a" type="f" corresp="#s6:l1">Meduse</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Gelijckenen in <rhyme label="b" type="m">all's</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Hy mag 'er wel op <rhyme label="c" type="f">roemen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Al sat ick op mijn <rhyme label="b" type="m" corresp="#s6:l3">hals</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Ick souw se niet durven <rhyme label="c" type="f" corresp="#s6:l4">noemen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Saturn, Neptuyn, en <rhyme label="a" type="f">Eris</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat sulcken boosen Meer <rhyme label="a" type="f" corresp="#s7:l1">is</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Mercuur der Guyten <rhyme label="b" type="m">tolck</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Met Pluto en <rhyme label="c" type="f">Vulcanis</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En all' dat klootjes <rhyme label="b" type="m" corresp="#s7:l3">volck</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daer elck meent hy de haen <rhyme label="c" type="f" corresp="#s7:l4">is</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">'k Lach eens met je <rhyme label="a" type="f">Geslachten</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Je Godheyt en je <rhyme label="a" type="f" corresp="#s8:l1">machten</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Waer op je stoft soo <rhyme label="b" type="m">breet</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">O Goden van <rhyme label="c" type="f">vermogen</rhyme>!</l>
        <l id="s8:l5" met="3" type="-+">Je bent, gelijckje <rhyme label="b" type="m" corresp="#s8:l3">weet</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Uyt Nasoos poot <rhyme label="c" type="f" corresp="#s8:l4">gesogen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">'k Vind' by de aerdsche <rhyme label="a" type="f">menschen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Daer 'k u niet voor souw <rhyme label="a" type="f" corresp="#s9:l1">wenschen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Hier sitmen aen den <rhyme label="b" type="m">disch</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Met Juffers en met <rhyme label="c" type="f">Heeren</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Hier leert men Vleysch en <rhyme label="b" type="m" corresp="#s9:l3">Vis</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Met glaesen wijn, <rhyme label="c" type="f" corresp="#s9:l4">lardeeren</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Hier singtmen uyt den <rhyme label="a" type="f">treuren</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Hier maecktmen duysend <rhyme label="a" type="f" corresp="#s10:l1">geuren</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Op maet-slagh van 't <rhyme label="b" type="m">gebit</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En naerstigheyt int <rhyme label="c" type="f">schencken</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Daer j' als een hontsvot <rhyme label="b" type="m" corresp="#s10:l3">sit</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En past op Junoos <rhyme label="c" type="f" corresp="#s10:l4">wencken</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Geveynstheyt is <rhyme label="a" type="f">gebannen</rhyme>.</l>
        <l id="s11:l2" met="3" type="-+">Musijck van Pint en <rhyme label="a" type="f" corresp="#s11:l1">kannen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Vervrolijckt onse <rhyme label="b" type="m">borst</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Ons hart springt in den <rhyme label="c" type="f">zetel</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Niet anders als een <rhyme label="b" type="m" corresp="#s11:l3">worst</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Doet in een warme <rhyme label="c" type="f" corresp="#s11:l4">ketel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Geen van ons die met <rhyme label="a" type="f">rimp'len</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Sijn vreugt soeckt te <rhyme label="a" type="f" corresp="#s12:l1">bewimp'len</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Maer elck leyt op sijn <rhyme label="b" type="m">plat</rhyme>;</l>
        <l id="s12:l4" met="3" type="-+">Ons voor-hooft en ons' <rhyme label="c" type="f">sinnen</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Zijn beyde even <rhyme label="b" type="m" corresp="#s12:l3">glat</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">'t Een buyten 't ander <rhyme label="c" type="f" corresp="#s12:l4">binnen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Hier sit ick by 't <rhyme label="a" type="f">geselschap</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Daer 'k vaeck med' in duël <rhyme label="a" type="f" corresp="#s13:l1">stap</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Soo 'r is een storm te <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Op Haesen, of <rhyme label="c" type="f">Konijnen</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Of rompen van 't <rhyme label="b" type="m" corresp="#s13:l3">Kalckoen</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Of borstelloose <rhyme label="c" type="f" corresp="#s13:l4">Swijnen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Hier kan men aerdigh <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s14:l2" met="3" type="-+">De kunst van <rhyme label="a" type="f" corresp="#s14:l1">trans-formeeren</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Niet in een Swaen of <rhyme label="b" type="m">Stier</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Of sulck soort van <rhyme label="c" type="f">grimassen</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Men maeckt van Kaes <rhyme label="b" type="m" corresp="#s14:l3">alhier</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Scheeps-kielen tot <rhyme label="c" type="f" corresp="#s14:l4">Pinassen</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Van 't broot een <rhyme label="a" type="f">Bartholomeeuwis</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Die sonder veel geschreeuw <rhyme label="a" type="f" corresp="#s15:l1">is</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Al wert sijn lijf <rhyme label="b" type="m">gevilt</rhyme>;</l>
        <l id="s15:l4" met="3" type="-+">Van bouten <rhyme label="c" type="f">spijcker-booren</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Hier temt men 't woeste <rhyme label="b" type="m" corresp="#s15:l3">Wilt</rhyme></l>
        <l id="s15:l6" met="3" type="-+">In 't ry-parck van <rhyme label="c" type="f" corresp="#s15:l4">tailjoren</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Noch hondert andere <rhyme label="a" type="f">dingen</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Daer 'k nu niet van wil <rhyme label="a" type="f" corresp="#s16:l1">singen</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Want kreeg j'er af de <rhyme label="b" type="m">weet</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Je souwt wel ruylen <rhyme label="c" type="f">willen</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Je donder voor een <rhyme label="b" type="m" corresp="#s16:l3">scheet</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Je blixem voor twee <rhyme label="c" type="f" corresp="#s16:l4">billen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Vaert wel dan Messieurs <rhyme label="a" type="f">Goden</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Malkand'ren niet te <rhyme label="a" type="f" corresp="#s17:l1">noden</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Houw jy den Hemel <rhyme label="b" type="m">in</rhyme>;</l>
        <l id="s17:l4" met="3" type="-+">Wy blijven hier <rhyme label="c" type="f">beneden</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Wy zijn hier naer ons <rhyme label="b" type="m" corresp="#s17:l3">sin</rhyme></l>
        <l id="s17:l6" met="3" type="-+">En wonder wel te <rhyme label="c" type="f" corresp="#s17:l4">vreden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
