        <l id="s1:l1" met="4" type="+-">DAer men Lievers Hooft siet <rhyme label="a" type="f">cieren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Brosterhuysen, Ban, <rhyme label="b" type="m">Bostijn</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Met d’Eer-krans van <rhyme label="a" type="f" corresp="#s1:l1">Lauwerieren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Nickels met <rhyme label="b" type="m" corresp="#s1:l2">Roosemarijn</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Daerm’ oock Ockes, Flodorp, <rhyme label="c" type="m">Goor</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Grift in ‘t mellick-wil <rhyme label="c" type="m" corresp="#s1:l5">Yvoir</rhyme>.</l>
