        <l id="s1:l7" met="2" type="-+">vnnd woll <rhyme label="c" type="m" corresp="#s1:l5">erdachtt</rhyme> /</l>
        <l id="s1:l8" met="4" type="-+">a ch allenn menschenn vrspr ng <rhyme label="b" type="m" corresp="#s1:l4">bist</rhyme></l>
        <l id="s1:l9" met="2" type="-+">vonn lob so <rhyme label="d" type="m">reich</rhyme> /</l>
        <l id="s1:l10" met="2" type="-+">Das dir <rhyme label="d" type="m" corresp="#s1:l9">gleich</rhyme></l>
        <l id="s1:l11" met="2" type="+-">sigh nichts <rhyme label="e" type="m">erfindt</rhyme>  /</l>
        <l id="s1:l12" met="2" type="-+">das weiblich biltt / gantz <rhyme label="f" type="m">vberwindt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">O weiblich z <rhyme label="a" type="m">chtt</rhyme> /</l>
