        <l id="s2:l8" met="4" type="-+">an dich zergahett kein fre dt a ff <rhyme label="b" type="m" corresp="#s2:l4">erdtt</rhyme></l>
        <l id="s2:l9" met="2" type="-+">d rch deinen <rhyme label="d" type="m">schertz</rhyme> /</l>
