        <l id="s3:l10" met="2" type="-+">all dinch  <rhyme label="d" type="m" corresp="#s3:l9">gemein</rhyme> /</l>
        <l id="s3:l11" met="2" type="+-">gehorsam <rhyme label="e" type="m">seintt</rhyme></l>
