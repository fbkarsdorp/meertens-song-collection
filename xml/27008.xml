        <l id="s2:l3" met="2" type="-+">nhemantt gen <rhyme label="a" type="m" corresp="#s2:l1">gts</rhyme></l>
        <l id="s2:l4" met="4" type="-+">zu preisenn dich / gn gksam mitt <rhyme label="b" type="m">werdt</rhyme>  /</l>
        <l id="s2:l5" met="2" type="-+">du hochstes g <rhyme label="c" type="m">itt</rhyme> /</l>
