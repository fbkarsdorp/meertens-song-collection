    <melody>Psalm 121 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk was in angst en <rhyme label="a" type="m">kommernis</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">De Moord-trompette <rhyme label="b" type="m">klonck</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">het dreygend yser <rhyme label="b" type="m" corresp="#s1:l2">blonck</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">De noodt doet soecken daer niet <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">’kSagh na ’tgeberght om <rhyme label="c" type="f">hooge</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met hulp-verwachtend’ <rhyme label="c" type="f" corresp="#s1:l5">ooge</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Geen schepsel magh op schepsel <rhyme label="a" type="m">staen</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">De Schepper selver <rhyme label="b" type="m">is’t</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Wiens hulp alleen niet <rhyme label="b" type="m" corresp="#s2:l2">mist</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Hy kan, en heeft my <rhyme label="a" type="m" corresp="#s2:l1">bygestaen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">(Doe hooge ramp my <rhyme label="c" type="f">naeckte</rhyme>)</l>
        <l id="s2:l6" met="3" type="-+">Die Aerd en Hemel <rhyme label="c" type="f" corresp="#s2:l5">maeckte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waer toe noch angstigh, mijn <rhyme label="a" type="m">gemoedt</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Hy is’t die voor den <rhyme label="b" type="m">val</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Vw’ voet bestenden <rhyme label="b" type="m" corresp="#s3:l2">sal</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Hy die om u de wake <rhyme label="a" type="m" corresp="#s3:l1">doet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">(Als Herder om sijn <rhyme label="c" type="f">schapen</rhyme>)</l>
        <l id="s3:l6" met="3" type="-+">Weet nimmermeer van <rhyme label="c" type="f" corresp="#s3:l5">slapen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het onvergancklijck <rhyme label="a" type="m">ooge-licht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat alle ding <rhyme label="b" type="m">beraeckt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En Israël <rhyme label="b" type="m" corresp="#s4:l2">bewaeckt</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Met onafmattelijck <rhyme label="a" type="m" corresp="#s4:l1">gesicht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Blijft eeuwlijck <rhyme label="c" type="f">onbestreden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van <rhyme label="c" type="f" corresp="#s4:l5">sluymer-vallicheden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Heer uw’ wacht. wiens gunst en <rhyme label="a" type="m">kracht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">V als ter schadu <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En maeckt dat u niet <rhyme label="b" type="m" corresp="#s5:l2">schaedt</rhyme></l>
        <l id="s5:l4" met="4" type="-+">De Son by daegh, de Maen by <rhyme label="a" type="m" corresp="#s5:l1">nacht</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Hy wanckt te geenen <rhyme label="c" type="f">tijde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die Godt heeft aen sijn’ <rhyme label="c" type="f" corresp="#s5:l5">zijde</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Heere sal van alle <rhyme label="a" type="m">quaedt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat oyt op menschen <rhyme label="b" type="m">viel</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Behoeden uwe <rhyme label="b" type="m" corresp="#s6:l2">ziel</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En, ’tzy ghy uyt off inne <rhyme label="a" type="m" corresp="#s6:l1">gaet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Van nu tot allen <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Een wakend’ ooge <rhyme label="c" type="f" corresp="#s6:l5">dragen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
