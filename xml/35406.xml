<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35406</id>
    <date>
      <year-start>1710</year-start>
      <year-end>1733</year-end>
    </date>
    <location>
      <orginal-name>brugge</orginal-name>
      <standard-name>Brugge, Belgium</standard-name>
      <coordinates>
        <lon>51.2315785</lon>
        <lat>3.20809951603418</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Als Atalante doden maakte</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Jacobus de Ruyter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CUpido Godt der <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Brenght nu alles aen den <rhyme label="b" type="m">dans</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En stort verliefde <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">In de Dochters en <rhyme label="b" type="m" corresp="#s1:l2">Jongmans</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Nu inden <rhyme label="c" type="m">Somer-tijdt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Is dat een yder <rhyme label="c" type="m" corresp="#s1:l5">vrijdt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">d' Een raeckt tot sijn <rhyme label="d" type="m">besluyt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En d' ander werdt de <rhyme label="d" type="m" corresp="#s1:l7">Bruydt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">De wereldt raeckt aen 't <rhyme label="e" type="f">branden</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Al de jeught wilt seffens <rhyme label="d" type="m" corresp="#s1:l7">uyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De groote <rhyme label="a" type="f">Potentaeten</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Krijghsman, Borgers en <rhyme label="b" type="m">Matroos</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En konnen 't oock niet <rhyme label="a" type="f" corresp="#s2:l1">laeten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Yder soeckt een rinse <rhyme label="b" type="m" corresp="#s2:l2">doos</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Coopman en <rhyme label="c" type="m">Winckelier</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Den Cramer en <rhyme label="c" type="m" corresp="#s2:l5">Grossier</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Den cloecken <rhyme label="d" type="m">Ambochtsman</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Den Boer of <rhyme label="d" type="m" corresp="#s2:l7">Acker-man</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Het loopt al om te <rhyme label="e" type="f">trouwen</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Die daer toe geraecken <rhyme label="d" type="m" corresp="#s2:l7">kan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Anthoen soeckt <rhyme label="a" type="f">Anthonette</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En Passchier <rhyme label="b" type="m">Passchasia</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En Rosier sijn <rhyme label="a" type="f" corresp="#s3:l1">Rosette</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Cornelis <rhyme label="b" type="m" corresp="#s3:l2">Cornelia</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Christoffel mint <rhyme label="c" type="m">Christijn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En Jacob <rhyme label="c" type="m" corresp="#s3:l5">Jacomijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ambroos <rhyme label="d" type="m">Ambrosia</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Martinus <rhyme label="d" type="m" corresp="#s3:l7">Martina</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Een yder soeckt sijn <rhyme label="e" type="f">gayken</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Ja hy loopt het achter <rhyme label="d" type="m" corresp="#s3:l7">na</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dan sietmen noch wel <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Twee of dry naer eene <rhyme label="b" type="m">Meyt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die 't dan somtijdts <rhyme label="a" type="f" corresp="#s4:l1">bekoopen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Met een blauwken soo men <rhyme label="b" type="m" corresp="#s4:l2">seyt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">David en <rhyme label="c" type="m">Daniel</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Loopen naer <rhyme label="c" type="m" corresp="#s4:l5">Isabel</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Geraert naer <rhyme label="d" type="m">Boudewijn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Met Hans naer <rhyme label="d" type="m" corresp="#s4:l7">Willemijn</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Een yder wilt de <rhyme label="e" type="f">naeste</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Vande soete Dierkens <rhyme label="d" type="m" corresp="#s4:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Abram stelt sijne <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Door 't gesicht van <rhyme label="b" type="m">Silvia</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Anselmus doolt in 't <rhyme label="a" type="f" corresp="#s5:l1">minnen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Van de schoone <rhyme label="b" type="m" corresp="#s5:l2">Helena</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Paulus van <rhyme label="c" type="m">Pauwelijn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Amant van <rhyme label="c" type="m" corresp="#s5:l5">Angelijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Carolus van <rhyme label="d" type="m">Clara</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Andreas van <rhyme label="d" type="m" corresp="#s5:l7">Sara</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">En Adriaen van <rhyme label="e" type="f">sijne</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Wel-beminde <rhyme label="d" type="m" corresp="#s5:l7">Agatha</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Lenaert die raeckt aen 't <rhyme label="a" type="f">raesen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En Quitin volght sijne <rhyme label="b" type="m">spoor</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En loopen peur als <rhyme label="a" type="f" corresp="#s6:l1">dwaesen</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Naer Magriet en <rhyme label="b" type="m" corresp="#s6:l2">Leonoor</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Balthasar naer <rhyme label="c" type="m">Florens</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Lucas naer <rhyme label="c" type="m" corresp="#s6:l5">Amorens</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Hendrijck naer <rhyme label="d" type="m">Henderijn</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En Joseph naer <rhyme label="d" type="m" corresp="#s6:l7">Justijn</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En Pieter Jan en <rhyme label="e" type="f">Carel</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Naer Rosa, Martha, <rhyme label="d" type="m" corresp="#s6:l7">Pirijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Boertjens en <rhyme label="a" type="f">Boerinnen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Vryen oock al onder <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Herders en <rhyme label="a" type="f" corresp="#s7:l1">Herderinnen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Vindtmen dickwils met hun <rhyme label="b" type="m" corresp="#s7:l2">tween</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Want Joris, Joos en <rhyme label="c" type="m">Claes</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Theunis en <rhyme label="c" type="m" corresp="#s7:l5">Bonefaes</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">By Mayken, Claesken <rhyme label="d" type="m">Tan</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En Griet, Betken en <rhyme label="d" type="m" corresp="#s7:l7">An</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">En Lieven met sijn <rhyme label="e" type="f">Goolken</rhyme>,</l>
        <l id="s7:l10" met="4" type="+-">Kommen t' samen <rhyme label="d" type="m" corresp="#s7:l7">achteran</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Filiander trouwen <rhyme label="a" type="f">Herder</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Mint Philida boven <rhyme label="b" type="m">maet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En Celadon gaet <rhyme label="a" type="f" corresp="#s8:l1">veerdigh</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Schoon dat hem Astrea <rhyme label="b" type="m" corresp="#s8:l2">haet</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Hylas en <rhyme label="c" type="m">Dianier</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En Dafnis en <rhyme label="c" type="m" corresp="#s8:l5">Rosier</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Claerind' en <rhyme label="d" type="m">Coridon</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Amaran en <rhyme label="d" type="m" corresp="#s8:l7">Damon</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Filiander en <rhyme label="e" type="f">Belinde</rhyme>,</l>
        <l id="s8:l10" met="4" type="+-">Galathe en <rhyme label="d" type="m" corresp="#s8:l7">Celidon</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dochters die tot haer <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s9:l2" met="4" type="+-">En soo ver gekomen <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Dat sy het selver <rhyme label="a" type="f" corresp="#s9:l1">vraegen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Als Anna aen <rhyme label="b" type="m" corresp="#s9:l2">Valentijn</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Barbara aen <rhyme label="c" type="m">Reynier</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Marij aen <rhyme label="c" type="m" corresp="#s9:l5">Olivier</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Josijne aen <rhyme label="d" type="m">Servaes</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">En Agatha aen <rhyme label="d" type="m" corresp="#s9:l7">Raes</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">En Petronel aen <rhyme label="e" type="f">Symoen</rhyme>,</l>
        <l id="s9:l10" met="4" type="+-">En Maeghdalena aen <rhyme label="d" type="m" corresp="#s9:l7">Donaes</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dus verstreckt d' <rhyme label="a" type="f">Heerschappe</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Van Cupido breedt en <rhyme label="b" type="m">wijt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hy doet de Jongmans <rhyme label="a" type="f" corresp="#s10:l1">vryen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Of sy worden selfs <rhyme label="b" type="m" corresp="#s10:l2">gevrijt</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">dit geschiet over <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">In 't Bosch by Bergh en <rhyme label="c" type="m" corresp="#s10:l5">dal</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">In Steden en in 't <rhyme label="d" type="m">Hof</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">dus geeft hem altoos <rhyme label="d" type="m" corresp="#s10:l7">lof</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Van uwe jonge <rhyme label="e" type="f">jaeren</rhyme>,</l>
        <l id="s10:l10" met="4" type="+-">Tot gy wederkeert in <rhyme label="d" type="m" corresp="#s10:l7">stof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
