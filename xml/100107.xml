<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100107</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laura zat laatst bij de beek</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs de Son het Velt <rhyme label="a" type="m">begaf</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dreef ick mijne soete <rhyme label="b" type="f">Schapen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Van de grase bloemtjes <rhyme label="a" type="m" corresp="#s1:l1">af</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">In haer Hutje om te <rhyme label="b" type="f" corresp="#s1:l2">slapen</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">En doe al de Herders <rhyme label="b" type="f" corresp="#s1:l2">knapen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Sliepen, gingh ick oock um <rhyme label="c" type="m">rust</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">By een Beeckje dat soet <rhyme label="d" type="f">stroomde</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Daer mijn weligh Hertje <rhyme label="d" type="f" corresp="#s1:l7">droomde</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dingen die noyt zijn <rhyme label="c" type="m" corresp="#s1:l6">bewust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Mijn docht dat mijn Jeugdigh <rhyme label="a" type="m">vleesch</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Was door Naso, steen <rhyme label="b" type="f">geworden</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Dat geen Jeugt meer dertel <rhyme label="a" type="m" corresp="#s2:l1">rees</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Maer bleef stijf gelijck een <rhyme label="b" type="f" corresp="#s2:l2">horden</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Oogen na geen kijcken <rhyme label="b" type="f" corresp="#s2:l2">porden</rhyme>:</l>
        <l id="s2:l6" met="4" type="+-">Mont en Tong gaf geen <rhyme label="c" type="m">geluyt</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">Handen mogt geen Staf meer <rhyme label="d" type="f">voeren</rhyme>:</l>
        <l id="s2:l8" met="4" type="+-">Handen mogt geen Vee meer <rhyme label="d" type="f" corresp="#s2:l7">roeren</rhyme>:</l>
        <l id="s2:l9" met="4" type="+-">Ach al 't Leven was my <rhyme label="c" type="m" corresp="#s2:l6">uyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Yder sagh de gave <rhyme label="a" type="m">Steen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Yder riep O! Goon 'k moet <rhyme label="b" type="f">schreyen</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Is de Menscheyt dus <rhyme label="a" type="m" corresp="#s3:l1">verdween</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Soete Steen men sal u <rhyme label="b" type="f" corresp="#s3:l2">leyen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En u Steenigh lijf <rhyme label="b" type="f" corresp="#s3:l2">bereyen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Tot een decksel voor een <rhyme label="c" type="m">Graf</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Tot een Sercq daer Maegden <rhyme label="d" type="f">rusten</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Herders-steen waer zijn u <rhyme label="d" type="f" corresp="#s3:l7">lusten</rhyme>?</l>
        <l id="s3:l9" met="4" type="+-">Alles is gesneden <rhyme label="c" type="m" corresp="#s3:l6">af</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Naso gaf mijn weer <rhyme label="a" type="m">gevoel</rhyme>:</l>
        <l id="s4:l2" met="4" type="+-">Naso gaf mijn snelder <rhyme label="b" type="f">Oogen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Naso gaf mijn bly <rhyme label="a" type="m" corresp="#s4:l1">gewoel</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Naso gaf al mijn <rhyme label="b" type="f" corresp="#s4:l2">vermogen</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Naso was met mijn <rhyme label="b" type="f" corresp="#s4:l2">bewogen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Soete Naso 't is nu <rhyme label="c" type="m">vreugt</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Soete Naso laet mijn <rhyme label="d" type="f">blyven</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Laet mijn Sercq dit Graf <rhyme label="d" type="f" corresp="#s4:l7">geryven</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Want geen soet myn soeter <rhyme label="c" type="m" corresp="#s4:l6">heugt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Voel nu 't jonge jeugdigh <rhyme label="a" type="m">volck</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Mijn met vlugge voeties <rhyme label="b" type="f">treden</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">'k Sie ha soet! haer dicke <rhyme label="a" type="m" corresp="#s5:l1">wolck</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">'k Sie de Nomphjes al haer <rhyme label="b" type="f" corresp="#s5:l2">leden</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Wonder net en braaf <rhyme label="b" type="f" corresp="#s5:l2">besneden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Kameniertjes bly van <rhyme label="c" type="m">Geest</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">'t Scheen mijn hant haer sagtheydt <rhyme label="d" type="f">grepen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">O sy waren eens <rhyme label="d" type="f" corresp="#s5:l7">geslepen</rhyme></l>
        <l id="s5:l9" met="4" type="+-">En gemaeckt na eene <rhyme label="c" type="m" corresp="#s5:l6">leest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
