    <tags>
      <tag translation="farewell song">afscheidslied</tag>
      <tag translation="">geestelijk</tag>
      <tag translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Al binnen hoge muren</melody>
    <categories>
      <category translation="occasions">gelegenheden</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">V Beyden doet mijnder <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Mijnder herten groote <rhyme label="b" type="m">pijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick wil mi gaen <rhyme label="a" type="f" corresp="#s1:l1">begheuen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al in een <rhyme label="b" type="m" corresp="#s1:l2">cloosterkijn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ick en wil niet langher <rhyme label="c" type="f">beydem</rhyme></l>
        <l id="s1:l6" met="3" type="-+">V beyden dat doet mi <rhyme label="d" type="m">pijn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ick wil mi gaen <rhyme label="c" type="f" corresp="#s1:l5">begheuen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In een cloosterken wil ick <rhyme label="d" type="m" corresp="#s1:l6">sijn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ic en wil niet langer <rhyme label="a" type="f">beyden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Den tijt en die is <rhyme label="b" type="m">hier</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Adieu wij moeten <rhyme label="a" type="f" corresp="#s2:l1">scheyden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Mijn vriendekens laet ic <rhyme label="b" type="m" corresp="#s2:l2">hier</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Al doer die wilde <rhyme label="c" type="f">bosscagie</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Al ouer dat blacke <rhyme label="d" type="m">velt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Daer toe wil ic mi gaen <rhyme label="c" type="f" corresp="#s2:l5">bereiden</rhyme></l>
        <l id="s2:l8" met="3" type="-+">In een cloosterken wel <rhyme label="d" type="m" corresp="#s2:l6">ghestelt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Adieu mijn vrienden en <rhyme label="a" type="f">magen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Adieu der werelt <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Nu wil ic gaen <rhyme label="a" type="f" corresp="#s3:l1">draghen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Een wijlken op mijn <rhyme label="b" type="m" corresp="#s3:l2">hooft</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Ter eeren Christus doornen <rhyme label="c" type="f">crone</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die hi aent cruyce <rhyme label="d" type="m">droech</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dat is Iesus Marien <rhyme label="c" type="f" corresp="#s3:l5">sone</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Wilt ons houden in v <rhyme label="d" type="m" corresp="#s3:l6">behoet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Adieu alle mijn <rhyme label="a" type="f">ghespelinnen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Adieu mijn hertekens <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Witte cleederen gae ick <rhyme label="a" type="f" corresp="#s4:l1">draghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ter eere van Christus <rhyme label="b" type="m" corresp="#s4:l2">doot</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Want Iesus was in suuerheyt <rhyme label="c" type="f">ontfanghen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Die heere der heeren <rhyme label="d" type="m">groot</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Och de werlt valt mi so <rhyme label="c" type="f" corresp="#s4:l5">strange</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ende alder werelt <rhyme label="d" type="m" corresp="#s4:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu rade ick alle ionge <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die mitter werlt becommert <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dat sy haer willen <rhyme label="a" type="f" corresp="#s5:l1">begheuen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Al in een <rhyme label="b" type="m" corresp="#s5:l2">cloosterkijn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dat si die werelt <rhyme label="c" type="f">laten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ende dienen der heeren here <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s5:l7" met="3" type="-+">God wilse houden in <rhyme label="c" type="f" corresp="#s5:l5">stade</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Al doer sijn heylich <rhyme label="d" type="m" corresp="#s5:l6">bloet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mer die dit liedeken <rhyme label="a" type="f">dichte</rhyme></l>
        <l id="s6:l2" met="3" type="-+">God gheue haer goeden <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Al is haer herteken so <rhyme label="a" type="f" corresp="#s6:l1">lichte</rhyme></l>
        <l id="s6:l4" met="3" type="-+">God houdese in sijn <rhyme label="b" type="m" corresp="#s6:l2">behoet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Dat si haer mocht <rhyme label="c" type="f">bekeeren</rhyme></l>
        <l id="s6:l6" met="3" type="-+">O heere mijn brudegom <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Wilt mi den rechten wech toch <rhyme label="c" type="f" corresp="#s6:l5">leeren</rhyme></l>
        <l id="s6:l8" met="3" type="-+">O heere mijn minneken <rhyme label="d" type="m" corresp="#s6:l6">soet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
