      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">LIef ick moet u <rhyme label="a" type="f">klaghen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Dat ick tydt op <rhyme label="b" type="m">tydt</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Sonder vanck moet <rhyme label="a" type="f" corresp="#s1:l1">jaghen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En u heb voor niet <rhyme label="b" type="m" corresp="#s1:l2">ghevrydt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Peyst hoe my het werck <rhyme label="c" type="m">verdriet</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Daer-men gheenen loon <rhyme label="c" type="m" corresp="#s1:l5">gheniet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">'k Heb u dan met <rhyme label="a" type="f">Dichten</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Dan vereert met <rhyme label="b" type="m">Sanck</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Ick quitteer myn <rhyme label="a" type="f" corresp="#s2:l1">plichten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maer ick hebbe luttel <rhyme label="b" type="m" corresp="#s2:l2">danck</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Ey verandert eens van <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Jont my uwe <rhyme label="c" type="m" corresp="#s2:l5">weder-min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">'k Heb expres gaen <rhyme label="a" type="f">planten</rhyme></l>
        <l id="s3:l2" met="3" type="+-">By den Water <rhyme label="b" type="m">stroom</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Eenen feer <rhyme label="a" type="f" corresp="#s3:l1">playsanten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Ende schoonen <rhyme label="b" type="m" corresp="#s3:l2">Krieckẽ-boom</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Op dat ghy soudt met <rhyme label="c" type="m">ghenucht</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Daer van eten d'eerste <rhyme label="c" type="m" corresp="#s3:l5">vrucht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Als ick maer een <rhyme label="a" type="f">Musken</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Sien op eenen <rhyme label="b" type="m">tack</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Dat daer met een <rhyme label="a" type="f" corresp="#s4:l1">kusken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Stelt de Krieckẽ; roep ick <rhyme label="b" type="m" corresp="#s4:l2">strack</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Spaert dit boomkẽ <rhyme label="c" type="m">Kriecken-dief</rhyme></l>
        <l id="s4:l6" met="4" type="+-">d'Eerste vrucht is voor myn <rhyme label="c" type="m" corresp="#s4:l5">Lief</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Als men in ons <rhyme label="a" type="f">woonste</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Fruyt oft Appels <rhyme label="b" type="m">pluckt</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Seynd' ick u de <rhyme label="a" type="f" corresp="#s5:l1">schoonste</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Die het beste zyn <rhyme label="b" type="m" corresp="#s5:l2">gheluckt</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'k Seynd' u Druyven <rhyme label="c" type="m">als-mensnyt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Oft iet anders naer den <rhyme label="c" type="m" corresp="#s5:l5">tydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Als ick roof een <rhyme label="a" type="f">nestjen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">'t Wordt u t' huys <rhyme label="b" type="m">ghebrocht</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">En op een <rhyme label="a" type="f" corresp="#s6:l1">requestjen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Daer uw' trouw wort in <rhyme label="b" type="m" corresp="#s6:l2">versocht</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Daer gheefdy my nihil <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Is dat niet te sueren <rhyme label="c" type="m" corresp="#s6:l5">sop</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Ick dien u soo <rhyme label="a" type="f">vlytigh</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Als een <rhyme label="b" type="m">Lammeken</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">En ghy houwt u <rhyme label="a" type="f" corresp="#s7:l1">spytigh</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Als een hoornigh <rhyme label="b" type="m" corresp="#s7:l2">Rammeken</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Ick schenck u myn eyghen <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Maer u hert blyft even <rhyme label="c" type="m" corresp="#s7:l5">hert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Isser grooter <rhyme label="a" type="f">sonde</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Als <rhyme label="b" type="m">ondanckbaerheydt</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Isser grooter <rhyme label="a" type="f" corresp="#s8:l1">Wonde</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Als een eer'lyck Vryer <rhyme label="b" type="m" corresp="#s8:l2">leydt</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Die nu heeft gedaen soo <rhyme label="c" type="m">lanck</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Groote moeyt voor kleynen <rhyme label="c" type="m" corresp="#s8:l5">danck</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">'k Hop' dat ick uw <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Even-wel op 't <rhyme label="b" type="m">lest</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Sal door goetheydt <rhyme label="a" type="f" corresp="#s9:l1">winnen</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Met den fiat op 't <rhyme label="b" type="m" corresp="#s9:l2">request</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Als den Boer syn Koren <rhyme label="c" type="m">saeyt</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Wordt hy met de hop <rhyme label="c" type="m" corresp="#s9:l5">ghepaeyt</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
