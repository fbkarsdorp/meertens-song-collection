<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>114875</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Rozemond die lag gedoken</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs de duyster was <rhyme label="a" type="f">verwesen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Door het rijsen van de <rhyme label="b" type="m">Son</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ben ick van mijn bed <rhyme label="a" type="f" corresp="#s1:l1">geresen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Na de Schuyt, soo gauw ick <rhyme label="b" type="m" corresp="#s1:l2">kon</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Om te Visschen aen de kant. Van <rhyme label="c" type="m">het</rhyme></l>
        <l id="s1:l6" met="4" type="+-">groene <rhyme label="c" type="m" corresp="#s1:l5">Binnen-landt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Maeckte doen mijn Schuytjen <rhyme label="a" type="f">vaerdigh</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Droogh en schoon van 't vuyle <rhyme label="b" type="m">nat</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Mast en Spriet en Angels <rhyme label="a" type="f" corresp="#s2:l1">aerdigh</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Zeyl en Kloet, en Kaer en <rhyme label="b" type="m" corresp="#s2:l2">Vat</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Kaeds en Kan, en Vuur-slagh <rhyme label="c" type="m">ree</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Pijp Toeback en Doosje <rhyme label="c" type="m" corresp="#s2:l5">mee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doe gingh ick de Wormen <rhyme label="a" type="f">halen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Leydse in een ander <rhyme label="b" type="m">vat</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Maecktse schoon en roodt als <rhyme label="a" type="f" corresp="#s3:l1">Kralen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Met een Ey-door, en soo <rhyme label="b" type="m" corresp="#s3:l2">nat</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Lietse kruypen door de <rhyme label="c" type="m">Mos</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Van een oude Wilgen <rhyme label="c" type="m" corresp="#s3:l5">schos</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Daer nae repje aen het <rhyme label="a" type="f">zeylen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'t Zuyden wintjen dreef my <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'k Had nae drooghte niet te <rhyme label="a" type="f" corresp="#s4:l1">peylen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'t Land was aen het hooge <rhyme label="b" type="m" corresp="#s4:l2">boordt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">'t Bruysde voor de Steven <rhyme label="c" type="m">wit</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En ick speelde Maetjen <rhyme label="c" type="m" corresp="#s4:l5">zit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">My waer schier geen tijdt <rhyme label="a" type="f">ontkomen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ick was aen de nieuwe <rhyme label="b" type="m">Weel</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">'k Heb mijn Zeyltjen <rhyme label="a" type="f" corresp="#s5:l1">in-genomen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En siet daer het worde <rhyme label="b" type="m" corresp="#s5:l2">geel</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">'t Was of het soo wesen <rhyme label="c" type="m">wou</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">'t Sontjen trock als in de <rhyme label="c" type="m" corresp="#s5:l5">kou</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ick gingh my tot Visschen <rhyme label="a" type="f">setten</rhyme></l>
        <l id="s6:l2" met="4" type="+-">In het Schuytjen aen het <rhyme label="b" type="m">Riet</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Doch most eerst de hoeckjes <rhyme label="a" type="f" corresp="#s6:l1">wetten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Maer wat was dat een <rhyme label="b" type="m" corresp="#s6:l2">verdriet</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">'k Hoopte, hoopende ick <rhyme label="c" type="m">dacht</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Visjes ey, ey Visjes <rhyme label="c" type="m" corresp="#s6:l5">wacht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'t Aesje was nau uyt de <rhyme label="a" type="f">kijcker</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">'t Was hael op, en smijt weer <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">'t Dobbertje sonk als een <rhyme label="a" type="f" corresp="#s7:l1">Spijcker</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Het was niet als buyt op <rhyme label="b" type="m" corresp="#s7:l2">buyt</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Gantsch hoe gingh het nae mijn <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">'k Speelde slechts legh in, legh <rhyme label="c" type="m" corresp="#s7:l5">in</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dan een Baersje, dan een <rhyme label="a" type="f">Vooren</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Dan een Pletje, dan een <rhyme label="b" type="m">Bley</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">'t Souder menigh een <rhyme label="a" type="f" corresp="#s8:l1">bekooren</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">'t Scheen al Vis: in't lest ick <rhyme label="b" type="m" corresp="#s8:l2">sey</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Visje maet, het gae u <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Ick noch eensjes komen <rhyme label="c" type="m" corresp="#s8:l5">sel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
