  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Die lang In duijsternis, hebben <rhyme label="a" type="f">gelegen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Opent u oogen, tis meer dan <rhyme label="b" type="m">tyt</rhyme></l>
        <l id="s1:l3" met="5" type="-+">En tot beveijnsden paeijs zynde <rhyme label="a" type="f" corresp="#s1:l1">genegen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">al twelck jegens der Natueren <rhyme label="b" type="m" corresp="#s1:l2">stryt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">wilt sulcke hoope laten <rhyme label="c" type="f">varen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Och wilt u vaders <rhyme label="d" type="m">lant</rhyme></l>
        <l id="s1:l7" met="3" type="-+">doch doen geen <rhyme label="d" type="m" corresp="#s1:l6">schant</rhyme></l>
        <l id="s1:l8" met="5" type="+-">Maer Int gulden Jaer, goe Faem <rhyme label="c" type="f" corresp="#s1:l5">bewaren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">De Spaenschen natij wilt doch al <rhyme label="a" type="f">versaecken</rhyme></l>
        <l id="s2:l2" met="4" type="-+">want haer voornemen, strijt tegens <rhyme label="b" type="m">reen</rhyme></l>
        <l id="s2:l3" met="5" type="-+">Om vant Nederlant nieu spanigen te <rhyme label="a" type="f" corresp="#s2:l1">maecken</rhyme></l>
        <l id="s2:l4" met="4" type="-+">De Mans dooden de vrouwen <rhyme label="b" type="m" corresp="#s2:l2">vertreen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Dit lant Schijnt haer tot rooff <rhyme label="c" type="f">gegeven</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Tis een verkeerde <rhyme label="d" type="m">vree</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Bewaert u <rhyme label="d" type="m" corresp="#s2:l6">stee</rhyme>,</l>
        <l id="s2:l8" met="5" type="+-">Wilt Int gulden Jaer volstandich <rhyme label="c" type="f" corresp="#s2:l5">leven</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Schoone belooften zonder de te <rhyme label="a" type="f">houden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">wilt die geensints doch gelooven <rhyme label="b" type="m">nyet</rhyme></l>
        <l id="s3:l3" met="5" type="-+">Want Schyn zonder deucht die can <rhyme label="a" type="f" corresp="#s3:l1">wonderbrouwen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">alsmen dagelicx voor oogen <rhyme label="b" type="m" corresp="#s3:l2">ziet</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Ipocresy wilt van u <rhyme label="c" type="f">weeren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">meest haer Feijten <rhyme label="d" type="m">gedaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">In boucken <rhyme label="d" type="m" corresp="#s3:l6">staen</rhyme></l>
        <l id="s3:l8" met="5" type="+-">wilt die Int gulden Jaer <rhyme label="c" type="f" corresp="#s3:l5">visiteren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Laet d'Indianen, ons een spiegel <rhyme label="a" type="f">blijven</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Die haer In vrientschap ontfingen <rhyme label="b" type="m">al</rhyme></l>
        <l id="s4:l3" met="5" type="-+">die wreetheijt daer gedaen is nijet om <rhyme label="a" type="f" corresp="#s4:l1">schrijven</rhyme></l>
        <l id="s4:l4" met="4" type="-+">veel menschen vermoort sonder <rhyme label="b" type="m" corresp="#s4:l2">getal</rhyme></l>
        <l id="s4:l5" met="4" type="-+">haer lust streckt, brant hangen, <rhyme label="c" type="f">verdrencken</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Zutpheen, Naerden, <rhyme label="d" type="m">onsoet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Bleeff goet en <rhyme label="d" type="m" corresp="#s4:l6">Bloet</rhyme></l>
        <l id="s4:l8" met="5" type="+-">wilt doch daer Int gulden Jaer om <rhyme label="c" type="f" corresp="#s4:l5">denken</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Maer god wilt Bidden die dit lant <rhyme label="a" type="f">regieren</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Wy scheijt te geven, dach ende <rhyme label="b" type="m">nacht</rhyme></l>
        <l id="s5:l3" met="5" type="-+">haer hert en tonge voorsichtich te <rhyme label="a" type="f" corresp="#s5:l1">stieren</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Tot voordeel die op de waerheyt <rhyme label="b" type="m" corresp="#s5:l2">wacht</rhyme></l>
        <l id="s5:l5" met="4" type="-+">d'Inquisitij stellen al <rhyme label="c" type="f">bezyden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">en het gecroonde <rhyme label="d" type="m">beest</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Daermen oft <rhyme label="d" type="m" corresp="#s5:l6">leest</rhyme></l>
        <l id="s5:l8" met="5" type="+-">hier Int Gulden Jaer doch nijet te <rhyme label="c" type="f" corresp="#s5:l5">lijden</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Prince onzen Leeu, wilt doch helpen <rhyme label="a" type="f">vichten</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Bewaren u thuijn van hollant <rhyme label="b" type="m">schoon</rhyme></l>
        <l id="s6:l3" met="5" type="-+">Op dat graeff Maurits, goe orden mach <rhyme label="a" type="f" corresp="#s6:l1">stichten</rhyme></l>
        <l id="s6:l4" met="4" type="-+">en als Bataviers voeren den <rhyme label="b" type="m" corresp="#s6:l2">Croon</rhyme></l>
        <l id="s6:l5" met="4" type="-+">voor u vryheyt en <rhyme label="c" type="f">Privilege</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Laet Lieffd' tis Godt <rhyme label="d" type="m">bekent</rhyme></l>
        <l id="s6:l7" met="3" type="-+">zijn <rhyme label="d" type="m" corresp="#s6:l6">tFondament</rhyme></l>
        <l id="s6:l8" met="5" type="+-">Int gulden Jaer toont deze <rhyme label="c" type="f" corresp="#s6:l5">remege</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
