  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlt ge ô Doris my <rhyme label="a" type="f">begeeven</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Boeyt U blonde <rhyme label="b" type="m">Klorimeen</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Kiest gy 't zoet van 't Echte <rhyme label="a" type="f" corresp="#s1:l1">Leven</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Is de min dan nooyt te <rhyme label="b" type="m" corresp="#s1:l2">vreen</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Moet zy al myn <rhyme label="c" type="f">lievelingen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Kussend uit myn handen <rhyme label="c" type="f" corresp="#s1:l5">wringen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En ontrooven myn <rhyme label="d" type="m">gezight</rhyme>?</l>
        <l id="s1:l8" met="2" type="+-">Goôn! sluit <rhyme label="e" type="f">d'oogen</rhyme>;</l>
        <l id="s1:l9" met="2" type="+-">Keert 't <rhyme label="e" type="f" corresp="#s1:l8">vermoogen</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Van het Dartel <rhyme label="d" type="m" corresp="#s1:l7">Minnewigt</rhyme>.</l>
      </lg>
    </lg>
