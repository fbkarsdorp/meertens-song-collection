        <l id="s5:l3" met="3" type="-+">Bouen alle schoone <rhyme label="a" type="f" corresp="#s5:l1">amoreuse</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Suldi int herte die liefste <rhyme label="b" type="m" corresp="#s5:l2">zijn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Al legghen die niders <rhyme label="c" type="f">laghen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Hout mi in v <rhyme label="d" type="m">behoet</rhyme></l>
