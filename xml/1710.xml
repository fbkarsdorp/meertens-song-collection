        <l id="s4:l3" met="3" type="-+">Ick blijue ghequetst van <rhyme label="a" type="f" corresp="#s4:l1">binnen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">doort deruen van mijns liefs roode <rhyme label="b" type="m" corresp="#s4:l2">mont</rhyme> |</l>
        <l id="s4:l5" met="3" type="-+">Nv ende tot allen <rhyme label="c" type="f">daghen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wort si van mi <rhyme label="d" type="m">ghegroet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wort si van mi <rhyme label="d" type="m" corresp="#s4:l6">ghegroet</rhyme></l>
