<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8123</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody>Lief uitverkoren lief triomfant</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Ionckheydt groene, Bloeyende <rhyme label="a" type="m">Ieught</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In dit saysoene, Schickt u tot <rhyme label="a" type="m" corresp="#s1:l1">deught</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wilt u begheven, tot <rhyme label="b" type="m">sedigheyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Alle u leven // daer toe <rhyme label="b" type="m" corresp="#s1:l3">bereydt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Met sout doch menght Wat ghy <rhyme label="c" type="m">voortbrenght</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Spreeckt ofte singht // alsoo ons Paulus <rhyme label="b" type="m" corresp="#s1:l3">seydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laet u doch raden, o Iongheydt <rhyme label="a" type="m">fier</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Teder van bladen soet van <rhyme label="a" type="m" corresp="#s2:l1">manier</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wilt u doch keeren, van 's werelts <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vreest doch den Heere // terwijl ghy <rhyme label="b" type="m" corresp="#s2:l3">meught</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Want siet die doodt, spaert kleyn noch <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Hoe jongh, hoe oudt, 't wordt al tot hem <rhyme label="b" type="m" corresp="#s2:l3">geveught</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En wilt niet treden // om wat nieus te <rhyme label="a" type="m">sien</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Als Dyna deden // ten ouden <rhyme label="a" type="m" corresp="#s3:l1">tien</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Als sy aenschoude, des Landes <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Quam sy in rouwe, en swaer <rhyme label="b" type="m" corresp="#s3:l3">verseer</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Haer schoonheydt jent // heeft hy <rhyme label="c" type="m">gheschent</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">V hier van went // ghy jonghe dochters <rhyme label="b" type="m" corresp="#s3:l3">teer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer wilt aenschouwen, Het vroom <rhyme label="a" type="m">ghemoet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Vande ghetrouwe // dochter Iephta <rhyme label="a" type="m" corresp="#s4:l1">soet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die gingh versmaden // al 'swereldts <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En liet verbraden, haer lichaem <rhyme label="b" type="m" corresp="#s4:l3">teer</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Op den Outaer // van haren <rhyme label="c" type="m">Vaer</rhyme></l>
        <l id="s4:l6" met="5" type="-+">Spieghelt u klaer // en houdt dit voor een <rhyme label="b" type="m" corresp="#s4:l3">leer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dit differenti, wilt doch <rhyme label="a" type="m">aensien</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En maeckt doch menti // al van des <rhyme label="a" type="m" corresp="#s5:l1">twien</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Hoe d'een bedorven // ende <rhyme label="b" type="m">beschaemt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">D'ander ghestorven // heel <rhyme label="b" type="m" corresp="#s5:l3">ghehoorsaemt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dus wilt doch fijn // de ouders <rhyme label="c" type="m">dijn</rhyme></l>
        <l id="s5:l6" met="5" type="-+">Ghehoorsaem zijn // sedich en <rhyme label="b" type="m" corresp="#s5:l3">bequaem</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Op dat ghy princelijcke Dochters <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Mocht mede van de wijse Maeghden <rhyme label="a" type="m" corresp="#s6:l1">zijn</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Die metten Heere, werdt <rhyme label="b" type="m">ingheleyt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Want tot d' oneere // sal worden <rhyme label="b" type="m" corresp="#s6:l3">gheseyt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Dit droevigh Liedt // ick ken u <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s6:l6" met="5" type="-+">Gaet in 't verdriet // day voor u is <rhyme label="b" type="m" corresp="#s6:l3">bereydt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daer sal gheschieden // soo groot <rhyme label="a" type="m">dangier</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Nae Schrifts bedieden // in 't Helsche <rhyme label="a" type="m" corresp="#s7:l1">vyer</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Wringhen der handen // druck ende <rhyme label="b" type="m">pijn</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Knerssen der tanden // t'eeuwighen <rhyme label="b" type="m" corresp="#s7:l3">tijn</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Hierom o jeught // schickt u tot <rhyme label="c" type="m">deught</rhyme>,</l>
        <l id="s7:l6" met="5" type="-+">Op dat ghy meught // hier van bevrijdet <rhyme label="b" type="m" corresp="#s7:l3">zijn</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">En dan meught hooren, het soet <rhyme label="a" type="m">gheluyt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Mijn uytvercooren // mijn lieve <rhyme label="a" type="m" corresp="#s8:l1">Bruydt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Wilt tot my treden, ontfanght de <rhyme label="b" type="m">kroon</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En gaet in vreden // in 's Hemels <rhyme label="b" type="m" corresp="#s8:l3">troon</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Blincken hoort mijn // als der Sonnen <rhyme label="c" type="m">schijn</rhyme>,</l>
        <l id="s8:l6" met="5" type="-+">Dat sal dan zijn // der uytvercoren <rhyme label="b" type="m" corresp="#s8:l3">loon</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dus wilt vercieren, u Lampen <rhyme label="a" type="m">subijt</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Met goe manieren // waer dat ghy <rhyme label="a" type="m" corresp="#s9:l1">zijt</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Laet u licht rijsen, wilt liefde <rhyme label="b" type="m">schoon</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Altijdt bewijsen, soo sal Godts <rhyme label="b" type="m" corresp="#s9:l3">Soon</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">V blijde ontfaen // en voor u <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s9:l6" met="5" type="-+">En dienen saen // al in zijn Vaders <rhyme label="b" type="m" corresp="#s9:l3">Throon</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Oorlof hier mede, o Ieught <rhyme label="a" type="m">eerbaer</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Godts eeuwighe vrede wensch ick u <rhyme label="a" type="m" corresp="#s10:l1">klaer</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Dat by ons blijve // tot 't ende <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s10:l4" met="4" type="-+">En ons verstijve // in een vroom <rhyme label="b" type="m" corresp="#s10:l3">ghemoet</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">En nae desen tijt, te zijn <rhyme label="c" type="m">verblijdt</rhyme>,</l>
        <l id="s10:l6" met="5" type="-+">Wensch ick met vlijt // de Ieught tot eenen <rhyme label="b" type="m" corresp="#s10:l3">groet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
