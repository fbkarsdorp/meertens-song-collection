<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179659</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody>Cupido triomfant</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ZIngt nu den Heere <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gy Menschen <rhyme label="b" type="f">allegare</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En ook gy Engelen <rhyme label="a" type="m" corresp="#s1:l1">rey</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">In deze nieuwe <rhyme label="b" type="f" corresp="#s1:l2">Iaren</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Godt heeft gezonden <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Den Engel <rhyme label="c" type="m" corresp="#s1:l5">Gabriël</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Met boodschap aen Maria <rhyme label="c" type="m" corresp="#s1:l5">wel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik groet u Maget <rhyme label="a" type="m">rein</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van Godt <rhyme label="b" type="f">Gebenedyden</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Gy zult de moeder <rhyme label="a" type="m" corresp="#s2:l1">zyn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Waer door wy ons <rhyme label="b" type="f" corresp="#s2:l2">verblyden</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Op dat gy baren <rhyme label="c" type="m">zel</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gods Soon <rhyme label="c" type="m" corresp="#s2:l5">Emanuël</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">De Vrede Vorst van <rhyme label="c" type="m" corresp="#s2:l5">Israël</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daer is nooit blyder <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Op Aerde meer <rhyme label="b" type="f">gevonden</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Als men Augustus <rhyme label="a" type="m" corresp="#s3:l1">zag</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Regeeren ’s Werelds <rhyme label="b" type="f" corresp="#s3:l2">ronden</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Tot Bethlehem <rhyme label="c" type="m">vermaerd</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">So ons de Schrift <rhyme label="c" type="m" corresp="#s3:l5">verklaerd</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Is Godt in ’t Vlees <rhyme label="c" type="m" corresp="#s3:l5">geopenbaert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Siet hier de Heer van <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die is ons nu <rhyme label="b" type="f">geboren</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">By Beesten in een <rhyme label="a" type="m" corresp="#s4:l1">stal</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Den Heyland <rhyme label="b" type="f" corresp="#s4:l2">uitverkooren</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Des Hemels <rhyme label="c" type="m">Majesteid</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Hier in een kribbe <rhyme label="c" type="m" corresp="#s4:l5">leid</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Het Kindje Iezus vol <rhyme label="c" type="m" corresp="#s4:l5">zoetigheid</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Engelen met <rhyme label="a" type="m">kragt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Gods lof en eer <rhyme label="b" type="f">verbryden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Te midden in de <rhyme label="a" type="m" corresp="#s5:l1">nagt</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Iuist daer de herders <rhyme label="b" type="f" corresp="#s5:l2">weyden</rhyme></l>
        <l id="s5:l5" met="3" type="-+">haer Vee in ’t grazig <rhyme label="c" type="m">veld</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Sy waren zeer <rhyme label="c" type="m" corresp="#s5:l5">ontsteld</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Des heeren Engel heeft haer <rhyme label="c" type="m" corresp="#s5:l5">verteld</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Als dat geboren <rhyme label="a" type="m">was</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Den aldergrootsten <rhyme label="b" type="f">Koning</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Tot Bethlehem op dit <rhyme label="a" type="m" corresp="#s6:l1">pas</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Al in een slegte <rhyme label="b" type="f" corresp="#s6:l2">wooning</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Gaet zoekt u God van <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die u verlossen <rhyme label="c" type="m" corresp="#s6:l5">zal</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Leyd by de beesten in een <rhyme label="c" type="m" corresp="#s6:l5">Stal</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Herdertjes <rhyme label="a" type="m">zertyn</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hebben het Kind <rhyme label="b" type="f">gevonden</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Al in de Kribbe <rhyme label="a" type="m" corresp="#s7:l1">kleyn</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In doekjes teer <rhyme label="b" type="f" corresp="#s7:l2">gewonden</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Sy knielden daer voor <rhyme label="c" type="m">neer</rhyme>!</l>
        <l id="s7:l6" met="3" type="-+">Aenbaden Godt den <rhyme label="c" type="m" corresp="#s7:l5">heer</rhyme></l>
        <l id="s7:l7" met="4" type="-+">En loofden Iezus ’t kindje <rhyme label="c" type="m" corresp="#s7:l5">teer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">d’Herders van stonden <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die Offerden den <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">’t Was melk, Room en <rhyme label="a" type="m" corresp="#s8:l1">Saen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">En spraken op die <rhyme label="b" type="f" corresp="#s8:l2">keeren</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Moest gy ô groote <rhyme label="c" type="m">Godt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Om onzer Sonden <rhyme label="c" type="m" corresp="#s8:l5">lot</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Hier leggen in een beesten <rhyme label="c" type="m" corresp="#s8:l5">kot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Een sterre klaer <rhyme label="a" type="m">verscheen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Drie wyzen uit het <rhyme label="b" type="f">oosten</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die trokken haestig <rhyme label="a" type="m" corresp="#s9:l1">heen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">De Ster die haer <rhyme label="b" type="f" corresp="#s9:l2">vertroosten</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">Die is haer <rhyme label="c" type="m">voorgegaen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Zy volgden agter <rhyme label="c" type="m" corresp="#s9:l5">aen</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Tot dat de Ster bleef stille <rhyme label="c" type="m" corresp="#s9:l5">staen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Boven de arme <rhyme label="a" type="m">Stal</rhyme></l>
        <l id="s10:l2" met="3" type="-+">hy roerden niet een <rhyme label="b" type="f">vinne</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">De wyzen onder <rhyme label="a" type="m" corresp="#s10:l1">al</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Die traden aenstonds <rhyme label="b" type="f" corresp="#s10:l2">binnen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Alwaer zy vonden <rhyme label="c" type="m">spa</rhyme>:</l>
        <l id="s10:l6" met="3" type="-+">het Kindje Iezus <rhyme label="c" type="m" corresp="#s10:l5">dra</rhyme>:</l>
        <l id="s10:l7" met="4" type="-+">Met maegt en moeder <rhyme label="c" type="m" corresp="#s10:l5">Maria</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Daer lag die groote <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">In doekjes teer <rhyme label="b" type="f">gewonden</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">By Beesten in een <rhyme label="a" type="m" corresp="#s11:l1">kot</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Om onze boze <rhyme label="b" type="f" corresp="#s11:l2">zonden</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">De Wyzen met <rhyme label="c" type="m">ootmoet</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Vielen by het Kind te <rhyme label="c" type="m" corresp="#s11:l5">voet</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">ô heyland Iezus weest <rhyme label="c" type="m" corresp="#s11:l5">gegroet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">z’Offerden menig <rhyme label="a" type="m">fout</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Zeer mildelyk haer <rhyme label="b" type="f">gaven</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Wierrook, mirhe en <rhyme label="a" type="m" corresp="#s12:l1">Goud</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Om den heer te <rhyme label="b" type="f" corresp="#s12:l2">behagen</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Voor Iezus kindje <rhyme label="c" type="m">kleen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Toen keerden zy weer <rhyme label="c" type="m" corresp="#s12:l5">heen</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Van daer zy ware kome <rhyme label="c" type="m" corresp="#s12:l5">treen</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ag late wy <rhyme label="a" type="m">terstond</rhyme></l>
        <l id="s13:l2" met="3" type="-+">mee na dien Iezus <rhyme label="b" type="f">tragten</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Die ons verlost van <rhyme label="a" type="m" corresp="#s13:l1">zond</rhyme>’</l>
        <l id="s13:l4" met="3" type="-+">Eer wy daer in <rhyme label="b" type="f" corresp="#s13:l2">versmagten</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">hem bidden met <rhyme label="c" type="m">ootmoed</rhyme>:</l>
        <l id="s13:l6" met="3" type="-+">Welkom Iezus <rhyme label="c" type="m" corresp="#s13:l5">zoet</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">Gy hemels Koning weest <rhyme label="c" type="m" corresp="#s13:l5">gegroet</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Laet ons met <rhyme label="a" type="m">dankbaerheid</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Den heer looven en <rhyme label="b" type="f">pryzen</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Die ons de <rhyme label="a" type="m" corresp="#s14:l1">Saligheid</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">In Iezus komt <rhyme label="b" type="f" corresp="#s14:l2">bewyzen</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Die onze zonden <rhyme label="c" type="m">zwaer</rhyme>:</l>
        <l id="s14:l6" met="3" type="-+">Torst op zyn schouders <rhyme label="c" type="m" corresp="#s14:l5">daer</rhyme>,</l>
        <l id="s14:l7" met="4" type="-+">Looft Iezus zoet in ’t Nieuwe <rhyme label="c" type="m" corresp="#s14:l5">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
