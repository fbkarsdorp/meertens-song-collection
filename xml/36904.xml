  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ICk kom' hier <rhyme label="a" type="f">planten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Met geluyt van Luyt en <rhyme label="b" type="m">Fluyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Eenen groenen en <rhyme label="a" type="f" corresp="#s1:l1">playsanten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Mey Boom, voor uw' deur schoon <rhyme label="b" type="m" corresp="#s1:l2">Bruydt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Want een-icder moet <rhyme label="c" type="f">beleyen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat g'om u raer <rhyme label="d" type="m">schoonigheydt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Van schoon Meyskens en schoon <rhyme label="c" type="f" corresp="#s1:l5">Meyen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Verr' den schoonsten Meyboom <rhyme label="d" type="m" corresp="#s1:l6">zyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ghy gaet te <rhyme label="a" type="f">boven</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Door uw minnelyck <rhyme label="b" type="m">ghelaet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Al wat datter in de <rhyme label="a" type="f" corresp="#s2:l1">Hoven</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Lieffelyck en jeughdigh <rhyme label="b" type="m" corresp="#s2:l2">staet</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Het bevalligh <rhyme label="c" type="f">Nachtegaeltjen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Dat soo soet singt in den <rhyme label="d" type="m">Mey</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Swyght, en luystert naer uw <rhyme label="c" type="f" corresp="#s2:l5">taeltjen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Als g'een Liedt singt in de <rhyme label="d" type="m" corresp="#s2:l6">wey</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">O soetste <rhyme label="a" type="f">Meysken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Die ick lief heb ende <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ghy bloeyt jeughdigh als een <rhyme label="a" type="f" corresp="#s3:l1">reysken</rhyme></l>
        <l id="s3:l4" met="4" type="+-">In den soeten koelen <rhyme label="b" type="m" corresp="#s3:l2">Mey</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Uw geil hair en purp're <rhyme label="c" type="f">montjen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Met uw loddelyck <rhyme label="d" type="m">ghelaet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Blinckt ghelyck een <rhyme label="c" type="f" corresp="#s3:l5">morghenstondtjen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Bloost ghelyck den <rhyme label="d" type="m" corresp="#s3:l6">dagheraet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">En om te <rhyme label="a" type="f">thoonen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Dat uw' schoonheydt spant de <rhyme label="b" type="m">kroon</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Boven al-te-mael de <rhyme label="a" type="f" corresp="#s4:l1">schoone</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Die de Werelt stelt ten <rhyme label="b" type="m" corresp="#s4:l2">thoon</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Gaen ick uwen Mey-boom <rhyme label="c" type="f">kroonen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Ende danssen om de <rhyme label="d" type="m">Kroon</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Singhende met soete <rhyme label="c" type="f" corresp="#s4:l5">thoonen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Liefste lief gy zyt heel <rhyme label="d" type="m" corresp="#s4:l6">schoon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Nu gaen ick <rhyme label="a" type="f">schryven</rhyme></l>
        <l id="s5:l2" met="4" type="+-">In den Mey-boom die hier <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Dat ick uwen Dienaer <rhyme label="a" type="f" corresp="#s5:l1">blyve</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Tot dat 't leven my <rhyme label="b" type="m" corresp="#s5:l2">verlaet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Daer op kus ick dit Mey <rhyme label="c" type="f">Boomken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En op dat het beter <rhyme label="d" type="m">groeyt</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Sal het met een <rhyme label="c" type="f" corresp="#s5:l5">trane-stroomken</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Van myn ooghskens zyn <rhyme label="d" type="m" corresp="#s5:l6">besproeyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ick sal <rhyme label="a" type="f">volherden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">In uw' min Ã´ soete <rhyme label="b" type="m">Maeght</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Dat noch niet en is kant 't <rhyme label="a" type="f" corresp="#s6:l1">werden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Als het u maer en <rhyme label="b" type="m" corresp="#s6:l2">behaeght</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Valt den Boom ten eersten slagh <rhyme label="c" type="f">niet</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Daer van is kleyn <rhyme label="d" type="m">swarigheyt</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Hy en groeyt op eenen dagh <rhyme label="c" type="f" corresp="#s6:l5">niet</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Die volhert, die wint den <rhyme label="d" type="m" corresp="#s6:l6">stryt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
