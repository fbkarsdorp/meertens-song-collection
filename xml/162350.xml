<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>162350</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Allemande de Pologne</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE schrift die heeft ons <rhyme label="a" type="m">opgeleyt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Gelijc ons Petrus heeft <rhyme label="a" type="m" corresp="#s1:l1">geseyt</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Datmen sal houden <rhyme label="a" type="m" corresp="#s1:l1">maticheyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wt 't geloove dat <rhyme label="b" type="m">sticht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Van al dat God ons geeft <rhyme label="a" type="m" corresp="#s1:l1">planteyt</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Datmen matich sal sijn <rhyme label="a" type="m" corresp="#s1:l1">gecleyt</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Dat daer mach wesen <rhyme label="a" type="m" corresp="#s1:l1">onderscheyt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Tusschen duyster en <rhyme label="b" type="m" corresp="#s1:l4">licht</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Want hoochmoet // die <rhyme label="c" type="m">doet</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Den menschen over <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s1:l11" met="2" type="-+">Haest metter <rhyme label="c" type="m" corresp="#s1:l9">spoet</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Oock bringen totten <rhyme label="d" type="m" corresp="#s1:l10">val</rhyme>,</l>
        <l id="s1:l13" met="4" type="-+">want wie seer praelt // so schrift <rhyme label="e" type="m">verhaelt</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Hem dat verderven <rhyme label="d" type="m" corresp="#s1:l10">sal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Paulus die badt met goet <rhyme label="a" type="m">bediet</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Deur Gods barmherticheit <rhyme label="a" type="m" corresp="#s2:l1">geschiet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Begeeft u lichamen met <rhyme label="a" type="m" corresp="#s2:l1">vliet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot eenen offer <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Want hy doch alle dingen <rhyme label="a" type="m" corresp="#s2:l1">siet</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Seyt hy die ons het beste <rhyme label="a" type="m" corresp="#s2:l1">riet</rhyme></l>
        <l id="s2:l7" met="4" type="-+">En stelt u als die weerelt <rhyme label="a" type="m" corresp="#s2:l1">niet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Maer verandert u <rhyme label="b" type="m" corresp="#s2:l4">tot</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Een kleyn // kint <rhyme label="c" type="m">reyn</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Door nieuwe sinnen <rhyme label="d" type="m">vry</rhyme></l>
        <l id="s2:l11" met="2" type="-+">Die sonde <rhyme label="c" type="m" corresp="#s2:l9">gemeyn</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Gansch afgestorven <rhyme label="d" type="m" corresp="#s2:l10">ghy</rhyme></l>
        <l id="s2:l13" met="4" type="-+">Op dat ghy meucht // proeven met <rhyme label="e" type="m">vreucht</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Wat sHeeren wille <rhyme label="d" type="m" corresp="#s2:l10">zy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den wille des Heeren is <rhyme label="a" type="m">dat</rhyme>:</l>
        <l id="s3:l2" met="4" type="-+">Ghy oock sult weten hoe en <rhyme label="a" type="m" corresp="#s3:l1">wat</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ghy hier sult besitten u <rhyme label="a" type="m" corresp="#s3:l1">vat</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In heyligheydt en <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Niet in vleesch lusten seyt hy <rhyme label="a" type="m" corresp="#s3:l1">plat</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Gelijck als die Heydenen <rhyme label="a" type="m" corresp="#s3:l1">prat</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Die daer geen wetenschap <rhyme label="a" type="m" corresp="#s3:l1">gehadt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Hebben van God den <rhyme label="b" type="m" corresp="#s3:l4">Heer</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Maer verblent // <rhyme label="c" type="m">gewent</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Tot wellusten <rhyme label="d" type="m">versaemt</rhyme></l>
        <l id="s3:l11" met="2" type="-+">Maer nu <rhyme label="c" type="m" corresp="#s3:l9">present</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Ons sulcx niet en <rhyme label="d" type="m" corresp="#s3:l10">betaemt</rhyme></l>
        <l id="s3:l13" met="4" type="-+">Daer is veel lust // eertijts <rhyme label="e" type="m">geblust</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Daer van men hem nu <rhyme label="d" type="m" corresp="#s3:l10">schaemt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die nieu herboorte die daer <rhyme label="a" type="m">deur</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Gods geest geschiet, en de <rhyme label="a" type="m" corresp="#s4:l1">schrifteur</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Die maect oock inden mensche <rhyme label="a" type="m" corresp="#s4:l1">peur</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Een creatuere <rhyme label="b" type="m">nieu</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Verkiest dat leven voor die <rhyme label="a" type="m" corresp="#s4:l1">keur</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Want die werelt is vol <rhyme label="a" type="m" corresp="#s4:l1">besmeur</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Al staetse schoone in haer <rhyme label="a" type="m" corresp="#s4:l1">fleur</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Secht haer eeuwich <rhyme label="b" type="m" corresp="#s4:l4">adieu</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Den heesch // vant <rhyme label="c" type="m">vleesch</rhyme></l>
        <l id="s4:l10" met="3" type="+-">Moetmen oock dooden <rhyme label="d" type="m">want</rhyme></l>
        <l id="s4:l11" met="2" type="-+">So schrift <rhyme label="c" type="m" corresp="#s4:l9">bewees</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Ist oock eenen <rhyme label="d" type="m" corresp="#s4:l10">vyant</rhyme></l>
        <l id="s4:l13" met="4" type="-+">Het soect den wech // na schrifts <rhyme label="e" type="m">uytleg</rhyme></l>
        <l id="s4:l14" met="3" type="-+">Al daermen eewich <rhyme label="d" type="m" corresp="#s4:l10">brant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hoe can die overwinnen <rhyme label="a" type="m">tquaet</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Die noch sijn vleesch te moete <rhyme label="a" type="m" corresp="#s5:l1">gaet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">En geeft hem al daer na dat <rhyme label="a" type="m" corresp="#s5:l1">staet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die hebben geenen <rhyme label="b" type="m">strijt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En gaen op gepronckt <rhyme label="a" type="m" corresp="#s5:l1">delicaet</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">De werelt gelijc deur den <rhyme label="a" type="m" corresp="#s5:l1">draet</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Die hem zelven geen gade <rhyme label="a" type="m" corresp="#s5:l1">slaet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat leven wort hy <rhyme label="b" type="m" corresp="#s5:l4">quijt</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Den naem // <rhyme label="c" type="m">eersaem</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Van Christum niet en <rhyme label="d" type="m">gelt</rhyme></l>
        <l id="s5:l11" met="2" type="-+">Die <rhyme label="c" type="m" corresp="#s5:l9">onbequaem</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Daer onder sijn <rhyme label="d" type="m" corresp="#s5:l10">getelt</rhyme></l>
        <l id="s5:l13" met="4" type="-+">want maegden dwaes // werden <rhyme label="e" type="m">eylaes</rhyme></l>
        <l id="s5:l14" met="3" type="-+">Buyten die deur <rhyme label="d" type="m" corresp="#s5:l10">gestelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Om dat die dochters Syons <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Alsoo seyt Esaias <rhyme label="a" type="m" corresp="#s6:l1">claer</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Stout waren in het <rhyme label="a" type="m" corresp="#s6:l1">openbaer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Deur hoogen moet <rhyme label="b" type="m">verdooft</rhyme></l>
        <l id="s6:l5" met="4" type="-+">So sant God ooc een straffe <rhyme label="a" type="m" corresp="#s6:l1">zwaer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Neemt dit tot een <rhyme label="a" type="m" corresp="#s6:l1">Exemplaer</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">En gaf voor dat gecrolde <rhyme label="a" type="m" corresp="#s6:l1">haer</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Een leelijck calu <rhyme label="b" type="m" corresp="#s6:l4">hooft</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Haer blancket // <rhyme label="c" type="m">besmet</rhyme></l>
        <l id="s6:l10" met="3" type="+-">Haer weelde <rhyme label="d" type="m">onghemack</rhyme>,</l>
        <l id="s6:l11" met="2" type="-+">Voor tGordel <rhyme label="c" type="m" corresp="#s6:l9">net</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Een coorde die niet en <rhyme label="d" type="m" corresp="#s6:l10">brack</rhyme>,</l>
        <l id="s6:l13" met="4" type="-+">Voor soeten reuck // stanck, en voor <rhyme label="e" type="m">Heuck</rhyme></l>
        <l id="s6:l14" met="3" type="-+">Gaf God een engen <rhyme label="d" type="m" corresp="#s6:l10">sack</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Prince niemant dit liet <rhyme label="a" type="m">vercleent</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Mijn selven heb ick oock <rhyme label="a" type="m" corresp="#s7:l1">gemeent</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Dat wy voor dien diet al <rhyme label="a" type="m" corresp="#s7:l1">verleent</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Hier wandelen <rhyme label="b" type="m">oprecht</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Want wie met die sonde <rhyme label="a" type="m" corresp="#s7:l1">vereent</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">En sijn misdaet niet en <rhyme label="a" type="m" corresp="#s7:l1">beweent</rhyme></l>
        <l id="s7:l7" met="4" type="-+">maer wert daer deur noch meer <rhyme label="a" type="m" corresp="#s7:l1">versteent</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Die is des sonden <rhyme label="b" type="m" corresp="#s7:l4">knecht</rhyme></l>
        <l id="s7:l9" met="2" type="-+">Elck dient // zijn <rhyme label="c" type="m">vrient</rhyme></l>
        <l id="s7:l10" met="3" type="+-">Gehoorsaem en <rhyme label="d" type="m">subjeckt</rhyme></l>
        <l id="s7:l11" met="2" type="-+">Maer Godt door <rhyme label="c" type="m" corresp="#s7:l9">wient</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Al sal worden <rhyme label="d" type="m" corresp="#s7:l10">verweckt</rhyme></l>
        <l id="s7:l13" met="4" type="-+">In genen dach // tot veel <rhyme label="e" type="m">geclach</rhyme>,</l>
        <l id="s7:l14" met="3" type="-+">Daer werdet al <rhyme label="d" type="m" corresp="#s7:l10">ontdeckt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
