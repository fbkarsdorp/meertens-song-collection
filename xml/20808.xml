    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MEnschen geeft geen moed <rhyme label="a" type="f">verlooren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Want de Maget is <rhyme label="a" type="f" corresp="#s1:l1">gebooren</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Uyt de welck' sal komen <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">T' onser welstand, 't eeuwigh <rhyme label="b" type="m" corresp="#s1:l3">Woord</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dese Maeght zal goet we'er <rhyme label="a" type="f">maecken</rhyme></l>
        <l id="s2:l2" met="4" type="+-">By den Heer ons' scheurde <rhyme label="a" type="f" corresp="#s2:l1">saecken</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Daer toe is die Maeght <rhyme label="b" type="m">gestelt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van den Heer op 's werelts <rhyme label="b" type="m" corresp="#s2:l3">veld</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">S' heeft den Helschen Draeck <rhyme label="a" type="f">vertreden</rhyme>;</l>
        <l id="s3:l2" met="4" type="+-">Dies is 't meer dan recht, en <rhyme label="a" type="f" corresp="#s3:l1">reden</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Dat op haer <rhyme label="b" type="m">Geboorten-Tijd</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Alle menschen zijn <rhyme label="b" type="m" corresp="#s3:l3">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sy zal onsen Heyland <rhyme label="a" type="f">baeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Laet dan alle droefheyd <rhyme label="a" type="f" corresp="#s4:l1">vaeren</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Sy zal ons des hemels <rhyme label="b" type="m">kroon</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Werven van haer lieven <rhyme label="b" type="m" corresp="#s4:l3">Soon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sy sal gra√ßy ons <rhyme label="a" type="f">verwerven</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Werven, segg' ick, voor ons <rhyme label="a" type="f" corresp="#s5:l1">sterven</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Door de kracht van haer <rhyme label="b" type="m">gebe'en</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Dies houdt op u droef <rhyme label="b" type="m" corresp="#s5:l3">gesteen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laet ons Haer geluck gaen <rhyme label="a" type="f">bieden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Hoort ghy 't menschen, hoort ghy 't <rhyme label="a" type="f" corresp="#s6:l1">Lieden</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Veel geluck op desen <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Op de welck' Sy 't licht eerst <rhyme label="b" type="m" corresp="#s6:l3">sagh</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Veel geluck, O Pronck der <rhyme label="a" type="f">Maeghden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Die uw' Schepper soo <rhyme label="a" type="f" corresp="#s7:l1">behaeghden</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dat in uwe schoot quam <rhyme label="b" type="m">ne'er</rhyme></l>
        <l id="s7:l4" met="4" type="+-">'s Hemels Vorst, en 's werelts <rhyme label="b" type="m" corresp="#s7:l3">Heer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Veel geluck, O Kroon der <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">In de wiegh noch <rhyme label="a" type="f" corresp="#s8:l1">aen-te-schouwen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Aen-te-schouwen kleyn van <rhyme label="b" type="m">le'en</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Maer verstandigh, vol van <rhyme label="b" type="m" corresp="#s8:l3">re'en</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Zijt ons' Troost, ons' Baeck, ons' <rhyme label="a" type="f">Ancker</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Maeck dat wy verschijnen <rhyme label="a" type="f" corresp="#s9:l1">blancker</rhyme></l>
        <l id="s9:l3" met="4" type="+-">In den hemel als de <rhyme label="b" type="m">Sonn'</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Hoort Ghy 't onser <rhyme label="b" type="m" corresp="#s9:l3">vreughden-Bron</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
