    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb de groene straten</melody>
  <author>Johannes Stichter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IK groet u vol van <rhyme label="a" type="f">gratie</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Sprak d'Engel <rhyme label="b" type="m">Gabriël</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Met u tot aller <rhyme label="a" type="f" corresp="#s1:l1">spatie</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Is God <rhyme label="b" type="m" corresp="#s1:l2">Emmanuël</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want onder alle <rhyme label="a" type="f">Vrouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Zijt gy <rhyme label="b" type="m">gebenedijt</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Gelukkig die <rhyme label="a" type="f" corresp="#s2:l1">aanschouwen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Uw' glorie met <rhyme label="b" type="m" corresp="#s2:l2">Iolijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gebenedijt moet <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Iesus uws Lighaams <rhyme label="b" type="m">dragt</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Door hem zijn wy <rhyme label="a" type="f" corresp="#s3:l1">geresen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Al uyt des vyants <rhyme label="b" type="m" corresp="#s3:l2">magt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maria Godes <rhyme label="a" type="f">Moeder</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wilt ons gedagtig <rhyme label="b" type="m">zijn</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Bid voor onsen <rhyme label="a" type="f" corresp="#s4:l1">Hoeder</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Nu., en in doodes <rhyme label="b" type="m" corresp="#s4:l2">pijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
