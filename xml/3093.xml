<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3093</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag>farewell song</tag>
      <tag>dance song</tag>
    </tags>
    <melody>Meisje moet ik bij u slapen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VRijers maer wat hebje <rhyme label="a" type="f">kluchjes</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Met de Dochters in den <rhyme label="b" type="m">Haegh</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Alle <rhyme label="b" type="m" corresp="#s1:l2">Daegh</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Want sy betoonen door haer <rhyme label="a" type="f" corresp="#s1:l1">suchjes</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Dat sy zijn tot trouwen <rhyme label="b" type="m" corresp="#s1:l2">graegh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De bloote Borsis sietmen <rhyme label="a" type="f">spelen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Onder 't dunne Floers of <rhyme label="b" type="m">Kantijn</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Suyver <rhyme label="b" type="m" corresp="#s2:l2">fijn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat is te seggen: komt my <rhyme label="a" type="f" corresp="#s2:l1">strelen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Want ick soeck ghestrelt te <rhyme label="b" type="m" corresp="#s2:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dese Haeghse <rhyme label="a" type="f">Herderinnen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Hebben met een soete <rhyme label="b" type="m">reen</rhyme></l>
        <l id="s3:l3" met="2" type="+-"><rhyme label="b" type="m" corresp="#s3:l2">Aen-ghebeen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Wenus de Moeder van het <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Om haer jonghe Soontjen <rhyme label="b" type="m" corresp="#s3:l2">kleen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Venus nam haer <rhyme label="a" type="f">Cuppedootje</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Liet hem uyt den Hemel <rhyme label="b" type="m">hoogh</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">En het <rhyme label="b" type="m" corresp="#s4:l2">vloogh</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Na dese Rey; maer het kleyne <rhyme label="a" type="f" corresp="#s4:l1">Goodtje</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Kregen sy wel haest int <rhyme label="b" type="m" corresp="#s4:l2">Oogh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy maeckten daer de vreught met <rhyme label="a" type="f">danssen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">En sprongen lustigh in de <rhyme label="b" type="m">Windt</rhyme></l>
        <l id="s5:l3" met="2" type="+-">Met het <rhyme label="b" type="m" corresp="#s5:l2">kindt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Sy kroonden hem met <rhyme label="a" type="f" corresp="#s5:l1">Myrthe-kranssen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Het Boefje was van yder <rhyme label="b" type="m" corresp="#s5:l2">bemindt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Men gingh het op de schoodt <rhyme label="a" type="f">doudeyne</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Men kustent met een soet <rhyme label="b" type="m">ghestreel</rhyme></l>
        <l id="s6:l3" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s6:l2">ghespeel</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Men sancker: O Boefje jonghe Boefje <rhyme label="a" type="f" corresp="#s6:l1">kleyne</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Heer, wat is u vleysje <rhyme label="b" type="m" corresp="#s6:l2">eel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sy eerden hem ghelijck een <rhyme label="a" type="f">Engeltje</rhyme></l>
        <l id="s7:l2" met="4" type="-+">En tegen met hem op de <rhyme label="b" type="m">Jacht</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Maer het <rhyme label="b" type="m" corresp="#s7:l2">lacht</rhyme>:</l>
        <l id="s7:l4" met="4" type="-+">Want dit verbraste oolick <rhyme label="a" type="f" corresp="#s7:l1">Bengeltje</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Toonden haest syn Boefjes <rhyme label="b" type="m" corresp="#s7:l2">kracht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Tien vergulde schoone <rhyme label="a" type="f">schichjens</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Trock hy uyt sijn Koocker <rhyme label="b" type="m">root</rhyme>,</l>
        <l id="s8:l3" met="2" type="+-">En hy <rhyme label="b" type="m" corresp="#s8:l2">schoot</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Onder den hoop van dese <rhyme label="a" type="f" corresp="#s8:l1">Nichjes</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Dat een yder angstigh <rhyme label="b" type="m" corresp="#s8:l2">vloot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">d' Een die lockte als een <rhyme label="a" type="f">Minnetje</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Door der liefden soeten <rhyme label="b" type="m">brandt</rhyme></l>
        <l id="s9:l3" met="2" type="+-">Diese <rhyme label="b" type="m" corresp="#s9:l2">vant</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Pronckten haer op als een <rhyme label="a" type="f" corresp="#s9:l1">Goddinnetje</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Geestigh met een Haeghs <rhyme label="b" type="m" corresp="#s9:l2">verstandt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">d' Ander gingh haer ouders <rhyme label="a" type="f">vlieden</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Na een ander plaetsje <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s10:l3" met="2" type="+-">En seer <rhyme label="b" type="m" corresp="#s10:l2">bly</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Gingh daer haer liefste lief <rhyme label="a" type="f" corresp="#s10:l1">ontbieden</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Om de soete <rhyme label="b" type="m" corresp="#s10:l2">sneuckery</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Een die doolde langhs de <rhyme label="a" type="f">Stranden</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Schreef daer met haer eyghen <rhyme label="b" type="m">bloedt</rhyme>,</l>
        <l id="s11:l3" met="2" type="+-">Ay soo <rhyme label="b" type="m" corresp="#s11:l2">soet</rhyme>!</l>
        <l id="s11:l4" met="4" type="-+">Dat haer de Min sou het hartje <rhyme label="a" type="f" corresp="#s11:l1">afbranden</rhyme></l>
        <l id="s11:l5" met="4" type="+-">Soo hy niet haer lusjes en <rhyme label="b" type="m" corresp="#s11:l2">boet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Veel die kregen schoone <rhyme label="a" type="f">Bloesjes</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Op haer teere Kaeckjes <rhyme label="b" type="m">net</rhyme>,</l>
        <l id="s12:l3" met="2" type="+-"><rhyme label="b" type="m" corresp="#s12:l2">gheblancket</rhyme>;</l>
        <l id="s12:l4" met="4" type="-+">Maer dese kluer als roode <rhyme label="a" type="f" corresp="#s12:l1">Roosjens</rhyme></l>
        <l id="s12:l5" met="4" type="+-">Die verloormen haest in 't <rhyme label="b" type="m" corresp="#s12:l2">Bedt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Dus soo komt dat menich <rhyme label="a" type="f">Meysie</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Door het spelen van het <rhyme label="b" type="m">Kint</rhyme></l>
        <l id="s13:l3" met="2" type="+-">Is <rhyme label="b" type="m" corresp="#s13:l2">verblint</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Bieden haer blanck en poesel <rhyme label="a" type="f" corresp="#s13:l1">vleysje</rhyme></l>
        <l id="s13:l5" met="4" type="+-">Aen die geen die haer niet <rhyme label="b" type="m" corresp="#s13:l2">mint</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Jongmans wilje gaen uyt <rhyme label="a" type="f">Vrijen</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Houtje uyt de Cap van de <rhyme label="b" type="m">Sot</rhyme></l>
        <l id="s14:l3" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s14:l2">Marot</rhyme>:</l>
        <l id="s14:l4" met="4" type="-+">Want siet je souter haest <rhyme label="a" type="f" corresp="#s14:l1">bedijen</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">En dan waerje yders <rhyme label="b" type="m" corresp="#s14:l2">spot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
