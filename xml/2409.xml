    <tags>
      <tag translation="love song">liefdeslied</tag>
    </tags>
    <melody>Gij zotten en zottinnen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WIe heymelick wil <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die moet seer veel <rhyme label="b" type="f">verdraghen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">So ick nv moet <rhyme label="a" type="f" corresp="#s1:l1">bekinnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">By nachten en by <rhyme label="b" type="f" corresp="#s1:l2">daghen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die my sou wel <rhyme label="b" type="f" corresp="#s1:l2">behaghen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En derf icx segghen <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Certeen ick mocht haer <rhyme label="b" type="f" corresp="#s1:l2">claghen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Sy gaef my troostelicx <rhyme label="c" type="m" corresp="#s1:l6">yet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">So Paris na <rhyme label="a" type="f">Helene</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Verlangde altijts <rhyme label="b" type="f">eere</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">So haeckt mijn hert <rhyme label="a" type="f" corresp="#s2:l1">certeene</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Na dat maechdelien <rhyme label="b" type="f" corresp="#s2:l2">teere</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Sy is de ick <rhyme label="b" type="f" corresp="#s2:l2">bgeere</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Boven der werelt <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Om haer lijd'ick <rhyme label="b" type="f" corresp="#s2:l2">verseere</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">en lig in Venus <rhyme label="c" type="m" corresp="#s2:l6">gloet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Op haer oochskens <rhyme label="a" type="f">cristalich</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die als die robijn <rhyme label="b" type="f">blincken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Op haer lippekens <rhyme label="a" type="f" corresp="#s3:l1">coralich</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">So is alle mijn <rhyme label="b" type="f" corresp="#s3:l2">ghedincken</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Creech ick een minnelijck <rhyme label="b" type="f" corresp="#s3:l2">wincken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Eens van haeren <rhyme label="c" type="m">persoon</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Certeen ick sou haer <rhyme label="b" type="f" corresp="#s3:l2">schincken</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Mijn hert daer voor te <rhyme label="c" type="m" corresp="#s3:l6">loon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy jonghe <rhyme label="a" type="f">jonghelinghen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Als ghy yemant wilt <rhyme label="b" type="f">vrijen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wilt u so niet <rhyme label="a" type="f" corresp="#s4:l1">bedwinghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ghelijck ick doe by <rhyme label="b" type="f" corresp="#s4:l2">tiien</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Schaemte die doe <rhyme label="b" type="f" corresp="#s4:l2">mijen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dus heb ick dit <rhyme label="c" type="m">misval</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Steltse altijt ter <rhyme label="b" type="f" corresp="#s4:l2">zijen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Want u dat baten <rhyme label="c" type="m" corresp="#s4:l6">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
