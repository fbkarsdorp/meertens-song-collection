    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Solaas wil ik hanteren</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk wil mi gaen <rhyme label="a" type="f">ontladen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ende verlichten minen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ende wachten mi van <rhyme label="a" type="f" corresp="#s1:l1">quaden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat sal mi wesen <rhyme label="b" type="m" corresp="#s1:l2">goet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Van sonden wil ick mi <rhyme label="c" type="f">keeren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In mijnder iongher <rhyme label="d" type="m">tijt</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Ick bid v ghenade lieue <rhyme label="c" type="f" corresp="#s1:l5">heere</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s1:l6">sijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn oogen moeten <rhyme label="a" type="f">aenscouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Na dat ick heb <rhyme label="b" type="m">verdient</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Eest blischap ofte <rhyme label="a" type="f" corresp="#s2:l1">rouwe</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Soe rade ick minen <rhyme label="b" type="m" corresp="#s2:l2">vrient</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dat hi hem van sonden <rhyme label="c" type="f">keere</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Want cort is sinen <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s2:l5">heere</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s2:l6">sijt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Had ic voer tswaer <rhyme label="a" type="f">verliesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghepeyst dat waer mi <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">So soude mi god <rhyme label="a" type="f" corresp="#s3:l1">verkiesen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Als hi den salighen <rhyme label="b" type="m" corresp="#s3:l2">doet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Mer neen ic / my rouwet <rhyme label="c" type="f">seere</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Alle minen verloren <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s3:l5">heere</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s3:l6">sijt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nyemant en derf hem <rhyme label="a" type="f">poghen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Te comen daer God <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hi en moet na sijn <rhyme label="a" type="f" corresp="#s4:l1">vermoghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Van sonden wesen <rhyme label="b" type="m" corresp="#s4:l2">vry</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ende dienen onsen <rhyme label="c" type="f">heere</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wanneer hi heeft den <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s4:l5">heere</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s4:l6">sijt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Noch soe wil ick <rhyme label="a" type="f">hopen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Op God / ende onser <rhyme label="b" type="m">vrouwen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Gods gracie staet altijt <rhyme label="a" type="f" corresp="#s5:l1">open</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Als ons die sonden <rhyme label="b" type="m" corresp="#s5:l2">rouwen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Milde so is die <rhyme label="c" type="f">heere</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Onseker is onsen <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s5:l5">heere</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s5:l6">sijt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Aldus so wil ick <rhyme label="a" type="f">temmen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat ionghe leuen <rhyme label="b" type="m">mijn</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Dat ic namaels mach <rhyme label="a" type="f" corresp="#s6:l1">clemmen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Aldaer die salighe <rhyme label="b" type="m" corresp="#s6:l2">sijn</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Daer is so lanck so <rhyme label="c" type="f">meere</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Blischap in alder <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s6:l5">heere</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s6:l6">sijt</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O radt van <rhyme label="a" type="f">auontueren</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Suldy niet omme <rhyme label="b" type="m">slaen</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Oft mi mocht troost <rhyme label="a" type="f" corresp="#s7:l1">ghebueren</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Van dat ic heb <rhyme label="b" type="m" corresp="#s7:l2">misdaen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Ic en sondichde <rhyme label="c" type="f">nemmermeere</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Al had ic wel den <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s7:l5">heere</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s7:l6">sijt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O god van <rhyme label="a" type="f">hemelrijcke</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">Ic stae hier als een arm <rhyme label="b" type="m">man</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Seer <rhyme label="a" type="f" corresp="#s8:l1">ellendichlijcke</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Siet mi met uwer genaden <rhyme label="b" type="m" corresp="#s8:l2">an</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Gheeft dat ic mi <rhyme label="c" type="f">bekeere</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Het is my meer dan <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s8:l5">heere</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s8:l6">sijt</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ouder mach ic <rhyme label="a" type="f">worden</rhyme>.</l>
        <l id="s9:l2" met="3" type="-+">Mer iongher <rhyme label="b" type="m">nemmermeer</rhyme>.</l>
        <l id="s9:l3" met="3" type="-+">Ende namaels onder der <rhyme label="a" type="f" corresp="#s9:l1">eerden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ende laten goet ende <rhyme label="b" type="m" corresp="#s9:l2">eer</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Waer toe dat ic mi <rhyme label="c" type="f">keere</rhyme>.</l>
        <l id="s9:l6" met="3" type="-+">Altoos gaet minen <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s9:l5">heere</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s9:l6">sijt</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ghestadich wil ic <rhyme label="a" type="f">dienen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ende wesen <rhyme label="b" type="m">onderdaen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Het staet noch te <rhyme label="a" type="f" corresp="#s10:l1">gheschieden</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Dat ick sal moeten <rhyme label="b" type="m" corresp="#s10:l2">gaen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Van daer ic niet en <rhyme label="c" type="f">keere</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Nemmermeer te gheender <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Ick bid ghenade lieue <rhyme label="c" type="f" corresp="#s10:l5">heere</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s10:l6">sijt</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Nu bidt voor mi ten <rhyme label="a" type="f">besten</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Ghi heylighen alle <rhyme label="b" type="m">ghemeen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Al come ic nv ten <rhyme label="a" type="f" corresp="#s11:l1">lesten</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Ick ben nochtans <rhyme label="b" type="m" corresp="#s11:l2">een</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Daer Christus onse lieue <rhyme label="c" type="f">here</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Om sterf op eenen <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Ic bid ghenade lief <rhyme label="c" type="f" corresp="#s11:l5">heere</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Want ghi ghenadich <rhyme label="d" type="m" corresp="#s11:l6">sijt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
