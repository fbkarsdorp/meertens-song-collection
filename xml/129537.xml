<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>129537</id>
    <date>
      <year-start>1708</year-start>
      <year-end>1708</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>seasonal song</tag>
    </tags>
    <melody>Cette cruelle</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De winter is voorby <rhyme label="a" type="f">gestreken</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">De Somer die staat voor de <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die ons soo lieffelick komt <rhyme label="a" type="f" corresp="#s1:l1">queecken</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Met Bloemtjes alsoo soet van <rhyme label="b" type="m" corresp="#s1:l2">geur</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">De warme <rhyme label="c" type="m">dagen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Lief, die komen <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Komt laet ons <rhyme label="c" type="m" corresp="#s1:l5">jagen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Daar de bloemtjes <rhyme label="d" type="m" corresp="#s1:l6">staen</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Die cierlyck open <rhyme label="c" type="m" corresp="#s1:l5">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Mey die toont sijn beste <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Van Bloemen, Kruyden en van <rhyme label="b" type="m">Gras</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy doet uyt lopen de groene <rhyme label="a" type="f" corresp="#s2:l1">bladen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Van al ‘t lieffelik <rhyme label="b" type="m" corresp="#s2:l2">Boom-gewas</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Waar op het <rhyme label="c" type="m">diertje</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Met een soete <rhyme label="d" type="m">taal</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Singt tiere <rhyme label="c" type="m" corresp="#s2:l5">liertje</rhyme> -</l>
        <l id="s2:l8" met="3" type="-+">Maer de <rhyme label="d" type="m" corresp="#s2:l6">Nachtegael</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Verciert het <rhyme label="c" type="m" corresp="#s2:l5">altemael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In ‘t veldt daar hoort men de leeuw’rik <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Al quelende vliegt hy na de <rhyme label="b" type="m">lucht</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">In’t Duyn daar siet men de Knijntjes <rhyme label="a" type="f" corresp="#s3:l1">springen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Sy leven daer in soet <rhyme label="b" type="m" corresp="#s3:l2">genucht</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Het <rhyme label="c" type="m">mosse-prytje</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Roept staeg: ‘dief, dief, <rhyme label="d" type="m">dief</rhyme>’;</l>
        <l id="s3:l7" met="2" type="-+">Het Honingh <rhyme label="c" type="m" corresp="#s3:l5">beytje</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Heeft de bloempjes <rhyme label="d" type="m" corresp="#s3:l6">lief</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Sy staen tot sijn <rhyme label="c" type="m" corresp="#s3:l5">gerief</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">‘t Swalfje met sijn snelle <rhyme label="a" type="f">wiecken</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Vliegt als een Schim stracks hier en <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Soo dra den dagh begint te <rhyme label="a" type="f" corresp="#s4:l1">kriecken</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Komt den gebeckten <rhyme label="b" type="m" corresp="#s4:l2">Oyevaer</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Na buiten <rhyme label="c" type="m">vliegen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Om te soecken <rhyme label="d" type="m">aas</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Hy gaat <rhyme label="c" type="m" corresp="#s4:l5">bedriegen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Den Kickvors: hy den <rhyme label="d" type="m" corresp="#s4:l6">baas</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Speelt met een groot <rhyme label="c" type="m" corresp="#s4:l5">geraas</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Twee Schaapjens aan een tuygh <rhyme label="a" type="f">gestrengelt</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Die trecken samen eenen <rhyme label="b" type="m">leyn</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Wat verder zitter een en <rhyme label="a" type="f" corresp="#s5:l1">hengelt</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Daer dient patientie by te <rhyme label="b" type="m" corresp="#s5:l2">zijn</rhyme>;</l>
        <l id="s5:l5" met="2" type="-+">Al sijn <rhyme label="c" type="m">verlangen</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Is na snoeck of <rhyme label="d" type="m">baers</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Kan hy die <rhyme label="c" type="m" corresp="#s5:l5">vangen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Hy houdtse voor wat <rhyme label="d" type="m" corresp="#s5:l6">raers</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Maer vangt somtyts wat <rhyme label="c" type="m" corresp="#s5:l5">aars</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">‘s Morgens kraeyt den boer sijn <rhyme label="a" type="f">Haantje</rhyme>;</l>
        <l id="s6:l2" met="4" type="-+">Maer komt eens aen de <rhyme label="b" type="m">water-kant</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Daar ziet men ‘t witte geveederde <rhyme label="a" type="f" corresp="#s6:l1">Swaantje</rhyme>;</l>
        <l id="s6:l4" met="4" type="-+">Langes de willige <rhyme label="b" type="m" corresp="#s6:l2">abundant</rhyme></l>
        <l id="s6:l5" met="2" type="-+">De vogeltjes <rhyme label="c" type="m">zingen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Boven in ‘t <rhyme label="d" type="m">geboomt</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">De visjes <rhyme label="c" type="m" corresp="#s6:l5">springen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Daar 't water <rhyme label="d" type="m" corresp="#s6:l6">stroomt</rhyme>:</l>
        <l id="s6:l9" met="3" type="-+">‘t Is lust onder ‘t <rhyme label="c" type="m" corresp="#s6:l5">geboomt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">‘t Is lust om te gaan <rhyme label="a" type="f">vermeyden</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Wanneer den dagh begint te kriecken <rhyme label="b" type="m">aan</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Siet men de Koeytjes in de <rhyme label="a" type="f" corresp="#s7:l1">weyde</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Met soete melckje zwaer <rhyme label="b" type="m" corresp="#s7:l2">belaen</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Waar op het <rhyme label="c" type="m">boertje</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Met sijn Huys vrouw <rhyme label="d" type="m">past</rhyme>:</l>
        <l id="s7:l7" met="2" type="-+">Hy neemt een <rhyme label="c" type="m" corresp="#s7:l5">snoertje</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Bind haer beentjes <rhyme label="d" type="m" corresp="#s7:l6">vast</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Ontslaet haer van de <rhyme label="c" type="m" corresp="#s7:l5">last</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Nu wil ik eens gaan <rhyme label="a" type="f">besluyten</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Van al het geen ik heb <rhyme label="b" type="m">gesien</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Die meer wil kijcken die gaet na <rhyme label="a" type="f" corresp="#s8:l1">buiten</rhyme>:</l>
        <l id="s8:l4" met="4" type="-+">Treet met u twee’n, Maer ik was <rhyme label="b" type="m" corresp="#s8:l2">alleen</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">‘t Komt al van <rhyme label="c" type="m">boven</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Dese <rhyme label="d" type="m">soetigheyt</rhyme>:</l>
        <l id="s8:l7" met="2" type="-+">Laet ons gaen <rhyme label="c" type="m" corresp="#s8:l5">loven</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Godes <rhyme label="d" type="m" corresp="#s8:l6">Majestijt</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Tot in der <rhyme label="c" type="m" corresp="#s8:l5">eeuwigheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
