<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160100</id>
    <date>
      <year-start>1669</year-start>
      <year-end>1669</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik voer al over zee</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LOoft my <rhyme label="a" type="m">nakoomeling</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Looft my nakoomeling, ik <rhyme label="a" type="m" corresp="#s1:l1">zing</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoe 'k Bachus by de <rhyme label="b" type="f">rotzen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoë</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zagh zitten in een <rhyme label="a" type="m" corresp="#s1:l1">kring</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zagh 'er hoe dat <rhyme label="a" type="m">hy</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ik zagh 'er hoe dat hy, daar <rhyme label="a" type="m" corresp="#s2:l1">by</rhyme></l>
        <l id="s2:l3" met="3" type="-+">De Nimfen en de <rhyme label="b" type="f">Saters</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Evoé, <rhyme label="c" type="m">Evoë</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Opzong zijn <rhyme label="a" type="m" corresp="#s2:l1">Poëzy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O Evoë! mijn <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">O Evoë! mijn hert, dat <rhyme label="a" type="m" corresp="#s3:l1">wert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Door eene versche <rhyme label="b" type="f">vreeze</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Evoẽ, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En beevinge <rhyme label="a" type="m" corresp="#s3:l1">benerdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O Bachus! mijn <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">O Bachus! mijn gemoedt, en <rhyme label="a" type="m" corresp="#s4:l1">bloedt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Van uwe godtheit <rhyme label="b" type="f">zwanger</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Door vreugde wordt <rhyme label="a" type="m" corresp="#s4:l1">gevoedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O Liber! <rhyme label="a" type="m">Evoẽ</rhyme>!</l>
        <l id="s5:l2" met="4" type="-+">O Liber! Evoẽ! die <rhyme label="a" type="m" corresp="#s5:l1">meê</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Gevreest zijt door u <rhyme label="b" type="f">wijngaartspiets</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Evoẽ, <rhyme label="c" type="m">Evoë</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Genade geef en <rhyme label="a" type="m" corresp="#s5:l1">vreê</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Het staat nu heeden <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Het staat nu heeden vry, voor <rhyme label="a" type="m" corresp="#s6:l1">my</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Van wijnbron en <rhyme label="b" type="f">Thyaden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoé</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Te schaateren heel <rhyme label="a" type="m" corresp="#s6:l1">bly</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">En van de <rhyme label="a" type="m">mellekvliet</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En van de mellekvliet, een <rhyme label="a" type="m" corresp="#s7:l1">liedt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Te zingen, en den <rhyme label="b" type="f">hooning</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoë</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Die uit de struiken <rhyme label="a" type="m" corresp="#s7:l1">schiet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">'k Heb oorlof en ook <rhyme label="a" type="m">stof</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">'k Heb oorlof en ook stof, de <rhyme label="a" type="m" corresp="#s8:l1">lof</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Van uwe Bruidt te <rhyme label="b" type="f">zingen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoë</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Gesteldt aan't <rhyme label="a" type="m" corresp="#s8:l1">Starrenhof</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Het staatme vry met <rhyme label="a" type="m">zangk</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Het staatme vry met zangk, en <rhyme label="a" type="m" corresp="#s9:l1">klank</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Van Pentheus en <rhyme label="b" type="f">Lykurgus</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoë</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Te melden <rhyme label="a" type="m" corresp="#s9:l1">d'ondergangk</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Gy leidt, ô <rhyme label="a" type="m">Evoë</rhyme>!</l>
        <l id="s10:l2" met="4" type="-+">Gy leidt, ô Evoẽ, ook <rhyme label="a" type="m" corresp="#s10:l1">meê</rhyme></l>
        <l id="s10:l3" met="3" type="-+">De stroomen van de <rhyme label="b" type="f">vlieten</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Evoẽ, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En temdt de woeste <rhyme label="a" type="m" corresp="#s10:l1">Zee</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wanneer gy zijt van <rhyme label="a" type="m">nat</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Wanneer gy zijt van nat, <rhyme label="a" type="m" corresp="#s11:l1">bespat</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Op afgescheiden <rhyme label="b" type="f">heuvels</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoë</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Heeft schaad op u geen <rhyme label="a" type="m" corresp="#s11:l1">vat</rhyme>;</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Al strikte gy te <rhyme label="a" type="m">hoop</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Al strikte gy te hoop, ter <rhyme label="a" type="m" corresp="#s12:l1">loop</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">De vlechten der <rhyme label="b" type="f">Bachanten</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Evoẽ, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Met eenen <rhyme label="a" type="m" corresp="#s12:l1">adderknoop</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Toen 't heilloos <rhyme label="a" type="m">Reuzenrot</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Toen 't heilloos Reuzenrot, dien <rhyme label="a" type="m" corresp="#s13:l1">Godt</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Jupijn om hoog <rhyme label="b" type="f">bestormde</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">En klom na 't Hemelsch <rhyme label="a" type="m" corresp="#s13:l1">slot</rhyme>:</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Toen hebt gy Rekus <rhyme label="a" type="m">zelf</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Toen hebt gy Rekus zelf, ja <rhyme label="a" type="m" corresp="#s14:l1">zelf</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Met leeuwenmuil en <rhyme label="b" type="f">klaauwen</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Gestooten van 't <rhyme label="a" type="m" corresp="#s14:l1">gewelf</rhyme>:</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Hoewel dat yder <rhyme label="a" type="m">docht</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Hoewel dat yder docht, gy <rhyme label="a" type="m" corresp="#s15:l1">mocht</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Boert, dans; en spel veel <rhyme label="b" type="f">beeter</rhyme></l>
        <l id="s15:l4" met="4" type="+-">Evoẽ, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Verstaan dan <rhyme label="a" type="m" corresp="#s15:l1">oorlogs-tocht</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Maar gy, ô <rhyme label="a" type="m">Evoẽ</rhyme>!</l>
        <l id="s16:l2" met="4" type="-+">Maar gy, ô Evoẽ! waart <rhyme label="a" type="m" corresp="#s16:l1">meê</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Wel afgerecht op <rhyme label="b" type="f">beyde</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Evoẽ, <rhyme label="c" type="m">Evoẽ</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Den oorloogh en de <rhyme label="a" type="m" corresp="#s16:l1">vreê</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">De helhondt staakte <rhyme label="a" type="m">ras</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">De helhondt staakte ras, 't <rhyme label="a" type="m" corresp="#s17:l1">gebas</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Toen hy zagh uwen <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Evoë, <rhyme label="c" type="m">Evoô</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Daar 't hooft verçiert meê <rhyme label="a" type="m" corresp="#s17:l1">was</rhyme>:</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Hy kwispelstaarte <rhyme label="a" type="m">zoet</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Hy kwispelstaarte zoet, en <rhyme label="a" type="m" corresp="#s18:l1">groett</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">En lekte, met dry <rhyme label="b" type="f">tongen</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Evoẽ, <rhyme label="c" type="m">Evoë</rhyme></l>
        <l id="s18:l5" met="3" type="-+">In 't scheiden uwen <rhyme label="a" type="m" corresp="#s18:l1">voet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
