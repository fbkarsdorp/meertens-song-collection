<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178365</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Mars van Dessau</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Zing mee, <rhyme label="a" type="m">Hoezée</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zo Juichen Batos <rhyme label="b" type="f">neeven</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die voor Oranje <rhyme label="b" type="f" corresp="#s1:l2">geeven</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Lyf en <rhyme label="c" type="m">Goed</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Wil men hun 't recht <rhyme label="d" type="f">berooven</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Dan vol <rhyme label="c" type="m" corresp="#s1:l4">moed</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Is het al Oranje <rhyme label="d" type="f" corresp="#s1:l5">boven</rhyme>;</l>
        <l id="s1:l8" met="3" type="+-">Weg dat snood <rhyme label="c" type="m" corresp="#s1:l4">gebroed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Die Vorst! Die <rhyme label="a" type="m">torst</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Al Nederlands <rhyme label="b" type="f">belangen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Zal haast de snoodaarts <rhyme label="b" type="f" corresp="#s2:l2">vangen</rhyme></l>
        <l id="s2:l4" met="2" type="+-">In den <rhyme label="c" type="m">Kuil</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Door hun voor hem <rhyme label="d" type="f">gegraven</rhyme>.</l>
        <l id="s2:l6" met="2" type="+-">Hy de <rhyme label="c" type="m" corresp="#s2:l4">Zuil</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Voor alle Neerlands <rhyme label="d" type="f" corresp="#s2:l5">braaven</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Tart het snood <rhyme label="c" type="m" corresp="#s2:l4">gehuil</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ons Goed en <rhyme label="a" type="m">Bloed</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Besteên wy voor <rhyme label="b" type="f">Oranje</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">Hy hoed ons vry <rhyme label="b" type="f" corresp="#s3:l2">Germanje</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-"><rhyme label="c" type="m">Overheerd</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Door snoode <rhyme label="d" type="f">Lasteraaren</rhyme></l>
        <l id="s3:l6" met="2" type="+-">'t Recht <rhyme label="c" type="m" corresp="#s3:l4">verkeerdt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hersteld by zyn <rhyme label="d" type="f" corresp="#s3:l5">verjaaren</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Daar men <rhyme label="c" type="m" corresp="#s3:l4">Illumneerdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Hoezée 's Lands <rhyme label="a" type="m">bee</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ten Hemel <rhyme label="b" type="f">opgeheeven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Doet 's Lands verdrukkers <rhyme label="b" type="f" corresp="#s4:l2">beeven</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Ons, <rhyme label="c" type="m">volmoed</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">De Stem by 't Speeltuyg <rhyme label="d" type="f">paren</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Snood <rhyme label="c" type="m" corresp="#s4:l4">gebroed</rhyme>!</l>
        <l id="s4:l7" met="3" type="-+">Wie drygt gy met <rhyme label="d" type="f" corresp="#s4:l5">gevaaren</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Daar ons God <rhyme label="c" type="m" corresp="#s4:l4">behoedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Het Regt, <rhyme label="a" type="m">ontzegt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Aan Ed'le <rhyme label="b" type="f">Batavieren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die smaaden Vrycorps <rhyme label="b" type="f" corresp="#s5:l2">dieren</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Maar <rhyme label="c" type="m">getrouw</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Nog voor Oranje <rhyme label="d" type="f">leeven</rhyme>;</l>
        <l id="s5:l6" met="2" type="+-">'t <rhyme label="c" type="m" corresp="#s5:l4">Staatsgebouw</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Word door dien Vorst <rhyme label="d" type="f" corresp="#s5:l5">verheven</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Boven druk en <rhyme label="c" type="m" corresp="#s5:l4">rouw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Uw nood, <rhyme label="a" type="m">vergroot</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">ô Monsters! door uw <rhyme label="b" type="f">grillen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maar hoe zult gy <rhyme label="b" type="f" corresp="#s6:l2">gillen</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Eens ten <rhyme label="c" type="m">top</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Van uw woên <rhyme label="d" type="f">gesteegen</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Als g'een <rhyme label="c" type="m" corresp="#s6:l4">strop</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Hebt tot uw loon <rhyme label="d" type="f" corresp="#s6:l5">gekreegen</rhyme>!</l>
        <l id="s6:l8" met="3" type="+-">ô Myn Kop! myn <rhyme label="c" type="m" corresp="#s6:l4">Kop</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Maar wy, Die <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En trouw den Vorst <rhyme label="b" type="f">beminnen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zullen eens <rhyme label="b" type="f" corresp="#s7:l2">verwinnen</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">En, door <rhyme label="c" type="m">God</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Met hem <rhyme label="d" type="f">triompheeren</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Om ons <rhyme label="c" type="m" corresp="#s7:l4">Lot</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Daar de Heer der <rhyme label="d" type="f" corresp="#s7:l5">Heeren</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Al uw doen <rhyme label="c" type="m" corresp="#s7:l4">bespot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">ô Heer! <rhyme label="a" type="m">Verëer</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Aan onzen Vorst veel <rhyme label="b" type="f">daagen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">In Uw <rhyme label="b" type="f" corresp="#s8:l2">Welbehaagen</rhyme>.</l>
        <l id="s8:l4" met="2" type="+-">Spaar ons <rhyme label="c" type="m">Land</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Zyn Gemalin en <rhyme label="d" type="f">Spruiten</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">Hoed uw <rhyme label="c" type="m" corresp="#s8:l4">Hand</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Stoot alle twisters <rhyme label="d" type="f" corresp="#s8:l5">buiten</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Herstel d'Eendrachts <rhyme label="c" type="m" corresp="#s8:l4">band</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">ô Heer! ai <rhyme label="a" type="m">leer</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Al die uw Willem <rhyme label="b" type="f">haaten</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Hun boos gedrag <rhyme label="b" type="f" corresp="#s9:l2">verlaaten</rhyme>.</l>
        <l id="s9:l4" met="2" type="+-">'t Blind <rhyme label="c" type="m">gezicht</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Van Neêrlands <rhyme label="d" type="f">onheilzoekers</rhyme>,</l>
        <l id="s9:l6" met="2" type="+-">Word <rhyme label="c" type="m" corresp="#s9:l4">verlicht</rhyme>;</l>
        <l id="s9:l7" met="3" type="-+">Leer Hun, dat Gy uw <rhyme label="d" type="f" corresp="#s9:l5">vloekers</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">Door uw oordeel <rhyme label="c" type="m" corresp="#s9:l4">richt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
