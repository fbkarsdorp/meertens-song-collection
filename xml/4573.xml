    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Petit Bordeaux</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GOeden avont zy u <rhyme label="a" type="f">allen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Excuseert mijn <rhyme label="b" type="m">stoutigheyt</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">'k Kom soo uyt den Hemel <rhyme label="a" type="f" corresp="#s1:l1">vallen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Om te doen wat Moeder <rhyme label="b" type="m" corresp="#s1:l2">zeyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Jonge, seyse gist're <rhyme label="a" type="f">morgen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Gaat ter Bruyloft na van <rhyme label="b" type="m">Laar</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Doet je Koocker wel <rhyme label="a" type="f" corresp="#s2:l1">voorsorge</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dat je staal weer treft een <rhyme label="b" type="m" corresp="#s2:l2">paar</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gaat de Gasten wat <rhyme label="a" type="f">verheugen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Singt dan op de Bruylofts <rhyme label="b" type="m">zaal</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Drinkt eens om met lust'ge <rhyme label="a" type="f" corresp="#s3:l1">teugen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ick zal doen een zoet <rhyme label="b" type="m" corresp="#s3:l2">verhaal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dat je wist de lieve <rhyme label="a" type="f">kluchten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Die men in de Echt <rhyme label="b" type="m">geniet</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Als twee herten tot een <rhyme label="a" type="f" corresp="#s4:l1">zuchten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'t Klagen geeft dan geen <rhyme label="b" type="m" corresp="#s4:l2">verdriet</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Kusjes die de Honich <rhyme label="a" type="f">tarten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Lonkjes van de kuysche <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Kittelen soo die lieve <rhyme label="a" type="f" corresp="#s5:l1">harten</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Denkt wat voor een God ick <rhyme label="b" type="m" corresp="#s5:l2">bin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wie dat maer mijn schicht moet <rhyme label="a" type="f">derven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Heeft geen lust ter werelt <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Souje dan wel willen <rhyme label="a" type="f" corresp="#s6:l1">sterven</rhyme>?</l>
        <l id="s6:l4" met="4" type="+-">Die mijn Moeders vreugt <rhyme label="b" type="m" corresp="#s6:l2">geniet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">De Tijdt was nu in Venus <rhyme label="a" type="f">Tempel</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Dat men vierde 't <rhyme label="b" type="m">heyligdom</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Als'er naarde veel haar <rhyme label="a" type="f" corresp="#s7:l1">drempel</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Om te wesen <rhyme label="b" type="m" corresp="#s7:l2">Bruydegom</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Al de Hemel Goden <rhyme label="a" type="f">songen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Bacchus schonk sijn Nekter <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Al de Cupidootjes <rhyme label="a" type="f" corresp="#s8:l1">sprongen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Om dat quam van Laar sijn <rhyme label="b" type="m" corresp="#s8:l2">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">d'Godt spraak riep, vereende <rhyme label="a" type="f">zielen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Heyl en Vree zy u <rhyme label="b" type="m">altijdt</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Niemant mach u rust <rhyme label="a" type="f" corresp="#s9:l1">vernielen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Tot ghy blickt in <rhyme label="b" type="m" corresp="#s9:l2">eeuwigheyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Geen tong en kon d'vreugt <rhyme label="a" type="f">uyt-galmen</rhyme>:</l>
        <l id="s10:l2" met="4" type="+-">Vraegt het die van dese <rhyme label="b" type="m">nacht</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Kampen sullen om de <rhyme label="a" type="f" corresp="#s10:l1">Palmen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Op het dons ick houw-de <rhyme label="b" type="m" corresp="#s10:l2">wacht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">d Fackel vlamt al in veel <rhyme label="a" type="f">glooren</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Doch hy moet haest uyt <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Als de Bruygom laet sijn <rhyme label="a" type="f" corresp="#s11:l1">tooren</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Met sijn Bruydt sal slapen <rhyme label="b" type="m" corresp="#s11:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Seg nu Jongmans, en gy <rhyme label="a" type="f">Maagde</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Wensje niet mee na dees <rhyme label="b" type="m">staet</rhyme>?</l>
        <l id="s12:l3" met="4" type="+-">'t Geen dit Paar soo wel <rhyme label="a" type="f" corresp="#s12:l1">behaagde</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Doet het oock eer 't werdt te <rhyme label="b" type="m" corresp="#s12:l2">laet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">'k Wensje alle goede <rhyme label="a" type="f">morgen</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">d'Middernacht is al <rhyme label="b" type="m">voorby</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">'k Moet mijn Kooker gaan <rhyme label="a" type="f" corresp="#s13:l1">voorsorgen</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Om te doen wat Moeder <rhyme label="b" type="m" corresp="#s13:l2">sey</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
