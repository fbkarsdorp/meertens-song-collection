  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">L stlich so hab ich mich a <rhyme label="a" type="m">ßerweltt</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">meins hertzen ein ewigenn <rhyme label="b" type="m">kron</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">Die mir jn meinem hertzenn <rhyme label="a" type="m" corresp="#s1:l1">gefeltt</rhyme> /</l>
