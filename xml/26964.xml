        <l id="s4:l4" met="3" type="-+">Die liebde sall werden <rhyme label="b" type="m" corresp="#s4:l2">claer</rhyme> /</l>
        <l id="s4:l5" met="4" type="-+">die ich z  ihr hab <rhyme label="c" type="f">getragenn</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">So manniche st ndt vnnd <rhyme label="d" type="m">tagh</rhyme> /</l>
        <l id="s4:l7" met="4" type="-+">Nh n will ich offentlich <rhyme label="c" type="f" corresp="#s4:l5">sagen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Das machett Ven s <rhyme label="d" type="m" corresp="#s4:l6">macht</rhyme></l>
