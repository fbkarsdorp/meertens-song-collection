      <category translation="work">werk</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Aan boord, aan boord, het <rhyme label="a" type="f">Zeemansleven</rhyme></l>
        <l id="s1:l2" met="4" type="-+">vivat de zee! vivat de <rhyme label="b" type="m">zee</rhyme></l>
        <l id="s1:l3" met="4" type="-+">wat kan ons meer genoegen <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme></l>
        <l id="s1:l4" met="4" type="-+">wie gaat er mee? wie gaat er <rhyme label="b" type="m" corresp="#s1:l2">mee</rhyme></l>
        <l id="s1:l5" met="3" type="+-">hoezee, hoezee, <rhyme label="b" type="m" corresp="#s1:l2">hoezee</rhyme></l>
        <l id="s1:l6" met="3" type="-+">wie gaat er met ons <rhyme label="b" type="m" corresp="#s1:l2">meÃª</rhyme></l>
        <l id="s1:l7" met="3" type="-+">gaan wij weer aan <rhyme label="c" type="m">boord</rhyme></l>
        <l id="s1:l8" met="5" type="-+">verheugd en blij zijn wij zoo het <rhyme label="c" type="m" corresp="#s1:l7">behoort</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">En kaatje wil niet <rhyme label="d" type="f">huilen</rhyme></l>
        <l id="s1:l10" met="4" type="+-">daar heb je mijn beste <rhyme label="d" type="f" corresp="#s1:l9">muilen</rhyme></l>
        <l id="s1:l11" met="3" type="-+">en Kaatje wil niet <rhyme label="d" type="f" corresp="#s1:l9">huilen</rhyme></l>
        <l id="s1:l12" met="4" type="+-">daar heb je mijn <rhyme label="e" type="m">beugeltas</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Aan boord, aan boord, het <rhyme label="a" type="f">Zeemansleven</rhyme></l>
        <l id="s2:l2" met="4" type="-+">vivat de zee, vivat de <rhyme label="b" type="m">zee</rhyme></l>
        <l id="s2:l3" met="4" type="-+">voor gevaar zullen wij niet <rhyme label="a" type="f" corresp="#s2:l1">beven</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">wie gaat er mee? leve de <rhyme label="b" type="m" corresp="#s2:l2">zee</rhyme></l>
        <l id="s2:l5" met="3" type="+-">hoezee, hoezee, <rhyme label="b" type="m" corresp="#s2:l2">hoezee</rhyme></l>
        <l id="s2:l6" met="3" type="-+">het schip ligt op de <rhyme label="b" type="m" corresp="#s2:l2">ree</rhyme></l>
        <l id="s2:l7" met="3" type="-+">de zeilen zijn reeds <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s2:l8" met="5" type="-+">treur maar niet lieve meid het is een <rhyme label="c" type="m" corresp="#s2:l7">halfjaar</rhyme></l>
        <l id="s2:l9" met="3" type="-+">En kaatje wil niet <rhyme label="d" type="f">huilen</rhyme></l>
        <l id="s2:l10" met="4" type="+-">daar heb je mijn beste <rhyme label="d" type="f" corresp="#s2:l9">muilen</rhyme></l>
        <l id="s2:l11" met="3" type="-+">en Kaatje wil niet <rhyme label="d" type="f" corresp="#s2:l9">huilen</rhyme></l>
        <l id="s2:l12" met="4" type="+-">daar heb je mijn <rhyme label="e" type="m">beugeltas</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Aan boord, aan boord het <rhyme label="a" type="f">zeemansleven</rhyme></l>
        <l id="s3:l2" met="4" type="-+">vivat de zee vivat de <rhyme label="b" type="m">zee</rhyme></l>
        <l id="s3:l3" met="4" type="-+">dat is juist het beste <rhyme label="a" type="f" corresp="#s3:l1">leven</rhyme></l>
        <l id="s3:l4" met="4" type="-+">jongens vaar mee, leve de <rhyme label="b" type="m" corresp="#s3:l2">zee</rhyme></l>
        <l id="s3:l5" met="3" type="+-">hoezee hoezee <rhyme label="b" type="m" corresp="#s3:l2">hoezee</rhyme></l>
        <l id="s3:l6" met="3" type="-+">ga vrolijk naar de <rhyme label="b" type="m" corresp="#s3:l2">zee</rhyme></l>
        <l id="s3:l7" met="3" type="-+">wij houden ons <rhyme label="c" type="m">konstant</rhyme></l>
        <l id="s3:l8" met="5" type="-+">vat maar aan jongens knap en bij de <rhyme label="c" type="m" corresp="#s3:l7">hand</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">En kaatje wil niet <rhyme label="d" type="f">huilen</rhyme></l>
        <l id="s3:l10" met="4" type="+-">daar heb je mijn beste <rhyme label="d" type="f" corresp="#s3:l9">muilen</rhyme></l>
        <l id="s3:l11" met="3" type="-+">en Kaatje wil niet <rhyme label="d" type="f" corresp="#s3:l9">huilen</rhyme></l>
        <l id="s3:l12" met="4" type="+-">daar heb je mijn <rhyme label="e" type="m">beugeltas</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Aan boord, aan boord, het <rhyme label="a" type="f">zeemansleven</rhyme></l>
        <l id="s4:l2" met="4" type="-+">vivat de zee vivat de <rhyme label="b" type="m">zee</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Wij zullen ons op reis <rhyme label="a" type="f" corresp="#s4:l1">begeven</rhyme></l>
        <l id="s4:l4" met="4" type="-+">van hier na de ree, van hier naar de <rhyme label="b" type="m" corresp="#s4:l2">ree</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">hoezee, hoezee <rhyme label="b" type="m" corresp="#s4:l2">hoezee</rhyme></l>
        <l id="s4:l6" met="3" type="-+">toch gaan wij nu meer <rhyme label="b" type="m" corresp="#s4:l2">mee</rhyme></l>
        <l id="s4:l7" met="3" type="-+">meisjes wees <rhyme label="c" type="m">getroost</rhyme></l>
        <l id="s4:l8" met="5" type="-+">onze reis die is maar voor een korte <rhyme label="c" type="m" corresp="#s4:l7">poos</rhyme></l>
        <l id="s4:l9" met="3" type="-+">En kaatje wil niet <rhyme label="d" type="f">huilen</rhyme></l>
        <l id="s4:l10" met="4" type="+-">daar heb je mijn beste <rhyme label="d" type="f" corresp="#s4:l9">muilen</rhyme></l>
        <l id="s4:l11" met="3" type="-+">en Kaatje wil niet <rhyme label="d" type="f" corresp="#s4:l9">huilen</rhyme></l>
        <l id="s4:l12" met="4" type="+-">daar heb je mijn <rhyme label="e" type="m">beugeltas</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Aan boord aan boord, het <rhyme label="a" type="f">zeemansleven</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">vivat de zee vivat de <rhyme label="b" type="m">zee</rhyme></l>
        <l id="s5:l3" met="4" type="-+">een Hollandsch matroos en zal niet <rhyme label="a" type="f" corresp="#s5:l1">beven</rhyme></l>
        <l id="s5:l4" met="4" type="-+">jongens maar mee naar de <rhyme label="b" type="m" corresp="#s5:l2">zee</rhyme></l>
        <l id="s5:l5" met="3" type="+-">hoezee hoezee <rhyme label="b" type="m" corresp="#s5:l2">hoezee</rhyme></l>
        <l id="s5:l6" met="3" type="-+">van daag nog na de <rhyme label="b" type="m" corresp="#s5:l2">ree</rhyme></l>
        <l id="s5:l7" met="3" type="-+">gaan wij hand aan <rhyme label="c" type="m">hand</rhyme></l>
        <l id="s5:l8" met="5" type="-+">ieder matroos is op zee een ferme <rhyme label="c" type="m" corresp="#s5:l7">klant</rhyme></l>
        <l id="s5:l9" met="3" type="-+">En kaatje wil niet <rhyme label="d" type="f">huilen</rhyme></l>
        <l id="s5:l10" met="4" type="+-">daar heb je mijn beste <rhyme label="d" type="f" corresp="#s5:l9">muilen</rhyme></l>
        <l id="s5:l11" met="3" type="-+">en Kaatje wil niet <rhyme label="d" type="f" corresp="#s5:l9">huilen</rhyme></l>
        <l id="s5:l12" met="4" type="+-">daar heb je mijn <rhyme label="e" type="m">beugeltas</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Aan boord, aan boord, het <rhyme label="a" type="f">zeemansleven</rhyme></l>
        <l id="s6:l2" met="4" type="-+">vivat de zee, vivat de <rhyme label="b" type="m">zee</rhyme></l>
        <l id="s6:l3" met="4" type="-+">wij zullen ons op schip <rhyme label="a" type="f" corresp="#s6:l1">begeven</rhyme></l>
        <l id="s6:l4" met="4" type="-+">kom dan nu mee, vlug naar de <rhyme label="b" type="m" corresp="#s6:l2">zee</rhyme></l>
        <l id="s6:l5" met="3" type="+-">hoezee hoezee <rhyme label="b" type="m" corresp="#s6:l2">hoezee</rhyme></l>
        <l id="s6:l6" met="3" type="-+">wij leven wel te <rhyme label="b" type="m" corresp="#s6:l2">vree</rhyme></l>
        <l id="s6:l7" met="3" type="-+">treurt niet in dezen <rhyme label="c" type="m">tijd</rhyme></l>
        <l id="s6:l8" met="5" type="-+">wij keeren ras weder vrolijk en <rhyme label="c" type="m" corresp="#s6:l7">verblijd</rhyme>.</l>
        <l id="s6:l9" met="3" type="-+">En kaatje wil niet <rhyme label="d" type="f">huilen</rhyme></l>
        <l id="s6:l10" met="4" type="+-">daar heb je mijn beste <rhyme label="d" type="f" corresp="#s6:l9">muilen</rhyme></l>
        <l id="s6:l11" met="3" type="-+">en Kaatje wil niet <rhyme label="d" type="f" corresp="#s6:l9">huilen</rhyme></l>
        <l id="s6:l12" met="4" type="+-">daar heb je mijn <rhyme label="e" type="m">beugeltas</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Aan boord, aan boord, het <rhyme label="a" type="f">zeemansleven</rhyme></l>
        <l id="s7:l2" met="4" type="-+">vivat de zee, vivat de <rhyme label="b" type="m">zee</rhyme></l>
        <l id="s7:l3" met="4" type="-+">wij zullen ons op schip <rhyme label="a" type="f" corresp="#s7:l1">begeven</rhyme></l>
        <l id="s7:l4" met="4" type="-+">eerst op de ree, eerst op de <rhyme label="b" type="m" corresp="#s7:l2">ree</rhyme></l>
        <l id="s7:l5" met="3" type="+-">hoezee, hoezee, <rhyme label="b" type="m" corresp="#s7:l2">hoezee</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">komt jongens ga maar <rhyme label="b" type="m" corresp="#s7:l2">mee</rhyme></l>
        <l id="s7:l7" met="3" type="-+">vrolijk nu naar de <rhyme label="c" type="m">oost</rhyme></l>
        <l id="s7:l8" met="5" type="-+">hartjelief houd u goed en wees <rhyme label="c" type="m" corresp="#s7:l7">getroost</rhyme></l>
        <l id="s7:l9" met="3" type="-+">En kaatje wil niet <rhyme label="d" type="f">huilen</rhyme></l>
        <l id="s7:l10" met="4" type="+-">daar heb je mijn beste <rhyme label="d" type="f" corresp="#s7:l9">muilen</rhyme></l>
        <l id="s7:l11" met="3" type="-+">en Kaatje wil niet <rhyme label="d" type="f" corresp="#s7:l9">huilen</rhyme></l>
        <l id="s7:l12" met="4" type="+-">daar heb je mijn <rhyme label="e" type="m">beugeltas</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
