    <melody>Psalm 128 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SAlich is hi <rhyme label="a" type="f">bedeghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">So wie den Heere <rhyme label="b" type="m">vreest</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ende wandelt in sinen <rhyme label="a" type="f" corresp="#s1:l1">weghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Rechtsinnich inden <rhyme label="b" type="m" corresp="#s1:l2">Gheest</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Den arbeyt uwer <rhyme label="c" type="f">Handen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">V wel sal connen <rhyme label="d" type="m">voen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">En sult in alle <rhyme label="c" type="f" corresp="#s1:l5">standen</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Spoedich zijn in v <rhyme label="d" type="m" corresp="#s1:l6">doen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">V Wijf sal vruchtbaer <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als Wijnstocken <rhyme label="b" type="m">vruchtbaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die dijnen Huyse <rhyme label="a" type="f" corresp="#s2:l1">aencleuen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">So sullen oock <rhyme label="b" type="m" corresp="#s2:l2">voorwaer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">V Kinderen <rhyme label="c" type="f">becliuen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In groot ghetal <rhyme label="d" type="m">ghesont</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Als planten van <rhyme label="c" type="f" corresp="#s2:l5">Oliuen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ouer v Tafel <rhyme label="d" type="m" corresp="#s2:l6">ront</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">So sal bespoedicht <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die Godt ten rechten <rhyme label="b" type="m">dient</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wt Syon God <rhyme label="a" type="f" corresp="#s3:l1">ghepresen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">V salich maken <rhyme label="b" type="m" corresp="#s3:l2">wilt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Op dat ghi ghansch v <rhyme label="c" type="f">leuen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Tvoorspoet <rhyme label="d" type="m">Hierusalem</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Aenschout, en daer <rhyme label="c" type="f" corresp="#s3:l5">beneuen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Godt prijst met hert en <rhyme label="d" type="m" corresp="#s3:l6">stem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">En sult wt uwen <rhyme label="a" type="f">sade</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dobbel gheslachte <rhyme label="b" type="m">sien</rhyme></l>
        <l id="s4:l3" met="3" type="-+">In Israhel <rhyme label="a" type="f" corresp="#s4:l1">ghenade</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ende vrede bouen <rhyme label="b" type="m" corresp="#s4:l2">dien</rhyme></l>
        <l id="s4:l5" met="3" type="-+">V si o Heer <rhyme label="c" type="f">ghepresen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Lof inder <rhyme label="d" type="m">eeuwicheyt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Gheeft dat wi moghen <rhyme label="c" type="f" corresp="#s4:l5">wesen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Tot uwen dienst <rhyme label="d" type="m" corresp="#s4:l6">bereyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
