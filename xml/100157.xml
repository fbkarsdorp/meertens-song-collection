  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy siet niet blinden <rhyme label="a" type="m">Minne-Godt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe datge myne <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Doet eene Maeght <rhyme label="b" type="f" corresp="#s1:l2">beminnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die dryft met u en my de <rhyme label="a" type="m" corresp="#s1:l1">spot</rhyme>?</l>
        <l id="s1:l5" met="6" type="+-">Ick gis, sy is, gewis, de Min, Doot <rhyme label="c" type="m">Vyandin</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Eentwee de <rhyme label="d" type="f">Daphne</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Die aen Diaen, haer Crans <rhyme label="e" type="m">voortaen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Heeft wijen <rhyme label="e" type="m" corresp="#s1:l7">gaen</rhyme>.</l>
        <l id="s1:l9" met="4" type="+-">Of gis ick mis, en is, <rhyme label="c" type="m" corresp="#s1:l5">zywis</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">niet die sy <rhyme label="c" type="m" corresp="#s1:l5">is</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Is sy niet die zy is in <rhyme label="a" type="m">schijn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Getroffen vande <rhyme label="b" type="f">Flitsen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die niet met Gulde <rhyme label="b" type="f" corresp="#s2:l2">spitsen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Maer die met loot beslaeghen <rhyme label="a" type="m" corresp="#s2:l1">zyn</rhyme>?</l>
        <l id="s2:l5" met="6" type="+-">Ick gis, sy is, gewis, de Min, Doot <rhyme label="c" type="m">Vyandin</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Eentwee de <rhyme label="d" type="f">Daphne</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">Die aen Diaen, haer Crans <rhyme label="e" type="m">voortaen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Heeft wijen <rhyme label="e" type="m" corresp="#s2:l7">gaen</rhyme>.</l>
        <l id="s2:l9" met="4" type="+-">Of gis ick mis, en is, <rhyme label="c" type="m" corresp="#s2:l5">zywis</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">niet die sy <rhyme label="c" type="m" corresp="#s2:l5">is</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick loop naer haer; sy loopt voor <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ick roep, ick, ick <rhyme label="b" type="f">vleye</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Iae klaege, suchte, <rhyme label="b" type="f" corresp="#s3:l2">schreye</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Sy lacht, om myne droefheyt <rhyme label="a" type="m" corresp="#s3:l1">bly</rhyme>,</l>
        <l id="s3:l5" met="6" type="+-">Ick gis, sy is, gewis, de Min, Doot <rhyme label="c" type="m">Vyandin</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Eentwee de <rhyme label="d" type="f">Daphne</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Die aen Diaen, haer Crans <rhyme label="e" type="m">voortaen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Heeft wijen <rhyme label="e" type="m" corresp="#s3:l7">gaen</rhyme>.</l>
        <l id="s3:l9" met="4" type="+-">Of gis ick mis, en is, <rhyme label="c" type="m" corresp="#s3:l5">zywis</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">niet die sy <rhyme label="c" type="m" corresp="#s3:l5">is</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy dwonght des al met grooten <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dwinght hare teere <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Tot vierich <rhyme label="b" type="f" corresp="#s4:l2">VVederminnen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Van ongeveinsder herten; <rhyme label="a" type="m" corresp="#s4:l1">of</rhyme></l>
        <l id="s4:l5" met="6" type="+-">Ick gis, zy is gewis, de Min Doot <rhyme label="c" type="m">Vyandin</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Een tweede <rhyme label="d" type="f">Dafne</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Die aen, Diaen haer Crans <rhyme label="e" type="m">voortaen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Heeft wijen <rhyme label="e" type="m" corresp="#s4:l7">gaen</rhyme>.</l>
        <l id="s4:l9" met="4" type="+-">En gis, ick mis, soo is, zy <rhyme label="c" type="m" corresp="#s4:l5">wis</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">niet die zy <rhyme label="c" type="m" corresp="#s4:l5">is</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
