    <tags>
      <tag standard-name="mythologisch lied" translation="mythological song">mythologisch lied</tag>
    </tags>
    <melody>De lustelijke mei is nu in de tijd</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  <author>Michiel Vlack</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ghy Musen fijn,, op dit <rhyme label="a" type="m">termijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Coomt wilt my gheselschap <rhyme label="b" type="f">houwen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">V medelijn,, en claer <rhyme label="a" type="m" corresp="#s1:l1">aenschijn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Daer op sal ick <rhyme label="b" type="f" corresp="#s1:l2">betrouwen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">V gratie ist dien ick <rhyme label="c" type="m">soeck</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Gheeft my verstant soo sal ick <rhyme label="c" type="m" corresp="#s1:l5">cloeck</rhyme></l>
        <l id="s1:l7" met="3" type="-+">V cracht daer by <rhyme label="d" type="f">ghedencken</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Een vrolijck liet oock <rhyme label="d" type="f" corresp="#s1:l7">schencken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sol wyt befaemt,, Phoebus <rhyme label="a" type="m">ghenaemt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Claer licht des Hemels <rhyme label="b" type="f">schoone</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Als ghy voort quaemt,, Soot oock <rhyme label="a" type="m" corresp="#s2:l1">betaemt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Buyten des Hemels <rhyme label="b" type="f" corresp="#s2:l2">throne</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Hebt ghy Vulcanum <rhyme label="c" type="m">openbaert</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Daer Mars en Venus <rhyme label="c" type="m" corresp="#s2:l5">onghespaert</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Sijn beddeken <rhyme label="d" type="f">besmetten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ghy woudet gaern <rhyme label="d" type="f" corresp="#s2:l7">beletten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Coemt hier zeer saen,, op dese <rhyme label="a" type="m">baen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vulcane wilt u <rhyme label="b" type="f">spoeden</rhyme></l>
        <l id="s3:l3" met="4" type="-+">En wiltse vaen,, tis haest <rhyme label="a" type="m" corresp="#s3:l1">ghedaen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Soo als ick can <rhyme label="b" type="f" corresp="#s3:l2">vermoeden</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Sy legghen nu en slapen <rhyme label="c" type="m">vast</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Brenght voort u Net, indient wel <rhyme label="c" type="m" corresp="#s3:l5">past</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Het is in u <rhyme label="d" type="f">vermooghen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Sy hebben u <rhyme label="d" type="f" corresp="#s3:l7">bedrooghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Beschaemt haer nu,, die quellen <rhyme label="a" type="m">u</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Soo sy oock voormaels <rhyme label="b" type="f">deeden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Tis my een gruw,, soo dat ick <rhyme label="a" type="m" corresp="#s4:l1">spuw</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Van alle dees <rhyme label="b" type="f" corresp="#s4:l2">boosheden</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Vulcanus heeftet haest <rhyme label="c" type="m">verstaen</rhyme></l>
        <l id="s4:l6" met="4" type="-+">En heeft ghevolcht Phoebi <rhyme label="c" type="m" corresp="#s4:l5">vermaen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Hy heeftse naeckt <rhyme label="d" type="f">ghevonden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Vast in syn Net <rhyme label="d" type="f" corresp="#s4:l7">ghebonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy Gooden hoort,, coemt rechte <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En wilt dit spel <rhyme label="b" type="f">aenmercken</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Jck ben ghestoort,, verstaet dit <rhyme label="a" type="m" corresp="#s5:l1">woort</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Of dit zijn goede <rhyme label="b" type="f" corresp="#s5:l2">wercken</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Voorwaer ick was ghereyst van <rhyme label="c" type="m">huys</rhyme></l>
        <l id="s5:l6" met="4" type="-+">En hier gheschiet sulcken <rhyme label="c" type="m" corresp="#s5:l5">abuys</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Jck macht immer niet <rhyme label="d" type="f">lyden</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Ja nimmermeer <rhyme label="d" type="f" corresp="#s5:l7">verblyden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Al watter was,, inden Hemel <rhyme label="a" type="m">ras</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Js derwaerts haest <rhyme label="b" type="f">ghekomen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Mars op dat pas,, soo groen als <rhyme label="a" type="m" corresp="#s6:l1">gras</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Venus hebbent <rhyme label="b" type="f" corresp="#s6:l2">vernomen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Elck dreef waerlijck met hun sijn <rhyme label="c" type="m">spot</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Sommich wild' wel het waer haer <rhyme label="c" type="m" corresp="#s6:l5">lot</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Al soud hy sulcx <rhyme label="d" type="f">beeruen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Hy achtet voor een <rhyme label="d" type="f" corresp="#s6:l7">sterven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Prince de Goon,, aldaer <rhyme label="a" type="m">ontboon</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hadden hier in <rhyme label="b" type="f">behaghen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">De Goddinnen vloon,, van sulck <rhyme label="a" type="m" corresp="#s7:l1">vertoon</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Soo haest als sy dat <rhyme label="b" type="f" corresp="#s7:l2">saghen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Neptunus badt Vulcanum <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Dat hy Mars, Venus, los liet <rhyme label="c" type="m" corresp="#s7:l5">gaen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Hy hoord' Neptuni <rhyme label="d" type="f">beede</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Dies vlooch elck op sijn <rhyme label="d" type="f" corresp="#s7:l7">stede</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
