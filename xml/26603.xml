  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Alsmen schreef <rhyme label="a" type="f">duysentvijfhondert</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En drie en tseuentich <rhyme label="b" type="m">iaer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Mocht elck wel zijn <rhyme label="a" type="f" corresp="#s1:l1">verwondert</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al vande Spaensche <rhyme label="b" type="m" corresp="#s1:l2">schaer</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Sy quamen wt <rhyme label="c" type="f">gestreken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Hondert twintich vaendel <rhyme label="d" type="m">sterck</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Voor Alcmaer soot is <rhyme label="c" type="f" corresp="#s1:l5">gebleken</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Als tyrannen sy gingen te <rhyme label="d" type="m" corresp="#s1:l6">werck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Naer Coedijck sy eerst <rhyme label="a" type="f">passeerden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Den Vriesche poort hebben sy <rhyme label="b" type="m">beset</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Voor den sluys men haer eerst <rhyme label="a" type="f" corresp="#s2:l1">keerden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Haer passagie was daer <rhyme label="b" type="m" corresp="#s2:l2">belet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ouer tvelt quamen sy <rhyme label="c" type="f">loopen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Als leeuwen en besten <rhyme label="d" type="m">verwoet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Met hoy en stroy by <rhyme label="c" type="f" corresp="#s2:l5">hoopen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Maecten sy een wech met <rhyme label="d" type="m" corresp="#s2:l6">spoet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Men sach doen mede <rhyme label="a" type="f">trecken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wten Nypoort veel Spangiaerts <rhyme label="b" type="m">gelijck</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sy pasten op wallen noch <rhyme label="a" type="f" corresp="#s3:l1">hecken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sy vreesden geen water noch <rhyme label="b" type="m" corresp="#s3:l2">slijck</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Sy sagen nae haer <rhyme label="c" type="f">voordel</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Den Rinmolens namen sy <rhyme label="d" type="m">in</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Alst bleek naer smenschen <rhyme label="c" type="f" corresp="#s3:l5">oordel</rhyme></l>
        <l id="s3:l8" met="3" type="-+">T was den burgers niet van haer <rhyme label="d" type="m" corresp="#s3:l6">sin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die burgers ende <rhyme label="a" type="f">soldaten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sy werden tsamen <rhyme label="b" type="m">beraen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Sy wildent daer niet by <rhyme label="a" type="f" corresp="#s4:l1">laten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Een tocht hebben sy <rhyme label="b" type="m" corresp="#s4:l2">gedaen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Te water en te <rhyme label="c" type="f">lande</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Vielen sy dapperlick <rhyme label="d" type="m">aen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En cregen die molens aen <rhyme label="c" type="f" corresp="#s4:l5">brande</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Maer een huyse dat bleef noch <rhyme label="d" type="m" corresp="#s4:l6">staen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die Spangiaers maecten een <rhyme label="a" type="f">walle</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Om thuys met aller <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Die burgers en soldaten <rhyme label="a" type="f" corresp="#s5:l1">alle</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Arbeyden oock dach en <rhyme label="b" type="m" corresp="#s5:l2">nacht</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Sy maecten den stadt <rhyme label="c" type="f">vaste</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Daert noch seer was van <rhyme label="d" type="m">noodt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Vyten Tyran sy niet en <rhyme label="c" type="f" corresp="#s5:l5">paste</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Als hy den stadt by eyschten <rhyme label="d" type="m" corresp="#s5:l6">bloot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Het was den Spangiaers groote <rhyme label="a" type="f">spijte</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat sy cregen geen <rhyme label="b" type="m">gehoor</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Sy achten den stadt niet een <rhyme label="a" type="f" corresp="#s6:l1">myte</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Eer sy daer quamen <rhyme label="b" type="m" corresp="#s6:l2">voor</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Daer zijn niet in dan <rhyme label="c" type="f">boeren</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Maecten sy malcander <rhyme label="d" type="m">wijs</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Voorlopers en meet sulcke <rhyme label="c" type="f" corresp="#s6:l5">loeren</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Sy souden wel behalen <rhyme label="d" type="m" corresp="#s6:l6">prys</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Tgrof geschut ghingen sy <rhyme label="a" type="f">stellen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Voor den Rooden tooren en Vriesche <rhyme label="b" type="m">poort</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Sy meendent al mede te <rhyme label="a" type="f" corresp="#s7:l1">vellen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En dan te bedriuen haer <rhyme label="b" type="m" corresp="#s7:l2">moort</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Maer die Meere der <rhyme label="c" type="f">heyrscharen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Wiens macht haer te bouen <rhyme label="d" type="m">gaet</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Conde den stadt wel <rhyme label="c" type="f" corresp="#s7:l5">bewaren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">En beschermen van sulcker <rhyme label="d" type="m" corresp="#s7:l6">quaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Daechs naer sinte Lambert seer <rhyme label="a" type="f">vrogen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Begonsten die Spangiaers <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Te schieten naer haer <rhyme label="a" type="f" corresp="#s8:l1">vermogen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Met twintich grof stucken <rhyme label="b" type="m" corresp="#s8:l2">swaer</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Ontrent twee duysent <rhyme label="c" type="f">schoten</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Seer veerdich met groot <rhyme label="d" type="m">geschal</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Sy spaerden truyt noch <rhyme label="c" type="f" corresp="#s8:l5">cloten</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Om te breken muer en <rhyme label="d" type="m" corresp="#s8:l6">wal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">T geuiel anders dan sy <rhyme label="a" type="f">waende</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Al viel den muer daer <rhyme label="b" type="m">neer</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Die wal die bleef noch <rhyme label="a" type="f" corresp="#s9:l1">staende</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Met alle den borst <rhyme label="b" type="m" corresp="#s9:l2">geweer</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Die burgers ghingent <rhyme label="c" type="f">maken</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Metten knechten en menich <rhyme label="d" type="m">vrou</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Veel eerder dan syt <rhyme label="c" type="f" corresp="#s9:l5">braken</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Die Spangiaers cregen een groote <rhyme label="d" type="m" corresp="#s9:l6">iou</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Naer Middach ten drie <rhyme label="a" type="f">uren</rhyme></l>
        <l id="s10:l2" met="3" type="-+">In haer oorden stonden sy <rhyme label="b" type="m">radt</rhyme></l>
        <l id="s10:l3" met="3" type="-+">En quamen al totten <rhyme label="a" type="f" corresp="#s10:l1">mueren</rhyme></l>
        <l id="s10:l4" met="3" type="-+">om te bestormen den <rhyme label="b" type="m" corresp="#s10:l2">stadt</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Het ghinck doen op een <rhyme label="c" type="f">vechten</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Aen twee plaetss tseffens <rhyme label="d" type="m">ghelijck</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Mannen/ vrouwen en <rhyme label="c" type="f" corresp="#s10:l5">lantsknechten</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Sy weerden haer vroom arm ende <rhyme label="d" type="m" corresp="#s10:l6">rijck</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">By de Vriesche poort en Roo <rhyme label="a" type="f">tooren</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Wast dat sy stormden dus <rhyme label="b" type="m">strang</rhyme></l>
        <l id="s11:l3" met="3" type="-+">En geduerde wel wilt <rhyme label="a" type="f" corresp="#s11:l1">hooren</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Stijf vier half uren <rhyme label="b" type="m" corresp="#s11:l2">lang</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Tot drie mael toe sy <rhyme label="c" type="f">leyden</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Versch volck aen en <rhyme label="d" type="m">aff</rhyme></l>
        <l id="s11:l7" met="3" type="-+">En schoten noch sonder <rhyme label="c" type="f" corresp="#s11:l5">verbeyden</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Maet haer grof schut euen <rhyme label="d" type="m" corresp="#s11:l6">straf</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Dit mocht haer noch al niet <rhyme label="a" type="f">baten</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Sy moesten af wijcken met <rhyme label="b" type="m">schandt</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Veel volcx hebben sy <rhyme label="a" type="f" corresp="#s12:l1">ghelaten</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Capiteyns Vaendragers <rhyme label="b" type="m" corresp="#s12:l2">menigerhandt</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Die daer sochten den <rhyme label="c" type="f">voorboyten</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Cregen betaling <rhyme label="d" type="m">ongespaert</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Don Frederick sachte wel <rhyme label="c" type="f" corresp="#s12:l5">stoyten</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Vanden wal af <rhyme label="d" type="m" corresp="#s12:l6">nederwaert</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Noch quamen die Spangiaers <rhyme label="a" type="f">vermeten</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Doen den storm opt stercste <rhyme label="b" type="m">was</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Met schuyten naer den <rhyme label="a" type="f" corresp="#s13:l1">soutketen</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Om die toueruallen <rhyme label="b" type="m" corresp="#s13:l2">ras</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Maer sy cregen sulcke <rhyme label="c" type="f">pruymen</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Men schot ender inne <rhyme label="d" type="m">terstont</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Haer schuyten moesten sy <rhyme label="c" type="f" corresp="#s13:l5">ruymen</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Twee raecten daer inden <rhyme label="d" type="m" corresp="#s13:l6">gront</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Vyten Geeft sy mede <rhyme label="a" type="f">stonden</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Ten storme wel toe <rhyme label="b" type="m">gerust</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Aende brug hoort mijn <rhyme label="a" type="f" corresp="#s14:l1">bermonden</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Is al haer moet <rhyme label="b" type="m" corresp="#s14:l2">geblust</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Wie daer by quam moest <rhyme label="c" type="f">blijuen</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Soo wel waren sy <rhyme label="d" type="m">onthaelt</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Sy en conden niet <rhyme label="c" type="f" corresp="#s14:l5">bedrijuen</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Al haren anslach is <rhyme label="d" type="m" corresp="#s14:l6">gefaelt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Daer nae t'was <rhyme label="a" type="f">Saterdaghe</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Doen hielden sy haer so <rhyme label="b" type="m">stil</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Sy deden tocht noch <rhyme label="a" type="f" corresp="#s15:l1">aenslaghe</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Sy maecten oock gheen <rhyme label="b" type="m" corresp="#s15:l2">geschil</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Men condt anders niet <rhyme label="c" type="f">gissen</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Oft sy laten al den <rhyme label="d" type="m">dach</rhyme></l>
        <l id="s15:l7" met="3" type="-+">Vigelien ofte <rhyme label="c" type="f" corresp="#s15:l5">Sielmissen</rhyme></l>
        <l id="s15:l8" met="3" type="-+">Voor den gebleuenen inden <rhyme label="d" type="m" corresp="#s15:l6">slach</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Die Spangiaerts ouer <rhyme label="a" type="f">traden</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Den Sondach met boos <rhyme label="b" type="m">bestier</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Om te verhalen haer <rhyme label="a" type="f" corresp="#s16:l1">schaden</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Acht hondert mael sy gaven <rhyme label="b" type="m" corresp="#s16:l2">vyer</rhyme></l>
        <l id="s16:l5" met="3" type="-+">En quamen ooc ten <rhyme label="c" type="f">percke</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Om weder te stormen <rhyme label="d" type="m">fel</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Den brug quam niet te <rhyme label="c" type="f" corresp="#s16:l5">wercke</rhyme></l>
        <l id="s16:l8" met="3" type="-+">Sy werden geschoten heel <rhyme label="d" type="m" corresp="#s16:l6">rebel</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Een Capiteyn quam aen <rhyme label="a" type="f">vlieden</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Om den brug te dwingen <rhyme label="b" type="m">voort</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Hy smeet die arme <rhyme label="a" type="f" corresp="#s17:l1">lieden</rhyme></l>
        <l id="s17:l4" met="3" type="-+">En doorstackse heel <rhyme label="b" type="m" corresp="#s17:l2">verstoort</rhyme></l>
        <l id="s17:l5" met="3" type="-+">Het duerde niet seer <rhyme label="c" type="f">langhe</rhyme></l>
        <l id="s17:l6" met="3" type="-+">Syn leuen nam oock een <rhyme label="d" type="m">endt</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Sy mochten al wel zijn <rhyme label="c" type="f" corresp="#s17:l5">bange</rhyme></l>
        <l id="s17:l8" met="3" type="-+">Die den brugge quamen <rhyme label="d" type="m" corresp="#s17:l6">ontrent</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Die burgers toonde <rhyme label="a" type="f">couragie</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Die knechten waren wel <rhyme label="b" type="m">gemoet</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Die vrouwen met haer <rhyme label="a" type="f" corresp="#s18:l1">coockeragie</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Verwachten haer aencoomste met <rhyme label="b" type="m" corresp="#s18:l2">spoet</rhyme></l>
        <l id="s18:l5" met="3" type="-+">Die meyskens ende <rhyme label="c" type="f">knapen</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Het was al aende <rhyme label="d" type="m">vest</rhyme></l>
        <l id="s18:l7" met="3" type="-+">Wel voorzien met S.Stevens <rhyme label="c" type="f" corresp="#s18:l5">wapens</rhyme></l>
        <l id="s18:l8" met="3" type="-+">Elck wilde doen syn <rhyme label="d" type="m" corresp="#s18:l6">best</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Sy en dorsten niet aen <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Theeft haer niet wel <rhyme label="b" type="m">behaecht</rhyme></l>
        <l id="s19:l3" met="3" type="-+">Sy sagen den burgers <rhyme label="a" type="f" corresp="#s19:l1">vromen</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Sy cregen herten <rhyme label="b" type="m" corresp="#s19:l2">versaecht</rhyme></l>
        <l id="s19:l5" met="3" type="-+">Die windt die haer was <rhyme label="c" type="f">mede</rhyme></l>
        <l id="s19:l6" met="3" type="-+">Die heeft hem <rhyme label="d" type="m">omgedrayt</rhyme></l>
        <l id="s19:l7" met="3" type="-+">Gods hande was metten <rhyme label="c" type="f" corresp="#s19:l5">stede</rhyme></l>
        <l id="s19:l8" met="3" type="-+">Syn lof moet zijn <rhyme label="d" type="m" corresp="#s19:l6">verbreyt</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Hoe souden wy u niet <rhyme label="a" type="f">louen</rhyme></l>
        <l id="s20:l2" met="3" type="-+">O alder heeren <rhyme label="b" type="m">heer</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Aen ons die waren <rhyme label="a" type="f" corresp="#s20:l1">verschouen</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Hebt ghy verbreyt u <rhyme label="b" type="m" corresp="#s20:l2">eer</rhyme></l>
        <l id="s20:l5" met="3" type="-+">ghy hebt willen <rhyme label="c" type="f">behouwen</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Die crachteloos waren <rhyme label="d" type="m">geacht</rhyme></l>
        <l id="s20:l7" met="3" type="-+">En die op haer selfs <rhyme label="c" type="f" corresp="#s20:l5">bouwen</rhyme></l>
        <l id="s20:l8" met="3" type="-+">Zijn geuallen met al haer <rhyme label="d" type="m" corresp="#s20:l6">pracht</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Lof Princelijcke <rhyme label="a" type="f">vader</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Lof Heere <rhyme label="b" type="m">gebenedijt</rhyme></l>
        <l id="s21:l3" met="3" type="-+">Wy dancken u alle <rhyme label="a" type="f" corresp="#s21:l1">gader</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Dat ghy ons hebt <rhyme label="b" type="m" corresp="#s21:l2">bevrijt</rhyme></l>
        <l id="s21:l5" met="3" type="-+">Als wy waren seven <rhyme label="c" type="f">weken</rhyme></l>
        <l id="s21:l6" met="3" type="-+">Omringt met groot <rhyme label="d" type="m">gewelt</rhyme></l>
        <l id="s21:l7" met="3" type="-+">ghy hebt ons niet <rhyme label="c" type="f" corresp="#s21:l5">besweecken</rhyme></l>
        <l id="s21:l8" met="3" type="-+">Die viant moest ruymen <rhyme label="d" type="m" corresp="#s21:l6">tvelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
