<song>
  <meta>
    <id>21053</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Tien geboden</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GRoote God, heerscher der <rhyme label="a" type="f">Heyrscharen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die 't alles weet en oock <rhyme label="b" type="m">door-siet</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Wonderlijck mach nieu u <rhyme label="a" type="f" corresp="#s1:l1">verklaren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Want wat ghy wilt seer haest <rhyme label="b" type="m" corresp="#s1:l2">geschiet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het houwelijck een staet van <rhyme label="a" type="f">waerden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Gebiedt ghy Heer na wil en <rhyme label="b" type="m">wensch</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Om te vermeren op der <rhyme label="a" type="f" corresp="#s2:l1">aerden</rhyme></l>
        <l id="s2:l4" met="4" type="-+">V even-beelt, 't werck is den <rhyme label="b" type="m" corresp="#s2:l2">mensch</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dewijl dat ghy dan Heere <rhyme label="a" type="f">goedigh</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Een Autheur zijt van desen <rhyme label="b" type="m">staet</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Soo bidden wy u seer <rhyme label="a" type="f" corresp="#s3:l1">ootmoedigh</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Diese aengrijpt doch niet <rhyme label="b" type="m" corresp="#s3:l2">verlaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Met namen Heer wort u <rhyme label="a" type="f">gebeden</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Voor onsen Bruygom lief en <rhyme label="b" type="m">waerdt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Laet uwen Engel met hem <rhyme label="a" type="f" corresp="#s4:l1">treden</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Op dat hy dees staet recht <rhyme label="b" type="m" corresp="#s4:l2">aenvaerdt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wilt oock de Bruydt gedachtigh <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Stiert al haer doen na u <rhyme label="b" type="m">beleydt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Op dat dit Houwelijck, mits <rhyme label="a" type="f" corresp="#s5:l1">desen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Hun t' saem gedy ter <rhyme label="b" type="m" corresp="#s5:l2">zalicheydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Soo 't u belieft, o Heer en <rhyme label="a" type="f">Vader</rhyme>!</l>
        <l id="s6:l2" met="4" type="-+">Laet liefd' en vreed' doch zijn <rhyme label="b" type="m">present</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">En geeft hun oock kind 'ren te <rhyme label="a" type="f" corresp="#s6:l1">gader</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Daer uwen naem door wordt <rhyme label="b" type="m" corresp="#s6:l2">bekent</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Al wat zy Prins ter handen <rhyme label="a" type="f">trecken</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Laet dat van u gezegent <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Alst Evangely gaet <rhyme label="a" type="f" corresp="#s7:l1">ontdecken</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Daer ghy van Water maecktet <rhyme label="b" type="m" corresp="#s7:l2">Wijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
