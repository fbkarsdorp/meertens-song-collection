  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">TOrtel-duyfke myn <rhyme label="a" type="f">beminde</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Die van alle kant <rhyme label="b" type="m">gejaeght</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Vlucht als een' <rhyme label="a" type="f" corresp="#s1:l1">Hinde</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Doch soo ick dickwils <rhyme label="a" type="f" corresp="#s1:l1">vinde</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">U herte <rhyme label="b" type="m" corresp="#s1:l2">klaeght</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Oeck wel gheheele <rhyme label="c" type="f">nachten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ey matight uwe <rhyme label="c" type="f" corresp="#s1:l6">klachten</rhyme>,</l>
        <l id="s1:l8" met="5" type="-+">Dat u gheen druck oft wanhop' en <rhyme label="a" type="f" corresp="#s1:l1">verslinde</rhyme>;</l>
        <l id="s1:l9" met="2" type="-+"><rhyme label="d" type="f">Tortel-duyfke</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+"><rhyme label="d" type="f" corresp="#s1:l9">Tortel-duyfke</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Tortel-duyfke myn <rhyme label="a" type="f" corresp="#s1:l1">Beminde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Eenigh Trooster aller <rhyme label="a" type="f">Zielen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Doot, en hel met al haer <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Zyn op myn' <rhyme label="a" type="f" corresp="#s2:l1">hielen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En willen my <rhyme label="a" type="f" corresp="#s2:l1">vernielen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Hebt toch eens <rhyme label="b" type="m" corresp="#s2:l2">acht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Op al myn droevigh <rhyme label="c" type="f">kermen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En wilt my doch <rhyme label="c" type="f" corresp="#s2:l6">beschermen</rhyme></l>
        <l id="s2:l8" met="5" type="-+">Van al die om myn' teere leden <rhyme label="a" type="f" corresp="#s2:l1">krielen</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Eenig <rhyme label="d" type="f">Trooster</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Eenig <rhyme label="d" type="f" corresp="#s2:l9">Trooster</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Eenig Trooster aller <rhyme label="a" type="f" corresp="#s2:l1">Zielen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dochter Sions myns <rhyme label="a" type="f">verkoren</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Druck en pyn en swerte <rhyme label="b" type="m">nyt</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Die u <rhyme label="a" type="f" corresp="#s3:l1">verstôren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zyn voor u nutte <rhyme label="a" type="f" corresp="#s3:l1">sporen</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Hier in den <rhyme label="b" type="m" corresp="#s3:l2">tydt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Maer wacht hier - naermaels <rhyme label="c" type="f">zeghen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Want die is u <rhyme label="c" type="f" corresp="#s3:l6">verkreghen</rhyme>,</l>
        <l id="s3:l8" met="5" type="-+">Die ick oyt koos, en gingen no <rhyme label="a" type="f" corresp="#s3:l1">verloren</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Dochter <rhyme label="d" type="f">Sions</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Dochter <rhyme label="d" type="f" corresp="#s3:l9">Sions</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Dochter Sions <rhyme label="a" type="f" corresp="#s3:l1">uyt-verkoren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Opper-hoeder uwer <rhyme label="a" type="f">Kercken</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Ach! het vleesch is al te <rhyme label="b" type="m">blint</rhyme></l>
        <l id="s4:l3" met="2" type="-+">'t En kan niet <rhyme label="a" type="f" corresp="#s4:l1">mercken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Uw' wonderbare <rhyme label="a" type="f" corresp="#s4:l1">wercken</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">'t Is <rhyme label="b" type="m" corresp="#s4:l2">onbesint</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En doet my dickmaels <rhyme label="c" type="f">dwalet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Gansch buyten uwe <rhyme label="c" type="f" corresp="#s4:l6">palen</rhyme>,</l>
        <l id="s4:l8" met="5" type="-+">Ghy daer-om wilt myn' swacke Ziel' <rhyme label="a" type="f" corresp="#s4:l1">verstercken</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+"><rhyme label="d" type="f">Opper-hoeder</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+"><rhyme label="d" type="f" corresp="#s4:l9">Opper-hoeder</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Opper-hoeder uwer <rhyme label="a" type="f" corresp="#s4:l1">Kercken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sunamite myn' <rhyme label="a" type="f">Vrindinne</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Al maeckt het u lyden <rhyme label="b" type="m">droef</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Weest koel van <rhyme label="a" type="f" corresp="#s5:l1">sinne</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">'t Is uyt oprechte <rhyme label="a" type="f" corresp="#s5:l1">minne</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Dat ick u <rhyme label="b" type="m" corresp="#s5:l2">proef</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">Gheen klachten van-der <rhyme label="c" type="f">hellen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Gheen doodt en kan u <rhyme label="c" type="f" corresp="#s5:l6">quellen</rhyme>,</l>
        <l id="s5:l8" met="5" type="-+">Ghy zyt myn erf oock van den <rhyme label="a" type="f" corresp="#s5:l1">aen-beginne</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+"><rhyme label="d" type="f">Tortel-duyfken</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">Dochter <rhyme label="d" type="f" corresp="#s5:l9">Sions</rhyme></l>
        <l id="s5:l11" met="4" type="+-">Sunamite myn' <rhyme label="a" type="f" corresp="#s5:l1">Vrindinne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ziel-verlosser myn <rhyme label="a" type="f">verlangen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ick ben langh de Wereldt <rhyme label="b" type="m">sat</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">De tranen <rhyme label="a" type="f" corresp="#s6:l1">hanghen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Staegh op myn' droeve <rhyme label="a" type="f" corresp="#s6:l1">wanghen</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Ach siltigh <rhyme label="b" type="m" corresp="#s6:l2">nat</rhyme>!</l>
        <l id="s6:l6" met="3" type="-+">Wanneêr sal 't toch eens <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat ick van druck <rhyme label="c" type="f" corresp="#s6:l6">ghenesen</rhyme></l>
        <l id="s6:l8" met="5" type="-+">Myn hooghste goedt u Jesu sal <rhyme label="a" type="f" corresp="#s6:l1">ontfanghen</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Eenigh <rhyme label="d" type="f">Trooster</rhyme></l>
        <l id="s6:l10" met="2" type="-+"><rhyme label="d" type="f" corresp="#s6:l9">Opper-hoeder</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Ziel-verlosser myn <rhyme label="a" type="f" corresp="#s6:l1">verlanghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
