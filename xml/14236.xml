    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IN vruechden is alle de werelt <rhyme label="a" type="m">wijt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die enghelen brenghen ons groot <rhyme label="a" type="m" corresp="#s1:l1">iolijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">met vruechden hier <rhyme label="b" type="f">beneden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">¶ Want aen maghet <rhyme label="a" type="m">ghebenedijt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">so is volcomen nv tet ter <rhyme label="a" type="m" corresp="#s2:l1">tijt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">onser <rhyme label="b" type="f">salicheden</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">¶ Natuer is bouen den loop <rhyme label="a" type="m">ghegaen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">een suuer maecht die heeft <rhyme label="a" type="m" corresp="#s3:l1">ontfaen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">een kint in <rhyme label="b" type="f">aertrijcke</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">¶ Seer cortelic ist al <rhyme label="a" type="m">voldaen</rhyme>.</l>
        <l id="s4:l2" met="4" type="-+">dat ysayas dede <rhyme label="a" type="m" corresp="#s4:l1">verstaen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">dwelc was seer <rhyme label="b" type="f">wonderlike</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">¶ Dat licht der sonnen is ons <rhyme label="a" type="m">geboren</rhyme>.</l>
        <l id="s5:l2" met="4" type="-+">die moeder is maecht ende was te <rhyme label="a" type="m" corresp="#s5:l1">voren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ende bleef in <rhyme label="b" type="f">reynicheden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Inder scriftueren so machmen <rhyme label="a" type="m">horen</rhyme>.</l>
        <l id="s6:l2" met="4" type="-+">hoe dat bi adam was <rhyme label="a" type="m" corresp="#s6:l1">verloren</rhyme></l>
        <l id="s6:l3" met="3" type="-+">tgeslacht der <rhyme label="b" type="f">menschelicheden</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">¶ Kinder nv laet ons blide <rhyme label="a" type="m">sijn</rhyme>.</l>
        <l id="s7:l2" met="4" type="-+">ende louen dat soete <rhyme label="a" type="m" corresp="#s7:l1">kindekijn</rhyme></l>
        <l id="s7:l3" met="3" type="-+">gheboren <rhyme label="b" type="f">menschelijke</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">¶ Want hi is comen als ic <rhyme label="a" type="m">beuroede</rhyme></l>
        <l id="s8:l2" met="4" type="-+">pm te verlossen met sinen <rhyme label="a" type="m" corresp="#s8:l1">bloede</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">ons allen <rhyme label="b" type="f">ghemeenlijke</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">¶ Ihesus so is die name <rhyme label="a" type="m">sijn</rhyme>.</l>
        <l id="s9:l2" met="4" type="-+">hi weende als ander <rhyme label="a" type="m" corresp="#s9:l1">kindekijn</rhyme></l>
        <l id="s9:l3" met="3" type="-+">na sijnder <rhyme label="b" type="f">menschelicheden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">¶ Twee stomme beesten bekenden <rhyme label="a" type="m">wel</rhyme>.</l>
        <l id="s10:l2" met="4" type="-+">al haren scepper en niemant <rhyme label="a" type="m" corresp="#s10:l1">el</rhyme></l>
        <l id="s10:l3" met="3" type="-+">bi sijnder <rhyme label="b" type="f">godlijcheden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">¶ Een cribbeken stont daer <rhyme label="a" type="m">ghemaect</rhyme></l>
        <l id="s11:l2" met="4" type="-+">daer wert hi in gheleyt al <rhyme label="a" type="m" corresp="#s11:l1">naect</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">die heere van <rhyme label="b" type="f">hemelrijcke</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">¶ Drie coningen quamen wt verre <rhyme label="a" type="m">lande</rhyme></l>
        <l id="s12:l2" met="4" type="-+">ende si brochten daer <rhyme label="a" type="m" corresp="#s12:l1">offerhande</rhyme></l>
        <l id="s12:l3" met="3" type="-+">seer blide ende <rhyme label="b" type="f">oetmoedelijke</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">¶ Nv laet ons bidden dat <rhyme label="a" type="m">kindekijn</rhyme></l>
        <l id="s13:l2" met="4" type="-+">met maria die maghet <rhyme label="a" type="m" corresp="#s13:l1">fijn</rhyme></l>
        <l id="s13:l3" met="3" type="-+">dat si ons wil <rhyme label="b" type="f">ontfermen</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">¶ Als wi hier niet langer en mogen <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s14:l2" met="4" type="-+">dat si ons dan van die eewighe <rhyme label="a" type="m" corresp="#s14:l1">pijn</rhyme>.</l>
        <l id="s14:l3" met="3" type="-+">wil behoeden ende <rhyme label="b" type="f">beschermen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
