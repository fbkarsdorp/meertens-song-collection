  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'Ave Maria, maghet <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gi suverlicker sijt <rhyme label="a" type="m" corresp="#s1:l1">alleyn</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Der werrelts cierheit al <rhyme label="a" type="m" corresp="#s1:l1">ghemeyn</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Ontsprekelic u <rhyme label="b" type="m">suverheyt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Gaet opter meyen <rhyme label="b" type="m" corresp="#s1:l4">daghereyt</rhyme></l>
        <l id="s1:l6" met="4" type="-+">End al dat in haer is <rhyme label="b" type="m" corresp="#s1:l4">bereyt</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">In minen <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Staet uwe <rhyme label="c" type="m" corresp="#s1:l7">myn</rhyme>.</l>
        <l id="s1:l9" met="2" type="-+">O suver <rhyme label="d" type="m">maecht</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Mijn hert, mijn <rhyme label="c" type="m" corresp="#s1:l7">syn</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">So waer ic <rhyme label="c" type="m" corresp="#s1:l7">byn</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Gi wel <rhyme label="d" type="m" corresp="#s1:l9">behaecht</rhyme></l>
        <l id="s1:l13" met="4" type="-+">Van u te sijn mijn ziel <rhyme label="d" type="m" corresp="#s1:l9">beclaecht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Verlanghen heeft my <rhyme label="a" type="m">ombevaen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Als ic aensie ster ende <rhyme label="a" type="m" corresp="#s2:l1">maen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">End voert denck hoet mit u mach <rhyme label="a" type="m" corresp="#s2:l1">staen</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Gi sijt die edel suver <rhyme label="b" type="m">bloem</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Daer uut ghesproten is Goots <rhyme label="b" type="m" corresp="#s2:l4">soen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Jhesus heer van den hoghen <rhyme label="b" type="m" corresp="#s2:l4">throen</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Al <rhyme label="c" type="m">hemmelrijc</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Hem zeer <rhyme label="c" type="m" corresp="#s2:l7">verblijt</rhyme></l>
        <l id="s2:l9" met="2" type="-+">In u <rhyme label="d" type="m">claerheyt</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Want gi die <rhyme label="c" type="m" corresp="#s2:l7">sijt</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Die men <rhyme label="c" type="m" corresp="#s2:l7">ghelijct</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Der <rhyme label="d" type="m" corresp="#s2:l9">daghereyt</rhyme></l>
        <l id="s2:l13" met="4" type="-+">Mit al der voghelen sang <rhyme label="d" type="m" corresp="#s2:l9">bereyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ontsprekelic, o edel <rhyme label="a" type="m">bruyt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ju suete roec, u suet <rhyme label="a" type="m" corresp="#s3:l1">gheluyt</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Gaet boven alle sang end <rhyme label="a" type="m" corresp="#s3:l1">cruyt</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Hi mach wel draghen hoghe <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Die u kijst voer dat eertsche <rhyme label="b" type="m" corresp="#s3:l4">goet</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Ende altijt uwe wille <rhyme label="b" type="m" corresp="#s3:l4">doet</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">Want hi <rhyme label="c" type="m">ontfaen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Sel sonder <rhyme label="c" type="m" corresp="#s3:l7">waen</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Een brugoms <rhyme label="d" type="m">croen</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Die nijt <rhyme label="c" type="m" corresp="#s3:l7">vergaen</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Mer als die <rhyme label="c" type="m" corresp="#s3:l7">maen</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Sel rayen <rhyme label="d" type="m" corresp="#s3:l9">scoen</rhyme>,</l>
        <l id="s3:l13" met="4" type="-+">Die gi hem edel bruyt selt <rhyme label="d" type="m" corresp="#s3:l9">doen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat sel ic van u spreken <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Voer allen maechden heb di <rhyme label="a" type="m" corresp="#s4:l1">eer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Van Jhesu, onsen lieve <rhyme label="a" type="m" corresp="#s4:l1">heer</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Gi sijt sijn moeder, hi u <rhyme label="b" type="m">kijnt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Die u volcomelicke <rhyme label="b" type="m" corresp="#s4:l4">mynt</rhyme>.</l>
        <l id="s4:l6" met="4" type="-+">Al u begheert gi aen hem <rhyme label="b" type="m" corresp="#s4:l4">vijnt</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">Maria <rhyme label="c" type="m">vrou</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Hier om hi <rhyme label="c" type="m" corresp="#s4:l7">sou</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Die u ter <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Mocht sijn <rhyme label="c" type="m" corresp="#s4:l7">ghetrou</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Van allen <rhyme label="c" type="m" corresp="#s4:l7">rou</rhyme></l>
        <l id="s4:l12" met="2" type="-+">Werden <rhyme label="d" type="m" corresp="#s4:l9">verblijt</rhyme></l>
        <l id="s4:l13" met="4" type="-+">Hier na mit u in <rhyme label="d" type="m" corresp="#s4:l9">hemmelrijc</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O suver maecht, verleent doch <rhyme label="a" type="m">my</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Dat ic u hier ghetrouwe <rhyme label="a" type="m" corresp="#s5:l1">sy</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Bi wien ic bin of wie bi <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Wat ic aenscou, tis my al <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Hoe scoen tmach sijn in dit <rhyme label="b" type="m" corresp="#s5:l4">verdriet</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Als ic denck dat gi scoenre <rhyme label="b" type="m" corresp="#s5:l4">sijt</rhyme>.</l>
        <l id="s5:l7" met="2" type="-+">O lieve <rhyme label="c" type="m">macht</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Oft u <rhyme label="c" type="m" corresp="#s5:l7">behaecht</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Laet my doch <rhyme label="d" type="m">sijn</rhyme></l>
        <l id="s5:l10" met="2" type="-+">By u <rhyme label="c" type="m" corresp="#s5:l7">ghedaecht</rhyme>;</l>
        <l id="s5:l11" met="2" type="+-">End nijt <rhyme label="c" type="m" corresp="#s5:l7">beclaecht</rhyme></l>
        <l id="s5:l12" met="2" type="-+">Mer fris en <rhyme label="d" type="m" corresp="#s5:l9">fijn</rhyme></l>
        <l id="s5:l13" met="4" type="-+">Als een brugom in u <rhyme label="d" type="m" corresp="#s5:l9">aenschijn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O coninghin, maect dat <rhyme label="a" type="m">bereyt</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Mijn ziel, als sy van hene <rhyme label="a" type="m" corresp="#s6:l1">scheyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Moet sijn in hemmelrijc <rhyme label="a" type="m" corresp="#s6:l1">gheleyt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Daer alle zielen spannen <rhyme label="b" type="m">croen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">End blidelicken singhen <rhyme label="b" type="m" corresp="#s6:l4">scoen</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Mit Jhesu uwen lieven <rhyme label="b" type="m" corresp="#s6:l4">soen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Diet wel <rhyme label="c" type="m">besiet</rhyme>.</l>
        <l id="s6:l8" met="2" type="-+">Tis groot <rhyme label="c" type="m" corresp="#s6:l7">verdriet</rhyme></l>
        <l id="s6:l9" met="2" type="-+">Van daen te <rhyme label="d" type="m">sijn</rhyme></l>
        <l id="s6:l10" met="2" type="-+">Daer vruechd <rhyme label="c" type="m" corresp="#s6:l7">ghesciet</rhyme></l>
        <l id="s6:l11" met="2" type="+-">End droefheyt <rhyme label="c" type="m" corresp="#s6:l7">vliet</rhyme></l>
        <l id="s6:l12" met="2" type="-+">Mit alle <rhyme label="d" type="m" corresp="#s6:l9">pijn</rhyme>.</l>
        <l id="s6:l13" met="4" type="-+">O maecht, helpt my daer in die <rhyme label="d" type="m" corresp="#s6:l9">schijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O vrou, wat sel ic aene <rhyme label="a" type="m">gaen</rhyme>?</l>
        <l id="s7:l2" met="4" type="-+">Mijn sonden doen my dic <rhyme label="a" type="m" corresp="#s7:l1">verstaen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Dat mijn ghebet wort nijt <rhyme label="a" type="m" corresp="#s7:l1">ontfaen</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">O moeder der <rhyme label="b" type="m">ontfermherticheyt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Al daer die sonder troest op <rhyme label="b" type="m" corresp="#s7:l4">heyt</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Troest my, mijn sonden sijn mijn <rhyme label="b" type="m" corresp="#s7:l4">leyt</rhyme>.</l>
        <l id="s7:l7" met="2" type="-+">O lieve <rhyme label="c" type="m">maecht</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Ic word <rhyme label="c" type="m" corresp="#s7:l7">versaecht</rhyme>.</l>
        <l id="s7:l9" met="2" type="-+">Och wat <rhyme label="d" type="m">bediet</rhyme>?</l>
        <l id="s7:l10" met="2" type="-+">Mijn hert dat <rhyme label="c" type="m" corresp="#s7:l7">claacht</rhyme></l>
        <l id="s7:l11" met="2" type="+-">Van snachs tot <rhyme label="c" type="m" corresp="#s7:l7">daechs</rhyme>,</l>
        <l id="s7:l12" met="2" type="-+">Gi antwert <rhyme label="d" type="m" corresp="#s7:l9">niet</rhyme></l>
        <l id="s7:l13" met="4" type="-+">En weet hoet mit my is <rhyme label="d" type="m" corresp="#s7:l9">ghesciet</rhyme>.'</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">'Die saack sel ic ju segghen <rhyme label="a" type="m">kijnt</rhyme>:</l>
        <l id="s8:l2" met="4" type="-+">Seer weynich heb di my <rhyme label="a" type="m" corresp="#s8:l1">ghemynt</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Want ydel ghedacht u hert <rhyme label="a" type="m" corresp="#s8:l1">verblint</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Ic bin so edel, scoen end <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Wie dat my mynt daer wil ic <rhyme label="b" type="m" corresp="#s8:l4">sijn</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Alleen mit mijn cleyn <rhyme label="b" type="m" corresp="#s8:l4">kijndekijn</rhyme>.</l>
        <l id="s8:l7" met="2" type="-+">Heb my te <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s8:l8" met="2" type="-+">End mijn kijnts <rhyme label="c" type="m" corresp="#s8:l7">myn</rhyme></l>
        <l id="s8:l9" met="2" type="-+">So sel u <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s8:l10" met="2" type="-+">Ghescien van <rhyme label="c" type="m" corresp="#s8:l7">hym</rhyme></l>
        <l id="s8:l11" met="2" type="+-">End daer ic <rhyme label="c" type="m" corresp="#s8:l7">bin</rhyme></l>
        <l id="s8:l12" met="2" type="-+">Ist dat gijt <rhyme label="d" type="m" corresp="#s8:l9">doet</rhyme></l>
        <l id="s8:l13" met="4" type="-+">Suldi hiernamels draghen <rhyme label="d" type="m" corresp="#s8:l9">moet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wilt alle yelheyt van u <rhyme label="a" type="m">sclaen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Ghedencket mijns end mijns kijnts <rhyme label="a" type="m" corresp="#s9:l1">naem</rhyme></l>
        <l id="s9:l3" met="4" type="-+">So sel ic u dic spreken <rhyme label="a" type="m" corresp="#s9:l1">aen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">End draecht sijn liden in ju <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Dat hi leed end die grote <rhyme label="b" type="m" corresp="#s9:l4">smert</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Doe hi om u ghecruset <rhyme label="b" type="m" corresp="#s9:l4">wert</rhyme>.</l>
        <l id="s9:l7" met="2" type="-+">Om u leet <rhyme label="c" type="m">hi</rhyme></l>
        <l id="s9:l8" met="2" type="-+">Bi sinen <rhyme label="c" type="m" corresp="#s9:l7">ty</rhyme></l>
        <l id="s9:l9" met="2" type="-+">So groot <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s9:l10" met="2" type="-+">Lijdt gi nu <rhyme label="c" type="m" corresp="#s9:l7">vry</rhyme></l>
        <l id="s9:l11" met="2" type="+-">Alleen om <rhyme label="c" type="m" corresp="#s9:l7">di</rhyme></l>
        <l id="s9:l12" met="2" type="-+">Dat hi <rhyme label="d" type="m" corresp="#s9:l9">ghebiet</rhyme>.</l>
        <l id="s9:l13" met="4" type="-+">Hier na sel u wel sijn <rhyme label="d" type="m" corresp="#s9:l9">ghesciet</rhyme>.'</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">'Maria maecht, ic uwen <rhyme label="a" type="m">raet</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Mit ganselicker trouwen <rhyme label="a" type="m" corresp="#s10:l1">vaet</rhyme></l>
        <l id="s10:l3" met="4" type="-+">End wil ontsegghen alle <rhyme label="a" type="m" corresp="#s10:l1">quaet</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Gaet wech van my, o <rhyme label="b" type="m">ydelheyt</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Maria heeft my waer <rhyme label="b" type="m" corresp="#s10:l4">gheseyt</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Want gi mijn hert dicwijl <rhyme label="b" type="m" corresp="#s10:l4">verleyt</rhyme>.</l>
        <l id="s10:l7" met="2" type="-+">So mach die <rhyme label="c" type="m">bloem</rhyme></l>
        <l id="s10:l8" met="2" type="-+">Mit haren <rhyme label="c" type="m" corresp="#s10:l7">soen</rhyme></l>
        <l id="s10:l9" met="2" type="-+">My spreken <rhyme label="d" type="m">aen</rhyme></l>
        <l id="s10:l10" met="2" type="-+">End gheven <rhyme label="c" type="m" corresp="#s10:l7">loen</rhyme>,</l>
        <l id="s10:l11" met="2" type="+-">Een suver <rhyme label="c" type="m" corresp="#s10:l7">croen</rhyme></l>
        <l id="s10:l12" met="2" type="-+">Als ic sel <rhyme label="d" type="m" corresp="#s10:l9">gaen</rhyme></l>
        <l id="s10:l13" met="4" type="-+">Van heen bi haer boven die <rhyme label="d" type="m" corresp="#s10:l9">maen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Daer si is roes van <rhyme label="a" type="m">paradijs</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Scoen, edel, claer, frisch, suet end <rhyme label="a" type="m" corresp="#s11:l1">wijs</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Voer allen meechden draeght si <rhyme label="a" type="m" corresp="#s11:l1">prijs</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Wye soudmen vijnden haers <rhyme label="b" type="m">ghelijc</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Die God, ons vaer van <rhyme label="b" type="m" corresp="#s11:l4">hemmelrijc</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Mit sinen vrucht <rhyme label="b" type="m" corresp="#s11:l4">ghebenedijt</rhyme>?</l>
        <l id="s11:l7" met="2" type="-+">Nyemant <rhyme label="c" type="m">voerwaer</rhyme>,</l>
        <l id="s11:l8" met="2" type="-+">Dat is doch <rhyme label="c" type="m" corresp="#s11:l7">claer</rhyme></l>
        <l id="s11:l9" met="2" type="-+">Een <rhyme label="d" type="m">yghelijc</rhyme>.</l>
        <l id="s11:l10" met="2" type="-+">Want tis van <rhyme label="c" type="m" corresp="#s11:l7">haer</rhyme></l>
        <l id="s11:l11" met="2" type="+-">Een seker <rhyme label="c" type="m" corresp="#s11:l7">maer</rhyme></l>
        <l id="s11:l12" met="2" type="-+">Over <rhyme label="d" type="m" corresp="#s11:l9">eertrijc</rhyme>,</l>
        <l id="s11:l13" met="4" type="-+">Dat si is vrou van <rhyme label="d" type="m" corresp="#s11:l9">hemmelrijc</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Waerlic si is die edel <rhyme label="a" type="m">vrou</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Diemen mit rechte dienen <rhyme label="a" type="m" corresp="#s12:l1">sou</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Want si haer dienres is <rhyme label="a" type="m" corresp="#s12:l1">ghetrou</rhyme>.</l>
        <l id="s12:l4" met="4" type="-+">Gheloeft des vry, so wie haer <rhyme label="b" type="m">dient</rhyme></l>
        <l id="s12:l5" met="4" type="-+">End haer mit rechter herten <rhyme label="b" type="m" corresp="#s12:l4">meeynt</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Si sel hem sijn een grote <rhyme label="b" type="m" corresp="#s12:l4">vrient</rhyme>.</l>
        <l id="s12:l7" met="2" type="-+">Haer si lof, <rhyme label="c" type="m">eer</rhyme></l>
        <l id="s12:l8" met="2" type="-+">Mit onsen <rhyme label="c" type="m" corresp="#s12:l7">heer</rhyme></l>
        <l id="s12:l9" met="2" type="-+">Tot alre <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s12:l10" met="2" type="-+">Die ons hier <rhyme label="c" type="m" corresp="#s12:l7">neer</rhyme></l>
        <l id="s12:l11" met="2" type="+-">Wil maken <rhyme label="c" type="m" corresp="#s12:l7">seer</rhyme></l>
        <l id="s12:l12" met="2" type="-+">In hem <rhyme label="d" type="m" corresp="#s12:l9">verblijt</rhyme></l>
        <l id="s12:l13" met="4" type="-+">End daer na gheven ewich <rhyme label="d" type="m" corresp="#s12:l9">rijc</rhyme>.'</l>
      </lg>
    </lg>
  </text>
</song>
