  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoort Cloris myn <rhyme label="a" type="m">versoeck</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En thooont u soet <rhyme label="b" type="m">ghelaet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Aen die u gunste <rhyme label="c" type="m">draeght</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Bethoont dat al de <rhyme label="b" type="m" corresp="#s1:l2">daet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Aen uwen sin <rhyme label="c" type="m" corresp="#s1:l3">behaeght</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van die u gunste <rhyme label="c" type="m" corresp="#s1:l3">draeght</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De reden die my <rhyme label="a" type="m">dwinght</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Te volghen uwe <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat is u <rhyme label="c" type="m">schoonigheydt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Waer door ghy mynen <rhyme label="b" type="m" corresp="#s2:l2">sin</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Houdt t'uwen dienst <rhyme label="c" type="m" corresp="#s2:l3">bereydt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat is u <rhyme label="c" type="m" corresp="#s2:l3">schoonigheydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De straelen uws <rhyme label="a" type="m">ghesicht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En lieve lipkens <rhyme label="b" type="m">root</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hebben my 'thert <rhyme label="c" type="m">door-wont</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">V boesem wit, en <rhyme label="b" type="m" corresp="#s3:l2">bloot</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En poesel borstjens <rhyme label="c" type="m" corresp="#s3:l3">ront</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hebben my 'thert <rhyme label="c" type="m" corresp="#s3:l3">door-wont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Laet dan de <rhyme label="a" type="m">lieflyckheyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Besloten in u <rhyme label="b" type="m">deucht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Aen my gheopent <rhyme label="c" type="m">zyn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gunt my Goddin u <rhyme label="b" type="m" corresp="#s4:l2">jeucht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En laet u minne <rhyme label="c" type="m" corresp="#s4:l3">pyn</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Aen my gheopent <rhyme label="c" type="m" corresp="#s4:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dan sal ick tot u <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Beroemen uwe <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Boven al 'tmaeghd' <rhyme label="c" type="m">gheslacht</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En ghy myn Cloris <rhyme label="b" type="m" corresp="#s5:l2">teer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Sult van my zyn <rhyme label="c" type="m" corresp="#s5:l3">gheacht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Boven al 'tmaeghd' <rhyme label="c" type="m" corresp="#s5:l3">gheslacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
