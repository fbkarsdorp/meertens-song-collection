<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184264</id>
    <date>
      <year-start>1815</year-start>
      <year-end>1815</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Je le compare avec Louis</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laastmaal ging ik door het <rhyme label="a" type="m">Veld</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met myn Brak en <rhyme label="b" type="f">Haazewinden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Om het wild eens te <rhyme label="b" type="f" corresp="#s1:l2">verslinden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Langzaam aan ik wierd <rhyme label="a" type="m" corresp="#s1:l1">ontsteld</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Digt al by die groene <rhyme label="c" type="f">Falyën</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Begon myn hert schier te <rhyme label="c" type="f" corresp="#s1:l5">verblyën</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Een zoet geluit, een zoet <rhyme label="d" type="m">geluit</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Van een schoon <rhyme label="e" type="f">Veldgodinne</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Ik bleef wat staan <rhyme label="e" type="f" corresp="#s1:l8">verzinnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Door den zank en <rhyme label="a" type="m">melody</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Van die schoone <rhyme label="b" type="f">Herderinne</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wierd myn hert gekweld van <rhyme label="b" type="f" corresp="#s2:l2">binne</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Ik voegde my stil aan haar <rhyme label="a" type="m" corresp="#s2:l1">zy</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Ik zei zoete lief ik moet wat <rhyme label="c" type="f">rusten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Aen uw zy en in dees <rhyme label="c" type="f" corresp="#s2:l5">Kusten</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">ô Zoete velde blom, ô zoete velde <rhyme label="d" type="m">blom</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Mogt ik uw Dienaer <rhyme label="e" type="f">wezen</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Dan was myn pyn <rhyme label="e" type="f" corresp="#s2:l8">genezen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Jager ik blyf in <rhyme label="a" type="m">eenigheid</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En met myn schaepkens wil ik <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">In het zoete veld <rhyme label="b" type="f" corresp="#s3:l2">verheven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">By de zoete <rhyme label="a" type="m" corresp="#s3:l1">liberteit</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Want ik heb geen lust in <rhyme label="c" type="f">paere</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ik ben nog te jong van <rhyme label="c" type="f" corresp="#s3:l5">jaere</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Ach Jager zoet, Ach Jager <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Wilt een ander maer <rhyme label="e" type="f">minne</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Maer geen <rhyme label="e" type="f" corresp="#s3:l8">Herderinne</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Schoonste Velde-blom van 't <rhyme label="a" type="m">land</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Laet my uwe Dienaer <rhyme label="b" type="f">wezen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">In het groene veld <rhyme label="b" type="f" corresp="#s4:l2">verheven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">By uw schaepkens heel <rhyme label="a" type="m" corresp="#s4:l1">sermand</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Ik zal uw schaepkens helpen <rhyme label="c" type="f">weiden</rhyme>:</l>
        <l id="s4:l6" met="4" type="+-">En van den Wolf wel <rhyme label="c" type="f" corresp="#s4:l5">bevryden</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">By dag en nagt, by dag en <rhyme label="d" type="m">nagt</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Zal ik by uw schaepkens <rhyme label="e" type="f">wezen</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Wilt my uw trouw nu <rhyme label="e" type="f" corresp="#s4:l8">geven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Jaeger ik heb geen <rhyme label="a" type="m">vermaek</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Als in het zoet vogels <rhyme label="b" type="f">zingen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daarom wilt uw gouw <rhyme label="b" type="f" corresp="#s5:l2">verzinnen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Want gy my niet aen en <rhyme label="a" type="m" corresp="#s5:l1">staet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">En laet my in deze <rhyme label="c" type="f">kusten</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Hier al by myn schaepkens <rhyme label="c" type="f" corresp="#s5:l5">rusten</rhyme>:</l>
        <l id="s5:l7" met="4" type="+-">En by het geklank, en by het <rhyme label="d" type="m">geklank</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Van de vogelen <rhyme label="e" type="f">dieren</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Die hier in het veld al <rhyme label="e" type="f" corresp="#s5:l8">zwieren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">ô Schoonste blom van het <rhyme label="a" type="m">land</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Laet my tog uw min <rhyme label="b" type="f">verwerven</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Anders moet ik treurig <rhyme label="b" type="f" corresp="#s6:l2">sterven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En zyt doch door min <rhyme label="a" type="m" corresp="#s6:l1">constand</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Want ziet de vogelen en <rhyme label="c" type="f">dieren</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Hoe zy in de velde <rhyme label="c" type="f" corresp="#s6:l5">zwieren</rhyme>:</l>
        <l id="s6:l7" met="4" type="+-">Al door de min, al door de <rhyme label="d" type="m">min</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">En met zoete <rhyme label="e" type="f">tyden</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Haer tot de min <rhyme label="e" type="f" corresp="#s6:l8">bereiden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Het paert door min al wat 'er <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Ja de visschen in de <rhyme label="b" type="f">stromen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Het gevogelt komt ons <rhyme label="b" type="f" corresp="#s7:l2">tonen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Hoe het met zyn gaaiken <rhyme label="a" type="m" corresp="#s7:l1">herleeft</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Ja het wreedste dier op <rhyme label="c" type="f">d'aerde</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Door de min het ook al <rhyme label="c" type="f" corresp="#s7:l5">paerde</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Schoonste Godin, Schoonste <rhyme label="d" type="m">Godin</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Laet my tog troost <rhyme label="e" type="f">verwerven</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">Of ik moet van liefde <rhyme label="e" type="f" corresp="#s7:l8">sterven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Jager zo ik ken uw <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Ik zal myn trouw in 't veld u <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">By myn Schaepkens zoet <rhyme label="b" type="f" corresp="#s8:l2">verheven</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Gedaen is al uw pyn en <rhyme label="a" type="m" corresp="#s8:l1">smert</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Ik zie uw <rhyme label="c" type="f">standvastigheden</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Nu zullen wu haest op <rhyme label="c" type="f" corresp="#s8:l5">heden</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">In 't groene <rhyme label="d" type="m">veld</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Met vreugden <rhyme label="e" type="f">herleven</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">Al door de min <rhyme label="e" type="f" corresp="#s8:l8">gedreven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Alle Minnaers van het <rhyme label="a" type="m">veld</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Wilt nu al hier <rhyme label="b" type="f">vergaeren</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Herders hoort naer myn <rhyme label="b" type="f" corresp="#s9:l2">verklaren</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Op de Bruilofst-Feest <rhyme label="a" type="m" corresp="#s9:l1">verzelt</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Speelt nu ook op de <rhyme label="c" type="f">Trompetten</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Cither, Fluit en de <rhyme label="c" type="f" corresp="#s9:l5">Cornette</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Maekt zoet geschal, maekt zoet <rhyme label="d" type="m">geschal</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">En laet de Romers <rhyme label="e" type="f">klinken</rhyme>,</l>
        <l id="s9:l9" met="3" type="+-">Wilt de Santen nu <rhyme label="e" type="f" corresp="#s9:l8">drinken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
