    <melody>Nu laat ons allegaar</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Om nu te moegen <rhyme label="a" type="f">ueten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat Ick heb aen <rhyme label="b" type="f">geeheeuen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat ons Die wingaert <rhyme label="a" type="f" corresp="#s1:l1">spruiten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">seer mindelick aen <rhyme label="b" type="f" corresp="#s1:l2">kleeuen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daer wij vroelick bij <rhyme label="b" type="f" corresp="#s1:l2">leeuen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">verhuecht ons sinnen <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ick salt u liefie <rhyme label="b" type="f" corresp="#s1:l2">geeuen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">tot voldoeninge van het <rhyme label="c" type="m" corresp="#s1:l6">spel</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als gijt lief hebt <rhyme label="a" type="f">ontfangen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">volcht mijn als Ick doe <rhyme label="b" type="f">voeren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">voldoet ons met een <rhyme label="a" type="f" corresp="#s2:l1">sange</rhyme></l>
        <l id="s2:l4" met="3" type="-+">laet u klincker eens <rhyme label="b" type="f" corresp="#s2:l2">hooren</rhyme></l>
        <l id="s2:l5" met="3" type="-+">om vruechde te <rhyme label="b" type="f" corresp="#s2:l2">oorboeren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">alle trueren nu <rhyme label="c" type="m">voersmaet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">hebt gij wat liefs <rhyme label="b" type="f" corresp="#s2:l2">voerkoeren</rhyme></l>
        <l id="s2:l8" met="3" type="-+">hem nimmer meer <rhyme label="c" type="m" corresp="#s2:l6">voerlaet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Tgelt u mijn naeste <rhyme label="a" type="f">gebuere</rhyme></l>
        <l id="s3:l2" met="3" type="-+">hier In deese <rhyme label="b" type="f">kontreije</rhyme></l>
        <l id="s3:l3" met="3" type="-+">tgaet seer soetelick <rhyme label="a" type="f" corresp="#s3:l1">Inne</rhyme></l>
        <l id="s3:l4" met="3" type="-+">tsmaeckt als <rhyme label="b" type="f" corresp="#s3:l2">suikereije</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Als Ick dit natie <rhyme label="b" type="f" corresp="#s3:l2">spreije</rhyme></l>
        <l id="s3:l6" met="3" type="-+">tfeerhuecht al mijnen <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">sinckt oeck een liedeken <rhyme label="b" type="f" corresp="#s3:l2">fraeije</rhyme></l>
        <l id="s3:l8" met="3" type="-+">als uwen schencker <rhyme label="c" type="m" corresp="#s3:l6">doet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Prinse het Is u <rhyme label="a" type="f">geschonken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">en u Jonstijge <rhyme label="b" type="f">heeren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">tijs schoentges <rhyme label="a" type="f" corresp="#s4:l1">uitgedroncken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Doet soe mee tis mijn <rhyme label="b" type="f" corresp="#s4:l2">begeeren</rhyme></l>
        <l id="s4:l5" met="3" type="-+">wilt Dit ten besten <rhyme label="b" type="f" corresp="#s4:l2">keeren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">het komt uit tsersen <rhyme label="c" type="m">gront</rhyme></l>
        <l id="s4:l7" met="3" type="-+">lief Dit is tot Uwer <rhyme label="b" type="f" corresp="#s4:l2">eeren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat sech Ick u goedt <rhyme label="c" type="m" corresp="#s4:l6">ront</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
