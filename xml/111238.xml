<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>111238</id>
    <date>
      <year-start>1785</year-start>
      <year-end>1785</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="zeemanslied" translation="sailor's song">zeemanslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="work">werk</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WEl wat is 'er beter <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Als te wezen een <rhyme label="b" type="m">Matroos</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Zy leven vergenoegt <rhyme label="b" type="m" corresp="#s1:l2">altoos</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En zy gaene lustig <rhyme label="c" type="f">zwiere</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">Met de Meisjes van de <rhyme label="d" type="m">trant</rhyme>?</l>
        <l id="s1:l6" met="4" type="-+">Zoo dra zy koomen aen <rhyme label="d" type="m" corresp="#s1:l5">land</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Voor eerst komen zy <rhyme label="a" type="f">aengetreeden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Aen de Buytenkant hoord <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer ziet men de Meisjes <rhyme label="b" type="m" corresp="#s2:l2">staen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die haer dan ook vriendelyk <rhyme label="c" type="f">groete</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wellekom myn lieve <rhyme label="d" type="m">vrint</rhyme>:</l>
        <l id="s2:l6" met="4" type="-+">Ziet wy wagte na u <rhyme label="d" type="m" corresp="#s2:l5">splint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dan ziet men ze lustig <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">In de Kapel- of <rhyme label="b" type="m">Nieuwenbrugsteeg</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Na die Kelders wel ter <rhyme label="b" type="m" corresp="#s3:l2">deeg</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Daer weet men ook van geen <rhyme label="c" type="f">treuren</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Zy danszen voor Fiool en <rhyme label="d" type="m">Lier</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Al met de Meisjes van <rhyme label="d" type="m" corresp="#s3:l5">plyzier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'t Hoekie om na 't <rhyme label="a" type="f">Wyngaertstraetje</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Daer komen zy heel <rhyme label="b" type="m">figeland</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Vinden daer Dames na de <rhyme label="b" type="m" corresp="#s4:l2">trand</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die zegge wild maer binnen <rhyme label="c" type="f">treeden</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Eyst een Flessie na je <rhyme label="d" type="m">lust</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">En daer op ook eens <rhyme label="d" type="m" corresp="#s4:l5">gekust</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Franse Halfsteeg en Kolk wilt <rhyme label="a" type="f">hooren</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Die passeeren zy met <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wardinnetjes staen zonder <rhyme label="b" type="m" corresp="#s5:l2">geween</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die haer ook zoo wel <rhyme label="c" type="f">ontvangen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En zy passen op haer <rhyme label="d" type="m">poen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Daerom is het haer te <rhyme label="d" type="m" corresp="#s5:l5">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zy zeggen laet ons niet <rhyme label="a" type="f">vergeeten</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">De ridder- en de <rhyme label="b" type="m">Ionkerstraet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Het Kollegat men dan in <rhyme label="b" type="m" corresp="#s6:l2">slaet</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">De Nieuwmarkt laten zy ook niet <rhyme label="c" type="f">leggen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">In de Momkelder hoord <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">De Kempies moeten zy ook in <rhyme label="d" type="m" corresp="#s6:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De Duivels hoek ook in <rhyme label="a" type="f">getreeden</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Daer vind men de grootste <rhyme label="b" type="m">pret</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">By Schele Griet of <rhyme label="b" type="m" corresp="#s7:l2">Antinet</rhyme>;</l>
        <l id="s7:l4" met="4" type="-+">By Moeder Traenoog met haer <rhyme label="c" type="f">puysie</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En by Vader Menke <rhyme label="d" type="m">Agril</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Daer speelen zy <rhyme label="d" type="m" corresp="#s7:l5">Honnebil</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dan ziet men ze weer <rhyme label="a" type="f">spanceeren</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">De Nes door na de <rhyme label="b" type="m">Bierkay</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daer zitten de Meisjes ook heel <rhyme label="b" type="m" corresp="#s8:l2">fraey</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Gekleed al na de nieuwste <rhyme label="c" type="f">Moode</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">In de steegjes alle <rhyme label="d" type="m">drie</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">'t Is alia Moode de <rhyme label="d" type="m" corresp="#s8:l5">parrie</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">De Nieuwendyk had ik haest <rhyme label="a" type="f">vergeeten</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Die gaen zy over als een <rhyme label="b" type="m">held</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Want wy hebben nog braef <rhyme label="b" type="m" corresp="#s9:l2">geld</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Zy moeten in 't Karremelksteegje <rhyme label="c" type="f">wezen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">By Moeder Gort of Pissege <rhyme label="d" type="m">Lys</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Die hout van een lange <rhyme label="d" type="m" corresp="#s9:l5">radys</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De Mouthaensteeg al voor 't <rhyme label="a" type="f">leste</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Ia daer moesten zy ook nog <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Om te drinken een Flessie <rhyme label="b" type="m" corresp="#s10:l2">Wyn</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Als zy dan braef <rhyme label="c" type="f">domineeren</rhyme>:</l>
        <l id="s10:l5" met="4" type="-+">By Jannetje en by Neeltje <rhyme label="d" type="m">Tuyt</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">En dan weer het Zeegat <rhyme label="d" type="m" corresp="#s10:l5">uit</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Oorlof dan Matroosies <rhyme label="a" type="f">alle</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Die dit Liedje heeft <rhyme label="b" type="m">gedigt</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">'t Was een Matroos zyn hert was <rhyme label="b" type="m" corresp="#s11:l2">ligt</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Aen de Caep de Bon <rhyme label="c" type="f">Esprance</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Hy heeft gedigt al uit syn <rhyme label="d" type="m">kop</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">By vermaerde Moeder <rhyme label="d" type="m" corresp="#s11:l5">Schepop</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
