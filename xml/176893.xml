  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Als myn Minnaar my weer <rhyme label="a" type="m">begroet</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Weder kusjes geeft als te <rhyme label="b" type="f">vooren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dan krygt het gras gewis nieuwe <rhyme label="a" type="m" corresp="#s1:l1">gloed</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">De vrolyke lent word dan <rhyme label="b" type="f" corresp="#s1:l2">herboren</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Stil ik hoor hem <rhyme label="c" type="f">roepen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wat verdriet, wat <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Ik zie myn waarde Minnaar <rhyme label="d" type="m" corresp="#s1:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als myn Minnaar hun lyden <rhyme label="a" type="m">zingt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Willen de Vogelen bitter <rhyme label="b" type="f">kweelen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als hunne stem de bossen <rhyme label="a" type="m" corresp="#s2:l1">doordringt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Moet gy de oorzaak daar van <rhyme label="b" type="f" corresp="#s2:l2">wezen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Stil ik hoor hem <rhyme label="c" type="f">roepen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Wat verdriet, wat <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">En zie myn waarde Minnaar <rhyme label="d" type="m" corresp="#s2:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ega die my wel duizond <rhyme label="a" type="m">maal</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Hebt vermoeid door myne <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Misschien komt myn minnaar niet <rhyme label="a" type="m" corresp="#s3:l1">weer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Om na hua iiefde te <rhyme label="b" type="f" corresp="#s3:l2">vragen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Stil ik hoor hem <rhyme label="c" type="f">roepen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Wat verdriet, wat <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ik vind myn waarde Minnaar <rhyme label="d" type="m" corresp="#s3:l6">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
