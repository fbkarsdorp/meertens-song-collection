<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>34224</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody>Comtesse de Corsol</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Soetste Nymphe <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Aen wie dat weerdt de <rhyme label="a" type="m" corresp="#s1:l1">kroon</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Der Goden waer <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Siet ick met groot <rhyme label="c" type="m">ootmoedt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Val u als nu te <rhyme label="c" type="m" corresp="#s1:l4">voet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En bidd’ u om mijn <rhyme label="b" type="f" corresp="#s1:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Komt Liefste metter <rhyme label="a" type="m">spoedt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ontdeckt uw’ wesen <rhyme label="a" type="m" corresp="#s2:l1">soet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En doet doch <rhyme label="b" type="f">assistenci</rhyme>’</l>
        <l id="s2:l4" met="3" type="-+">Als een die trouw’lijck <rhyme label="c" type="m">slaeft</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En wenscht te zijn <rhyme label="c" type="m" corresp="#s2:l4">gelaeft</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Door uwe bly <rhyme label="b" type="f" corresp="#s2:l3">presenci</rhyme>’.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Want geene tongh <rhyme label="a" type="m">voorwaer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En soud’ op duysent <rhyme label="a" type="m" corresp="#s3:l1">jaer</rhyme></l>
        <l id="s3:l3" met="3" type="-+">(Al hadd’ sy <rhyme label="b" type="f">eloquenci</rhyme>’)</l>
        <l id="s3:l4" met="3" type="-+">Vertellen ’t swaer <rhyme label="c" type="m">ellend</rhyme>’,</l>
        <l id="s3:l5" met="3" type="-+">’t Welck ick met groot <rhyme label="c" type="m" corresp="#s3:l4">torment</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Verdraegh door uw’ <rhyme label="b" type="f" corresp="#s3:l3">absenci</rhyme>’.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">My dunckt, alwaer ghy <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat daer by is <rhyme label="a" type="m" corresp="#s4:l1">altijdt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Mijn stemm’ vol <rhyme label="b" type="f">desolaci</rhyme>’,</l>
        <l id="s4:l4" met="3" type="-+">Die op uw’ woorden <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Gelijck den Echo <rhyme label="c" type="m" corresp="#s4:l4">doet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Antwoordt met <rhyme label="b" type="f" corresp="#s4:l3">lamentaci</rhyme>’.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Oorlof, o wreede <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Nu wordt van my <rhyme label="a" type="m" corresp="#s5:l1">geklaeght</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat ghy kort mijn jongh <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">’t Welck ghy door ’t lieve <rhyme label="c" type="m">licht</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Van uw’ seer soet <rhyme label="c" type="m" corresp="#s5:l4">gesicht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Wel kont verlengen <rhyme label="b" type="f" corresp="#s5:l3">geven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
