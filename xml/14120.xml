  <author>Pieter Sterlincx</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">O Nederlant <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hoe machman v <rhyme label="b" type="f">volprijsen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Ghy spant die <rhyme label="a" type="m" corresp="#s1:l1">croon</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Soomen wel can <rhyme label="b" type="f" corresp="#s1:l2">bewijsen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Al inde werelt <rhyme label="c" type="m">wijt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Maecte ghy groot <rhyme label="c" type="m" corresp="#s1:l5">iolijt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Deur neringhe <rhyme label="d" type="f">ghedreuen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Die t’aller <rhyme label="c" type="m" corresp="#s1:l5">tijt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">V heeft subijt // <rhyme label="d" type="f" corresp="#s1:l7">verheuen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Waerom dat ghy <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hebt geregeert seer <rhyme label="b" type="f">crachtich</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Wijs ende <rhyme label="a" type="m" corresp="#s2:l1">vroet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Bouen alle volcken <rhyme label="b" type="f" corresp="#s2:l2">machtich</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Met goede <rhyme label="c" type="m">policy</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Soo toonde ghy v <rhyme label="c" type="m" corresp="#s2:l5">vry</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Als de wijt <rhyme label="d" type="f">vermaerde</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Dies u elck <rhyme label="c" type="m" corresp="#s2:l5">bly</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Sonder ialousy // <rhyme label="d" type="f" corresp="#s2:l7">aenuaerde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Spaignien <rhyme label="a" type="m">vaillant</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Poortugael en <rhyme label="b" type="f">Noorweghen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Vrancrijck en <rhyme label="a" type="m" corresp="#s3:l1">Duytslant</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hebben van u <rhyme label="b" type="f" corresp="#s3:l2">vercreghen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Als Polen ende <rhyme label="c" type="m">Sweden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Met Persen en oock <rhyme label="c" type="m" corresp="#s3:l5">Meden</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Rijckdom en constighe <rhyme label="d" type="f">wercken</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Dies men v <rhyme label="c" type="m" corresp="#s3:l5">seden</rhyme></l>
        <l id="s3:l9" met="3" type="-+">In alle steden // can <rhyme label="d" type="f" corresp="#s3:l7">mercken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">In triumphen <rhyme label="a" type="m">fray</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ende ghebouselen <rhyme label="b" type="f">cierich</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Toony ghy v <rhyme label="a" type="m" corresp="#s4:l1">gay</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met allen seer <rhyme label="b" type="f" corresp="#s4:l2">manierich</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy waert oock seer volck <rhyme label="c" type="m">rijck</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Vol consten en <rhyme label="c" type="m" corresp="#s4:l5">practijck</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Bequaem tot alle <rhyme label="d" type="f">saecken</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Soo dat niet ws <rhyme label="c" type="m" corresp="#s4:l5">ghelijck</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Sonder beswijck // en is te <rhyme label="d" type="f" corresp="#s4:l7">raecken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Maer o Heere <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Hoe qualijck soutmen <rhyme label="b" type="f">gheloouen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Dat ghy zijt tot <rhyme label="a" type="m" corresp="#s5:l1">spot</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Van veel die u <rhyme label="b" type="f" corresp="#s5:l2">beroouen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Van lijf en oock van <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met wreetheyt soo <rhyme label="c" type="m" corresp="#s5:l5">onsoet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En maeckent al tot <rhyme label="d" type="f">schanden</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Dat ghy met <rhyme label="c" type="m" corresp="#s5:l5">spoet</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Hebt ghewonnen vroet // met u <rhyme label="d" type="f" corresp="#s5:l7">handen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">V Palleysen <rhyme label="a" type="m">al</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Sietmen nv <rhyme label="b" type="f">ruineren</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">En oock comen ten <rhyme label="a" type="m" corresp="#s6:l1">val</rhyme></l>
        <l id="s6:l4" met="3" type="-+">De neringhe deur <rhyme label="b" type="f" corresp="#s6:l2">t’raseren</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Van Mars die ons <rhyme label="c" type="m">alsnoch</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dreyghende is deur <rhyme label="c" type="m" corresp="#s6:l5">bedroch</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Van ghiericheyt, een <rhyme label="d" type="f">verseeren</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Vande armen, dus roep ick <rhyme label="c" type="m" corresp="#s6:l5">och</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Heer wilse toch // <rhyme label="d" type="f" corresp="#s6:l7">bekeeren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Alle consten <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Sijn nu van u <rhyme label="b" type="f">verscheyden</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Weluaert leyt <rhyme label="a" type="m" corresp="#s7:l1">doot</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Niemant wil t’ghewelt <rhyme label="b" type="f" corresp="#s7:l2">verbeyden</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dies ist daer <rhyme label="c" type="m">desolaet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Meest door de eyghen <rhyme label="c" type="m" corresp="#s7:l5">haet</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Alle dees <rhyme label="d" type="f">Nederlanden</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Elck stelt zijnen <rhyme label="c" type="m" corresp="#s7:l5">staet</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Inden hoochsten graet // vol <rhyme label="d" type="f" corresp="#s7:l7">schanden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Ghy Princen al <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die wt ionsten nu <rhyme label="b" type="f">versamen</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Laet v dees <rhyme label="a" type="m" corresp="#s8:l1">manier</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ten goede zijn, tuwer <rhyme label="b" type="f" corresp="#s8:l2">vramen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Veriaecht oock Momus <rhyme label="c" type="m">fel</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Met Zopelus <rhyme label="c" type="m" corresp="#s8:l5">opstel</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Want wy seer voor dees <rhyme label="d" type="f">schromen</rhyme></l>
        <l id="s8:l8" met="3" type="+-">Maer als vrienden <rhyme label="c" type="m" corresp="#s8:l5">wel</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Weest niet rebel // der vreuchden <rhyme label="d" type="f" corresp="#s8:l7">blomen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
