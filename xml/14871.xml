  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">PRonck je van dees <rhyme label="a" type="f">Maechden</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Heyl moet u <rhyme label="b" type="f">ghemoeten</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Dus het flickrent <rhyme label="a" type="f" corresp="#s1:l1">daeghen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Met ghesanghe <rhyme label="b" type="f" corresp="#s1:l2">groeten</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">So sulje <rhyme label="b" type="f" corresp="#s1:l2">boeten</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Hier int bedoude <rhyme label="c" type="m">gras</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">De schulde // die ick <rhyme label="d" type="f">dulde</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Om reden // lest <rhyme label="e" type="f">leden</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Doen ick by u <rhyme label="c" type="m" corresp="#s1:l6">was</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Coridon ay <rhyme label="a" type="f">stille</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Wat sal dit nu <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Waer toe dese <rhyme label="a" type="f" corresp="#s2:l1">grille</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Laet myn kruytjens <rhyme label="b" type="f" corresp="#s2:l2">lesen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Nimphjen ‘kwaer <rhyme label="b" type="f" corresp="#s2:l2">ghenesen</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Ghy my een kus toe <rhyme label="c" type="m">liet</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Gaet vanme // wilt <rhyme label="d" type="f">anme</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Rood’ kaecken // niet <rhyme label="e" type="f">raecken</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Want ten lust my <rhyme label="c" type="m" corresp="#s2:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Sydy graech om <rhyme label="a" type="f">kussen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Gaet soeckt u <rhyme label="b" type="f">beminde</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Die sal t’vier wel <rhyme label="a" type="f" corresp="#s3:l1">blussen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Gins aen gheene <rhyme label="b" type="f" corresp="#s3:l2">Linde</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Daer sult ghy haer <rhyme label="b" type="f" corresp="#s3:l2">vinde</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Pluckende <rhyme label="c" type="m">Lauwerlof</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Pryst die // bewyst <rhyme label="d" type="f">die</rhyme></l>
        <l id="s3:l8" met="2" type="-+">De smarte // uws <rhyme label="e" type="f">harte</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Tredet van my <rhyme label="c" type="m" corresp="#s3:l6">of</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Straffe <rhyme label="a" type="f">Galatea</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Kout als Noorder <rhyme label="b" type="f">Klippen</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Om dat ik niet ree <rhyme label="a" type="f" corresp="#s4:l1">stae</rhyme></l>
        <l id="s4:l4" met="3" type="+-">V te bien myn <rhyme label="b" type="f" corresp="#s4:l2">Lippen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Coridon myn <rhyme label="b" type="f" corresp="#s4:l2">slippen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Laet los schaemt ghy u <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s4:l7" met="2" type="-+">V smeecken // schoon <rhyme label="d" type="f">spreecken</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Noch loncken // <rhyme label="e" type="f">ontfoncken</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Mijn boesem <rhyme label="c" type="m" corresp="#s4:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Waer ick eens <rhyme label="a" type="f">Amintas</rhyme></l>
        <l id="s5:l2" met="3" type="+-">K’sou eer kusjens <rhyme label="b" type="f">kryghen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">O die soo bemint <rhyme label="a" type="f" corresp="#s5:l1">was</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Wilt vry daer van <rhyme label="b" type="f" corresp="#s5:l2">swyghen</rhyme></l>
        <l id="s5:l5" met="3" type="+-">K’sie een bloosje <rhyme label="b" type="f" corresp="#s5:l2">styghen</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Int hooghe voorhooft <rhyme label="c" type="m">blanck</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Harder // loopt <rhyme label="d" type="f">varder</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Dit lellen // en <rhyme label="e" type="f">quellen</rhyme></l>
        <l id="s5:l9" met="3" type="+-">Weet ick u gheen <rhyme label="c" type="m" corresp="#s5:l6">danck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Ghy behoort u te <rhyme label="a" type="f">schaemen</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Sulckx een Nimph te <rhyme label="b" type="f">verghen</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Met verdichte <rhyme label="a" type="f" corresp="#s6:l1">naemen</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Schaemt root my te <rhyme label="b" type="f" corresp="#s6:l2">terghen</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Galatea  <rhyme label="b" type="f" corresp="#s6:l2">verberghen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Wilt u ontstelde <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s6:l7" met="2" type="-+">T’lichte // <rhyme label="d" type="f">t’ghesichte</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Vws ooghen // <rhyme label="e" type="f">vermoghen</rhyme></l>
        <l id="s6:l9" met="3" type="+-">Veel Aertsche <rhyme label="c" type="m" corresp="#s6:l6">Goddin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Sou mijn ooghens <rhyme label="a" type="f">luyster</rhyme></l>
        <l id="s7:l2" met="3" type="+-">V het hart <rhyme label="b" type="f">ontfoncken</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">K’wensje liever <rhyme label="a" type="f" corresp="#s7:l1">duyster</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Datse nimmer <rhyme label="b" type="f" corresp="#s7:l2">bloncken</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Coridon <rhyme label="b" type="f" corresp="#s7:l2">beschoncken</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Is van een vreemde <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Laet my //en gaet <rhyme label="d" type="f">ghy</rhyme></l>
        <l id="s7:l8" met="2" type="-+">V soete // te <rhyme label="e" type="f">moete</rhyme></l>
        <l id="s7:l9" met="3" type="+-">En gunt my eens <rhyme label="c" type="m" corresp="#s7:l6">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
