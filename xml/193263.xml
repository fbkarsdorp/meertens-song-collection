<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>193263</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon lief wil mij troost geven</melody>
  <author>Aegidius Haeffacker</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ADieu, adieu wy <rhyme label="a" type="f">scheyden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Adieu o <rhyme label="b" type="m">Kevelaer</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Al trecken wy ter <rhyme label="a" type="f" corresp="#s1:l1">heyden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wy laten t'herte <rhyme label="b" type="m" corresp="#s1:l2">daer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Adieu o Maget <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die ons bewaren <rhyme label="c" type="m" corresp="#s1:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy sullen ons <rhyme label="a" type="f">gesellen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Als wy zijn weer <rhyme label="b" type="m">gekeert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Tot uwen lof <rhyme label="a" type="f" corresp="#s2:l1">vertellen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe ghy hier wort <rhyme label="b" type="m" corresp="#s2:l2">ge-eert</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Adieu o Maget <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die ons bewaren <rhyme label="c" type="m" corresp="#s2:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die aen de Samber <rhyme label="a" type="f">woonen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En die van <rhyme label="b" type="m">Kempen-landt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om eer aen u te <rhyme label="a" type="f" corresp="#s3:l1">toonen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sijn daer gelijcker <rhyme label="b" type="m" corresp="#s3:l2">handt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Adieu o Maget <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die ons bewaren <rhyme label="c" type="m" corresp="#s3:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy hebben daer <rhyme label="a" type="f">gelaten</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Van die zijn boven <rhyme label="b" type="m">Rhijn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Als oock van die u <rhyme label="a" type="f" corresp="#s4:l1">haten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En die beneden <rhyme label="b" type="m" corresp="#s4:l2">zijn</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Adieu o Maeget <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die ons bewaren <rhyme label="c" type="m" corresp="#s4:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Maes moet oock <rhyme label="a" type="f">belijden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat sy siet op de <rhyme label="b" type="m">baen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Pelgrims t'allen <rhyme label="a" type="f" corresp="#s5:l1">tijden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Soo vlijtigh naer u <rhyme label="b" type="m" corresp="#s5:l2">gaen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Adieu o Maeget <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die ons bewaren <rhyme label="c" type="m" corresp="#s5:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Adieu, Adieu y <rhyme label="a" type="f">scheyden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Adieu dan <rhyme label="b" type="m">Kevelaer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Adieu wy gaen ter <rhyme label="a" type="f" corresp="#s6:l1">heyden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Maer laten 't herte <rhyme label="b" type="m" corresp="#s6:l2">daer</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Adieu o Maeget <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die ons bewaren <rhyme label="c" type="m" corresp="#s6:l5">moet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
