<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>120776</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>deventer</orginal-name>
      <standard-name>Deventer, The Netherlands</standard-name>
      <coordinates>
        <lon>52.2532272</lon>
        <lat>6.1584725</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O schoonste aller <rhyme label="a" type="f">vrouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die men ter waereld <rhyme label="b" type="m">vind</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik kom u te <rhyme label="a" type="f" corresp="#s1:l1">aanschouwen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gy word van myn <rhyme label="b" type="m" corresp="#s1:l2">bemind</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">'k Heb u nog niet <rhyme label="c" type="f">vergeeten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Schoon ik geweest ben uit 't <rhyme label="d" type="m">land</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">De min blyft <rhyme label="c" type="f" corresp="#s1:l5">onversleeten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Staat in myn hert <rhyme label="d" type="m" corresp="#s1:l6">geplant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wel Philis hoe durft gy <rhyme label="a" type="f">spreeken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat gy myn trouw <rhyme label="b" type="m">bemind</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Uw ontrouw is <rhyme label="a" type="f" corresp="#s2:l1">gebleeken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">'t Is veel te laat myn <rhyme label="b" type="m" corresp="#s2:l2">vrind</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Gy loopt en gy gaat <rhyme label="c" type="f">zwieren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gy draaid gelyk de <rhyme label="d" type="m">wind</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maar dat zyn geen <rhyme label="c" type="f" corresp="#s2:l5">manieren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Van een zoo'n trouwe <rhyme label="d" type="m" corresp="#s2:l6">vrind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe kunt gy my <rhyme label="a" type="f">bezwaaren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Rosilia myn <rhyme label="b" type="m">vrindin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Want in ons kindsche <rhyme label="a" type="f" corresp="#s3:l1">jaaren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Droegen wy liefde en <rhyme label="b" type="m" corresp="#s3:l2">min</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maar als wy gingen op <rhyme label="c" type="f">schoolen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Kenden wy geen <rhyme label="d" type="m">natuur</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Zoo doet de min ons <rhyme label="c" type="f" corresp="#s3:l5">doolen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Door uwe schoonheid <rhyme label="d" type="m" corresp="#s3:l6">puur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'t Zyn al verlooren <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik wil myn liefde en <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Aan geen manspersoon <rhyme label="a" type="f" corresp="#s4:l1">besteeden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die tegen myn eer en <rhyme label="b" type="m" corresp="#s4:l2">zin</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Verlaat zyn Moeder en <rhyme label="c" type="f">Vader</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En zyn lief <rhyme label="d" type="m">Vaderland</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Gy doet als een <rhyme label="c" type="f" corresp="#s4:l5">Verraader</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Gy word van myn <rhyme label="d" type="m" corresp="#s4:l6">verband</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gedenkt de eerste <rhyme label="a" type="f">liefde</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Gedenkt de eerste <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die beide ons hart <rhyme label="a" type="f" corresp="#s5:l1">doorgriefde</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Toon dat gy maaget <rhyme label="b" type="m" corresp="#s5:l2">zyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Met een meedoogend <rhyme label="c" type="f">harte</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Al door de liefde haar <rhyme label="d" type="m">kragt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Laat gy myn in de <rhyme label="c" type="f" corresp="#s5:l5">smarte</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ik sterf van <rhyme label="d" type="m" corresp="#s5:l6">minneklagt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wel Philis gy hebt myn <rhyme label="a" type="f">verlaaten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">In 't midden van myn <rhyme label="b" type="m">gloed</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Gy minde de <rhyme label="a" type="f" corresp="#s6:l1">Soldaaten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En hebt tegen myn <rhyme label="b" type="m" corresp="#s6:l2">gemoed</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">In Holland dienst <rhyme label="c" type="f">genoomen'</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die zyn Vaderland <rhyme label="d" type="m">verlaat</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Mag ik myn lief niet <rhyme label="c" type="f" corresp="#s6:l5">noemen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Gy word van myn <rhyme label="d" type="m" corresp="#s6:l6">gehaat</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Adieu dan myn <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Adieu, <rhyme label="b" type="m">Rosilia</rhyme>!</l>
        <l id="s7:l3" met="3" type="-+">Kan ik geen troost meer <rhyme label="a" type="f" corresp="#s7:l1">vinden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Adieu zoet lief, ik <rhyme label="b" type="m" corresp="#s7:l2">ga</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Adieu, en ik ga <rhyme label="c" type="f">doolen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Tot dat de dood my <rhyme label="d" type="m">scheid</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Myn lief is myn <rhyme label="c" type="f" corresp="#s7:l5">ontstoolen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Door myn <rhyme label="d" type="m" corresp="#s7:l6">ontrouwigheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
