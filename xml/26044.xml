    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Grijp een moed gij edele jongelingen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Het eerste <rhyme label="a" type="m">Ghebod</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Eer Adam wert <rhyme label="b" type="f">beschuldight</rhyme> /</l>
        <l id="s1:l3" met="2" type="-+">Beval hem <rhyme label="a" type="m" corresp="#s1:l1">God</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Wast en <rhyme label="b" type="f" corresp="#s1:l2">vermenichvuldight</rhyme> /</l>
        <l id="s1:l5" met="4" type="-+">Dit is u lot / dit is u <rhyme label="a" type="m" corresp="#s1:l1">lot</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dus elck een volght <rhyme label="b" type="f" corresp="#s1:l2">gheduldigh</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Het eerste <rhyme label="a" type="m" corresp="#s1:l1">Ghebod</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Gods eyghen <rhyme label="a" type="m">aerdt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Streckt ons te <rhyme label="b" type="f">genereren</rhyme> /</l>
        <l id="s2:l3" met="2" type="-+">Die ons <rhyme label="a" type="m" corresp="#s2:l1">verklaert</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat wy souden <rhyme label="b" type="f" corresp="#s2:l2">vermeren</rhyme> /</l>
        <l id="s2:l5" met="4" type="-+">Dus u vergaert / dus u <rhyme label="a" type="m" corresp="#s2:l1">vergaert</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En volght met reyn <rhyme label="b" type="f" corresp="#s2:l2">begheeren</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Gods eyghen <rhyme label="a" type="m" corresp="#s2:l1">aerdt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Het <rhyme label="a" type="m">huwelijck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Also van God <rhyme label="b" type="f">beschreven</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Ons al <rhyme label="a" type="m" corresp="#s3:l1">ghelijck</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Om ons langh te doen <rhyme label="b" type="f" corresp="#s3:l2">leven</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Hier op Aedtrijck / hier op <rhyme label="a" type="m" corresp="#s3:l1">Aedtrijck</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Heeft God seer hoogh <rhyme label="b" type="f" corresp="#s3:l2">verheven</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Het <rhyme label="a" type="m" corresp="#s3:l1">huwelick</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Gheen meerder <rhyme label="a" type="m">vreught</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Daer twee te samen <rhyme label="b" type="f">paeren</rhyme></l>
        <l id="s4:l3" met="2" type="-+">In eer en <rhyme label="a" type="m" corresp="#s4:l1">deucht</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">Lijf en Ziel sy <rhyme label="b" type="f" corresp="#s4:l2">vergaren</rhyme></l>
        <l id="s4:l5" met="4" type="-+">In een ghevueught / in een <rhyme label="a" type="m" corresp="#s4:l1">ghevueght</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Als sy eendracht <rhyme label="b" type="f" corresp="#s4:l2">bewaren</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Gheen meerder <rhyme label="a" type="m" corresp="#s4:l1">vreucht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ter werelt <rhyme label="a" type="m">wyt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Vint men gheen meerder <rhyme label="b" type="f">lijen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Als twist en <rhyme label="a" type="m" corresp="#s5:l1">spijt</rhyme> /</l>
        <l id="s5:l4" met="3" type="-+">Man en Wijf <rhyme label="b" type="f" corresp="#s5:l2">partijen</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Dus u vermyt / Dus u <rhyme label="a" type="m" corresp="#s5:l1">vermyt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Set al onvree <rhyme label="b" type="f" corresp="#s5:l2">besijen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Ter Werelt <rhyme label="a" type="m" corresp="#s5:l1">wijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Reyn liefde <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Moet met liefde zijn <rhyme label="b" type="f">ghetoghen</rhyme> /</l>
        <l id="s6:l3" met="2" type="-+">Des Menschen <rhyme label="a" type="m" corresp="#s6:l1">ghemoet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Kan gheen ghewelt <rhyme label="b" type="f" corresp="#s6:l2">ghedooghen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Als't Lichaem doet / Als't Lichaem <rhyme label="a" type="m" corresp="#s6:l1">doet</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dus draeght na u <rhyme label="b" type="f" corresp="#s6:l2">vermoghen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Reyn liefde <rhyme label="a" type="m" corresp="#s6:l1">soet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Het <rhyme label="a" type="m">fondament</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Des houwelickx <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Is goedt en <rhyme label="a" type="m" corresp="#s7:l1">jent</rhyme> /</l>
        <l id="s7:l4" met="3" type="-+">Dat liefde wt <rhyme label="b" type="f" corresp="#s7:l2">ghelesen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">'tZy u bekent / 'tZy u <rhyme label="a" type="m" corresp="#s7:l1">bekent</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Laet liefd' der liefden <rhyme label="b" type="f" corresp="#s7:l2">wesen</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Het <rhyme label="a" type="m" corresp="#s7:l1">fondament</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
