      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Phyllis kwam zich buigen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Lieve <rhyme label="a" type="f">Linde-boomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Koele <rhyme label="a" type="f" corresp="#s1:l1">water-stroomen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Cristalyne <rhyme label="b" type="m">vliet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hier wil ick mijn minne <rhyme label="c" type="m">voen</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">En mijn Amaril <rhyme label="d" type="f">verwachten</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Onder 't blader-rijcke <rhyme label="c" type="m" corresp="#s1:l4">groen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Daer de pluymde <rhyme label="a" type="f">dieren</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Vrolijck <rhyme label="a" type="f" corresp="#s2:l1">tierelieren</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Met een soet <rhyme label="b" type="m">gesangh</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Daer de Lommer dicht en <rhyme label="c" type="m">stil</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Selver schijnt met my te <rhyme label="d" type="f">wachten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Op de komst van <rhyme label="c" type="m" corresp="#s2:l4">Amaril</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Minne doet my <rhyme label="a" type="f">klaghen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Minne wil 't <rhyme label="a" type="f" corresp="#s3:l1">verdraghen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Al wat min my <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Schoon mijn Nymph my plaeght met <rhyme label="c" type="m">pijn</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Noch en sal 't my niet <rhyme label="d" type="f">verdrieten</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Mach maer 't eynde vreughde <rhyme label="c" type="m" corresp="#s3:l4">zijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Ick en acht geen <rhyme label="a" type="f">lye</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Mach ick maer <rhyme label="a" type="f" corresp="#s4:l1">verblye</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Door u weder <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Amarillis mach mijn <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Dese vreughde maer <rhyme label="d" type="f">genieten</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Liefde lijdt met vreugde <rhyme label="c" type="m" corresp="#s4:l4">smert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Lieve <rhyme label="a" type="f">Herderinne</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Die mijn hert en <rhyme label="a" type="f" corresp="#s5:l1">sinne</rhyme></l>
        <l id="s5:l3" met="3" type="+-">hebt in u <rhyme label="b" type="m">gebiedt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Hoe langh ghy mijn lyen <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'t Lyen sal my niet <rhyme label="d" type="f">verdrieten</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Soo ghy 't in het eynd <rhyme label="c" type="m" corresp="#s5:l4">versoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
