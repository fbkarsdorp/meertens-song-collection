<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186888</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Stoorder onzer Rust en <rhyme label="a" type="f">Vreeden</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Vriend, nu in Vyand <rhyme label="b" type="m">verkeert</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Rusie zoeker zonder <rhyme label="a" type="f" corresp="#s1:l1">reeden</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">'t Onregt dog zich zelven <rhyme label="b" type="m" corresp="#s1:l2">deert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Trotze Brit komt u <rhyme label="a" type="f">gevallen</rhyme>?</l>
        <l id="s2:l2" met="4" type="+-">Snoode breeker van 't <rhyme label="b" type="m">Verbond</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Dat gy had, om Hollands <rhyme label="a" type="f" corresp="#s2:l1">wallen</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">'t Aandoen in haar zwaksten <rhyme label="b" type="m" corresp="#s2:l2">stond</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ducht Verrader onze <rhyme label="a" type="f">Staaren</rhyme>.</l>
        <l id="s3:l2" met="4" type="+-">Schoon 's Volks' Zonden zyn zeer <rhyme label="b" type="m">groot</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Heeft den Hemel nooit <rhyme label="a" type="f" corresp="#s3:l1">verlaaten</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Men roept God aan in de <rhyme label="b" type="m" corresp="#s3:l2">nood</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Blyft g' Ontrouw wy hebben <rhyme label="a" type="f">Buuren</rhyme>!</l>
        <l id="s4:l2" met="4" type="+-">Waar mee gy in Tweedragt <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die ons Roer Trouw helpen <rhyme label="a" type="f" corresp="#s4:l1">stuuren</rhyme>?</l>
        <l id="s4:l4" met="4" type="+-">Vrankryk ons nog niet <rhyme label="b" type="m" corresp="#s4:l2">begeeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">O hoe vielt g' op onze <rhyme label="a" type="f">Scheepen</rhyme>!</l>
        <l id="s5:l2" met="4" type="+-">Slokhals in ons swaksten <rhyme label="b" type="m">stand</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Meenden g'ins u nest te <rhyme label="a" type="f" corresp="#s5:l1">sleepen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dogge-breeker van 't <rhyme label="b" type="m" corresp="#s5:l2">verband</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Nu is uw Tractaat <rhyme label="a" type="f">vervallen</rhyme>!</l>
        <l id="s6:l2" met="4" type="+-">Kromwels helsche <rhyme label="b" type="m">Dwinglandy</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Ruykt nu niet aan Hollands <rhyme label="a" type="f" corresp="#s6:l1">wallen</rhyme>!</l>
        <l id="s6:l4" met="4" type="+-">Hollands Leeuw vegt hem thans <rhyme label="b" type="m" corresp="#s6:l2">vry</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wilt nu Kruyt nog Kogels <rhyme label="a" type="f">spaeren</rhyme>!</l>
        <l id="s7:l2" met="4" type="+-">Nog geen grof geschut <rhyme label="b" type="m">ontzien</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Helden donderd door de <rhyme label="a" type="f" corresp="#s7:l1">baaren</rhyme>!</l>
        <l id="s7:l4" met="4" type="+-">Vrankryk zal u bystand <rhyme label="b" type="m" corresp="#s7:l2">bien</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Gaat in Godes naam te <rhyme label="a" type="f">velden</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Op de ruymen <rhyme label="b" type="m">OceÃ«aan</rhyme>!</l>
        <l id="s8:l3" met="4" type="+-">Mannen die uw keven <rhyme label="a" type="f" corresp="#s8:l1">stelden</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Om 't geweld te weeder <rhyme label="b" type="m" corresp="#s8:l2">staan</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Doet de Koopmanschap <rhyme label="a" type="f">herleeven</rhyme>!</l>
        <l id="s9:l2" met="4" type="+-">Die schier in het graf <rhyme label="b" type="m">gelegt</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Hollandsch wallen heeft <rhyme label="a" type="f" corresp="#s9:l1">begeeven</rhyme>!</l>
        <l id="s9:l4" met="4" type="+-">En ons 't noodigsten <rhyme label="b" type="m" corresp="#s9:l2">ontzegt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Onze Helden staan, <rhyme label="a" type="f">Verrader</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Klaar om u den koot te <rhyme label="b" type="m">staan</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Komt den Doggerbank niet <rhyme label="a" type="f" corresp="#s10:l1">nader</rhyme></l>
        <l id="s10:l4" met="4" type="+-">'t Zal der op zyn Zoutmans <rhyme label="b" type="m" corresp="#s10:l2">gaan</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Ruyter, Tromp ons <rhyme label="a" type="f">Batavieren</rhyme>.</l>
        <l id="s11:l2" met="4" type="+-">Leeven nog in 't vry <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">God zal onze waapens <rhyme label="a" type="f" corresp="#s11:l1">stieren</rhyme></l>
        <l id="s11:l4" met="4" type="+-">Koopmanschap herleeven <rhyme label="b" type="m" corresp="#s11:l2">spoed</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Mogt een wel herstelden <rhyme label="a" type="f">Vreede</rhyme>.</l>
        <l id="s12:l2" met="4" type="+-">Aan het lieve <rhyme label="b" type="m">Vaderland</rhyme>.</l>
        <l id="s12:l3" met="4" type="+-">Brengen heil en voorspoed <rhyme label="a" type="f" corresp="#s12:l1">meede</rhyme>.</l>
        <l id="s12:l4" met="4" type="+-">Wyl 't bestierd word met <rhyme label="b" type="m" corresp="#s12:l2">Verstand</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
