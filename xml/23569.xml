    <melody>Wie wil mee te Bethlehem</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wie wil mede toe <rhyme label="a" type="m">Bethlehem</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+"><rhyme label="b" type="m">Kyrieleyson</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Daer vint men den Coninc van <rhyme label="a" type="m" corresp="#s1:l1">Iesusalem</rhyme>,</l>
        <l id="s1:l4" met="6" type="-+">Alleluya, Alleluya, <rhyme label="c" type="m">Alleluya</rhyme>,</l>
        <l id="s1:l5" met="6" type="-+">Gelovet sy de reyne Maget <rhyme label="c" type="m" corresp="#s1:l4">Maria</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+"><rhyme label="c" type="m" corresp="#s1:l4">Alleluya</rhyme>.</l>
        <l id="s1:l7" met="6" type="-+">Gelovet sy de reyne Maget <rhyme label="c" type="m" corresp="#s1:l4">Maria</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+"><rhyme label="c" type="m" corresp="#s1:l4">Alleluya</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nu hebben wy dat kindeken <rhyme label="a" type="m">ghevonden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+"><rhyme label="b" type="m">Kyrieleyson</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In snoode doeckelkens <rhyme label="a" type="m" corresp="#s2:l1">gewonden</rhyme></l>
        <l id="s2:l4" met="6" type="-+">Alleluya, Alleluya, <rhyme label="c" type="m">Alleluya</rhyme>,</l>
        <l id="s2:l5" met="6" type="-+">Ghelovet sy de reyne Maget <rhyme label="c" type="m" corresp="#s2:l4">Maria</rhyme></l>
        <l id="s2:l6" met="2" type="-+"><rhyme label="c" type="m" corresp="#s2:l4">Alleluya</rhyme>.</l>
        <l id="s2:l7" met="6" type="-+">Gelovet sy die Maghet <rhyme label="c" type="m" corresp="#s2:l4">Maria</rhyme>.</l>
        <l id="s2:l8" met="2" type="-+"><rhyme label="c" type="m" corresp="#s2:l4">Alleluya</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
