<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185446</id>
    <date>
      <year-start>1830</year-start>
      <year-end>1830</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Koop, koop maar lepels</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Wat hoor ik <rhyme label="a" type="f">praten</rhyme>?</l>
        <l id="s1:l2" met="4" type="-+">Een Yzere Spoorweg die men <rhyme label="b" type="m">maakt</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Zoo langs de de <rhyme label="a" type="f" corresp="#s1:l1">straaten</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Tot ons <rhyme label="b" type="m" corresp="#s1:l2">vermaak</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dit Rytuig is een fyn <rhyme label="c" type="m">model</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Het is een kunst, verstaat gy <rhyme label="c" type="m" corresp="#s1:l5">wel</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Men ryd in TWINTIG MINUTEN <rhyme label="d" type="m">tyd</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Van Amsterdam tot <rhyme label="e" type="f">Haarlem</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Stap, stap in een <rhyme label="f" type="f">Wagen</rhyme>,</l>
        <l id="s1:l10" met="5" type="-+">Ja, ja ik heb ze van alle <rhyme label="g" type="m">soort</rhyme>,</l>
        <l id="s1:l11" met="5" type="-+">Men ryd alle dagen, Zoo het <rhyme label="g" type="m" corresp="#s1:l10">behoort</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Het is een heel <rhyme label="a" type="f">vertier</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Te ryden zonder Paard of <rhyme label="b" type="m">Zweep</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">'T is voor uw <rhyme label="a" type="f" corresp="#s2:l1">Plaizier</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Vat gy dien <rhyme label="b" type="m" corresp="#s2:l2">kneep</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Een Rytuig trekt er veele <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Men is er spoedig, waar men <rhyme label="c" type="m" corresp="#s2:l5">hoort</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Men Vliegt gelyk een Kogel <rhyme label="d" type="m">heen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En kunt geen tyd <rhyme label="e" type="f">verliezen</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Stap, stap in een <rhyme label="f" type="f">Wagen</rhyme>,</l>
        <l id="s2:l10" met="5" type="-+">Ja, ja ik heb ze van alle <rhyme label="g" type="m">soort</rhyme>,</l>
        <l id="s2:l11" met="5" type="-+">Men ryd alle dagen, Zoo het <rhyme label="g" type="m" corresp="#s2:l10">behoort</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Te Brussel, wat <rhyme label="a" type="f">klugten</rhyme>!</l>
        <l id="s3:l2" met="4" type="-+">Daar ryd de Stoom eerst in de <rhyme label="b" type="m">Sloot</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Dit zyn de <rhyme label="a" type="f" corresp="#s3:l1">vrugten</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Daar zat de Kunst in <rhyme label="b" type="m" corresp="#s3:l2">nood</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Nu sprak de Stuurder van dit <rhyme label="c" type="m">Werk</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">'K ben in de Kunst der Stoom niet <rhyme label="c" type="m" corresp="#s3:l5">sterk</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Als gy myn helpt uit de <rhyme label="d" type="m">Brand</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dan zal ik 't weet <rhyme label="e" type="f">Probeeren</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Stap, stap in een <rhyme label="f" type="f">Wagen</rhyme>,</l>
        <l id="s3:l10" met="5" type="-+">Ja, ja ik heb ze van alle <rhyme label="g" type="m">soort</rhyme>,</l>
        <l id="s3:l11" met="5" type="-+">Men ryd alle dagen, Zoo het <rhyme label="g" type="m" corresp="#s3:l10">behoort</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Doch na een korte <rhyme label="a" type="f">tyd</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Toen reed men Vrolyk naar elks <rhyme label="b" type="m">wil</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">O! wat groote <rhyme label="a" type="f" corresp="#s4:l1">spyt</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Daar stond de Stoom weer <rhyme label="b" type="m" corresp="#s4:l2">stil</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Wat of de Stuurder <rhyme label="c" type="m">ondernam</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">De Stoom die toch niet verder <rhyme label="c" type="m" corresp="#s4:l5">kwam</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Men trok, een elk deed zyn <rhyme label="d" type="m">best</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En werkten als de <rhyme label="e" type="f">Paarden</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Stap, stap in een <rhyme label="f" type="f">Wagen</rhyme>,</l>
        <l id="s4:l10" met="5" type="-+">Ja, ja ik heb ze van alle <rhyme label="g" type="m">soort</rhyme>,</l>
        <l id="s4:l11" met="5" type="-+">Men ryd alle dagen, Zoo het <rhyme label="g" type="m" corresp="#s4:l10">behoort</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Kom maakt uw <rhyme label="a" type="f">gereed</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">De kunst die is reeds spoedig <rhyme label="b" type="m">klaar</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Men heeft al <rhyme label="a" type="f" corresp="#s5:l1">Gemeed'</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Vind maar geen <rhyme label="b" type="m" corresp="#s5:l2">bezwaar</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Haarlem, Lyden en <rhyme label="c" type="m">Rotterdam</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">By uw komt ook die Kunsten <rhyme label="c" type="m" corresp="#s5:l5">an</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Gy wordt er het eerste mee <rhyme label="d" type="m">bedeeld</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Men wil het hier mee <rhyme label="e" type="f">Probeeren</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Stap, stap in een <rhyme label="f" type="f">Wagen</rhyme>,</l>
        <l id="s5:l10" met="5" type="-+">Ja, ja ik heb ze van alle <rhyme label="g" type="m">soort</rhyme>,</l>
        <l id="s5:l11" met="5" type="-+">Men ryd alle dagen, Zoo het <rhyme label="g" type="m" corresp="#s5:l10">behoort</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Zoo doen ik <rhyme label="a" type="f">Wenschen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">De Stoom-Machine veel <rhyme label="b" type="m">gewin</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">En alle <rhyme label="a" type="f" corresp="#s6:l1">Menschen</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Na hunnen <rhyme label="b" type="m" corresp="#s6:l2">zin</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Voor een Vyfje is men <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Dit kost uw 't eerste Togtje <rhyme label="c" type="m" corresp="#s6:l5">maar</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Doch Stuurder als ik uw raden <rhyme label="d" type="m">mag</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Houd uw beter als de <rhyme label="e" type="f">BELGEN</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Stap, stap in een <rhyme label="f" type="f">Wagen</rhyme>,</l>
        <l id="s6:l10" met="5" type="-+">Ja, ja ik heb ze van alle <rhyme label="g" type="m">soort</rhyme>,</l>
        <l id="s6:l11" met="5" type="-+">Men ryd alle dagen, Zoo het <rhyme label="g" type="m" corresp="#s6:l10">behoort</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
