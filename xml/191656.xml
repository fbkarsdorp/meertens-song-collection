<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191656</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Fortuin helaas</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CUpid'o God der <rhyme label="a" type="m">Minn'</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wilt van den Hemel <rhyme label="b" type="f">komen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En werkt in dees' haar <rhyme label="a" type="m" corresp="#s1:l1">zinn'</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat Liefd' nooit mag <rhyme label="b" type="f" corresp="#s1:l2">verkleumen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Wilt 't geen gy hebt <rhyme label="c" type="m">geplant</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Gestadelyk <rhyme label="d" type="f">vermeeren</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Zo zal m' aan alle <rhyme label="c" type="m" corresp="#s1:l5">Kant</rhyme></l>
        <l id="s1:l8" met="3" type="-+">U pryzen ende <rhyme label="d" type="f" corresp="#s1:l6">eeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Apol der wyzen <rhyme label="a" type="m">God</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wilt dog dees twee <rhyme label="b" type="f">Gelieven</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Door uwes wysheids <rhyme label="a" type="m" corresp="#s2:l1">lot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Zeer mildelyk <rhyme label="b" type="f" corresp="#s2:l2">gerieven</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wilt door uw wysheid <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Haar zinnen zo <rhyme label="d" type="f">versterken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat zy geluk en <rhyme label="c" type="m" corresp="#s2:l5">nood</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Verstaan als Goden <rhyme label="d" type="f" corresp="#s2:l6">werken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Pallas, ô kuisch <rhyme label="a" type="m">Godin</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Wilt door uw groote <rhyme label="b" type="f">kragten</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Instorten, kuissche <rhyme label="a" type="m" corresp="#s3:l1">Min</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In deeze haar <rhyme label="b" type="f" corresp="#s3:l2">gedagten</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Keert dog van haar het <rhyme label="c" type="m">geen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Hen hier in mogt <rhyme label="d" type="f">beletten</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Zo zal m' uw Lof <rhyme label="c" type="m" corresp="#s3:l5">alleen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Voor d'andere Goden <rhyme label="d" type="f" corresp="#s3:l6">zetten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Godinnen des <rhyme label="a" type="m">gelank</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wilt u dog herwaarts <rhyme label="b" type="f">voegen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En wilt dees twee ten <rhyme label="a" type="m" corresp="#s4:l1">dank</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ons met uw stem <rhyme label="b" type="f" corresp="#s4:l2">vernoegen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wilt met uw zoet <rhyme label="c" type="m">geluyt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Nu en tot allen <rhyme label="d" type="f">tyden</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ons Bruygom en <rhyme label="c" type="m" corresp="#s4:l5">Bruyd</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dog minnelyk <rhyme label="d" type="f" corresp="#s4:l6">verblyden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy goden al te <rhyme label="a" type="m">zaam</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wilt u tot deze <rhyme label="b" type="f">keeren</rhyme>!</l>
        <l id="s5:l3" met="3" type="-+">En wilt om uwen <rhyme label="a" type="m" corresp="#s5:l1">Naam</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Al 't quaat dog van haar <rhyme label="b" type="f" corresp="#s5:l2">weeren</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Verlengt haar jonge <rhyme label="c" type="m">tyd</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Tot Nestors oude <rhyme label="d" type="f">Jaaren</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En gunt dat zy <rhyme label="c" type="m" corresp="#s5:l5">verblyd</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Haar Kind'ren weer zien <rhyme label="d" type="f" corresp="#s5:l6">Paren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">ô Hymen <rhyme label="a" type="m">Hymeneê</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Wilt nu met vreugt <rhyme label="b" type="f">ontsluyten</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Het geen daar na met <rhyme label="a" type="m" corresp="#s6:l1">weê</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Voortbrengt zo schoone <rhyme label="b" type="f" corresp="#s6:l2">Spruyten</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Wilt dog den Bruylofts <rhyme label="c" type="m">Throon</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Zeer zoetelyk <rhyme label="d" type="f">verçieren</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Zo zal m'u tot uw <rhyme label="c" type="m" corresp="#s6:l5">loon</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Altoos en eeuwig <rhyme label="d" type="f" corresp="#s6:l6">vieren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
