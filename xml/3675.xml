    <melody>Psalm 065 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WILT doch v' ghericht' <rhyme label="a" type="f">ouergheuen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Vwen Koningh O <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">End v' gherechticheit daer <rhyme label="a" type="f" corresp="#s1:l1">neuen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Sijnen sone met <rhyme label="b" type="m" corresp="#s1:l2">eer'</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Op dat hy al v' volck <rhyme label="c" type="f">voortrede</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In der <rhyme label="d" type="m">gherechticheit</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">End d'aerme bescherme met <rhyme label="c" type="f" corresp="#s1:l5">vrede</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In aller <rhyme label="d" type="m" corresp="#s1:l6">billicheit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat de berghen den vrede <rhyme label="a" type="f">rycke</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Onder t'volck bringhen <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">End die hueuelen <rhyme label="a" type="f" corresp="#s2:l1">desghelycke</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat recht Heer nae v' <rhyme label="b" type="m" corresp="#s2:l2">Woort</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Hy sal die by dat recht <rhyme label="c" type="f">bewaeren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die met noot sijn <rhyme label="d" type="m">ghequelt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">End wtroeyen diese <rhyme label="c" type="f" corresp="#s2:l5">beswaeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Met kracht ende <rhyme label="d" type="m" corresp="#s2:l6">ghewelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Van een yder der <rhyme label="a" type="f">onderdaenen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sult ghy steedts eer' <rhyme label="b" type="m">ontfaen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">So langh' als de Sonn'end de <rhyme label="a" type="f" corresp="#s3:l1">Maene</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sullen schynen voort <rhyme label="b" type="m" corresp="#s3:l2">aen</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Hy daelt als de reghen <rhyme label="c" type="f">bequaeme</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Op t'veldt aen elcken <rhyme label="d" type="m">kant</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">End als de daw seer <rhyme label="c" type="f" corresp="#s3:l5">aenghenaeme</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Op dat verdorde <rhyme label="d" type="m" corresp="#s3:l6">landt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Onder sijn ryck, sullen schoon <rhyme label="a" type="f">bloeyen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die goede menschen <rhyme label="b" type="m">al</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">In paeys sullen se so langh' <rhyme label="a" type="f" corresp="#s4:l1">groeyen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Als de Maen' schynen <rhyme label="b" type="m" corresp="#s4:l2">sal</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Van d'een Zee streckt sijn ryck' <rhyme label="c" type="f">ghepresen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Tot d'ander Zee <rhyme label="d" type="m">bekent</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Van Euphrates sal't verbreydt <rhyme label="c" type="f" corresp="#s4:l5">wesen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Tot aen der węerelt <rhyme label="d" type="m" corresp="#s4:l6">eyndt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hem sullen met ghevouden <rhyme label="a" type="f">handen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Mooren vallen te <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Oocksullen kussen sijn <rhyme label="a" type="f" corresp="#s5:l1">vianden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">D'ęerde met groot' <rhyme label="b" type="m" corresp="#s5:l2">ootmoedt</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Die in't Męer hersschen, end <rhyme label="c" type="f">d'eylanden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sullen gheschenck doen <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">D'Arabers sullen met <rhyme label="c" type="f" corresp="#s5:l5">verstande</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">T'selue doen al <rhyme label="d" type="m" corresp="#s5:l6">ghemeyn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Alle Koninghen sullen <rhyme label="a" type="f">t'saeme</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hem aenbidden met <rhyme label="b" type="m">een</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Die heydnen sullen sijnen <rhyme label="a" type="f" corresp="#s6:l1">naeme</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Prysen groot ende <rhyme label="b" type="m" corresp="#s6:l2">kleen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Hy sal den aermen t'sijner <rhyme label="c" type="f">baeten</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Verlossen die nu <rhyme label="d" type="m">schreyt</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">End helpen hem die is <rhyme label="c" type="f" corresp="#s6:l5">verlaeten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Wt sijn <rhyme label="d" type="m" corresp="#s6:l6">elendicheit</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy sal den aermen end den <rhyme label="a" type="f">kleenen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ghenaedigh sijn end <rhyme label="b" type="m">goedt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Den ghenen die schreyen end <rhyme label="a" type="f" corresp="#s7:l1">weenen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Werdt hy vriendtlick end <rhyme label="b" type="m" corresp="#s7:l2">goedt</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Hy sal se voor t'ghewelt <rhyme label="c" type="f">bewaeren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">End voor bedrogh seer <rhyme label="d" type="m">quaedt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Hy sal oock t'bloedt sijner <rhyme label="c" type="f" corresp="#s7:l5">dienaeren</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Hoogh' achten vroegh end <rhyme label="d" type="m" corresp="#s7:l6">spaedt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Den aermen sal hy oock <rhyme label="a" type="f">wtgheuen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat Arabische <rhyme label="b" type="m">goudt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Se sullen hem alle haer <rhyme label="a" type="f" corresp="#s8:l1">leuen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Dienen t'saem met <rhyme label="b" type="m" corresp="#s8:l2">eenuoudt</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Dat koorn sal ouervloedigh <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">T'veld vol sijnd' ouer <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Ghelyck die boomen hoogh' <rhyme label="c" type="f" corresp="#s8:l5">gheresen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Des Libans ruysschen <rhyme label="d" type="m" corresp="#s8:l6">sal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dan sullen bloeyen in de <rhyme label="a" type="f">steden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Borgher ende <rhyme label="b" type="m">koopman</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Se sullen toenemen in <rhyme label="a" type="f" corresp="#s9:l1">vrede</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ghelyck t'groen gras voort <rhyme label="b" type="m" corresp="#s9:l2">aen</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Des Koninghs naemsal bekent <rhyme label="c" type="f">blyuen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Elck sal dies doen <rhyme label="d" type="m">vermaen</rhyme>:</l>
        <l id="s9:l7" met="4" type="-+">Sijn roem sal so langhe <rhyme label="c" type="f" corresp="#s9:l5">beklyuen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Als Sonn' end Maene <rhyme label="d" type="m" corresp="#s9:l6">staen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Der heydenen alle <rhyme label="a" type="f">gheslachten</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Werden hem <rhyme label="b" type="m">onderdaen</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">Se sullen hem gheluckigh <rhyme label="a" type="f" corresp="#s10:l1">achten</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">End prysen hem voort <rhyme label="b" type="m" corresp="#s10:l2">aen</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Sprekende: Gheloeft sij de <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Des volcks van <rhyme label="d" type="m">Israel</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Die in't werck is wonderlick <rhyme label="c" type="f" corresp="#s10:l5">seere</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Iae hy end niemant <rhyme label="d" type="m" corresp="#s10:l6">el</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
