    <categories>
      <category translation="theatre">theater</category>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ha wat zijn wy arreme <rhyme label="a" type="f">slaven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ha wat zijnwe een arrem <rhyme label="b" type="m">geslacht</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Van by dagen en ook by <rhyme label="b" type="m" corresp="#s1:l2">nacht</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Geduriglijk te moeten <rhyme label="a" type="f" corresp="#s1:l1">draven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Had ik nu een zoopje te <rhyme label="a" type="f">drinken</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Van Rosolis, of <rhyme label="b" type="m">Brandewijn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Zoo zou my 't hert vry wat moediger <rhyme label="b" type="m" corresp="#s2:l2">zijn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat ik my nu heel voel <rhyme label="a" type="f" corresp="#s2:l1">ontzinken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
