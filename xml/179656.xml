<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179656</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOm Vrinden blyft wat <rhyme label="a" type="m">staan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En hoord dit Lied eens <rhyme label="a" type="m" corresp="#s1:l1">aan</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">’t Geen ik uw zal <rhyme label="b" type="f">verhalen</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Van twee Vrouwtjes <rhyme label="c" type="m">pertinent</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">In de Nieuwendammer <rhyme label="b" type="f" corresp="#s1:l3">palen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zyn zy beide wel <rhyme label="c" type="m" corresp="#s1:l4">bekend</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haar Man die had met haar drie <rhyme label="a" type="m">Jaren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Getrouwd geweest hoord myn <rhyme label="a" type="m" corresp="#s2:l1">verklaren</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sy heeft in die drie Jaren <rhyme label="b" type="f">tyd</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Met haar Vaders knegt wild <rhyme label="c" type="m">weten</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En dat met zeer groote <rhyme label="b" type="f" corresp="#s2:l3">vleid</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Negenhonderd Gulden <rhyme label="c" type="m" corresp="#s2:l4">opgevreten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maar nu zyn zy <rhyme label="a" type="m">beide</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Al van malkaar <rhyme label="a" type="m" corresp="#s3:l1">gescheiden</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Maar nu kan zy met <rhyme label="b" type="f">fatzoen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En zeer netjes en <rhyme label="c" type="m">bequaam</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Met myn Jan haar dingen <rhyme label="b" type="f" corresp="#s3:l3">doen</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">En Tryntje Cornelis is haar <rhyme label="c" type="m" corresp="#s3:l4">naam</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">En myn Jan dat is een <rhyme label="a" type="m">plug</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En Tryn die hangt hem op de <rhyme label="a" type="m" corresp="#s4:l1">rug</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nu kan myn Jan voor haar gaan <rhyme label="b" type="f">varen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En dat al door de zee zeer <rhyme label="c" type="m">styf</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Door de wilde woeste <rhyme label="b" type="f" corresp="#s4:l3">baren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">voor dat Gebochelde schieve <rhyme label="c" type="m" corresp="#s4:l4">Wyf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maar ik denk zy zal haar niet meer <rhyme label="a" type="m">verstikken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Of in ’t Banket haar keel <rhyme label="a" type="m" corresp="#s5:l1">verslikken</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ik denk men zal nu niet meer <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En zoo men ook wel denken <rhyme label="c" type="m">kan</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Rookvlees voor Jan haar <rhyme label="b" type="f" corresp="#s5:l3">welbeminde</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En drooge Aardappels voor haar <rhyme label="c" type="m" corresp="#s5:l4">Man</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maar om nu van ons Tryn te <rhyme label="a" type="m">sluyten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sal ik nog een stukje <rhyme label="a" type="m" corresp="#s6:l1">uyten</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">dat zy wel op passen <rhyme label="b" type="f">moet</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">en dan wel met groote <rhyme label="c" type="m">iever</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">dat niet gestolen word haar <rhyme label="b" type="f" corresp="#s6:l3">goed</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Met Jan haar zoeten <rhyme label="c" type="m" corresp="#s6:l4">liever</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maar ik zal nu zonder <rhyme label="a" type="m">falen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Van ’t andere vrouwtje gaan <rhyme label="a" type="m" corresp="#s7:l1">verhalen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">en hoe dat zy dat heeft <rhyme label="b" type="f">beleid</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Al met een Schoenmaker <rhyme label="c" type="m">koen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Om haar Man te raken <rhyme label="b" type="f" corresp="#s7:l3">kwyd</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Hoord eens wat de vrouwtjes <rhyme label="c" type="m" corresp="#s7:l4">doen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Zy zyn nu zoo te <rhyme label="a" type="m">zaam</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Na een Sielkoper <rhyme label="a" type="m" corresp="#s8:l1">gegaan</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">en dat al zonder <rhyme label="b" type="f">dulden</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">en met een zeer groote <rhyme label="c" type="m">vlyd</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Verkogt dat voor drie <rhyme label="b" type="f" corresp="#s8:l3">Gulden</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Nu is zy haar Man ook <rhyme label="c" type="m" corresp="#s8:l4">kwyd</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En haer Naem wild <rhyme label="a" type="m">weten</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">die is ons niet <rhyme label="a" type="m" corresp="#s9:l1">vergeten</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">en haer Naem is Wiepje <rhyme label="b" type="f">dan</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">de Schoenmaker niet <rhyme label="c" type="m">vergeten</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Japie de <rhyme label="b" type="f" corresp="#s9:l3">Lulleman</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">die hy verkoopt wild <rhyme label="c" type="m" corresp="#s9:l4">weten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
