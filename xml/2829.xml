  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Schoen lief en <rhyme label="a" type="m">mach</rhyme></l>
        <l id="s1:l2" met="3" type="-+">u hart beweegen <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s1:l3" met="2" type="-+">mijn swaer <rhyme label="a" type="m" corresp="#s1:l1">geklach</rhyme></l>
        <l id="s1:l4" met="3" type="-+">en woerde vol <rhyme label="b" type="m" corresp="#s1:l2">verdriet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Ey neemt toch <rhyme label="c" type="m">acht</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Op mijn lief vol <rhyme label="d" type="m">torment</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Tis In u <rhyme label="c" type="m" corresp="#s1:l5">macht</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Eewich soelaes <rhyme label="d" type="m" corresp="#s1:l6">apsent</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">U hert <rhyme label="a" type="m">sertein</rhyme></l>
        <l id="s2:l2" met="3" type="-+">moet Iser syn of <rhyme label="b" type="m">stael</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Of soen ick <rhyme label="a" type="m" corresp="#s2:l1">mein</rhyme></l>
        <l id="s2:l4" met="3" type="-+">steen achtich <rhyme label="b" type="m" corresp="#s2:l2">altemael</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Och waert soe <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">gy sout sin <rhyme label="d" type="m">gemoeveert</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Duer swaer <rhyme label="c" type="m" corresp="#s2:l5">voerdriet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat mijn Jonck hert duer <rhyme label="d" type="m" corresp="#s2:l6">beert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Snachs als men <rhyme label="a" type="m">placht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">te slaepen In sin <rhyme label="b" type="m">rust</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Dan maeck <rhyme label="a" type="m" corresp="#s3:l1">gewach</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In mijn trueren <rhyme label="b" type="m" corresp="#s3:l2">geblust</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Ick neem den <rhyme label="c" type="m">ganck</rhyme></l>
        <l id="s3:l6" met="3" type="-+">u huis schoen lief <rhyme label="d" type="m">ontrent</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Daer maeckt <rhyme label="c" type="m" corresp="#s3:l5">geklach</rhyme></l>
        <l id="s3:l8" met="3" type="-+">soe meenich <rhyme label="d" type="m" corresp="#s3:l6">Isterement</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Die sterren <rhyme label="a" type="m">klaer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">die straele van de <rhyme label="b" type="m">maen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Schijnen <rhyme label="a" type="m" corresp="#s4:l1">voerwaer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">beweecht doer mijn <rhyme label="b" type="m" corresp="#s4:l2">bestaen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">maer u <rhyme label="c" type="m">gemoet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">blift altijt <rhyme label="d" type="m">opstinaet</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Sint wat gij <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">prinses eert valt te <rhyme label="d" type="m" corresp="#s4:l6">laet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
