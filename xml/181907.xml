      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Eenzaam bosje hoor mijn klachten</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WAt al heil is <rhyme label="a" type="f">toegezonden</rhyme>:</l>
        <l id="s1:l2" met="4" type="+-">Aan Prins Weilburg en <rhyme label="b" type="m">Carolyn</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Hoe juigt de Prinsgezinde <rhyme label="a" type="f" corresp="#s1:l1">tongen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Die haar trouw hebben <rhyme label="b" type="m" corresp="#s1:l2">gezien</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">In het Haegje schoon <rhyme label="c" type="f">verheeven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">De vreugde nooit na eisch <rhyme label="c" type="f" corresp="#s1:l5">beschreeven</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Van Burgery of ander <rhyme label="b" type="m" corresp="#s1:l2">Lien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">’t Was al Viva geluk lang <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Binnen het jaar een jonge <rhyme label="b" type="m">Zoon</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">De Hemel heeft haar gunst <rhyme label="a" type="f" corresp="#s2:l1">gegeeven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Waarder als Paerels aan haar <rhyme label="b" type="m" corresp="#s2:l2">Kroon</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Een jonge Prins te <rhyme label="c" type="f">gelyke</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">By schat van eenig <rhyme label="c" type="f" corresp="#s2:l5">Koningryke</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Buigt waerd’ Ouders voor Gods <rhyme label="b" type="m" corresp="#s2:l2">Troon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Den Hemel heeft na wensch op <rhyme label="a" type="f">aarde</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Door natuur u mee <rhyme label="b" type="m">gedeelt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat gy ziet het zaad van <rhyme label="a" type="f" corresp="#s3:l1">waerde</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Uit u beidens Bloed <rhyme label="b" type="m" corresp="#s3:l2">geteelt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Veels geluk Doorlugte <rhyme label="c" type="f">Vader</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Eenen Wel Bron van den <rhyme label="c" type="f" corresp="#s3:l5">Ader</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Waar uit eerst vloeide u eigen <rhyme label="b" type="m" corresp="#s3:l2">Beelt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dus wenscht men ook de blyde <rhyme label="a" type="f">Moeder</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Met haar eerst geteelde <rhyme label="b" type="m">Spruit</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Den Hemelvoogt tot een <rhyme label="a" type="f" corresp="#s4:l1">behoeder</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En dat nooit liefde het herte <rhyme label="b" type="m" corresp="#s4:l2">sluit</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Dat zy haar Soontje ziet <rhyme label="c" type="f">opgroeijen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En Jaarlyks haar Stam mag <rhyme label="c" type="f" corresp="#s4:l5">bloeijen</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Tot dat de tyd het baaren <rhyme label="b" type="m" corresp="#s4:l2">sluit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wy wensche ook onze Prins van <rhyme label="a" type="f">waarde</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Met zyn Oomschap veel <rhyme label="b" type="m">geluk</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Dat ook zyn Ega op der <rhyme label="a" type="f" corresp="#s5:l1">Aarde</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Voor hem opgroeit zonder eenig <rhyme label="b" type="m" corresp="#s5:l2">druk</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Dat hy door liefden ook mag <rhyme label="c" type="f">paaren</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">In ’t bloeien van zyn Lente <rhyme label="c" type="f" corresp="#s5:l5">Jaaren</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Tot Neerlands Seegen en <rhyme label="b" type="m" corresp="#s5:l2">geluk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Voorts wensche wy onse <rhyme label="a" type="f">Mogentheeden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En al de vrienden van het <rhyme label="b" type="m">kind</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">De liefde van <rhyme label="a" type="f" corresp="#s6:l1">Eendragtigheeden</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Vader en Moeder wel <rhyme label="b" type="m" corresp="#s6:l2">gesind</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Jehovah zegent haere <rhyme label="c" type="f">Spruit</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Met die wensch wil ik toe <rhyme label="c" type="f" corresp="#s6:l5">sluyte</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Lang bloeid d’ Orange spruid <rhyme label="b" type="m" corresp="#s6:l2">bemind</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
