<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161366</id>
    <date>
      <year-start>1690</year-start>
      <year-end>1690</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Rein maagdje met eer</melody>
  <author>Barent Pietersz. Kompas</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh overgoede <rhyme label="a" type="f">Vader</rhyme>!</l>
        <l id="s1:l2" met="3" type="+-">Sie doch nu eens <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">My, die hier tot u <rhyme label="a" type="f" corresp="#s1:l1">nader</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Wilt my niet <rhyme label="b" type="m" corresp="#s1:l2">versmaen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Verhoor, o Heer mijn <rhyme label="c" type="m">klacht</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">Want, ick gansch swack en <rhyme label="d" type="f">teder</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Bevind' mijn kleyne <rhyme label="c" type="m" corresp="#s1:l5">kracht</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dies val ick voor u <rhyme label="d" type="f" corresp="#s1:l6">neder</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Seer veel zijn mijn <rhyme label="a" type="f">vyanden</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Heer 't is u <rhyme label="b" type="m">bekent</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hoe dat sy my <rhyme label="a" type="f" corresp="#s2:l1">aenranden</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En steedts sijn <rhyme label="b" type="m" corresp="#s2:l2">omtrent</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">De Wereldt, Satan         't <rhyme label="c" type="m">Vleesch</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die locken my tot <rhyme label="d" type="f">sonden</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Sy zijn 't, daer ick voor <rhyme label="c" type="m" corresp="#s2:l5">vrees</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat sy my sullen <rhyme label="d" type="f" corresp="#s2:l6">wonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doch wilt haer krachten <rhyme label="a" type="f">breecken</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Geef, o stercke <rhyme label="b" type="m">Heer</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">Dat ick haer loose <rhyme label="a" type="f" corresp="#s3:l1">treecken</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Doch ontvlieden <rhyme label="b" type="m" corresp="#s3:l2">leer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">O hulp voor kleyn en <rhyme label="c" type="m">groot</rhyme>!</l>
        <l id="s3:l6" met="3" type="-+">Ick kom om bystandt <rhyme label="d" type="f">smeecken</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Laet doch uw' hulp' in <rhyme label="c" type="m" corresp="#s3:l5">noodt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">My nimmermeer <rhyme label="d" type="f" corresp="#s3:l6">ontbreecken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vergeef, o Heer mijn <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Dat ick 's Vleeschs <rhyme label="b" type="m">bekoor</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">t' Eeniger tijdt of <rhyme label="a" type="f" corresp="#s4:l1">stonde</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Gaf soo veel <rhyme label="b" type="m" corresp="#s4:l2">gehoor</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">'t Berouwt my, dat ick <rhyme label="c" type="m">oyt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van u ben <rhyme label="d" type="f">afgeweecken</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Gedenck, o Heere <rhyme label="c" type="m" corresp="#s4:l5">noyt</rhyme>!</l>
        <l id="s4:l8" met="3" type="-+">Aen dese mijn <rhyme label="d" type="f" corresp="#s4:l6">gebreecken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'k Hoop voortaen op te <rhyme label="a" type="f">passen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Heere help my <rhyme label="b" type="m">dan</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">En laet mijn krachten <rhyme label="a" type="f" corresp="#s5:l1">wassen</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Dat ick als een <rhyme label="b" type="m" corresp="#s5:l2">Man</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Mijn vyandt ga te <rhyme label="c" type="m">keer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met 't geestelijcke <rhyme label="d" type="f">wapen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Op dat hy my niet <rhyme label="c" type="m" corresp="#s5:l5">meer</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Houdt in de sonde <rhyme label="d" type="f" corresp="#s5:l6">slapen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Leer my voor alle <rhyme label="a" type="f">dingen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Mijnen noodt en <rhyme label="b" type="m">druck</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Heede voor u <rhyme label="a" type="f" corresp="#s6:l1">bringen</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Met dit <rhyme label="b" type="m" corresp="#s6:l2">wapen-stuck</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Daer kan men alle <rhyme label="c" type="m">pijl</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Des boosen mee <rhyme label="d" type="f">uytblussen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die hy, tot mijn <rhyme label="c" type="m" corresp="#s6:l5">onheyl</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Sou schieten, <rhyme label="d" type="f" corresp="#s6:l6">ondertusschen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
