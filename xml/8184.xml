    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WIe sou sijn jeuchdigh hert en <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Niet trecken tot de soete <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die sulcken schoonen <rhyme label="a" type="m" corresp="#s1:l1">Engelin</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Aanschouwt met <rhyme label="b" type="f">oogen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Ick meen het over-schoone <rhyme label="c" type="m">Beeld</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Catrina van de deucht <rhyme label="c" type="m" corresp="#s1:l5">geteelt</rhyme>;</l>
        <l id="s1:l7" met="4" type="-+">In wiens gelaat Diana <rhyme label="c" type="m" corresp="#s1:l5">speelt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Vol van <rhyme label="b" type="f" corresp="#s1:l4">meedoogen</rhyme>.</l>
      </lg>
