  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mag ich vngefall erwerenn a ch <rhyme label="a" type="m">nitt</rhyme> /</l>
        <l id="s1:l2" met="2" type="-+">z  dieser <rhyme label="b" type="m">frist</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">Moß alletzeitt hertz / m tt vnnd <rhyme label="c" type="m">syn</rhyme> /</l>
        <l id="s1:l4" met="4" type="-+">Jn grossem tra renn vnd ellende <rhyme label="c" type="m" corresp="#s1:l3">sein</rhyme> /</l>
        <l id="s1:l5" met="4" type="-+">Auff erden iß kein schwerer <rhyme label="c" type="m" corresp="#s1:l3">pein</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Och das ich k ndtt vonn hertzenn gr <rhyme label="a" type="m">ndt</rhyme> /</l>
        <l id="s2:l2" met="2" type="-+">So fill der st <rhyme label="b" type="m">ndt</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">reddenn mit dir nach meins hertz <rhyme label="c" type="m">boger</rhyme> /</l>
        <l id="s2:l4" met="4" type="-+">Jch hoff die zeitt vnnd st ndtt k mpt <rhyme label="c" type="m" corresp="#s2:l3">schier</rhyme> /</l>
        <l id="s2:l5" met="4" type="-+">A ff erdenn ich nitt liebers <rhyme label="c" type="m" corresp="#s2:l3">boger</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Das will ich noch bekennenn th <rhyme label="a" type="m">nn</rhyme> /</l>
        <l id="s3:l2" met="2" type="-+">Das sich die hochste <rhyme label="b" type="m">kron</rhyme> /</l>
        <l id="s3:l3" met="4" type="-+">die ich zu den fre den hab a <rhyme label="c" type="m">ßerweltt</rhyme> /</l>
        <l id="s3:l4" met="4" type="-+">Vnd all mein vertra wenn auff dich <rhyme label="c" type="m" corresp="#s3:l3">gesteltt</rhyme> /</l>
        <l id="s3:l5" met="4" type="-+">Jch weiß kein lieber a ff dieser <rhyme label="c" type="m" corresp="#s3:l3">weltt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gott iß mein ze <rhyme label="a" type="m">gh</rhyme> /</l>
        <l id="s4:l2" met="2" type="-+">das ich nitt <rhyme label="b" type="m">leich</rhyme> /</l>
        <l id="s4:l3" met="4" type="-+">Wan ich dich nitt ansehe off stettes mag bei dir <rhyme label="c" type="m">sein</rhyme> /</l>
        <l id="s4:l4" met="4" type="-+">So leidt mein hertz die groste <rhyme label="c" type="m" corresp="#s4:l3">pein</rhyme> /</l>
        <l id="s4:l5" met="4" type="-+">Das ich kein st ndtt mag frolich <rhyme label="c" type="m" corresp="#s4:l3">sein</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">An dir ich kein vnfall en sp <rhyme label="a" type="m">ir</rhyme> /</l>
        <l id="s5:l2" met="2" type="-+">ehe ich verloir / dein lieb vnnd g <rhyme label="b" type="m">nst</rhyme></l>
        <l id="s5:l3" met="4" type="-+">a ß hertzenn br <rhyme label="c" type="m">nst</rhyme> /</l>
        <l id="s5:l4" met="4" type="-+">So wehr mir mein  <rhyme label="c" type="m" corresp="#s5:l3">beschwerett</rhyme> /</l>
        <l id="s5:l5" met="4" type="-+">Vnd kans  vergessenn nh noch <rhyme label="c" type="m" corresp="#s5:l3">nymmermehe</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hertzliches lieb / dein stoltzer <rhyme label="a" type="m">leib</rhyme> /</l>
        <l id="s6:l2" met="2" type="-+">mein zeitt verdreiff / Wendtt mir mein <rhyme label="b" type="m">schmertz</rhyme> /</l>
        <l id="s6:l3" met="4" type="-+">trewlichs <rhyme label="c" type="m">hertz</rhyme> /</l>
        <l id="s6:l4" met="4" type="-+">Jch verwacht altzeitt des gl ckes <rhyme label="c" type="m" corresp="#s6:l3">tagh</rhyme> /</l>
        <l id="s6:l5" met="4" type="-+">erredde mich baldt / ehe ich <rhyme label="c" type="m" corresp="#s6:l3">verzagh</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ein trostlich wortt sei z  mir <rhyme label="a" type="m">sprach</rhyme> /</l>
        <l id="s7:l2" met="2" type="-+">ich habs <rhyme label="b" type="m">betrachtt</rhyme> /</l>
        <l id="s7:l3" met="4" type="-+">hertzliebste mein / dein schware <rhyme label="c" type="m">clag</rhyme> /</l>
        <l id="s7:l4" met="4" type="-+">Jch bitt schweig still vnnd nemantz <rhyme label="c" type="m" corresp="#s7:l3">sag</rhyme> /</l>
        <l id="s7:l5" met="4" type="-+">Das ich dir mein getre wes hertz <rhyme label="c" type="m" corresp="#s7:l3">nachthrag</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Betrew nemantz fill / Jn diesem <rhyme label="a" type="m">spill</rhyme> /</l>
        <l id="s8:l2" met="2" type="-+">D  sichst doch <rhyme label="b" type="m">woll</rhyme> /</l>
        <l id="s8:l3" met="4" type="-+">das sich die bose schnode  <rhyme label="c" type="m">weltt</rhyme> /</l>
        <l id="s8:l4" met="4" type="-+">So gar a ff das liegen hatt <rhyme label="c" type="m" corresp="#s8:l3">gestallt</rhyme> /</l>
        <l id="s8:l5" met="4" type="-+">Sich dich woll f r / mein getrewer <rhyme label="c" type="m" corresp="#s8:l3">hildt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Och weiblichs bildt / damitt schweig <rhyme label="a" type="m">still</rhyme> /</l>
        <l id="s9:l2" met="2" type="-+">vnd zweiffell nitt <rhyme label="b" type="m">daran</rhyme> /</l>
        <l id="s9:l3" met="4" type="-+">Das es erf r des kleffers <rhyme label="c" type="m">schar</rhyme> /</l>
        <l id="s9:l4" met="4" type="-+">Die lieb th itt mich bezwingenn <rhyme label="c" type="m" corresp="#s9:l3">gar</rhyme> /</l>
        <l id="s9:l5" met="4" type="-+">noch soll es werdenn kein mensch <rhyme label="c" type="m" corresp="#s9:l3">gewar</rhyme>.</l>
      </lg>
    </lg>
