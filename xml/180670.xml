<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180670</id>
    <date>
      <year-start>1688</year-start>
      <year-end>1688</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik zucht zucht op zucht</melody>
  <author>Philippus van Sorgen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoewel den grooten <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van niemant op der <rhyme label="b" type="f">Aerde</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">(Volgens sijn swaer <rhyme label="a" type="m" corresp="#s1:l1">gebodt</rhyme>)</l>
        <l id="s1:l4" met="3" type="-+">Geroemt wort na sijn <rhyme label="b" type="f" corresp="#s1:l2">waerde</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Nochtans moet sijnen <rhyme label="c" type="f">zegen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die hy aen ons <rhyme label="d" type="m">betoont</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hem worden aller <rhyme label="c" type="f" corresp="#s1:l5">wegen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Met Daanckbaerheyt <rhyme label="d" type="m" corresp="#s1:l6">beloont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn Ziel! looft dan den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wilt hem doch altijt <rhyme label="b" type="f">vresen</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Prijst hem, en geef hem <rhyme label="a" type="m" corresp="#s2:l1">eer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En wilt hem danckbaer <rhyme label="b" type="f" corresp="#s2:l2">wesen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Voor alle de <rhyme label="c" type="f">weldaden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die hy in <rhyme label="d" type="m">overvloet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">(Uyt loutere <rhyme label="c" type="f" corresp="#s2:l5">genaden</rhyme>)</l>
        <l id="s2:l8" met="3" type="-+">Aen u soo goedigh <rhyme label="d" type="m" corresp="#s2:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy is dien Godt <rhyme label="a" type="m">alleyn</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">Van wien dat alle <rhyme label="b" type="f">dingen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">(Als uyt een klaer <rhyme label="a" type="m" corresp="#s3:l1">Fonteyn</rhyme>)</l>
        <l id="s3:l4" met="3" type="-+">Seer overvloedigh <rhyme label="b" type="f" corresp="#s3:l2">springen</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s3:l5">bewijsen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s3:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy geeft my niet <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hier op der Werd’ het <rhyme label="b" type="f">leven</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Maer heeft my oock met <rhyme label="a" type="m" corresp="#s4:l1">een</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Een Menschen-ziel <rhyme label="b" type="f" corresp="#s4:l2">gegeven</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s4:l5">bewijsen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s4:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hy heeft my boven <rhyme label="a" type="m">dien</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sijn kennis laten <rhyme label="b" type="f">leeren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En laet mijn daeg’licks <rhyme label="a" type="m" corresp="#s5:l1">sien</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hoe dat men hem moet <rhyme label="b" type="f" corresp="#s5:l2">eeren</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s5:l5">bewijsen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s5:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O! aldergrootst <rhyme label="a" type="m">geluck</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat Godt in mijn <rhyme label="b" type="f">ellenden</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En mijn seer swaren <rhyme label="a" type="m" corresp="#s6:l1">druck</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Komt een Verlosser <rhyme label="b" type="f" corresp="#s6:l2">senden</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s6:l5">bewijsen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s6:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O! wat een groot <rhyme label="a" type="m">vermaeck</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Kan nu mijn ziel <rhyme label="b" type="f">genieten</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Nu Godt soo goeden <rhyme label="a" type="m" corresp="#s7:l1">saeck</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Op haer komt uyt te <rhyme label="b" type="f" corresp="#s7:l2">gieten</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s7:l5">bewijsen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s7:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Want daer ick lach <rhyme label="a" type="m">versmacht</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">In mijne vuyle <rhyme label="b" type="f">wonden</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Heeft hy aen my <rhyme label="a" type="m" corresp="#s8:l1">gedacht</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En sijnen Soon <rhyme label="b" type="f" corresp="#s8:l2">gesonden</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s8:l5">bewijsen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s8:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick hadde door mijn <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Verdient veel sware <rhyme label="b" type="f">slagen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Maer hy heeft tot <rhyme label="a" type="m" corresp="#s9:l1">versoen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Al mijne schult <rhyme label="b" type="f" corresp="#s9:l2">gedragen</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s9:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s9:l5">bewijsen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s9:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ach! wat een grooten <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ach! wat een rijcken <rhyme label="b" type="f">segen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Dat hy my door sijn <rhyme label="a" type="m" corresp="#s10:l1">bloet</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Versoening’ heeft <rhyme label="b" type="f" corresp="#s10:l2">gekregen</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s10:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s10:l5">bewijsen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s10:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Hy heeft voor my den <rhyme label="a" type="m">strijt</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Dien swaren strijt <rhyme label="b" type="f">gestreden</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">My van de doodt <rhyme label="a" type="m" corresp="#s11:l1">bevrijt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Voor my de straf <rhyme label="b" type="f" corresp="#s11:l2">geleden</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s11:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s11:l5">bewijsen</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s11:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Hy heeft my hier <rhyme label="a" type="m">gekocht</rhyme>;</l>
        <l id="s12:l2" met="3" type="-+">Ia met den doodt te <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Op dat hy alsoo <rhyme label="a" type="m" corresp="#s12:l1">mocht</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Mijn eeuwigh heyl <rhyme label="b" type="f" corresp="#s12:l2">verwerven</rhyme>.</l>
        <l id="s12:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s12:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s12:l5">bewijsen</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s12:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Hy is mijn goeden <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Mijn eeuwigen <rhyme label="b" type="f">behoeder</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Die my bemint soo <rhyme label="a" type="m" corresp="#s13:l1">seer</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Hy is mijn <rhyme label="b" type="f" corresp="#s13:l2">Zielen-voeder</rhyme>.</l>
        <l id="s13:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s13:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s13:l5">bewijsen</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s13:l6">vermach</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Ick sal door sijn <rhyme label="a" type="m">geleyt</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Ten Hemel zijn <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">En daer in <rhyme label="a" type="m" corresp="#s14:l1">eeuwigheyt</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">In hem, en door hem <rhyme label="b" type="f" corresp="#s14:l2">leven</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">Daerom sal ick hem <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">En dancken nacht en <rhyme label="d" type="m">dach</rhyme>;</l>
        <l id="s14:l7" met="3" type="-+">Hem altijt eer <rhyme label="c" type="f" corresp="#s14:l5">bewijsen</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Soo veel als ick <rhyme label="d" type="m" corresp="#s14:l6">vermach</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
