    <tags/>
    <melody>Jaapje sta stil</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Jaepje sta stil: Iaepje sta <rhyme label="a" type="m">stil</rhyme>:</l>
        <l id="s1:l2" met="4" type="-+">Hoord eens wat ik u moet <rhyme label="b" type="f">vertellen</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Iaepje sta stil: Iaepje sta <rhyme label="a" type="m" corresp="#s1:l1">stil</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Hoord eens wat ik u zeggen <rhyme label="a" type="m" corresp="#s1:l1">wil</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Laest liep ons Kaetje met <rhyme label="c" type="f">Krelis</rhyme>:</l>
        <l id="s1:l6" met="4" type="-+">Zo zoetjes door de kramen men in 't <rhyme label="d" type="m">stil</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">'k Geloof zy gingen aen den <rhyme label="d" type="m" corresp="#s1:l6">dril</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wat dunkje van Iaepje sta <rhyme label="d" type="m" corresp="#s1:l6">stil</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Malle Zottin: 't Is de <rhyme label="a" type="m">min</rhyme>:</l>
        <l id="s2:l2" met="4" type="-+">Die de Ionkheid trekt te <rhyme label="b" type="f">zamen</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Malle Zottin: 't Is de <rhyme label="a" type="m" corresp="#s2:l1">min</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Sta stil en doet 'er myn <rhyme label="a" type="m" corresp="#s2:l1">zin</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Gy bent zoo een aerdig <rhyme label="c" type="f">Meisje</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Willen wy 't zamen ook eens <rhyme label="d" type="m">doen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Gaen om een zwiertje met <rhyme label="d" type="m" corresp="#s2:l6">fatsoen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Sta stil of je maekt my <rhyme label="d" type="m" corresp="#s2:l6">groen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Iaepje wat praet, Iaepje wat <rhyme label="a" type="m">praet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ik durf het zeker niet <rhyme label="b" type="f">wagen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Iaepje wat praet: Iaepje wat <rhyme label="a" type="m" corresp="#s3:l1">praet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Sta stil: 't Is nu te <rhyme label="a" type="m" corresp="#s3:l1">laet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Myn Moertje zou zeker <rhyme label="c" type="f">kyven</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Als ik by tyds niet thuys en <rhyme label="d" type="m">was</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">De Buuren quamen 'er aen te <rhyme label="d" type="m" corresp="#s3:l6">pas</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ey Iaepje sta stil wat <rhyme label="d" type="m" corresp="#s3:l6">ras</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Truitje zoet: onnoselen <rhyme label="a" type="m">bloed</rhyme>:</l>
        <l id="s4:l2" met="4" type="-+">Wat Bruytje dat oude <rhyme label="b" type="f">Besje</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Truitje zoet: onnoselen <rhyme label="a" type="m" corresp="#s4:l1">bloed</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Sta stil wat hebje weer <rhyme label="a" type="m" corresp="#s4:l1">spoet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Kom Meisje ik zal u <rhyme label="c" type="f">Tracteren</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Op Wyn en Bier al watje <rhyme label="d" type="m">lust</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">En stellen jou Moertje wel <rhyme label="d" type="m" corresp="#s4:l6">gerust</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Sta stil voldoet 'er myn <rhyme label="d" type="m" corresp="#s4:l6">lus</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Aerdige Iaep, wat benje een <rhyme label="a" type="m">Knaep</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Waer zou ik met u <rhyme label="b" type="f">lopen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Aerdige Iaep, wat benje een <rhyme label="a" type="m" corresp="#s5:l1">Knaep</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Sta stil jou lossen <rhyme label="a" type="m" corresp="#s5:l1">aep</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Maer willen wy eens gaen <rhyme label="c" type="f">keyken</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Om Magito zyn konst te <rhyme label="d" type="m">sien</rhyme>:</l>
        <l id="s5:l7" met="4" type="-+">En wat 'er voorders zal <rhyme label="d" type="m" corresp="#s5:l6">geschien</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Sa stil Iaep wil ter niet <rhyme label="d" type="m" corresp="#s5:l6">vlien</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'t Is naer de pret van <rhyme label="a" type="m">Marionet</rhyme>:</l>
        <l id="s6:l2" met="4" type="-+">Het spelletje van Ian <rhyme label="b" type="f">Klaese</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">'t Is naer de pret van <rhyme label="a" type="m" corresp="#s6:l1">Marionet</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Sta stil Truy met een <rhyme label="a" type="m" corresp="#s6:l1">set</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Daer hebje de klugt al <rhyme label="c" type="f">gaende</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Zy kruypen uit en in de <rhyme label="d" type="m">Zak</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Ziet eens wat is Ian Klaese <rhyme label="d" type="m" corresp="#s6:l6">zwak</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Sta stil nu Truitje tot <rhyme label="d" type="m" corresp="#s6:l6">strak</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ia zoo waer, dat is wel <rhyme label="a" type="m">raer</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Zy slaen malkaer met <rhyme label="b" type="f">stokken</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Ia zoo waer, dat is wel <rhyme label="a" type="m" corresp="#s7:l1">raer</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Sta stil myn lieve <rhyme label="a" type="m" corresp="#s7:l1">Uaer</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">De droes wie kan hier <rhyme label="c" type="f">scheiden</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ik wagt 'er aen 't gaet zoo 't <rhyme label="d" type="m">wil</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Al sloeg myn Moertje my op myn <rhyme label="d" type="m" corresp="#s7:l6">bil</rhyme>;</l>
        <l id="s7:l8" met="3" type="-+">Kom lustig nou Iaepje sta <rhyme label="d" type="m" corresp="#s7:l6">stil</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Zoete Meid: wat ben ik <rhyme label="a" type="m">verblyd</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Nu gy my 't Iawoord hebt <rhyme label="b" type="f">gegeven</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Zoete Meid, wat ben ik <rhyme label="a" type="m" corresp="#s8:l1">verblyd</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Sta stil nu is het <rhyme label="a" type="m" corresp="#s8:l1">tyd</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Wy zullen Zaem eens <rhyme label="c" type="f">dansen</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Gy weet wel wat ik zeggen <rhyme label="d" type="m">wil</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Gy gaet met Iaepje nu aen den <rhyme label="d" type="m" corresp="#s8:l6">dril</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Het deuntje van Iaepje sta <rhyme label="d" type="m" corresp="#s8:l6">stil</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
