<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180275</id>
    <date>
      <year-start>1783</year-start>
      <year-end>1783</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het kuipertje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EEn Kuypertje vol jaloerse <rhyme label="a" type="m">pyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Vreyde een Meisje jong van <rhyme label="b" type="f">jaren</rhyme> ,</l>
        <l id="s1:l3" met="4" type="-+">En hy dagte jaer Man te <rhyme label="a" type="m" corresp="#s1:l1">zyn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Maer hy kon haer niet <rhyme label="b" type="f" corresp="#s1:l2">behagen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Hy was een vreyer na haer <rhyme label="c" type="m">wil</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Vreyde het Meisje in het <rhyme label="c" type="m" corresp="#s1:l5">stil</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Kuypertje kuypertje goede <rhyme label="d" type="m">gezel</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Werkt en maekt jou kuypje <rhyme label="d" type="m" corresp="#s1:l7">wel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Eens in de grond' van eene <rhyme label="a" type="m">kuyp</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Sat daer een vreyer vol van <rhyme label="b" type="f">minnen</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Terwijl dat d' Aer stond op de <rhyme label="a" type="m" corresp="#s2:l1">sluys</rhyme>;</l>
        <l id="s2:l4" met="4" type="-+">Trad deze Gryzaert de winkel <rhyme label="b" type="f" corresp="#s2:l2">binne</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Door de zoete min <rhyme label="c" type="m">volmaekt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Het voorwerp van de <rhyme label="c" type="m" corresp="#s2:l5">minnespraek</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Kuipertje Kuipertje goede <rhyme label="d" type="m">Gezel</rhyme>:</l>
        <l id="s2:l8" met="4" type="+-">Werkt en maekt jou kuypje <rhyme label="d" type="m" corresp="#s2:l7">wel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Kuypertje heeft geen <rhyme label="a" type="m">agterdogt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hy werkt altoos even <rhyme label="b" type="f">spoedig</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">Maer ziet terwyl dat hy <rhyme label="a" type="m" corresp="#s3:l1">wrogt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Van zyn Hand niet minder <rhyme label="b" type="f" corresp="#s3:l2">moedig</rhyme>!</l>
        <l id="s3:l5" met="4" type="-+">En op zijn lieve Vrijsters <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Geeft hy een Soen tot minne <rhyme label="c" type="m" corresp="#s3:l5">pand</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Kuypertje Kuypertje goede <rhyme label="d" type="m">gezel</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Werkt en maekt jou kuypje <rhyme label="d" type="m" corresp="#s3:l7">wel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">d' Een is verslonde door zijn <rhyme label="a" type="m">brand</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En wel te vreede van <rhyme label="b" type="f">beginne</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Niet vernoegt met een <rhyme label="a" type="m" corresp="#s4:l1">hand</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Tragt om nog wat meer te <rhyme label="b" type="f" corresp="#s4:l2">winnen</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">En tegens het Oor van de oude <rhyme label="c" type="m">pry</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Gaf hy haer nog een Soen of <rhyme label="c" type="m" corresp="#s4:l5">dry</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Kuypertje Kuypertje goede <rhyme label="d" type="m">gezel</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Werkt en maekt jou kuypje <rhyme label="d" type="m" corresp="#s4:l7">wel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
