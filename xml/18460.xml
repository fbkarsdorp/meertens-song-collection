<song>
  <meta>
    <id>18460</id>
    <date>
      <year-start>1903</year-start>
      <year-end>1908</year-end>
    </date>
    <tags/>
    <melody>Slaap kindje slaap</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Slaap, kindje slaap;</l>
        <l id="s1:l2">daar buiten loopt een schaap,</l>
        <l id="s1:l3">een schaap met witte voetjes,</l>
        <l id="s1:l4">dat drinkt zijn melk zoo zoetjes;</l>
        <l id="s1:l5">slaap, kindje slaap.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Slaap, kindje slaap;</l>
        <l id="s2:l2">daar buiten loopt een schaap,</l>
        <l id="s2:l3">daar buiten loopt een bonte koe,</l>
        <l id="s2:l4">het kindje doet zijn oogjes toe;</l>
        <l id="s2:l5">slaap, kindje slaap.</l>
      </lg>
    </lg>
  </text>
</song>
