  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Overal vrolykheid,</l>
        <l id="s1:l2">Overal lust.</l>
        <l id="s1:l3">Maegden van fier gelaet,</l>
        <l id="s1:l4">Knapen zoo vroom en draet</l>
        <l id="s1:l5">Overal vrolykheid,</l>
        <l id="s1:l6">Overal lust.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Vlaenderen, elders mag 't</l>
        <l id="s2:l2">Schooner wel zyn.</l>
        <l id="s2:l3">Herten zoo lief, zoo teer</l>
        <l id="s2:l4">Vind ik wel nergens weer.</l>
        <l id="s2:l5">Vlaenderen, elders mag 't</l>
        <l id="s2:l6">Schooner wel zyn.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">Vlaenderen, boven al</l>
        <l id="s3:l2">Heb ik u lief;</l>
        <l id="s3:l3">Gy zyt myn lust, myn smert,</l>
        <l id="s3:l4">Gy ligt my diep in 't hert.</l>
        <l id="s3:l5">Vlaenderen, boven al</l>
        <l id="s3:l6">Heb ik u lief.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1">Vlaenderen, 't allen tyd</l>
        <l id="s4:l2">Blyft ge my lief.</l>
        <l id="s4:l3">Neem van my hert en hand,</l>
        <l id="s4:l4">Neemt myne trouw te pand;</l>
        <l id="s4:l5">Vlaenderen, 't allen tyd</l>
        <l id="s4:l6">Blyft ge my lief.</l>
      </lg>
    </lg>
  </text>
