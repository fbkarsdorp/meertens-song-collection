<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24926</id>
    <date>
      <year-start>1390</year-start>
      <year-end>1400</year-end>
    </date>
    <location>
      <orginal-name>brugge</orginal-name>
      <standard-name>Brugge, Belgium</standard-name>
