<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>61336</id>
    <date>
      <year-start>1810</year-start>
      <year-end>1810</year-end>
    </date>
    <tags>
      <tag standard-name="amusementslied" translation="amusement song">amusementslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Moeder geef myn eenen Man,</l>
        <l id="s1:l2">Kise mise moese spatie,</l>
        <l id="s1:l3">Die ons de tyd verdryven kan,</l>
        <l id="s1:l4">Soete lieve myd en die laat kussen</l>
        <l id="s1:l5">Van bombe latie Peeperkoek</l>
        <l id="s1:l6">Van bombe latie pa valdera.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Dogter gy zyt veel te klein,</l>
        <l id="s2:l2">Kise mise moese spatie.</l>
        <l id="s2:l3">Soete leve meid en die meid die laat</l>
        <l id="s2:l4">haar kussen</l>
        <l id="s2:l5">Van bombe latie Peeperkoek</l>
        <l id="s2:l6">Van bombe latie pa valdera.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">Moeder ik ben groot genoeg,</l>
        <l id="s3:l2">Kise mise moese spatie,</l>
        <l id="s3:l3">Want ik heb van Jantje Paf geproevd</l>
        <l id="s3:l4">Soete lieve Meid die laat haar kussen</l>
        <l id="s3:l5">Van bombe latie Peeperkoek</l>
        <l id="s3:l6">Van bombe latie pa valdera.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1">Heb gy van Jantje Pav geproevd</l>
        <l id="s4:l2">Kise Mise Moese spatie,</l>
        <l id="s4:l3">Dan heb gy voor 9 Maanden genoeg.</l>
        <l id="s4:l4">Soete lieve meid die laat haar kusen</l>
        <l id="s4:l5">Van bombe latie peeperkoek</l>
        <l id="s4:l6">Van bombe latie pa valdera.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1">Het is niet in het bed geschiet,</l>
        <l id="s5:l2">Kise Mise Moese Spatie</l>
        <l id="s5:l3">Het koste een stoter in het lange riet</l>
        <l id="s5:l4">Soete lieve meid die laat haar kussen</l>
        <l id="s5:l5">Van bombe latie peeperkoek</l>
        <l id="s5:l6">Van bombe latie pa valdera.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1">Op den Uithoorn by Tryntje slov</l>
        <l id="s6:l2">Kise mize moeze Spazie</l>
        <l id="s6:l3">Doet de Aardappelen Boer op de pov.</l>
        <l id="s6:l4">Soete lieve meid die laat haar kussen</l>
        <l id="s6:l5">Van bombe laze peeperkoek</l>
        <l id="s6:l6">Van bombe laze pa valdera.</l>
      </lg>
    </lg>
  </text>
</song>
