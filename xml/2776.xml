    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Doer liefde reyn verwonnen ick blyven <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s1:l2" met="5" type="-+">om u serteyn voerteer ic vlies en <rhyme label="a" type="m" corresp="#s1:l1">bloet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die u bemint uit tsersen <rhyme label="b" type="f">goet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Voerderreuen Doet al myi <rhyme label="b" type="f" corresp="#s1:l3">gemoet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dat Ick van u moet <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat Doet myn groete <rhyme label="c" type="m" corresp="#s1:l5">pyn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Of gy Gevoelde wat pyn ick om u <rhyme label="a" type="m">ly</rhyme></l>
        <l id="s2:l2" met="5" type="-+">gy sout myn met herten alsoo <rhyme label="a" type="m" corresp="#s2:l1">bly</rhyme></l>
        <l id="s2:l3" met="3" type="-+">of gy naet lyden <rhyme label="b" type="f">poechde</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat u minnaer <rhyme label="b" type="f" corresp="#s2:l3">gedoechde</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Het waer hem alsoe <rhyme label="c" type="m">goedt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">te koemen metter <rhyme label="c" type="m" corresp="#s2:l5">spoet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Alle myn betrouwe set ick op godt en <rhyme label="a" type="m">u</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Daer ic op bouwe en op voerlaet oeck <rhyme label="a" type="m" corresp="#s3:l1">nu</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Gy syt myn hoep myn <rhyme label="b" type="f">toeuerlaet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer alle mynen sin op <rhyme label="b" type="f" corresp="#s3:l3">staet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">.... my en niet en <rhyme label="c" type="m">voerlaet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">maer koemt myn toch te <rhyme label="c" type="m" corresp="#s3:l5">baet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Geen ander en Isser In schoenheyt alsoe <rhyme label="a" type="m">ryck</rhyme></l>
        <l id="s4:l2" met="5" type="-+">In mynen sinne vant Ick noeit haers <rhyme label="a" type="m" corresp="#s4:l1">gelyck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Want haer liefelycke weese <rhyme label="b" type="f">bly</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Voerhuecht myn hert vol <rhyme label="b" type="f" corresp="#s4:l3">melody</rhyme></l>
        <l id="s4:l5" met="3" type="-+">van fantasy soe <rhyme label="c" type="m">bly</rhyme></l>
        <l id="s4:l6" met="3" type="-+">ach alsoe <rhyme label="c" type="m" corresp="#s4:l5">bly</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Laet hem genieten die hem tot u went <rhyme label="a" type="m">went</rhyme></l>
        <l id="s5:l2" met="5" type="-+">tsou hem voerdrieten en dat met swaer <rhyme label="a" type="m" corresp="#s5:l1">torment</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dat hy niet sou <rhyme label="b" type="f">gewinnen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die u met reinder <rhyme label="b" type="f" corresp="#s5:l3">sinnen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">uit liefde <rhyme label="c" type="m">mint</rhyme></l>
        <l id="s5:l6" met="3" type="-+">soe wert hy heel <rhyme label="c" type="m" corresp="#s5:l5">voerblint</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Prinsesse goedich laet hem int ent <rhyme label="a" type="m">preesent</rhyme></l>
        <l id="s6:l2" met="5" type="-+">uit liefde spoedich die u int ent <rhyme label="a" type="m" corresp="#s6:l1">preesent</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ick hoep den tyt te <rhyme label="b" type="f">beyden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat wy ons leeuen <rhyme label="b" type="f" corresp="#s6:l3">leyde</rhyme></l>
        <l id="s6:l5" met="3" type="-+">In eer en oeck in <rhyme label="c" type="m">duecht</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Waerom Ick ben <rhyme label="c" type="m" corresp="#s6:l5">voerhuecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
