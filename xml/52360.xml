<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>52360</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Willem van Geldof</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SA Ionkheid met <rhyme label="a" type="f">hopen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Spoet uw na den <rhyme label="b" type="m">strijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">En wilt u <rhyme label="c" type="f">verkopen</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">'t Is maer een kleinen <rhyme label="b" type="m" corresp="#s1:l2">tijd</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">En dient voor zes <rhyme label="d" type="f">Iaren</rhyme>:</l>
        <l id="s1:l6" met="3" type="+-">dan zijt gy een <rhyme label="e" type="m">Held</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die mee kan <rhyme label="f" type="f">verhaelen</rhyme>;</l>
        <l id="s1:l8" met="3" type="-+">Al van het <rhyme label="e" type="m" corresp="#s1:l6">Oorlogs-veld</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Of zyt gy <rhyme label="a" type="f">genegen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Te vaern op de <rhyme label="b" type="m">See</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Wilt na Amsterdam <rhyme label="c" type="f">treden</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">de Scheepen leggen <rhyme label="b" type="m" corresp="#s2:l2">ree</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Om mede te <rhyme label="d" type="f">vegten</rhyme>:</l>
        <l id="s2:l6" met="3" type="+-">Voor ons <rhyme label="e" type="m">Vaderland</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Toont u als Krijgs <rhyme label="f" type="f">knegten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Neemt het zwaerd in de <rhyme label="e" type="m" corresp="#s2:l6">hand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Want daer is niet te <rhyme label="a" type="f">winnen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">In dezen slegten <rhyme label="b" type="m">tijd</rhyme>;</l>
        <l id="s3:l3" met="3" type="+-">Want het gaet aen mijn <rhyme label="c" type="f">zinne</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Seit Teeuwes teegen zyn <rhyme label="b" type="m" corresp="#s3:l2">Meid</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">dan komt scheele <rhyme label="d" type="f">Ioris</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Seyt ik ben veel te <rhyme label="e" type="m">luy</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Ik worde nu <rhyme label="f" type="f">Soldaetje</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Dat voor mijn manke <rhyme label="e" type="m" corresp="#s3:l6">Truy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Dan komt Iochem <rhyme label="a" type="f">kromneus</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">zeit ik ben een <rhyme label="b" type="m">held</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Ik wil mee gaen <rhyme label="c" type="f">vegten</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">in het Oorlogs <rhyme label="b" type="m" corresp="#s4:l2">veld</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">En jagen de <rhyme label="d" type="f">Franssen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">mee van de <rhyme label="e" type="m">baen</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Want mijn geboggelt <rhyme label="f" type="f">wijfje</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">Kan dan mee aen het zoetelen <rhyme label="e" type="m" corresp="#s4:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Siet Lubbert de <rhyme label="a" type="f">wever</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Met Ian den <rhyme label="b" type="m">Boereknegt</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Zeyt ik ga na het <rhyme label="c" type="f">Leger</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Waer door dat veel <rhyme label="b" type="m" corresp="#s5:l2">meisjes</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">zijn in 't <rhyme label="d" type="f">verdriet</rhyme>:</l>
        <l id="s5:l6" met="3" type="+-">Die anders een <rhyme label="e" type="m">reisje</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Speelen met klein <rhyme label="f" type="f">Piet</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Ziet Gillis van <rhyme label="e" type="m" corresp="#s5:l6">Zwollen</rhyme>;</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">zeid ik word <rhyme label="a" type="f">soldaet</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Eer ik meer zou <rhyme label="b" type="m">luystere</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Ma mijn meyt haer <rhyme label="c" type="f">praet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ik laet haer <rhyme label="b" type="m" corresp="#s6:l2">praet</rhyme>:</l>
        <l id="s6:l5" met="3" type="+-">Ik laet haer nu <rhyme label="d" type="f">zitten</rhyme>:</l>
        <l id="s6:l6" met="3" type="+-">met een kindt aen het <rhyme label="e" type="m">been</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">zy mag dan maer <rhyme label="f" type="f">denken</rhyme>:</l>
        <l id="s6:l8" met="3" type="-+">zy is het niet <rhyme label="e" type="m" corresp="#s6:l6">alleen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">In Deventer hoort men <rhyme label="a" type="f">klagen</rhyme>:</l>
        <l id="s7:l2" met="3" type="-+">Al menig Ionge <rhyme label="b" type="m">meyt</rhyme>:</l>
        <l id="s7:l3" met="3" type="+-">hoe zy in deze <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">haer Vryers raken <rhyme label="b" type="m" corresp="#s7:l2">quyt</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">die door het stryden <rhyme label="d" type="f">nu</rhyme></l>
        <l id="s7:l6" met="3" type="+-">zy aen een <rhyme label="e" type="m">kant</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">waer door menig <rhyme label="f" type="f">Boertje</rhyme>;</l>
        <l id="s7:l8" met="3" type="-+">kan blussen zijn <rhyme label="e" type="m" corresp="#s7:l6">minne-brant</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Daerom Ionge <rhyme label="a" type="f">Dogters</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Al wie dat gy <rhyme label="b" type="m">zyt</rhyme>:</l>
        <l id="s8:l3" met="3" type="+-">hebt gy een <rhyme label="c" type="f">Vryer</rhyme></l>
        <l id="s8:l4" met="3" type="-+">gaet mee na den <rhyme label="b" type="m" corresp="#s8:l2">Stryt</rhyme>:</l>
        <l id="s8:l5" met="3" type="+-">Verkoopt daer <rhyme label="d" type="f">Jannever</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Tabak; wijn of <rhyme label="e" type="m">Bier</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">dan hebt gy van <rhyme label="f" type="f">vryer</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Zomtijds nog <rhyme label="e" type="m" corresp="#s8:l6">pleisier</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Ziet Vannis de <rhyme label="a" type="f">snyder</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">met zijn regtvaerdig <rhyme label="b" type="m">haer</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">zyt ik en wil niet <rhyme label="c" type="f">lyen</rhyme>;</l>
        <l id="s9:l4" met="3" type="-+">dat de Franssen blyven <rhyme label="b" type="m" corresp="#s9:l2">daer</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">ik wil mee gaen <rhyme label="d" type="f">vergten</rhyme></l>
        <l id="s9:l6" met="3" type="+-">als een dapper <rhyme label="e" type="m">Helt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Als wy dan <rhyme label="f" type="f">kampeeren</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Dan win ik nog <rhyme label="e" type="m" corresp="#s9:l6">gelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
