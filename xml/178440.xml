  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Comt ter feesten // suyver <rhyme label="a" type="f">gheesten</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Godts dienaren <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die daer boven // in Godts <rhyme label="c" type="f">hoven</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Sijnen lof verbreyt // tot alder <rhyme label="d" type="m">tijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Helpt ons singhen // groote <rhyme label="a" type="f">dinghen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Van een suyver <rhyme label="b" type="m">Maeght</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die ons’ leven // heeft <rhyme label="c" type="f">ghegheven</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Brenghende hier voort // Godts eeuwigh <rhyme label="d" type="m">woort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hoogh ghepresen // <rhyme label="a" type="f">uytghelesen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Maeght en moeder <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Naer u wenscyhen // alle <rhyme label="c" type="f">menschen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">V soeckt groot en cleyn // in d’aerdsche <rhyme label="d" type="m">pleyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laet doch dalen // uwe <rhyme label="a" type="f">stralen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Der <rhyme label="b" type="m">bermherticheyt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wilt u ooghen // naer ons <rhyme label="c" type="f">booghen</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Keert van ons het quaet // o <rhyme label="d" type="m">toeverlaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Heel deurwonden // deur de <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Maeckt ons al <rhyme label="b" type="m">ghesont</rhyme></l>
        <l id="s5:l3" met="4" type="+-">En u daeden // vol <rhyme label="c" type="f">ghenaeden</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Toont ons al ghemeyn // als <rhyme label="d" type="m">medicijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Al ons crachten // en <rhyme label="a" type="f">ghedachten</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Zijn tot uwen <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Elck een prijst // en <rhyme label="c" type="f">bewijst</rhyme></l>
        <l id="s6:l4" met="5" type="-+">V alderhooghste eer // reyn maghet <rhyme label="d" type="m">teer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">In de sure // laetste <rhyme label="a" type="f">ure</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Als men sterven <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s7:l3" met="4" type="+-">By ons staet // metter <rhyme label="c" type="f">daet</rhyme></l>
        <l id="s7:l4" met="5" type="-+">En neemt in u behoet // Maria <rhyme label="d" type="m">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
