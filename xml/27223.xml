    <melody>Wee mij ik mag wel klagen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wee my wie sal ick <rhyme label="a" type="f">claghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">myn druck en swaer <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">dat my moet nu ten <rhyme label="a" type="f" corresp="#s1:l1">daghen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">om een die my ghebiet / <rhyme label="b" type="m" corresp="#s1:l2">siet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">sy staet in th[.]rt gheplant / <rhyme label="c" type="m">want</rhyme></l>
        <l id="s1:l6" met="3" type="-+">schoonder en weet ick <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die lieft heeft my <rhyme label="a" type="f">verwonnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">al van myn dees lieff <rhyme label="b" type="m">ydoon</rhyme></l>
        <l id="s2:l3" met="3" type="-+">och hadt ickt noydt <rhyme label="a" type="f" corresp="#s2:l1">begonnen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">want voor mynen loon / <rhyme label="b" type="m" corresp="#s2:l2">schoon</rhyme></l>
        <l id="s2:l5" met="4" type="-+">sal my het leuen pyn / <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">alhier in tswerelts <rhyme label="b" type="m" corresp="#s2:l2">throon</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick heb den tyt <rhyme label="a" type="f">gheweten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">maer het is <rhyme label="b" type="m">ghepasseert</rhyme></l>
        <l id="s3:l3" met="3" type="-+">dat ick wist haer <rhyme label="a" type="f" corresp="#s3:l1">secreten</rhyme></l>
        <l id="s3:l4" met="4" type="-+">van haer ben ick gheleert / <rhyme label="b" type="m" corresp="#s3:l2">eert</rhyme></l>
        <l id="s3:l5" met="4" type="-+">sy en blyft niet constant / <rhyme label="c" type="m">want</rhyme></l>
        <l id="s3:l6" met="3" type="-+">sy my die rugh toe <rhyme label="b" type="m" corresp="#s3:l2">keert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat baet toch al myn <rhyme label="a" type="f">clachten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">want sy my heel <rhyme label="b" type="m">verlaet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">ick vrees gheen troost te <rhyme label="a" type="f" corresp="#s4:l1">wachten</rhyme></l>
        <l id="s4:l4" met="4" type="-+">van dat maechdelyck saet / <rhyme label="b" type="m" corresp="#s4:l2">raet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">het soude wesen goet / <rhyme label="c" type="m">spoet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">ghy v eer tvill te <rhyme label="b" type="m" corresp="#s4:l2">laet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy jonckmans meucht wel <rhyme label="a" type="f">letten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">die by dochters <rhyme label="b" type="m">verkeert</rhyme></l>
        <l id="s5:l3" met="3" type="-+">den sin daer na te <rhyme label="a" type="f" corresp="#s5:l1">setten</rhyme></l>
        <l id="s5:l4" met="4" type="-+">dat ghy door haer bedroch / <rhyme label="b" type="m" corresp="#s5:l2">noch</rhyme></l>
        <l id="s5:l5" met="4" type="-+">off door arghelist / <rhyme label="c" type="m">mist</rhyme></l>
        <l id="s5:l6" met="3" type="-+">niet achter tnet en <rhyme label="b" type="m" corresp="#s5:l2">vist</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Orloff ghy jonghe <rhyme label="a" type="f">gheesten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">volcht niet myn liefde <rhyme label="b" type="m">blint</rhyme></l>
        <l id="s6:l3" met="3" type="-+">in bancketten noch op <rhyme label="a" type="f" corresp="#s6:l1">feesten</rhyme></l>
        <l id="s6:l4" met="4" type="-+">versint eer ghy begint / <rhyme label="b" type="m" corresp="#s6:l2">mint</rhyme></l>
        <l id="s6:l5" met="4" type="-+">oock niet en betrout / <rhyme label="c" type="m">bout</rhyme></l>
        <l id="s6:l6" met="3" type="-+">maer daer ghy liefde <rhyme label="b" type="m" corresp="#s6:l2">vint</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
