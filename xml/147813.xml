<song>
  <meta>
    <id>147813</id>
    <date>
      <year-start>1718</year-start>
      <year-end>1718</year-end>
    </date>
    <location>
      <orginal-name>steenwijk</orginal-name>
      <standard-name>Steenwijk, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7895555</lon>
        <lat>6.1209399</lat>
      </coordinates>
