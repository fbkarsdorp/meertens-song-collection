<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164161</id>
    <date>
      <year-start>1749</year-start>
      <year-end>1749</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ha ha zie zo</melody>
  <author>F. Pitton</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WIE is thans niet <rhyme label="a" type="m">verheugd</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En toont een blyde <rhyme label="a" type="m" corresp="#s1:l1">vreugd</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Daer nu de Brit en <rhyme label="b" type="m">Batavier</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Van vreugd zyn opgenomen <rhyme label="b" type="m" corresp="#s1:l3">schier</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Elk zingt en <rhyme label="c" type="m">springt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Men roept houzé dat <rhyme label="c" type="m" corresp="#s1:l5">klinkt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dewyl d'Oranje <rhyme label="a" type="m">Spruit</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Prins Friso met zyn <rhyme label="a" type="m" corresp="#s2:l1">Bruid</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">De Kroonprinces van 't Britse <rhyme label="b" type="m">Ryk</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Getreden is in 't <rhyme label="b" type="m" corresp="#s2:l3">Huwelyk</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Met pragt, met <rhyme label="c" type="m">pragt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Is deze Trouw <rhyme label="c" type="m" corresp="#s2:l5">volbragt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Staatzie was zoo <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Als Frederik Hendriks <rhyme label="a" type="m" corresp="#s3:l1">Zoon</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Den tweden Willem, ooit heeft <rhyme label="b" type="m">gehad</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Wanneer Hy in het Huwelyk <rhyme label="b" type="m" corresp="#s3:l3">trad</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Elk ziet, elk <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wat voortyds is <rhyme label="c" type="m" corresp="#s3:l5">geschied</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ten vyf uuren was <rhyme label="a" type="m">in</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sint James het <rhyme label="a" type="m" corresp="#s4:l1">begin</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Van de Cermonie groot van <rhyme label="b" type="m">pragt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die duurde tot na <rhyme label="b" type="m" corresp="#s4:l3">middernagt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Vol vreugd, vol <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat elk nog lang <rhyme label="c" type="m" corresp="#s4:l5">geheugd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een pyper trok voor <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daar agter zag men <rhyme label="a" type="m" corresp="#s5:l1">gaan</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Tamboers, Trompetters, ry aan <rhyme label="b" type="m">ry</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">De Keteltrom die was 'er <rhyme label="b" type="m" corresp="#s5:l3">by</rhyme>;</l>
        <l id="s5:l5" met="2" type="-+">Getrom, <rhyme label="c" type="m">gebrom</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Gespeel men hoorde <rhyme label="c" type="m" corresp="#s5:l5">alom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Den Prins als <rhyme label="a" type="m">Bruidegom</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Had Englands Order <rhyme label="a" type="m" corresp="#s6:l1">om</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Aan 't Lyf een schoon Goudlakens <rhyme label="b" type="m">Kleed</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Daar vlyt en Kunst was aan <rhyme label="b" type="m" corresp="#s6:l3">besteet</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Elk zingt, dat 't <rhyme label="c" type="m">klinkt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hoe Nassous Zon thans <rhyme label="c" type="m" corresp="#s6:l5">blinkt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Duc Grafton, Lord <rhyme label="a" type="m">Hervy</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die gingen naast Zyn <rhyme label="a" type="m" corresp="#s7:l1">zy</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Met Scarbourough en <rhyme label="b" type="m">Wilmington</rhyme>;</l>
        <l id="s7:l4" met="4" type="-+">Dit Paar had elk hun Order <rhyme label="b" type="m" corresp="#s7:l3">om</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Ter eer, ter <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Van Nassous Vorst en <rhyme label="c" type="m" corresp="#s7:l5">Heer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Van Lynden, <rhyme label="a" type="m">Alua</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En Dunkan volgden <rhyme label="a" type="m" corresp="#s8:l1">na</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Met 's Princen Edelluy, die <rhyme label="b" type="m">paar</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Aan paar, ook gingen naast <rhyme label="b" type="m" corresp="#s8:l3">malkaar</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Vol pragt, vol <rhyme label="c" type="m">pragt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Na elk zyn rang <rhyme label="c" type="m" corresp="#s8:l5">mêbragt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wanneer zyn Hoogheid <rhyme label="a" type="m">kwam</rhyme></l>
        <l id="s9:l2" met="3" type="-+">In de Kapel, soo <rhyme label="a" type="m" corresp="#s9:l1">nam</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Hy plaats op een verheven <rhyme label="b" type="m">Troon</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Als den aanstaende Konings <rhyme label="b" type="m" corresp="#s9:l3">Zoon</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">'t Musyk, 't <rhyme label="c" type="m">Musyk</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Men hoorde <rhyme label="c" type="m" corresp="#s9:l5">heerelyk</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Men keerden aanstonds <rhyme label="a" type="m">weêr</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Om ook die zelfde <rhyme label="a" type="m" corresp="#s10:l1">Eer</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Te tonnen aan de <rhyme label="b" type="m">Kroonprinces</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Des Princen Bruid, en <rhyme label="b" type="m" corresp="#s10:l3">Minnares</rhyme>;</l>
        <l id="s10:l5" met="2" type="-+">De Zon, de <rhyme label="c" type="m">Zon</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Van 't gantsche <rhyme label="c" type="m" corresp="#s10:l5">Albion</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Zy was zeer schoon <rhyme label="a" type="m">gekleet</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En prachtig in 't <rhyme label="a" type="m" corresp="#s11:l1">Habeet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Van Zilver Lake aan alle <rhyme label="b" type="m">kant</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Beleit met Paarle en <rhyme label="b" type="m" corresp="#s11:l3">Diamant</rhyme>,</l>
        <l id="s11:l5" met="2" type="-+">De Sleep, de <rhyme label="c" type="m">Sleep</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Zes Ellen ruim <rhyme label="c" type="m" corresp="#s11:l5">begreep</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Die nagedragen <rhyme label="a" type="m">wierd</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Van tien Leddys, <rhyme label="a" type="m" corresp="#s12:l1">verçiert</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Met witte Kleed'ren van <rhyme label="b" type="m">Satyn</rhyme>;</l>
        <l id="s12:l4" met="4" type="-+">Veel Dames floten toen den <rhyme label="b" type="m" corresp="#s12:l3">trein</rhyme>,</l>
        <l id="s12:l5" met="2" type="-+">Vol pragt, vol <rhyme label="c" type="m">pragt</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Naar elk haar Staat meê <rhyme label="c" type="m" corresp="#s12:l5">bragt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Toen haare Hoogheid <rhyme label="a" type="m">kwam</rhyme></l>
        <l id="s13:l2" met="3" type="-+">In de Kapel, soo <rhyme label="a" type="m" corresp="#s13:l1">nam</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Zy plaats regt voor den <rhyme label="b" type="m">Bruidegom</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">De Dames bleven staan <rhyme label="b" type="m" corresp="#s13:l3">rontom</rhyme>:</l>
        <l id="s13:l5" met="2" type="-+">'t Musyk, 't <rhyme label="c" type="m">Musyk</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Men hoorde <rhyme label="c" type="m" corresp="#s13:l5">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">De Koning, <rhyme label="a" type="m">Koningin</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">En hun Familie, <rhyme label="a" type="m" corresp="#s14:l1">in</rhyme></l>
        <l id="s14:l3" met="4" type="-+">De Kerk gekomen zynde, <rhyme label="b" type="m">daar</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Wierd men veel Adeldom <rhyme label="b" type="m" corresp="#s14:l3">gewaar</rhyme>,</l>
        <l id="s14:l5" met="2" type="-+">Elk nam, elk <rhyme label="c" type="m">nam</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Zyn plaats die hem <rhyme label="c" type="m" corresp="#s14:l5">toekwam</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Na 't Orgel had <rhyme label="a" type="m">gespeelt</rhyme></l>
        <l id="s15:l2" met="3" type="-+">En yders Oor <rhyme label="a" type="m" corresp="#s15:l1">gestreelt</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Zoo nam Prins Friso by de <rhyme label="b" type="m">Hand</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">De Kroonprinces van <rhyme label="b" type="m" corresp="#s15:l3">Engeland</rhyme>,</l>
        <l id="s15:l5" met="2" type="-+">Dit paar, dit <rhyme label="c" type="m">Paar</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Ging aanstonds na 't <rhyme label="c" type="m" corresp="#s15:l5">Autaar</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Zy knielden neder <rhyme label="a" type="m">ras</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">En Londons Bisschop <rhyme label="a" type="m" corresp="#s16:l1">las</rhyme></l>
        <l id="s16:l3" met="4" type="-+">Voor hen het Huwlyks <rhyme label="b" type="m">Formulier</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Dat pas gedaan was, of toen <rhyme label="b" type="m" corresp="#s16:l3">wierd</rhyme>,</l>
        <l id="s16:l5" met="2" type="-+">'t Musyk, 't <rhyme label="c" type="m">Musyk</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Gehoord weer <rhyme label="c" type="m" corresp="#s16:l5">heerelyk</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Toen yder keerde <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Genoot den Prins die <rhyme label="a" type="m" corresp="#s17:l1">Eer</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Dat Hy by de Getrouwden <rhyme label="b" type="m">ging</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Zyn Bruid die zelfde Eer <rhyme label="b" type="m" corresp="#s17:l3">ontfing</rhyme>.</l>
        <l id="s17:l5" met="2" type="-+">Met pragt, met <rhyme label="c" type="m">pragt</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Is deze Trouw <rhyme label="c" type="m" corresp="#s17:l5">volbragt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Het Jonge Paar <rhyme label="a" type="m">ontfinf</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Des Konings <rhyme label="a" type="m" corresp="#s18:l1">Zegening</rhyme>:</l>
        <l id="s18:l3" met="4" type="-+">En 's avonds zag men op de <rhyme label="b" type="m">Zaal</rhyme></l>
        <l id="s18:l4" met="4" type="-+">Van Staat een kostelyk <rhyme label="b" type="m" corresp="#s18:l3">Avondmaal</rhyme>,</l>
        <l id="s18:l5" met="2" type="-+">Vol vreugd, vol, <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Een yder was <rhyme label="c" type="m" corresp="#s18:l5">verheugd</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Al de Adel met <rhyme label="a" type="m">malkaar</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Begroette 's nagts dit <rhyme label="a" type="m" corresp="#s19:l1">Paar</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Met veel geluk en <rhyme label="b" type="m">Zegening</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">Toen volgde Hun <rhyme label="b" type="m" corresp="#s19:l3">Bedlegering</rhyme>:</l>
        <l id="s19:l5" met="2" type="-+">Het Bedt, het <rhyme label="c" type="m">Bedt</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Was kostelyk en <rhyme label="c" type="m" corresp="#s19:l5">net</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Een yder was <rhyme label="a" type="m">verheugd</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">En toonde zyne <rhyme label="a" type="m" corresp="#s20:l1">vreugd</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">De Huizen waren <rhyme label="b" type="m">geillumineert</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">Hier vloog het Vuurwerk, gints <rhyme label="b" type="m" corresp="#s20:l3">ageert</rhyme></l>
        <l id="s20:l5" met="2" type="-+">'t Kanon, 't <rhyme label="c" type="m">Kanon</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Men spaarde geen <rhyme label="c" type="m" corresp="#s20:l5">Pikton</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">De Hemel schenk dit <rhyme label="a" type="m">Paar</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Dat Zy nog binnen 't <rhyme label="a" type="m" corresp="#s21:l1">Jaar</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">Een welgemaakte frisse Zoon <rhyme label="b" type="m">aanschouwen</rhyme>,</l>
        <l id="s21:l4" met="4" type="-+">Hunne Deugd ten <rhyme label="b" type="m" corresp="#s21:l3">loon</rhyme>,</l>
        <l id="s21:l5" met="2" type="-+">Zo bloeit, en <rhyme label="c" type="m">groeit</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">Oranje <rhyme label="c" type="m" corresp="#s21:l5">onvermoeit</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Zoo blyft d'Oranje <rhyme label="a" type="m">Stam</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">(Wiens Tak op 't laatste <rhyme label="a" type="m" corresp="#s22:l1">kwam</rhyme>)</l>
        <l id="s22:l3" met="4" type="-+">Steeds bloeijen, en den Hemel <rhyme label="b" type="m">geeft</rhyme></l>
        <l id="s22:l4" met="4" type="-+">Dat Frisoos Naam geduurig <rhyme label="b" type="m" corresp="#s22:l3">leeft</rhyme>,</l>
        <l id="s22:l5" met="2" type="-+">In spyt, in <rhyme label="c" type="m">spyt</rhyme>,</l>
        <l id="s22:l6" met="3" type="-+">Van die het Hem <rhyme label="c" type="m" corresp="#s22:l5">benyt</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Die Eer past hem <rhyme label="a" type="m">alleen</rhyme>,</l>
        <l id="s23:l2" met="3" type="-+">Oranje, anders <rhyme label="a" type="m" corresp="#s23:l1">geen</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">Wyl zyn Voorouders met hun <rhyme label="b" type="m">goed</rhyme>,</l>
        <l id="s23:l4" met="4" type="-+">Ontziende niet haar eigen <rhyme label="b" type="m" corresp="#s23:l3">Bloed</rhyme>,</l>
        <l id="s23:l5" met="2" type="-+">Dit Land, dit <rhyme label="c" type="m">Land</rhyme>,</l>
        <l id="s23:l6" met="3" type="-+">Ontrukten Spanjaards <rhyme label="c" type="m" corresp="#s23:l5">hand</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Wie is dan niet <rhyme label="a" type="m">verblyd</rhyme>,</l>
        <l id="s24:l2" met="3" type="-+">En Zingt in deze <rhyme label="a" type="m" corresp="#s24:l1">Tyd</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Ter Eer van dees doorlugte <rhyme label="b" type="m">Trou</rhyme>,</l>
        <l id="s24:l4" met="4" type="-+">Vry op Wilhelmus van <rhyme label="b" type="m" corresp="#s24:l3">Nassou</rhyme>,</l>
        <l id="s24:l5" met="2" type="-+">En zingt, en <rhyme label="c" type="m">springt</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">En roept Houzée dat 't <rhyme label="c" type="m" corresp="#s24:l5">klinkt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
