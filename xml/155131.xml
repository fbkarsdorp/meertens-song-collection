<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155131</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Rozemond die lag gedoken</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">BRuydegom wilt vrolijck <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">U van herten seer <rhyme label="b" type="m">verblijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want ghy in den Echt <rhyme label="a" type="f" corresp="#s1:l1">gepresen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Naer u wens gebonden <rhyme label="b" type="m" corresp="#s1:l2">zijt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'t Minnaers hert hebt gy <rhyme label="c" type="m">gejaeght</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Sy is Bruyt, en Vrou, en <rhyme label="c" type="m" corresp="#s1:l5">Maegt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Komt ghy Nimphjes, soete <rhyme label="a" type="f">dieren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Vlegt een krans van <rhyme label="b" type="m">Maegdekruyt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wilt voor 't lest daer mee <rhyme label="a" type="f" corresp="#s2:l1">vercieren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">'t Ieughdigh hooft van onse <rhyme label="b" type="m" corresp="#s2:l2">Bruyt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Want ick sie zy sal 't <rhyme label="c" type="m">bestaen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Om met beeck ten strijt te <rhyme label="c" type="m" corresp="#s2:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Strijt dan lustig met <rhyme label="a" type="f">malkander</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Soo 't behoort na Liefdens <rhyme label="b" type="m">aert</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Als twee Liefkens by den <rhyme label="a" type="f" corresp="#s3:l1">ander</rhyme></l>
        <l id="s3:l4" met="4" type="+-">'t Samen in de deught <rhyme label="b" type="m" corresp="#s3:l2">gepaert</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Treet vry aen, en weest <rhyme label="c" type="m">verblijt</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Niemant blijft in dese <rhyme label="c" type="m" corresp="#s3:l5">strijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wat al lonckjes, wat al <rhyme label="a" type="f">kusjes</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Geeft ons Bruydegom sijn <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'t Zijn de vlaegjes, 't zijn de <rhyme label="a" type="f" corresp="#s4:l1">lusjes</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die sijn hertje geven <rhyme label="b" type="m" corresp="#s4:l2">uyt</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Laet ons dan oock in het <rhyme label="c" type="m">ront</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Eensjes kussen mont aen <rhyme label="c" type="m" corresp="#s4:l5">mont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wie sal onse Bruyt <rhyme label="a" type="f">geleyen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Als het tijt is om te <rhyme label="b" type="m">gaen</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Speelnoots ghy moet u <rhyme label="a" type="f" corresp="#s5:l1">bereyen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Meysjes 't dient van u <rhyme label="b" type="m" corresp="#s5:l2">gedaen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Brenght haer vry tot aen het <rhyme label="c" type="m">bedt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Maer weest vorder geen <rhyme label="c" type="m" corresp="#s5:l5">belet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laet nu eens de glaesjes <rhyme label="a" type="f">klinken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat wy mogen na den <rhyme label="b" type="m">eys</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Elck een frisse roemer <rhyme label="a" type="f" corresp="#s6:l1">drincken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Speelnoot 'k brengh u dit een <rhyme label="b" type="m" corresp="#s6:l2">reys</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Sent het voort ter eeren <rhyme label="c" type="m">om</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Van ons Bruyt en <rhyme label="c" type="m" corresp="#s6:l5">Bruydegom</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wy dan sullen ons <rhyme label="a" type="f">vermaken</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Met gesangh en kusjes <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Aen die schoon gebloosde <rhyme label="a" type="f" corresp="#s7:l1">kaken</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Yder dan sijn plicht <rhyme label="b" type="m" corresp="#s7:l2">voldoet</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">'t Ander dat wy noch <rhyme label="c" type="m">vermoen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Laten wy den Bruygom <rhyme label="c" type="m" corresp="#s7:l5">doen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
