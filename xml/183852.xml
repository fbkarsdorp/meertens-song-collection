<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183852</id>
    <date>
      <year-start>1806</year-start>
      <year-end>1806</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Liefde die van alle tijden</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Welke schoonheid, welke <rhyme label="a" type="f">straalen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Welk een aldoordringend <rhyme label="b" type="m">licht</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Zie 'k van 't oosten op mij <rhyme label="a" type="f" corresp="#s1:l1">daalen</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Welk een glans dekt mijn <rhyme label="b" type="m" corresp="#s1:l2">gezicht</rhyme>!</l>
        <l id="s1:l5" met="2" type="-+">Met ziel en <rhyme label="c" type="f">zinnen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Zal 'k, Orde! u <rhyme label="c" type="f" corresp="#s1:l5">minnen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Die mij leert, verrukt en <rhyme label="b" type="m" corresp="#s1:l2">stricht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ik zie, treffende, op dit <rhyme label="a" type="f">heden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat de Vrije - <rhyme label="b" type="m">Metslaarij</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Steunt op waarheid, deugd en <rhyme label="a" type="f" corresp="#s2:l1">reden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Vreemd van laage <rhyme label="b" type="m" corresp="#s2:l2">kunstnaarij</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Wars van <rhyme label="c" type="f">gebreken</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Van list en <rhyme label="c" type="f" corresp="#s2:l5">treken</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Is zij van zedenloosheid <rhyme label="b" type="m" corresp="#s2:l2">vrij</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">ô Laat andren zich <rhyme label="a" type="f">vermaaken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">In de heerschzucht, weelde en <rhyme label="b" type="m">pracht</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Of naar goud, door gouddorst, <rhyme label="a" type="f" corresp="#s3:l1">haaken</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Hier wordt dát verguisd, <rhyme label="b" type="m" corresp="#s3:l2">verächt</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Vrij - <rhyme label="c" type="f">Metselaaren</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">In kunst <rhyme label="c" type="f" corresp="#s3:l5">ervaaren</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Zijn steeds door nedrigheid <rhyme label="b" type="m" corresp="#s3:l2">geächt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kunne vol <rhyme label="a" type="f">bevalligheden</rhyme>!</l>
        <l id="s4:l2" met="4" type="+-">Stel 't vooröordeel toch ter <rhyme label="b" type="m">zij'</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Nooit ziet ge u zoo <rhyme label="a" type="f" corresp="#s4:l1">aangebeden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Dan van ons. - <rhyme label="b" type="m" corresp="#s4:l2">Bescheiden-vrij</rhyme></l>
        <l id="s4:l5" met="2" type="-+">U te <rhyme label="c" type="f">beminnen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Uw gunst te <rhyme label="c" type="f" corresp="#s4:l5">winnen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Zijn wetten der Vrij - <rhyme label="b" type="m" corresp="#s4:l2">Metslaarij</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zie ons, op des meesters <rhyme label="a" type="f">wenken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Vaardig, eensgezind en <rhyme label="b" type="m">blij'</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">D'eêlsten nektar bruisschend <rhyme label="a" type="f" corresp="#s5:l1">schenken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Om, naar eisch der <rhyme label="b" type="m" corresp="#s5:l2">Metslaarij</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Driemaal drie <rhyme label="c" type="f">keeren</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">U daarmeê te <rhyme label="c" type="f" corresp="#s5:l5">eeren</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Bij de allerschoonste <rhyme label="b" type="m" corresp="#s5:l2">harmonij</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
