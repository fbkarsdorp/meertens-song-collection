  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Z  wem sall ick <rhyme label="a" type="f">gedenckenn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">hertz aller liebste <rhyme label="b" type="m">mein</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">gros ellendt / th tt mich <rhyme label="a" type="f" corresp="#s1:l1">krencken</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">das ich nitt bei ir mag <rhyme label="b" type="m" corresp="#s1:l2">sein</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Jch hab mich <rhyme label="c" type="f">understandenn</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">mitt frembdenn w nder <rhyme label="d" type="m">schertz</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">So hast  mir <rhyme label="c" type="f" corresp="#s1:l5">umbfangenn</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">mein gem tt / unnd a ch mein <rhyme label="d" type="m" corresp="#s1:l6">hertz</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Feins lieb d  darfs nitt <rhyme label="a" type="f">dencken</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">das ich will abbe <rhyme label="b" type="m">laen</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Jch will vonn deinentt <rhyme label="a" type="f" corresp="#s2:l1">wegenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Mein vatter vnnd m tter <rhyme label="b" type="m" corresp="#s2:l2">verlan</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Des las mich feins lieb <rhyme label="c" type="f">genesenn</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">das ich dir so traw will <rhyme label="d" type="m">sein</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">th n mir dein hertz a <rhyme label="c" type="f" corresp="#s2:l5">ffschliesenn</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">schle s mich feins lieb <rhyme label="d" type="m" corresp="#s2:l6">darJnn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ess ist unnd wirtt mir nymmer <rhyme label="a" type="f">kein</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">so lieb als d  mir <rhyme label="b" type="m">bist</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">kein falsche kleffer mich daran <rhyme label="a" type="f" corresp="#s3:l1">jrrett</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">die lieb gwaltig <rhyme label="b" type="m" corresp="#s3:l2">ist</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">mit dir z  th n  vnnd z  <rhyme label="c" type="f">lassenn</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">all was z  denn erhn <rhyme label="d" type="m">gehoirtt</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Jch bin vnnd pleib dein <rhyme label="c" type="f" corresp="#s3:l5">aigen</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">dein aigenn ganntz vn <rhyme label="d" type="m" corresp="#s3:l6">erkertt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ein blomlein an der <rhyme label="a" type="f">heidenn</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">mitt nhamenn vergis nitt <rhyme label="b" type="m">mein</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Las dir das blomlein <rhyme label="a" type="f" corresp="#s4:l1">wachsenn</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">woll jnn dem hertzenn <rhyme label="b" type="m" corresp="#s4:l2">dein</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Ker dich an keinem Kleffer <rhyme label="c" type="f">nitt</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">So pleibenn mir alle weg <rhyme label="d" type="m">stehenn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Las dir das blomlein <rhyme label="c" type="f" corresp="#s4:l5">wachsenn</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">so offt dein hertz <rhyme label="d" type="m" corresp="#s4:l6">begertt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Jch w nsch dir heimlichs <rhyme label="a" type="f">leidenn</rhyme> /</l>
        <l id="s5:l2" met="3" type="-+">So fill als ich es <rhyme label="b" type="m">hab</rhyme> /</l>
        <l id="s5:l3" met="3" type="-+">So magst  mich nitt <rhyme label="a" type="f" corresp="#s5:l1">meidenn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Jm Jar nitt einenn <rhyme label="b" type="m" corresp="#s5:l2">tag</rhyme> /</l>
        <l id="s5:l5" met="3" type="-+">D  m st dich selber <rhyme label="c" type="f">erbarmen</rhyme> /</l>
        <l id="s5:l6" met="3" type="-+">Vnd offt gedenckenn an <rhyme label="d" type="m">mich</rhyme>/</l>
        <l id="s5:l7" met="3" type="-+">Schlus mich jn deinen <rhyme label="c" type="f" corresp="#s5:l5">armen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">erfrewe dich vnnd <rhyme label="d" type="m" corresp="#s5:l6">mich</rhyme>.</l>
      </lg>
    </lg>
