    <melody>Hansje sneed het koren</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Het soude gaste ten ooereloch <rhyme label="a" type="m">vaeren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">ten ooereloch alsoe <rhyme label="b" type="f">fiere</rhyme></l>
        <l id="s1:l3" met="4" type="-+">sy noeden een al van haer <rhyme label="a" type="m" corresp="#s1:l1">kaeren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">om eens met haer te <rhyme label="b" type="f" corresp="#s1:l2">bieren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Door haer versouckt wilt dit <rhyme label="a" type="m">verstaen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">mosten sy tschiedam <rhyme label="b" type="f">koemen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">een vaen 2 ofte 3 ten kost niet <rhyme label="a" type="m" corresp="#s2:l1">schaen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">sy spraeken als den <rhyme label="b" type="f" corresp="#s2:l2">vroeme</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Tschiedam Gekoemen vrooelick en <rhyme label="a" type="m">bly</rhyme></l>
        <l id="s3:l2" met="3" type="-+">soe hebben sy haer <rhyme label="b" type="f">geuonden</rhyme></l>
        <l id="s3:l3" met="4" type="-+">sy gingen spaenseeren sy aen <rhyme label="a" type="m" corresp="#s3:l1">sy</rhyme></l>
        <l id="s3:l4" met="3" type="-+">om te hoeren haer <rhyme label="b" type="f" corresp="#s3:l2">gronden</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het was te Delft wille wy <rhyme label="a" type="m">syn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">om te drinken De <rhyme label="b" type="f">foeye</rhyme></l>
        <l id="s4:l3" met="4" type="-+">tDocht haer te weesen hups en <rhyme label="a" type="m" corresp="#s4:l1">fyn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">maer twas een <rhyme label="b" type="f" corresp="#s4:l2">kaelenoeye</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">van Daer scheydende saeuons <rhyme label="a" type="m">laet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">om naer Delft te <rhyme label="b" type="f">trecken</rhyme></l>
        <l id="s5:l3" met="4" type="-+">het was luttel tot haeren <rhyme label="a" type="m" corresp="#s5:l1">baet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Den duiuel ginck op <rhyme label="b" type="f" corresp="#s5:l2">krecken</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Te Delft gekoemen niet Drooch maer <rhyme label="a" type="m">nat</rhyme></l>
        <l id="s6:l2" met="3" type="-+">nae de herberch syn sy <rhyme label="b" type="f">gegangen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">sy gingen sitte op haer <rhyme label="a" type="m" corresp="#s6:l1">plat</rhyme></l>
        <l id="s6:l4" met="3" type="-+">om een luttel te <rhyme label="b" type="f" corresp="#s6:l2">wangen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sy banckte daer al uiter <rhyme label="a" type="m">borst</rhyme></l>
        <l id="s7:l2" met="3" type="-+">en Droncken den wyn uit <rhyme label="b" type="f">schaelen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">sy Droncken met smaeck en groeten <rhyme label="a" type="m" corresp="#s7:l1">Dorst</rhyme></l>
        <l id="s7:l4" met="3" type="-+">twas gy syt eele <rhyme label="b" type="f" corresp="#s7:l2">baesen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De ooerelochs gasten hoort toch <rhyme label="a" type="m">dit</rhyme></l>
        <l id="s8:l2" met="3" type="-+">lieten de weerdin <rhyme label="b" type="f">reekenen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">de foeyers werde geesmeete met <rhyme label="a" type="m" corresp="#s8:l1">spit</rhyme></l>
        <l id="s8:l4" met="3" type="-+">In haer buers hadde sy <rhyme label="b" type="f" corresp="#s8:l2">teekenen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Bet was de man een daelder <rhyme label="a" type="m">net</rhyme></l>
        <l id="s9:l2" met="3" type="-+">sy waeren onder haer <rhyme label="b" type="f">seeuenen</rhyme></l>
        <l id="s9:l3" met="4" type="-+">daer laege sommyge op het <rhyme label="a" type="m" corresp="#s9:l1">bet</rhyme></l>
        <l id="s9:l4" met="3" type="-+">het gincker niet al te <rhyme label="b" type="f" corresp="#s9:l2">deegene</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Haer susters koen liepen voor <rhyme label="a" type="m">speck</rhyme></l>
        <l id="s10:l2" met="3" type="-+">die foeyers en dorste niet <rhyme label="b" type="f">spreeken</rhyme></l>
        <l id="s10:l3" met="4" type="-+">het was ter stont hout uwen <rhyme label="a" type="m" corresp="#s10:l1">beck</rhyme></l>
        <l id="s10:l4" met="3" type="-+">gy lechter tuis ter <rhyme label="b" type="f" corresp="#s10:l2">weecken</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Sy hadde nochtans hoert myn <rhyme label="a" type="m">beuroen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Ick secht u sonder <rhyme label="b" type="f">faelen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">te segchen een woert stout ende <rhyme label="a" type="m" corresp="#s11:l1">koen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">elck moet den daelder <rhyme label="b" type="f" corresp="#s11:l2">betaelen</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Daer kreechger een al uit den <rhyme label="a" type="m">hoop</rhyme></l>
        <l id="s12:l2" met="3" type="-+">een klinck al voer syn <rhyme label="b" type="f">wangen</rhyme></l>
        <l id="s12:l3" met="4" type="-+">terstont maer dat hy heene <rhyme label="a" type="m" corresp="#s12:l1">droop</rhyme></l>
        <l id="s12:l4" met="3" type="-+">syn tyt was daer niet <rhyme label="b" type="f" corresp="#s12:l2">lange</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Prinse de foeyers wel <rhyme label="a" type="m">bekent</rhyme></l>
        <l id="s13:l2" met="3" type="-+">sy syn heene <rhyme label="b" type="f">geuaeren</rhyme></l>
        <l id="s13:l3" met="4" type="-+">godt weet waer dat sy syn <rhyme label="a" type="m" corresp="#s13:l1">volent</rhyme></l>
        <l id="s13:l4" met="3" type="-+">en wil de foeygeeuers <rhyme label="b" type="f" corresp="#s13:l2">bewaeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
