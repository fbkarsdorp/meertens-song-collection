    <melody>Schoon lief gij zijt prijs waard alleen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Altyt / verslyt / ick lief <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Met vreesen / gansch en <rhyme label="b" type="m">gaer</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Die lyt / subyt / wylt dynstych <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Genesen / myn <rhyme label="b" type="m" corresp="#s1:l2">ghevaer</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Maeckt dat ick wert <rhyme label="b" type="m" corresp="#s1:l2">ghewaer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">V hulp tot my <rhyme label="c" type="f">ghereesen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Door praet / ghelaet / schoon <rhyme label="c" type="f" corresp="#s1:l6">wtghelesen</rhyme></l>
        <l id="s1:l8" met="5" type="-+">Met raet / bystaet / in desen nieuwen <rhyme label="b" type="m" corresp="#s1:l2">jaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Compt gheeft / beleeft / schoone <rhyme label="a" type="f">jonckvrauwe</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Gheen flauwe / v <rhyme label="b" type="m">dienaer</rhyme></l>
        <l id="s2:l3" met="4" type="-+">My gheeft / beleeft / blyschap voor <rhyme label="a" type="f" corresp="#s2:l1">rauwe</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Door trauwe / vruecht <rhyme label="b" type="m" corresp="#s2:l2">eerbaer</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Maeckt v en my een <rhyme label="b" type="m" corresp="#s2:l2">paer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Zoo zal myn druck <rhyme label="c" type="f">cesseeren</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Toch spoet / zeer soet / al v <rhyme label="c" type="f" corresp="#s2:l6">begeeren</rhyme></l>
        <l id="s2:l8" met="5" type="-+">My doet / ontmoet / in desen nieuwen <rhyme label="b" type="m" corresp="#s2:l2">jaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Voor dyn aenschyn wyl ick myn <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Nu gheuen zonder <rhyme label="b" type="m">vaer</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Troost myn zeer fyn och lyef <rhyme label="a" type="f" corresp="#s3:l1">verheeuen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Nyet sneuen laet voor <rhyme label="b" type="m" corresp="#s3:l2">waer</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Doet slyten tlyden <rhyme label="b" type="m" corresp="#s3:l2">swaer</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat my nu quelt <rhyme label="c" type="f">ghestadych</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Dus vrau myn rau verdryft <rhyme label="c" type="f" corresp="#s3:l6">ghenadych</rhyme></l>
        <l id="s3:l8" met="5" type="-+">Dye jau dient trau in desen nieu <rhyme label="b" type="m" corresp="#s3:l2">jaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gheulyt zeer blyt prynses <rhyme label="a" type="f">verheeuen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Dycht neuen v <rhyme label="b" type="m">dienaer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Dyt spydt benydt laet zyn <rhyme label="a" type="f" corresp="#s4:l1">verdreuen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Het sneuen en <rhyme label="b" type="m" corresp="#s4:l2">mysbaer</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Twelck my quelt voor als <rhyme label="b" type="m" corresp="#s4:l2">naer</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wylt toch myn pyn <rhyme label="c" type="f">versaechten</rhyme></l>
        <l id="s4:l7" met="4" type="-+">En ick my lyef by v mach <rhyme label="c" type="f" corresp="#s4:l6">vernachten</rhyme></l>
        <l id="s4:l8" met="5" type="-+">Heel vry en bly in desen nyeuwe <rhyme label="b" type="m" corresp="#s4:l2">jaer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
