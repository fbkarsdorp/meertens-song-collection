<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13368</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="zeemanslied" translation="sailor's song">zeemanslied</tag>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Edele artiesten koen</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="work">werk</category>
    </categories>
  <author>Willem Schellinger</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LUystert toe al te <rhyme label="a" type="m">saem</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe dat wy zijn <rhyme label="b" type="f">gevaren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Al in des Heeren <rhyme label="a" type="m" corresp="#s1:l1">Naem</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ick sal het u <rhyme label="b" type="f" corresp="#s1:l2">verklaren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Van Amsterdam de <rhyme label="c" type="f">Steede</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Nae 't Texel seer <rhyme label="d" type="m">saljant</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Van daer oock in goe <rhyme label="c" type="f" corresp="#s1:l5">Vreede</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Nae Madera <rhyme label="d" type="m" corresp="#s1:l6">bekant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In 't Jaer sestien hondert <rhyme label="a" type="m">ree</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En vijf en vijftigh <rhyme label="b" type="f">meede</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Doe liepen wy in <rhyme label="a" type="m" corresp="#s2:l1">Zee</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Al van Koopvaerders <rhyme label="b" type="f" corresp="#s2:l2">Reede</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Uyt Texel sonder <rhyme label="c" type="f">schroomen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Den drie en twintighste <rhyme label="d" type="m">Mey</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Met St. Louwies seer <rhyme label="c" type="f" corresp="#s2:l5">vroome</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die Godt den Heer <rhyme label="d" type="m" corresp="#s2:l6">geley</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy quamen niet so <rhyme label="a" type="m">ras</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Al op de Kust <rhyme label="b" type="f">Brittanjen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Of kregen op dat <rhyme label="a" type="m" corresp="#s3:l1">pas</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Een schoot al na <rhyme label="b" type="f" corresp="#s3:l2">Oranjen</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Dat wy voort moesten <rhyme label="c" type="f">strijcke</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Voor Pleymuyens <rhyme label="d" type="m">Kasteel</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En voor de <rhyme label="c" type="f" corresp="#s3:l5">Republijcke</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van Engelant seer <rhyme label="d" type="m" corresp="#s3:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sy riepen over <rhyme label="a" type="m">luyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En kreeten <rhyme label="b" type="f">altesamen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Van wen jour Schip? spreeckt <rhyme label="a" type="m" corresp="#s4:l1">uyt</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Ons Schipper riep <rhyme label="b" type="f" corresp="#s4:l2">bequame</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Van Amsterdam seer <rhyme label="c" type="f">rustigh</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">De Steert die riep <rhyme label="d" type="m">weerom</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wil maeck jou Pey seer <rhyme label="c" type="f" corresp="#s4:l5">lustigh</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Voor Cromwel een goe <rhyme label="d" type="m" corresp="#s4:l6">som</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wy quamen voort aen <rhyme label="a" type="m">lant</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Sy wouden ons op <rhyme label="b" type="f">knoopen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Om dat wy so <rhyme label="a" type="m" corresp="#s5:l1">saljant</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daer quamen in <rhyme label="b" type="f" corresp="#s5:l2">geloopen</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Al met de Vlagh van <rhyme label="c" type="f">boven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van Sijde fijn <rhyme label="d" type="m">gebreyt</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">'t Welck Cromwel gingh <rhyme label="c" type="f" corresp="#s5:l5">verdoven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat dee den Steert seer <rhyme label="d" type="m" corresp="#s5:l6">leyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Doen wy weer Zeylden <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van Pleymuyen met <rhyme label="b" type="f">lusten</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met noch ses Scheepen <rhyme label="a" type="m" corresp="#s6:l1">voort</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Al langhs d'Engelse <rhyme label="b" type="f" corresp="#s6:l2">kusten</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Wy waren niet <rhyme label="c" type="f">gekomen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dichte by Leesaerts <rhyme label="d" type="m">hoeck</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Of liepen sonder <rhyme label="c" type="f" corresp="#s6:l5">schroomen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">In Vaelmuyen weer <rhyme label="d" type="m" corresp="#s6:l6">kloeck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Den derden dagh hier <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Het Wintje liep na 't <rhyme label="b" type="f">Noorden</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Zijn wy weer t'Zeyl <rhyme label="a" type="m" corresp="#s7:l1">gegaen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Al langhs de Engelse <rhyme label="b" type="f" corresp="#s7:l2">Boorden</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Ons Zeyltjes sagh men <rhyme label="c" type="f">vallen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Men riep, hael Schoot en <rhyme label="d" type="m">Bras</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ons Mars-Zeyls onder <rhyme label="c" type="f" corresp="#s7:l5">allen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Nae boven reesen <rhyme label="d" type="m" corresp="#s7:l6">ras</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wy Zeylden seer <rhyme label="a" type="m">valjant</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Tot dat wy zijn <rhyme label="b" type="f">gekomen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Al op ons hooghte <rhyme label="a" type="m" corresp="#s8:l1">kant</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Hebben doe waer <rhyme label="b" type="f" corresp="#s8:l2">genomen</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">De Sarters die wy <rhyme label="c" type="f">sagen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Den achtsten July <rhyme label="d" type="m">klaer</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Maderas soet <rhyme label="c" type="f" corresp="#s8:l5">gewagen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Is lieffelijck, en <rhyme label="d" type="m" corresp="#s8:l6">raer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Het is een treffelijck <rhyme label="a" type="m">Landt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Bewoont van <rhyme label="b" type="f">Portegeesen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ghy vint daer <rhyme label="a" type="m" corresp="#s9:l1">abondant</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Veel Vruchten hoogh <rhyme label="b" type="f" corresp="#s9:l2">gepreesen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Als Appelen van <rhyme label="c" type="f">Oranje</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Lamoen, Sitroene <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Druyve, Nooten, <rhyme label="c" type="f" corresp="#s9:l5">Karstanje</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Spaens Speck seer fraey ten <rhyme label="d" type="m" corresp="#s9:l6">toon</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Het Wijntje edel <rhyme label="a" type="m">raer</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Voorwaer ick kan niet <rhyme label="b" type="f">rusten</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Wort u geschoncken <rhyme label="a" type="m" corresp="#s10:l1">klaer</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Ja glaesjes vol met <rhyme label="b" type="f" corresp="#s10:l2">lusten</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Een schuttel <rhyme label="c" type="f">Moerebeyen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Appelen en Peere <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Gaen sy u daer <rhyme label="c" type="f" corresp="#s10:l5">bereyen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Tot een Bancketje <rhyme label="d" type="m" corresp="#s10:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Noch vint gy daer seer <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Vygen en <rhyme label="b" type="f">Apperekoosen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Comcommers fraey ten <rhyme label="a" type="m" corresp="#s11:l1">toon</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">De Krop-sla siet men <rhyme label="b" type="f" corresp="#s11:l2">bloosen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">De Pruymen staen mee <rhyme label="c" type="f">lustigh</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Van tweederhande <rhyme label="d" type="m">kleur</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">De Karsen staen mee <rhyme label="c" type="f" corresp="#s11:l5">rustigh</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Treffelijck in haer <rhyme label="d" type="m" corresp="#s11:l6">fleur</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Oorlof tot een <rhyme label="a" type="m">besluyt</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ons Schip dat is <rhyme label="b" type="f">geladen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Met K'naris Wijn en <rhyme label="a" type="m" corresp="#s12:l1">Fruyt</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En Kisjes met <rhyme label="b" type="f" corresp="#s12:l2">Suckaden</rhyme>.</l>
        <l id="s12:l5" met="3" type="-+">Godt hoedt ons sonder <rhyme label="c" type="f">schroomen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Voor klippe, zandt en <rhyme label="d" type="m">strant</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Op dat wy mogen <rhyme label="c" type="f" corresp="#s12:l5">komen</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Weer in ons <rhyme label="d" type="m" corresp="#s12:l6">Vaderlant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
