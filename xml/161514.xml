  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LIef, ach' soete <rhyme label="a" type="f">Harderinne</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Afgodesje van mijn <rhyme label="b" type="m">ziel</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dien ick als mijn hert <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Soetert die my wel <rhyme label="b" type="m" corresp="#s1:l2">beviel</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Moet ick steets dragen // moet ick steets <rhyme label="c" type="f">dragen</rhyme></l>
        <l id="s1:l6" met="8" type="+-">V ongemeene wreveltheden // schoon mijn deden tot u <rhyme label="d" type="f">treden</rhyme></l>
        <l id="s1:l7" met="4" type="-+">In ootmoet tot u alle <rhyme label="c" type="f" corresp="#s1:l5">daghen</rhyme>.</l>
      </lg>
    </lg>
