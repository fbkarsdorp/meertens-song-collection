<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179666</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody>Menuet van Locatelli</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">STuurt myn <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Tot u liefdens <rhyme label="a" type="m" corresp="#s1:l1">Godin</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat ik wederom myn Cloris <rhyme label="a" type="m" corresp="#s1:l1">bloemen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Het vermogen, van haar <rhyme label="b" type="f">oogen</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Ik ook <rhyme label="a" type="m" corresp="#s1:l1">ondervin</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Haar deugden en <rhyme label="c" type="f">manieren</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Die haar <rhyme label="c" type="f" corresp="#s1:l6">wezen</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Haar zoete byzyn, zal altyd <rhyme label="d" type="m">myn</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Grooten vreugde <rhyme label="d" type="m" corresp="#s1:l8">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Maar <rhyme label="a" type="m">Godin</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Stuurt ook haar <rhyme label="a" type="m" corresp="#s2:l1">zin</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat ik hoope op <rhyme label="a" type="m" corresp="#s2:l1">wedermin</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Dat naar <rhyme label="b" type="f">verblyden</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Ik ook <rhyme label="a" type="m" corresp="#s2:l1">ondervin</rhyme>:</l>
        <l id="s2:l6" met="3" type="+-">Ik kan alles <rhyme label="c" type="f">vinde</rhyme></l>
        <l id="s2:l7" met="3" type="+-">In â€™t oog myns <rhyme label="c" type="f" corresp="#s2:l6">beminden</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Ach! vont ook zy, alles in <rhyme label="d" type="m">my</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Ik was eeuwig <rhyme label="d" type="m" corresp="#s2:l8">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
