  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Op de Breestraet by de Hoeren van <rhyme label="a" type="m">Scheer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Daer heb ik myn geldje <rhyme label="a" type="m" corresp="#s1:l1">verteert</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Dat spyt my zoo <rhyme label="a" type="m" corresp="#s1:l1">zeer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ik ga 'er niet <rhyme label="a" type="m" corresp="#s1:l1">meer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Op de Breestraet by de Hoeren van <rhyme label="a" type="m" corresp="#s1:l1">Scheer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Hoeren op de Breestraet van <rhyme label="a" type="m">Scheer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die hebben my danssen <rhyme label="a" type="m" corresp="#s2:l1">geleert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ja danssen <rhyme label="a" type="m" corresp="#s2:l1">geleert</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Myn geldje <rhyme label="a" type="m" corresp="#s2:l1">verteert</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Op de Breestraet by de Hoeren van <rhyme label="a" type="m" corresp="#s2:l1">Scheer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Al gaen ik op de Breestraet by <rhyme label="a" type="m">Scheer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">By de Hoeren gaen ik niet <rhyme label="a" type="m" corresp="#s3:l1">meer</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Ik heb veel <rhyme label="a" type="m" corresp="#s3:l1">geleert</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Myn vingers <rhyme label="a" type="m" corresp="#s3:l1">bezeert</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Op de Breestraet by de Hoeren van <rhyme label="a" type="m" corresp="#s3:l1">Scheer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Nu gaen ik uit Scheer zyn huis <rhyme label="a" type="m">uit</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De Hoeren die lachen myn <rhyme label="a" type="m" corresp="#s4:l1">uit</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zy lachen zoo <rhyme label="a" type="m" corresp="#s4:l1">zeer</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Want ik hed 'er niet <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ik heb 't gelaten by de Hoeren van <rhyme label="a" type="m" corresp="#s4:l1">Scheer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ach Broeder nu valt my wat <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wat voor ons het beste wil <rhyme label="a" type="m" corresp="#s5:l1">zyn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Oostinje geeft <rhyme label="a" type="m" corresp="#s5:l1">weer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Twee maenden geld <rhyme label="a" type="m" corresp="#s5:l1">heer</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dan gaene wy zwiere by de Hoeren van <rhyme label="a" type="m" corresp="#s5:l1">Scheer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Nu gaen ik al weder by <rhyme label="a" type="m">Scheer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Twee maenden Geld heb ik nu <rhyme label="a" type="m" corresp="#s6:l1">weer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ik ben gebleven die <rhyme label="a" type="m" corresp="#s6:l1">nagt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Tot den andere <rhyme label="a" type="m" corresp="#s6:l1">dag</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Al weder myn Geldje zoo <rhyme label="a" type="m" corresp="#s6:l1">doorgebragt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ach Broeder nu weet ik geen <rhyme label="a" type="m">raed</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Wy moeten voor den duyvel aen <rhyme label="a" type="m" corresp="#s7:l1">boord</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Geen stuyver Geld <rhyme label="a" type="m" corresp="#s7:l1">meer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Den buydel is <rhyme label="a" type="m" corresp="#s7:l1">leeg</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Het is gebleven by de Hoeren van <rhyme label="a" type="m" corresp="#s7:l1">Scheer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Nu bennen wy weder aen <rhyme label="a" type="m">boord</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">De Ankers die ligten wy <rhyme label="a" type="m" corresp="#s8:l1">voort</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wy moeten na <rhyme label="a" type="m" corresp="#s8:l1">Zee</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Dat spyt my zoo <rhyme label="a" type="m" corresp="#s8:l1">zeer</rhyme>;</l>
        <l id="s8:l5" met="4" type="-+">Dat ik niet kan by de Hoeren van <rhyme label="a" type="m" corresp="#s8:l1">Scheer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Nu gaene wy weder na <rhyme label="a" type="m">Zee</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dat Geldje dat brengen wy <rhyme label="a" type="m" corresp="#s9:l1">mee</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ik verbly my zoo <rhyme label="a" type="m" corresp="#s9:l1">zeer</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Om de Hoeren van <rhyme label="a" type="m" corresp="#s9:l1">Scheer</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Myn Geldje breng ik daer <rhyme label="a" type="m" corresp="#s9:l1">weer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Wie heeft dan dit Liedje <rhyme label="a" type="m">gemaekt</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Op het Schip Medenblik is 't <rhyme label="a" type="m" corresp="#s10:l1">geraekt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Die het Liedje heeft <rhyme label="a" type="m" corresp="#s10:l1">gemaekt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Heeft het wel <rhyme label="a" type="m" corresp="#s10:l1">bedagt</rhyme>;</l>
        <l id="s10:l5" met="4" type="-+">Hy heeft al zyn Geldje door <rhyme label="a" type="m" corresp="#s10:l1">gebragt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
