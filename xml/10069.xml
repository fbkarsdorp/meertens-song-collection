    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VRijster had jy ronde <rhyme label="a" type="f">schijven</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Ghy soudt wel haest zyn <rhyme label="b" type="m">getrouwt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maar men laet uw daar om <rhyme label="a" type="f" corresp="#s1:l1">blyven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Vrijster hadje ronde <rhyme label="a" type="f" corresp="#s1:l1">schyven</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Ghy soud om geen Vryer <rhyme label="a" type="f" corresp="#s1:l1">kijven</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Vryers haken na het <rhyme label="b" type="m" corresp="#s1:l2">gout</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">'t Gout dat doet de beurse <rhyme label="a" type="f" corresp="#s1:l1">stijve</rhyme></l>
        <l id="s1:l8" met="4" type="+-">'t Maack een jonck al was hy <rhyme label="b" type="m" corresp="#s1:l2">out</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Vrijster had jy ronde <rhyme label="a" type="f" corresp="#s1:l1">schijve</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Ghy zoudt wel haest zyn <rhyme label="b" type="m" corresp="#s1:l2">getrouwt</rhyme>.</l>
      </lg>
    </lg>
