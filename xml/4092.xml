<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4092</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="Christmas song">kerstlied</tag>
      <tag translation="narrative song">verhalend lied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody>Heer Jezus Christus van Nazarene</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Alle die engelen van <rhyme label="a" type="f">hemelrijc</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ende al die herderkens van <rhyme label="a" type="f" corresp="#s1:l1">aertrijc</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Sij songen / si hadden groot <rhyme label="b" type="m">iolijt</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s1:l4">reene</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s1:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Herodes die heuet <rhyme label="a" type="f">vernomen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Daer is een nyen coninc <rhyme label="a" type="f" corresp="#s2:l1">gecomen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Die rijcker soude sijn dan <rhyme label="b" type="m">hi</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s2:l4">reene</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s2:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Herodes heuet <rhyme label="a" type="f">ghesworen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Hi sal alle kinderen dooden in <rhyme label="a" type="f" corresp="#s3:l1">thoren</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Die onder die twee iaren <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s3:l4">reene</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s3:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hi dede soecken hier en <rhyme label="a" type="f">daer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ionge kinderkens van twee <rhyme label="a" type="f" corresp="#s4:l1">iaer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Die dede hi alle nemen <rhyme label="b" type="m">dlijf</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s4:l4">reene</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s4:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Doen Maria dat <rhyme label="a" type="f">verhoorde</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Datmen alle kinderen <rhyme label="a" type="f" corresp="#s5:l1">vermoorde</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Des leedt si in haer hert <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s5:l4">reene</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s5:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Si sprac tot Ioseph sonder <rhyme label="a" type="f">sparen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Maect u bereet / laet ons gaen <rhyme label="a" type="f" corresp="#s6:l1">varen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Wi souden wech sijn / tis meer dan <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s6:l4">reene</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s6:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Maria quam aldaer <rhyme label="a" type="f">gheuaren</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Dair een man sijn coren ginc <rhyme label="a" type="f" corresp="#s7:l1">sayen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Si groete hem ende reet <rhyme label="b" type="m">voerbi</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s7:l4">reene</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s7:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">God groet v man diet coren <rhyme label="a" type="f">saeyt</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Comter yemant die v na mi <rhyme label="a" type="f" corresp="#s8:l1">vraecht</rhyme></l>
        <l id="s8:l3" met="4" type="-+">So segt dat ic hier leden <rhyme label="b" type="m">ben</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s8:l4">reene</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s8:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Doe Maria was wech <rhyme label="a" type="f">gheuaren</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Doen quam Herodes met sijnder <rhyme label="a" type="f" corresp="#s9:l1">scaren</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Dat coren was rijp ter seluer <rhyme label="b" type="m">tijt</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s9:l4">reene</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s9:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">God groet v man die mayt v <rhyme label="a" type="f">coren</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Saechdy hier eenige vrouwe te <rhyme label="a" type="f" corresp="#s10:l1">voren</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Die met haer hadde een <rhyme label="b" type="m">kindekijn</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s10:l4">reene</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s10:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Doen ic hier sayde dat ic nu <rhyme label="a" type="f">maye</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Doe sach ic hier een schoon vrouwe <rhyme label="a" type="f" corresp="#s11:l1">varen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Si was ghecleet met wit <rhyme label="b" type="m">habijt</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s11:l4">reene</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s11:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Herodes sprac tot sinen <rhyme label="a" type="f">heeren</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Wij willen weder thuyswaert <rhyme label="a" type="f" corresp="#s12:l1">keeren</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Si mach wel wten lande <rhyme label="b" type="m">sijn</rhyme>.</l>
        <l id="s12:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s12:l4">reene</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s12:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Herodes sprac tot sinen <rhyme label="a" type="f">lieden</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Hoort wat ic v sal <rhyme label="a" type="f" corresp="#s13:l1">ghebieden</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Gaet henen soect dat <rhyme label="b" type="m">kindekijn</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s13:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s13:l4">reene</rhyme></l>
        <l id="s13:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s13:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Si ghingen van lande te lande <rhyme label="a" type="f">varen</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Die onder de twee iaren <rhyme label="a" type="f" corresp="#s14:l1">waren</rhyme></l>
        <l id="s14:l3" met="4" type="-+">Die dooden sij tot alder <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s14:l4">reene</rhyme></l>
        <l id="s14:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s14:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Doen waenden sij dat kindeken te <rhyme label="a" type="f">verdoen</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Mer het was in egipten <rhyme label="a" type="f" corresp="#s15:l1">ghevloen</rhyme></l>
        <l id="s15:l3" met="4" type="-+">Ten wilde noch ghedoot sijn <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s15:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s15:l4">reene</rhyme></l>
        <l id="s15:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s15:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Duysent ende .xliiij. duysent <rhyme label="a" type="f">reyn</rhyme></l>
        <l id="s16:l2" met="4" type="-+">Waren daer ghedoot kinderen <rhyme label="a" type="f" corresp="#s16:l1">cleyn</rhyme></l>
        <l id="s16:l3" met="4" type="-+">Sij willen alle Gods engelen <rhyme label="b" type="m">sijn</rhyme>.</l>
        <l id="s16:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s16:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s16:l4">reene</rhyme></l>
        <l id="s16:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s16:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Nu bidden wij desen <rhyme label="a" type="f">kinde</rhyme></l>
        <l id="s17:l2" met="4" type="-+">Dat hi ons allen <rhyme label="a" type="f" corresp="#s17:l1">bringhe</rhyme></l>
        <l id="s17:l3" met="4" type="-+">Hier bouen in sijn eewich <rhyme label="b" type="m">rijck</rhyme></l>
        <l id="s17:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s17:l5" met="4" type="-+">Hi is gheboren van eender maghet <rhyme label="c" type="f" corresp="#s17:l4">reene</rhyme></l>
        <l id="s17:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s17:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Alle die engelen van <rhyme label="a" type="f">hemelrijc</rhyme></l>
        <l id="s18:l2" met="4" type="-+">Ende alle clergie van <rhyme label="a" type="f" corresp="#s18:l1">eertrijc</rhyme></l>
        <l id="s18:l3" met="4" type="-+">Si verblijt van desen <rhyme label="b" type="m">dinck</rhyme></l>
        <l id="s18:l4" met="4" type="-+">Iesus kerst van <rhyme label="c" type="f">nazareene</rhyme></l>
        <l id="s18:l5" met="4" type="-+">Gheboren van eender maghet <rhyme label="c" type="f" corresp="#s18:l4">reene</rhyme></l>
        <l id="s18:l6" met="4" type="-+">Daer om is God <rhyme label="b" type="m" corresp="#s18:l3">ghebenedijt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
