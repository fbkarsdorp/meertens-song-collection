    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SIjt nu verheucht weest <rhyme label="a" type="f">rustich</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En vrolijck van <rhyme label="b" type="m">ghemoet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ick min een Meysken <rhyme label="a" type="f" corresp="#s1:l1">lustich</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Schoon en vry // van aenschijn <rhyme label="c" type="m">bly</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Van spraeck en wesen <rhyme label="b" type="m" corresp="#s1:l2">soet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als ick haer mach <rhyme label="a" type="f">aenschouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soo is mijn hart <rhyme label="b" type="m">verheucht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ick seg adieu veel <rhyme label="a" type="f" corresp="#s2:l1">rouwen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">T'verlicht het hert, van pijn en <rhyme label="c" type="m">smert</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In mijne jonghe <rhyme label="b" type="m" corresp="#s2:l2">jeucht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy is nu in mijn <rhyme label="a" type="f">armen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ick cus haer rooder <rhyme label="b" type="m">mondt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Mocht ick haer eens <rhyme label="a" type="f" corresp="#s3:l1">verwarmen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dees waerde vrou // op eer en <rhyme label="c" type="m">trou</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Mijn hert sou zijn <rhyme label="b" type="m" corresp="#s3:l2">ghesont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dat nu noch niet wil <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Can sy my gunnen <rhyme label="b" type="m">haest</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Mijn druck mach sy <rhyme label="a" type="f" corresp="#s4:l1">ghenesen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Met fier ghelaet, en soete <rhyme label="c" type="m">praet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dus ben ick niet <rhyme label="b" type="m" corresp="#s4:l2">verbaest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Princen wilt vrolijck <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En drinckt den coelen <rhyme label="b" type="m">Wijn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als sy my troost wil <rhyme label="a" type="f" corresp="#s5:l1">gheven</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Die ick bemin // met hart en <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die liefste sal sy <rhyme label="b" type="m" corresp="#s5:l2">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
