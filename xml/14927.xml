  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">CAmeniere <rhyme label="a" type="f">Cameniere</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Gaet wat haest en comt ras <rhyme label="b" type="m">weer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wild u tot myn liefste <rhyme label="a" type="f" corresp="#s1:l1">stieren</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Seght myn Man die is te <rhyme label="b" type="m" corresp="#s1:l2">scheer</rhyme>,</l>
        <l id="s1:l5" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m">hola</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Ick hou de Vrouwe wel <rhyme label="d" type="f">sotte</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Die een schoone Maecht <rhyme label="e" type="m">begheert</rhyme>,</l>
        <l id="s1:l8" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m" corresp="#s1:l5">hola</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Ick hou de Vrouwe wel <rhyme label="d" type="f" corresp="#s1:l6">sotte</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Die een schoone Maecht <rhyme label="e" type="m" corresp="#s1:l7">begheert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">De Cameniere leeck wat <rhyme label="a" type="f">aerdich</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Nam haer parste saye <rhyme label="b" type="m">Huyck</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Sedy myn Heer myn Vrouwe <rhyme label="a" type="f" corresp="#s2:l1">waerdich</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Seght haer Man die is te <rhyme label="b" type="m" corresp="#s2:l2">scheer</rhyme>,</l>
        <l id="s2:l5" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m">hola</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Ick hou de Vrouwe wel <rhyme label="d" type="f">sotte</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Die een schoone Maecht <rhyme label="e" type="m">begheert</rhyme>,</l>
        <l id="s2:l8" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m" corresp="#s2:l5">hola</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Ick hou de Vrouwe wel <rhyme label="d" type="f" corresp="#s2:l6">sotte</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Die een schoone Maecht <rhyme label="e" type="m" corresp="#s2:l7">begheert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waerom soud ick gaen te <rhyme label="a" type="f">Roomen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Als d’aflaten men hier <rhyme label="b" type="m">vint</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hy omhelsche sonder <rhyme label="a" type="f" corresp="#s3:l1">schroomen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">En legt haer opt groentje <rhyme label="b" type="m" corresp="#s3:l2">neer</rhyme>,</l>
        <l id="s3:l5" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m">hola</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Ick hou de Vrouwe wel <rhyme label="d" type="f">sotte</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Die een schoone Maecht <rhyme label="e" type="m">begheert</rhyme>,</l>
        <l id="s3:l8" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m" corresp="#s3:l5">hola</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Ick hou de Vrouwe wel <rhyme label="d" type="f" corresp="#s3:l6">sotte</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Die een schoone Maecht <rhyme label="e" type="m" corresp="#s3:l7">begheert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer haer Vrou hun daer <rhyme label="a" type="f">bekipte</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Wat schand doeje roeptse <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Swijgt maer wy swijghen myn <rhyme label="a" type="f" corresp="#s4:l1">Vrouwe</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Sonder datmen maeckt <rhyme label="b" type="m" corresp="#s4:l2">misbaer</rhyme>.</l>
        <l id="s4:l5" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m">hola</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Ick hou de Vrouwe wel <rhyme label="d" type="f">sotte</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Die een schoone Maecht <rhyme label="e" type="m">begheert</rhyme>,</l>
        <l id="s4:l8" met="6" type="+-">Hola, hola, hola, hola, hola, <rhyme label="c" type="m" corresp="#s4:l5">hola</rhyme></l>
