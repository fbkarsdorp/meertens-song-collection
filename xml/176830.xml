    <melody>Psalm 128 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Godt weest hoogh <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van ons, voor al het <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ghy ons hebt <rhyme label="a" type="f" corresp="#s1:l1">bewesen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dees Dach in <rhyme label="b" type="m" corresp="#s1:l2">overvloet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ghy hebt al onse <rhyme label="c" type="f">weghen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van ‘s Duyvels macht <rhyme label="d" type="m">bevrijt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En beschermet wel te <rhyme label="c" type="f" corresp="#s1:l5">deghen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Onder u Vleug’len <rhyme label="d" type="m" corresp="#s1:l6">wijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Heer, wilt ons doch <rhyme label="a" type="f">vergheven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ons seylen groot en <rhyme label="b" type="m">kleen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die wy hebben <rhyme label="a" type="f" corresp="#s2:l1">bedreven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van dees Dach, en <rhyme label="b" type="m" corresp="#s2:l2">voor-heen</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">En als nu ons <rhyme label="c" type="f">Lichame</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dees Nacht te ruste <rhyme label="d" type="m">gaet</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Soo laet ons ziel oock <rhyme label="c" type="f" corresp="#s2:l5">t’same</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Rusten in u <rhyme label="d" type="m" corresp="#s2:l6">ghenad</rhyme>’.</l>
      </lg>
    </lg>
  </text>
</song>
