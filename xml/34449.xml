    <melody>Ik drink de nieuwe most</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">THaumantis mijn’ <rhyme label="a" type="m">vrindinn</rhyme>’,</l>
        <l id="s1:l2" met="3" type="-+">Vooghdersse van mijn’ <rhyme label="a" type="m" corresp="#s1:l1">minn</rhyme>’!</l>
        <l id="s1:l3" met="2" type="-+">Wiens soet <rhyme label="b" type="m">ghelaet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Verwint mijn’ hert’ en <rhyme label="a" type="m" corresp="#s1:l1">sinn</rhyme>’,</l>
        <l id="s1:l5" met="5" type="-+">En alle schoonheyt verr’ te boven <rhyme label="b" type="m" corresp="#s1:l3">gaet</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Mijn’ aerdtsche <rhyme label="a" type="m" corresp="#s1:l1">Af-godinn</rhyme>’</l>
        <l id="s1:l7" met="2" type="-+">Verkeert uw’ <rhyme label="c" type="f">zeden</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Drinckt med’-dooghen <rhyme label="a" type="m" corresp="#s1:l1">in</rhyme>:</l>
        <l id="s1:l9" met="2" type="-+">Het is wel <rhyme label="c" type="f" corresp="#s1:l7">reden</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Dat ghy my <rhyme label="c" type="f" corresp="#s1:l7">heden</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Weder-minne <rhyme label="d" type="m">draeght</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">O vrindelijcke <rhyme label="d" type="m" corresp="#s1:l11">Maeght</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">’t En is geen’ korten <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat ick u hebb’ <rhyme label="a" type="m" corresp="#s2:l1">ghevrijt</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">U hebb’ <rhyme label="b" type="m">gheviert</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Dat ick met alle <rhyme label="a" type="m" corresp="#s2:l1">vlijt</rhyme></l>
        <l id="s2:l5" met="5" type="-+">Hebb’ uwen lof door menigh dicht <rhyme label="b" type="m" corresp="#s2:l3">verciert</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">En ghy noch <rhyme label="a" type="m" corresp="#s2:l1">even-wel</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Spot met mijn’ <rhyme label="c" type="f">jonste</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En blijft even <rhyme label="a" type="m" corresp="#s2:l1">fel</rhyme>:</l>
        <l id="s2:l9" met="2" type="-+">Noch minn’, noch <rhyme label="c" type="f" corresp="#s2:l7">konste</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">O <rhyme label="c" type="f" corresp="#s2:l7">alder-schoonste</rhyme>!</l>
        <l id="s2:l11" met="3" type="+-">Soo veel wercken <rhyme label="d" type="m">kann</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Dat ghy my kiest voor <rhyme label="d" type="m" corresp="#s2:l11">man</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo ghy den Adel <rhyme label="a" type="m">mindt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ick ben een edel <rhyme label="a" type="m" corresp="#s3:l1">kindt</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Ghelijck als <rhyme label="b" type="m">ghy</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">En soo ghy zijt <rhyme label="a" type="m" corresp="#s3:l1">ghesint</rhyme></l>
        <l id="s3:l5" met="5" type="-+">Op gelt en schatten, die hebb’ ick oock <rhyme label="b" type="m" corresp="#s3:l3">by</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Al wat ghy wenschen <rhyme label="a" type="m" corresp="#s3:l1">meught</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Sal ick u <rhyme label="c" type="f">gheven</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">(Soo ghy my <rhyme label="a" type="m" corresp="#s3:l1">verheught</rhyme>)</l>
        <l id="s3:l9" met="2" type="-+">Wy konnen <rhyme label="c" type="f" corresp="#s3:l7">leven</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">En daer <rhyme label="c" type="f" corresp="#s3:l7">beneven</rhyme></l>
        <l id="s3:l11" met="3" type="+-">(Soo het u <rhyme label="d" type="m">ghelieft</rhyme>)</l>
        <l id="s3:l12" met="3" type="-+">Van alles zijn <rhyme label="d" type="m" corresp="#s3:l11">gherieft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick weet dat uw’ <rhyme label="a" type="m">Galant</rhyme></l>
        <l id="s4:l2" met="3" type="-+">(Die door uw’ minn’ oock <rhyme label="a" type="m" corresp="#s4:l1">brant</rhyme>)</l>
        <l id="s4:l3" met="2" type="-+">Veel praet van <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En dat den valschen <rhyme label="a" type="m" corresp="#s4:l1">quant</rhyme></l>
        <l id="s4:l5" met="5" type="-+">U doet ghelooven dat ick elders <rhyme label="b" type="m" corresp="#s4:l3">vry</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">Nochtans uw’ minn’ <rhyme label="a" type="m" corresp="#s4:l1">alleen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Wil ick <rhyme label="c" type="f">verwerven</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">‘k En soeck anders <rhyme label="a" type="m" corresp="#s4:l1">gheen</rhyme>:</l>
        <l id="s4:l9" met="2" type="-+">Moet ick die <rhyme label="c" type="f" corresp="#s4:l7">derven</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Soo moet ick <rhyme label="c" type="f" corresp="#s4:l7">sterven</rhyme>;</l>
        <l id="s4:l11" met="3" type="+-">‘k Leve (soo ghy <rhyme label="d" type="m">wilt</rhyme>)</l>
        <l id="s4:l12" met="3" type="-+">Oft val door Clothos <rhyme label="d" type="m" corresp="#s4:l11">schilt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En soo de bleecke <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s5:l2" met="3" type="-+">My ruckt na Charons <rhyme label="a" type="m" corresp="#s5:l1">boot</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Sal mijnen <rhyme label="b" type="m">gheest</rhyme></l>
        <l id="s5:l4" met="3" type="-+">(Die ghy van ’t lijf <rhyme label="a" type="m" corresp="#s5:l1">ontbloot</rhyme>)</l>
        <l id="s5:l5" met="5" type="-+">U komen maken dagh en nacht <rhyme label="b" type="m" corresp="#s5:l3">bevreest</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">‘k Sal in de <rhyme label="a" type="m" corresp="#s5:l1">duysternis</rhyme></l>
        <l id="s5:l7" met="2" type="-+">U komen <rhyme label="c" type="f">plaghen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">(Houdt het voor <rhyme label="a" type="m" corresp="#s5:l1">ghewis</rhyme>)</l>
        <l id="s5:l9" met="2" type="-+">Om dat mijn’ <rhyme label="c" type="f" corresp="#s5:l7">daghen</rhyme></l>
        <l id="s5:l10" met="2" type="-+">My zijn <rhyme label="c" type="f" corresp="#s5:l7">ontdraghen</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Door dijn stuer <rhyme label="d" type="m">ghemoet</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Het welck my sterven <rhyme label="d" type="m" corresp="#s5:l11">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
