    <melody>Laatst lag ik onder een boom</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Zang Heldin klinkt nu <rhyme label="a" type="m">alleen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Den lof van wyd berugte <rhyme label="b" type="f">lieden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die in uw zalen komen <rhyme label="a" type="m" corresp="#s1:l1">treên</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En u Valençe haar gunst <rhyme label="b" type="f" corresp="#s1:l2">aanbieden</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Ik zong wel <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Van Nymphjes <rhyme label="c" type="m" corresp="#s1:l5">teer</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">En Harders, maar nu swierd een ander <rhyme label="c" type="m" corresp="#s1:l5">veer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'k Verban nu Venus, en haar <rhyme label="a" type="m">Zoon</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die niemant ooit en heeft <rhyme label="b" type="f">verdonkerd</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Valençia uw goude <rhyme label="a" type="m" corresp="#s2:l1">kroon</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Staat nu op 't heerelykst en <rhyme label="b" type="f" corresp="#s2:l2">flonkerd</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Ey zie ze <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Die tot de <rhyme label="c" type="m" corresp="#s2:l5">Maan</rhyme></l>
        <l id="s2:l7" met="5" type="-+">Door haar ontelb're Lauwerieren <rhyme label="c" type="m" corresp="#s2:l5">gaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
