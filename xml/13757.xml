<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13757</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>shepherd's song</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GAet voort mijn wolligh <rhyme label="a" type="m">vee</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En graest van <rhyme label="a" type="m" corresp="#s1:l1">lieverlee</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Terwijl dat ick moet <rhyme label="b" type="f">beyde</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hier by dees klaere <rhyme label="c" type="m">bron</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ghelijck mijn <rhyme label="c" type="m" corresp="#s1:l4">morghen-son</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn gister-avondt <rhyme label="b" type="f" corresp="#s1:l3">seyde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Aurora <rhyme label="a" type="m">dagh-bodin</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ick bid, doet mijn <rhyme label="a" type="m" corresp="#s2:l1">goddin</rhyme></l>
        <l id="s2:l3" met="3" type="-+">De paeden veyligh <rhyme label="b" type="f">maecken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ay goden voeght u <rhyme label="c" type="m">t’saem</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En maeckt dees plaets <rhyme label="c" type="m" corresp="#s2:l4">bequaem</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tot uytkomst van mijn <rhyme label="b" type="f" corresp="#s2:l3">saecken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn dunckt ick sie van <rhyme label="a" type="m">verr</rhyme>’</l>
        <l id="s3:l2" met="3" type="-+">Mijn goude <rhyme label="a" type="m" corresp="#s3:l1">morghen-sterr</rhyme>’</l>
        <l id="s3:l3" met="3" type="-+">Al soetjens komen <rhyme label="b" type="f">treden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Duyckt Phebus, Phebus <rhyme label="c" type="m">duyckt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">V glans-rijck-oogh <rhyme label="c" type="m" corresp="#s3:l4">toeluyckt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">‘k Ben met dees Son te <rhyme label="b" type="f" corresp="#s3:l3">vreden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Mijn waerde <rhyme label="a" type="m">pronck-goddin</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wat loon sal ick u <rhyme label="a" type="m" corresp="#s4:l1">min</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Segh Galathea <rhyme label="b" type="f">gheven</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Swijgh Cloris, Cloris <rhyme label="c" type="m">swijght</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Mijn kaeck een bloosje <rhyme label="c" type="m" corresp="#s4:l4">krijght</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ach! wat heb ick <rhyme label="b" type="f" corresp="#s4:l3">bedreven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoe dus mijn waerde <rhyme label="a" type="m">vrouw</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wat nevellighe <rhyme label="a" type="m" corresp="#s5:l1">rouw</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Doet dus u ziel <rhyme label="b" type="f">ontstellen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wat isser dat u <rhyme label="c" type="m">smert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Of u benauwde <rhyme label="c" type="m" corresp="#s5:l4">hert</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met kille-vrees komt <rhyme label="b" type="f" corresp="#s5:l3">quellen</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ik bid u lief <rhyme label="a" type="m">verschoon</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mijn min, die ick <rhyme label="a" type="m" corresp="#s6:l1">betoon</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Door te <rhyme label="b" type="f">vrypostigheden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">‘k Doe meer dan my <rhyme label="c" type="m">betaemt</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">De maeghdelijcke <rhyme label="c" type="m" corresp="#s6:l4">schaemt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dwinght my tot <rhyme label="b" type="f" corresp="#s6:l3">droevigheden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Doch so ick heb <rhyme label="a" type="m">misdaen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wilt by geen ander <rhyme label="a" type="m" corresp="#s7:l1">gaen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En daer mijn fout <rhyme label="b" type="f">vertellen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Want soo ghy ‘t Cloris <rhyme label="c" type="m">dee</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ick soud’ door harten <rhyme label="c" type="m" corresp="#s7:l4">wee</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Gaen daelen laegh ter <rhyme label="b" type="f" corresp="#s7:l3">hellen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Neen lief, veel eer de <rhyme label="a" type="m">aerdt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Sich met den hemel <rhyme label="a" type="m" corresp="#s8:l1">paerdt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Eer ick u soude <rhyme label="b" type="f">schennen</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Oock hebt ghy niet <rhyme label="c" type="m">misdaen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">‘kBid’ laet u trueren <rhyme label="c" type="m" corresp="#s8:l4">staen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En wilt tot vreughde <rhyme label="b" type="f" corresp="#s8:l3">wennen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wel aen dan ick <rhyme label="a" type="m">vertrouw</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En Cloris daer voor <rhyme label="a" type="m" corresp="#s9:l1">houw</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Komt laet ons boswaert <rhyme label="b" type="f">treden</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Ick set mijn ziel te <rhyme label="c" type="m">rust</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Daer op wert ghy <rhyme label="c" type="m" corresp="#s9:l4">ghekust</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Tot slot van onse <rhyme label="b" type="f" corresp="#s9:l3">reden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
