<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>29333</id>
    <date>
      <year-start>1576</year-start>
      <year-end>1600</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
