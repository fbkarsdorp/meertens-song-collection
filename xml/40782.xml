<song>
  <meta>
    <id>40782</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 034 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'k SIl, on-ophâdd'lijck', <rhyme label="a" type="m">nea</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Sleau, laerdig, all' mijn libb'ne <rhyme label="b" type="m">stuwn'</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">d'HEER' God lof-sjongje', uyt <rhyme label="b" type="m" corresp="#s1:l2">herte-gruwn'</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">'t Leyt m' yn mijn muwlle', az <rhyme label="a" type="m" corresp="#s1:l1">brea</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">O so sil 'k my <rhyme label="c" type="m">ontjaen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ynne' HEER', dat dy seftmoed'ge <rhyme label="d" type="m">ljue</rhyme></l>
        <l id="s1:l7" met="4" type="-+">My herckje', in yn Godsill'ge <rhyme label="d" type="m" corresp="#s1:l6">njue</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Gaest blier, 't sell' lof-luwd' <rhyme label="c" type="m" corresp="#s1:l5">slaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Meyts' great, mey my, meytfs' <rhyme label="a" type="m">great</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Meyts' great dy greate' HEER' God, mey <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Litt' uwz sijn oer-great' Namme', <rhyme label="b" type="m" corresp="#s2:l2">hert-bly</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op-helje' oer' wijd' <rhyme label="a" type="m" corresp="#s2:l1">Wrâd-kleatt'</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">'k Hab sijke' uwz ljeav' mijlde <rhyme label="c" type="m">HEER'</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Sijkjende fuwn', <rhyme label="d" type="m">treast-antwird'-ree</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">My lossjende' uwt neare aengste' in <rhyme label="d" type="m" corresp="#s2:l6">wee</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Tanck habb' God, yn yv'ge' <rhyme label="c" type="m" corresp="#s2:l5">eer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dy fromme' habbe' him oons <rhyme label="a" type="m">joen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ja ney him sljuercke' az streamm'-wiets <rhyme label="b" type="m">rin</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Jae wird'ne nea rea-schamme', yn <rhyme label="b" type="m" corresp="#s3:l2">nin</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Herr' dwaen, affe' op him <rhyme label="a" type="m" corresp="#s3:l1">stoen'</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">d'Illind'ge kiermde' in <rhyme label="c" type="m">bea</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Luwd-rofte' yn gâl: God hercke', in <rhyme label="d" type="m">holp'</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Him uwtte aengst', See-djiepp' gruwle' <rhyme label="d" type="m" corresp="#s3:l6">oer-stjolp'</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wijde' yp'ne tuwt fenn' <rhyme label="c" type="m" corresp="#s3:l5">Dea</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Goads Ing'le leg'ret <rhyme label="a" type="m">him</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ruwn om dy jinge, waems from <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Goads freeze' huwzwest', dat jearm naet <rhyme label="b" type="m" corresp="#s4:l2">dert</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hy tjochtse' uwtt' nea-druwcx <rhyme label="a" type="m" corresp="#s4:l1">klimm'</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Kijts', loyts', sjog, preuwkje' in <rhyme label="c" type="m">smeyts'</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ho goe de HEERE' iz: wa op him <rhyme label="d" type="m">set</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Sijn hoop'-klim, dear 's naet dat him <rhyme label="d" type="m" corresp="#s4:l6">let</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Hy swomt yn <rhyme label="c" type="m" corresp="#s4:l5">siel-formeyts</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Goad-heyl'ge, yn fromme' <rhyme label="a" type="m">yen-fâd'</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Goad-frees'ne' ô freesje' uwz ljeave HEER' <rhyme label="b" type="m">God</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Want dy him freesje' (ô sillig <rhyme label="b" type="m" corresp="#s5:l2">lot</rhyme>!)</l>
        <l id="s5:l4" met="3" type="-+">Ontbreckt nin <rhyme label="a" type="m" corresp="#s5:l1">onder-hâd</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">'t Lieuw'-jong, yn 't woedjen <rhyme label="c" type="m">dol</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Somm's hong're ly't, ho red to <rhyme label="d" type="m">jacht</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Mar, dy de' HEER' sijkje, dey in <rhyme label="d" type="m" corresp="#s5:l6">nacht</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">'t Rint herre', yn all' dwaen <rhyme label="c" type="m" corresp="#s5:l5">wol</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Komm' jong' tjierll', lien' my 't <rhyme label="a" type="m">ear</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Herts', swiete jeugd; Ick bin yn <rhyme label="b" type="m">til</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Mey' Goads freez', dy 'k ontfâdje <rhyme label="b" type="m" corresp="#s6:l2">sil</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Herts', bernkens, ney mijn <rhyme label="a" type="m" corresp="#s6:l1">leer'</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Iz immens hert in <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Lang libbjen? deagen folle' in <rhyme label="d" type="m">nolck</rhyme>?</l>
        <l id="s6:l7" met="4" type="-+">Om wolferts floede', uwt Goads <rhyme label="d" type="m" corresp="#s6:l6">seyn-kolck</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">t'Ynjen? (ô swiet <rhyme label="c" type="m" corresp="#s6:l5">gewin</rhyme>!)</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dy folg' mijn rie: Dat <rhyme label="a" type="m">hy</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Sijn tong' fen quea kat knett' ynn' <rhyme label="b" type="m">knôtt'</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dat hy sijn lipp', so, sluwte', yn 't <rhyme label="b" type="m" corresp="#s7:l2">slôt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dats' nea bedroch <rhyme label="a" type="m" corresp="#s7:l1">uwt-spy'</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Wijckje' oef fen quea, dog <rhyme label="c" type="m">goe</rhyme>;</l>
        <l id="s7:l6" met="4" type="-+">Sijkje', eernje' in folgje', (yn wol in <rhyme label="d" type="m">wee</rhyme>)</l>
        <l id="s7:l7" met="4" type="-+">Dy oer-djoer', gea-leaz', God-ljeav' <rhyme label="d" type="m" corresp="#s7:l6">Free</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">'t Wirdst' dat dy Wrâd ae <rhyme label="c" type="m" corresp="#s7:l5">sjoe</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Goads klear sjeande' eag stiet <rhyme label="a" type="m">opp'</rhyme></l>
        <l id="s8:l2" met="4" type="-+">R juechtfirdige', ô sijn yp'ne' ear' <rhyme label="b" type="m">let</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Op jearme' eernst-heftige' <rhyme label="b" type="m" corresp="#s8:l2">herts-gebedt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Op herre', ynn' neade', <rhyme label="a" type="m" corresp="#s8:l1">holp-rop</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Mar, sijn ontstits'ne, <rhyme label="c" type="m">soer</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Grijmme'eansicht' striel't, quea-dieders, <rhyme label="d" type="m">trog</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Snijt jerm' tinss'nisse', az mey in <rhyme label="d" type="m" corresp="#s8:l6">hogg'</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Uwtte' Yerde', in wijt se' yn 't <rhyme label="c" type="m" corresp="#s8:l5">fjoer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De' oprjuechte ljue, <rhyme label="a" type="m">for'-seyd</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Dy roppe de' HEERE', yn gaest-driftme, <rhyme label="b" type="m">oon</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Hy hercketse', hy seyntse', uwt sijn <rhyme label="b" type="m" corresp="#s9:l2">troon'</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Holp' yn herr' <rhyme label="a" type="m" corresp="#s9:l1">swierigheyt'</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">d'Holp-firdde' HEERE' iz nea <rhyme label="c" type="m">fier</rhyme>;</l>
        <l id="s9:l6" met="4" type="-+">d'Hert-breck' Suwn'-bicht're, ynn' gaest <rhyme label="d" type="m">forsleyn</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Behad-mijld rjuecht hy him <rhyme label="d" type="m" corresp="#s9:l6">oereyn'</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Loyts', so 's him de' HEER' <rhyme label="c" type="m" corresp="#s9:l5">goertier</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">d'Rjuechtfird'ge, from in <rhyme label="a" type="m">sljuecht</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Duwck't ond're 't steez-oone' uwncke' op <rhyme label="b" type="m">uwnck'</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Mar Goads hân blieuwt, all' stuwne', him <rhyme label="b" type="m" corresp="#s10:l2">njuenck'</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Stoan't, stjuwg't, till't, hâd 't him <rhyme label="a" type="m" corresp="#s10:l1">rjuecht</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Hy werret yen for <rhyme label="c" type="m">yen</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">(: Ho kin 't de' op-rjuechte' ae quelck oef <rhyme label="d" type="m">quea</rhyme>!)</l>
        <l id="s10:l7" met="4" type="-+">Sijn eyn lijck'niss', fen ljea to <rhyme label="d" type="m" corresp="#s10:l6">ljea</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Dat jearm' nea breckt yen <rhyme label="c" type="m" corresp="#s10:l5">bien</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Mey boosheyt' wirt <rhyme label="a" type="m">oer-kolpp'</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Dy godleaze'; eack, dy de' oprjuechte' <rhyme label="b" type="m">haett'</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Wirt trogge' eyn-schild'ge dey-dolck' <rhyme label="b" type="m" corresp="#s11:l2">daet</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Ynn' Dea-gluwps woeste' <rhyme label="a" type="m" corresp="#s11:l1">oer-gjolpp'</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Dogg 't sill'ge siel loss't <rhyme label="c" type="m">God</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Sijns tjienners. All' wa de HEER' <rhyme label="d" type="m">fortrouwt</rhyme></l>
        <l id="s11:l7" met="4" type="-+">Tjuwg't hy onschildig. Dearom <rhyme label="d" type="m" corresp="#s11:l6">bouwt</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Elck fromme' op him sijn <rhyme label="c" type="m" corresp="#s11:l5">slot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
