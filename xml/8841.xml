<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8841</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="psalm">psalm</tag>
    </tags>
    <melody>Von allen Menschen abgewand</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O God mijn Godt, ick waecke <rhyme label="a" type="m">vroech</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Mijn Siel heeft groot <rhyme label="b" type="f">Verlanghe</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Nae dy in desen Landen <rhyme label="a" type="m" corresp="#s1:l1">drooch</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Mijn Vleesch is oock wel <rhyme label="b" type="f" corresp="#s1:l2">Bange</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Ick haeck' al nae dijn <rhyme label="c" type="m">Heylichdom</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Want dijne Goetheyt is so <rhyme label="c" type="m" corresp="#s1:l5">Vroom</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Jae, Beter dan dit <rhyme label="d" type="f">Leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daer woud' ick geerne Loven <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Mijn Leven lanck dy <rhyme label="b" type="f">Roemen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Op heffen mijne handen <rhyme label="a" type="m" corresp="#s2:l1">bly</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In dijn Naem sonder <rhyme label="b" type="f" corresp="#s2:l2">schroomen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Dat waer doch in mijns herten <rhyme label="c" type="m">gront</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Mijn Vreuchd', dat ick met blijden <rhyme label="c" type="m" corresp="#s2:l5">Mont</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dy staedich mochte <rhyme label="d" type="f">Prijsen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick legg' my slapen, oft ick <rhyme label="a" type="m">Waeck</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van dy ick Denck' en <rhyme label="b" type="f">spreke</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Onder dijn Vleughel my <rhyme label="a" type="m" corresp="#s3:l1">vermaeck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dijn Hulp my niet <rhyme label="b" type="f" corresp="#s3:l2">ontbreke</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Mijn Siel ghestadich hangt aen <rhyme label="c" type="m">dy</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Dijn Rechte Handt bewaret <rhyme label="c" type="m" corresp="#s3:l5">my</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat ick mach vrolijck <rhyme label="d" type="f">singhen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy staen na myner Sielen <rhyme label="a" type="m">fel</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Verschoonen niet met <rhyme label="b" type="f">allen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Maer sullen varen all' ter <rhyme label="a" type="m" corresp="#s4:l1">Hel</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Int Sweert sy moeten <rhyme label="b" type="f" corresp="#s4:l2">vallen</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Den Vossen worden sy te <rhyme label="c" type="m">deel</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Bedroghen end' bedorven <rhyme label="c" type="m" corresp="#s4:l5">heel</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat is haer loon na <rhyme label="d" type="f">rechte</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Coninck hem in Godt <rhyme label="a" type="m">verheucht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met blijtschap nae <rhyme label="b" type="f">betame</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wie hem ghetroulijck tot hem <rhyme label="a" type="m" corresp="#s5:l1">voecht</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die crijcht een loflijck <rhyme label="b" type="f" corresp="#s5:l2">name</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Maer all' die valsch van tonghen <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Voor ooghen goet, maer vol <rhyme label="c" type="m" corresp="#s5:l5">fenijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Met schand' noch sullen <rhyme label="d" type="f">swijghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
