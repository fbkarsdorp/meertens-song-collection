<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>38620</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Ik prees weleer de stille eenzaamheden</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Ick mach als Maro wel van Troyen <rhyme label="a" type="f">spreken</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat eertijts wierdt <rhyme label="a" type="f" corresp="#s1:l1">ghesteken</rhyme></l>
        <l id="s1:l3" met="1" type="-+">Inden <rhyme label="b" type="m">brandt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Och doet my <rhyme label="b" type="m" corresp="#s1:l3">onderstant</rhyme>!</l>
        <l id="s1:l5" met="5" type="-+">AEneas droech sijn vader uyt het <rhyme label="c" type="m">vier</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Ick blijf alleenich <rhyme label="c" type="m" corresp="#s1:l5">hier</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Daer my de ginsters van de minne <rhyme label="d" type="f">loncken</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Het hert doen <rhyme label="d" type="f" corresp="#s1:l7">voncken</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Als een AEthna <rhyme label="c" type="m" corresp="#s1:l5">schier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Want siet Helena wederom <rhyme label="a" type="f">herschapen</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Daer teghen kan gheen <rhyme label="a" type="f" corresp="#s2:l1">wapen</rhyme></l>
        <l id="s2:l3" met="1" type="-+">Haer <rhyme label="b" type="m">ghesicht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Is als een <rhyme label="b" type="m" corresp="#s2:l3">blixem-licht</rhyme>:</l>
        <l id="s2:l5" met="5" type="-+">Sy is veel weerder als de schoon <rhyme label="c" type="m">Heleên</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met oodtmoet <rhyme label="c" type="m" corresp="#s2:l5">aenghebeên</rhyme>,</l>
        <l id="s2:l7" met="5" type="-+">Haer eer en deucht vlieght boven al de <rhyme label="d" type="f">sterren</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Mijn sinnen <rhyme label="d" type="f" corresp="#s2:l7">werren</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">In haer <rhyme label="c" type="m" corresp="#s2:l5">godd'lijckheên</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Robbijne lippen, ach yvoire <rhyme label="a" type="f">tanden</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">O beldt van Iupijns <rhyme label="a" type="f" corresp="#s3:l1">handen</rhyme>!</l>
        <l id="s3:l3" met="1" type="-+">Wiens <rhyme label="b" type="m">tael</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Verdooft den <rhyme label="b" type="m" corresp="#s3:l3">achtegael</rhyme>?</l>
        <l id="s3:l5" met="5" type="-+">Gheen Goden konnen met den Nectar <rhyme label="c" type="m">wijn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dunckt my soo vrolijck <rhyme label="c" type="m" corresp="#s3:l5">sijn</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">Dan ick, als voor mijn ooghen maer ten toon <rhyme label="d" type="f">leydt</rhyme></l>
        <l id="s3:l8" met="2" type="-+">De groote <rhyme label="d" type="f" corresp="#s3:l7">schoonheydt</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Van haer lief <rhyme label="c" type="m" corresp="#s3:l5">aenschijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">O! pronck der maeghden, setel vande <rhyme label="a" type="f">wijsheyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghy die den loon en prijs <rhyme label="a" type="f" corresp="#s4:l1">sijt</rhyme></l>
        <l id="s4:l3" met="1" type="-+">Ende <rhyme label="b" type="m">winst</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Van alle mijnen <rhyme label="b" type="m" corresp="#s4:l3">dinst</rhyme>:</l>
        <l id="s4:l5" met="5" type="-+">Ick laet Abisach en Rebecca <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s4:l6" met="3" type="-+">De lofbaer vrouwe <rhyme label="c" type="m" corresp="#s4:l5">schaer</rhyme>:</l>
        <l id="s4:l7" met="5" type="-+">Helena ach Helena sal ick <rhyme label="d" type="f">roemen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Moeght ick my <rhyme label="d" type="f" corresp="#s4:l7">noemen</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Maer een slaef van <rhyme label="c" type="m" corresp="#s4:l5">haer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
