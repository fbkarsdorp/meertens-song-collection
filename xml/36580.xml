<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>36580</id>
    <date>
      <year-start>1722</year-start>
      <year-end>1722</year-end>
    </date>
    <location>
      <orginal-name>duinkerken</orginal-name>
      <standard-name>Duinkerken, France</standard-name>
      <coordinates>
        <lon>51.1907405</lon>
        <lat>2.4978867</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kluchtlied" translation="humorous song">kluchtlied</tag>
    </tags>
    <melody>Meisjes die de tijd verdriet</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Uyt-strijckers stelt u op de <rhyme label="a" type="m">baen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met suypers, <rhyme label="b" type="f">deughenieten</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Om naer het Lecker-landt te <rhyme label="a" type="m" corresp="#s1:l1">gaen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Laet het u niet <rhyme label="b" type="f" corresp="#s1:l2">verdrieten</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Want een Schip seer lanck en <rhyme label="c" type="m">breedt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Light tot Duynkerck al <rhyme label="c" type="m" corresp="#s1:l5">ghereedt</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Die weg wilt vaeren maeckt u <rhyme label="c" type="m" corresp="#s1:l5">bereedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daer zijn dry hondert <rhyme label="a" type="m">Winckeliers</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die hebben 't al <rhyme label="b" type="f">versopen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Met wel ses hondert <rhyme label="a" type="m" corresp="#s2:l1">Herbergiers</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Zijn in dit Schip <rhyme label="b" type="f" corresp="#s2:l2">ghekropen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Gaet op 't Schip al slecht en <rhyme label="c" type="m">goe</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Al die zijt van wercken <rhyme label="c" type="m" corresp="#s2:l5">moe</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Om wegh te vaeren naer 't Eylandt <rhyme label="c" type="m" corresp="#s2:l5">toe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daer sult ghy Banckeroetiers <rhyme label="a" type="m">ghelijck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Te gaeder weeldigh <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sa haest u die wilt wesen <rhyme label="a" type="m" corresp="#s3:l1">rijck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wilt u op 't Schip <rhyme label="b" type="f" corresp="#s3:l2">begheven</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Spoeyt u deughenieten <rhyme label="c" type="m">gaet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Op het Schip eer 't wordt te <rhyme label="c" type="m" corresp="#s3:l5">laet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Want hier te leven 'k en weet gheen <rhyme label="c" type="m" corresp="#s3:l5">raedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Capiteyn die heet van <rhyme label="a" type="m">Lier</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hy is te leegh te <rhyme label="b" type="f">wercken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Het is den meesten <rhyme label="a" type="m" corresp="#s4:l1">Banckroetier</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Soo ieder wel kan <rhyme label="b" type="f" corresp="#s4:l2">mercken</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Die wilt op-gheschreven <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Vindt u al in 't wilde <rhyme label="c" type="m" corresp="#s4:l5">swijn</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Haest drincke-broers die leeft in <rhyme label="c" type="m" corresp="#s4:l5">pijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want Pier Fricas dat is den <rhyme label="a" type="m">Kock</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En Joos suyp-uyt is <rhyme label="b" type="f">deken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Den Schipper heet Jan <rhyme label="a" type="m" corresp="#s5:l1">Pelsserock</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die sal het Schip <rhyme label="b" type="f" corresp="#s5:l2">af-steken</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Spoeyt u volck van achter <rhyme label="c" type="m">nat</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Stelt u haestigh op de <rhyme label="c" type="m" corresp="#s5:l5">pat</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Vaeght aen de poort hier u vuyl <rhyme label="c" type="m" corresp="#s5:l5">gat</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Veel bijstier Koop-lieden met <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die gaen ons oock <rhyme label="b" type="f">verlaeten</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Met al die hier en elders <rhyme label="a" type="m" corresp="#s6:l1">scheen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Te wesen hoogh van <rhyme label="b" type="f" corresp="#s6:l2">staeten</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Cachet, Wapens van dees <rhyme label="c" type="m">Lien</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Worden op dit Schip <rhyme label="c" type="m" corresp="#s6:l5">ghesien</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Om staet en af-komst te gaen <rhyme label="c" type="m" corresp="#s6:l5">bedien</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Noch wordt dit schoone Schip <rhyme label="a" type="m">gelaen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Met duysent <rhyme label="b" type="f">Boodts-ghesellen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die al 't schoon goedt heben <rhyme label="a" type="m" corresp="#s7:l1">verdaen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Moeten nu luysen <rhyme label="b" type="f" corresp="#s7:l2">tellen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Al die leven in 't <rhyme label="c" type="m">verdriet</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Koop-liens en Marchands van <rhyme label="c" type="m" corresp="#s7:l5">niet</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Niemant het vaeren u en <rhyme label="c" type="m" corresp="#s7:l5">verbiedt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Soo wel van 't Landt als uyt de <rhyme label="a" type="m">Sted'</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Komt volck van kalaerts <rhyme label="b" type="f">bende</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Gaet op het Schip en vaert doch <rhyme label="a" type="m" corresp="#s8:l1">med'</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Goed' daghen zijn ten <rhyme label="b" type="f" corresp="#s8:l2">ende</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Backers, Brouwers vooren <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Met de Koopliens oock van <rhyme label="c" type="m" corresp="#s8:l5">graen</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Laet de schut-vorcke en paele <rhyme label="c" type="m" corresp="#s8:l5">staen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Veel Lijne-Weverkens te <rhyme label="a" type="m">gaer</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Gaen 't Lecker-landt oock <rhyme label="b" type="f">soecken</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Schoen-lappers, menigh <rhyme label="a" type="m" corresp="#s9:l1">Matsenaer</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Die 't wercken schier <rhyme label="b" type="f" corresp="#s9:l2">vervloecken</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Om dat de winst is te <rhyme label="c" type="m">kleen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Druckers, Schilderaers met <rhyme label="c" type="m" corresp="#s9:l5">een</rhyme></l>
        <l id="s9:l7" met="4" type="-+">Sy willen van dees kunst gaen <rhyme label="c" type="m" corresp="#s9:l5">scheen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Sneyers en Zeyle-maeckers <rhyme label="a" type="m">vroet</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Hebben niet veel te <rhyme label="b" type="f">doene</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Met ander Kooplien sonder <rhyme label="a" type="m" corresp="#s10:l1">goedt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Ambachten by <rhyme label="b" type="f" corresp="#s10:l2">millioenen</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Die te veel hier schuldigh <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Suypers van den <rhyme label="c" type="m" corresp="#s10:l5">Brandewijn</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Gaet oock te scheep dronck' als een <rhyme label="c" type="m" corresp="#s10:l5">swijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Legaerts en suypers <rhyme label="a" type="m">altemael</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">En magh ick niet <rhyme label="b" type="f">vergheten</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Die van Jan pover oft Pier <rhyme label="a" type="m" corresp="#s11:l1">schrael</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Seer stijf worden <rhyme label="b" type="f" corresp="#s11:l2">ghebeten</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Hoort wel wat dat ick u <rhyme label="c" type="m">segh</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Die met 't Schip wilt vaeren <rhyme label="c" type="m" corresp="#s11:l5">wegh</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Moet sien naer Bacchus die kaele <rhyme label="c" type="m" corresp="#s11:l5">vlegh</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ghy jonghe Vrijsters sonder <rhyme label="a" type="m">goedt</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Die 't Maeghdom zijt <rhyme label="b" type="f">ghenomen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Wilt ghy oock spelen <rhyme label="a" type="m" corresp="#s12:l1">banckeroet</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En voor Zee-baeren <rhyme label="b" type="f" corresp="#s12:l2">schroomen</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Zijt ghy 't water <rhyme label="c" type="m">onghewent</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Ghy kont wachten een <rhyme label="c" type="m" corresp="#s12:l5">moment</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Tot dat Pier Pack op den  waghen <rhyme label="c" type="m" corresp="#s12:l5">sendt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
