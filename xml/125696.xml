        <l id="s1:l4" met="3" type="-+">'k Beloof dat ik haar zou <rhyme label="c" type="f">steeken</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Gy verstaat my <rhyme label="b" type="m" corresp="#s1:l3">wel</rhyme>.</l>
