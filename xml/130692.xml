  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Weg, Venus <rhyme label="a" type="m">wigt</rhyme>!</l>
        <l id="s1:l2" met="2" type="-+">Met al uw <rhyme label="b" type="f">liegen</rhyme>,</l>
        <l id="s1:l3" met="1" type="-+"><rhyme label="b" type="f" corresp="#s1:l2">Bedriegen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gaet vry uit mijn <rhyme label="a" type="m" corresp="#s1:l1">gezigt</rhyme>!</l>
        <l id="s1:l5" met="2" type="-+">Weg, snoode <rhyme label="c" type="m">meid</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Die mijn <rhyme label="c" type="m" corresp="#s1:l5">verleid</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Gy sult mijn soo in slaep niet <rhyme label="b" type="f" corresp="#s1:l2">wiegen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wat heb ik <rhyme label="a" type="m">staag</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Mijn tijd <rhyme label="b" type="f">versleten</rhyme>,</l>
        <l id="s2:l3" met="1" type="-+"><rhyme label="b" type="f" corresp="#s2:l2">Vergeten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Om u, trouwloose <rhyme label="a" type="m" corresp="#s2:l1">Maagd</rhyme>!</l>
        <l id="s2:l5" met="2" type="-+">Maer nu <rhyme label="c" type="m">voortaan</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Is dat <rhyme label="c" type="m" corresp="#s2:l5">gedaan</rhyme></l>
        <l id="s2:l7" met="4" type="-+">En schey ’er uit,   dit moet gy <rhyme label="b" type="f" corresp="#s2:l2">weten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Gy toonde <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Veel <rhyme label="b" type="f">soetigheden</rhyme>,</l>
        <l id="s3:l3" met="1" type="-+">Maer <rhyme label="b" type="f" corresp="#s3:l2">heden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sien ik, is het maar <rhyme label="a" type="m" corresp="#s3:l1">schijn</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Gy zwoer mijn <rhyme label="c" type="m">trouw</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">O schoone <rhyme label="c" type="m" corresp="#s3:l5">Vrouw</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">En nu lagt gy wat met mijn <rhyme label="b" type="f" corresp="#s3:l2">reden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ik wierd <rhyme label="a" type="m">altijd</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Van u <rhyme label="b" type="f">gepresen</rhyme></l>
        <l id="s4:l3" met="1" type="-+">Voor <rhyme label="b" type="f" corresp="#s4:l2">dezen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maer nu, eerloose <rhyme label="a" type="m" corresp="#s4:l1">meid</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Wijst gy mijn <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Voort na mijn <rhyme label="c" type="m" corresp="#s4:l5">graf</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Maer ’t kan geen kwaad, ’t mag soo wel <rhyme label="b" type="f" corresp="#s4:l2">wesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Gy bedriegt <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">O <rhyme label="b" type="f">Tygerinne</rhyme>,</l>
        <l id="s5:l3" met="1" type="-+">Door ’t <rhyme label="b" type="f" corresp="#s5:l2">minnen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En door uw valsche <rhyme label="a" type="m" corresp="#s5:l1">schijn</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Adieu, ‘k ga <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">- Weg mal <rhyme label="c" type="m" corresp="#s5:l5">geween</rhyme> -</l>
        <l id="s5:l7" met="4" type="-+">En trek van u gantsch af mijn <rhyme label="b" type="f" corresp="#s5:l2">zinnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">O blaeuwe <rhyme label="a" type="m">scheen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Hoe sal dit <rhyme label="b" type="f">lukken</rhyme>?</l>
        <l id="s6:l3" met="1" type="-+">An <rhyme label="b" type="f" corresp="#s6:l2">stukken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soo raakt dit heele <rhyme label="a" type="m" corresp="#s6:l1">been</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Og, og, dan <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Ik, O gantsch <rhyme label="c" type="m" corresp="#s6:l5">bloed</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">In ’t laast nog springen op twee <rhyme label="b" type="f" corresp="#s6:l2">krukken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Ik sal dees <rhyme label="a" type="m">pijn</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Wel haast <rhyme label="b" type="f">verzagten</rhyme>.</l>
        <l id="s7:l3" met="1" type="-+">Weg <rhyme label="b" type="f" corresp="#s7:l2">klagten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Met een glas Rinse <rhyme label="a" type="m" corresp="#s7:l1">Wijn</rhyme>!</l>
        <l id="s7:l5" met="2" type="-+">Dan hoef ik <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Met groot <rhyme label="c" type="m" corresp="#s7:l5">verdriet</rhyme></l>
        <l id="s7:l7" met="4" type="-+">En ongeduld de meid te <rhyme label="b" type="f" corresp="#s7:l2">wagten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Het Wyntje <rhyme label="a" type="m">doet</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Ons vrolik <rhyme label="b" type="f">Wesen</rhyme>,</l>
        <l id="s8:l3" met="1" type="-+"><rhyme label="b" type="f" corresp="#s8:l2">Gepresen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En maakt rijk sonder <rhyme label="a" type="m" corresp="#s8:l1">goed</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">En ’t doed ons <rhyme label="c" type="m">deugd</rhyme></l>
        <l id="s8:l6" met="2" type="-+">En ’t maakt de <rhyme label="c" type="m" corresp="#s8:l5">vreugd</rhyme></l>
        <l id="s8:l7" met="4" type="-+">En ’t kan ons van ons smert <rhyme label="b" type="f" corresp="#s8:l2">genesen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Laet ons dan <rhyme label="a" type="m">saan</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Maer lustig <rhyme label="b" type="f">drinken</rhyme></l>
        <l id="s9:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s9:l2">klinken</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Het gaet er soo op <rhyme label="a" type="m" corresp="#s9:l1">aen</rhyme>!</l>
        <l id="s9:l5" met="2" type="-+">Uit Pot en <rhyme label="c" type="m">Kan</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Daer hou ik <rhyme label="c" type="m" corresp="#s9:l5">van</rhyme></l>
        <l id="s9:l7" met="4" type="-+">En van eens helder te <rhyme label="b" type="f" corresp="#s9:l2">rinkinken</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Verwagt de <rhyme label="a" type="m">straf</rhyme></l>
        <l id="s10:l2" met="2" type="-+">Voor uwe <rhyme label="b" type="f">daden</rhyme>!</l>
        <l id="s10:l3" met="1" type="-+">Komt <rhyme label="b" type="f" corresp="#s10:l2">quaden</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Vermorsselt haer tot <rhyme label="a" type="m" corresp="#s10:l1">kaf</rhyme>!</l>
        <l id="s10:l5" met="2" type="-+">Komt slim <rhyme label="c" type="m">gespuis</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Voert haer uit ’t <rhyme label="c" type="m" corresp="#s10:l5">huis</rhyme></l>
        <l id="s10:l7" met="4" type="-+">En toond in ’t minsten geen <rhyme label="b" type="f" corresp="#s10:l2">genaden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Geen Jongmans <rhyme label="a" type="m">zin</rhyme></l>
        <l id="s11:l2" met="2" type="-+">Moet men <rhyme label="b" type="f">verwekken</rhyme></l>
        <l id="s11:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s11:l2">gekken</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Daer na weer met zijn <rhyme label="a" type="m" corresp="#s11:l1">min</rhyme>!</l>
        <l id="s11:l5" met="2" type="-+">Gy Dogters, <rhyme label="c" type="m">zaan</rhyme></l>
        <l id="s11:l6" met="2" type="-+">Leerd hier dog <rhyme label="c" type="m" corresp="#s11:l5">aen</rhyme>:</l>
        <l id="s11:l7" met="4" type="-+">Laet u dit voor een leering <rhyme label="b" type="f" corresp="#s11:l2">strekken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
