  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Vrijers, wel, wat hebje <rhyme label="a" type="f">klugjes</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Met de meysjes in den <rhyme label="b" type="m">Haag</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Alle <rhyme label="b" type="m" corresp="#s1:l2">daag</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Want sy betoone met haer <rhyme label="a" type="f" corresp="#s1:l1">sugjes</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Dat sy zijn tot trouwen <rhyme label="b" type="m" corresp="#s1:l2">graag</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">De bloote Borsjes siet men <rhyme label="a" type="f">speelen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Onder ‘t dunne Floers   of <rhyme label="b" type="m">Cantyn</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Suyver <rhyme label="b" type="m" corresp="#s2:l2">fijn</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Dat is te seggen: komt myn <rhyme label="a" type="f" corresp="#s2:l1">streelen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Want ik soeck gestreelt te <rhyme label="b" type="m" corresp="#s2:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dese Haagze <rhyme label="a" type="f">Herderinne</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Hebbe met haar soete <rhyme label="b" type="m">reen</rhyme></l>
        <l id="s3:l3" met="2" type="+-">
          <rhyme label="b" type="m" corresp="#s3:l2">Aangebeen</rhyme>
        </l>
        <l id="s3:l4" met="4" type="+-">Venus, d’ Moeder van het <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Om haar jongste Zoontje <rhyme label="b" type="m" corresp="#s3:l2">kleen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Venus nam het <rhyme label="a" type="f">Cupidootje</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Liet hem uyt den Hemel <rhyme label="b" type="m">hoog</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Maar het <rhyme label="b" type="m" corresp="#s4:l2">vloog</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Naar dese rey, maar ‘t kleyne <rhyme label="a" type="f" corresp="#s4:l1">Gootje</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Kreeg sijn boel haast in het <rhyme label="b" type="m" corresp="#s4:l2">oog</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sy maakte   daar de vreugt met <rhyme label="a" type="f">dansse</rhyme></l>
        <l id="s5:l2" met="4" type="+-">En spronge lustig in d' <rhyme label="b" type="m">wind</rhyme></l>
        <l id="s5:l3" met="2" type="+-">Met het <rhyme label="b" type="m" corresp="#s5:l2">kint</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Sy kroonde hem met Minne <rhyme label="a" type="f" corresp="#s5:l1">kransse</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">‘t Boefje was daar seer <rhyme label="b" type="m" corresp="#s5:l2">bemint</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Men ging het op de schoot <rhyme label="a" type="f">doudynen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Met kussen, met een soet <rhyme label="b" type="m">gestreel</rhyme></l>
        <l id="s6:l3" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s6:l2">gequeel</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Men songer daar: ‘O Jonge Borsje   <rhyme label="a" type="f" corresp="#s6:l1">kleyne</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Heer, wat is uw vleesje <rhyme label="b" type="m" corresp="#s6:l2">eel</rhyme>.’</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Sy eerden hem gelijk een <rhyme label="a" type="f">Engeltje</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">En tegen   met hem op d' <rhyme label="b" type="m">Jagt</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Maar ‘t <rhyme label="b" type="m" corresp="#s7:l2">lagt</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Want dit verbruyd’   olicke <rhyme label="a" type="f" corresp="#s7:l1">bengeltje</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Toont haar sijn boefjes <rhyme label="b" type="m" corresp="#s7:l2">kragt</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Tien vergulde schoone <rhyme label="a" type="f">schigtjes</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Trok hy uyt sijn koker <rhyme label="b" type="m">root</rhyme></l>
        <l id="s8:l3" met="2" type="+-">En hy <rhyme label="b" type="m" corresp="#s8:l2">schoot</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Onder d’hoop van dese soete <rhyme label="a" type="f" corresp="#s8:l1">Nigjes</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Dat een ider laggend' <rhyme label="b" type="m" corresp="#s8:l2">vloot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">d’ Een die lonckte met sijn <rhyme label="a" type="f">minnetje</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Door der   liefde zoete <rhyme label="b" type="m">brant</rhyme></l>
        <l id="s9:l3" met="2" type="+-">Diese <rhyme label="b" type="m" corresp="#s9:l2">vant</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Pronkte haar op als een <rhyme label="a" type="f" corresp="#s9:l1">Godinnetje</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Geestig met een Haags <rhyme label="b" type="m" corresp="#s9:l2">verstant</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">d’ Ander ging haar ouders <rhyme label="a" type="f">vlieden</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Na d’ een of ander plaats <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s10:l3" met="2" type="+-">En seer <rhyme label="b" type="m" corresp="#s10:l2">bly</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Ging daar haar alderliefste Lief <rhyme label="a" type="f" corresp="#s10:l1">ontbieden</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Om d' soete <rhyme label="b" type="m" corresp="#s10:l2">snakery</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Een die doolde langes de <rhyme label="a" type="f">stranden</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Schreef daar met haer   eigen <rhyme label="b" type="m">bloet</rhyme></l>
        <l id="s11:l3" met="2" type="+-">- Ag, so <rhyme label="b" type="m" corresp="#s11:l2">soet</rhyme> -</l>
        <l id="s11:l4" met="4" type="+-">Dat haar de min het herte souw of <rhyme label="a" type="f" corresp="#s11:l1">branden</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Soo hy   niet haar lusjes <rhyme label="b" type="m" corresp="#s11:l2">boet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Veel die kreege schoone <rhyme label="a" type="f">bloosjes</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Op haar kaakjes, <rhyme label="b" type="m">net</rhyme></l>
        <l id="s12:l3" met="2" type="+-"><rhyme label="b" type="m" corresp="#s12:l2">Geblancket</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">Maar dese kleur als roode <rhyme label="a" type="f" corresp="#s12:l1">roosjes</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Vloogen haar wel haast op ‘t <rhyme label="b" type="m" corresp="#s12:l2">bed</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Dus soo komt dat meenig <rhyme label="a" type="f">meysjen</rhyme></l>
        <l id="s13:l2" met="4" type="+-">Door het speelen met het <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s13:l3" met="2" type="+-">Wert <rhyme label="b" type="m" corresp="#s13:l2">verblint</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Bieden haar blank en poesel <rhyme label="a" type="f" corresp="#s13:l1">vleysje</rhyme></l>
        <l id="s13:l5" met="4" type="+-">Aan die geen die haar niet <rhyme label="b" type="m" corresp="#s13:l2">mind</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Jongmans, wilje gaan uyt <rhyme label="a" type="f">vryen</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Houd uw uyt de kap van de <rhyme label="b" type="m">Sot</rhyme></l>
        <l id="s14:l3" met="2" type="+-">Van <rhyme label="b" type="m" corresp="#s14:l2">Marot</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Want gy en souter niet <rhyme label="a" type="f" corresp="#s14:l1">bedyen</rhyme></l>
        <l id="s14:l5" met="4" type="+-">En dan waarje iders <rhyme label="b" type="m" corresp="#s14:l2">spot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
