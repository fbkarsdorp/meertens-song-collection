  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ay myn soetste <rhyme label="a" type="f">Goddinne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Diane van myn <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Myn lust, wensch, siel en <rhyme label="a" type="f" corresp="#s1:l1">sinne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Beweecht u in myn <rhyme label="b" type="m" corresp="#s1:l2">smert</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Siet siet d’ellend’ waer mee myn droeve <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van u ghepynicht <rhyme label="b" type="m" corresp="#s1:l2">wert</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Die my wilt <rhyme label="c" type="f" corresp="#s1:l5">begheven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wild ghy mijn schoon <rhyme label="a" type="f">Heldinne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">My bannen wyt van <rhyme label="b" type="m">u</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Wat sult ghy dan doch <rhyme label="a" type="f" corresp="#s2:l1">winne</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ey! ey! dat seght my <rhyme label="b" type="m" corresp="#s2:l2">nu</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Niet anders dan dat ick ontydich <rhyme label="c" type="f">sterve</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En voor u wreetheyt <rhyme label="b" type="m" corresp="#s2:l2">schuw</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Moet als ballinck <rhyme label="c" type="f" corresp="#s2:l5">swerven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo myn staghe <rhyme label="a" type="f">ghebeden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">V doch beweghen <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Noch myn afghesloofde <rhyme label="a" type="f" corresp="#s3:l1">leden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dan op myn Minne <rhyme label="b" type="m" corresp="#s3:l2">siet</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Ick bid, ick smeeck, de oorsaeck van myn <rhyme label="c" type="f">plaghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En blyven int <rhyme label="b" type="m" corresp="#s3:l2">verdriet</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Om u te <rhyme label="c" type="f" corresp="#s3:l5">behaghen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
