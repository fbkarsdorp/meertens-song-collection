<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187411</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1790</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat is ons al vreugde gegeven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Lustig, Rustig, lieve <rhyme label="a" type="f">Vrinden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die zig hier ter disch <rhyme label="a" type="f" corresp="#s1:l1">bevinden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s1:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Kan de Zaan-vreugd uw <rhyme label="a" type="f">verheugen</rhyme>?</l>
        <l id="s2:l2" met="4" type="-+">Zoo als myn hervat uw <rhyme label="a" type="f" corresp="#s2:l1">teugen</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s2:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s2:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zy die 't Boeyertje <rhyme label="a" type="f">behooren</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Roepen!... Neigd na ons uw <rhyme label="a" type="f" corresp="#s3:l1">ooren</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s3:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s3:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vraag het Koopluy, <rhyme label="a" type="f">Makelaaren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Of mê aan Tavel niet moet <rhyme label="a" type="f" corresp="#s4:l1">paaren</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s4:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s4:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Yder mag 't zyn Vroutje <rhyme label="a" type="f">vraagen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">ô! Zy roepen met <rhyme label="a" type="f" corresp="#s5:l1">behaagen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s5:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s5:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vraag het aan het jonge <rhyme label="a" type="f">Volkje</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Vraag het zelfs den Drukker <rhyme label="a" type="f" corresp="#s6:l1">T..kje</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s6:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s6:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Heere voegje naast uw <rhyme label="a" type="f">Vrysters</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Drinkt en klinkt en zingt als <rhyme label="a" type="f" corresp="#s7:l1">lysters</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s7:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s7:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maar laat ons ook niet <rhyme label="a" type="f">vergeeten</rhyme>;</l>
        <l id="s8:l2" met="4" type="-+">Om de Lekkere Baars te <rhyme label="a" type="f" corresp="#s8:l1">eeten</rhyme>!</l>
        <l id="s8:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s8:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s8:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Men mag matig vroolyk <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Guile vreugde word <rhyme label="a" type="f" corresp="#s9:l1">gepreezen</rhyme>!</l>
        <l id="s9:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s9:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s9:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Weg; met Staats-of Kerk <rhyme label="a" type="f">geschillen</rhyme>!</l>
        <l id="s10:l2" met="4" type="-+">'k Wagt myn voor die vitten <rhyme label="a" type="f" corresp="#s10:l1">willen</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s10:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s10:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Nu eens uit de borst <rhyme label="a" type="f">gezongen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">'t Kurkje is van de vles <rhyme label="a" type="f" corresp="#s11:l1">gesprongen</rhyme>....!</l>
        <l id="s11:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s11:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s11:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Heer Tractant wy zyn uw <rhyme label="a" type="f">dankbaar</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">'t Visje fris, en 't Vlees van pas <rhyme label="a" type="f" corresp="#s12:l1">gaar</rhyme>;</l>
        <l id="s12:l3" met="4" type="-+">Waare Kinderen van <rhyme label="b" type="m">vermaak</rhyme>!</l>
        <l id="s12:l4" met="4" type="-+">Drink en Kus, dat is uw <rhyme label="b" type="m" corresp="#s12:l3">zaak</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Laaten we onze geest niet <rhyme label="a" type="f">kwellen</rhyme>!</l>
        <l id="s13:l2" met="4" type="-+">'t Wintje doet de zeyltjes <rhyme label="a" type="f" corresp="#s13:l1">zwellen</rhyme>;</l>
        <l id="s13:l3" met="4" type="-+">Weg met Vis, met Vlees, en <rhyme label="b" type="m">Ham</rhyme>!</l>
        <l id="s13:l4" met="4" type="-+">Ik verlang weêr na den <rhyme label="b" type="m" corresp="#s13:l3">Dam</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
