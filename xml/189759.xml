    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mijnheer Lalande</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ROsind myn <rhyme label="a" type="f">Engel</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zoo g' u Minnaars leven <rhyme label="b" type="m">lieft</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Staak uw lonkjes, daar de <rhyme label="a" type="f" corresp="#s1:l1">Bengel</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Van de min my 't hart door <rhyme label="b" type="m" corresp="#s1:l2">grieft</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Toen ik u verkooren <rhyme label="c" type="m">maagt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Laast myn smarte had <rhyme label="c" type="m" corresp="#s1:l5">geklaagt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Wert uw hertje, door myn <rhyme label="d" type="f">schryen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Tot myn wedermin <rhyme label="c" type="m" corresp="#s1:l5">belaagt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Gy liet myn <rhyme label="a" type="f">bressen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Schoone aan u neckter <rhyme label="b" type="m">mond</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Toen door duizent kusjes <rhyme label="a" type="f" corresp="#s2:l1">lessen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maar in plaas dat gy myn <rhyme label="b" type="m" corresp="#s2:l2">wondt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Hier door heel genezen <rhyme label="c" type="m">zou</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Wert ik van uw <rhyme label="c" type="m" corresp="#s2:l5">hooningdou</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Iu een feller vlam <rhyme label="d" type="f">bezingelt</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Ã´ Myn gadeloze <rhyme label="c" type="m" corresp="#s2:l5">vrouw</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dus heb ik 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Met een onverbreekbre <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En hondert tederheen <rhyme label="a" type="f" corresp="#s3:l1">gegeven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Aan Rosinda myn <rhyme label="b" type="m" corresp="#s3:l2">vriendin</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Hier op zy my 'er lipjes <rhyme label="c" type="m">boot</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ik storf met haar de minne <rhyme label="c" type="m" corresp="#s3:l5">doodt</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Zeg bevalge <rhyme label="d" type="f">Dwinglandye</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Wat voor vreucht gy my niet <rhyme label="c" type="m" corresp="#s3:l5">boodt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
