  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VRou Venus heel <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Die lag by <rhyme label="a" type="m" corresp="#s1:l1">Adoôn</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Vol grillen en <rhyme label="b" type="f">lusten</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Wanneer hy haar <rhyme label="b" type="f" corresp="#s1:l3">kusten</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Daar veel <rhyme label="a" type="m" corresp="#s1:l1">Minne-Goôn</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Dees twee <rhyme label="c" type="f">bevonde</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Zy spronge in 't <rhyme label="c" type="f" corresp="#s1:l6">ronde</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Op een blyde <rhyme label="a" type="m" corresp="#s1:l1">toon</rhyme>:</l>
        <l id="s1:l9" met="2" type="-+">En strooide met <rhyme label="d" type="m">vreugd</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Onder het zoet <rhyme label="e" type="f">galmen</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Met bloemen en <rhyme label="e" type="f" corresp="#s1:l10">palmen</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Ieder waar <rhyme label="d" type="m" corresp="#s1:l9">verheugd</rhyme>:</l>
        <l id="s1:l13" met="2" type="-+">Tot dat de <rhyme label="f" type="m">Min</rhyme>,</l>
        <l id="s1:l14" met="2" type="-+">De liefde liet <rhyme label="f" type="m" corresp="#s1:l13">in</rhyme>,</l>
        <l id="s1:l15" met="2" type="-+">Met streelen en <rhyme label="g" type="f">streng'len</rhyme>,</l>
        <l id="s1:l16" met="2" type="-+">Zig t'zaam te <rhyme label="g" type="f" corresp="#s1:l15">vermeng'len</rhyme>,</l>
        <l id="s1:l17" met="2" type="-+">Heel luchtig van <rhyme label="f" type="m" corresp="#s1:l13">zin</rhyme>,</l>
        <l id="s1:l18" met="2" type="-+">Dus heeft dan dien <rhyme label="h" type="m">bloet</rhyme>,</l>
        <l id="s1:l19" met="2" type="-+">Adonis met <rhyme label="i" type="f">Venus</rhyme></l>
        <l id="s1:l20" met="2" type="-+">Zyne lust <rhyme label="h" type="m" corresp="#s1:l18">geboet</rhyme>.</l>
      </lg>
    </lg>
  </text>
