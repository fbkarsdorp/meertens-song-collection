<song>
  <meta>
    <id>179948</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Das Lieben ist mein Leben</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom, laat ons vrolyk <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m">dera</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Nu gy myn <rhyme label="a" type="f" corresp="#s1:l1">uitgelezen</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m" corresp="#s1:l2">dera</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Heden heuchlyk jarig <rhyme label="c" type="m">zyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Hu, hu, <rhyme label="d" type="m">hu</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Jarig <rhyme label="c" type="m" corresp="#s1:l5">zyt</rhyme>;</l>
        <l id="s1:l8" met="4" type="-+">'k Ben om dat geluk <rhyme label="c" type="m" corresp="#s1:l5">verblyd</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s1:l6">hu</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Faldera, fal lal derie <rhyme label="b" type="m" corresp="#s1:l2">dera</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">'k Ben om dat geluk <rhyme label="c" type="m" corresp="#s1:l5">verblyd</rhyme>,</l>
        <l id="s1:l12" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s1:l6">hu</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat pretjes en wat <rhyme label="a" type="f">laghjes</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m">dera</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wat rampen en wat <rhyme label="a" type="f" corresp="#s2:l1">klaghjes</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m" corresp="#s2:l2">dera</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Had gy al, myn <rhyme label="c" type="m">Engelin</rhyme>?</l>
        <l id="s2:l6" met="3" type="+-">Hu, hu, <rhyme label="d" type="m">hu</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-"><rhyme label="c" type="m" corresp="#s2:l5">Engelin</rhyme>?</l>
        <l id="s2:l8" met="4" type="-+">Om den kleinen God der <rhyme label="c" type="m" corresp="#s2:l5">min</rhyme>,</l>
        <l id="s2:l9" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s2:l6">hu</rhyme>?</l>
        <l id="s2:l10" met="4" type="-+">Faldera, fal lal derie <rhyme label="b" type="m" corresp="#s2:l2">dera</rhyme>,</l>
        <l id="s2:l11" met="4" type="-+">Om den kleinen God der <rhyme label="c" type="m" corresp="#s2:l5">min</rhyme>,</l>
        <l id="s2:l12" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s2:l6">hu</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gy wenschten <rhyme label="a" type="f">menigwerven</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m">dera</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Te leven en te <rhyme label="a" type="f" corresp="#s3:l1">sterven</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m" corresp="#s3:l2">dera</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Maar de dood heeft u <rhyme label="c" type="m">gespaard</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Hu, hu, <rhyme label="d" type="m">hu</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">U <rhyme label="c" type="m" corresp="#s3:l5">gespaard</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Om van my te zyn <rhyme label="c" type="m" corresp="#s3:l5">verjaard</rhyme>,</l>
        <l id="s3:l9" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s3:l6">hu</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Faldera, fal lal derie <rhyme label="b" type="m" corresp="#s3:l2">dera</rhyme>,</l>
        <l id="s3:l11" met="4" type="-+">Om van my te zyn <rhyme label="c" type="m" corresp="#s3:l5">verjaard</rhyme>,</l>
        <l id="s3:l12" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s3:l6">hu</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Leef lang, bevryd van <rhyme label="a" type="f">smarte</rhyme>;</l>
        <l id="s4:l2" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m">dera</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Leef met een vrolyk <rhyme label="a" type="f" corresp="#s4:l1">harte</rhyme>;</l>
        <l id="s4:l4" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m" corresp="#s4:l2">dera</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Vlie de onnutte zorg <rhyme label="c" type="m">altyd</rhyme>:</l>
        <l id="s4:l6" met="3" type="+-">Hu, hu, <rhyme label="d" type="m">hu</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Zorg <rhyme label="c" type="m" corresp="#s4:l5">altyd</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Stoor u aan geen haat noch <rhyme label="c" type="m" corresp="#s4:l5">nyd</rhyme>,</l>
        <l id="s4:l9" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s4:l6">hu</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Faldera, fal lal derie <rhyme label="b" type="m" corresp="#s4:l2">dera</rhyme>,</l>
        <l id="s4:l11" met="4" type="-+">Stoor u aan geen haat noch <rhyme label="c" type="m" corresp="#s4:l5">nyd</rhyme>,</l>
        <l id="s4:l12" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s4:l6">hu</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Kom, daar eens op <rhyme label="a" type="f">geklonken</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m">dera</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En 't glas schoon <rhyme label="a" type="f" corresp="#s5:l1">uitgedronken</rhyme>;</l>
        <l id="s5:l4" met="5" type="-+">Falderie, faldera, fal lal derie <rhyme label="b" type="m" corresp="#s5:l2">dera</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Nu myn wenschen, lieve <rhyme label="c" type="m">Zus</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Hu, hu, <rhyme label="d" type="m">hu</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Lieve <rhyme label="c" type="m" corresp="#s5:l5">Zus</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Eens bevestigd met een <rhyme label="c" type="m" corresp="#s5:l5">kus</rhyme>,</l>
        <l id="s5:l9" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s5:l6">hu</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Faldera, fal lal derie <rhyme label="b" type="m" corresp="#s5:l2">dera</rhyme>,</l>
        <l id="s5:l11" met="4" type="-+">Eens bevestigd met een <rhyme label="c" type="m" corresp="#s5:l5">kus</rhyme>,</l>
        <l id="s5:l12" met="5" type="+-">Hu, hu, hu, hu, <rhyme label="d" type="m" corresp="#s5:l6">hu</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
