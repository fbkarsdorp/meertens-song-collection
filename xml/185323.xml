<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185323</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik zal u eeuwig minnen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Waar blyft gy, myn <rhyme label="a" type="f">beminden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik ben hier in <rhyme label="b" type="m">onrust</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik dagt u hier te <rhyme label="a" type="f" corresp="#s1:l1">vinden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Te smaaken <rhyme label="b" type="m" corresp="#s1:l2">minne-lust</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Ag bloem, vol <rhyme label="c" type="m">waard</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Myn schat op <rhyme label="c" type="m" corresp="#s1:l5">aard</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wilt u tot myn <rhyme label="d" type="f">begeeven</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Want ziet ik steek vol <rhyme label="e" type="m">minnepyn</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Waar zyt gy, ag, waar mag gy <rhyme label="e" type="m" corresp="#s1:l8">zyn</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Ziet myn jong <rhyme label="f" type="m">hert</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Wagt u met <rhyme label="f" type="m" corresp="#s1:l10">smert</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Waar blyft gy myn <rhyme label="a" type="f" corresp="#s1:l1">beminden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wie doet u schoonheid <rhyme label="a" type="f">smaaken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat ik u hier niet <rhyme label="b" type="m">vind</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wie mag u lippies <rhyme label="a" type="f" corresp="#s2:l1">raaken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wat minnaar gy <rhyme label="b" type="m" corresp="#s2:l2">bemint</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Zyt gy <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Al met die <rhyme label="c" type="m" corresp="#s2:l5">geen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Laat u in stil <rhyme label="d" type="f">genaaken</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">In u vertrek, daar gy nu <rhyme label="e" type="m">blyst</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Daar gy de venus-lust <rhyme label="e" type="m" corresp="#s2:l8">bedryft</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Ag, denkt gy <rhyme label="f" type="m">niet</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Op myn <rhyme label="f" type="m" corresp="#s2:l10">verdriet</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Maar wel op u <rhyme label="a" type="f" corresp="#s2:l1">vermaaken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laat u geluid nu <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Verheugd my, waarde <rhyme label="b" type="m">pand</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Laat ik u lust dog <rhyme label="a" type="f" corresp="#s3:l1">stooren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Bint u in geen <rhyme label="b" type="m" corresp="#s3:l2">verband</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Opent, met <rhyme label="c" type="m">spoet</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Myn lieffie <rhyme label="c" type="m" corresp="#s3:l5">zoet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Myn lief, myn <rhyme label="d" type="f">uytverkooren</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Laat my niet langer in deez' <rhyme label="e" type="m">pyn</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Laat ik alleen u minnaar <rhyme label="e" type="m" corresp="#s3:l8">zyn</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Ik bemin u <rhyme label="f" type="m">zeer</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Bemint my <rhyme label="f" type="m" corresp="#s3:l10">weer</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Wilt my trouw liefde <rhyme label="a" type="f" corresp="#s3:l1">toonen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
