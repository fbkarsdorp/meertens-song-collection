    <melody>Psalm 017 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O GODT gheen God heb' ick dan <rhyme label="a" type="m">v</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Van s'morgens aen bidd' ick v <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Myn siel verlanght nae v gaer <rhyme label="b" type="f" corresp="#s1:l2">seere</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die gantsch in my versmeltet <rhyme label="a" type="m" corresp="#s1:l1">nu</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Gheheel verdrooght is myn <rhyme label="c" type="f">lichaeme</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Myn krachten vergaen al <rhyme label="d" type="m">ghelyck</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">My dorstet als een dorr' <rhyme label="d" type="m" corresp="#s1:l6">ęerdryck</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Nae v in dees plaets' <rhyme label="c" type="f" corresp="#s1:l5">onbequaeme</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Op dat ick noch eens aensien <rhyme label="a" type="m">magh</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">V' hęerlickheit nae dit <rhyme label="b" type="f">benauwen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">So ick die lieflick te <rhyme label="b" type="f" corresp="#s2:l2">aenschauwen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">In uwen schoonen tempel <rhyme label="a" type="m" corresp="#s2:l1">plagh</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Want veel beter is v' <rhyme label="c" type="f">ghenaede</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dan s'menschen leuen self hier <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Daer sal oock mynen mondt <rhyme label="d" type="m" corresp="#s2:l6">gewis</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Vwen lof spreken vroegh end <rhyme label="c" type="f" corresp="#s2:l5">spaede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daer sal ick singhen v' eer' <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">So langh' als ick werd' in dit <rhyme label="b" type="f">leuen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">V met handen hoogh' op <rhyme label="b" type="f" corresp="#s3:l2">gheheuen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Sal ick O Godt aenroepen <rhyme label="a" type="m" corresp="#s3:l1">daer</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Dit souden myns herten vreughd <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">End' oock al myn ghenoecht' <rhyme label="d" type="m">alleyn</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Mocht' ick met hert' end monde <rhyme label="d" type="m" corresp="#s3:l6">reyn</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">V altyt louen Heer <rhyme label="c" type="f" corresp="#s3:l5">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Als ick rust' op dat bedde <rhyme label="a" type="m">myn</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">End ouerleggh' al uwe <rhyme label="b" type="f">krachten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">So moeten dan al myn <rhyme label="b" type="f" corresp="#s4:l2">ghedachten</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Des nachts met v onledigh <rhyme label="a" type="m" corresp="#s4:l1">sijn</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Want in myn verdriet end myn <rhyme label="c" type="f">sorghen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Hebt ghy my geholpen <rhyme label="d" type="m">eenpaer</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Dies prys' ick v, ghy hebt <rhyme label="d" type="m" corresp="#s4:l6">voorwaer</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Met v' vleughelen my <rhyme label="c" type="f" corresp="#s4:l5">verborghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Myn siel' hanght v so vastlick <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dat se van v gheensins kan <rhyme label="b" type="f">wycken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">V' handt bewaert my <rhyme label="b" type="f" corresp="#s5:l2">desghelycken</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Voor allen die my teghen <rhyme label="a" type="m" corresp="#s5:l1">staen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Maer sy die myn siel' met <rhyme label="c" type="f">onwęerde</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Ouervallen willen met <rhyme label="d" type="m">leedt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Sullen in den afgront seer <rhyme label="d" type="m" corresp="#s5:l6">wreedt</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Verstooten werden onder d' <rhyme label="c" type="f" corresp="#s5:l5">ęerde</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Versneden werdt tot stucken <rhyme label="a" type="m">kleyn</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Met den swęerd' end tot roof <rhyme label="b" type="f">ghegeuen</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Den vossen end dieren <rhyme label="b" type="f" corresp="#s6:l2">daerneuen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">T'goedt myner vianden <rhyme label="a" type="m" corresp="#s6:l1">ghemeyn</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Dan sal de Koningh hem <rhyme label="c" type="f">verblyden</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">In uw ouerwinninghe <rhyme label="d" type="m">Heer'</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Wie v kent sal v' lof end <rhyme label="d" type="m" corresp="#s6:l6">eer'</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Wtbreyden klaer aen allen <rhyme label="c" type="f" corresp="#s6:l5">zyden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
