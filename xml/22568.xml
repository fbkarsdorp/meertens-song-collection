    <melody>Vrouw Venus gij zijt zo schoon wijf</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'Her Danel, ghy sijt soe schoenen <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghi staet in minen <rhyme label="b" type="f">sinnen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Nu, laet ons in die camer <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">En wesen daer met vroechden <rhyme label="b" type="f" corresp="#s1:l2">inee</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">E ende E, met vroechden <rhyme label="b" type="f" corresp="#s1:l2">inee</rhyme>!</l>
        <l id="s1:l6" met="4" type="-+">Nu, laet ons in die camer <rhyme label="c" type="m" corresp="#s1:l3">gaen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">En wesen daer met vroechden <rhyme label="b" type="f" corresp="#s1:l2">inee</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">E ende E, met vroechden <rhyme label="b" type="f" corresp="#s1:l2">inee</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'Dat en doe ic niet, dat laet ic <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Vrou Venis', seit hy, <rhyme label="b" type="f">'vrouwee</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ic heb Maria, die Moeder <rhyme label="c" type="m">Gods</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ghelovet, liefgee, mijnre <rhyme label="b" type="f" corresp="#s2:l2">trouwe</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">E ende E, mijnre <rhyme label="b" type="f" corresp="#s2:l2">trouwe</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">Ic heb Maria, die Moeder <rhyme label="c" type="m" corresp="#s2:l3">Gods</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Ghelovet, liefgee, mijnre <rhyme label="b" type="f" corresp="#s2:l2">trouwe</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">E ende E, mijnre <rhyme label="b" type="f" corresp="#s2:l2">trouwe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'Heb ghy Maria, die Moeder <rhyme label="a" type="m">Gods</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ghelovet juer <rhyme label="b" type="f">trouwee</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Soe gaetes Haer of, ende coemtes my <rhyme label="c" type="m">an</rhyme>!</l>
        <l id="s3:l4" met="4" type="-+">Ten sel ju, liefgee, niet <rhyme label="b" type="f" corresp="#s3:l2">berouwen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">E ende E, niet <rhyme label="b" type="f" corresp="#s3:l2">berouwee</rhyme>!</l>
        <l id="s3:l6" met="4" type="-+">Soe gaetes Haer of, ende coemtes my <rhyme label="c" type="m" corresp="#s3:l3">an</rhyme>!</l>
        <l id="s3:l7" met="4" type="-+">Ten sel ju, liefgee, niet <rhyme label="b" type="f" corresp="#s3:l2">berouwen</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">E ende E, niet <rhyme label="b" type="f" corresp="#s3:l2">berouwee</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'Ghin icx Haer of en quam ic es u <rhyme label="a" type="m">an</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ic vuer uut mijne <rhyme label="b" type="f">sinnen</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Als ic u driewerf nuchtere <rhyme label="c" type="m">aensie</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">My dunct, ghi sijt die <rhyme label="b" type="f" corresp="#s4:l2">duvelinnee</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">E ende E, die <rhyme label="b" type="f" corresp="#s4:l2">duvelinne</rhyme>!</l>
        <l id="s4:l6" met="4" type="-+">Als ic u driewerf nuchtere <rhyme label="c" type="m" corresp="#s4:l3">aensie</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">My dunct, ghi sijt die <rhyme label="b" type="f" corresp="#s4:l2">duvelinnee</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">E ende E, die <rhyme label="b" type="f" corresp="#s4:l2">duvelinne</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Heer Danel strec te Romen <rhyme label="a" type="m">waert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Al in die guede <rhyme label="b" type="f">steden</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Daer ghinc hy voer die paeus <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Met also groter <rhyme label="b" type="f" corresp="#s5:l2">oetmoedicheden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">E ende E, met groter <rhyme label="b" type="f" corresp="#s5:l2">oetmoedichede</rhyme>.</l>
        <l id="s5:l6" met="4" type="-+">Daer ghinc hy voer die paeus <rhyme label="c" type="m" corresp="#s5:l3">staen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Met also groter <rhyme label="b" type="f" corresp="#s5:l2">oetmoedicheden</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">E ende E, met groter <rhyme label="b" type="f" corresp="#s5:l2">oetmoedichede</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hi sprac met groter <rhyme label="a" type="m">vuergicheit</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En met also grooter <rhyme label="b" type="f">berouwee</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">'Ic heb ghesondicht seven <rhyme label="c" type="m">jaer</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Al met vrou Venis, die valsche <rhyme label="b" type="f" corresp="#s6:l2">vrouwee</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">E ende E, die valsche <rhyme label="b" type="f" corresp="#s6:l2">vrouwe</rhyme>!</l>
        <l id="s6:l6" met="4" type="-+">'Ic heb ghesondicht seven <rhyme label="c" type="m" corresp="#s6:l3">jaer</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Al met vrou Venis, die valsche <rhyme label="b" type="f" corresp="#s6:l2">vrouwee</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">E ende E, die valsche <rhyme label="b" type="f" corresp="#s6:l2">vrouwe</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">'Heb gy ghesondicht seven <rhyme label="a" type="m">jaer</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Al met vrou Venis, die <rhyme label="b" type="f">vrouwee</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Soe en moechdi nu noch <rhyme label="c" type="m">nummermeer</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Dat aenschin Gods <rhyme label="b" type="f" corresp="#s7:l2">aenschouwee</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">E ende E, dat aenschin Gods <rhyme label="b" type="f" corresp="#s7:l2">aenschouwen</rhyme>!</l>
        <l id="s7:l6" met="4" type="-+">Soe en moechdi nu noch <rhyme label="c" type="m" corresp="#s7:l3">nummermeer</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Dat aenschin Gods <rhyme label="b" type="f" corresp="#s7:l2">aenschouwee</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">E ende E, dat aenschin Gods <rhyme label="b" type="f" corresp="#s7:l2">aenschouwen</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">'Och, mach ic nu noch <rhyme label="a" type="m">nummermeer</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat aenschin Gods <rhyme label="b" type="f">aenschouwen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Soe wil ic weer in den berch <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Al by vrou Venis, die valsche <rhyme label="b" type="f" corresp="#s8:l2">vrouwee</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">E ende E, die valsche <rhyme label="b" type="f" corresp="#s8:l2">vrouwe</rhyme>!</l>
        <l id="s8:l6" met="4" type="-+">Soe wil ic weer in den berch <rhyme label="c" type="m" corresp="#s8:l3">gaen</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Al by vrou Venis, die valsche <rhyme label="b" type="f" corresp="#s8:l2">vrouwee</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">E ende E, die valsche <rhyme label="b" type="f" corresp="#s8:l2">vrouwe</rhyme>!'</l>
      </lg>
    </lg>
  </text>
</song>
