    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Ingrate je ne puis</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Mijn sinnen alle vijf sijn schier <rhyme label="a" type="f">besweken</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">By u <rhyme label="b" type="m">Goddin</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Wins soetheyt als een <rhyme label="c" type="m">bie</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Stort honich <rhyme label="b" type="m" corresp="#s1:l2">in</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Den mondt, door't liefflijck <rhyme label="a" type="f" corresp="#s1:l1">spreken</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">O! Muse-stem ach ooghen <rhyme label="c" type="m" corresp="#s1:l3">die</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Doorstralen <rhyme label="d" type="m">'thooft</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Van die <rhyme label="d" type="m" corresp="#s1:l7">ghelooft</rhyme>,</l>
        <l id="s1:l9" met="1" type="-+">Te <rhyme label="e" type="f">winnen</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Inden strijdt, laes! wordt <rhyme label="d" type="m" corresp="#s1:l7">ontrooft</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Van kracht en <rhyme label="e" type="f" corresp="#s1:l9">sinnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Onwinnelijcke maeght ô! ziel mijns <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Hoe naerder <rhyme label="b" type="m">'tlicht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van u schijnt tot <rhyme label="c" type="m">gheluck</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Hoe swaerder,, <rhyme label="b" type="m" corresp="#s2:l2">schicht</rhyme></l>
        <l id="s2:l5" met="3" type="-+">My wordt in't hert <rhyme label="a" type="f" corresp="#s2:l1">ghedreven</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Dies voel te samen vreucht en <rhyme label="c" type="m" corresp="#s2:l3">druck</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Hoe lang sal <rhyme label="d" type="m">ick</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Den echten <rhyme label="d" type="m" corresp="#s2:l7">strick</rhyme></l>
        <l id="s2:l9" met="1" type="-+"><rhyme label="e" type="f">Betrachten</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Daer ick schier door hitte <rhyme label="d" type="m" corresp="#s2:l7">stick</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">In mijn <rhyme label="e" type="f" corresp="#s2:l9">ghedachten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Soo u medooghentheydt der minne <rhyme label="a" type="f">treken</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Is <rhyme label="b" type="m">onghewis</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Soo sal ick in't <rhyme label="c" type="m">ghetraen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ghelijck een <rhyme label="b" type="m" corresp="#s3:l2">smis</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Door't water meer <rhyme label="a" type="f" corresp="#s3:l1">onsteken</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En altijdt meer in branden <rhyme label="c" type="m" corresp="#s3:l3">gaen</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">De tongh die <rhyme label="d" type="m">klaeght</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Het hert dat <rhyme label="d" type="m" corresp="#s3:l7">jaeght</rhyme>.</l>
        <l id="s3:l9" met="1" type="-+">
          <rhyme label="e" type="f">'Tverlanghen</rhyme>
        </l>
        <l id="s3:l10" met="3" type="-+">Naer u gonst ô! weerde <rhyme label="d" type="m" corresp="#s3:l7">maeght</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Houdt my <rhyme label="e" type="f" corresp="#s3:l9">ghevanghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
