<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191703</id>
    <date>
      <year-start>1653</year-start>
      <year-end>1653</year-end>
    </date>
    <location>
      <orginal-name>burghoorn</orginal-name>
      <standard-name>Burghoorn</standard-name>
    </location>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GEEF my een kus, neyg my u <rhyme label="a" type="m">mond</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gy die my aen u ziel <rhyme label="a" type="m" corresp="#s1:l1">verbond</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">U Min is beter als de <rhyme label="b" type="m">wijn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Haer die krank van u liefde <rhyme label="b" type="m" corresp="#s1:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Maegdekens van ons <rhyme label="a" type="m">gewest</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Hebben haer Min op u <rhyme label="a" type="m" corresp="#s2:l1">gevest</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Om dat u Naem vol kost lijk <rhyme label="b" type="m">goed</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van Oly en van Balssem <rhyme label="b" type="m" corresp="#s2:l3">vloed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Reyk my de hand, ik loop u <rhyme label="a" type="m">na</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ik speur u stap, ik volg u <rhyme label="a" type="m" corresp="#s3:l1">scha'e</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Breng my, mijn hert, in u <rhyme label="b" type="m">Palleys</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Daer ik van u mijn lusten <rhyme label="b" type="m" corresp="#s3:l3">eysch</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Breng my in uwe <rhyme label="a" type="m">binnen-sael</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Daer ik u kuys gevry <rhyme label="a" type="m" corresp="#s4:l1">verhael</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Daer ik u teere Min, mijn <rhyme label="b" type="m">Lief</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ver boven allen wijn <rhyme label="b" type="m" corresp="#s4:l3">verhief</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'kBen schoon, al ben ik bruyn van <rhyme label="a" type="m">vel</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Jerusalem, besiet my <rhyme label="a" type="m" corresp="#s5:l1">wel</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Kedars, en Sal'mons Tent <rhyme label="b" type="m">gelijk</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Van buyten slecht, van binnen <rhyme label="b" type="m" corresp="#s5:l3">rijk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Mijn Broeders sonden my <rhyme label="a" type="m">alleen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">In sonne-schijn, ten wijngaert <rhyme label="a" type="m" corresp="#s6:l1">heen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Daer wierd ik swart en <rhyme label="b" type="m">onbedacht</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Nam op mijn eygen druyf geen <rhyme label="b" type="m" corresp="#s6:l3">acht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Mijn Lief, mijn Ziel, waer is de <rhyme label="a" type="m">Straet</rhyme>.</l>
        <l id="s7:l2" met="4" type="-+">Daer uwe kudd' ter weyde <rhyme label="a" type="m" corresp="#s7:l1">gaet</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Wijst my waer gy u tenten <rhyme label="b" type="m">spreyd</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En 'smiddags uwe Schaepkens <rhyme label="b" type="m" corresp="#s7:l3">leyd</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Waerom soud gy verborgen <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">En ik in stage sorgen <rhyme label="a" type="m" corresp="#s8:l1">zijn</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">By and're Herders mijn <rhyme label="b" type="m">verdriet</rhyme>?</l>
        <l id="s8:l4" met="4" type="-+">Neen, neen, die wey en dient my <rhyme label="b" type="m" corresp="#s8:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Weet gy't niet, schoonste van het <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Waer dat gy mijne Weyde <rhyme label="a" type="m" corresp="#s9:l1">vand</rhyme>?</l>
        <l id="s9:l3" met="4" type="-+">Gaet uyt, siet na der Schaepjes <rhyme label="b" type="m">tre'en</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En weydet daer u Geytjes <rhyme label="b" type="m" corresp="#s9:l3">heen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">'K gelijk haer, die mijn ziel <rhyme label="a" type="m">gewan</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">By Pharoos deftig <rhyme label="a" type="m" corresp="#s10:l1">koets-gespan</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Haer doppen en haer <rhyme label="b" type="m">hals-cieraet</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Stelt mijne liefde buyten <rhyme label="b" type="m" corresp="#s10:l3">maet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wy sullen, liefste <rhyme label="a" type="m">Vrouwelijn</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Staeg aen u spangen besig <rhyme label="a" type="m" corresp="#s11:l1">zijn</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Aen uwe ringen, aen het <rhyme label="b" type="m">goud</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Dat gy van mijnder minne <rhyme label="b" type="m" corresp="#s11:l3">houd</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Terwijl den Konink sit ten <rhyme label="a" type="m">disch</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">En ‘trond’ geselschap vrolijk <rhyme label="a" type="m" corresp="#s12:l1">is</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Sal met den Nardus van mijn <rhyme label="b" type="m">deugt</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Sijn heele Taeffel zijn <rhyme label="b" type="m" corresp="#s12:l3">verheugt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ik ben mijns Liefs, en hy is <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Gelijk een <rhyme label="a" type="m" corresp="#s13:l1">Myrrhe-bundelkijn</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Dat menigmael gedrukt, <rhyme label="b" type="m">gekust</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">'sNachts tusschen mijne borsjes <rhyme label="b" type="m" corresp="#s13:l3">rust</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Waer by gelijk ik noch miin <rhyme label="a" type="m">Lief</rhyme>?</l>
        <l id="s14:l2" met="4" type="-+">Die miine min ten hemel <rhyme label="a" type="m" corresp="#s14:l1">hief</rhyme>;</l>
        <l id="s14:l3" met="4" type="-+">By Cyper-druyf, die men in't <rhyme label="b" type="m">Land</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Van Engaddi de beste <rhyme label="b" type="m" corresp="#s14:l3">vant</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">U oogskens, schoonste <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Die ick in't gansch geweste <rhyme label="a" type="m" corresp="#s15:l1">vin</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Zijn even in <rhyme label="b" type="m">onnooselheyd</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Als't alder-soetste duyf ken <rhyme label="b" type="m" corresp="#s15:l3">heyt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Kom in mijn huys van <rhyme label="a" type="m">Ceder-hout</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Van Libanon siju kruyn <rhyme label="a" type="m" corresp="#s16:l1">geboud</rhyme>.</l>
        <l id="s16:l3" met="4" type="-+">Kom, schoonen Herder, maek my <rhyme label="b" type="m">bly</rhyme></l>
        <l id="s16:l4" met="4" type="-+">In mijn <rhyme label="b" type="m" corresp="#s16:l3">Cypressen-galery</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Ah! schoonen Herder, spoedu <rhyme label="a" type="m">wat</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">En let eens op dit groene <rhyme label="a" type="m" corresp="#s17:l1">blad</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Dat mijn gardijn, die door u <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">En 'tbruylofs-bed geladen <rhyme label="b" type="m" corresp="#s17:l3">heeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
