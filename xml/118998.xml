    <tags>
      <tag standard-name="drinklied" translation="drinking song">drinklied</tag>
      <tag standard-name="studentenlied" translation="student song">studentenlied</tag>
    </tags>
    <melody>Pots honderdduizend slapperment</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">RUrsus ad <rhyme label="a" type="f">diversorium</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik heb al weêr an <rhyme label="b" type="m">dorst</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Et cibus quasi <rhyme label="a" type="f" corresp="#s1:l1">corium</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Bezwaarlyk voor myn <rhyme label="b" type="m" corresp="#s1:l2">borst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vamur jam ut <rhyme label="a" type="f">Domini</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Op dat men onzen <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Met vreugd, Amici <rhyme label="a" type="f" corresp="#s2:l1">optimi</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In Kan en Glas <rhyme label="b" type="m" corresp="#s2:l2">verslyd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Jam vitra <rhyme label="a" type="f">proponemus</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zo laat ons vrolyk <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Et Bachus <rhyme label="a" type="f" corresp="#s3:l1">honoremus</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De vinder van de <rhyme label="b" type="m" corresp="#s3:l2">Wyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Post capiamus <rhyme label="a" type="f">fistulam</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gevult met goê <rhyme label="b" type="m">Tabak</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Je plenam habes <rhyme label="a" type="f" corresp="#s4:l1">festistulam</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Zo haald hem uit uw <rhyme label="b" type="m" corresp="#s4:l2">zak</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Et plena &amp; <rhyme label="a" type="f">letitia</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Tal lal, drie tal lal <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hic absis <rhyme label="a" type="f" corresp="#s5:l1">avaritia</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tal lal, drie tal lal <rhyme label="b" type="m" corresp="#s5:l2">la</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hec bella &amp; <rhyme label="a" type="f">certatio</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Daar niet met al aan <rhyme label="b" type="m">schort</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Honesta certe <rhyme label="a" type="f" corresp="#s6:l1">eratio</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">'t Gaat als een Varken <rhyme label="b" type="m" corresp="#s6:l2">knort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sunt Studiosorum <rhyme label="a" type="f">mores</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Men zuipt, men raast, men <rhyme label="b" type="m">klinkt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Acquirunt hic <rhyme label="a" type="f" corresp="#s7:l1">honores</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die 't meesten zwelgt en <rhyme label="b" type="m" corresp="#s7:l2">drinkt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
