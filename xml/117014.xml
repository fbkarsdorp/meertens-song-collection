      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Vriendelijke <rhyme label="a" type="m">Godt</rhyme>!</l>
        <l id="s1:l2" met="3" type="+-">Hoe zal ik u <rhyme label="b" type="f">loven</rhyme>?</l>
        <l id="s1:l3" met="3" type="+-">Allerhoogste <rhyme label="a" type="m" corresp="#s1:l1">Lot</rhyme>!</l>
        <l id="s1:l4" met="3" type="+-">Verheft mijne <rhyme label="c" type="m">ziel</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">(Die licht neder <rhyme label="c" type="m" corresp="#s1:l4">viel</rhyme>,)</l>
        <l id="s1:l6" met="3" type="+-">Van de Aerd’ na <rhyme label="b" type="f" corresp="#s1:l2">boven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Waar mijn ooge <rhyme label="a" type="m">keert</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Boven of na <rhyme label="b" type="f">ond’ren</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Steeds wortze <rhyme label="a" type="m" corresp="#s2:l1">geleert</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Van uwe <rhyme label="c" type="m">goetheyt</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Waar in al ‘t zoet <rhyme label="c" type="m" corresp="#s2:l4">leydt</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Altoos zietze <rhyme label="b" type="f" corresp="#s2:l2">wond’ren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">‘k Zie op Berg en <rhyme label="a" type="m">Dal</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Allerlei <rhyme label="b" type="f">gewassen</rhyme>;</l>
        <l id="s3:l3" met="3" type="+-">Gy maakt dan het <rhyme label="a" type="m" corresp="#s3:l1">al</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Zoo weeldrig <rhyme label="c" type="m">op-groeyt</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Als ’t gy maar <rhyme label="c" type="m" corresp="#s3:l4">besproeit</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Met u <rhyme label="b" type="f" corresp="#s3:l2">regen-plassen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">’t Wild, en tamme <rhyme label="a" type="m">Vee</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Laat gy daar op <rhyme label="b" type="f">weiden</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Elk in zijne <rhyme label="a" type="m" corresp="#s4:l1">stee</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">U Hand die <rhyme label="c" type="m">voedze</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">De mensch <rhyme label="c" type="m" corresp="#s4:l4">verdoetze</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Dien gyz’ hebt <rhyme label="b" type="f" corresp="#s4:l2">bescheiden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">‘k Zie de boomen <rhyme label="a" type="m">staan</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Groeyende om <rhyme label="b" type="f">hooge</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Met haar groene <rhyme label="a" type="m" corresp="#s5:l1">blaan</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Vercieren de <rhyme label="c" type="m">lucht</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Beladen met <rhyme label="c" type="m" corresp="#s5:l4">vrucht</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Door de smaak en ’t <rhyme label="b" type="f" corresp="#s5:l2">ooge</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">‘t Vliegende <rhyme label="a" type="m">gediert</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Komt zijn huis daar <rhyme label="b" type="f">vesten</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">‘t Zingt daar, ‘t vliegt en <rhyme label="a" type="m" corresp="#s6:l1">zwiert</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">’t Weet van geen <rhyme label="c" type="m">onlust</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">’t Kan vernoegt met <rhyme label="c" type="m" corresp="#s6:l4">rust</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Wonen in de <rhyme label="b" type="f" corresp="#s6:l2">nesten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">d’Aerde draagt haar <rhyme label="a" type="m">kruidt</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Voedsel voor de <rhyme label="b" type="f">menschen</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Schatten deeltze <rhyme label="a" type="m" corresp="#s7:l1">uit</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Grooten <rhyme label="c" type="m">overvloedt</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Allerhande <rhyme label="c" type="m" corresp="#s7:l4">goet</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Geefze, naa ons <rhyme label="b" type="f" corresp="#s7:l2">wenschen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Op dan mijn <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Om uw Godt te <rhyme label="b" type="f">prijzen</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Voor het eeuwig <rhyme label="a" type="m" corresp="#s8:l1">goedt</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Al wat gy hier <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Is daar by als <rhyme label="c" type="m" corresp="#s8:l4">niet</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Gy moet hooger <rhyme label="b" type="f" corresp="#s8:l2">rijzen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">‘k Ben voor ’s werelds <rhyme label="a" type="m">gront</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">In Christo <rhyme label="b" type="f">verkoren</rhyme></l>
        <l id="s9:l3" met="3" type="+-">‘k Staa in Gods <rhyme label="a" type="m" corresp="#s9:l1">verbondt</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">De Heer is mijn <rhyme label="c" type="m">Godt</rhyme>!</l>
        <l id="s9:l5" met="3" type="+-">Ik ben van hem <rhyme label="c" type="m" corresp="#s9:l4">tot</rhyme></l>
        <l id="s9:l6" met="3" type="+-">Zijn kindt nieuws <rhyme label="b" type="f" corresp="#s9:l2">herboren</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">‘k Heb het Rijkste <rhyme label="a" type="m">erf</rhyme></l>
        <l id="s10:l2" met="3" type="+-">‘k Ben een <rhyme label="b" type="f">Hemels-Koning</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">‘k Vreeze geen <rhyme label="a" type="m" corresp="#s10:l1">verderff</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Door ’t Middelaats <rhyme label="c" type="m">Bloedt</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Dat mijn zonden <rhyme label="c" type="m" corresp="#s10:l4">boet</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Krijg ik d’hoogste <rhyme label="b" type="f" corresp="#s10:l2">woning</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">‘k Heb het vrye <rhyme label="a" type="m">recht</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Aan Christi <rhyme label="b" type="f">weldaden</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">‘k Ben hem tot een <rhyme label="a" type="m" corresp="#s11:l1">knecht</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Die zijnen dienst <rhyme label="c" type="m">doe</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">Hy geeft my loon <rhyme label="c" type="m" corresp="#s11:l4">toe</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">Enkel uit <rhyme label="b" type="f" corresp="#s11:l2">genaden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Wat zal ik nu <rhyme label="a" type="m">Heer</rhyme>?</l>
        <l id="s12:l2" met="3" type="+-">Vader! van de <rhyme label="b" type="f">lichten</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">Doen tot uwer <rhyme label="a" type="m" corresp="#s12:l1">eer</rhyme>?</l>
        <l id="s12:l4" met="3" type="+-">Die ’t beste deel <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Waar door de ziel <rhyme label="c" type="m" corresp="#s12:l4">leeft</rhyme>,</l>
        <l id="s12:l6" met="3" type="+-">Voor u <rhyme label="b" type="f" corresp="#s12:l2">aangezichte</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Mijn ziel is <rhyme label="a" type="m">bereidt</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Steeds voor u te <rhyme label="b" type="f">wandelen</rhyme></l>
        <l id="s13:l3" met="3" type="+-">In <rhyme label="a" type="m" corresp="#s13:l1">gehoorzaamheit</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">Na ’t heilig <rhyme label="c" type="m">gebodt</rhyme>,</l>
        <l id="s13:l5" met="3" type="+-">Sterkt my doch ô <rhyme label="c" type="m" corresp="#s13:l4">Godt</rhyme>!</l>
        <l id="s13:l6" met="3" type="+-">Dat ik trouwlijk <rhyme label="b" type="f" corresp="#s13:l2">handle</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">Eens in ’t Hemels <rhyme label="a" type="m">hof</rhyme>,</l>
        <l id="s14:l2" met="3" type="+-">Zijnde <rhyme label="b" type="f">opgenomen</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">Zal ik uwen <rhyme label="a" type="m" corresp="#s14:l1">lof</rhyme>,</l>
        <l id="s14:l4" met="3" type="+-">Volmaakt en <rhyme label="c" type="m">vuurig</rhyme>,</l>
        <l id="s14:l5" met="3" type="+-">Eeuwig <rhyme label="c" type="m" corresp="#s14:l4">geduurig</rhyme>,</l>
        <l id="s14:l6" met="3" type="+-">Tot u laten <rhyme label="b" type="f" corresp="#s14:l2">komen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
