<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>163936</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De Savoyaardse meisjes</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WEl Amsterdamsche <rhyme label="a" type="f">mysjes</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wilt gy mee aan de <rhyme label="b" type="m">trant</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Om met ons ook een <rhyme label="a" type="f" corresp="#s1:l1">rysje</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Te doen naar 't warme <rhyme label="b" type="m" corresp="#s1:l2">Lant</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Gelyk een aerdig <rhyme label="c" type="f">diertje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat by ons is aen <rhyme label="d" type="m">boort</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die wou voor een <rhyme label="c" type="f" corresp="#s1:l5">plaiziertje</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Al met ons vaere <rhyme label="d" type="m" corresp="#s1:l6">voort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een maegt van 20 <rhyme label="a" type="f">jaeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wou mede aen de <rhyme label="b" type="m">Trant</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Om met ons mee te <rhyme label="a" type="f" corresp="#s2:l1">vare</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al na dat Aepe <rhyme label="b" type="m" corresp="#s2:l2">Lant</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Haer naem houd men <rhyme label="c" type="f">secreeten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Doe zy nog vryjer <rhyme label="d" type="m">was</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wiert heel anders <rhyme label="c" type="f" corresp="#s2:l5">geheete</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Wat dunkt u van die <rhyme label="d" type="m" corresp="#s2:l6">Tas</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy was een zoete <rhyme label="a" type="f">Engel</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Is dat geen Rare <rhyme label="b" type="m">klugt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wat dunkt u van die <rhyme label="a" type="f" corresp="#s3:l1">bengel</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Verheugt op het <rhyme label="b" type="m" corresp="#s3:l2">gerugt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van ons schepsvolk <rhyme label="c" type="f">aerdig</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die rare snaeke <rhyme label="d" type="m">zyn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Want die wouden wel <rhyme label="c" type="f" corresp="#s3:l5">vaerdig</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dat de helft zoo mokt <rhyme label="d" type="m" corresp="#s3:l6">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Het Schip wil ik niet <rhyme label="a" type="f">noemen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daer dit is op <rhyme label="b" type="m">geschied</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Daer valt genoeg te <rhyme label="a" type="f" corresp="#s4:l1">roemen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Op de Inhoud van dit <rhyme label="b" type="m" corresp="#s4:l2">Lied</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En van dit Meisje <rhyme label="c" type="f">aerdig</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In Tessel waer <rhyme label="d" type="m">gebeurt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Een stuk zyn aendagt <rhyme label="c" type="f" corresp="#s4:l5">waerdig</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Wie is die daerom <rhyme label="d" type="m" corresp="#s4:l6">Treurt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het is een mysje <rhyme label="a" type="f">aerdig</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Gebooren in <rhyme label="b" type="m">Holland</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Daerom is zy wel <rhyme label="a" type="f" corresp="#s5:l1">waerdig</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Te gaen mee aen de <rhyme label="b" type="m" corresp="#s5:l2">Trant</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Maer zy mag nu niet <rhyme label="c" type="f">meede</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Nu het is <rhyme label="d" type="m">openbaer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Met ons van deze <rhyme label="c" type="f" corresp="#s5:l5">reede</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Al lyt ons scheepje <rhyme label="d" type="m" corresp="#s5:l6">klaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dit doet ons seer <rhyme label="a" type="f">verdriete</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Wyl wy liefhebbers <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Om vogeltjes te <rhyme label="a" type="f" corresp="#s6:l1">schieten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">By zulke mysjes <rhyme label="b" type="m" corresp="#s6:l2">fyn</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ia wy woude nog veel <rhyme label="c" type="f">liever</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dat 'er vyftig van die <rhyme label="d" type="m">zoort</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Tot yder zyn <rhyme label="c" type="f" corresp="#s6:l5">gerieve</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ware by ons aen <rhyme label="d" type="m" corresp="#s6:l6">boort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy die dit Lietje <rhyme label="a" type="f">digte</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat was een raere <rhyme label="b" type="m">snaek</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Hy was ook van de <rhyme label="a" type="f" corresp="#s7:l1">ligte</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En hielt wel van 't <rhyme label="b" type="m" corresp="#s7:l2">vermaek</rhyme></l>
        <l id="s7:l5" met="3" type="-+">En zulke klugjes <rhyme label="c" type="f">meede</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die waere na zyn <rhyme label="d" type="m">zin</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Hy is altyt te <rhyme label="c" type="f" corresp="#s7:l5">vreede</rhyme>:</l>
        <l id="s7:l8" met="3" type="-+">Te speele het spel der <rhyme label="d" type="m" corresp="#s7:l6">min</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dat alle mysjes <rhyme label="a" type="f">voere</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Wy zoude te vreede <rhyme label="b" type="m">zyn</rhyme></l>
        <l id="s8:l3" met="3" type="-+">En ons niet eens en <rhyme label="a" type="f" corresp="#s8:l1">roere</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Hoor dit mooy mysies <rhyme label="b" type="m" corresp="#s8:l2">fyn</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ia wy mogten het wel <rhyme label="c" type="f">lyde</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Want wy liefhebbers <rhyme label="d" type="m">zyn</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Te slape met ons <rhyme label="c" type="f" corresp="#s8:l5">beyde</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Tot dat Aurora <rhyme label="d" type="m" corresp="#s8:l6">schynt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
