    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Wat is er groter vreugde op aarde</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Elise, die mijn <rhyme label="a" type="m">gemoed</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Met de <rhyme label="a" type="m" corresp="#s1:l1">gloed</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van u minne-vlamme <rhyme label="b" type="f">streelde</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Als de kragt der <rhyme label="c" type="m">min-godin</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Eerst ontstak myn hert in <rhyme label="c" type="m" corresp="#s1:l4">min</rhyme></l>
        <l id="s1:l6" met="4" type="+-">En haar soetheid <rhyme label="b" type="f" corresp="#s1:l3">mededeelden</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat vreugd genoot myn Ziel die <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Eer de <rhyme label="a" type="m" corresp="#s2:l1">nyd</rhyme></l>
        <l id="s2:l3" met="4" type="+-">My u lieflijk bysijn <rhyme label="b" type="f">roofden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">En het al te wrede <rhyme label="c" type="m">lot</rhyme></l>
        <l id="s2:l5" met="4" type="+-">My benam het soet <rhyme label="c" type="m" corresp="#s2:l4">genot</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Dat uw liefde my <rhyme label="b" type="f" corresp="#s2:l3">beloofden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maar wat smaakt myn hert <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Nu 't sig <rhyme label="a" type="m" corresp="#s3:l1">ziet</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Van die lieve vreugd <rhyme label="b" type="f">ontsteken</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Die het eer by u <rhyme label="c" type="m">genoot</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Doe uw oog, met schoot op <rhyme label="c" type="m" corresp="#s3:l4">schoot</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Eerst begon mynam te <rhyme label="b" type="f" corresp="#s3:l3">kweken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ag hemel, dat ik eenmaal <rhyme label="a" type="m">sag</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Dat die <rhyme label="a" type="m" corresp="#s4:l1">dag</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Soo vol blydschap mogt <rhyme label="b" type="f">herleven</rhyme>!</l>
        <l id="s4:l4" met="4" type="+-">Hoe sou my u lief <rhyme label="c" type="m">gezigt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Soo vol schoon en vroly <rhyme label="c" type="m" corresp="#s4:l4">ligt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Weer volmaakte vreugde <rhyme label="b" type="f" corresp="#s4:l3">geven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Doch schoon my 't wrede lot <rhyme label="a" type="m">verbied</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Myn <rhyme label="a" type="m" corresp="#s5:l1">verdriet</rhyme></l>
        <l id="s5:l3" met="4" type="+-">U, mijn schoone, self te <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Zal ik tonen dat mijn <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Hoe gevoelig van die <rhyme label="c" type="m" corresp="#s5:l4">smert</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Alles kan om u <rhyme label="b" type="f" corresp="#s5:l3">verdragen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
