  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Inde <rhyme label="a" type="m">min</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En is gheen ghenuchten te <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Inde <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En is gheenen troost oft <rhyme label="a" type="m" corresp="#s1:l1">ghewin</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Ach schoon <rhyme label="b" type="f" corresp="#s1:l2">Clarinde</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die ick <rhyme label="b" type="f" corresp="#s1:l2">beminde</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Ach schoon <rhyme label="b" type="f" corresp="#s1:l2">Clarinde</rhyme>!</l>
        <l id="s1:l8" met="3" type="-+">Ick stel u uyt mijnen <rhyme label="a" type="m" corresp="#s1:l1">sin</rhyme>:</l>
        <l id="s1:l9" met="6" type="+-">Want vergheefs tracht ick naer uwe liefde en <rhyme label="c" type="m">wijsheyt</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Mits u <rhyme label="d" type="m">hert</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Als een <rhyme label="d" type="m" corresp="#s1:l10">bert</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">En ghy als ijs <rhyme label="c" type="m" corresp="#s1:l9">sijt</rhyme>:</l>
        <l id="s1:l13" met="2" type="+-">'Tschijnt ghy <rhyme label="e" type="m">maeght</rhyme>,</l>
        <l id="s1:l14" met="2" type="+-">Voor die <rhyme label="e" type="m" corresp="#s1:l13">claeght</rhyme>,</l>
        <l id="s1:l15" met="2" type="-+">Te dier van prijs <rhyme label="c" type="m" corresp="#s1:l9">zijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Gaet vrij <rhyme label="a" type="m">heên</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ghy onbedachte <rhyme label="b" type="f">Clarinde</rhyme>?</l>
        <l id="s2:l3" met="2" type="+-">Gaet vry <rhyme label="a" type="m" corresp="#s2:l1">heên</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En vlucht voor mijn droevich <rhyme label="a" type="m" corresp="#s2:l1">gheween</rhyme>?</l>
        <l id="s2:l5" met="2" type="-+">De taye <rhyme label="b" type="f" corresp="#s2:l2">linden</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Buyghen voor <rhyme label="b" type="f" corresp="#s2:l2">winden</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">De taye <rhyme label="b" type="f" corresp="#s2:l2">linden</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Beven: en voor mijn <rhyme label="a" type="m" corresp="#s2:l1">ghebeên</rhyme></l>
        <l id="s2:l9" met="6" type="+-">Blijft u hert, als diamant stets <rhyme label="c" type="m">onbewoghen</rhyme>:</l>
        <l id="s2:l10" met="2" type="+-">Wreetste <rhyme label="d" type="m">dier</rhyme>?</l>
        <l id="s2:l11" met="2" type="+-">Aen een <rhyme label="d" type="m" corresp="#s2:l10">gier</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">Moet g'hebben <rhyme label="c" type="m" corresp="#s2:l9">ghesoghen</rhyme>:</l>
        <l id="s2:l13" met="2" type="+-">Want u <rhyme label="e" type="m">min</rhyme>,</l>
        <l id="s2:l14" met="2" type="+-"><rhyme label="e" type="m" corresp="#s2:l13">Herderin'</rhyme>,</l>
        <l id="s2:l15" met="2" type="-+">Is sonder <rhyme label="c" type="m" corresp="#s2:l9">medoghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
