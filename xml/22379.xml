<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22379</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag>sung Lord's Prayer</tag>
    </tags>
    <melody>Ach Vater unser der du bist</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Och Vader onse die daer <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Int hemelsch <rhyme label="a" type="m" corresp="#s1:l1">beurijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hooch ouer ons, dies ghy in den <rhyme label="b" type="m">gheest</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt aenghebeden <rhyme label="c" type="f">wesen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Dijn heylich Name sy <rhyme label="b" type="m" corresp="#s1:l3">vermeest</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Vry en <rhyme label="b" type="m" corresp="#s1:l3">onbeureest</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Op aerden in ons, breet ende <rhyme label="a" type="m" corresp="#s1:l1">wijt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Als in den Hemel <rhyme label="c" type="f" corresp="#s1:l4">ghepresen</rhyme>:</l>
        <l id="s1:l9" met="4" type="-+">Dijn rijck ghenadich ons <rhyme label="d" type="m">toewent</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En laet ons vast <rhyme label="e" type="f">blijuen</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Wat u mishaghet hier <rhyme label="d" type="m" corresp="#s1:l9">present</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Wilt dat wt ons <rhyme label="e" type="f" corresp="#s1:l10">verdrijuen</rhyme>,</l>
        <l id="s1:l13" met="4" type="-+">Op dat wy moeghen altijdt <rhyme label="d" type="m" corresp="#s1:l9">bekent</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">In uwen rijcke <rhyme label="e" type="f" corresp="#s1:l10">verstijuen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Met rechten Heere bidden <rhyme label="a" type="m">wy</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Dijnen wille laet <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Op eerden hier gheschieden <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als in des Hemels <rhyme label="c" type="f">throone</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Want niemandt comen can by <rhyme label="b" type="m" corresp="#s2:l3">dy</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Hoe groot hy <rhyme label="b" type="m" corresp="#s2:l3">sy</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Dan die zijnen wille hier <rhyme label="a" type="m" corresp="#s2:l1">alleyn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Met u vereenicht <rhyme label="c" type="f" corresp="#s2:l4">schoone</rhyme>:</l>
        <l id="s2:l9" met="4" type="-+">Gheeft ons heden, ons daghelijcx <rhyme label="d" type="m">broot</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">En die siele hare <rhyme label="e" type="f">spijsen</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Twelck is u Goddelijck woort, seer <rhyme label="d" type="m" corresp="#s2:l9">bloot</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Dat wijt hooren en <rhyme label="e" type="f" corresp="#s2:l10">prijsen</rhyme>,</l>
        <l id="s2:l13" met="4" type="-+">Daer door ghy ons tot der salicheyt <rhyme label="d" type="m" corresp="#s2:l9">groot</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Den rechten wech doet <rhyme label="e" type="f" corresp="#s2:l10">wijsen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Oock onse schult ende <rhyme label="a" type="m">misdaet</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Heer ons <rhyme label="a" type="m" corresp="#s3:l1">quijtlaet</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Waer door wy u vertoornen <rhyme label="b" type="m">hier</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wilt ons die niet <rhyme label="c" type="f">toemeten</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Want wy ons schuldenaeren met <rhyme label="b" type="m" corresp="#s3:l3">baet</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Quijten sonder <rhyme label="b" type="m" corresp="#s3:l3">haet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Daer met sy ons misdeden <rhyme label="a" type="m" corresp="#s3:l1">schier</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En willent heel <rhyme label="c" type="f" corresp="#s3:l4">vergheten</rhyme>:</l>
        <l id="s3:l9" met="4" type="-+">Laet ons in quellinghe vallen <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Waer door wy mochten <rhyme label="e" type="f">verderuen</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Voor sulcken quade hulpe <rhyme label="d" type="m" corresp="#s3:l9">biet</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Daer door die siele sou <rhyme label="e" type="f" corresp="#s3:l10">steruen</rhyme>,</l>
        <l id="s3:l13" met="4" type="-+">En maeckt ons Heere nae dit <rhyme label="d" type="m" corresp="#s3:l9">verdriet</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">In dijnen rijcke <rhyme label="e" type="f" corresp="#s3:l10">eruen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
