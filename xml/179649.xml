<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179649</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IK droomde dat ik lag te <rhyme label="a" type="f">Slapen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">By een Herderinne met haer <rhyme label="a" type="f" corresp="#s1:l1">Schapen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Og myn dogd en og myn <rhyme label="b" type="m">dogt</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Hier zal ik vreugde <rhyme label="a" type="f" corresp="#s1:l1">rapen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Ik voegde myn terstond daer <rhyme label="c" type="m">by</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Alzoo zagjes aen haer <rhyme label="c" type="m" corresp="#s1:l5">zy</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Toen dogt ik, toen dogt <rhyme label="d" type="m">ik</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s1:l7">schrik</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s1:l7">schrik</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ik dees stonde lag in <rhyme label="a" type="f">ruste</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Want ik vol <rhyme label="a" type="f" corresp="#s2:l1">Minneluste</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">En og myn dogt en og myn <rhyme label="b" type="m">dogt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ik plukte het roosje van <rhyme label="a" type="f" corresp="#s2:l1">minnelusten</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Ik was te vol <rhyme label="c" type="m">fantezy</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">In de droom vermaekte <rhyme label="c" type="m" corresp="#s2:l5">my</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Toen schoot ik toen schoot <rhyme label="d" type="m">ik</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s2:l7">schrik</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s2:l7">schrik</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Och ik lag te vol met <rhyme label="a" type="f">stonden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">By een Herderinne vol met <rhyme label="a" type="f" corresp="#s3:l1">wonden</rhyme></l>
        <l id="s3:l3" met="4" type="-+">En og myn dogt en og myn <rhyme label="b" type="m">dogt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hier legt ik vast <rhyme label="a" type="f" corresp="#s3:l1">gebonden</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Want ik zoenden haer <rhyme label="c" type="m">zoetjes</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Toen verroerde zy haer <rhyme label="c" type="m" corresp="#s3:l5">voetjes</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Toen voelde ik toen voelde <rhyme label="d" type="m">ik</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s3:l7">schrik</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s3:l7">schrik</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat is het Droomen en het <rhyme label="a" type="f">Minnen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Als men daer op steld zyn <rhyme label="a" type="f" corresp="#s4:l1">zinnen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Og myn dogt en og myn <rhyme label="b" type="m">dogt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ik zie myn <rhyme label="a" type="f" corresp="#s4:l1">Herderinne</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En toen vond ik myn <rhyme label="c" type="m">bedrogen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Want zy is van myn <rhyme label="c" type="m" corresp="#s4:l5">gevlogen</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Nu dogt ik nu dogt <rhyme label="d" type="m">ik</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s4:l7">schrik</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Maer helaes ik <rhyme label="d" type="m" corresp="#s4:l7">schrik</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
