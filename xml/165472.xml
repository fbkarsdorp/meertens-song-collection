    <tags>
      <tag translation="New Year's song">nieuwjaarslied</tag>
      <tag translation="admonitory song">vermaanlied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody>Cupido's felle stralen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ghij schepsels alte <rhyme label="a" type="m">saem</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In adam heel <rhyme label="b" type="m">verstickt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">looft nu des hoochstens <rhyme label="a" type="m" corresp="#s1:l1">naem</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die ons heeft <rhyme label="b" type="m" corresp="#s1:l2">toegeschickt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">zijn soon, <rhyme label="c" type="m">ydoon</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ons heylant zeer <rhyme label="a" type="m" corresp="#s1:l1">bequaem</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Van uijt Maria de <rhyme label="a" type="m">maecht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is hy van godt <rhyme label="b" type="m">verweckt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">zij bleeff maecht <rhyme label="a" type="m" corresp="#s2:l1">onversaecht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">uijt Davidts stam <rhyme label="b" type="m" corresp="#s2:l2">correckt</rhyme></l>
        <l id="s2:l5" met="2" type="-+">doort woort,, <rhyme label="c" type="m">gehoort</rhyme></l>
        <l id="s2:l6" met="3" type="-+">dat den vader <rhyme label="a" type="m" corresp="#s2:l1">behaecht</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">voor ons erff sonden <rhyme label="a" type="m">al</rhyme></l>
        <l id="s3:l2" met="3" type="-+">heeft hy betaelt de <rhyme label="b" type="m">boet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">zuijvert ons In dit <rhyme label="a" type="m" corresp="#s3:l1">dal</rhyme></l>
        <l id="s3:l4" met="3" type="-+">met zijn dierbaer <rhyme label="b" type="m" corresp="#s3:l2">bloet</rhyme></l>
        <l id="s3:l5" met="2" type="-+">en doot,, <rhyme label="c" type="m">minJoot</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">voor cleyn groot ende <rhyme label="a" type="m" corresp="#s3:l1">smal</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Des vaders gramschap <rhyme label="a" type="m">hij</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In alles versoent <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s4:l3" met="3" type="-+">t'serpent ons waer <rhyme label="a" type="m" corresp="#s4:l1">partij</rhyme></l>
        <l id="s4:l4" met="3" type="-+">door zyn macht nu <rhyme label="b" type="m" corresp="#s4:l2">sneeft</rhyme></l>
        <l id="s4:l5" met="2" type="-+">zyn cop,, en <rhyme label="c" type="m">crop</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">vertreet hy aen deen <rhyme label="a" type="m" corresp="#s4:l1">zij</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dewijl hy heeft <rhyme label="a" type="m">voldaen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">twelck was <rhyme label="b" type="m">gepropheteert</rhyme></l>
        <l id="s5:l3" met="3" type="-+">gaet nu den zijne <rhyme label="a" type="m" corresp="#s5:l1">paen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">voldoet zijn zoet <rhyme label="b" type="m" corresp="#s5:l2">begeert</rhyme></l>
        <l id="s5:l5" met="2" type="-+">met cracht,, en <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">vast op zijn woort wilt <rhyme label="a" type="m" corresp="#s5:l1">staen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Leght al u zonden <rhyme label="a" type="m">off</rhyme></l>
        <l id="s6:l2" met="3" type="-+">tot noch toe boos <rhyme label="b" type="m">gepleecht</rhyme></l>
        <l id="s6:l3" met="3" type="-+">bekent dat ghy zyt <rhyme label="a" type="m" corresp="#s6:l1">stoff</rhyme></l>
        <l id="s6:l4" met="3" type="-+">der aerden <rhyme label="b" type="m" corresp="#s6:l2">onbeweecht</rhyme></l>
        <l id="s6:l5" met="2" type="-+">hoort hem,, zijn <rhyme label="c" type="m">stem</rhyme></l>
        <l id="s6:l6" met="3" type="-+">gegeven uijt zijn <rhyme label="a" type="m" corresp="#s6:l1">hoff</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">van nieuws Doch al <rhyme label="a" type="m">begint</rhyme></l>
        <l id="s7:l2" met="3" type="-+">te leven goet <rhyme label="b" type="m">oprecht</rhyme></l>
        <l id="s7:l3" met="3" type="-+">voorwis hy die <rhyme label="a" type="m" corresp="#s7:l1">bemint</rhyme></l>
        <l id="s7:l4" met="3" type="-+">en blijft In hem <rhyme label="b" type="m" corresp="#s7:l2">gehecht</rhyme></l>
        <l id="s7:l5" met="2" type="-+">die dat,, voet <rhyme label="c" type="m">pat</rhyme></l>
        <l id="s7:l6" met="3" type="-+">hem te gaen <rhyme label="a" type="m" corresp="#s7:l1">onderwint</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Tis godt een groote <rhyme label="a" type="m">vreucht</rhyme></l>
        <l id="s8:l2" met="3" type="-+">die hem hout <rhyme label="b" type="m">onbesmet</rhyme></l>
        <l id="s8:l3" met="3" type="-+">altyt wandelt In <rhyme label="a" type="m" corresp="#s8:l1">deucht</rhyme></l>
        <l id="s8:l4" met="3" type="-+">als accoleijen <rhyme label="b" type="m" corresp="#s8:l2">wit</rhyme></l>
        <l id="s8:l5" met="2" type="-+">zijn tyt,, <rhyme label="c" type="m">verslijt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In zyn wetten <rhyme label="a" type="m" corresp="#s8:l1">verheucht</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Met mij princen <rhyme label="a" type="m">bekent</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Datter geen <rhyme label="b" type="m">salicheyt</rhyme></l>
        <l id="s9:l3" met="3" type="-+">en is In zoucken <rhyme label="a" type="m" corresp="#s9:l1">Jent</rhyme></l>
        <l id="s9:l4" met="3" type="-+">dan aen zijn <rhyme label="b" type="m" corresp="#s9:l2">Magisteijt</rhyme></l>
        <l id="s9:l5" met="2" type="-+">tnieu Jaer te <rhyme label="c" type="m">gaer</rhyme></l>
        <l id="s9:l6" met="3" type="-+">toont lieffd' es <rhyme label="a" type="m" corresp="#s9:l1">t'fondament</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
