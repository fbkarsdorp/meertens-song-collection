<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20369</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Sarabande Pinel</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DOen ick lest wandelde over de <rhyme label="a" type="f">Helder</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Quam my soo aerdigh een Nymphje te <rhyme label="b" type="m">moet</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Seecker haer ooghjes die scheenen veel <rhyme label="a" type="f" corresp="#s1:l1">snelder</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Dan het ghesicht van de A - renden <rhyme label="b" type="m" corresp="#s1:l2">doet</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Ick dacht, dit 's 't Puyckje dat boven al <rhyme label="c" type="m">roemt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Misschien is sy nae dees plaets ghe- <rhyme label="c" type="m" corresp="#s1:l5">-noemt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Datse ghehouden wordt voor een <rhyme label="d" type="m">Goddin</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">En d'alder-opperste Harder- <rhyme label="d" type="m" corresp="#s1:l7">-in</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Haer geele vlechjes die waren <rhyme label="a" type="f">gestrengelt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Met zijde Snoertjes, van menich <rhyme label="b" type="m">couleur</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">'t Kransje van veelderley Bloempjes <rhyme label="a" type="f" corresp="#s2:l1">gemengelt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Stondt aen haer Hoetje, so schoon in sijn <rhyme label="b" type="m" corresp="#s2:l2">fleur</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Haer brave Leed'tjes, haer Lipjes schoon <rhyme label="c" type="m">root</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Haer vlugghe treed'tjes, en Borsjes <rhyme label="c" type="m" corresp="#s2:l5">bloot</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">Haer vlammend' Ooghjes op 't korste <rhyme label="d" type="m">gheseydt</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Hebben benomen mijn <rhyme label="d" type="m" corresp="#s2:l7">vryigheydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Singende quamse haer Schaepjes aen <rhyme label="a" type="f">drijven</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wacker, en vaerdigh, om brengen op 't <rhyme label="b" type="m">stal</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Daer ick wel wenschten een nachtje te <rhyme label="a" type="f" corresp="#s3:l1">blijven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">'k Vollighd' haer soetjes beneden in 't <rhyme label="b" type="m" corresp="#s3:l2">dal</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Tot aen haer woningh, en Vadertjes <rhyme label="c" type="m">huys</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Daer ick haer miste, dat was mijn <rhyme label="c" type="m" corresp="#s3:l5">kruys</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Droevigh al suchtende keerden ick <rhyme label="d" type="m">mee</rhyme>:</l>
        <l id="s3:l8" met="4" type="+-">Socht om te rusten mijn <rhyme label="d" type="m" corresp="#s3:l7">legher-stee</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer het geprickelde Hartje, vol <rhyme label="a" type="f">Minne</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Liet my niet slapen, doch sluymerd' ick <rhyme label="b" type="m">veel</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Stadigh soo woelden mijn besighe <rhyme label="a" type="f" corresp="#s4:l1">sinnen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Over de schoonheydt van sulcken <rhyme label="b" type="m" corresp="#s4:l2">Juweel</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Sy schijnt gheboren uyt Edelen <rhyme label="c" type="m">stam</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Doen sy van voren my teghen <rhyme label="c" type="m" corresp="#s4:l5">quam</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Dacht ick, Bellinde, o schoonste van <rhyme label="d" type="m">al</rhyme>!</l>
        <l id="s4:l8" met="4" type="+-">Schilder noyt schoonder af malen <rhyme label="d" type="m" corresp="#s4:l7">sal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Liggende wacker, so quam my te <rhyme label="a" type="f">binnen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">dat ick wou maecken een Liedtjen of <rhyme label="b" type="m">twee</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om te gaen offeren aen de <rhyme label="a" type="f" corresp="#s5:l1">Goddinne</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Mogelijck sal haer mijn smerte doen <rhyme label="b" type="m" corresp="#s5:l2">wee</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Het schielijck reysen dat is nu <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">'k Wil liever weyden, en met haer <rhyme label="c" type="m" corresp="#s5:l5">gaen</rhyme>:</l>
        <l id="s5:l7" met="4" type="+-">Mocht ick ten eersten, gelijckmen hier <rhyme label="d" type="m">doet</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Neffens haer Queesten, dat waer soo <rhyme label="d" type="m" corresp="#s5:l7">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
