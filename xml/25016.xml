  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Joncfrouwe edel, <rhyme label="a" type="f">goedertieren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Welgeraket van <rhyme label="a" type="f" corresp="#s1:l1">manieren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Als ghi ghebiedt, so sal ic <rhyme label="a" type="f" corresp="#s1:l1">vieren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Tfernoy, daer ic ben <rhyme label="b" type="f">inne</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Dat ic dus moete <rhyme label="c" type="f">quelen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat doet mire liefste <rhyme label="b" type="f" corresp="#s1:l4">minne</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">In cans mi niet <rhyme label="c" type="f" corresp="#s1:l5">ghehelen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Ghewaerlike ic <rhyme label="b" type="f" corresp="#s1:l4">ontsinne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">U eigen wil ic <rhyme label="a" type="f">wesen</rhyme>;</l>
        <l id="s2:l2" met="4" type="+-">Wet vorwaer, in cans <rhyme label="a" type="f" corresp="#s2:l1">genesen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Het ensi alsoe, dat ic in <rhyme label="a" type="f" corresp="#s2:l1">desen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Troest moghe an u <rhyme label="b" type="f">ghewinnen</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Dat ic dus moete <rhyme label="c" type="f">quelen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat doet mire liefste <rhyme label="b" type="f" corresp="#s2:l4">minne</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">In cans mi niet <rhyme label="c" type="f" corresp="#s2:l5">ghehelen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Ghewaerlike ic <rhyme label="b" type="f" corresp="#s2:l4">ontsinne</rhyme>.</l>
      </lg>
    </lg>
  </text>
