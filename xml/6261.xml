  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">PHilles <rhyme label="a" type="f">Oogen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Door lonkjens altoos <rhyme label="b" type="f">blijer</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Hebben wel <rhyme label="c" type="m">eer</rhyme></l>
        <l id="s1:l4" met="2" type="+-">My <rhyme label="a" type="f" corresp="#s1:l1">bewoogen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Te worden haaren <rhyme label="b" type="f" corresp="#s1:l2">vryer</rhyme>;</l>
        <l id="s1:l6" met="2" type="-+">Maar nu is 't <rhyme label="c" type="m" corresp="#s1:l3">veer</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Nu zy mijn <rhyme label="d" type="m">hart</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Beprikkelt heeft met <rhyme label="d" type="m" corresp="#s1:l7">smart</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">En ik mijn <rhyme label="e" type="f">zinnen</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Haar, door het <rhyme label="e" type="f" corresp="#s1:l9">minnen</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Heb laten <rhyme label="e" type="f" corresp="#s1:l9">winnen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Stoot zy my in <rhyme label="f" type="m">gevaar</rhyme>,</l>
        <l id="s1:l13" met="5" type="-+">Om dat, om dat ik niemant min als <rhyme label="f" type="m" corresp="#s1:l12">haar</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Mijne <rhyme label="a" type="f">pijnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Bruykt zy tot <rhyme label="b" type="f">lekkernijen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Voor haren <rhyme label="c" type="m">geest</rhyme>.</l>
        <l id="s2:l4" met="2" type="+-">Hoe kan 't <rhyme label="a" type="f" corresp="#s2:l1">schijnen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat zy mijn smart en <rhyme label="b" type="f" corresp="#s2:l2">lijen</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Noch eens <rhyme label="c" type="m" corresp="#s2:l3">geneest</rhyme>?</l>
        <l id="s2:l7" met="2" type="-+">Neen, neen, ik <rhyme label="d" type="m">schaf</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Voor eerst dit vryen <rhyme label="d" type="m" corresp="#s2:l7">af</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">Wil zy niet <rhyme label="e" type="f">achten</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Mijn droeve <rhyme label="e" type="f" corresp="#s2:l9">klachten</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Zo zal ik <rhyme label="e" type="f" corresp="#s2:l9">wachten</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Tot zy haar eens bedenkt, bedenkt, <rhyme label="f" type="m">bedenkt</rhyme>,</l>
        <l id="s2:l13" met="5" type="-+">En graagh my het Ia-woord <rhyme label="f" type="m" corresp="#s2:l12">schenkt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
