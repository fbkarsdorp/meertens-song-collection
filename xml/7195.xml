<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>7195</id>
    <date>
      <year-start>1577</year-start>
      <year-end>1577</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
