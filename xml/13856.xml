  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DRooght af u nat <rhyme label="a" type="m">ghetraen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">’t Schreyen mach nou niet <rhyme label="b" type="f">baten</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Kom Neeltje, laet ons <rhyme label="a" type="m" corresp="#s1:l1">gaen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Daer wy wat verder <rhyme label="b" type="f" corresp="#s1:l2">praten</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Hier is’t daer wy lest <rhyme label="b" type="f" corresp="#s1:l2">saten</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Neeltje, nou spreeckt vry <rhyme label="c" type="m">uyt</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Wil jou de Knecht <rhyme label="b" type="f" corresp="#s1:l2">verlaten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Soo doet hy als een <rhyme label="c" type="m" corresp="#s1:l6">Guyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ock Trijn, och lieve <rhyme label="a" type="m">Trijn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Och! wat sal ick <rhyme label="b" type="f">beginnen</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Siet al mijn vrienden <rhyme label="a" type="m" corresp="#s2:l1">zijn</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Schier dol, en uyt haer <rhyme label="b" type="f" corresp="#s2:l2">sinnen</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Die mijn pleghen te <rhyme label="b" type="f" corresp="#s2:l2">minnen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Daer van werd ick <rhyme label="c" type="m">gheaet</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Ach! ‘t hart leydt my van <rhyme label="b" type="f" corresp="#s2:l2">binnen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Als een vuyl Ey en <rhyme label="c" type="m" corresp="#s2:l6">slaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat Neeltje, benje <rhyme label="a" type="m">mal</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Ick sou de Guyt <rhyme label="b" type="f">vergeten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En achten ‘t <rhyme label="a" type="m" corresp="#s3:l1">niet-met-al</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Schoon al waer ick <rhyme label="b" type="f" corresp="#s3:l2">ghebeten</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Neeltje dit moet jy <rhyme label="b" type="f" corresp="#s3:l2">weten</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">‘t Quaedt loont sen Meester <rhyme label="c" type="m">wel</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Denckt dat jy noch selt <rhyme label="b" type="f" corresp="#s3:l2">eten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Als hy ‘t niet hebben <rhyme label="c" type="m" corresp="#s3:l6">sel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Iae Trijn, dat hoop ick <rhyme label="a" type="m">mee</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Maer ‘t gaet mijn soo ter <rhyme label="b" type="f">harten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Aen ‘t lijf is niet een <rhyme label="a" type="m" corresp="#s4:l1">stee</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Daer ‘t mijn niet schijnt te <rhyme label="b" type="f" corresp="#s4:l2">smarten</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Sijn sarren en sijn <rhyme label="b" type="f" corresp="#s4:l2">tarten</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Dat doet mijn jonck hart <rhyme label="c" type="m">wee</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Och! wist jy Trijn wat <rhyme label="b" type="f" corresp="#s4:l2">parten</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat hy wel met mijn <rhyme label="c" type="m" corresp="#s4:l6">dee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maer Neeltje, wat jy <rhyme label="a" type="m">seght</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Ick docht somtijdts voor <rhyme label="b" type="f">desen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat lijckt een stemmigh <rhyme label="a" type="m" corresp="#s5:l1">Knecht</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Een Ionckman <rhyme label="b" type="f" corresp="#s5:l2">uytghelesen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Fraey van pastuer en <rhyme label="b" type="f" corresp="#s5:l2">wesen</rhyme>;</l>
        <l id="s5:l6" met="3" type="+-">Wel wat men hoort en <rhyme label="c" type="m">siet</rhyme>!</l>
        <l id="s5:l7" met="3" type="-+">Men mach de Knechts wel <rhyme label="b" type="f" corresp="#s5:l2">vreesen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">S’en deughen immers <rhyme label="c" type="m" corresp="#s5:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Iae Trijn, had jy ‘t <rhyme label="a" type="m">ghesien</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Als hy mijn eerst quam <rhyme label="b" type="f">vryen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Van honderde niet <rhyme label="a" type="m" corresp="#s6:l1">ien</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Die haer soo keunen <rhyme label="b" type="f" corresp="#s6:l2">myen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Gheensins en mochten <rhyme label="b" type="f" corresp="#s6:l2">lyen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Dat ick wat kreegh of <rhyme label="c" type="m">deed</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Maer was tot allen <rhyme label="b" type="f" corresp="#s6:l2">ryen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ghewilligh voor my <rhyme label="c" type="m" corresp="#s6:l6">reedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Iae, jae, dat is den <rhyme label="a" type="m">aerdt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wech, wech met die soo <rhyme label="b" type="f">vlaeyen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Daer ben ick voor <rhyme label="a" type="m" corresp="#s7:l1">vervaert</rhyme>.</l>
        <l id="s7:l4" met="3" type="+-">Nou Neeltjen eer wy <rhyme label="b" type="f" corresp="#s7:l2">schaeyen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Soo dienter tusschen <rhyme label="b" type="f" corresp="#s7:l2">baeyen</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Een goedt opstel <rhyme label="c" type="m">ghemaeckt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Hoe wy den Haspel <rhyme label="b" type="f" corresp="#s7:l2">draeyen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Dat hy in’t net eens <rhyme label="c" type="m" corresp="#s7:l6">raeckt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Trijntje dat most <rhyme label="a" type="m">gheschien</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Als hy my socht te <rhyme label="b" type="f">spreecken</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dan soud’ ick jou <rhyme label="a" type="m" corresp="#s8:l1">ontbien</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Stil-zwijghende <rhyme label="b" type="f" corresp="#s8:l2">versteecken</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Wou hy sijn woordt dan <rhyme label="b" type="f" corresp="#s8:l2">breecken</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Soo wasset waerdt dat <rhyme label="c" type="m">hy</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Wierdt dapper <rhyme label="b" type="f" corresp="#s8:l2">uytghestreecken</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Voor alle sijn <rhyme label="c" type="m" corresp="#s8:l6">boevery</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dat’s goedt, nou laet ons <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">De vlugghe Tijdt loopt <rhyme label="b" type="f">heenen</rhyme>.</l>
        <l id="s9:l3" met="3" type="-+">Ach! komt hy tavondt <rhyme label="a" type="m" corresp="#s9:l1">aen</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Hoe sellen dese <rhyme label="b" type="f" corresp="#s9:l2">steenen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Hem springhen voor de <rhyme label="b" type="f" corresp="#s9:l2">Beenen</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Dat zweer ick, heet ick <rhyme label="c" type="m">Trijn</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Wat of de Boeven <rhyme label="b" type="f" corresp="#s9:l2">meenen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Dat sy ‘t al meester <rhyme label="c" type="m" corresp="#s9:l6">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
