<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2379</id>
    <date>
      <year-start>1587</year-start>
      <year-end>1587</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags>
      <tag>reflective song</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Die de wereld iets wel beziet</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ter liefden Godts willen wij <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die therte doet <rhyme label="a" type="f" corresp="#s1:l1">opspringen</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">Sij is den fleur van alle <rhyme label="b" type="m">ieucht</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Wij en connense niet <rhyme label="c" type="f">volprijsen</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Sy doet die ziele <rhyme label="c" type="f" corresp="#s1:l4">verrijsen</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Uutter sonden totter <rhyme label="b" type="m" corresp="#s1:l3">deucht</rhyme> /</l>
        <l id="s1:l7" met="4" type="-+">Inwendich inden gheest sticht zy haer <rhyme label="b" type="m" corresp="#s1:l3">vreucht</rhyme>/</l>
        <l id="s1:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s1:l8">zyt</rhyme> /</l>
        <l id="s1:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s1:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Liefde die can <rhyme label="a" type="f">decken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Veel sonden ende <rhyme label="a" type="f" corresp="#s2:l1">vlecken</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">Die de menschen hebben <rhyme label="b" type="m">misdaen</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Sij sullen tot alle <rhyme label="c" type="f">tyden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Tegen hen sonden <rhyme label="c" type="f" corresp="#s2:l4">strijden</rhyme> /</l>
        <l id="s2:l6" met="4" type="-+">Die met Godts liefde zyn <rhyme label="b" type="m" corresp="#s2:l3">bevaen</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Sij doet die oogen storten menigen <rhyme label="b" type="m" corresp="#s2:l3">traen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme> /</l>
        <l id="s2:l9" met="3" type="-+">O mensche wie dat ghij <rhyme label="d" type="m" corresp="#s2:l8">zyt</rhyme> /</l>
        <l id="s2:l10" met="4" type="-+">Schout discoort / vliet haet ende <rhyme label="d" type="m" corresp="#s2:l8">nijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Liefde can <rhyme label="a" type="f">verdraegen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Veel lijdens sonder <rhyme label="a" type="f" corresp="#s3:l1">claegen</rhyme> /</l>
        <l id="s3:l3" met="4" type="-+">Dwelc haeren beminden <rhyme label="b" type="m">zint</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Met duchten ende <rhyme label="c" type="f">suchten</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Wilt liefde altyt <rhyme label="c" type="f" corresp="#s3:l4">vluchten</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Int herte van haer lief <rhyme label="b" type="m" corresp="#s3:l3">bemint</rhyme> /</l>
        <l id="s3:l7" met="4" type="-+">Alle quade begeerten zy <rhyme label="b" type="m" corresp="#s3:l3">verwint</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s3:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s3:l8">zyt</rhyme> /</l>
        <l id="s3:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s3:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Liefde sal altyt <rhyme label="a" type="f">wercken</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">Een goet propoost <rhyme label="a" type="f" corresp="#s4:l1">verstercken</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Int herte daer zy is <rhyme label="b" type="m">gheplant</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">De deuchden doet zy <rhyme label="c" type="f">groyen</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Ende tquaet <rhyme label="c" type="f" corresp="#s4:l4">uutroyen</rhyme> /</l>
        <l id="s4:l6" met="4" type="-+">Zy en compt met geene ydele <rhyme label="b" type="m" corresp="#s4:l3">hant</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Liefde gaet te boven alle <rhyme label="b" type="m" corresp="#s4:l3">verstant</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s4:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s4:l8">zyt</rhyme> /</l>
        <l id="s4:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s4:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Liefde doet nau <rhyme label="a" type="f">hooren</rhyme>/</l>
        <l id="s5:l2" met="3" type="-+">Zy luystert met haeren <rhyme label="a" type="f" corresp="#s5:l1">ooren</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Naer die stemme des bruydegoms <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hy roept haer / compt myn <rhyme label="c" type="f">schoone</rhyme></l>
        <l id="s5:l5" met="3" type="-+">In de steengaeten <rhyme label="c" type="f" corresp="#s5:l4">woonen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Van myn bloedige wonden <rhyme label="b" type="m" corresp="#s5:l3">soet</rhyme> /</l>
        <l id="s5:l7" met="4" type="-+">Daer wordi bewaert in <rhyme label="b" type="m" corresp="#s5:l3">tegenspoet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s5:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s5:l8">zyt</rhyme> /</l>
        <l id="s5:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s5:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Liefde sal altyt <rhyme label="a" type="f">toonen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat zy begeert te <rhyme label="a" type="f" corresp="#s6:l1">woonen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Daer haeren beminden <rhyme label="b" type="m">woont</rhyme> /</l>
        <l id="s6:l4" met="3" type="-+">Zy en heeft hier gheen <rhyme label="c" type="f">ruste</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Voor dat zy blust met <rhyme label="c" type="f" corresp="#s6:l4">luste</rhyme> /</l>
        <l id="s6:l6" met="4" type="-+">En dat lief met liefde wort <rhyme label="b" type="m" corresp="#s6:l3">gheloont</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Boven in den hemel is zy <rhyme label="b" type="m" corresp="#s6:l3">ghecroont</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s6:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s6:l8">zyt</rhyme> /</l>
        <l id="s6:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s6:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Liefde sal altyt <rhyme label="a" type="f">vraegen</rhyme> /</l>
        <l id="s7:l2" met="3" type="-+">Hoe dat zy mocht <rhyme label="a" type="f" corresp="#s7:l1">behaegen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Jesus haeren lieve <rhyme label="b" type="m">bemint</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Zy acht haer selven <rhyme label="c" type="f">cleyne</rhyme>/</l>
        <l id="s7:l5" met="3" type="-+">En hout haer herte <rhyme label="c" type="f" corresp="#s7:l4">reyne</rhyme> /</l>
        <l id="s7:l6" met="4" type="-+">Tot bidden es zy meest <rhyme label="b" type="m" corresp="#s7:l3">ghesint</rhyme> /</l>
        <l id="s7:l7" met="4" type="-+">Haer gebreken zy ootmoedelyck <rhyme label="b" type="m" corresp="#s7:l3">kint</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s7:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s7:l8">zyt</rhyme> /</l>
        <l id="s7:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s7:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Liefde wilt gaerne <rhyme label="a" type="f">derven</rhyme> /</l>
        <l id="s8:l2" met="3" type="-+">En in haer <rhyme label="a" type="f" corresp="#s8:l1">versterven</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Tgene dat haer lief <rhyme label="b" type="m">mishaecht</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Want om zynent <rhyme label="c" type="f">wille</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Is zy eenich ende <rhyme label="c" type="f" corresp="#s8:l4">stille</rhyme> /</l>
        <l id="s8:l6" met="4" type="-+">Naer aertsche zaeken zy luttel <rhyme label="b" type="m" corresp="#s8:l3">vraecht</rhyme>.</l>
        <l id="s8:l7" met="4" type="-+">Haers liefs lyden zy int herte <rhyme label="b" type="m" corresp="#s8:l3">draecht</rhyme> /</l>
        <l id="s8:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s8:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s8:l8">zyt</rhyme> /</l>
        <l id="s8:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s8:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Liefde doet hier <rhyme label="a" type="f">haecken</rhyme> /</l>
        <l id="s9:l2" met="3" type="-+">Smelten ende <rhyme label="a" type="f" corresp="#s9:l1">blaecken</rhyme></l>
        <l id="s9:l3" met="4" type="-+">De ziele naerden hemel <rhyme label="b" type="m">schoon</rhyme> /</l>
        <l id="s9:l4" met="3" type="-+">Zy wilt tot allen <rhyme label="c" type="f">uren</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Haer dry crachten <rhyme label="c" type="f" corresp="#s9:l4">sturen</rhyme> /</l>
        <l id="s9:l6" met="4" type="-+">Zy soeckt Godt voor haeren <rhyme label="b" type="m" corresp="#s9:l3">loon</rhyme> /</l>
        <l id="s9:l7" met="4" type="-+">Het is het bruyloftcleedt van grooten <rhyme label="b" type="m" corresp="#s9:l3">toon</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s9:l9" met="3" type="-+">O Mensche wie dat ghij <rhyme label="d" type="m" corresp="#s9:l8">zyt</rhyme> /</l>
        <l id="s9:l10" met="4" type="-+">Schout ranceur / vliet haet ende <rhyme label="d" type="m" corresp="#s9:l8">nyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ter eeren ons lief <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die ons allen heeft <rhyme label="a" type="f" corresp="#s10:l1">genesen</rhyme> /</l>
        <l id="s10:l3" met="4" type="-+">Heb ick ghedicht dit gheestelyck <rhyme label="b" type="m">liedt</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Jesus van <rhyme label="c" type="f">Nazareene</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Is myn lief die ick <rhyme label="c" type="f" corresp="#s10:l4">meene</rhyme> /</l>
        <l id="s10:l6" met="4" type="-+">Hy can my helpen uut alle <rhyme label="b" type="m" corresp="#s10:l3">verdriet</rhyme> /</l>
        <l id="s10:l7" met="4" type="-+">Alle ander liefde en acht ick <rhyme label="b" type="m" corresp="#s10:l3">niet</rhyme>.</l>
        <l id="s10:l8" met="3" type="-+">Hebt lief tot alder <rhyme label="d" type="m">tyt</rhyme></l>
        <l id="s10:l9" met="3" type="-+">O mensche wie dat ghij <rhyme label="d" type="m" corresp="#s10:l8">zyt</rhyme>.</l>
        <l id="s10:l10" met="4" type="-+">Schout ranceur / vliedt haet ende <rhyme label="d" type="m" corresp="#s10:l8">nyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
