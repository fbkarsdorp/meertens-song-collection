<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188692</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Dansons la carmagnole</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O lieve Vrijheid, dierbaar <rhyme label="a" type="m">pand</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">O roem en eer van <rhyme label="a" type="m" corresp="#s1:l1">Nederland</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Te lang, te wreed <rhyme label="b" type="m">verdrukt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zijt den stalen dwang <rhyme label="b" type="m" corresp="#s1:l3">ontrukt</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>;</l>
        <l id="s1:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dansons la <rhyme label="c" type="f" corresp="#s1:l5">Carmagnole</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s1:l6">Canon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Naar zeven jaaren <rhyme label="a" type="m">baljitigschap</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Klimt, gij ten hoogen glorie <rhyme label="a" type="m" corresp="#s2:l1">trap</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En ziet het slaafsch <rhyme label="b" type="m">gebroed</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Onder uwen vrijen <rhyme label="b" type="m" corresp="#s2:l3">voet</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dansons la <rhyme label="c" type="f" corresp="#s2:l5">Carmagnole</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s2:l6">Canon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'t Was Vrankrijk waar gij 't eerst <rhyme label="a" type="m">verscheen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Gij zaagt aldaar 't geweld <rhyme label="a" type="m" corresp="#s3:l1">vertreen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En door der Gallen <rhyme label="b" type="m">vuist</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Al wat u weerstond <rhyme label="b" type="m" corresp="#s3:l3">vergruist</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dansons la <rhyme label="c" type="f" corresp="#s3:l5">Carmagnole</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s3:l6">Canon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Van daar toog gij naar ons <rhyme label="a" type="m">Gewest</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Waar gij uw zetel hebt <rhyme label="a" type="m" corresp="#s4:l1">gevest</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Bescherm nu Bato's <rhyme label="b" type="m">Kroost</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Lieve Vrijheid, al zijn <rhyme label="b" type="m" corresp="#s4:l3">troost</rhyme>!</l>
        <l id="s4:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dansons la <rhyme label="c" type="f" corresp="#s4:l5">Carmagnole</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s4:l6">Canon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Bataaf, verweer met kruid en <rhyme label="a" type="m">loot</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Uw lieve Vrijheid tot den <rhyme label="a" type="m" corresp="#s5:l1">dood</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Zoo valt, wat u <rhyme label="b" type="m">weÃªrstaat</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Voor uw welgegronde <rhyme label="b" type="m" corresp="#s5:l3">haat</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dansons la <rhyme label="c" type="f" corresp="#s5:l5">Carmagnole</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s5:l6">Canon</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Koom al wie u te snood <rhyme label="a" type="m">misleid</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Zaagt boeijen door <rhyme label="a" type="m" corresp="#s6:l1">heerschzuchtigheid</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Koom in onz' arm en <rhyme label="b" type="m">voel</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Wat ons kloppend hart <rhyme label="b" type="m" corresp="#s6:l3">bedoel</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dansons la <rhyme label="c" type="f" corresp="#s6:l5">Carmagnole</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s6:l6">Canon</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De lieve Vrijheid haat u <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Schoon gij haar weleer <rhyme label="a" type="m" corresp="#s7:l1">verstiet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">'t Vergeven staat ons <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat past een Vrijheids <rhyme label="b" type="m" corresp="#s7:l3">Zoon</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dansons la <rhyme label="c" type="f" corresp="#s7:l5">Carmagnole</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s7:l6">Canon</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Al wie opregt zijn schuld <rhyme label="a" type="m">belijdt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Word der Vrijheid <rhyme label="a" type="m" corresp="#s8:l1">toegewijd</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Neen: Vrijheid voedt geen <rhyme label="b" type="m">wraak</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Broederschap is haar <rhyme label="b" type="m" corresp="#s8:l3">vermaak</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Dansons la <rhyme label="c" type="f">Carmagnole</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">Vive le <rhyme label="d" type="m">son</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Dinsons la <rhyme label="c" type="f" corresp="#s8:l5">Carmagnole</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Vive le son du <rhyme label="d" type="m" corresp="#s8:l6">Canon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
