  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CUpido losen <rhyme label="a" type="m">Guyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Lanck volghden ick u <rhyme label="b" type="m">lot</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Maer nu vloeck ick u <rhyme label="a" type="m" corresp="#s1:l1">uyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En soeck een ander <rhyme label="b" type="m" corresp="#s1:l2">Godt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Want het vrijen baert <rhyme label="c" type="f">lijen</rhyme>:</l>
        <l id="s1:l6" met="3" type="+-">Maer Roemer en <rhyme label="b" type="m" corresp="#s1:l2">Pot</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Van my <rhyme label="c" type="f" corresp="#s1:l5">verblij'en</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Silvy mijn lief eerst <rhyme label="a" type="m">was</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">Maer nu bey ic het <rhyme label="b" type="m">vat</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En troetelt het <rhyme label="a" type="m" corresp="#s2:l1">Gelas</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En cus het Bacchi <rhyme label="b" type="m" corresp="#s2:l2">nat</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Haere saecken <rhyme label="c" type="f">vermaeken</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Maer ic segh u <rhyme label="b" type="m" corresp="#s2:l2">dat</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">T'is beter <rhyme label="c" type="f" corresp="#s2:l5">smaecken</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Weg blincen Godt vlucht <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Naer t'hol, daer Venis <rhyme label="b" type="m">zit</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En groet u Moeders <rhyme label="a" type="m" corresp="#s3:l1">Man</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Den <rhyme label="b" type="m" corresp="#s3:l2">Hoeren-rijcken-smidt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Minne dreven, ick <rhyme label="c" type="f">geve</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Aen t'gelas, en de <rhyme label="b" type="m" corresp="#s3:l2">can</rhyme>;</l>
        <l id="s3:l7" met="2" type="-+">Ach sachte <rhyme label="c" type="f" corresp="#s3:l5">leven</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">K'heb lanck genoech <rhyme label="a" type="m">geslaeft</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn noodt aen een <rhyme label="b" type="m">geklacht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Mijn klacht noyt tot me <rhyme label="a" type="m" corresp="#s4:l1">doocht</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Haer hoe ic sprack <rhyme label="b" type="m" corresp="#s4:l2">bewoch</rhyme></l>
        <l id="s4:l5" met="3" type="-+">k'Wil dan leven ende <rhyme label="c" type="f">sweven</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Naer mijn eygen <rhyme label="b" type="m" corresp="#s4:l2">lust</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Ende altijt zijn <rhyme label="c" type="f" corresp="#s4:l5">gerust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
