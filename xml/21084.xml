        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Engelse fortuin</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">GEluck, geluck, wy wenschen u te <rhyme label="a" type="m">saem</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Geluck in desen Staet seer <rhyme label="a" type="m" corresp="#s1:l1">aengenaem</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Bruyd'gom geluck, geluck vrou-Bruyt <rhyme label="b" type="m">eerbaer</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Veel veel gelucx, geluck van jaer tot <rhyme label="b" type="m" corresp="#s1:l3">jaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Geluck op 't eerste, tweed' en derde <rhyme label="a" type="m">bodt</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Geluckicheydt zy uwer beyde <rhyme label="a" type="m" corresp="#s2:l1">lot</rhyme>:</l>
        <l id="s2:l3" met="5" type="-+">Geluck als ghy gaet trouwen <rhyme label="b" type="m">openbaer</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Veel veel gelucx, geluck van jaer tot <rhyme label="b" type="m" corresp="#s2:l3">jaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Geluck wanneer ghy spannet <rhyme label="a" type="m">handen-trou</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Ter vaster liefd' geluck en geen <rhyme label="a" type="m" corresp="#s3:l1">berou</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Geluck en vrede soet u voegh te <rhyme label="b" type="m">gaer</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Veel veel gelucx, geluck van jaer tot <rhyme label="b" type="m" corresp="#s3:l3">jaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Geluck tot uwen arbeydt vroegh en <rhyme label="a" type="m">spaed'</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Geluck en heyl dan als ghy rusten <rhyme label="a" type="m" corresp="#s4:l1">gaet</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Geluck in al u doen sonder <rhyme label="b" type="m">gevaer</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Veel veel geluck, geluck van jaer tot <rhyme label="b" type="m" corresp="#s4:l3">jaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Princen, 't geluck komt van den Heere <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Hy geeft geluck die sijne wille <rhyme label="a" type="m" corresp="#s5:l1">doet</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Geluck als komt het eerste van dit <rhyme label="b" type="m">paer</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">En die daer na geluck in 't oude <rhyme label="b" type="m" corresp="#s5:l3">jaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
