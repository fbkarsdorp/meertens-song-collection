  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Nu zal ik <rhyme label="a" type="f">schryve</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Wyl ik moet <rhyme label="a" type="f" corresp="#s1:l1">blyve</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Aan wal in <rhyme label="b" type="m">eenzaamheit</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Ik moest gaan <rhyme label="c" type="f">trouwen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Als andere <rhyme label="c" type="f" corresp="#s1:l4">vrouwen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Maar ik ben myn fiellis <rhyme label="b" type="m" corresp="#s1:l3">kwyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Myn man is gaan varen na <rhyme label="d" type="m">zee</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Myn man is gaan vaaren na <rhyme label="d" type="m" corresp="#s1:l7">zee</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">zee ja zee ja zee der om d zee der om <rhyme label="d" type="m" corresp="#s1:l7">d</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Wie wil der nouw met hem <rhyme label="d" type="m" corresp="#s1:l7">mee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Nu kan ik <rhyme label="a" type="f">braveren</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Met al die <rhyme label="a" type="f" corresp="#s2:l1">heere</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die myn maar gunstig <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Om daar mee te <rhyme label="c" type="f">zwieren</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Als andere <rhyme label="c" type="f" corresp="#s2:l4">dieren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En denk ik ben nu <rhyme label="b" type="m" corresp="#s2:l3">allyn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Myn man is gaan varen na <rhyme label="d" type="m">zee</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Myn man is gaan varen na <rhyme label="d" type="m" corresp="#s2:l7">zee</rhyme>,</l>
        <l id="s2:l9" met="5" type="+-">zee ja zee ja zee der om d zee der om <rhyme label="d" type="m" corresp="#s2:l7">d</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Wie duyvel vaart met hem <rhyme label="d" type="m" corresp="#s2:l7">mee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Moet hy nouw <rhyme label="a" type="f">vegten</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Als dappere <rhyme label="a" type="f" corresp="#s3:l1">knegten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daar ben ik mee te <rhyme label="b" type="m">vree</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Al moet hy <rhyme label="c" type="f">klimme</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Na boove <rhyme label="c" type="f" corresp="#s3:l4">toespringe</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ik denk ik ben op goe <rhyme label="b" type="m" corresp="#s3:l3">ree</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Myn man is gaan varen na <rhyme label="d" type="m">zee</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Myn man is gaan varen na <rhyme label="d" type="m" corresp="#s3:l7">zee</rhyme>,</l>
        <l id="s3:l9" met="5" type="+-">zee ja zeeja zee der om d zee der om <rhyme label="d" type="m" corresp="#s3:l7">d</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Ik stuur hem een mantje <rhyme label="d" type="m" corresp="#s3:l7">mee</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Oorlof gy <rhyme label="a" type="f">vrouwe</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Laat u niet <rhyme label="a" type="f" corresp="#s4:l1">rouwe</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Raakt gy zoo u mannetjes <rhyme label="b" type="m">kwyt</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Wilt u <rhyme label="c" type="f">vermaaken</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Met andere <rhyme label="c" type="f" corresp="#s4:l4">snaaken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">zoo raakt gy uyt die <rhyme label="b" type="m" corresp="#s4:l3">spyd</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En denkt hy moest na <rhyme label="d" type="m">zee</rhyme>:</l>
        <l id="s4:l8" met="3" type="-+">Myn man is gaan varen na <rhyme label="d" type="m" corresp="#s4:l7">zee</rhyme>,</l>
        <l id="s4:l9" met="5" type="+-">zee ja zee ja zee der om d zee der om <rhyme label="d" type="m" corresp="#s4:l7">d</rhyme></l>
        <l id="s4:l10" met="3" type="-+">En stel u daar mee te <rhyme label="d" type="m" corresp="#s4:l7">vree</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
