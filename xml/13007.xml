    <melody>Hier buiten in de Houte</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TE Haarlem in den <rhyme label="a" type="f">Houte</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Keert de Molenaar <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Daar woont een Meisken <rhyme label="a" type="f" corresp="#s1:l1">stoute</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Om en om, en <rhyme label="b" type="m" corresp="#s1:l2">wederom</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En keert de Molenaar om, <rhyme label="c" type="m">Poffa</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dat hoorden een der <rhyme label="a" type="f">Knapen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Keert de Molenaar <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die woud by 't Meisken <rhyme label="a" type="f" corresp="#s2:l1">slapen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Om en om en <rhyme label="b" type="m" corresp="#s2:l2">wederom</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">En keert de Molenaar om, <rhyme label="c" type="m">Poffa</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dat mijn Moeder dat <rhyme label="a" type="f">wiste</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Keert de Molenaar <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zy sloot my in een <rhyme label="a" type="f" corresp="#s3:l1">kiste</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Om en om en <rhyme label="b" type="m" corresp="#s3:l2">wederom</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En keert de Molenaar om, <rhyme label="c" type="m">Poffa</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men zal u Moeder schenken <rhyme label="a" type="f">wijn</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Keert de Molenaar <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zy zal zoo wel te vreden <rhyme label="a" type="f" corresp="#s4:l1">zijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Om en om en <rhyme label="b" type="m" corresp="#s4:l2">wederom</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En keert de Molenaar om, <rhyme label="c" type="m">Poffa</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
