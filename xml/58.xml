<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>58</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>lamentation</tag>
    </tags>
    <melody>Het vinnige stralen van de zon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DAer gaat de koele Vrijer <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Daiphilo; kan 't <rhyme label="b" type="f">geschieden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Daiphilo ach! 't is tegens <rhyme label="a" type="m" corresp="#s1:l1">reen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dus Dorilee te <rhyme label="b" type="f" corresp="#s1:l2">vlieden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Is Daiphilo, dus los, dus <rhyme label="a" type="m">licht</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dus wulps en <rhyme label="b" type="f">ongebonden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dus geyl, dat hy op trouw noch <rhyme label="a" type="m" corresp="#s2:l1">plicht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Sijn sinnen kluys wil <rhyme label="b" type="f" corresp="#s2:l2">gronden</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghedenckt ghy Daiphilo niet <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wat trouw ghy hebt <rhyme label="b" type="f">ghesworen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">In 't suyer bos, en sprack veel <rhyme label="a" type="m" corresp="#s3:l1">eer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wenst ik my <rhyme label="b" type="f" corresp="#s3:l2">ongebooren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Eer ick mijn trouwe liefd' <rhyme label="a" type="m">verstoot</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ik sal u soo langh <rhyme label="b" type="f">lieven</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Tot dat de al verslind'bre <rhyme label="a" type="m" corresp="#s4:l1">dood</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ons sieltjes sal door <rhyme label="b" type="f" corresp="#s4:l2">grieven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Nimph Dorilee was u <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Het doelwit van u <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">De heelster van u pijn en <rhyme label="a" type="m" corresp="#s5:l1">smert</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De bron-aar van u <rhyme label="b" type="f" corresp="#s5:l2">minne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De scepter-swayster van u <rhyme label="a" type="m">siel</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">U hoogste vreughd ter <rhyme label="b" type="f">werelt</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Maer ach Fortuna 's wanckligh <rhyme label="a" type="m" corresp="#s6:l1">wiel</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Keert om nu 't blanck <rhyme label="b" type="f" corresp="#s6:l2">ghepeerelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Van d' hoofsche staat hem speelt in 't <rhyme label="a" type="m">hooft</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">'t Is Granida. (wiens <rhyme label="b" type="f">hulsel</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">De glans van sijn beloft <rhyme label="a" type="m" corresp="#s7:l1">verdooft</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Door 't schitterent <rhyme label="b" type="f" corresp="#s7:l2">vergulsel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ha Daiphilo, kan d' hoofsche <rhyme label="a" type="m">pracht</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">U sin soo licht <rhyme label="b" type="f">verovren</rhyme>?</l>
        <l id="s8:l3" met="4" type="-+">Sie toe, sie toe, van 't hof u <rhyme label="a" type="m" corresp="#s8:l1">wacht</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En laet u niet <rhyme label="b" type="f" corresp="#s8:l2">betovren</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Door opgepronckte schoone <rhyme label="a" type="m">schijn</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Doch eenmael onder <rhyme label="b" type="f">vonden</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Gevoeltmen eerst het scherp <rhyme label="a" type="m" corresp="#s9:l1">fenijn</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">'t Geen schuylt in d' hoofsche <rhyme label="b" type="f" corresp="#s9:l2">gronden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dewijl 't u nochtans soo <rhyme label="a" type="m">behaeght</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Gaet heen, Ã´ jonge <rhyme label="b" type="f">hooflingh</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Maar siet dat ghy daer na niet <rhyme label="a" type="m" corresp="#s10:l1">klaeght</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Te sijn een <rhyme label="b" type="f" corresp="#s10:l2">out-verschooflingh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
