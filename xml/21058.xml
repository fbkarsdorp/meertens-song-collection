<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21058</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Baise-moi ma Jeanneton</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VEel geluck, luckzaligh <rhyme label="a" type="m">paer</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Wenschen wy u <rhyme label="b" type="f">heden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die uyt liefde met <rhyme label="a" type="m" corresp="#s1:l1">malkaer</rhyme></l>
        <l id="s1:l4" met="3" type="+-">t' Samen zijt <rhyme label="b" type="f" corresp="#s1:l2">getreden</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Inden Heyl'gen <rhyme label="c" type="m">Echten-staet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Die te recht te boven <rhyme label="c" type="m" corresp="#s1:l5">gaet</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Al dat in dit <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Van ons werdt <rhyme label="d" type="f" corresp="#s1:l7">bedreven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wie kan dese lieve <rhyme label="a" type="m">vreught</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Loven na haer <rhyme label="b" type="f">waerde</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Want, daer is in eer en <rhyme label="a" type="m" corresp="#s2:l1">deught</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Waerlijck op ter <rhyme label="b" type="f" corresp="#s2:l2">aerde</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Niet soo loff'lijck, niet soo <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Niet soo noodigh, nut, noch <rhyme label="c" type="m" corresp="#s2:l5">goet</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Als het saligh <rhyme label="d" type="f">trouwen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En het heyligh <rhyme label="d" type="f" corresp="#s2:l7">houwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Houd u nu als Man en <rhyme label="a" type="m">Vrou</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Na Gods <rhyme label="b" type="f">welbehagen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wilt het soet en suer <rhyme label="a" type="m" corresp="#s3:l1">ghetrou</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Met malkander <rhyme label="b" type="f" corresp="#s3:l2">draghen</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Soeckt in alle dingh voor <rhyme label="c" type="m">al</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Hoe elck aer behagen <rhyme label="c" type="m" corresp="#s3:l5">sal</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Gelijck in <rhyme label="d" type="f">voor-tijden</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Doen d'een d'ander <rhyme label="d" type="f" corresp="#s3:l7">vrijde</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laet u soet lieve <rhyme label="a" type="m">min</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Nimmermeer vermind' <rhyme label="b" type="f">ren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Sijt t' saem eens niet hert en <rhyme label="a" type="m" corresp="#s4:l1">sin</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Datje sonder hind' <rhyme label="b" type="f" corresp="#s4:l2">ren</rhyme></l>
        <l id="s4:l5" met="4" type="+-">(Als de Lauwerieren <rhyme label="c" type="m">doen</rhyme>)</l>
        <l id="s4:l6" met="4" type="+-">'s Winters Somers even <rhyme label="c" type="m" corresp="#s4:l5">groen</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Meught in liefde <rhyme label="d" type="f">bloeyen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">En in deughden <rhyme label="d" type="f" corresp="#s4:l7">groeyen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Siet de Krayen man en <rhyme label="a" type="m">vrou</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Een goet voorschrift <rhyme label="b" type="f">schrijven</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Want sy tot der doot toe <rhyme label="a" type="m" corresp="#s5:l1">trou</rhyme></l>
        <l id="s5:l4" met="3" type="+-">By malkander <rhyme label="b" type="f" corresp="#s5:l2">blijven</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">En als 't Wijfken d'Eyers <rhyme label="c" type="m">broedt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">'t Manneken het vroutje <rhyme label="c" type="m" corresp="#s5:l5">voedt</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Daer aen meughie <rhyme label="d" type="f">leeren</rhyme></l>
        <l id="s5:l8" met="3" type="+-">'t Huys wel te <rhyme label="d" type="f" corresp="#s5:l7">regeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Prince Bruydegom en <rhyme label="a" type="m">Bruydt</rhyme></l>
        <l id="s6:l2" met="3" type="+-">d'Opper-Heer der <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wenschen wy, tot een <rhyme label="a" type="m" corresp="#s6:l1">besluyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Wil t t'saem <rhyme label="b" type="f" corresp="#s6:l2">vereeren</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Al het heyl, en luck, en <rhyme label="c" type="m">vreught</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Die ghy selver wenschen <rhyme label="c" type="m" corresp="#s6:l5">meught</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">En hier na te <rhyme label="d" type="f">samen</rhyme></l>
        <l id="s6:l8" met="3" type="+-">'t Eeuwigh leven, <rhyme label="d" type="f" corresp="#s6:l7">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
