  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Schoen lief doer u <rhyme label="a" type="f">genaeden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">U dienaer greasy <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">troest mijn eert valt te <rhyme label="a" type="f" corresp="#s1:l1">spaede</rhyme></l>
        <l id="s1:l4" met="3" type="-+">o reyn maechdelinck <rhyme label="b" type="m" corresp="#s1:l2">soet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">eer Ick onder de <rhyme label="b" type="m" corresp="#s1:l2">voet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Lech doer den druck <rhyme label="c" type="f">voortreeden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">en voerteer vleis en <rhyme label="b" type="m" corresp="#s1:l2">bloet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">omt deruen u schoen <rhyme label="c" type="f" corresp="#s1:l6">heeden</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als schier voer <rhyme label="a" type="f">wonnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Leef ick heel <rhyme label="b" type="m">deesoelaet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">In liefde vast <rhyme label="a" type="f" corresp="#s2:l1">gebonnen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van U reyn maechdelick <rhyme label="b" type="m" corresp="#s2:l2">saet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Komt geeft u dienaer <rhyme label="b" type="m" corresp="#s2:l2">vast</rhyme></l>
        <l id="s2:l6" met="3" type="-+">eer ick in druck <rhyme label="c" type="f">voerdwijne</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Want gij lief <rhyme label="b" type="m" corresp="#s2:l2">deelijkaet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">sijt mijn troest <rhyme label="c" type="f" corresp="#s2:l6">meedeesijne</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Want u schoenheyt seer <rhyme label="a" type="f">schoene</rhyme></l>
        <l id="s3:l2" met="3" type="-+">heeft mijn Jonck hert <rhyme label="b" type="m">duerwont</rhyme></l>
        <l id="s3:l3" met="3" type="-+">u ooghens bruin ten <rhyme label="a" type="f" corresp="#s3:l1">toene</rhyme></l>
        <l id="s3:l4" met="3" type="-+">en u lachgende <rhyme label="b" type="m" corresp="#s3:l2">mont</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Daer toe u borsies <rhyme label="b" type="m" corresp="#s3:l2">ront</rhyme></l>
        <l id="s3:l6" met="3" type="-+">U lipies als <rhyme label="c" type="f">koraele</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Troest mijn In korter <rhyme label="b" type="m" corresp="#s3:l2">stont</rhyme></l>
        <l id="s3:l8" met="3" type="-+">en geeneest toch mijn <rhyme label="c" type="f" corresp="#s3:l6">quale</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gij sijt voermaert met <rhyme label="a" type="f">deuchden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Oock als paelas seer <rhyme label="b" type="m">wijs</rhyme></l>
        <l id="s4:l3" met="3" type="-+">hij singt tis mijn een <rhyme label="a" type="f" corresp="#s4:l1">vruechden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Als den muisas <rhyme label="b" type="m" corresp="#s4:l2">propijs</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die daer singen om <rhyme label="b" type="m" corresp="#s4:l2">prijs</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In Leascreas <rhyme label="c" type="f">vaeleye</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Daer sij tot haeren <rhyme label="b" type="m" corresp="#s4:l2">spijs</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Haer schaepkens goet gean <rhyme label="c" type="f" corresp="#s4:l6">wijen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wanneer Ick dit <rhyme label="a" type="f">aenmerke</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Peins ick met swaer <rhyme label="b" type="m">voerdriet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">M.cht ick sin in den <rhyme label="a" type="f" corresp="#s5:l1">perken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">met dien Ick mijn lief <rhyme label="b" type="m" corresp="#s5:l2">hiet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Soo sou alle druck <rhyme label="b" type="m" corresp="#s5:l2">siet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">van mijn neemen een <rhyme label="c" type="f">keere</rhyme></l>
        <l id="s5:l7" met="3" type="-+">en sou tot vruechts <rhyme label="b" type="m" corresp="#s5:l2">geniet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">met U lief <rhyme label="c" type="f" corresp="#s5:l6">trijonfeere</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Oorlof hier mee <rhyme label="a" type="f">prinsesse</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dien ick alleen <rhyme label="b" type="m">bemin</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ick bidt u mijn <rhyme label="a" type="f" corresp="#s6:l1">maetresse</rhyme></l>
        <l id="s6:l4" met="3" type="-+">verandert toch van <rhyme label="b" type="m" corresp="#s6:l2">sin</rhyme></l>
        <l id="s6:l5" met="3" type="-+">opdat Ick eene <rhyme label="b" type="m" corresp="#s6:l2">gewin</rhyme></l>
        <l id="s6:l6" met="3" type="-+">u lief teeniger <rhyme label="c" type="f">ueren</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ist dat Ick geen troest <rhyme label="b" type="m" corresp="#s6:l2">vin</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Den doot sal Ick <rhyme label="c" type="f" corresp="#s6:l6">besueren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
