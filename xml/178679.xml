    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 075 Datheen</melody>
  <author>François Halma</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ONze Godt, wy roemen <rhyme label="a" type="m">bly</rhyme></l>
        <l id="s1:l2" met="4" type="+-">U, en uwen grooten <rhyme label="b" type="m">naam</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat gy dien aan ons <rhyme label="b" type="m" corresp="#s1:l2">bequaam</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Opent; dies verteltmen <rhyme label="a" type="m" corresp="#s1:l1">vry</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Uwe wonderen vol <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Door uw sterke handt <rhyme label="c" type="m" corresp="#s1:l5">volbragt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als ik kryge 't geen <rhyme label="a" type="m">bestemt</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Voor my is, dan zal ik <rhyme label="b" type="m">recht</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Richten, als uw trouwe <rhyme label="b" type="m" corresp="#s2:l2">knecht</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">'t Landt versmolt, door rou <rhyme label="a" type="m" corresp="#s2:l1">beklemt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Met d'inwoners, maar door <rhyme label="c" type="m">my</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Staan zyn pylers op een <rhyme label="c" type="m" corresp="#s2:l5">ry</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Zei d'onzinnigen, weest <rhyme label="a" type="m">toch</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Niet onzinnig, door den <rhyme label="b" type="m">toorn</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">'k Riep, verhoog niet meer den <rhyme label="b" type="m" corresp="#s3:l2">hoorn</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Godtloos volk, en mydt <rhyme label="a" type="m" corresp="#s3:l1">bedrog</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Spreek niet met een' styven <rhyme label="c" type="m">hals</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Woorden reukeloos en <rhyme label="c" type="m" corresp="#s3:l5">vals</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Want verhoginge van <rhyme label="a" type="m">staat</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Komt niet uit het Oost of <rhyme label="b" type="m">West</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Noch de woesteny; maar <rhyme label="b" type="m" corresp="#s4:l2">vest</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Zich daar Godt als richter <rhyme label="a" type="m" corresp="#s4:l1">gaat</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Hy vernedert deeze en <rhyme label="c" type="m">geen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En verhoogtze in 't stof <rhyme label="c" type="m" corresp="#s4:l5">vertreên</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Want men ziet in 's Heeren <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Eenen beker, daar de <rhyme label="b" type="m">wyn</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Van beroert is, roodt in <rhyme label="b" type="m" corresp="#s5:l2">schyn</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En hy schenkt daar uit in 't <rhyme label="a" type="m" corresp="#s5:l1">landt</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Doch de droessem wordt <rhyme label="c" type="m">geleegt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Van al die godtloosheit <rhyme label="c" type="m" corresp="#s5:l5">pleegt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maar ik zal in <rhyme label="a" type="m">eeuwigheit</rhyme></l>
        <l id="s6:l2" met="4" type="+-">'t Heil verkondigen; ik <rhyme label="b" type="m">zal</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Jakobs Godt met bly <rhyme label="b" type="m" corresp="#s6:l2">geschal</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Zingen, als hiertoe <rhyme label="a" type="m" corresp="#s6:l1">bereidt</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">'k Zal de goddeloozen <rhyme label="c" type="m">slaan</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En voor 't heil der vroomen <rhyme label="c" type="m" corresp="#s6:l5">staan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
