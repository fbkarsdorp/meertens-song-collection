<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>173915</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags>
      <tag standard-name="politiek lied" translation="political song">politiek lied</tag>
      <tag standard-name="vaderlands lied" translation="patriotic song">vaderlands lied</tag>
    </tags>
    <melody>Pantalone door kunst verheven</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Vryheids Zoonen! Eedele <rhyme label="a" type="f">Braven</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Zuylen van ons <rhyme label="b" type="m">Vaderland</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Wars van slegte en vuyge <rhyme label="a" type="f" corresp="#s1:l1">Slaven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Vryheids liefst en dierbaarst <rhyme label="b" type="m" corresp="#s1:l2">pand</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Door ons blyft de Vryheid <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Aan het Wimpelvoerend <rhyme label="d" type="m">Y</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Door ons zal den tweedragt <rhyme label="c" type="f" corresp="#s1:l5">sneven</rhyme>;</l>
        <l id="s1:l8" met="4" type="+-">Voor de braven <rhyme label="d" type="m" corresp="#s1:l6">Schuttery</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dwangzugt zal ons nooit <rhyme label="a" type="f">vervoeren</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Eendragt zy ons lust en <rhyme label="b" type="m">vreugd</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Laat den tweedragt op ons <rhyme label="a" type="f" corresp="#s2:l1">loeren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'t Schilt van ons, is blanken <rhyme label="b" type="m" corresp="#s2:l2">deugd</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Vryheid is ons aller <rhyme label="c" type="f">moeder</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Door haar zyn wy eeuwig <rhyme label="d" type="m">vry</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Wy, wy zyn haer ook ten <rhyme label="c" type="f" corresp="#s2:l5">hoeder</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Voor de brave <rhyme label="d" type="m" corresp="#s2:l6">Burgery</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat gewelt ons regt <rhyme label="a" type="f">betwisten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Vryheid zy ons steeds ten <rhyme label="b" type="m">spoor</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Aan bedrog of snooden <rhyme label="a" type="f" corresp="#s3:l1">listen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Geeft geen Patriot <rhyme label="b" type="m" corresp="#s3:l2">gehoor</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Maar voor eygen regt te <rhyme label="c" type="f">waken</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Maakt den Burger vry en <rhyme label="d" type="m">bly</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Waapenen zyn de beste <rhyme label="c" type="f" corresp="#s3:l5">zaaken</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Voor de brave <rhyme label="d" type="m" corresp="#s3:l6">Schuttery</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
