<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3994</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>mystical song</tag>
      <tag>praise</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Ik minde eens konings zoon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IN mi seluen so wil ic <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ende Iesum leeren <rhyme label="b" type="f">minnen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Hi is die schoonste bouen <rhyme label="c" type="m">al</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hi staet in mine <rhyme label="b" type="f" corresp="#s1:l2">sinnen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">So wie Iesus minne <rhyme label="a" type="m">draecht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die mach wel vrolijck <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Hi is een roose wide <rhyme label="c" type="m">ontdaen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Voer alle die hem aen <rhyme label="b" type="f" corresp="#s2:l2">cleuen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hi is een roode roose <rhyme label="a" type="m">root</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ende minnelijc int <rhyme label="b" type="f">aenscouwen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Hi is mijn lief mijn <rhyme label="c" type="m">minnekijn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hem wil ick sijn <rhyme label="b" type="f" corresp="#s3:l2">ghetrouwe</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hi is die schoonste die ic <rhyme label="a" type="m">weet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Waer vintmen sijns <rhyme label="b" type="f">ghelijcke</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Willen wi ons herte tot hem <rhyme label="c" type="m">keeren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hi sal ons maken <rhyme label="b" type="f" corresp="#s4:l2">rijcke</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hi is een heere bouen <rhyme label="a" type="m">al</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van hemel ende van <rhyme label="b" type="f">eerden</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Willen wi ons seluen wte <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hi sal ons wel <rhyme label="b" type="f" corresp="#s5:l2">gheweerden</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O Iesu alder liefste <rhyme label="a" type="m">heer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mocht ic ter minnen <rhyme label="b" type="f">comen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Ende mijn nature steruen <rhyme label="c" type="m">leeren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Het soude mi vele <rhyme label="b" type="f" corresp="#s6:l2">vromen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sy moet nochtans verwonnen <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Al sout haer grote pine <rhyme label="b" type="f">sijn</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Sal si comen in bliscap <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Daer die Cherubinnen <rhyme label="b" type="f" corresp="#s7:l2">sijn</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Iesus is een <rhyme label="a" type="m">fonteyne</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Wel hem die daer wt <rhyme label="b" type="f">drincken</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Hi is so suyuer ende so <rhyme label="c" type="m">reyne</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Een edel wijngaert <rhyme label="b" type="f" corresp="#s8:l2">rancke</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hier om so wil ic blidelijc <rhyme label="a" type="m">liden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Ten wort hier mer een corte <rhyme label="b" type="f">tijt</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Als ic come bi mijn soete <rhyme label="c" type="m">lief</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Daer sal ic driuen groot <rhyme label="b" type="f" corresp="#s9:l2">iolijt</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Die bliscap is daer so <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die eewelijck ghedueren <rhyme label="b" type="f">sal</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Hier om laet ons de werelt <rhyme label="c" type="m">versmaen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Ende God minnen bouen <rhyme label="b" type="f" corresp="#s10:l2">al</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Die dit lieken heeft <rhyme label="a" type="m">ghemaect</rhyme></l>
        <l id="s11:l2" met="3" type="-+">God wilse in duechden <rhyme label="b" type="f">sparen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Ende brengense na dese corte <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Al in der enghelen <rhyme label="b" type="f" corresp="#s11:l2">scharen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
