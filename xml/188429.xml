  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Roosje, waerdig om te <rhyme label="a" type="f">minnen</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Roosje, bloem der <rhyme label="a" type="f" corresp="#s1:l1">Herderinnen</rhyme>!</l>
        <l id="s1:l3" met="2" type="+-">Waer gaet <rhyme label="b" type="m">gy</rhyme>?</l>
        <l id="s1:l4" met="2" type="+-">Hoe haet <rhyme label="b" type="m" corresp="#s1:l3">gy</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Die geene die u liefde <rhyme label="c" type="m">draegt</rhyme>?</l>
        <l id="s1:l6" met="4" type="-+">Dat zy de liefde zelf <rhyme label="c" type="m" corresp="#s1:l5">geklaegt</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">Ik zie frisch ontloken <rhyme label="d" type="f">rozen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Op haer telgen lieflyk <rhyme label="d" type="f" corresp="#s1:l7">blozen</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">'k Zie geurge <rhyme label="e" type="f">Violieren</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Het klaverveld <rhyme label="e" type="f" corresp="#s1:l9">vercieren</rhyme>:</l>
        <l id="s1:l11" met="4" type="+-">Maer ach! o bitter hart <rhyme label="f" type="m">verdriet</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">Ik zie myn schoone Roosje <rhyme label="f" type="m" corresp="#s1:l11">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
