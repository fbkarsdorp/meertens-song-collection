    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag>mythological song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Te nacht seer sacht lach (ik) In <rhyme label="a" type="f">fantoesij</rhyme></l>
        <l id="s1:l2" met="5" type="-+">genuecht en vruecht begonst mijn geest te <rhyme label="a" type="f" corresp="#s1:l1">bestridden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Den heemel ront sach Ick <rhyme label="b" type="m">terstont</rhyme></l>
        <l id="s1:l4" met="5" type="-+">verfult met groet lust aen allen <rhyme label="a" type="f" corresp="#s1:l1">sijden</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Doen Ick soe dick docht om die <rhyme label="a" type="f">goddinne</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Mijn hert van smert verlichte seer van <rhyme label="a" type="f" corresp="#s2:l1">binnen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dat doer aensien van schoene <rhyme label="b" type="m">lien</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Die daer voergaedert waeren konstich van <rhyme label="a" type="f" corresp="#s2:l1">sinnen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Met macht geslacht der goden waer daer <rhyme label="a" type="f">tsaemen</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Den wijn seer fin schonk men naert <rhyme label="a" type="f" corresp="#s3:l1">betaemen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">men schanck men dranck men sanck men <rhyme label="b" type="m">spranck</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Elck brocht voir hem goddelicke <rhyme label="a" type="f" corresp="#s3:l1">naemen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Zeer hooch int oech sat Iupijter Inden <rhyme label="a" type="f">troen</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Sijn wif aent lif was Junoes tros en <rhyme label="a" type="f" corresp="#s4:l1">schoene</rhyme></l>
        <l id="s4:l3" met="4" type="-+">beneffens haer sadt aldaer venes <rhyme label="b" type="m">klaer</rhyme></l>
        <l id="s4:l4" met="5" type="-+">verdeelt met haer soen Kuibydi, <rhyme label="a" type="f" corresp="#s4:l1">Idoen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Van daer aen roeren die <rhyme label="a" type="f">Instrumenten</rhyme>  ;</l>
        <l id="s5:l2" met="5" type="-+">Tgeklanck dat spranck doort tseemels <rhyme label="a" type="f" corresp="#s5:l1">fiermaementen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">pallaen die maecht sanck <rhyme label="b" type="m">onversaecht</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Jo Jo Jo tot een <rhyme label="a" type="f" corresp="#s5:l1">present</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Een hoet seer soet wart daer op <rhyme label="a" type="f">gehangen</rhyme></l>
        <l id="s6:l2" met="5" type="-+">wie int lest liet blicken sin <rhyme label="a" type="f" corresp="#s6:l1">sangen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">reyn hermeny vroelick en <rhyme label="b" type="m">blij</rhyme></l>
        <l id="s6:l4" met="5" type="-+">liet elck persoen daer klincken eer Iet <rhyme label="a" type="f" corresp="#s6:l1">lanck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Die vloer vant koer daer Ickse op (sach) <rhyme label="a" type="f">dansen</rhyme></l>
        <l id="s7:l2" met="5" type="-+">die was seer ras bestroeit met roese <rhyme label="a" type="f" corresp="#s7:l1">kranse</rhyme></l>
        <l id="s7:l3" met="4" type="-+">elck hielt een tret seer konstich en <rhyme label="b" type="m">net</rhyme></l>
        <l id="s7:l4" met="5" type="-+">ter weerelt noeit meerder <rhyme label="a" type="f" corresp="#s7:l1">plasansy</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Den pris prins wis raet wie heeft <rhyme label="a" type="f">gekregen</rhyme></l>
        <l id="s8:l2" met="5" type="-+">mijn lief tgerief ten dient hier niet <rhyme label="a" type="f" corresp="#s8:l1">gesweegen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Want sij begost dat niemant en <rhyme label="b" type="m">kost</rhyme></l>
        <l id="s8:l4" met="5" type="-+">te singen en sij voleindet fraey te <rhyme label="a" type="f" corresp="#s8:l1">deegen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
