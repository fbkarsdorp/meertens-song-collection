<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188300</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De Savoyaardse meisjes</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IN 't lommer van deez' <rhyme label="a" type="f">Elzen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Alwaar myn Filis <rhyme label="b" type="m">rust</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Mag ik haar schoon <rhyme label="a" type="f" corresp="#s1:l1">omhelzen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En boeten mynen <rhyme label="b" type="m" corresp="#s1:l2">lust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haar net geschaarde <rhyme label="a" type="f">boogjes</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Verrukken myn <rhyme label="b" type="m">gemoed</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Terwyl zy met haar <rhyme label="a" type="f" corresp="#s2:l1">Oogjes</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ontsteekt myn <rhyme label="b" type="m" corresp="#s2:l2">Minnegloed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik zoog van haare <rhyme label="a" type="f">Lipjes</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Het Hemels <rhyme label="b" type="m">Ambrozyn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En stranden op haar <rhyme label="a" type="f" corresp="#s3:l1">klipjes</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daar duizend Roosjes <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Myn Liefje wierd niet <rhyme label="a" type="f">wakker</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Terwyl ik my <rhyme label="b" type="m">vergast</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Toen sloop ik in den <rhyme label="a" type="f" corresp="#s4:l1">Akker</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daar 't Rooze knopje <rhyme label="b" type="m" corresp="#s4:l2">wast</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daar stooide ik <rhyme label="a" type="f">onverdrooten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Tot dat zy wakker <rhyme label="b" type="m">wert</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Zy sprak ik ben <rhyme label="a" type="f" corresp="#s5:l1">geschooten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Getroffen aan myn <rhyme label="b" type="m" corresp="#s5:l2">Hart</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Zy droomde van haar <rhyme label="a" type="f">Herder</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En riep al woelend <rhyme label="b" type="m">uit</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Og Damon, og nog <rhyme label="a" type="f" corresp="#s6:l1">verder</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Tot ons de wellust <rhyme label="b" type="m" corresp="#s6:l2">stuit</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Zy kwam 't gezigt <rhyme label="a" type="f">ontsluiten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En gaf een diepen <rhyme label="b" type="m">zugt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Toen sloop ik stil na <rhyme label="a" type="f" corresp="#s7:l1">buiten</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">En maakte geen <rhyme label="b" type="m" corresp="#s7:l2">gerugt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Myn Lief had <rhyme label="a" type="f">uitgeslaapen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En ik voldaan myn <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Het Windje deed haar <rhyme label="a" type="f" corresp="#s8:l1">gapen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Dat eerst haar had <rhyme label="b" type="m" corresp="#s8:l2">gezust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
