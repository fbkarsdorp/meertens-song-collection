<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9561</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="May song">meilied</tag>
    </tags>
    <melody>Laura zat laatst bij de beek</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LEnte die ons we'er <rhyme label="a" type="m">ontmoet</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Met u soete <rhyme label="b" type="f">Somer-krachten</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die de Winter van ons <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Met haer lange koude <rhyme label="b" type="f" corresp="#s1:l2">nachten</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Och! hoe spee-len mijn <rhyme label="b" type="f" corresp="#s1:l2">ghedach-ten</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">Met inwendigh bly <rhyme label="c" type="m">geschal</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Door een heyligh vyer <rhyme label="d" type="f">ghedreven</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Op het eeuwigh saligh <rhyme label="d" type="f" corresp="#s1:l7">leven</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dat hier namaels wesen <rhyme label="c" type="m" corresp="#s1:l6">sal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Siet, hoe alle dingh <rhyme label="a" type="m">ontsluyt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat sijn wesen had' <rhyme label="b" type="f">verlooren</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Puylt nu we'er ter aerden <rhyme label="a" type="m" corresp="#s2:l1">uyt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En wordt op een nieu <rhyme label="b" type="f" corresp="#s2:l2">gebooren</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Maer veel schoonder dan te <rhyme label="b" type="f" corresp="#s2:l2">vooren</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">Daer het mede scheen <rhyme label="c" type="m">ghedaen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Siet men weder leven <rhyme label="d" type="f">krijgen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Ieuchdich uyt der aerden <rhyme label="d" type="f" corresp="#s2:l7">stijgen</rhyme></l>
        <l id="s2:l9" met="4" type="+-">En in volle lovers <rhyme label="c" type="m" corresp="#s2:l6">staen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Als ick dit wel <rhyme label="a" type="m">over-legh</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Moeten d'aerdtsche lusten <rhyme label="b" type="f">deysen</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Dat ick tot my selven <rhyme label="a" type="m" corresp="#s3:l1">segh</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Lieve, wilt het <rhyme label="b" type="f" corresp="#s3:l2">over-peysen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dat ghy eens van hier moet <rhyme label="b" type="f" corresp="#s3:l2">reysen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Onderwaerts ten grave <rhyme label="c" type="m">gaen</rhyme>.</l>
        <l id="s3:l7" met="4" type="+-">Maer al wordje <rhyme label="d" type="f">wegh-genomen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Vreest u niet, de tijdt sal <rhyme label="d" type="f" corresp="#s3:l7">komen</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Dat je weder op sult <rhyme label="c" type="m" corresp="#s3:l6">staen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">In de boomen, in het <rhyme label="a" type="m">gras</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Kan men onsen standt <rhyme label="b" type="f">bespeuren</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">'t Wordt gelijck het voortijdts <rhyme label="a" type="m" corresp="#s4:l1">was</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En het seydt ons van te <rhyme label="b" type="f" corresp="#s4:l2">veuren</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Wat na desen sal <rhyme label="b" type="f" corresp="#s4:l2">gebeuren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Wat hier namaels sal <rhyme label="c" type="m">geschien</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Als de Rechter aller <rhyme label="d" type="f">volcken</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Sal verschijnen in de <rhyme label="d" type="f" corresp="#s4:l7">wolcken</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">En sijn klaerheyt laten <rhyme label="c" type="m" corresp="#s4:l6">sien</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Beeldt u dit voor-seker <rhyme label="a" type="m">in</rhyme>;</l>
        <l id="s5:l2" met="4" type="+-">Schoon ghy zijt in 't graf <rhyme label="b" type="f">geleghen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En vergaen, des niet te <rhyme label="a" type="m" corresp="#s5:l1">min</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Ghy sult met een nieu <rhyme label="b" type="f" corresp="#s5:l2">beweghen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Van den Hemel <rhyme label="b" type="f" corresp="#s5:l2">af-gesegen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Met u waerde Vrient <rhyme label="c" type="m">gepaert</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">En volmaeckt in al de <rhyme label="d" type="f">leden</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Uyt den grave komen <rhyme label="d" type="f" corresp="#s5:l7">treden</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Schoonder dan ghy immer <rhyme label="c" type="m" corresp="#s5:l6">waert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
