<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>1697</id>
    <date>
      <year-start>1544</year-start>
      <year-end>1544</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody>Ik wil mij gaan verheugen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk wil mi gaen <rhyme label="a" type="f">verhuegen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Verblijden mijnen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ende doen na mijn <rhyme label="a" type="f" corresp="#s1:l1">vermoegen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al valtet mi <rhyme label="b" type="m" corresp="#s1:l2">teghenspoet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Vrou venus <rhyme label="b" type="m" corresp="#s1:l2">bloet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat is van dier <rhyme label="c" type="f">natueren</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Ist quaet ist <rhyme label="b" type="m" corresp="#s1:l2">goet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">die mint die moet <rhyme label="c" type="f" corresp="#s1:l6">besueren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die fiere <rhyme label="a" type="f">nachtegale</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Singt een so huebscen <rhyme label="b" type="m">sanck</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">mocht ic mijn lief <rhyme label="a" type="f" corresp="#s2:l1">betalen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Al in haer armkens <rhyme label="b" type="m" corresp="#s2:l2">blanck</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Sonder <rhyme label="b" type="m" corresp="#s2:l2">bedwanck</rhyme></l>
        <l id="s2:l6" met="3" type="-+">So soude ic met vruechden <rhyme label="c" type="f">leuen</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Alle mijn leuen <rhyme label="b" type="m" corresp="#s2:l2">lanck</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Waer alle mijn druc <rhyme label="c" type="f" corresp="#s2:l6">genesen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Reyn lelijcken wt <rhyme label="a" type="f">ghelesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">doet v <rhyme label="b" type="m">ootmoedicheyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Laet mi v dienaer <rhyme label="a" type="f" corresp="#s3:l1">wesen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Aensiet mijn <rhyme label="b" type="m" corresp="#s3:l2">bitterheyt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Weest wel <rhyme label="b" type="m" corresp="#s3:l2">bereyt</rhyme>. |</l>
        <l id="s3:l6" met="3" type="-+">Wilt mi wt minnen <rhyme label="c" type="f">ontfanghen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Doet mi <rhyme label="b" type="m" corresp="#s3:l2">bescheyt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">daer na staet mijn <rhyme label="c" type="f" corresp="#s3:l6">verlangen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Schoonder rose van <rhyme label="a" type="f">minnen</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">En heb ic noeyt <rhyme label="b" type="m">ghesien</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Si staet int herte van <rhyme label="a" type="f" corresp="#s4:l1">binnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die schoone van <rhyme label="b" type="m" corresp="#s4:l2">melodien</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Wilt tot mi <rhyme label="b" type="m" corresp="#s4:l2">vlien</rhyme></l>
        <l id="s4:l6" met="3" type="-+">wilt mi wt minnen <rhyme label="c" type="f">verwachten</rhyme></l>
        <l id="s4:l7" met="2" type="-+">doet mi <rhyme label="b" type="m" corresp="#s4:l2">bescheyt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Om v dole ick bi <rhyme label="c" type="f" corresp="#s4:l6">nachten</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O radt van <rhyme label="a" type="f">auontueren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Wildy niet omme <rhyme label="b" type="m">slaen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">mach mi geen troost <rhyme label="a" type="f" corresp="#s5:l1">gebueren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat cost mi menighen <rhyme label="b" type="m" corresp="#s5:l2">traen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Laet mi <rhyme label="b" type="m" corresp="#s5:l2">ontfaen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Reyn bloeme <rhyme label="c" type="f">ghepresen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Een troostelijc <rhyme label="b" type="m" corresp="#s5:l2">woort</rhyme></l>
        <l id="s5:l8" met="3" type="-+">So waer ic gans <rhyme label="c" type="f" corresp="#s5:l6">ghenesen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nv hoort ghi ionge <rhyme label="a" type="f">gesellen</rhyme> |</l>
        <l id="s6:l2" met="3" type="-+">Eer ghi dat minnen <rhyme label="b" type="m">begint</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Vrou venus sal v <rhyme label="a" type="f" corresp="#s6:l1">quellen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met woorden gelijc den <rhyme label="b" type="m" corresp="#s6:l2">wint</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Eer ghi <rhyme label="b" type="m" corresp="#s6:l2">beghint</rhyme></l>
        <l id="s6:l6" met="3" type="-+">So hoort na mijn <rhyme label="c" type="f">prologen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Aensiet dat <rhyme label="b" type="m" corresp="#s6:l2">eynt</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">So en wordy niet <rhyme label="c" type="f" corresp="#s6:l6">bedrogen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die dit liedeken <rhyme label="a" type="f">dichte</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Oft eerstwerf heeft <rhyme label="b" type="m">gestelt</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Vrou venus was zijn <rhyme label="a" type="f" corresp="#s7:l1">nichte</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Met haer was hi <rhyme label="b" type="m" corresp="#s7:l2">ghequelt</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Sijn vruecht <rhyme label="b" type="m" corresp="#s7:l2">versmelt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Hi leeft in grooten <rhyme label="c" type="f">trueren</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Daer om hi <rhyme label="b" type="m" corresp="#s7:l2">queelt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Si en mach hem niet <rhyme label="c" type="f" corresp="#s7:l6">gebueren</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
