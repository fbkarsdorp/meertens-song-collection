<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>114451</id>
    <date>
      <year-start>1745</year-start>
      <year-end>1745</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags>
      <tag>dialog song</tag>
      <tag>love song</tag>
    </tags>
    <melody>Ik breng u op een dronkje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAnneer zal ik u <rhyme label="a" type="f">vinde</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Myn Zielvermaek <rhyme label="a" type="f" corresp="#s1:l1">Clarinde</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Die ik zoo teer <rhyme label="b" type="m">bemin</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ach! wierd het my <rhyme label="c" type="f">gegeven</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Met u in d'Eght te <rhyme label="c" type="f" corresp="#s1:l4">Leven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">'k Boog my steets naer uw <rhyme label="b" type="m" corresp="#s1:l3">zin</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">'k Boog my steets naer uw <rhyme label="b" type="m" corresp="#s1:l3">zin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'k Zou met myn gantsche <rhyme label="a" type="f">Leven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Opregte blyken <rhyme label="a" type="f" corresp="#s2:l1">geven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat ik O! Ziels <rhyme label="b" type="m">vriendin</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">U tederlyk <rhyme label="c" type="f">Beminde</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Ey kom, myn Lief <rhyme label="c" type="f" corresp="#s2:l4">Clarinde</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gun dat ik u eens <rhyme label="b" type="m" corresp="#s2:l3">vin</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Gun dat ik u eens <rhyme label="b" type="m" corresp="#s2:l3">vin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ach! dat ik u <rhyme label="a" type="f">ontmoeten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'k Viel neder voor uw <rhyme label="a" type="f" corresp="#s3:l1">Voeten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">ô Haven myner <rhyme label="b" type="m">Jeugt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">'k Zou my met Eerbied <rhyme label="c" type="f">buigen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Myn Liefden u <rhyme label="c" type="f" corresp="#s3:l4">betuigen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Myn schat, myn Hoogste <rhyme label="b" type="m" corresp="#s3:l3">vreugd</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Myn schat, myn Hoogste <rhyme label="b" type="m" corresp="#s3:l3">vreugd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maer zagt! ik zie haer <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daer langs die groene <rhyme label="a" type="f" corresp="#s4:l1">Bomen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ik spoed my derwaerds <rhyme label="b" type="m">heen</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">'k Zal haer terstont <rhyme label="c" type="f">omhelzen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">In 't groen van deze <rhyme label="c" type="f" corresp="#s4:l4">Elzen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Met smeken en <rhyme label="b" type="m" corresp="#s4:l3">gebeen</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Met smeken en <rhyme label="b" type="m" corresp="#s4:l3">gebeen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">ô Wensch van myn <rhyme label="a" type="f">gedagten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Waer op myn zinnen <rhyme label="a" type="f" corresp="#s5:l1">wagten</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ik hiet u <rhyme label="b" type="m">Wellekom</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Myn Troost, myn <rhyme label="c" type="f">Uytverkoren</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">Waerd gy voor my <rhyme label="c" type="f" corresp="#s5:l4">geboren</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">ô Uitgelezen <rhyme label="b" type="m" corresp="#s5:l3">Blom</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">ô Uitgelezen <rhyme label="b" type="m" corresp="#s5:l3">Blom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ik zou niets meer <rhyme label="a" type="f">begeren</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">De Doot zou my nauw <rhyme label="a" type="f" corresp="#s6:l1">weren</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Van u myn Hoogste <rhyme label="b" type="m">Schat</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">De Nyt; mag met haer <rhyme label="c" type="f">Tanden</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Elk zoeken aen te <rhyme label="c" type="f" corresp="#s6:l4">randen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Op my heeft zy geen <rhyme label="b" type="m" corresp="#s6:l3">Vat</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Op my heeft zy geen <rhyme label="b" type="m" corresp="#s6:l3">Vat</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">'k Zou my gelukkig <rhyme label="a" type="f">agten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En steeds u wil <rhyme label="a" type="f" corresp="#s7:l1">betragten</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">ô Zoetste <rhyme label="b" type="m">Herderin</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">Ik zal u Eeuwig <rhyme label="c" type="f">Pryzen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En Liefde pligt <rhyme label="c" type="f" corresp="#s7:l4">bewyzen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Toond my slegs <rhyme label="b" type="m" corresp="#s7:l3">Wedermin</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Toond my slegs <rhyme label="b" type="m" corresp="#s7:l3">Wedermin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wel Hylas, uwe <rhyme label="a" type="f">Klagten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Wie zou die hier <rhyme label="a" type="f" corresp="#s8:l1">verwagten</rhyme>?</l>
        <l id="s8:l3" met="3" type="-+">'k Was hier na toe <rhyme label="b" type="m">getreen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Om my wat te <rhyme label="c" type="f">Verlusten</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En wat in 't Groen te <rhyme label="c" type="f" corresp="#s8:l4">rusten</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">In stilten hier <rhyme label="b" type="m" corresp="#s8:l3">alleen</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">In stilten hier <rhyme label="b" type="m" corresp="#s8:l3">alleen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Myn Zoetste Lief <rhyme label="a" type="f">Clarinde</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">'k Ben bly dat ik u <rhyme label="a" type="f" corresp="#s9:l1">vinde</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Myn Wensch was maer <rhyme label="b" type="m">alleen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Dat ik u mogt <rhyme label="c" type="f">ontmoeten</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Om te off'ren aen uw <rhyme label="c" type="f" corresp="#s9:l4">voeten</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Myn Smeken en <rhyme label="b" type="m" corresp="#s9:l3">gebeen</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Myn Smeken en <rhyme label="b" type="m" corresp="#s9:l3">gebeen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Laet ik uw gunst maer <rhyme label="a" type="f">Winnen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ik zal u Eeuwig <rhyme label="a" type="f" corresp="#s10:l1">Minnen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En u ten dienste <rhyme label="b" type="m">staen</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Ik zal uw Schaepjes <rhyme label="c" type="f">weyden</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En met vernoeging <rhyme label="c" type="f" corresp="#s10:l4">leyden</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">In Malsche <rhyme label="b" type="m" corresp="#s10:l3">Klaver-blaen</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">In Malsche <rhyme label="b" type="m" corresp="#s10:l3">Klaver-blaen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ik voel myn Hart <rhyme label="a" type="f">bestreden</rhyme></l>
        <l id="s11:l2" met="3" type="-+">ô Hylas! door uw <rhyme label="a" type="f" corresp="#s11:l1">beden</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Gy hebt myn Ziel <rhyme label="b" type="m">doorwondt</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Uw Min kan my <rhyme label="c" type="f">behagen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">'k Wil u myn gunst <rhyme label="c" type="f" corresp="#s11:l4">toedragen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En Minnen 't aller <rhyme label="b" type="m" corresp="#s11:l3">stondt</rhyme>.</l>
        <l id="s11:l7" met="3" type="-+">En Minnen 't aller <rhyme label="b" type="m" corresp="#s11:l3">stondt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ik wil my <rhyme label="a" type="f">overgeven</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Om Steeds met u te <rhyme label="a" type="f" corresp="#s12:l1">Leven</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Als eens gezind van <rhyme label="b" type="m">Aerd</rhyme>;</l>
        <l id="s12:l4" met="3" type="-+">Ik Wensch dat onze <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Geen droefheit mag <rhyme label="c" type="f" corresp="#s12:l4">najagen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Maer al wat vreugde <rhyme label="b" type="m" corresp="#s12:l3">baerdt</rhyme>.</l>
        <l id="s12:l7" met="3" type="-+">Maer al wat vreugde <rhyme label="b" type="m" corresp="#s12:l3">baerdt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Myn Lieve Ziel, <rhyme label="a" type="f">Clarinde</rhyme>!</l>
        <l id="s13:l2" met="3" type="-+">Wyl ik mag <rhyme label="a" type="f" corresp="#s13:l1">ondervinden</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Uw gunst en uwe <rhyme label="b" type="m">Trouw</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">'k Wensch ons zal zyn <rhyme label="c" type="f">gegeven</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Steeds eens gezind te <rhyme label="c" type="f" corresp="#s13:l4">leven</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Bevryd van <rhyme label="b" type="m" corresp="#s13:l3">naberouw</rhyme>.</l>
        <l id="s13:l7" met="3" type="-+">Bevryd van <rhyme label="b" type="m" corresp="#s13:l3">naberouw</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">'k Wensch ons niets zal <rhyme label="a" type="f">ontbreken</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Maer dat we ons zien <rhyme label="a" type="f" corresp="#s14:l1">ontsteken</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Lang door een zuiv're <rhyme label="b" type="m">Min</rhyme>:</l>
        <l id="s14:l4" met="3" type="-+">En dat wy alle <rhyme label="c" type="f">beyde</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">In kort ons zien <rhyme label="c" type="f" corresp="#s14:l4">verblyden</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Door Vrugten naer ons <rhyme label="b" type="m" corresp="#s14:l3">zin</rhyme>.</l>
        <l id="s14:l7" met="3" type="-+">Door Vrugten naer ons <rhyme label="b" type="m" corresp="#s14:l3">zin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
