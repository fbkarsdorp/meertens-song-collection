<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14062</id>
    <date>
      <year-start>1621</year-start>
      <year-end>1621</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La Chime</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Cluchtige, tuchtige <rhyme label="a" type="m">Harderin</rhyme>;</l>
        <l id="s1:l2" met="4" type="+-">Toertige, boertige, vrolicke <rhyme label="b" type="m">Maeghd</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Seght, hoe lang sult ghy noch voor my de <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Veynsen, die ghy in 't harte <rhyme label="b" type="m" corresp="#s1:l2">draeghd</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Of ghy u quaet op myn <rhyme label="c" type="m">gelaet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'k Weet dat ghy myn nochtans <rhyme label="d" type="m">bemind</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Want het betoond my al watter <rhyme label="e" type="m">woond</rhyme></l>
        <l id="s1:l8" met="4" type="+-">In u Valley, dat ghy myn <rhyme label="d" type="m" corresp="#s1:l6">besind</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als myn u hondtje, dat plat-beckje <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Wiggeld, het waggeld, het wispeld syn <rhyme label="b" type="m">staert</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'t Hippeld van vreughden, het strax na myn <rhyme label="a" type="m" corresp="#s2:l1">schiet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'t Weet nau wanneer het sich selven <rhyme label="b" type="m" corresp="#s2:l2">bedaert</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">'t Steeckt louter op syn geestige <rhyme label="c" type="m">kop</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Sonder dat ick 't aenroer of <rhyme label="d" type="m">hou</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En 't siet my an, niet anders, <rhyme label="e" type="m">dan</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Of het met vlyt my seggen <rhyme label="d" type="m" corresp="#s2:l6">wou</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Meend ghy dat Silvia quaed van u <rhyme label="a" type="m">peynst</rhyme>?</l>
        <l id="s3:l2" met="4" type="+-">O neen, haer hartje dat brand als een <rhyme label="b" type="m">vyer</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Maer dat sy haer nu dus voor u <rhyme label="a" type="m" corresp="#s3:l1">veynsd</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Doetse quansuys om de <rhyme label="b" type="m" corresp="#s3:l2">manier</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Dit syn de re'en; een Maeghd wil <rhyme label="c" type="m">gebe'en</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Wesen, en lang van haer Harder <rhyme label="d" type="m">gevryd</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Maer weet gewis, hoe schuw s' oock <rhyme label="e" type="m">is</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Dat ghy haer seer behaeghlyck <rhyme label="d" type="m" corresp="#s3:l6">syt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Och kon ick spreken! of konde ghy <rhyme label="a" type="m">maer</rhyme></l>
        <l id="s4:l2" met="4" type="+-">'t Waggelen van mijn startje <rhyme label="b" type="m">verstaen</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Wat soud' ick u al verhalen van <rhyme label="a" type="m" corresp="#s4:l1">haer</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Gluurtjes en suchtjes die naer u <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>!</l>
        <l id="s4:l5" met="4" type="+-">Titer ick sou strax al u <rhyme label="c" type="m">rou</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Wenden in soete <rhyme label="d" type="m">vrolickheyd</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Wist ghy het geen dat sy <rhyme label="e" type="m">alleen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Dickwils met tranen in d'oogen <rhyme label="d" type="m" corresp="#s4:l6">seyd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Titer is (seydse) myn hooghste <rhyme label="a" type="m">vermaeck</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Titer is al myns levens <rhyme label="b" type="m">vreughd</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Titer bemin ick, ick slaep of ick <rhyme label="a" type="m" corresp="#s5:l1">waeck</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Altyd gedenck ick aen Titertjes <rhyme label="b" type="m" corresp="#s5:l2">deughd</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Titertje vriend, die myn dus <rhyme label="c" type="m">diend</rhyme>!</l>
        <l id="s5:l6" met="4" type="+-">Ach! wat is 't my een groote <rhyme label="d" type="m">smart</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Dat ick myn veyns aers als ick <rhyme label="e" type="m">peyns</rhyme>!</l>
        <l id="s5:l8" met="4" type="+-">Want ick bemin u in myn <rhyme label="d" type="m" corresp="#s5:l6">hart</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Silvia, waerom schept ghy dan <rhyme label="a" type="m">geneughd</rhyme></l>
        <l id="s6:l2" met="4" type="+-">In u en myn te quellen <rhyme label="b" type="m">beyd</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Keerd doch, en doet wat ghy wesen <rhyme label="a" type="m" corresp="#s6:l1">meughd</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Soo sal in blydschap verkeeren ons <rhyme label="b" type="m" corresp="#s6:l2">leyd</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Soo sullen wy dan zy aen <rhyme label="c" type="m">zy</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Leven in alle soete <rhyme label="d" type="m">lust</rhyme>.</l>
        <l id="s6:l7" met="4" type="+-">Nu seght eens ia, myn <rhyme label="e" type="m">Silvia</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Daer op word ghy van Titer <rhyme label="d" type="m" corresp="#s6:l6">gekust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
