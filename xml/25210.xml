    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Nocte surgentes vigilemus omnes</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Opstaende in <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Laet ons al gaer <rhyme label="b" type="f">waken</rhyme></l>
        <l id="s1:l3" met="3" type="+-">makende <rhyme label="a" type="m" corresp="#s1:l1">geclacht</rhyme></l>
        <l id="s1:l4" met="3" type="+-">met gedencken <rhyme label="b" type="f" corresp="#s1:l2">raken</rhyme></l>
        <l id="s1:l5" met="3" type="+-">tot den heer moet ons <rhyme label="a" type="m" corresp="#s1:l1">wacht</rhyme></l>
        <l id="s1:l6" met="3" type="+-">een gesangh <rhyme label="b" type="f" corresp="#s1:l2">genaken</rhyme></l>
        <l id="s1:l7" met="3" type="+-">genoecht niet <rhyme label="b" type="f" corresp="#s1:l2">staecken</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Sulcx den Coninck <rhyme label="a" type="m">blij</rhyme></l>
        <l id="s2:l2" met="3" type="+-">louende <rhyme label="b" type="f">goertier</rhyme></l>
        <l id="s2:l3" met="3" type="+-">met heijligen <rhyme label="a" type="m" corresp="#s2:l1">vrij</rhyme></l>
        <l id="s2:l4" met="3" type="+-">verdienen oock <rhyme label="b" type="f" corresp="#s2:l2">hier</rhyme></l>
        <l id="s2:l5" met="3" type="+-">moogen ingaen <rhyme label="a" type="m" corresp="#s2:l1">wij</rhyme></l>
        <l id="s2:l6" met="3" type="+-">een hemels leuen <rhyme label="b" type="f" corresp="#s2:l2">fier</rhyme></l>
        <l id="s2:l7" met="3" type="+-">met haer leijden <rhyme label="b" type="f" corresp="#s2:l2">scier</rhyme></l>
      </lg>
    </lg>
  </text>
