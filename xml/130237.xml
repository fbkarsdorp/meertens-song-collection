    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Si c'est pour mon pucelage</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Op mijn Fluitje wil ik <rhyme label="a" type="f">speelen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Vrolyk van myn <rhyme label="b" type="m">Harderin</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Ik moet eens een Deuntje <rhyme label="a" type="f" corresp="#s1:l1">queelen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Want ik haer op Trouw <rhyme label="b" type="m" corresp="#s1:l2">bemin</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Echo, bost myn stem dog <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat myn Silvia het <rhyme label="c" type="m" corresp="#s1:l5">hoort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Silvia, aenhoort mijn <rhyme label="a" type="f">keeltje</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Want het tot u lof nu <rhyme label="b" type="m">zingt</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Dat   zoo klaer gelijk een <rhyme label="a" type="f" corresp="#s2:l1">Veeltje</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Door het groene Bosje <rhyme label="b" type="m" corresp="#s2:l2">klinkt</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Komt dog aen op het <rhyme label="c" type="m">geluyt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van mijn soete Harders <rhyme label="c" type="m" corresp="#s2:l5">Fluyt</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ik, helaes, bedroefden <rhyme label="a" type="f">Harder</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Moet alleen mijn Schaepjes <rhyme label="b" type="m">hoen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">En mijn Silvia gaet <rhyme label="a" type="f" corresp="#s3:l1">verder</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Weiden in het Bosjen <rhyme label="b" type="m" corresp="#s3:l2">groen</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Of   ik haer dryf agter <rhyme label="c" type="m">an</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ik haer dog niet krygen <rhyme label="c" type="m" corresp="#s3:l5">kan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Herderin, wilt my dog <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ik bid u, blyft een weinig <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Neigt tot myne klagten u <rhyme label="a" type="f" corresp="#s4:l1">ooren</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En ziet dog myn lyden <rhyme label="b" type="m" corresp="#s4:l2">aen</rhyme>!</l>
        <l id="s4:l5" met="4" type="+-">Siet hoe dat ik in myn <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Lyd' onlydelijke <rhyme label="c" type="m" corresp="#s4:l5">smert</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Mogt   ik eens myn Liefje <rhyme label="a" type="f">spreken</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Daer zy weyd al aen de <rhyme label="b" type="m">kant</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Van de klare <rhyme label="a" type="f" corresp="#s5:l1">water-beeken</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Daer veel Bloempjes staen <rhyme label="b" type="m" corresp="#s5:l2">geplant</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Dat ik haer daer vinden <rhyme label="c" type="m">zou</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Verdreven was al myn <rhyme label="c" type="m" corresp="#s5:l5">rouw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dat ik haer maer wist te <rhyme label="a" type="f">vinden</rhyme>!</l>
        <l id="s6:l2" met="4" type="+-">Dadelijk zou ik mijn <rhyme label="b" type="m">spoen</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Door de digt beblade <rhyme label="a" type="f" corresp="#s6:l1">Linden</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Om te komen in het <rhyme label="b" type="m" corresp="#s6:l2">groen</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Ras mijn Schaapjes, treed wat <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Want haer stem heb ik <rhyme label="c" type="m" corresp="#s6:l5">gehoort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Silvia hoor ik daer <rhyme label="a" type="f">zingen</rhyme>.</l>
        <l id="s7:l2" met="4" type="+-">Hemel, wat een soeten <rhyme label="b" type="m">thoon</rhyme>!</l>
        <l id="s7:l3" met="4" type="+-">Al myn aderen die <rhyme label="a" type="f" corresp="#s7:l1">springen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Daer is geen snare-spel zoo <rhyme label="b" type="m" corresp="#s7:l2">schoon</rhyme>!</l>
        <l id="s7:l5" met="4" type="+-">Want de klare Beeke <rhyme label="c" type="m">springt</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Wanneer dat myn Liefste <rhyme label="c" type="m" corresp="#s7:l5">zingt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hoe sal ik u dan met <rhyme label="a" type="f">kusjens</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">O myn zoete <rhyme label="b" type="m">Silvia</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En met lieffelijke <rhyme label="a" type="f" corresp="#s8:l1">lusjes</rhyme></l>
        <l id="s8:l4" met="4" type="+-">In myn armen nemen <rhyme label="b" type="m" corresp="#s8:l2">dra</rhyme>!</l>
        <l id="s8:l5" met="4" type="+-">Want u zoete <rhyme label="c" type="m">Rooder-mont</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Kon   myn hart maken <rhyme label="c" type="m" corresp="#s8:l5">gezont</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Princes, Herderin, ik <rhyme label="a" type="f">haake</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Na de uure dat ik <rhyme label="b" type="m">jou</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Sal in d'Echten-staat <rhyme label="a" type="f" corresp="#s9:l1">genaake</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">Dan sult gy zijn mijne <rhyme label="b" type="m" corresp="#s9:l2">Vrouw</rhyme></l>
        <l id="s9:l5" met="4" type="+-">En ons Schaapjes sullen <rhyme label="c" type="m">mêe</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Zijn vereenigt op die <rhyme label="c" type="m" corresp="#s9:l5">Stêe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
