<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185856</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nu laat ons allegaar</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wilt zijn verblijt, met groot <rhyme label="a" type="m">jolijt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Een kindt is ons <rhyme label="b" type="f">ghebooren</rhyme></l>
        <l id="s1:l3" met="4" type="-+">In desen tijt, comt al met <rhyme label="a" type="m" corresp="#s1:l1">vlijt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die hier zijn <rhyme label="b" type="f" corresp="#s1:l2">wtvercoren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Een Soon ghegeven, hem nu <rhyme label="c" type="m">eert</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Met hert ghebroken en <rhyme label="c" type="m" corresp="#s1:l5">verneert</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hy sal v niet <rhyme label="d" type="f">versmaden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Al comt ghy seer <rhyme label="d" type="f" corresp="#s1:l7">beladen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gods liefd’ aensiet, vergeet toch <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">De weldaet ons <rhyme label="b" type="f">bewesen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hoe groot geniet, ons is <rhyme label="a" type="m" corresp="#s2:l1">gheschiet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die door hem zijn <rhyme label="b" type="f" corresp="#s2:l2">genesen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">d’Engelen songhen blijtschap <rhyme label="c" type="m">fijn</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Glory zy God int hoochste <rhyme label="c" type="m" corresp="#s2:l5">zijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En vrede zijn al <rhyme label="d" type="f">desen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die goet van wil sal <rhyme label="d" type="f" corresp="#s2:l7">wesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Onnosel reyn, als kindren <rhyme label="a" type="m">cleyn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zo laet hy v <rhyme label="b" type="f">genaken</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Int hert alleyn, rust God <rhyme label="a" type="m" corresp="#s3:l1">certeyn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die door vreed’, liefde <rhyme label="b" type="f" corresp="#s3:l2">smaken</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Een Siel die God boven al <rhyme label="c" type="m">mint</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Hoe groote rust ist dat die <rhyme label="c" type="m" corresp="#s3:l5">vint</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Zeer licht sal hy’t al <rhyme label="d" type="f">derven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Die om God hier leert <rhyme label="d" type="f" corresp="#s3:l7">sterven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een vry ghemoet, lof-hertich <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat sal hem dick <rhyme label="b" type="f">verblijen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Een hertjen goet, door diep <rhyme label="a" type="m" corresp="#s4:l1">ootmoet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Beswaert hier oock geen <rhyme label="b" type="f" corresp="#s4:l2">lijen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Het ziet altijt zijn Schepper <rhyme label="c" type="m">wis</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Wiens weldaet hy ghedachtich <rhyme label="c" type="m" corresp="#s4:l5">is</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Die ons hier heeft <rhyme label="d" type="f">ghegeven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zijn lichaem hooch <rhyme label="d" type="f" corresp="#s4:l7">verheven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een hertjen teer, vol liefds <rhyme label="a" type="m">begeer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat leeft met diep <rhyme label="b" type="f">aenmercken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Door soet inkeer, in God zijn <rhyme label="a" type="m" corresp="#s5:l1">Heer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">t’Aenschouwen, hier zijn <rhyme label="b" type="f" corresp="#s5:l2">wercken</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Die hy voor ons hier heeft <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Wie God verlicht en kant niet <rhyme label="c" type="m" corresp="#s5:l5">schaen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat hy hem gaet <rhyme label="d" type="f">vermeyen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">In liefdens soete <rhyme label="d" type="f" corresp="#s5:l7">weyen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O Prins ghy zijt, <rhyme label="a" type="m">gebenedijt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die ons also <rhyme label="b" type="f">beminde</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Om ons profijt, schoudt ghy ons <rhyme label="a" type="m" corresp="#s6:l1">quijt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Als wijt niet en <rhyme label="b" type="f" corresp="#s6:l2">verdiende</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ons sonden lelijck ende <rhyme label="c" type="m">swaer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Door uwen Soon ons <rhyme label="c" type="m" corresp="#s6:l5">middelaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Zo wy hem hier <rhyme label="d" type="f">genieten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Die hem so gaet <rhyme label="d" type="f" corresp="#s6:l7">wtgieten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
