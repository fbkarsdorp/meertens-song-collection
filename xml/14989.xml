  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">G'lyck den grootsten <rhyme label="a" type="f">Rapsack</rhyme>,</l>
        <l id="s1:l2" met="6" type="+-">Vloot den Speck verbaest, Als een wind die <rhyme label="b" type="m">blaest</rhyme>.</l>
        <l id="s1:l3" met="3" type="+-">Siet hem met zijn <rhyme label="a" type="f" corresp="#s1:l1">knapsack</rhyme>,</l>
        <l id="s1:l4" met="6" type="+-">Loopen in der haest, Als een hond die <rhyme label="b" type="m" corresp="#s1:l2">raest</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">O ghy stad van <rhyme label="c" type="f">Leyden</rhyme>!</l>
        <l id="s1:l6" met="2" type="-+">Dit stuck <rhyme label="d" type="m">bemerck</rhyme>;</l>
        <l id="s1:l7" met="3" type="+-">En laet toch <rhyme label="c" type="f" corresp="#s1:l5">verbreyden</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Gods wonder <rhyme label="d" type="m" corresp="#s1:l6">werck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">De Bourgoensche <rhyme label="a" type="f">vanen</rhyme></l>
        <l id="s2:l2" met="6" type="+-">Vlogen op de vlucht, met een groot <rhyme label="b" type="m">gerucht</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En de <rhyme label="a" type="f" corresp="#s2:l1">Castil'janen</rhyme></l>
        <l id="s2:l4" met="6" type="+-">Waren oock vol sucht, en geheel <rhyme label="b" type="m" corresp="#s2:l2">beducht</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Door de hooge <rhyme label="c" type="f">stromen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">En menig <rhyme label="d" type="m">man</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Die sy sagen <rhyme label="c" type="f" corresp="#s2:l5">komen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Dick krielen <rhyme label="d" type="m" corresp="#s2:l6">an</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wilt Gods eer <rhyme label="a" type="f">verbreyen</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">Die nu kleyn en groot,, Vry maeckt vande <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s3:l3" met="3" type="+-">En naer droevig <rhyme label="a" type="f" corresp="#s3:l1">schreyen</rhyme></l>
        <l id="s3:l4" met="6" type="+-">U dus sent in noot,, overvloedig <rhyme label="b" type="m" corresp="#s3:l2">broot</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Lof dan, prys en <rhyme label="c" type="f">eere</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Moet syn <rhyme label="d" type="m">geseyt</rhyme></l>
        <l id="s3:l7" met="3" type="+-">God, ons aller <rhyme label="c" type="f" corresp="#s3:l5">Heere</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">In <rhyme label="d" type="m" corresp="#s3:l6">eeuwigheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
