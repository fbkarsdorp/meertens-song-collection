<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5180</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag>beggar song</tag>
    </tags>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilt nu ghy <rhyme label="a" type="f">Nederlanden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Te samen zijn <rhyme label="b" type="m">verblijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Middelburch is in <rhyme label="a" type="f" corresp="#s1:l1">handen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Des Princes nu ter <rhyme label="b" type="m" corresp="#s1:l2">tijt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Den Prince van <rhyme label="c" type="f">Orangien</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met sijn Vorstelijck <rhyme label="d" type="m">ghemoet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Drijft het Crijchsvolk na <rhyme label="c" type="f" corresp="#s1:l5">Spangien</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Uut Middelburch met <rhyme label="d" type="m" corresp="#s1:l6">spoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Alleyn tot u <rhyme label="a" type="f">welvaeren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Voert de Prins desen <rhyme label="b" type="m">strijdt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om u te <rhyme label="a" type="f" corresp="#s2:l1">openbaren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Hoe ellendich ghy <rhyme label="b" type="m" corresp="#s2:l2">zijt</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Soo ghy nu leert <rhyme label="c" type="f">regeeren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van vreemden spaenschen <rhyme label="d" type="m">aert</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die u soekt te <rhyme label="c" type="f" corresp="#s2:l5">pilleeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En daghelijcxs <rhyme label="d" type="m" corresp="#s2:l6">beswaert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe heeft Middelburch <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Verdraghen honghers <rhyme label="b" type="m">cracht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als t'Lants vryheyt <rhyme label="a" type="f" corresp="#s3:l1">ydoone</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Niet anders heeft <rhyme label="b" type="m" corresp="#s3:l2">bedacht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Wat hebben ander <rhyme label="c" type="f">steden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">End Landen int <rhyme label="d" type="m">ghemeyn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">So langhe alsins <rhyme label="c" type="f" corresp="#s3:l5">gheleden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Door die Spangiaerts <rhyme label="d" type="m" corresp="#s3:l6">onreyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Uwe dochters ende <rhyme label="a" type="f">vrouwen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Bedenckt met herten <rhyme label="b" type="m">vroet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De vryheyt der <rhyme label="a" type="f" corresp="#s4:l1">Landouwen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En de inwoonders <rhyme label="b" type="m" corresp="#s4:l2">soet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wilt vant ghewelt <rhyme label="c" type="f">bevryen</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">Van dees vreemder hier <rhyme label="d" type="m">naer</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Schattinghe en wilt niet <rhyme label="c" type="f" corresp="#s4:l5">lijen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Des thienden Pennincx <rhyme label="d" type="m" corresp="#s4:l6">swaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Spaensche <rhyme label="a" type="f">Inquisitueren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die u hebben <rhyme label="b" type="m">gheplaecht</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Verdrijft uyt uwer <rhyme label="a" type="f" corresp="#s5:l1">mueren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ontfanght u volck <rhyme label="b" type="m" corresp="#s5:l2">verjaecht</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Laet Godes woort <rhyme label="c" type="f">floreren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">U daer niet teghen <rhyme label="d" type="m">stelt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Soo sult ghy <rhyme label="c" type="f" corresp="#s5:l5">prospereren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">End vreesen gheen <rhyme label="d" type="m" corresp="#s5:l6">ghewelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wilt u tsamen <rhyme label="a" type="f">verbinden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Teghens de vreemden <rhyme label="b" type="m">wreet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Waeyt niet met alle <rhyme label="a" type="f" corresp="#s6:l1">winden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Bedenck doch uwen <rhyme label="b" type="m" corresp="#s6:l2">eedt</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Om tvolck thouden in <rhyme label="c" type="f">vreden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Hebt ghy ghesworen <rhyme label="d" type="m">tsaem</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Het landt by sijn <rhyme label="c" type="f" corresp="#s6:l5">vryheden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Te bewaeren <rhyme label="d" type="m" corresp="#s6:l6">bequaem</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Den Coninck is u <rhyme label="a" type="f">schuldich</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Na sijnen eedt tis <rhyme label="b" type="m">claer</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Tslants vryheyt <rhyme label="a" type="f" corresp="#s7:l1">menichvuldich</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Te beschermen <rhyme label="b" type="m" corresp="#s7:l2">eenpaer</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">So hy dat niet wil <rhyme label="c" type="f">houwen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Maer ghebruyckt cracht en <rhyme label="d" type="m">ghewelt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Van uwen eedt vol <rhyme label="c" type="f" corresp="#s7:l5">trouwen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Heeft hy u vry <rhyme label="d" type="m" corresp="#s7:l6">ghestelt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Den Prins wilt die bant <rhyme label="a" type="f">bieden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die voor u tieven <rhyme label="b" type="m">waecht</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Maeckt dat de vreemde <rhyme label="a" type="f" corresp="#s8:l1">vlieden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Daer over elck man <rhyme label="b" type="m" corresp="#s8:l2">claecht</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wilt ghy dit niet <rhyme label="c" type="f">beherten</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Soo ghy moetwille <rhyme label="d" type="m">drijft</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Tis weert dat ghy met <rhyme label="c" type="f" corresp="#s8:l5">smerten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Eewichlijck slaven <rhyme label="d" type="m" corresp="#s8:l6">blijft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dies met vromen <rhyme label="a" type="f">ghemoede</rhyme></l>
        <l id="s9:l2" met="3" type="-+">U Landt, lijf, ende <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wijf ende kint als de <rhyme label="a" type="f" corresp="#s9:l1">vroede</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Beschermt nu wat ghy <rhyme label="b" type="m" corresp="#s9:l2">doet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Verliest ghy nu ten <rhyme label="c" type="f">tijden</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Dese bequaemheyt <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">U naeckt van allen <rhyme label="c" type="f" corresp="#s9:l5">zijden</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Dienstbaerheydt, angst, ende <rhyme label="d" type="m" corresp="#s9:l6">noot</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Nederlandt ghy zijt <rhyme label="a" type="f">neder</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Door de Spaengiaerts <rhyme label="b" type="m">ghedruckt</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">Staet op, end slaet hem <rhyme label="a" type="f" corresp="#s10:l1">weder</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Die u soo scheert en <rhyme label="b" type="m" corresp="#s10:l2">pluckt</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Maeckt met den Prins <rhyme label="c" type="f">ghepresen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Een sterck verbondt <rhyme label="d" type="m">voortaen</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Voorspoedich sult ghy <rhyme label="c" type="f" corresp="#s10:l5">wesen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Oorloff, vaet dit <rhyme label="d" type="m" corresp="#s10:l6">vermaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
