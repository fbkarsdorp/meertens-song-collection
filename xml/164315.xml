<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164315</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Windje daar het bos af drilt</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WEl op, wel op, geselschap <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">'t Sal nu tijt van scheyden <rhyme label="a" type="m" corresp="#s1:l1">zijn</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wilt nu prijsen, eer <rhyme label="b" type="f">bewijsen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die 't ons alles heeft <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Laet ons die dancken eer men <rhyme label="c" type="m" corresp="#s1:l4">scheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy dancken dan met bly <rhyme label="a" type="m">geschal</rhyme></l>
        <l id="s2:l2" met="4" type="+-">U o Schepper boven <rhyme label="a" type="m" corresp="#s2:l1">al</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Voor u goetheyt, en de <rhyme label="b" type="f">soetheyt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die elck hier genoten <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Eere zy u die 't alles <rhyme label="c" type="m" corresp="#s2:l4">geeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy dancken oock het jonge <rhyme label="a" type="m">paer</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En de gasten <rhyme label="a" type="m" corresp="#s3:l1">allegaer</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die met vreughde, in de <rhyme label="b" type="f">deughde</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Neffens ons speelnootjes <rhyme label="c" type="m">fijn</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Hier mee van herten vrolijck <rhyme label="c" type="m" corresp="#s3:l4">zijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Komt nu al t'samen aen den <rhyme label="a" type="m">dans</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'t Is de vreught der <rhyme label="a" type="m" corresp="#s4:l1">Maeghden-krans</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want het teken, heeft <rhyme label="b" type="f">gebleken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat ons Bruytje vrouw sal <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Morgen al voor de <rhyme label="c" type="m" corresp="#s4:l4">Sonne-schijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'t Teken dat is de reyne <rhyme label="a" type="m">trou</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dat een Bruyt maeckt tot een <rhyme label="a" type="m" corresp="#s5:l1">vrou</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Al het wenschen, by de <rhyme label="b" type="f">menschen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Is de trou daer 't al aen <rhyme label="c" type="m">hanght</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Daer menig Maget na <rhyme label="c" type="m" corresp="#s5:l4">verlanght</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Tot een besluyt, 't is tijd van <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Want den dageraet komt <rhyme label="a" type="m" corresp="#s6:l1">aen</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Laen ons scheyden, en <rhyme label="b" type="f">geleyden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Elck sijn Liefje met <rhyme label="c" type="m">geneught</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">En met een kuste vol van <rhyme label="c" type="m" corresp="#s6:l4">vreught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
