  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HAar <rhyme label="a" type="f">Iudas-lokken</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die hangen by haar <rhyme label="b" type="m">Hoofd</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Als <rhyme label="a" type="f" corresp="#s1:l1">Zwavel-stokken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">'t Geen 't Fermiljoen <rhyme label="b" type="m" corresp="#s1:l2">verdooft</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Heel goed voor een <rhyme label="c" type="m">Monsieur</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die men noemt <rhyme label="c" type="m" corresp="#s1:l5">Reprateur</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Of voor Crispijn zijn <rhyme label="d" type="f">Gasten</rhyme>:</l>
        <l id="s1:l8" met="5" type="-+">Geen schoonder ik op Aarde meer <rhyme label="d" type="f" corresp="#s1:l7">verrasten</rhyme>.</l>
      </lg>
    </lg>
