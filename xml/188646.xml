<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188646</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Français marchant au combat</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Waar reez ooit schooner <rhyme label="a" type="m">Morgenstond</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ooit aangennamer <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dan toen men 't zegenrijkst <rhyme label="a" type="m" corresp="#s1:l1">verbond</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Voor ons geslooten <rhyme label="b" type="m" corresp="#s1:l2">zag</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Naauw stak de goude <rhyme label="c" type="m">zon</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Het hoofd uit Thetis <rhyme label="c" type="m" corresp="#s1:l5">bron</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Of 't algemeen gejuich hief <rhyme label="d" type="m">aan</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">De Vrijheid zal <rhyme label="d" type="m" corresp="#s1:l7">bestaan</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Steek! Batavieren steek den <rhyme label="a" type="m">kruin</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Nu moedig naar om <rhyme label="b" type="m">hoog</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Uw heil blinkt over zee en <rhyme label="a" type="m" corresp="#s2:l1">duin</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Uw' haters fel in 't <rhyme label="b" type="m" corresp="#s2:l2">oog</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Nu knarsetand de <rhyme label="c" type="m">nijd</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Daar heerschzucht berst van <rhyme label="c" type="m" corresp="#s2:l5">spijt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Elk zingt nu, trotsch de <rhyme label="d" type="m">Dwinglandij</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ons Vaderland is <rhyme label="d" type="m" corresp="#s2:l7">vrij</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hoe broederlijk omhelst de <rhyme label="a" type="m">Gal</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De Nederlandsche <rhyme label="b" type="m">Maagd</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">In deze kusch berust het <rhyme label="a" type="m" corresp="#s3:l1">all'</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wat onze Vrijheid <rhyme label="b" type="m" corresp="#s3:l2">schraagdt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Zeg nu nog, vuige <rhyme label="c" type="m">haat</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat ons de Gal <rhyme label="c" type="m" corresp="#s3:l5">verlaat</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Dat hij, genoopt door <rhyme label="d" type="m">zelsbelang</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ons lokte in zijnen <rhyme label="d" type="m" corresp="#s3:l7">dwang</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'t Is enkel liefde en <rhyme label="a" type="m">vrijheidsmin</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die hem door vuur en <rhyme label="b" type="m">staal</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dus voerden 't zuchtend Neêrland <rhyme label="a" type="m" corresp="#s4:l1">in</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Zijn grootsche <rhyme label="b" type="m" corresp="#s4:l2">zegepraal</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Was op het heil <rhyme label="c" type="m">verbond</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van Broederschap <rhyme label="c" type="m" corresp="#s4:l5">gegrond</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Dus deeld de dappre Frank ons <rhyme label="d" type="m">vreê</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En waare vriendschap <rhyme label="d" type="m" corresp="#s4:l7">meê</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Dag! o wondervolle <rhyme label="a" type="m">Dag</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Die 't laatste <rhyme label="b" type="m">Nageslacht</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Met dubble vreugde vieren <rhyme label="a" type="m" corresp="#s5:l1">mag</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daar ge ons de Vrijheid <rhyme label="b" type="m" corresp="#s5:l2">bragt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">In U legt ons <rhyme label="c" type="m">geluk</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">O Dag, bevrij van <rhyme label="c" type="m" corresp="#s5:l5">druk</rhyme>!</l>
        <l id="s5:l7" met="4" type="-+">Gij zult, 't zij Neêrland bloeije of <rhyme label="d" type="m">kwijn'</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Steeds een gedenkdag <rhyme label="d" type="m" corresp="#s5:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O lang gefolterd <rhyme label="a" type="m">Vaderland</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Deez Dag verbrak uw <rhyme label="b" type="m">boeij</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Gal schonk U de <rhyme label="a" type="m" corresp="#s6:l1">Broederhand</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat nu de vriendschap <rhyme label="b" type="m" corresp="#s6:l2">gloeij'</rhyme>!</l>
        <l id="s6:l5" met="3" type="-+">Uw saam verèend <rhyme label="c" type="m">Metaal</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En schittrend <rhyme label="c" type="m" corresp="#s6:l5">Helden-staal</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Beschermd de stoute <rhyme label="d" type="m">Zeevaardij</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En maakt den Handel <rhyme label="d" type="m" corresp="#s6:l7">vrij</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
