<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3875</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="mystiek lied" translation="mystical song">mystiek lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Ik weet een vrouwtje wel bereid</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VAn hooger minnen die ick <rhyme label="a" type="f">draghe</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Verblide ic sonder <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Want mi gheen dinc en mach <rhyme label="a" type="f" corresp="#s1:l1">versaghen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Bi mijn lief so eest al <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Lof eer dat hem <rhyme label="c" type="m">betaemt</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Coninc van hemel ende <rhyme label="d" type="f">eertrijc</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Is hi alleen <rhyme label="c" type="m" corresp="#s1:l5">ghenaemt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Scoonder man en wert nye <rhyme label="a" type="f">angescouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">In alle die werelt <rhyme label="b" type="m">wijt</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Sijn aenscijn blinct gelijc den <rhyme label="a" type="f" corresp="#s2:l1">gouwe</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Dat alle die sinnen <rhyme label="b" type="m" corresp="#s2:l2">verblijt</rhyme></l>
        <l id="s2:l5" met="3" type="+-">So schoon is hi <rhyme label="c" type="m">ghedaen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">So wie op hem set sijn <rhyme label="d" type="f">betrouwe</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Hi willen gheerne <rhyme label="c" type="m" corresp="#s2:l5">ontfaen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De sonne / de mane / den claren <rhyme label="a" type="f">dach</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En aldes hemels <rhyme label="b" type="m">vertuyt</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Al dat ter werelt wesen <rhyme label="a" type="f" corresp="#s3:l1">mach</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Biedt minen lieue <rhyme label="b" type="m" corresp="#s3:l2">saluyt</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Die ghene daert al aen <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Dat is mijn alder liefste <rhyme label="d" type="f">lief</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Daer al minen troost aen <rhyme label="c" type="m" corresp="#s3:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het wondert mi in alle <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat yemant is so <rhyme label="b" type="m">dwaes</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Mer dat hi bouen God te <rhyme label="a" type="f" corresp="#s4:l1">minnen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Verkiest eenich <rhyme label="b" type="m" corresp="#s4:l2">solaes</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Want God alleen doch <rhyme label="c" type="m">si</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Dien behoeuen alle <rhyme label="d" type="f">creaturen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">In hooch / in neder / in <rhyme label="c" type="m" corresp="#s4:l5">wi</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O heylige God der <rhyme label="a" type="f">seraphinen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">O minnelijck <rhyme label="b" type="m">toeuerlaet</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Ghi sijt het eynde / het <rhyme label="a" type="f" corresp="#s5:l1">beghinen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Daer alle wesen aen <rhyme label="b" type="m" corresp="#s5:l2">staet</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Ghi sijt vol minnen <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Wilt mi ontsteken in uwer <rhyme label="d" type="f">minnen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">O alder opperste <rhyme label="c" type="m" corresp="#s5:l5">goet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die daer eens mach in <rhyme label="a" type="f">tracteren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In sijnder minnen <rhyme label="b" type="m">gront</rhyme></l>
        <l id="s6:l3" met="4" type="+-">In dat suyuerlijck <rhyme label="a" type="f" corresp="#s6:l1">contempleren</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Waer hi van druck <rhyme label="b" type="m" corresp="#s6:l2">ghesont</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">O heere van <rhyme label="c" type="m">hemelrijck</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Aensiet doch onse crancheyt <rhyme label="d" type="f">groot</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ende ontfermt onser al <rhyme label="c" type="m" corresp="#s6:l5">ghelijck</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
