      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody>Hei hoe helder schijnt het maantje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EY! ick heb alreed' <rhyme label="a" type="f">ontfangen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Datje by mijn Suster <rhyme label="b" type="m">soeckt</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Let op mijne vaste <rhyme label="a" type="f" corresp="#s1:l1">gangen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Siet hoe dat ick ben <rhyme label="b" type="m" corresp="#s1:l2">verkloeckt</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Een muur Van <rhyme label="c" type="m">vuur</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Ben ick al <rhyme label="d" type="m">be-reedts</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Al geschiede my wat <rhyme label="d" type="m" corresp="#s1:l6">leedts</rhyme>:</l>
        <l id="s1:l8" met="4" type="+-">'En vuur, my oock niet en <rhyme label="e" type="m">let</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dat ick werde vast <rhyme label="e" type="m" corresp="#s1:l8">gheset</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Mijne Borsten als een <rhyme label="a" type="f">Tooren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Met de waerheyt <rhyme label="b" type="m">opghevult</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Komen vol en bol te <rhyme label="a" type="f" corresp="#s2:l1">vooren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Daer je voedtsel hale <rhyme label="b" type="m" corresp="#s2:l2">sult</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Van Melck, Voor <rhyme label="c" type="m">elck</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Die m'er dorstigh <rhyme label="d" type="m">is</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Kome, en hy sal <rhyme label="d" type="m" corresp="#s2:l6">ghewis</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Sijn kelck, met des Hemels <rhyme label="e" type="m">vocht</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Vulle, soo'se werdt <rhyme label="e" type="m" corresp="#s2:l8">gesocht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dus m'en lieve <rhyme label="a" type="f">Speel-genoten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Siet hoe ick, die was <rhyme label="b" type="m">ghequelt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Met ghenaden <rhyme label="a" type="f" corresp="#s3:l1">over-goten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Werde nu gherust <rhyme label="b" type="m" corresp="#s3:l2">gestelt</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Van 't oogh, Dat <rhyme label="c" type="m">hoogh</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Van den Hemel <rhyme label="d" type="m">vlood'</rhyme></l>
        <l id="s3:l7" met="4" type="+-">En me sijne gunste <rhyme label="d" type="m" corresp="#s3:l6">bood'</rhyme>:</l>
        <l id="s3:l8" met="4" type="+-">Ick vloogh he'en na mijn <rhyme label="e" type="m">gesindt'</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Even als die vrede <rhyme label="e" type="m" corresp="#s3:l8">vindt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
