  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VRiheyt van herten en gaue ik <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Om duysent mercken van <rhyme label="b" type="f">goude</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dat ic in eender missen <rhyme label="a" type="m" corresp="#s1:l1">lanck</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Dat ic in eender missen <rhyme label="a" type="m" corresp="#s1:l1">lanck</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Mijn hertken deruen <rhyme label="b" type="f" corresp="#s1:l2">soude</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daer om sijn sij vry <rhyme label="a" type="m">altoos</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ende gherust in minen <rhyme label="b" type="f">sinne</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dat ick oyt puere minne <rhyme label="a" type="m" corresp="#s2:l1">coos</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Dat ick oyt puere minne <rhyme label="a" type="m" corresp="#s2:l1">coos</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">In minen <rhyme label="b" type="f" corresp="#s2:l2">aenbeghinne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den voghelen is veel beter <rhyme label="a" type="m">gheschiet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die inder luchten <rhyme label="b" type="f">swimmen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Dan eenen die gheuangen <rhyme label="a" type="m" corresp="#s3:l1">leet</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dan eenen die gheuangen <rhyme label="a" type="m" corresp="#s3:l1">leet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Al inder lieden <rhyme label="b" type="f" corresp="#s3:l2">minne</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O edel herte nu hout v <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ende wacht v van <rhyme label="b" type="f">verliesen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Des menschen gelaet is <rhyme label="a" type="m" corresp="#s4:l1">duechdelijc</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Des menschen gelaet is <rhyme label="a" type="m" corresp="#s4:l1">duechdelijc</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ende twifelijc te <rhyme label="b" type="f" corresp="#s4:l2">verkiesen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een wile tooghen sij v een schoon <rhyme label="a" type="m">ghelaet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat dunct v int <rhyme label="b" type="f">aenschine</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Als ghi ws selfs berooft <rhyme label="a" type="m" corresp="#s5:l1">staet</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Als ghi ws selfs berooft <rhyme label="a" type="m" corresp="#s5:l1">staet</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">So laten sij v in die <rhyme label="b" type="f" corresp="#s5:l2">pine</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O edel herte nu hout v <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En staet daer na met <rhyme label="b" type="f">vlite</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Vriheyt is so edelen <rhyme label="a" type="m" corresp="#s6:l1">goet</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Vriheyt is so edelen <rhyme label="a" type="m" corresp="#s6:l1">goet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Het maect den menigen <rhyme label="b" type="f" corresp="#s6:l2">rijcke</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O edel herte nu hout v <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ende staet daer na met <rhyme label="b" type="f">sinne</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Ghi meynt daer uwen orbaer in <rhyme label="a" type="m" corresp="#s7:l1">doen</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Ghi meynt daer uwen orbaer in <rhyme label="a" type="m" corresp="#s7:l1">doen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Het is al vrientscap sonder <rhyme label="b" type="f" corresp="#s7:l2">minne</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Laet liden / laet liden / laet <rhyme label="a" type="m">ouergaen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Al dat metter tijt <rhyme label="b" type="f">vergaet</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Die in liden can <rhyme label="a" type="m" corresp="#s8:l1">verbliden</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Die in liden can <rhyme label="a" type="m" corresp="#s8:l1">verbliden</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Deewige goet wert hem <rhyme label="b" type="f" corresp="#s8:l2">bereet</rhyme>.</l>
      </lg>
    </lg>
  </text>
