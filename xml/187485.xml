<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187485</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Laast lag ik en sliep in ’t groene <rhyme label="a" type="m">veld</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">In myn droom zag ik myn zoetst <rhyme label="a" type="m" corresp="#s1:l1">beeld</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die daar my zeide schoon lief kom by <rhyme label="b" type="f">my</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Gy doet my treuren met <rhyme label="a" type="m" corresp="#s1:l1">geweld</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Gy word van my zo minnelyk <rhyme label="a" type="m">geagt</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Want u oogjes zyn vol van minne <rhyme label="a" type="m" corresp="#s2:l1">klagt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">U droevige klagen, by nagten en <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Want myn Jonkhertje van minne <rhyme label="a" type="m" corresp="#s2:l1">brand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Als Aurora verdween in ’t Zonne <rhyme label="a" type="m">Ligt</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Dan kwam Febus met zyn bleek <rhyme label="a" type="m" corresp="#s3:l1">gezigt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Met zyn straalen, kwam <rhyme label="b" type="f">nederdalen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Toen kreeg ik haar in ’t <rhyme label="a" type="m" corresp="#s3:l1">gezigt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ik zei myn lief myn schoonste <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Die ik aan een klaar waterkantje <rhyme label="a" type="m" corresp="#s4:l1">vind</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met zoo een diep ootmoet, nam ik afscheid met myn <rhyme label="b" type="f">hoed</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En zoo heb ik dees Maagd <rhyme label="a" type="m" corresp="#s4:l1">gegroed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Zy salveerde my op ’t zelve <rhyme label="a" type="m">pas</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Ik meenden dat ik in den Hemel <rhyme label="a" type="m" corresp="#s5:l1">was</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ja met lusten ging men <rhyme label="b" type="f">rusten</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">By malkanderen in ’t groene <rhyme label="a" type="m" corresp="#s5:l1">Gras</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Als wy dan spreeken van de <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Ik zei myn liefje schoonste <rhyme label="a" type="m" corresp="#s6:l1">Engelin</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ik zieder myn schoonder <rhyme label="b" type="f">Jonkvrouw</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Helpt my uit den Rouw geeft my maar een zoen op <rhyme label="a" type="m" corresp="#s6:l1">Trouw</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Gaat weg Jonkmans met al u loos <rhyme label="a" type="m">geschrey</rhyme></l>
        <l id="s7:l2" met="5" type="-+">Want de Jonksmans zyn vol <rhyme label="a" type="m" corresp="#s7:l1">bedriegery</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Zoekt u Pleisieren by de <rhyme label="b" type="f">Dieren</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Die daar zitten in de <rhyme label="a" type="m" corresp="#s7:l1">Wey</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Want gy bemind myn bloempje om zyn <rhyme label="a" type="m">fleur</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">En ’t Meisje om haar <rhyme label="a" type="m" corresp="#s8:l1">kleur</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Als het was geplukt, en in ’t zand <rhyme label="b" type="f">gerukt</rhyme></l>
        <l id="s8:l4" met="4" type="-+">En dan zoud gy ’er my laaten in <rhyme label="a" type="m" corresp="#s8:l1">druk</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">Ik zweer by Hemel, Sterre, Zon en <rhyme label="a" type="m">Maan</rhyme>,</l>
        <l id="s9:l2" met="5" type="-+">Dat ik myn liefje altoos by zal <rhyme label="a" type="m" corresp="#s9:l1">staan</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Zoo wel in lyden als in ’t <rhyme label="b" type="f">verblyden</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">ja tot dat de dood komt <rhyme label="a" type="m" corresp="#s9:l1">aan</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">Adju dan Lief nu neem ik myn <rhyme label="a" type="m">afscheid</rhyme></l>
        <l id="s10:l2" met="5" type="-+">Adju dan Herderin, adju dan waarde <rhyme label="a" type="m" corresp="#s10:l1">Bruid</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Myn genugt is uit, waar me ik myn <rhyme label="b" type="f">klagt</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Besluit, Adju waarde beminde <rhyme label="a" type="m" corresp="#s10:l1">Bruid</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="-+">Jonkman ik zie u <rhyme label="a" type="m">standvastigheid</rhyme>,</l>
        <l id="s11:l2" met="5" type="-+">Nu ben ik heel gantsch tot u <rhyme label="a" type="m" corresp="#s11:l1">bereid</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Nu wil ik myn leden, aan u <rhyme label="b" type="f">besteden</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Ja tot dat de dood ons <rhyme label="a" type="m" corresp="#s11:l1">scheid</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="5" type="-+">Jonkman als gy u Lief opregt <rhyme label="a" type="m">bemind</rhyme>,</l>
        <l id="s12:l2" met="5" type="-+">Hart en Liefde die ’t alles <rhyme label="a" type="m" corresp="#s12:l1">overwind</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Schept maar goede moet, zuur zal <rhyme label="b" type="f">worden</rhyme></l>
        <l id="s12:l4" met="4" type="-+">zoet, Als gy dan u best maar <rhyme label="a" type="m" corresp="#s12:l1">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
