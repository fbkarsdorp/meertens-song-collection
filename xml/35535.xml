  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ick bid u, o Iesu <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Door u smert en bitter <rhyme label="b" type="m">doodt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Doet my toch na mijn <rhyme label="a" type="f" corresp="#s1:l1">begheeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als ick lich in stervens <rhyme label="b" type="m" corresp="#s1:l2">noot</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Wilt u dan tot mij-waerts <rhyme label="a" type="f" corresp="#s1:l1">keeren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En ontfanghen in u <rhyme label="b" type="m" corresp="#s1:l2">schoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
