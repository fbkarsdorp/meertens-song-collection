<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21194</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
    </tags>
    <melody>Blijdschap van mij vlied</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Haerlem vast <rhyme label="a" type="m">ghebouwt</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Volckrijck, en vol <rhyme label="b" type="f">Neeringh</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Als men <rhyme label="a" type="m" corresp="#s1:l1">aenschouwt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Lust brenght lofs <rhyme label="b" type="f" corresp="#s1:l2">vermeeringh</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Ghy light in <rhyme label="c" type="m">Hollandt</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Daer elck woont in <rhyme label="d" type="f">vreden</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Lustigh, en <rhyme label="c" type="m" corresp="#s1:l5">playsant</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Als een Hooft der <rhyme label="d" type="f" corresp="#s1:l6">Steden</rhyme>.</l>
        <l id="s1:l9" met="3" type="+-">Wall', en Muer ter <rhyme label="e" type="m">noodt</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Hebt ghy voor <rhyme label="e" type="m" corresp="#s1:l9">aenstoot</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Met een kloecke <rhyme label="f" type="m">Borghery</rhyme>:</l>
        <l id="s1:l12" met="3" type="+-">Waters kleyn en <rhyme label="e" type="m" corresp="#s1:l9">groot</rhyme></l>
        <l id="s1:l13" met="3" type="+-">Vloeyen in den <rhyme label="e" type="m" corresp="#s1:l9">Schoot</rhyme></l>
        <l id="s1:l14" met="4" type="+-">Van u, vors en klaer daer <rhyme label="f" type="m" corresp="#s1:l11">by</rhyme>:</l>
        <l id="s1:l15" met="3" type="+-">Noch komt in u <rhyme label="g" type="f">spruyten</rhyme></l>
        <l id="s1:l16" met="3" type="+-">Een Rivier, die <rhyme label="g" type="f" corresp="#s1:l15">uyten</rhyme></l>
        <l id="s1:l17" met="4" type="+-">Drooghen Duyn haer voedtsel <rhyme label="h" type="m">haelt</rhyme>:</l>
        <l id="s1:l18" met="3" type="+-">Dat den Brouwer <rhyme label="g" type="f" corresp="#s1:l15">buyten</rhyme></l>
        <l id="s1:l19" met="3" type="+-">Doet halen met <rhyme label="g" type="f" corresp="#s1:l15">Schuyten</rhyme>,</l>
        <l id="s1:l20" met="4" type="+-">Van daer 't klaer en suyver <rhyme label="h" type="m" corresp="#s1:l17">daelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wandel-gheest treedt <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Vyt de Stadt wilt <rhyme label="b" type="f">wijcken</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Stapt buyten de <rhyme label="a" type="m" corresp="#s2:l1">Poort</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Gaet eens rondtom <rhyme label="b" type="f" corresp="#s2:l2">kijcken</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Hoven, Thuynen <rhyme label="c" type="m">veel</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Toonen u seer <rhyme label="d" type="f">cierigh</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Een Aerdtsch Luft-hoff <rhyme label="c" type="m" corresp="#s2:l5">eel</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Schoon beplant, <rhyme label="d" type="f" corresp="#s2:l6">playsierigh</rhyme>:</l>
        <l id="s2:l9" met="3" type="+-">Komt ghy <rhyme label="e" type="m">onverstout</rhyme></l>
        <l id="s2:l10" met="3" type="+-">In den groenen <rhyme label="e" type="m" corresp="#s2:l9">Hout</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Daer sult ghy met vreemt <rhyme label="f" type="m">gheschal</rhyme>,</l>
        <l id="s2:l12" met="3" type="+-">Door 't belovert <rhyme label="e" type="m" corresp="#s2:l9">Wout</rhyme></l>
        <l id="s2:l13" met="3" type="+-">Hooren <rhyme label="e" type="m" corresp="#s2:l9">menighfout</rhyme></l>
        <l id="s2:l14" met="4" type="+-">De Pluym-dierkens <rhyme label="f" type="m" corresp="#s2:l11">over-al</rhyme>,</l>
        <l id="s2:l15" met="3" type="+-">Vreuchdigh <rhyme label="g" type="f">triumpheren</rhyme>:</l>
        <l id="s2:l16" met="3" type="+-">Streckt dan u <rhyme label="g" type="f" corresp="#s2:l15">begheeren</rhyme></l>
        <l id="s2:l17" met="4" type="+-">Over 't veldt, nae Emaus <rhyme label="h" type="m">padt</rhyme>:</l>
        <l id="s2:l18" met="3" type="+-">Soo ghy Rooms kondt <rhyme label="g" type="f" corresp="#s2:l15">leeren</rhyme>,</l>
        <l id="s2:l19" met="3" type="+-">Ghy meught daer <rhyme label="g" type="f" corresp="#s2:l15">pauseeren</rhyme>,</l>
        <l id="s2:l20" met="4" type="+-">En maecken u top-zeyl <rhyme label="h" type="m" corresp="#s2:l17">nat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wilt oock mee <rhyme label="a" type="m">besien</rhyme></l>
        <l id="s3:l2" met="3" type="+-">De Heemsteesche <rhyme label="b" type="f">Weyen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Wilt u tot <rhyme label="a" type="m" corresp="#s3:l1">verblien</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Voorby 't Slot <rhyme label="b" type="f" corresp="#s3:l2">vermeyen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Neffens 't Bouwsel <rhyme label="c" type="m">deur</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Daer den Somer <rhyme label="d" type="f">blijcklijck</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Nu tooght in sijn <rhyme label="c" type="m" corresp="#s3:l5">fleur</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Milden zeghen, <rhyme label="d" type="f" corresp="#s3:l6">rijcklijck</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Soo raeckt ghy eer <rhyme label="e" type="m">lanck</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Op de Berghen <rhyme label="e" type="m" corresp="#s3:l9">blanck</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Waer in 't Wildt seer dertel <rhyme label="f" type="m">sprinckt</rhyme>:</l>
        <l id="s3:l12" met="3" type="+-">Neemt voort uwen <rhyme label="e" type="m" corresp="#s3:l9">ganck</rhyme></l>
        <l id="s3:l13" met="3" type="+-">Tot den <rhyme label="e" type="m" corresp="#s3:l9">Voghel-zanck</rhyme>,</l>
        <l id="s3:l14" met="4" type="+-">En den Aerden-Hout <rhyme label="f" type="m" corresp="#s3:l11">bedinckt</rhyme>;</l>
        <l id="s3:l15" met="3" type="+-">Wilt een weynigh <rhyme label="g" type="f">rusten</rhyme>,</l>
        <l id="s3:l16" met="3" type="+-">Treckt dan voort met <rhyme label="g" type="f" corresp="#s3:l15">lusten</rhyme></l>
        <l id="s3:l17" met="4" type="+-">By de spitse Zanden <rhyme label="h" type="m">heen</rhyme>,</l>
        <l id="s3:l18" met="3" type="+-">Met een <rhyme label="g" type="f" corresp="#s3:l15">ongheblusten</rhyme></l>
        <l id="s3:l19" met="3" type="+-">Yver, nae de <rhyme label="g" type="f" corresp="#s3:l15">Kusten</rhyme></l>
        <l id="s3:l20" met="4" type="+-">Of Palen van <rhyme label="h" type="m" corresp="#s3:l17">Over-Veen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">'t Over-Veens <rhyme label="a" type="m">ghesticht</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Trappelt wat <rhyme label="b" type="f">bezyen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Ghy krijght in 't <rhyme label="a" type="m" corresp="#s4:l1">ghesicht</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Puycke <rhyme label="b" type="f" corresp="#s4:l2">Bleyckeryen</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">Bloemendaels <rhyme label="c" type="m">versoeck</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Doet tot vreughts <rhyme label="d" type="f">vermeeren</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Ghy siet daer seer <rhyme label="c" type="m" corresp="#s4:l5">kloeck</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Gieten, rapen, <rhyme label="d" type="f" corresp="#s4:l6">keeren</rhyme>.</l>
        <l id="s4:l9" met="3" type="+-">Wandeldt <rhyme label="e" type="m">onbedaert</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Tot het oudt <rhyme label="e" type="m" corresp="#s4:l9">vermaert</rhyme></l>
        <l id="s4:l11" met="4" type="+-">Huys, in Breeroes <rhyme label="f" type="m">Heerlijckheydt</rhyme>,</l>
        <l id="s4:l12" met="3" type="+-">Komt dan langhst de <rhyme label="e" type="m" corresp="#s4:l9">Vaert</rhyme>,</l>
        <l id="s4:l13" met="3" type="+-">Die den Sander naer <rhyme label="e" type="m" corresp="#s4:l9">'t</rhyme></l>
        <l id="s4:l14" met="4" type="+-">Spaerne eertijdts heeft <rhyme label="f" type="m" corresp="#s4:l11">gheleydt</rhyme>,</l>
        <l id="s4:l15" met="3" type="+-">Tot ghewin <rhyme label="g" type="f">noodtsaecklijck</rhyme>,</l>
        <l id="s4:l16" met="3" type="+-">Deur de groent' <rhyme label="g" type="f" corresp="#s4:l15">vermaecklijck</rhyme>,</l>
        <l id="s4:l17" met="4" type="+-">Daer de Leeuwerck daelt en <rhyme label="h" type="m">rijst</rhyme>;</l>
        <l id="s4:l18" met="3" type="+-">Daer het Vee <rhyme label="g" type="f" corresp="#s4:l15">onspraecklijck</rhyme></l>
        <l id="s4:l19" met="3" type="+-">Graeft, met graegheydt <rhyme label="g" type="f" corresp="#s4:l15">smaecklijck</rhyme>,</l>
        <l id="s4:l20" met="4" type="+-">Daer den Mensch af wordt <rhyme label="h" type="m" corresp="#s4:l17">ghespijst</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Prins, als ghy dan <rhyme label="a" type="m">raeckt</rhyme></l>
        <l id="s5:l2" met="3" type="+-">By de Schooter <rhyme label="b" type="f">Boeren</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">En de Stadt <rhyme label="a" type="m" corresp="#s5:l1">ghenaeckt</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Wilt dan voort <rhyme label="b" type="f" corresp="#s5:l2">beloeren</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Het Treck-Vaerts <rhyme label="c" type="m">ghenot</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Zijnd' een groot <rhyme label="d" type="f">gherijven</rhyme>:</l>
        <l id="s5:l7" met="3" type="+-">Wilt dan voorder <rhyme label="c" type="m" corresp="#s5:l5">tot</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Rusten-burgh heen <rhyme label="d" type="f" corresp="#s5:l6">drijven</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Voort op alles <rhyme label="e" type="m">let</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Tot vermaecken <rhyme label="e" type="m" corresp="#s5:l9">bet</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Dicht aen Schoolenaers ghy <rhyme label="f" type="m">los</rhyme></l>
        <l id="s5:l12" met="3" type="+-">Wordt <rhyme label="e" type="m" corresp="#s5:l9">overgheset</rhyme>,</l>
        <l id="s5:l13" met="3" type="+-">Soo komt ghy weer <rhyme label="e" type="m" corresp="#s5:l9">net</rhyme></l>
        <l id="s5:l14" met="4" type="+-">Neffens 't aenghename <rhyme label="f" type="m" corresp="#s5:l11">Bos</rhyme>,</l>
        <l id="s5:l15" met="3" type="+-">Nae de Stadt <rhyme label="g" type="f">ghestreecken</rhyme>:</l>
        <l id="s5:l16" met="3" type="+-">Wilt dan recht uyt <rhyme label="g" type="f" corresp="#s5:l15">spreecken</rhyme>,</l>
        <l id="s5:l17" met="4" type="+-">Of dit niet in u <rhyme label="h" type="m">ghedacht</rhyme></l>
        <l id="s5:l18" met="3" type="+-">Helder kan <rhyme label="g" type="f" corresp="#s5:l15">deur-breecken</rhyme>,</l>
        <l id="s5:l19" met="3" type="+-">En sijn glants <rhyme label="g" type="f" corresp="#s5:l15">op-steecken</rhyme></l>
        <l id="s5:l20" met="4" type="+-">Boven vele Ste'en, vol <rhyme label="h" type="m" corresp="#s5:l17">pracht</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
