<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186898</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Het kuipertje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O Gy arme kaele <rhyme label="a" type="m">Brit</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wat doet u zoo moedig <rhyme label="b" type="f">wezen</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">'t Is Neerlands geld dat gy <rhyme label="a" type="m" corresp="#s1:l1">bezit</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Gy moog vry ons woede <rhyme label="b" type="f" corresp="#s1:l2">vrezen</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Want u magt moet aen een <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Gelyk de Teems eens was in <rhyme label="c" type="m" corresp="#s1:l5">brand</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Door de, door de kragt van 't <rhyme label="d" type="m">vuur</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">U moed zal niet zyn van <rhyme label="d" type="m" corresp="#s1:l7">duur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Geen Vorst heeft dit ooit <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat ge 么 Brit hier durft <rhyme label="b" type="f">beginnen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Om onze Overheid te <rhyme label="a" type="m" corresp="#s2:l1">smaen</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">Dog onz' magt zal altyd <rhyme label="b" type="f" corresp="#s2:l2">winnen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Want de magten van ons <rhyme label="c" type="m">Land</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Zyn tegen u 么 Dog <rhyme label="c" type="m" corresp="#s2:l5">bestand</rhyme></l>
        <l id="s2:l7" met="4" type="+-">En zyn, en zyn tot ons <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Altyd klaer tot <rhyme label="d" type="m" corresp="#s2:l7">tegenweer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wild gy onbesonne <rhyme label="a" type="m">Dog</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dan ons Nederland <rhyme label="b" type="f">regeeren</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Neen 么 Brit wy hebben <rhyme label="a" type="m" corresp="#s3:l1">nog</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Helden die u kunnen <rhyme label="b" type="f" corresp="#s3:l2">keeren</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">In zoo vals en boos <rhyme label="c" type="m">gedrag</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En die u met elke <rhyme label="c" type="m" corresp="#s3:l5">slag</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Kunne, kunne selle <rhyme label="d" type="m">neer</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Tot ons aller roem en <rhyme label="d" type="m" corresp="#s3:l7">eer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Neem vry al ons Scheepen <rhyme label="a" type="m">weg</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En doet maar na u <rhyme label="b" type="f">believen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Maer het geen dat ik u <rhyme label="a" type="m" corresp="#s4:l1">zeg</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Is waer en dat zonder <rhyme label="b" type="f" corresp="#s4:l2">liegen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dat het slegt met u zal <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Komt het op Betalen <rhyme label="c" type="m" corresp="#s4:l5">aen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Want u, want u kwynend <rhyme label="d" type="m">Land</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Heeft niets meer tot <rhyme label="d" type="m" corresp="#s4:l7">onderstand</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gy 么 Eng'land maakt u <rhyme label="a" type="m">thans</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Tot een spot voor alle <rhyme label="b" type="f">Menschen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Gy krygt nooit een <rhyme label="a" type="m" corresp="#s5:l1">eere-krans</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Al is dit steeds ook u <rhyme label="b" type="f" corresp="#s5:l2">wenschen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Want die eer is voor een <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Die Edelmoedig stryde <rhyme label="c" type="m" corresp="#s5:l5">kan</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Niet voor, niet voor 't Engels <rhyme label="d" type="m">bloed</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Want dat ons hier Zinge <rhyme label="d" type="m" corresp="#s5:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Mal en Arm zyt gy <rhyme label="a" type="m">Brit</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ryk en Wys de <rhyme label="b" type="f">Bataviere</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Gy geraekt nooit tot u <rhyme label="a" type="m" corresp="#s6:l1">wit</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Want als onze <rhyme label="b" type="f" corresp="#s6:l2">Officieren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Alle op u valle <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Moet uw magt gewis <rhyme label="c" type="m" corresp="#s6:l5">vergaen</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Dat is, dat is zo <rhyme label="d" type="m">gewis</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Als tweemael zeven veertien <rhyme label="d" type="m" corresp="#s6:l7">is</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
