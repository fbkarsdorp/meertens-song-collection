<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20417</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pots honderdduizend slapperment</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk ongheluckigh <rhyme label="a" type="f">Vryer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe ben ick in de <rhyme label="b" type="m">ly</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Ick kan gheen Vrijster <rhyme label="c" type="f">winnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hoe dapper dat ick <rhyme label="b" type="m" corresp="#s1:l2">vry</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Sy achten 't maer voor <rhyme label="c" type="f" corresp="#s1:l3">grillen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">'t En klemt niet wat ick <rhyme label="d" type="m">doe</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Sal dit noch langher <rhyme label="e" type="f">dueren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ick wordt mijn leven <rhyme label="d" type="m" corresp="#s1:l6">moe</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Jonghman waer toe dit <rhyme label="a" type="f">klaghen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ey! laet u grillen <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">'k En sagh van al mijn <rhyme label="c" type="f">daghen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Noyt sulcken vreemden <rhyme label="b" type="m" corresp="#s2:l2">Haen</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Ghy reutelt en ghy <rhyme label="c" type="f" corresp="#s2:l3">rammelt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Als of ghy had' een <rhyme label="d" type="m">gons</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ghy bongioert selfs in jou <rhyme label="e" type="f">Bocxsen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En leght die schult op <rhyme label="d" type="m" corresp="#s2:l6">ons</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy loopt by al die <rhyme label="a" type="f">Vrijsters</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En maeckt veel waters <rhyme label="b" type="m">vuyl</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Dan by een Mennist <rhyme label="c" type="f">Susje</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Of by een <rhyme label="b" type="m" corresp="#s3:l2">Kerrick-Vyl</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Met suycker-soete <rhyme label="c" type="f" corresp="#s3:l3">woorden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Payt ghy den gantschen <rhyme label="d" type="m">hoop</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Het schijnen mooye <rhyme label="e" type="f">Boonen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Doch woorden zijn <rhyme label="d" type="m" corresp="#s3:l6">goetkoop</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vindt ghy een aerdigh <rhyme label="a" type="f">Meysje</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Van Lichaem schoon en <rhyme label="b" type="m">fris</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghy doet haer strackx <rhyme label="c" type="f">ghelooven</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat sy de Liefste <rhyme label="b" type="m" corresp="#s4:l2">is</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Ghy steelt haer hart en <rhyme label="c" type="f" corresp="#s4:l3">sinnen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Sy meent sy is de <rhyme label="d" type="m">Bruyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Doch 't Meysje is <rhyme label="e" type="f">bedroghen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">De Vryer scheyter <rhyme label="d" type="m" corresp="#s4:l6">uyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'T is waer, ick dien de <rhyme label="a" type="f">Vrijsters</rhyme></l>
        <l id="s5:l2" met="3" type="-+">De Soolen uyt haer <rhyme label="b" type="m">Schoen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ick soeck haer te <rhyme label="c" type="f">behaghen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wat soud' ick anders <rhyme label="b" type="m" corresp="#s5:l2">doen</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">'t Streckt al tot naerder <rhyme label="c" type="f" corresp="#s5:l3">kennis</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Tot aenghenaem <rhyme label="d" type="m">vermaeck</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Maer stracks de knoop de <rhyme label="e" type="f">sluyten</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat is een zware <rhyme label="d" type="m" corresp="#s5:l6">saeck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Het sijn wel soete <rhyme label="a" type="f">Hertjens</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die waerdigh sijn <rhyme label="b" type="m">bemint</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Maer 'k wil soo hart niet <rhyme label="c" type="f">loopen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Als of ick waer <rhyme label="b" type="m" corresp="#s6:l2">verblindt</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Sy schijnen schier <rhyme label="c" type="f" corresp="#s6:l3">Goddinnen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Gekleet als Juffers <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Groots-hertigh, kleyn van <rhyme label="e" type="f">Have</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat staet my qualijck <rhyme label="d" type="m" corresp="#s6:l6">aen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maer een soet aerdich <rhyme label="a" type="f">Buysje</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Van middelbaren <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Die matigh weet te <rhyme label="c" type="f">voeghen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Haer Kleedingh en <rhyme label="b" type="m" corresp="#s7:l2">ghelaet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Niet grootsich in haer <rhyme label="c" type="f" corresp="#s7:l3">wapen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Vol sedigheyt en <rhyme label="d" type="m">deught</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Mocht my sulck een <rhyme label="e" type="f">ghebeuren</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Soo was mijn Hart <rhyme label="d" type="m" corresp="#s7:l6">verheught</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O Hooft vol <rhyme label="a" type="f">Muysen-nesten</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">Hoe krijght ghy noch een <rhyme label="b" type="m">Wijf</rhyme>?</l>
        <l id="s8:l3" met="3" type="-+">Al dese viese <rhyme label="c" type="f">vaesen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">En hebben niet om 't <rhyme label="b" type="m" corresp="#s8:l2">lijf</rhyme>?</l>
        <l id="s8:l5" met="3" type="-+">Dan schortet aen de <rhyme label="c" type="f" corresp="#s8:l3">kleedingh</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En dan aen gelt en <rhyme label="d" type="m">schat</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">Dan hier aen en dan daer <rhyme label="e" type="f">aen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Daer hapert altijt <rhyme label="d" type="m" corresp="#s8:l6">wat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
