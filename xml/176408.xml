  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wat een vreugd is ons <rhyme label="a" type="f">geboren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In ons dierbaar <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Nu men de Vrede komt na <rhyme label="c" type="f">horen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Tusschen ons en <rhyme label="b" type="m" corresp="#s1:l2">Engeland</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Die ons de vryheid reeds <rhyme label="d" type="f">bepalen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Te zyn verlost van ’t oorlogs <rhyme label="e" type="m">vuur</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Roepen my vivat <rhyme label="f" type="f">Colonialen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Wy zyn Jagers van <rhyme label="e" type="m" corresp="#s1:l6">plaisier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Men is reeds nu <rhyme label="a" type="f">ingenomen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Met gedagten van <rhyme label="b" type="m">vertrek</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Om ons oogmerk te <rhyme label="c" type="f">bekomen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Van te gaan na Oost en <rhyme label="b" type="m" corresp="#s2:l2">West</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Na het yland van <rhyme label="d" type="f">Surname</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Of na de Kaap de goede <rhyme label="e" type="m">Hoop</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En dan naa het <rhyme label="f" type="f">Dimmerarie</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">En de Es…quebo <rhyme label="e" type="m" corresp="#s2:l6">oock</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">o! Hoe zal men daar <rhyme label="a" type="f">floreeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">In het land waar de Suiker <rhyme label="b" type="m">groeit</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Appelon Zina daar <rhyme label="c" type="f">beneven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">d’Adels en Vygen in <rhyme label="b" type="m" corresp="#s3:l2">overvloed</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Coffy, Thee al wat heeft <rhyme label="d" type="f">namen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En Catoen voor Kleeren <rhyme label="e" type="m">fyn</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Roepen wy vivat <rhyme label="f" type="f">Surnamen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Als wy daar maar eens in <rhyme label="e" type="m" corresp="#s3:l6">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hoe zullen zy ons <rhyme label="a" type="f">geriefen</rhyme>.</l>
        <l id="s4:l2" met="4" type="+-">De zwarte Meisjes van dat <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Zy zyn dartel en vol van <rhyme label="c" type="f">liefde</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Voor een blanke uit <rhyme label="b" type="m" corresp="#s4:l2">Holland</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Zy zyn fraai van <rhyme label="d" type="f">Ledemaaten</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Fyn van haar en zagt van <rhyme label="e" type="m">vel</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">En van onder wel <rhyme label="f" type="f">geschapen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Haar Borsjes hebben de groote <rhyme label="e" type="m" corresp="#s4:l6">wel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Men zal daar geen kou <rhyme label="a" type="f">gevoelen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dan de winter daar niet <rhyme label="b" type="m">schaad</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Het is altyd volle <rhyme label="c" type="f">zomer</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Lieffelyk en <rhyme label="b" type="m" corresp="#s5:l2">delikaat</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Wat zal men daar een vreugd <rhyme label="d" type="f">beleeven</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">In dat warm en dierbaar <rhyme label="e" type="m">Land</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Daar den Hemel geeft de <rhyme label="f" type="f">zegen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Ook vergunt aan alle <rhyme label="e" type="m" corresp="#s5:l6">kant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laat ons dan met blydschap <rhyme label="a" type="f">scheiden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Van ons Ouders en <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En van alle lieve <rhyme label="c" type="f">Meisjes</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Die wy laaten in <rhyme label="b" type="m" corresp="#s6:l2">Holland</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Adju dan Vaderland en <rhyme label="d" type="f">vrinden</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Adju Meisjes veel <rhyme label="e" type="m">geagt</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Wy hoopen u weer te <rhyme label="f" type="f">vinden</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Misschien over een jaar of <rhyme label="e" type="m" corresp="#s6:l6">agt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
