<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>117255</id>
    <date>
      <year-start>1718</year-start>
      <year-end>1718</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het is nu ruim vijf jaar geleden</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MEysje Maet bent gy <rhyme label="a" type="f">beraden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Voor myn deel ik werd al <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">‘k Ben met Liefde <rhyme label="a" type="f" corresp="#s1:l1">overladen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Geeft myn nou een reys een <rhyme label="b" type="m" corresp="#s1:l2">Soen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">‘k Ben gereed toe brave <rhyme label="c" type="m">Meyd</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">‘k Wacht al met <rhyme label="c" type="m" corresp="#s1:l5">langmoedigheyd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wel hoe Zymen is dat <rhyme label="a" type="f">sollen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Laet dit Fikke fakken <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Weg met al dat <rhyme label="a" type="f" corresp="#s2:l1">sottebollen</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Meysje Maet laet my <rhyme label="b" type="m" corresp="#s2:l2">begaen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En laet staen al dat <rhyme label="c" type="m">gebruy</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Sie daer ginder komen <rhyme label="c" type="m" corresp="#s2:l5">Luy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Tryntje Lief nou toe een <rhyme label="a" type="f">reysje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Zoen weerom wel dat is <rhyme label="b" type="m">moy</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Och! wat hebje Poezel <rhyme label="a" type="f" corresp="#s3:l1">Vleysje</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">‘k Wou so graeg met jou na <rhyme label="b" type="m" corresp="#s3:l2">Koy</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">‘k Sou dan voele dat is <rhyme label="c" type="m">raer</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Waerje aengewassen <rhyme label="c" type="m" corresp="#s3:l5">waer</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Waerje een Pampiere <rhyme label="a" type="f">Solder</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">‘k Sou gelyk de Iongens <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Op jou loopen, holder, <rhyme label="a" type="f" corresp="#s4:l1">bolder</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">‘k Ben op sulken Kleyweg <rhyme label="b" type="m" corresp="#s4:l2">groen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Al sou ik dan in jou ter <rhyme label="c" type="m">sluyk</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Heel insakken tot myn <rhyme label="c" type="m" corresp="#s4:l5">buyk</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Want de Meysjes <rhyme label="a" type="f">altemale</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Zyn de modder gants <rhyme label="b" type="m">gelyk</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Glad van buyk, ruyg by de <rhyme label="a" type="f" corresp="#s5:l1">pale</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Ia geheel gelyk de <rhyme label="b" type="m" corresp="#s5:l2">slyk</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Komjer op je sakt <rhyme label="c" type="m">terstond</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">In haer sagte modder <rhyme label="c" type="m" corresp="#s5:l5">grond</rhyme>:</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daerom laet ik eens <rhyme label="a" type="f">probere</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Of jou soldertje is <rhyme label="b" type="m">droog</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Leg maer neer, sie daer ik <rhyme label="a" type="f" corresp="#s6:l1">zwere</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Duympjen op, dien Eed is <rhyme label="b" type="m" corresp="#s6:l2">hoog</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">So ik jou eens seer sal <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Segt dan vry ’t is geen <rhyme label="c" type="m" corresp="#s6:l5">fatzoen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Zymen laet die sotheyd <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Siet je kreukeld al myn <rhyme label="b" type="m">goed</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Nou ’t is wel ik sal ’t <rhyme label="a" type="f" corresp="#s7:l1">verklaren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Datje myn dit leet aen <rhyme label="b" type="m" corresp="#s7:l2">doet</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Gaet dan nou maer vry jou <rhyme label="c" type="m">gang</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Maer maekt het toch niet al <rhyme label="c" type="m" corresp="#s7:l5">t’lang</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Is ’t nou wel jou malle <rhyme label="a" type="f">Zotje</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Hebje nou jou Rol <rhyme label="b" type="m">gespeeld</rhyme>?</l>
        <l id="s8:l3" met="4" type="+-">Kyk nou siet hy als een <rhyme label="a" type="f" corresp="#s8:l1">Otje</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Toe nou, wie het eerst <rhyme label="b" type="m" corresp="#s8:l2">verveeld</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Maer de droely ’t is jou <rhyme label="c" type="m">schuld</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Als ik nou zwel met een <rhyme label="c" type="m" corresp="#s8:l5">bult</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
