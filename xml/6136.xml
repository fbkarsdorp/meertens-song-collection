    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Doot, o bitter <rhyme label="a" type="m">doot</rhyme> !</l>
        <l id="s1:l2" met="3" type="-+">Ghy spaert noch groot noch <rhyme label="b" type="f">clenen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ontneemt goet ende <rhyme label="a" type="m" corresp="#s1:l1">moet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Brengt ons droefheyt en <rhyme label="b" type="f" corresp="#s1:l2">wenen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s1:l5">bly</rhyme>;</l>
        <l id="s1:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s1:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe veel Lantsheeren <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sijn laes van u <rhyme label="b" type="f">verslonden</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hoe menich helt <rhyme label="a" type="m" corresp="#s2:l1">voorwaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In alsoo corte <rhyme label="b" type="f" corresp="#s2:l2">stonden</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s2:l5">bly</rhyme>;</l>
        <l id="s2:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s2:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat Princen oyt men <rhyme label="a" type="m">sach</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Alhier in groter <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">In eenen ooch <rhyme label="a" type="m" corresp="#s3:l1">opslach</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sachmen hier al <rhyme label="b" type="f" corresp="#s3:l2">passeren</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s3:l5">bly</rhyme>;</l>
        <l id="s3:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s3:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Met haren gelen <rhyme label="a" type="m">hayr</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Waer sijn die maegden <rhyme label="b" type="f">schoone</rhyme>?</l>
        <l id="s4:l3" met="3" type="-+">Om haer schoon aensicht <rhyme label="a" type="m" corresp="#s4:l1">claer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die spanden hier de <rhyme label="b" type="f" corresp="#s4:l2">croone</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s4:l5">bly</rhyme>;</l>
        <l id="s4:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s4:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'T sy edel, of <rhyme label="a" type="m">vileyn</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Niemant en is <rhyme label="b" type="f">gebleven</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die wrede doot <rhyme label="a" type="m" corresp="#s5:l1">certeyn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Eylaes sijn sy <rhyme label="b" type="f" corresp="#s5:l2">gegeven</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s5:l5">bly</rhyme>;</l>
        <l id="s5:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s5:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sy sijn nu <rhyme label="a" type="m">allegaer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Al van die wormen <rhyme label="b" type="f">geten</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hoe schoon, hoe rijck, hoe <rhyme label="a" type="m" corresp="#s6:l1">claer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Haer schoonheyt is <rhyme label="b" type="f" corresp="#s6:l2">versleten</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s6:l5">bly</rhyme>;</l>
        <l id="s6:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s6:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wat baet ons <rhyme label="a" type="m">overdaet</rhyme>?</l>
        <l id="s7:l2" met="3" type="-+">Wat baet dansen en <rhyme label="b" type="f">springen</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Wat baet ons stout <rhyme label="a" type="m" corresp="#s7:l1">gelaet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Die doot sal ons <rhyme label="b" type="f" corresp="#s7:l2">aendringen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s7:l5">bly</rhyme>;</l>
        <l id="s7:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s7:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O doot, o bitter <rhyme label="a" type="m">doot</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">En kanmen u niet <rhyme label="b" type="f">vlieden</rhyme>?</l>
        <l id="s8:l3" met="3" type="-+">Aensiet ghy goet noch <rhyme label="a" type="m" corresp="#s8:l1">moet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Moet ick oock aldus <rhyme label="b" type="f" corresp="#s8:l2">scheyden</rhyme>?</l>
        <l id="s8:l5" met="3" type="-+">Eylaes moet soo <rhyme label="c" type="m">voorby</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Soo ras ons leven <rhyme label="c" type="m" corresp="#s8:l5">bly</rhyme>;</l>
        <l id="s8:l7" met="4" type="-+">O vrede doot! hoe bitter sijt <rhyme label="c" type="m" corresp="#s8:l5">ghy</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wat is dan mijnen <rhyme label="a" type="m">troost</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In dit verganckelijck <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Dan dat ick Godt <rhyme label="a" type="m" corresp="#s9:l1">altoost</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Met herten mach <rhyme label="b" type="f" corresp="#s9:l2">aencleven</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Het moet toch al voor <rhyme label="c" type="m">by</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Soo ras mijn leven <rhyme label="c" type="m" corresp="#s9:l5">bly</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">O vrede doot! bitter sijt <rhyme label="c" type="m" corresp="#s9:l5">ghy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
