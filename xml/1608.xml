    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="occasions">gelegenheden</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ADieu Antwerpen ghenoechlijc <rhyme label="a" type="m">pleyn</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Van v so moet ick <rhyme label="b" type="f">scheyden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Ic laet daer in tliefste <rhyme label="a" type="m" corresp="#s1:l1">greyn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">God wil mijn boel <rhyme label="b" type="f" corresp="#s1:l2">geleyden</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als ic wten lande varen <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Al binnen den eersten <rhyme label="b" type="f">iaer</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Hoe dic verwandelt mijn <rhyme label="a" type="m" corresp="#s2:l1">bloet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ic sterue een <rhyme label="b" type="f" corresp="#s2:l2">martelaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ick vaer ouer die <rhyme label="a" type="m">zee</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Van rouwe wil ic <rhyme label="b" type="f">steruen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">dat doet mijn ionc herte <rhyme label="a" type="m" corresp="#s3:l1">wee</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Ick sal te male <rhyme label="b" type="f" corresp="#s3:l2">bederuen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Och venus vrouwe voeret mijnen <rhyme label="a" type="m">schilt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Aen v roepe ick <rhyme label="b" type="f">ghenade</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Helpt mi dat ick mijn boel <rhyme label="a" type="m" corresp="#s4:l1">ghewin</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ende staet mi nv te <rhyme label="b" type="f" corresp="#s4:l2">stade</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat iaer is lanck  mijn loon is <rhyme label="a" type="m">cranck</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Mijn hope heb ic <rhyme label="b" type="f">verloren</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Had ic mijn boel bider <rhyme label="a" type="m" corresp="#s5:l1">hant</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Geen goet en core ic daer <rhyme label="b" type="f" corresp="#s5:l2">voren</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Och lacen si en acht niet meer op <rhyme label="a" type="m">mi</rhyme></l>
        <l id="s6:l2" met="3" type="-+">des lijdt mijn herte <rhyme label="b" type="f">rouwe</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Si heeft een ander lieuer dan <rhyme label="a" type="m" corresp="#s6:l1">mi</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Si was mi noyt <rhyme label="b" type="f" corresp="#s6:l2">ghetrouwe</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die dit liedeken heeft <rhyme label="a" type="m">ghedicht</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hi heuet wel <rhyme label="b" type="f">ghesongen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Ter eeren van zijn soete <rhyme label="a" type="m" corresp="#s7:l1">lief</rhyme></l>
        <l id="s7:l4" met="3" type="-+">God sceyn der nijders <rhyme label="b" type="f" corresp="#s7:l2">tonghen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
