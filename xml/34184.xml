  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wie gheeft de kracht aen myn <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat sy myn hertje dwinghen <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Veel beter als den stercksten <rhyme label="b" type="m" corresp="#s1:l2">man</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Weet sy myn vryheydt vast te <rhyme label="a" type="f" corresp="#s1:l1">binden</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wie gheeft haer die <rhyme label="c" type="m">vooghdy</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat sy ghebiet aen <rhyme label="c" type="m" corresp="#s1:l5">my</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">Elck <rhyme label="d" type="m">lonck</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">Een <rhyme label="d" type="m" corresp="#s1:l7">vonck</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Schieten haer <rhyme label="e" type="f">ooghen</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">In myn jongh <rhyme label="f" type="m">hert</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Met groote <rhyme label="f" type="m" corresp="#s1:l10">smert</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Moet ick't <rhyme label="e" type="f" corresp="#s1:l9">ghedooghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wanneer de straeltjens van haer <rhyme label="a" type="f">ooghen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Bescheynen myn verlieft <rhyme label="b" type="m">ghemoet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ach! tis voor my te swaere <rhyme label="b" type="m" corresp="#s2:l2">boet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat haer ghesicht, heeft sulck <rhyme label="a" type="f" corresp="#s2:l1">vermooghen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">'kGheloof dat <rhyme label="c" type="m">'tMinne-wicht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Haer vrindelyck <rhyme label="c" type="m" corresp="#s2:l5">ghesicht</rhyme>,</l>
        <l id="s2:l7" met="1" type="-+">Voor <rhyme label="d" type="m">troon</rhyme>,</l>
        <l id="s2:l8" met="1" type="-+">En <rhyme label="d" type="m" corresp="#s2:l7">woon</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Plaets, heeft <rhyme label="e" type="f">verkoren</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Daer hy myn <rhyme label="f" type="m">macht</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Door syne <rhyme label="f" type="m" corresp="#s2:l10">kracht</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">Doet heel <rhyme label="e" type="f" corresp="#s2:l9">verloren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Soo niet, soo is het een <rhyme label="a" type="f">Goddinne</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die ick bemin, maer gheenen <rhyme label="b" type="m">mensch</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">'tIs Venus die door-siet myn <rhyme label="b" type="m" corresp="#s3:l2">wensch</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En komt beproeven myne <rhyme label="a" type="f" corresp="#s3:l1">sinnen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Want sulcken liefdens <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Is in gheen menschen <rhyme label="c" type="m" corresp="#s3:l5">macht</rhyme>,</l>
        <l id="s3:l7" met="1" type="-+"><rhyme label="d" type="m">Goddin</rhyme>,</l>
        <l id="s3:l8" met="1" type="-+">Die <rhyme label="d" type="m" corresp="#s3:l7">'kmin</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Thoont my <rhyme label="e" type="f">ghenaden</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Wilt myne <rhyme label="f" type="m">gonst</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Door liefdens <rhyme label="f" type="m" corresp="#s3:l10">konst</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Niet meer <rhyme label="e" type="f" corresp="#s3:l9">versmaden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zyt ghy dan Venus <rhyme label="a" type="f">over-schoone</rhyme>?</l>
        <l id="s4:l2" met="4" type="-+">Oft eenen mensch daer ick naer <rhyme label="b" type="m">tracht</rhyme>?</l>
        <l id="s4:l3" met="4" type="-+">Wie dat ghy zyt, aen-hoort myn <rhyme label="b" type="m" corresp="#s4:l2">klacht</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En wilt myn trouwe liefde <rhyme label="a" type="f" corresp="#s4:l1">loonen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Goddin soo ghy het <rhyme label="c" type="m">zyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Maeckt my myn droefheyt <rhyme label="c" type="m" corresp="#s4:l5">quyt</rhyme>,</l>
        <l id="s4:l7" met="1" type="-+">Oft <rhyme label="d" type="m">ghy</rhyme>,</l>
        <l id="s4:l8" met="1" type="-+"><rhyme label="d" type="m" corresp="#s4:l7">Party</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Van myn <rhyme label="e" type="f">ghedachten</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Die door <rhyme label="f" type="m">'tghesicht</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">My hebt <rhyme label="f" type="m" corresp="#s4:l10">verplicht</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+">Wilt myn smert <rhyme label="e" type="f" corresp="#s4:l9">achten</rhyme>.</l>
      </lg>
    </lg>
  </text>
