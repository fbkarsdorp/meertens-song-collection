<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5198</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag>beggar song</tag>
      <tag>wedding song</tag>
      <tag>orange song</tag>
      <tag>praise</tag>
    </tags>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilt nu tsaem vreucht <rhyme label="a" type="f">bedrijven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghy steden in <rhyme label="b" type="m">Hollandt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In Zeelant, Mannen en <rhyme label="a" type="f" corresp="#s1:l1">Wijven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Bedrijft vreucht aen elcken <rhyme label="b" type="m" corresp="#s1:l2">cant</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ter eeren den Prince <rhyme label="c" type="f">verheven</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sijn huysvrou Edel <rhyme label="d" type="m">vermaert</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat God gratie heeft <rhyme label="c" type="f" corresp="#s1:l5">ghegheven</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En trouwelijck <rhyme label="d" type="m" corresp="#s1:l6">bewaert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ten Briel is sy <rhyme label="a" type="f">ghecomen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Al met triumphe <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat hebben die burghers <rhyme label="a" type="f" corresp="#s2:l1">vernomen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Men schoot soo menighen <rhyme label="b" type="m" corresp="#s2:l2">schoot</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Tot des Prins t'sijner <rhyme label="c" type="f">vromen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Men nam haer byder <rhyme label="d" type="m">hant</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Men hiet haer <rhyme label="c" type="f" corresp="#s2:l5">wellecomen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Al in des Princen <rhyme label="d" type="m" corresp="#s2:l6">lant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als den Prins dat heeft <rhyme label="a" type="f">vernomen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van herten was hy <rhyme label="b" type="m">verblijt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hy voer al sonder <rhyme label="a" type="f" corresp="#s3:l1">schromen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Naer den Briel seer <rhyme label="b" type="m" corresp="#s3:l2">subijt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Nae haer was syn <rhyme label="c" type="f">verlanghen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Den Edelen Prince <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hy heeft sijn Bruydt <rhyme label="c" type="f" corresp="#s3:l5">ontfanghen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En vriendelijck <rhyme label="d" type="m" corresp="#s3:l6">ghegroet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men sach doen vreucht <rhyme label="a" type="f">oorboren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ten Briel ick seght u <rhyme label="b" type="m">plat</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Tromlen en trompetten mochtmen <rhyme label="a" type="f" corresp="#s4:l1">hooren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Op het hooft en inde <rhyme label="b" type="m" corresp="#s4:l2">stadt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">T'gheschut wert los <rhyme label="c" type="f">gheschooten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ter eeren den bruydt <rhyme label="d" type="m">minjoot</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ten heeft haer niet <rhyme label="c" type="f" corresp="#s4:l5">verdrooten</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Te schieten soo menighen <rhyme label="d" type="m" corresp="#s4:l6">schoot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Als sy binnen is <rhyme label="a" type="f">ghecomen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die jonckvrou reyn <rhyme label="b" type="m">ghemaniert</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Elck hiet haer <rhyme label="a" type="f" corresp="#s5:l1">wellecomen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Vreucht werter <rhyme label="b" type="m" corresp="#s5:l2">ghehantiert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Op den Toren sachmen <rhyme label="c" type="f">vyeren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Deur die stadt nacht end <rhyme label="d" type="m">dach</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Met lieflijcke <rhyme label="c" type="f" corresp="#s5:l5">manieren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Men hoorde daer gheen <rhyme label="d" type="m" corresp="#s5:l6">gheclach</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Van daer is sy <rhyme label="a" type="f">ghetoghen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Na Dort al metter <rhyme label="b" type="m">spoet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Alsomen sach voor <rhyme label="a" type="f" corresp="#s6:l1">ooghen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">God die heeftse <rhyme label="b" type="m" corresp="#s6:l2">behoet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Trompetten ende <rhyme label="c" type="f">Claroenen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Gaven daer groot <rhyme label="d" type="m">gheluyt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Elck een sachmen daer <rhyme label="c" type="f" corresp="#s6:l5">spoeyen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Om te eeren des Princen <rhyme label="d" type="m" corresp="#s6:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die van Dort cloeck van <rhyme label="a" type="f">aerde</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Waren seer haest <rhyme label="b" type="m">bedacht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Want sy gheen costen <rhyme label="a" type="f" corresp="#s7:l1">spaerden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Maer hebben de Bruyt <rhyme label="b" type="m" corresp="#s7:l2">verwacht</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">De schutters sachmen <rhyme label="c" type="f">treden</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Met vaendels wijt <rhyme label="d" type="m">ontdaen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Coragieus van <rhyme label="c" type="f" corresp="#s7:l5">seden</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Om de Bruyt te <rhyme label="d" type="m" corresp="#s7:l6">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De Borghers met <rhyme label="a" type="f">verlanghen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Liepen daer buyten de <rhyme label="b" type="m">poort</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Om de Bruyt eerlijck <rhyme label="a" type="f" corresp="#s8:l1">t'ontfanghen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Tgheschut wert daer <rhyme label="b" type="m" corresp="#s8:l2">ghehoort</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Men sach pecktonnen <rhyme label="c" type="f">branden</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Door die stadt hier en <rhyme label="d" type="m">daer</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Alle die Papisten ter <rhyme label="c" type="f" corresp="#s8:l5">schanden</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Ter eeren den Prins <rhyme label="d" type="m" corresp="#s8:l6">eerbaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De Heeren vander <rhyme label="a" type="f">stede</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Schout, Schepenen wel <rhyme label="b" type="m">ghemoet</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Burghemeesters en schutters <rhyme label="a" type="f" corresp="#s9:l1">mede</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Die quamen haer int <rhyme label="b" type="m" corresp="#s9:l2">ghemoet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Met vaendels wijt <rhyme label="c" type="f">ontloken</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Seer lustich <rhyme label="d" type="m">triumphant</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">End hebben seer vriendelijck <rhyme label="c" type="f" corresp="#s9:l5">ghesproken</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Weest willecom in <rhyme label="d" type="m" corresp="#s9:l6">Hollant</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dus wilt doch vreucht <rhyme label="a" type="f">hantieren</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ghy steden aen elcken <rhyme label="b" type="m">cant</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En wilt doch vrolijck <rhyme label="a" type="f" corresp="#s10:l1">vyeren</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Ter eeren den Prins <rhyme label="b" type="m" corresp="#s10:l2">valliant</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Danckende God <rhyme label="c" type="f">verheven</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Van dat Babel soo wert <rhyme label="d" type="m">verstoort</rhyme></l>
        <l id="s10:l7" met="3" type="-+">En dat hy ons heeft <rhyme label="c" type="f" corresp="#s10:l5">ghegheven</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Sijn heylighe Godlijcke <rhyme label="d" type="m" corresp="#s10:l6">woort</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ghy meucht u wel <rhyme label="a" type="f">verblijden</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Ghy steden hoogh <rhyme label="b" type="m">vermaert</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Want noyt tot gheenen <rhyme label="a" type="f" corresp="#s11:l1">tijden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">En waert ghy so <rhyme label="b" type="m" corresp="#s11:l2">bewaert</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Met sulcken Edelen <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s11:l6" met="3" type="-+">End een Princes seer <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Want sy beyde voor Christus <rhyme label="c" type="f" corresp="#s11:l5">leere</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Willen waghen lijf end <rhyme label="d" type="m" corresp="#s11:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Laet ons den Heere <rhyme label="a" type="f">dancken</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Ghy Menschen groot en <rhyme label="b" type="m">cleyn</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Hy sterckt ons arme <rhyme label="a" type="f" corresp="#s12:l1">crancken</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Alsmen mocht sien <rhyme label="b" type="m" corresp="#s12:l2">ghemeyn</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Te Leyden voor die <rhyme label="c" type="f">stede</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Was den vyant heel <rhyme label="d" type="m">verbaest</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Ende t'Alckmaer oock <rhyme label="c" type="f" corresp="#s12:l5">mede</rhyme>.</l>
        <l id="s12:l8" met="3" type="-+">Daer ruymden hy metter <rhyme label="d" type="m" corresp="#s12:l6">haest</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Oorlof Princeslijcke <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Wilt dit in danck <rhyme label="b" type="m">ontfaen</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Den Prins van Orangien ter <rhyme label="a" type="f" corresp="#s13:l1">eeren</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">So is dit Liet <rhyme label="b" type="m" corresp="#s13:l2">ghedaen</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Met sijn Princesse <rhyme label="c" type="f">verheven</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Godt spaertse beyde <rhyme label="d" type="m">ghesont</rhyme></l>
        <l id="s13:l7" met="3" type="-+">En wilse lang laten <rhyme label="c" type="f" corresp="#s13:l5">leven</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Bid ick uut ts'hertsen <rhyme label="d" type="m" corresp="#s13:l6">gront</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
