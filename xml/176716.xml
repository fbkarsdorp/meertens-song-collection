  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Tot u, o mijn <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Mach ick wel droevich <rhyme label="b" type="f">klaghen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Als ick sie hoe <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat men u Bruydt gaet <rhyme label="b" type="f" corresp="#s1:l2">jaghen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dat niemant <rhyme label="c" type="m">yet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Misdeed’, is nerghens <rhyme label="d" type="m">vry</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Die leeft in <rhyme label="c" type="m" corresp="#s1:l5">vreed</rhyme>’,</l>
        <l id="s1:l8" met="2" type="-+">Vervolghen <rhyme label="d" type="m" corresp="#s1:l6">sy</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wildy zaligh <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Bruyt mijn <rhyme label="b" type="f">Erf-ghename</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Druckt u neer, lijdt <rhyme label="a" type="m" corresp="#s2:l1">pijn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En wilt u dies niet <rhyme label="b" type="f" corresp="#s2:l2">schame</rhyme>,,</l>
        <l id="s2:l5" met="2" type="-+">Ick heb om <rhyme label="c" type="m">dy</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Des Sathans Hooft <rhyme label="d" type="m">vertreen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Daerom steeckt <rhyme label="c" type="m" corresp="#s2:l5">hy</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Dus in u <rhyme label="d" type="m" corresp="#s2:l6">zeen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Eylaesm O mijn <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sathan steeckt my ter <rhyme label="b" type="f">neere</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Als een Vrouwe <rhyme label="a" type="m" corresp="#s3:l1">teer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die baert in pijnen <rhyme label="b" type="f" corresp="#s3:l2">seere</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Oock als een <rhyme label="c" type="m">Roos</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Onder den Doorn <rhyme label="d" type="m">opwast</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ben ick <rhyme label="c" type="m" corresp="#s3:l5">altoos</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Des lijdens <rhyme label="d" type="m" corresp="#s3:l6">gast</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Noch een weynigh <rhyme label="a" type="m">lijdt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">O Bruyt, ‘t sal nemen <rhyme label="b" type="f">keere</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Als de werelt <rhyme label="a" type="m" corresp="#s4:l1">krijt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En roept, Berghen valt <rhyme label="b" type="f" corresp="#s4:l2">neere</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Dan suldy <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En blincken als de <rhyme label="d" type="m">Son</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Een wit kleedt <rhyme label="c" type="m" corresp="#s4:l5">aen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">O Bruyt <rhyme label="d" type="m" corresp="#s4:l6">Zyon</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ghenadighe <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wanneer sal dit <rhyme label="b" type="f">ghebeuren</rhyme>?</l>
        <l id="s5:l3" met="2" type="-+">Nae u langht my <rhyme label="a" type="m" corresp="#s5:l1">seer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Sathan wil <rhyme label="b" type="f" corresp="#s5:l2">verscheuren</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Mijn eyghen <rhyme label="c" type="m">Vleys</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Doet my so dick <rhyme label="d" type="m">ghewelt</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Mijn zondigh <rhyme label="c" type="m" corresp="#s5:l5">ghepeys</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">My oock seer <rhyme label="d" type="m" corresp="#s5:l6">quelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Een Christens leven <rhyme label="a" type="m">es</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Op aerden niet dan <rhyme label="b" type="f">strijden</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Aengrijpt mijn woort, dat <rhyme label="a" type="m" corresp="#s6:l1">mes</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Verwint tot allen <rhyme label="b" type="f" corresp="#s6:l2">tijden</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">In mijnen <rhyme label="c" type="m">aenvanck</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hadt ick gheen plaets noch <rhyme label="d" type="m">Huys</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Den Kelck ick <rhyme label="c" type="m" corresp="#s6:l5">dranck</rhyme></l>
        <l id="s6:l8" met="2" type="-+">En droegh mijn <rhyme label="d" type="m" corresp="#s6:l6">Kruys</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Neemt doch wegh van <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ist moghelijck, O <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Desen Kelck der <rhyme label="a" type="m" corresp="#s7:l1">pijn</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Valt my te swaer, ick <rhyme label="b" type="f" corresp="#s7:l2">teere</rhyme>:</l>
        <l id="s7:l5" met="2" type="-+">Mijn vleesch is <rhyme label="c" type="m">kranck</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Mijn gheest willigh <rhyme label="d" type="m">bereyt</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Nae de <rhyme label="c" type="m" corresp="#s7:l5">slachtbanck</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Te zijn <rhyme label="d" type="m" corresp="#s7:l6">gheleyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Ik help u, O <rhyme label="a" type="m">Bruyt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En roep u met <rhyme label="b" type="f">Name</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Wijckt ten strijdt niet <rhyme label="a" type="m" corresp="#s8:l1">uyt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ick u Hooft-Man <rhyme label="b" type="f" corresp="#s8:l2">bequame</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Sal voor u <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">In water en in <rhyme label="d" type="m">vuyr</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">En u <rhyme label="c" type="m" corresp="#s8:l5">ontfaen</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Als een Kint <rhyme label="d" type="m" corresp="#s8:l6">puyr</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Als ick ‘t perijckel <rhyme label="a" type="m">denck</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In der Woestijne <rhyme label="b" type="f">lange</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Bitter water <rhyme label="a" type="m" corresp="#s9:l1">drenck</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En bijten van der <rhyme label="b" type="f" corresp="#s9:l2">Slanghe</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">So sorgh ick <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dat ick u Name <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Versaeck O <rhyme label="c" type="m" corresp="#s9:l5">Heer</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">Sterckt my in <rhyme label="d" type="m" corresp="#s9:l6">noot</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Niet een hayr en <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Van uwe Hooft <rhyme label="b" type="f">af-glijden</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Oft ‘t belieft my <rhyme label="a" type="m" corresp="#s10:l1">al</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Ik stae tot uwer <rhyme label="b" type="f" corresp="#s10:l2">zijden</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Haest u treedt <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Met Iosua nae ‘t <rhyme label="d" type="m">Landt</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Door d’Enghe <rhyme label="c" type="m" corresp="#s10:l5">Poort</rhyme>,</l>
        <l id="s10:l8" met="2" type="-+">Spijt u <rhyme label="d" type="m" corresp="#s10:l6">Vyandt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Zijt ghenadich <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Mijn Vyandts die my <rhyme label="b" type="f">haten</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Haer hert <rhyme label="a" type="m" corresp="#s11:l1">omme-keer</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Dat sy ‘t vervolgen <rhyme label="b" type="f" corresp="#s11:l2">laten</rhyme>:</l>
        <l id="s11:l5" met="2" type="-+">Sy weten <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Watb sy quaets doen, O <rhyme label="d" type="m">Godt</rhyme>,</l>
        <l id="s11:l7" met="2" type="-+">Helps’ uyt ‘t <rhyme label="c" type="m" corresp="#s11:l5">verdriet</rhyme>,</l>
        <l id="s11:l8" met="2" type="-+">Bick ick in ‘t <rhyme label="d" type="m" corresp="#s11:l6">Slot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
