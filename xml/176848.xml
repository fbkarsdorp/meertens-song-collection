    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wy dancken u o <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Voor u ghenaden <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die ghy tot uws naems <rhyme label="a" type="f" corresp="#s1:l1">eere</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Aen u Schepselen <rhyme label="b" type="m" corresp="#s1:l2">doet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Die met herten <rhyme label="c" type="f">verlanghen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Daer naer hebben <rhyme label="d" type="m">ghestaen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Het Doopsel te <rhyme label="c" type="f" corresp="#s1:l5">ontfanghen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">‘t Welck aen haer is <rhyme label="d" type="m" corresp="#s1:l6">voldaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Alsoo sy nu <rhyme label="a" type="f">ghestorven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En oock begraven <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Hebben ghenaed’ <rhyme label="a" type="f" corresp="#s2:l1">verworven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Gheeft datse moghen <rhyme label="b" type="m" corresp="#s2:l2">sijn</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Verrijsen in ‘t nieu <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met Christo opstaen <rhyme label="d" type="m">wis</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En soecken daer <rhyme label="c" type="f" corresp="#s2:l5">beneven</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">‘t Gheen dat daer boven <rhyme label="d" type="m" corresp="#s2:l6">is</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gheeft haer Hemelsche <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van ‘t aertsche <rhyme label="b" type="m">af-ghewent</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat sy u recht <rhyme label="a" type="f" corresp="#s3:l1">beminnen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Door liefde in <rhyme label="b" type="m" corresp="#s3:l2">gheprent</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat haer wesen mach <rhyme label="c" type="f">blijcken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zijnde alsoo <rhyme label="d" type="m">bekleyt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Op datse u <rhyme label="c" type="f" corresp="#s3:l5">ghelijcken</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Namaels in <rhyme label="d" type="m" corresp="#s3:l6">heerlijckheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nu sy voor u <rhyme label="a" type="f">aenschouwen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghewasschen zijn heel <rhyme label="b" type="m">net</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat sy haer suyver <rhyme label="a" type="f" corresp="#s4:l1">houwen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En leven <rhyme label="b" type="m" corresp="#s4:l2">onbesmet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En ‘t verbont, nu <rhyme label="c" type="f">gheganghen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Willich uyt haren <rhyme label="d" type="m">mont</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dat sy vast daer <rhyme label="c" type="f" corresp="#s4:l5">aenhanghen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Trouwelijck tot <rhyme label="d" type="m" corresp="#s4:l6">alderstont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wy bidden u Heer <rhyme label="a" type="f">goedich</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat ghy daechlicks <rhyme label="b" type="m">vermeert</rhyme></l>
        <l id="s5:l3" met="3" type="-+">U Ghemeente gheeft <rhyme label="a" type="f" corresp="#s5:l1">spoedich</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Datter veel word’ <rhyme label="b" type="m" corresp="#s5:l2">bekeert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Sy moet daeghlijcks <rhyme label="c" type="f">vermeeren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En wassen in ‘t <rhyme label="d" type="m">ghetal</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Tot lof van uwer <rhyme label="c" type="f" corresp="#s5:l5">eeren</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Moet dit gheschieden <rhyme label="d" type="m" corresp="#s5:l6">al</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gheeft hier toen uwen <rhyme label="a" type="f">zeghen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Daer toe rechte <rhyme label="b" type="m">aendacht</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hemelschen dauw’ en <rhyme label="a" type="f" corresp="#s6:l1">reghen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Door uwes Gheestes <rhyme label="b" type="m" corresp="#s6:l2">kracht</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat sy veel vruchten <rhyme label="c" type="f">bringhen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">T’Uwer eeren <rhyme label="d" type="m">alleyn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dies wy nu Amen <rhyme label="c" type="f" corresp="#s6:l5">singhen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Van herten <rhyme label="d" type="m" corresp="#s6:l6">alghemeyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
