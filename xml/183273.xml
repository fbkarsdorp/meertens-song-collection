<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183273</id>
    <date>
      <year-start>1752</year-start>
      <year-end>1752</year-end>
    </date>
    <location>
      <orginal-name>gorinchem</orginal-name>
      <standard-name>Gorinchem, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8296133</lon>
        <lat>4.9738743</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 128 Datheen</melody>
  <author>Jacob Groenewegen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Iezus ligt en <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn Koning en mijn <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Aan wien ik heb <rhyme label="a" type="f" corresp="#s1:l1">gegeven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Mijn hert om voor u <rhyme label="b" type="m" corresp="#s1:l2">eer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">U naam en zaak te <rhyme label="c" type="f">stryden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ach had ik meerder <rhyme label="d" type="m">kragt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ik leg helaas ter <rhyme label="c" type="f" corresp="#s1:l5">zyden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Soo dikmaals eer ik <rhyme label="d" type="m" corresp="#s1:l6">dagt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik heb zoo veel <rhyme label="a" type="f">vyanden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O magtig hemels <rhyme label="b" type="m">Vorst</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ey leer gy dog mijn <rhyme label="a" type="f" corresp="#s2:l1">handen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ten stryde my uyt <rhyme label="b" type="m" corresp="#s2:l2">dost</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">’k Heb lust voor u te <rhyme label="c" type="f">leeve</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">In dit benede <rhyme label="d" type="m">perk</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maar Heer gy moet my <rhyme label="c" type="f" corresp="#s2:l5">geven</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Genade tot dit <rhyme label="d" type="m" corresp="#s2:l6">werk</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik moet u alle <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mijn Weeld lievent <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O ziele Koning <rhyme label="a" type="f" corresp="#s3:l1">klagen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met droefheid en met <rhyme label="b" type="m" corresp="#s3:l2">smert</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Mijn ziele is <rhyme label="c" type="f">inwendig</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Verdorven en <rhyme label="d" type="m">verkeert</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ia gantschelijk <rhyme label="c" type="f" corresp="#s3:l5">elendig</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Als gy ’t niet <rhyme label="d" type="m" corresp="#s3:l6">overheert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Rigt gy op in mijn <rhyme label="a" type="f">ziele</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">U zalige <rhyme label="b" type="m">heerschappij</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wilt Satans rijk <rhyme label="a" type="f" corresp="#s4:l1">verniele</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Verbreke gans in <rhyme label="b" type="m" corresp="#s4:l2">my</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Ik heb u trou <rhyme label="c" type="f">gezworen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">O grooten opper <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ik heb alleen <rhyme label="c" type="f" corresp="#s4:l5">verkooren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Te leven tot u <rhyme label="d" type="m" corresp="#s4:l6">eer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Komt Iezus o mijn <rhyme label="a" type="f">Koning</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En neemt mijn zondig <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En maakt het tot u <rhyme label="a" type="f" corresp="#s5:l1">woning</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dan is ’t bevrijd van <rhyme label="b" type="m" corresp="#s5:l2">smert</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Verdrijft die vreemde <rhyme label="c" type="f">heeren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die ’t huys nog houden <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wilt gy ’t alleen <rhyme label="c" type="f" corresp="#s5:l5">regeeren</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En buyge na u <rhyme label="d" type="m" corresp="#s5:l6">zin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O Iezus slaat aan <rhyme label="a" type="f">stukken</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">De grendels van mijn <rhyme label="b" type="m">ziel</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">(’t Sal anders nooyt <rhyme label="a" type="f" corresp="#s6:l1">gelukken</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Al dat u buyten <rhyme label="b" type="m" corresp="#s6:l2">hiel</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Verbreekt het laat het <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En maakt het my maar <rhyme label="d" type="m">kwijt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Laat ik u tog niet <rhyme label="c" type="f" corresp="#s6:l5">derven</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Die alles voor my <rhyme label="d" type="m" corresp="#s6:l6">zijt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn ziele is <rhyme label="a" type="f">gebonden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Aan eyge wil en <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">De Wereld en de <rhyme label="a" type="f" corresp="#s7:l1">Sonden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die nement dikwils <rhyme label="b" type="m" corresp="#s7:l2">in</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">En make dat mijn <rhyme label="c" type="f">herte</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Niet doet als swerve <rhyme label="d" type="m">om</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Tot mijn verdriet en <rhyme label="c" type="f" corresp="#s7:l5">smerte</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">O trek my Iezus <rhyme label="d" type="m" corresp="#s7:l6">kom</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En heyligt my van <rhyme label="a" type="f">binne</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ia coopt de wortel <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Van mijn verdorve <rhyme label="a" type="f" corresp="#s8:l1">zinne</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En op dat ik <rhyme label="b" type="m" corresp="#s8:l2">besluyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Mijn grootste wens en <rhyme label="c" type="f">bede</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">O zalig opper <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Is dit om hier <rhyme label="c" type="f" corresp="#s8:l5">benede</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Te leeve tot u <rhyme label="d" type="m" corresp="#s8:l6">eer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Verheerlijkt <rhyme label="a" type="f">inwendig</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In heyligheid, en <rhyme label="b" type="m">dan</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Met al mijn doen <rhyme label="a" type="f" corresp="#s9:l1">bestendig</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Te geve blijk daar <rhyme label="b" type="m" corresp="#s9:l2">van</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Tot dat mijn ziel <rhyme label="c" type="f">ombonden</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">U zuyver reyn en <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Volmaakt en zonder <rhyme label="c" type="f" corresp="#s9:l5">zonden</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Sal dienen voor den <rhyme label="d" type="m" corresp="#s9:l6">troon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
