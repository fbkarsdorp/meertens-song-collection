<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5248</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag standard-name="geuzenlied" translation="Geuzen song">geuzenlied</tag>
      <tag standard-name="meilied" translation="May song">meilied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>O schepper fier</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ghedankt zy <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die ons heeft laten <rhyme label="b" type="f">spruyten</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Vol des deugts <rhyme label="b" type="f" corresp="#s1:l2">virtuyten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Den Mey-boom <rhyme label="c" type="m">exellent</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Die hier het <rhyme label="a" type="m" corresp="#s1:l1">Slot</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van onzen Thuyn kan <rhyme label="b" type="f" corresp="#s1:l2">sluyten</rhyme></l>
        <l id="s1:l7" met="3" type="+-">'Twreet houden daer <rhyme label="b" type="f" corresp="#s1:l2">buyten</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Het welck de Schaepkens <rhyme label="c" type="m" corresp="#s1:l4">schent</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dezen Mey-boom groen en <rhyme label="d" type="f">trouwen</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Die den wolf kan buyten <rhyme label="d" type="f" corresp="#s1:l9">houwen</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Voor 'sdruks <rhyme label="e" type="m">geschrey</rhyme>:</l>
        <l id="s1:l12" met="3" type="-+">Looft Godt voor sulcken <rhyme label="e" type="m" corresp="#s1:l11">Mey</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hy is <rhyme label="a" type="m">verciert</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met schoon Orangien <rhyme label="b" type="f">Bloemen</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Keyserlijck te <rhyme label="b" type="f" corresp="#s2:l2">noemen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Eel van Nassouwen <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Hy vreest noch <rhyme label="a" type="m" corresp="#s2:l1">viert</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sijns vyandts hoge <rhyme label="b" type="f" corresp="#s2:l2">roemen</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Maer wel sonder <rhyme label="b" type="f" corresp="#s2:l2">schromen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Hem wel afbreucken <rhyme label="c" type="m" corresp="#s2:l4">doet</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Want hij twee Tyrannen <rhyme label="d" type="f">krachtich</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Derf weerstaen met Godt <rhyme label="d" type="f" corresp="#s2:l9">Almachtich</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Paus, Conigh <rhyme label="e" type="m">bey</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Looft Godt voor sulcken <rhyme label="e" type="m" corresp="#s2:l11">Mey</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dat hij <rhyme label="a" type="m">aenrecht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat gaet hem wel <rhyme label="b" type="f">ghedyen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Schier aen alle <rhyme label="b" type="f" corresp="#s3:l2">zyen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wat hy doet oft <rhyme label="c" type="m">beghint</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Des Heeren <rhyme label="a" type="m" corresp="#s3:l1">knecht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die blijckt hy wel int <rhyme label="b" type="f" corresp="#s3:l2">stryden</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Om 't volck te <rhyme label="b" type="f" corresp="#s3:l2">bevryden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Al die vroom zijn <rhyme label="c" type="m" corresp="#s3:l4">ghesint</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Godt die sterckt, hem sonder <rhyme label="d" type="f">schaden</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Door hem werckt, hy wonder <rhyme label="d" type="f" corresp="#s3:l9">daden</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Menigher <rhyme label="e" type="m">ley</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Looft Godt voor sulcken <rhyme label="e" type="m" corresp="#s3:l11">Mey</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Eer hy te <rhyme label="a" type="m">veldt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gaet trecken met <rhyme label="b" type="f">couragie</rhyme></l>
        <l id="s4:l3" met="3" type="+-">'tSijnder <rhyme label="b" type="f" corresp="#s4:l2">avontagie</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Schickt God dan wysen <rhyme label="c" type="m">raet</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Den kloecken <rhyme label="a" type="m" corresp="#s4:l1">heldt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Hy geeft oock goede <rhyme label="b" type="f" corresp="#s4:l2">gagie</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Soldaet, Ruyter, <rhyme label="b" type="f" corresp="#s4:l2">Pagie</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Te min wordt hy <rhyme label="c" type="m" corresp="#s4:l4">ghehaet</rhyme>:</l>
        <l id="s4:l9" met="4" type="+-">Niet bloet gierich, noch <rhyme label="d" type="f">hoveerdich</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Goedertierich, trou vol <rhyme label="d" type="f" corresp="#s4:l9">heerdich</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Voor dees <rhyme label="e" type="m">Contrey</rhyme>:</l>
        <l id="s4:l12" met="3" type="-+">Looft Godt voor sulcken <rhyme label="e" type="m" corresp="#s4:l11">Mey</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Sijns vyandts <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En hooghmoedich <rhyme label="b" type="f">vermeten</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Sal worden <rhyme label="b" type="f" corresp="#s5:l2">ghesleten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Oft de weerelt <rhyme label="c" type="m">dreyght</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Den grooten <rhyme label="a" type="m" corresp="#s5:l1">pracht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En sal Godt niet <rhyme label="b" type="f" corresp="#s5:l2">vergeten</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Maer worden <rhyme label="b" type="f" corresp="#s5:l2">verbeten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Hoe wreet hy is <rhyme label="c" type="m" corresp="#s5:l4">gheneycht</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Een kleyn hooft der <rhyme label="d" type="f">Batavieren</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Hem berooft veel qua <rhyme label="d" type="f" corresp="#s5:l9">manieren</rhyme></l>
        <l id="s5:l11" met="2" type="+-">Met 'tswaert uyt <rhyme label="e" type="m">d'schey</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Looft Godt voor sulcken <rhyme label="e" type="m" corresp="#s5:l11">Mey</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Dit is <rhyme label="a" type="m">gheschiet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ons Prins Maurits ter <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Die Godt laet <rhyme label="b" type="f" corresp="#s6:l2">vermeeren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">In kloeckheyt des <rhyme label="c" type="m">welvaert</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Teghen't <rhyme label="a" type="m" corresp="#s6:l1">verdriet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dat tot ons Landt mocht <rhyme label="b" type="f" corresp="#s6:l2">keeren</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Sulcke vroome <rhyme label="b" type="f" corresp="#s6:l2">Heeren</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Sijn daer in niet <rhyme label="c" type="m" corresp="#s6:l4">bezwaert</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Die in Liefd' haer herten <rhyme label="d" type="f">branden</rhyme></l>
        <l id="s6:l10" met="4" type="+-">Tot welvaert van steden, <rhyme label="d" type="f" corresp="#s6:l9">landen</rhyme></l>
        <l id="s6:l11" met="2" type="+-">Soo ick eerst <rhyme label="e" type="m">sey</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Looft Godt voor sulcken <rhyme label="e" type="m" corresp="#s6:l11">Mey</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Loff Eer en <rhyme label="a" type="m">Prijs</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Zy oock mijn Heeren <rhyme label="b" type="f">Staten</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Die hem niet <rhyme label="b" type="f" corresp="#s7:l2">verlaten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">In trouwicheyt <rhyme label="c" type="m">altijdt</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Met Raet, <rhyme label="a" type="m" corresp="#s7:l1">Advijs</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Oock wat dient t'zijnder <rhyme label="b" type="f" corresp="#s7:l2">baten</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">'sLandts goede <rhyme label="b" type="f" corresp="#s7:l2">ondersaten</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Doen wel haren <rhyme label="c" type="m" corresp="#s7:l4">vlijt</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">Zy willich en trouwe <rhyme label="d" type="f">blyven</rhyme></l>
        <l id="s7:l10" met="4" type="+-">Om den Mey-Boom <rhyme label="d" type="f" corresp="#s7:l9">t'onderstyven</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">In veldt oft <rhyme label="e" type="m">Wey</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">Looft Godt voor sulcken <rhyme label="e" type="m" corresp="#s7:l11">Mey</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
