  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HA viva,, <rhyme label="a" type="m">Victoria</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Den strijt hebben wy <rhyme label="b" type="f">ghewonnen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">t'Is al claer,, Den wijn is <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ach wilcom ghy malse <rhyme label="b" type="f" corresp="#s1:l2">tonnen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Nu bierken <rhyme label="d" type="m">vliet</rhyme></l>
        <l id="s1:l6" met="2" type="-+">VVant ghy zijt <rhyme label="d" type="m" corresp="#s1:l5">niet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">VVat beters heb ick <rhyme label="e" type="f">vernomen</rhyme>;</l>
        <l id="s1:l8" met="2" type="+-">VVech vvech <rhyme label="f" type="m">bier</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Gaet voorts van <rhyme label="f" type="m" corresp="#s1:l8">hier</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">VVy sien den coelen wijn <rhyme label="e" type="f" corresp="#s1:l7">comen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ach wat vreught,, Is by de <rhyme label="a" type="m">jeught</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als Bacchus hun comt <rhyme label="b" type="f">beloeren</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Doof en cranck,, Stom, blint en <rhyme label="c" type="m">manck</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die doet hy de keeltjens <rhyme label="b" type="f" corresp="#s2:l2">roeren</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Voorwaer hoe <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Is Bacchus <rhyme label="d" type="m" corresp="#s2:l5">bloet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Geperst uyt malse <rhyme label="e" type="f">druyven</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Dees <rhyme label="f" type="m">conserf</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Heeft <rhyme label="f" type="m" corresp="#s2:l8">menich-vverf</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Van ons de droefheydt doen <rhyme label="e" type="f" corresp="#s2:l7">schuyven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Vrindekens,, Ist Frans oft <rhyme label="a" type="m">Rens</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sa laet het ons eens gaen <rhyme label="b" type="f">proeven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">VVant ick docht,, Dat ons dit <rhyme label="c" type="m">vocht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Int minsten niet can <rhyme label="b" type="f" corresp="#s3:l2">bedroeven</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">t'Gelaesken <rhyme label="d" type="m">spoelt</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">VVant Bacchus <rhyme label="d" type="m" corresp="#s3:l5">woelt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Seer geren in schoon <rhyme label="e" type="f">gelaesen</rhyme>:</l>
        <l id="s3:l8" met="2" type="+-">Sa val <rhyme label="f" type="m">aen</rhyme>,,</l>
        <l id="s3:l9" met="2" type="+-">En laet niet <rhyme label="f" type="m" corresp="#s3:l8">staen</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Ghy moetse lustich uyt <rhyme label="e" type="f" corresp="#s3:l7">blaesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vrient t'waer schant,, Dat aen u <rhyme label="a" type="m">cant</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Den romer sou blijven <rhyme label="b" type="f">proencken</rhyme></l>
        <l id="s4:l3" met="4" type="-+">t'Cristelijn,, Gevult met <rhyme label="c" type="m">wijn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Moet lustich sijn uyt <rhyme label="b" type="f" corresp="#s4:l2">ghedroencken</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Sa sa dan <rhyme label="d" type="m">singht</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Dat t'wijntjen <rhyme label="d" type="m" corresp="#s4:l5">springht</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Int kelderken van ons <rhyme label="e" type="f">buycxken</rhyme></l>
        <l id="s4:l8" met="2" type="+-">Drinckt het <rhyme label="f" type="m">ront</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Tot inden <rhyme label="f" type="m" corresp="#s4:l8">gront</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Men sal noch tappen een <rhyme label="e" type="f" corresp="#s4:l7">cruycxken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
