    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Noyt geyle minne // mijn waerde <rhyme label="a" type="m">vrouw</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Socht u Philander <rhyme label="b" type="m">t'onderstaen</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Maer schoon Goddinne // getrouwe <rhyme label="a" type="m" corresp="#s1:l1">trouw</rhyme></l>
        <l id="s1:l4" met="4" type="-+">En ware Liefde te nemen <rhyme label="b" type="m" corresp="#s1:l2">aen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dies mint <rhyme label="c" type="f">Philander</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die noyt geen ander // als <rhyme label="d" type="m">Silvia</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Sal minne // Goddinne geeft 't woortje <rhyme label="d" type="m" corresp="#s1:l6">Ja</rhyme>.</l>
      </lg>
    </lg>
