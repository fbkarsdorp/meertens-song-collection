<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19180</id>
    <date>
      <year-start>1611</year-start>
      <year-end>1611</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon jonkheertje heus en trouw</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">COmt ter baen elck toon sijn <rhyme label="a" type="f">crachten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Grijpt de Bruyt elck houd vast <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Bruydegom ghy moet noch wat <rhyme label="a" type="f" corresp="#s1:l1">wachten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">VVy laten de bruyt niet <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ghy moet nochtans op het <rhyme label="c" type="m">lest</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">T'sal niet zijn doet vry u <rhyme label="c" type="m" corresp="#s1:l5">best</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Treckt met <rhyme label="d" type="m">cracht</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Bruyckt u <rhyme label="d" type="m" corresp="#s1:l7">macht</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">T'sal u <rhyme label="e" type="f">missen</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Daer ghy langh na hebt <rhyme label="d" type="m" corresp="#s1:l7">ghewacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Treckt by handen treckt by <rhyme label="a" type="f">mouwen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Bruydegom tis al haest u <rhyme label="b" type="m">tijt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Vryers ghy sult daer aen <rhyme label="a" type="f" corresp="#s2:l1">flouwen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">VVy zijn noch de Bruyt niet <rhyme label="b" type="m" corresp="#s2:l2">quijt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Lustich t'sal haest zijn <rhyme label="c" type="m">ghedaen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ia ja sy is al <rhyme label="c" type="m" corresp="#s2:l5">ghevaen</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">My is <rhyme label="d" type="m">bang</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">T'sal eer <rhyme label="d" type="m" corresp="#s2:l7">lang</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Beter <rhyme label="e" type="f">wesen</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Als ghy zijt uyt dit <rhyme label="d" type="m" corresp="#s2:l7">ghedrang</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Och en wilt my niet <rhyme label="a" type="f">ontcleden</rhyme></l>
        <l id="s3:l2" met="4" type="+-">K'weet niet wat wat my nakend' <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">VVilt niet schryen zijt te <rhyme label="a" type="f" corresp="#s3:l1">vreden</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Mennich daer na hakend' <rhyme label="b" type="m" corresp="#s3:l2">is</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Maer ick hebt noyt <rhyme label="c" type="m">ondersocht</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">T'most te vooren zijn <rhyme label="c" type="m" corresp="#s3:l5">bedocht</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Gheeft my <rhyme label="d" type="m">raedt</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">T'is te <rhyme label="d" type="m" corresp="#s3:l7">laet</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">T'can niet <rhyme label="e" type="f">helpen</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Blyft by my van hier niet <rhyme label="d" type="m" corresp="#s3:l7">gaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Neen wat soud u Bruydgom <rhyme label="a" type="f">segghen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Om dees nacht ist al <rhyme label="b" type="m">ghedaen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Sal een vreembd' man by my <rhyme label="a" type="f" corresp="#s4:l1">legghen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Dat en dorf ick niet <rhyme label="b" type="m" corresp="#s4:l2">bestaen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Met een nacht zijt ghyt <rhyme label="c" type="m">ghewent</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Maer hy is my <rhyme label="c" type="m" corresp="#s4:l5">onbekent</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">T'is wel <rhyme label="d" type="m">waer</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">T'valt my <rhyme label="d" type="m" corresp="#s4:l7">swaer</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">T'zijn gheen <rhyme label="e" type="f">saken</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Die u brenghen in <rhyme label="d" type="m" corresp="#s4:l7">ghevaer</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer siet een de deur is <rhyme label="a" type="f">open</rhyme></l>
        <l id="s5:l2" met="4" type="+-">T'is vergeefs dat ghy <rhyme label="b" type="m">begheert</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Bruydegom waer zijt ghy <rhyme label="a" type="f" corresp="#s5:l1">gheloopen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Sijt ghy nu int lest <rhyme label="b" type="m" corresp="#s5:l2">verveert</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Neen voorwaer nu ist mijn <rhyme label="c" type="m">tijt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Toont dat ghy gheen leerkint <rhyme label="c" type="m" corresp="#s5:l5">zijt</rhyme></l>
        <l id="s5:l7" met="2" type="+-">Nu <rhyme label="d" type="m">ghenacht</rhyme></l>
        <l id="s5:l8" met="2" type="+-">Rust ghy <rhyme label="d" type="m" corresp="#s5:l7">sacht</rhyme></l>
        <l id="s5:l9" met="2" type="+-">In die <rhyme label="e" type="f">armen</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Daer ghy lang na hebt <rhyme label="d" type="m" corresp="#s5:l7">ghewacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
