    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Waarheen mijn ziel waarheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAar heen myn ziel waar <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Daar is op aarde <rhyme label="a" type="m" corresp="#s1:l1">geen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Standvastigheid te <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Uw Klores myn <rhyme label="c" type="m">vergeet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Klaag ik vergeefs de <rhyme label="b" type="f" corresp="#s1:l3">Winden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Myn bitt'ren harten <rhyme label="c" type="m" corresp="#s1:l4">leet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">O min was dat u <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">O neen het is enkel <rhyme label="d" type="m" corresp="#s1:l7">roet</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Uw Min valt myn zoo <rhyme label="e" type="f">lastig</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Dat myn geduurigh <rhyme label="f" type="m">vleijd</rhyme>;</l>
        <l id="s1:l11" met="3" type="-+">Niet is op aard <rhyme label="e" type="f" corresp="#s1:l9">Standvastig</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Dan de <rhyme label="f" type="m" corresp="#s1:l10">Onstandvastigheid</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Triumf myn ziel <rhyme label="a" type="m">Triumf</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik heb op aerde <rhyme label="a" type="m" corresp="#s2:l1">een</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Standvastigheit <rhyme label="b" type="f">gekregen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Uw Kloris myn <rhyme label="c" type="m">begeer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Strooid Roosjes op de <rhyme label="b" type="f" corresp="#s2:l3">Wegen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daar ik met myn Lief op <rhyme label="c" type="m" corresp="#s2:l4">tree</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">O min wat zyt gy <rhyme label="d" type="m">zoet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Hoe edel is u <rhyme label="d" type="m" corresp="#s2:l7">gloed</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Uw min valt myn nooit <rhyme label="e" type="f">lastig</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Die myn geduurig <rhyme label="f" type="m">vleyd</rhyme>,</l>
        <l id="s2:l11" met="3" type="-+">Ik blyf u myn lief <rhyme label="e" type="f" corresp="#s2:l9">standvastig</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Tot in der <rhyme label="f" type="m" corresp="#s2:l10">Eeuwigheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
