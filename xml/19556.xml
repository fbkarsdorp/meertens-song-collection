      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">WAt vermaak vernoegde ziellust <rhyme label="a" type="m">baard</rhyme>,</l>
        <l id="s1:l2" met="6" type="-+">Bind ik in der daat, nu oprecht, heel volmaakt, inzen <rhyme label="a" type="m" corresp="#s1:l1">aard</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">De Hemel, Arfilon// Die <rhyme label="b" type="m">gon</rhyme></l>
        <l id="s1:l4" met="4" type="-+">V ziel//  em mijn gemoed ‘er <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En voords onfeilbre <rhyme label="c" type="m" corresp="#s1:l4">rust</rhyme>,</l>
        <l id="s1:l6" met="1" type="-+">Wiens <rhyme label="d" type="m">wiel</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">‘T gun op den Heilweg <rhyme label="e" type="m">loopt</rhyme></l>
        <l id="s1:l8" met="1" type="-+">en <rhyme label="e" type="m" corresp="#s1:l7">knoopt</rhyme>.</l>
