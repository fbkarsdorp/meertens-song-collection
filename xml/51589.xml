    <tags>
      <tag translation="narrative song">verhalend lied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wil deez' grap nu eens <rhyme label="a" type="f">aanhooren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Van een bakker en schutters <rhyme label="b" type="m">vrouw</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wat een mensch al komt te <rhyme label="a" type="f" corresp="#s1:l1">voren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Menig deegje wordt gekneed zeer <rhyme label="b" type="m" corresp="#s1:l2">gaauw</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat de vrouw kwam te <rhyme label="c" type="f">vermaken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als zij aten op hun <rhyme label="d" type="m">gemak</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Klein goed en meer andere <rhyme label="c" type="f" corresp="#s1:l5">zaken</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Soms van 't lekkerste <rhyme label="d" type="m" corresp="#s1:l6">gebak</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Vrolijk had men dan een <rhyme label="e" type="f">leven</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Deze bakker en schutters <rhyme label="f" type="m">vrouw</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Als zij waren bij <rhyme label="g" type="f">elkander</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Dacht hij om geen <rhyme label="f" type="m" corresp="#s1:l10">naberouw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Na verloop van veertien <rhyme label="a" type="f">maanden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Kwam de man al met <rhyme label="b" type="m">verlof</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Zag zijn vrouw met een kindje <rhyme label="a" type="f" corresp="#s2:l1">zitten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Stond te kijken als een <rhyme label="b" type="m" corresp="#s2:l2">mof</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En begon haar toen te <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Wie was deze <rhyme label="d" type="m">remplaçant</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Of 'k zal u de deur <rhyme label="c" type="f" corresp="#s2:l5">uitjagen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Spreek waart gij zoo in de <rhyme label="d" type="m" corresp="#s2:l6">brand</rhyme>?</l>
        <l id="s2:l9" met="4" type="+-">Vrolijk heeft ook onze <rhyme label="e" type="f">bakker</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Mij gebragt van 't fijnst <rhyme label="f" type="m">gebak</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">ô Die vent die was zoo <rhyme label="g" type="f">wakker</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Eer hij 't brood in de oven <rhyme label="f" type="m" corresp="#s2:l10">stak</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">ô Wat stond die man te <rhyme label="a" type="f">kijken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Als hij hoorde van deez' <rhyme label="b" type="m">klant</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En begon te <rhyme label="a" type="f" corresp="#s3:l1">practiseren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Om zich te helpen uit de <rhyme label="b" type="m" corresp="#s3:l2">brand</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Want daar is niet veel te <rhyme label="c" type="f">werken</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En de winter die komt <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Hij dacht lk zal 't <rhyme label="c" type="f" corresp="#s3:l5">overleggen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Of ik wat beginnen <rhyme label="d" type="m" corresp="#s3:l6">kan</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Vrolijk is de man <rhyme label="e" type="f">gelopen</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Naar de bakker met een <rhyme label="f" type="m">mand</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Om een brood voor pap te <rhyme label="g" type="f">kopen</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Het was voor die kleine <rhyme label="f" type="m" corresp="#s3:l10">klant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Baas sprak hij geeft mijn een <rhyme label="a" type="f">broodje</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Voor dees kleine die gij hier <rhyme label="b" type="m">ziet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om het daar wat mee te <rhyme label="a" type="f" corresp="#s4:l1">laven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Maar het is het mijne <rhyme label="b" type="m" corresp="#s4:l2">niet</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Ik koom het u maar <rhyme label="c" type="f">bezorgen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Bevend zag de vend hem <rhyme label="d" type="m">aan</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">Lieve vriend wagt toch tot <rhyme label="c" type="f" corresp="#s4:l5">morgen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Neemt dit kind hier toch van <rhyme label="d" type="m" corresp="#s4:l6">daan</rhyme>;</l>
        <l id="s4:l9" met="4" type="+-">Neen dit geen vrolijk <rhyme label="e" type="f">leven</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Voor deez' bakker toen ter <rhyme label="f" type="m">tijd</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Drie honderd guldens moest hij <rhyme label="g" type="f">geven</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Of hij raakte het kind niet <rhyme label="f" type="m" corresp="#s4:l10">kwijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Met het kind zal ik <rhyme label="a" type="f">vertrekken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Maar toch morgen koom ik <rhyme label="b" type="m">weer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Denk ik laat mij niet <rhyme label="a" type="f" corresp="#s5:l1">begekkken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dan leg ik het, voor u voeten <rhyme label="b" type="m" corresp="#s5:l2">neer</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Den anderen dag is hij <rhyme label="c" type="f">gekomen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Drie honderd guldens waren <rhyme label="d" type="m">klaar</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Want hij was vol angst en <rhyme label="c" type="f" corresp="#s5:l5">schromen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">En bevreest voor meer <rhyme label="d" type="m" corresp="#s5:l6">gevaar</rhyme>;</l>
        <l id="s5:l9" met="4" type="+-">Baas dat is geen vrolijk <rhyme label="e" type="f">leven</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Dat gij zoo veel betalen <rhyme label="f" type="m">moet</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">En u geld zoo weg moet <rhyme label="g" type="f">geven</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">Keurt gij dit nu nog niet <rhyme label="f" type="m" corresp="#s5:l10">goed</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Toen de man is t' huis <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dagt nu heb ik wat in de <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Om de winter door te <rhyme label="a" type="f" corresp="#s6:l1">komen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Of mij te helpen uit de <rhyme label="b" type="m" corresp="#s6:l2">brand</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Doch ik raad u bakkers <rhyme label="c" type="f">baasen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Wilt gij een kansje wagen <rhyme label="d" type="m">gaan</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Zoekt het bij geen schutters <rhyme label="c" type="f" corresp="#s6:l5">vrouwen</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Neen blijft liever daar van <rhyme label="d" type="m" corresp="#s6:l6">daan</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Want het is geen vrolijk <rhyme label="e" type="f">leven</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Als men 't kind ligt voor u <rhyme label="f" type="m">voet</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Om zoo veel centen af te <rhyme label="g" type="f">geven</rhyme>,</l>
        <l id="s6:l12" met="4" type="+-">En daar bij mogt wezen <rhyme label="f" type="m" corresp="#s6:l10">zoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
