  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">CHere <rhyme label="a" type="m">Cloris</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Votre <rhyme label="a" type="m" corresp="#s1:l1">Esprit</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Vos Charmes <rhyme label="b" type="f">redoutables</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Vos jeux <rhyme label="c" type="m">percant</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Votre air <rhyme label="c" type="m" corresp="#s1:l4">Charmant</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Vous rendent <rhyme label="b" type="f" corresp="#s1:l3">adorable</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Vous me <rhyme label="a" type="m">blesses</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Vous me <rhyme label="a" type="m" corresp="#s2:l1">Li√©s</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Sans cesse je <rhyme label="b" type="f">soupire</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Sans vous je <rhyme label="c" type="m">meurs</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Rend votre <rhyme label="c" type="m" corresp="#s2:l4">coeur</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">C'est que le mien <rhyme label="b" type="f" corresp="#s2:l3">desire</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Si vous <rhyme label="a" type="m">m'aimez</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Rien peut <rhyme label="a" type="m" corresp="#s3:l1">troubler</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Le repos de nos <rhyme label="b" type="f">ames</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Le dieu <rhyme label="c" type="m">d'amour</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Dans ces beaux <rhyme label="c" type="m" corresp="#s3:l4">jours</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Couronnera nos <rhyme label="b" type="f" corresp="#s3:l3">Flammes</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Si la <rhyme label="a" type="m">Riguer</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">De votre <rhyme label="a" type="m" corresp="#s4:l1">Coeur</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Fuyant, faisoit <rhyme label="b" type="f">entendre</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+">Ces mots si <rhyme label="c" type="m">doux</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Je suis <rhyme label="c" type="m" corresp="#s4:l4">avous</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Mon coeur veut bien se <rhyme label="b" type="f" corresp="#s4:l3">rendre</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Alors nos <rhyme label="a" type="m">feux</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Sans cesse <rhyme label="a" type="m" corresp="#s5:l1">heureux</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Seront toujours <rhyme label="b" type="f">unies</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Charmant <rhyme label="c" type="m">destin</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Qu'auront <rhyme label="c" type="m" corresp="#s5:l4">enfin</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Deux ames bien <rhyme label="b" type="f" corresp="#s5:l3">ravies</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
