  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Of ic der vrouwen <rhyme label="a" type="m">minnentlijch</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Hertze ende zin gegheven <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Sone willich voort up <rhyme label="a" type="m" corresp="#s1:l1">erderijch</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Met onsteide niet vorder <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Maer ghetrauwelijc <rhyme label="b" type="m" corresp="#s1:l2">vulstaen</rhyme></l>
        <l id="s1:l6" met="4" type="-+">In al mijn doghen groos ende <rhyme label="c" type="m">clein</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Dor di <rhyme label="c" type="m" corresp="#s1:l6">allein</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Ende anders <rhyme label="c" type="m" corresp="#s1:l6">ghein</rhyme></l>
        <l id="s1:l9" met="4" type="-+">So hanich tlast in mi <rhyme label="b" type="m" corresp="#s1:l2">ontfaen</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Soe hout mijn hertze in vruechden <rhyme label="a" type="m" corresp="#s1:l1">rijch</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Haer anscijn es so wel <rhyme label="b" type="m" corresp="#s1:l2">ghedaen</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Ich wil haer dienen <rhyme label="a" type="m" corresp="#s1:l1">zekerlijch</rhyme></l>
        <l id="s1:l13" met="4" type="-+">Met trouwen sonder <rhyme label="b" type="m" corresp="#s1:l2">avelaen</rhyme></l>
        <l id="s1:l14" met="4" type="+-">Minlijc houtsi mi <rhyme label="b" type="m" corresp="#s1:l2">ghevaen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Of ic der vrouwen <rhyme label="a" type="m">minnentlijch</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Hertze ende zin gegheven <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Sone willich voort up <rhyme label="a" type="m" corresp="#s2:l1">erderijch</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Met onsteide niet vorder <rhyme label="b" type="m" corresp="#s2:l2">gaen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Maer ghetrauwelijc <rhyme label="b" type="m" corresp="#s2:l2">vulstaen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Wi soudso mi dan doen <rhyme label="c" type="m">bezwijch</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Mijn hertze en can di nicht <rhyme label="c" type="m" corresp="#s2:l6">ontgaen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Up erde bestu mijn <rhyme label="c" type="m" corresp="#s2:l6">hemelrijch</rhyme></l>
        <l id="s2:l9" met="4" type="-+">Ic dinke om haer wes ich <rhyme label="b" type="m" corresp="#s2:l2">vermaen</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Al mijn leit mach si <rhyme label="a" type="m" corresp="#s2:l1">verslaen</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Met harer zoetzer worden <rhyme label="b" type="m" corresp="#s2:l2">ein</rhyme></l>
        <l id="s2:l12" met="4" type="-+">O vrouwe <rhyme label="a" type="m" corresp="#s2:l1">rein</rhyme></l>
        <l id="s2:l13" met="4" type="-+">Sijt mer <rhyme label="b" type="m" corresp="#s2:l2">ghemein</rhyme></l>
        <l id="s2:l14" met="4" type="+-">Ich blive di eewich <rhyme label="b" type="m" corresp="#s2:l2">onderdaen</rhyme></l>
      </lg>
    </lg>
  </text>
