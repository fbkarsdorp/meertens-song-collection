<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184259</id>
    <date>
      <year-start>1815</year-start>
      <year-end>1815</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laast een dag ik zou gaan <rhyme label="a" type="f">maken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Den Visiet en <rhyme label="b" type="m">Compliment</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maar een Vrouwsbeeld tot myn <rhyme label="c" type="f">zeide</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Of ik was met haar <rhyme label="b" type="m" corresp="#s1:l2">content</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Maar ik maak myn <rhyme label="d" type="f">reverensie</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Nam myn stokje en ik ging <rhyme label="e" type="m">weg</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Sus zei ik myn lieve <rhyme label="f" type="f">popje</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Ben ik u hier in de <rhyme label="e" type="m" corresp="#s1:l6">weg</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">En ik moet u nog <rhyme label="a" type="f">verhalen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Wat men overkomen <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En wat list de Meisjes <rhyme label="c" type="f">bruiken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En ik my ook niet <rhyme label="b" type="m" corresp="#s2:l2">vergis</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dat zy Jonkmans graag <rhyme label="d" type="f">vervoeren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En ons brengen op de <rhyme label="e" type="m">weg</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Maar wy zullen ons niet <rhyme label="f" type="f">verstoren</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Na die hoog geslagen <rhyme label="e" type="m" corresp="#s2:l6">pret</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wel wat denkt gy lieve <rhyme label="a" type="f">Meisjes</rhyme>?</l>
        <l id="s3:l2" met="4" type="+-">Denkt gy dat ik ben heel <rhyme label="b" type="m">stom</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">Denkt gy dat 't Matrooze <rhyme label="c" type="f">vleisje</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Groeit gelyk een wilde <rhyme label="b" type="m" corresp="#s3:l2">Boom</rhyme>?</l>
        <l id="s3:l5" met="4" type="+-">Neen, weg met al uw <rhyme label="d" type="f">gedagten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Van my krygt gy geen <rhyme label="e" type="m">gehoor</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Wilt gy een Matroos <rhyme label="f" type="f">veragten</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Dan dwaald gy van 't regte <rhyme label="e" type="m" corresp="#s3:l6">spoor</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik zal nooit geen Meisje <rhyme label="a" type="f">zoeken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Die de hoogmoed veel <rhyme label="b" type="m">behaagd</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Maar ik zal 't <rhyme label="c" type="f">onderzoeken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Wat zoete liefde <rhyme label="b" type="m" corresp="#s4:l2">draagd</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Ik zal al de Meisjes <rhyme label="d" type="f">roemen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die maar eer en deugd <rhyme label="e" type="m">bezit</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Zulke die zyn waard te <rhyme label="f" type="f">roemen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En die zyn myn doel en <rhyme label="e" type="m" corresp="#s4:l6">wit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ziet ik doe nog maar een <rhyme label="a" type="f">reisje</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ja al op de woeste <rhyme label="b" type="m">Zee</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daar heeft ook al meenig <rhyme label="c" type="f">Meisje</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Haar Beminde ook al <rhyme label="b" type="m" corresp="#s5:l2">mee</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Want ziet het Matrooze <rhyme label="d" type="f">leven</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Haar plyzier is op de <rhyme label="e" type="m">Zee</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Tusschen Aard en Hemel <rhyme label="f" type="f">zweeven</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Tot wy komen op de <rhyme label="e" type="m" corresp="#s5:l6">Zee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ik wil myn de tyd <rhyme label="a" type="f">ontgeven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Van de aldereerste <rhyme label="b" type="m">Stam</rhyme>!</l>
        <l id="s6:l3" met="4" type="+-">Adam heeft een ploeg <rhyme label="c" type="f">gedreven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Eva zat in Tuin en <rhyme label="b" type="m" corresp="#s6:l2">Span</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Kleeren die zyn haar <rhyme label="d" type="f">gegeven</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En dat van een schapen <rhyme label="e" type="m">vel</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Yder maar vernoegt moet <rhyme label="f" type="f">leven</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Dan zo is het altyd <rhyme label="e" type="m" corresp="#s6:l6">wel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
