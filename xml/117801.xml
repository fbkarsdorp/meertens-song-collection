<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>117801</id>
    <date>
      <year-start>1741</year-start>
      <year-end>1741</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Truitje mijn zoete engelin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mompeer, myn Hart is vol van <rhyme label="a" type="m">Pyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ag! ag! wat mag myn Nakent <rhyme label="a" type="m" corresp="#s1:l1">zyn</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Want heele Nagte leg ik staag en <rhyme label="b" type="f">Drome</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Als dat myn Septer en myn <rhyme label="c" type="m">Kroon</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Van Koning Karel, Keyzers <rhyme label="c" type="m" corresp="#s1:l4">Zoon</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Nog zal werden <rhyme label="b" type="f" corresp="#s1:l3">ontnome</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Waar zal ik <rhyme label="d" type="m">heên</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Nog zal werden <rhyme label="b" type="f" corresp="#s1:l3">ontnome</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Engelant komt al op myn <rhyme label="a" type="m">An</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Met menigte van Duyzent <rhyme label="a" type="m" corresp="#s2:l1">Man</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Om Koning Karel te <rhyme label="b" type="f">Konvoyeren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En zo te Brengen in myn <rhyme label="c" type="m">Land</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Waar door dat ik met Grote <rhyme label="c" type="m" corresp="#s2:l4">schand</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Zal moete <rhyme label="b" type="f" corresp="#s2:l3">Retireren</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Weêr na <rhyme label="d" type="m">Vrankryk</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Zal moete <rhyme label="b" type="f" corresp="#s2:l3">Retireren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daar by komt de Hollandze <rhyme label="a" type="m">Vloot</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Met Oorlogschepen Klyn en <rhyme label="a" type="m" corresp="#s3:l1">Groot</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">En bom Kitse-Branders <rhyme label="b" type="f">uytgelezen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Om in myn Land te valle <rhyme label="c" type="m">dan</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En myn zo eens te Tasten <rhyme label="c" type="m" corresp="#s3:l4">an</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat my Eylaas doet <rhyme label="b" type="f" corresp="#s3:l3">Beven</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Komt en ziet <rhyme label="d" type="m">Hier</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Dat my Eylaas doet <rhyme label="b" type="f" corresp="#s3:l3">Beven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ik zeg Groot-Vader voor het <rhyme label="a" type="m">Lest</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En zo gy niet en doet u <rhyme label="a" type="m" corresp="#s4:l1">Best</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Zo zullen Wy het al te zaam <rhyme label="b" type="f">beklagen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Want zo Gy hier niet voor en <rhyme label="c" type="m">zyt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Zo raak ik zeker myn Kroon <rhyme label="c" type="m" corresp="#s4:l4">kwyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En myn Volk die krygt <rhyme label="b" type="f" corresp="#s4:l3">slagen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Weg is den <rhyme label="d" type="m">Bruy</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">En myn Volk die krygt <rhyme label="b" type="f" corresp="#s4:l3">slagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Komt hier gy Papen en <rhyme label="a" type="m">Jesuwiet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ey helpt myn uyt dit <rhyme label="a" type="m" corresp="#s5:l1">Verdriet</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Gy Quezels, Klopjes, Nonnen en <rhyme label="b" type="f">Bagynen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Ik Bid u Offerkist op <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Eer ik uyt Spanje Vlugten <rhyme label="c" type="m" corresp="#s5:l4">moet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Verzet u Kap en <rhyme label="b" type="f" corresp="#s5:l3">Keuvel</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Komt myn te <rhyme label="d" type="m">Baat</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Verzet u Kap en <rhyme label="b" type="f" corresp="#s5:l3">Keuvel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Want ik ben Dapper in de <rhyme label="a" type="m">Ly</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Den Spanjaart Roept van alle <rhyme label="a" type="m" corresp="#s6:l1">zy</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Lang leeft de Koning van <rhyme label="b" type="f">Allemanjen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dan Roepen zy nog al <rhyme label="c" type="m">Mafoy</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Viva, Viva, Viva la <rhyme label="c" type="m" corresp="#s6:l4">Roy</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Koning Karel van <rhyme label="b" type="f" corresp="#s6:l3">Spanjen</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Tot mynder <rhyme label="d" type="m">Spyt</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Koning Karel van <rhyme label="b" type="f" corresp="#s6:l3">Spanjen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
