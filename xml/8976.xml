<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8976</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Onze vader in hemelrijk</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">MYn Gelooff aen Godt Almachtich <rhyme label="a" type="m">cleefft</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Den Vader die gheschapen <rhyme label="a" type="m" corresp="#s1:l1">heeft</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Door syn Woort HEmel ende <rhyme label="b" type="m">Aerd'</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ons wt Ghenaed' Eewich <rhyme label="b" type="m" corresp="#s1:l3">bewaert</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Lijff, Siel end Naeringh' hy ons <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">End om syns Soons willen ons <rhyme label="c" type="m" corresp="#s1:l5">Lieft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Mijn Gelooff houdt oock aen JEsum <rhyme label="a" type="m">Christ</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die GOdes Eenighe Soon <rhyme label="a" type="m" corresp="#s2:l1">is</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Van Vader Eewichlijck <rhyme label="b" type="m">gheboor'n</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Door hem all' Dinck bestaet te <rhyme label="b" type="m" corresp="#s2:l3">voor'n</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die t'onsen Heyl van Hemel <rhyme label="c" type="m">quam</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">End onse Sonden op hem <rhyme label="c" type="m" corresp="#s2:l5">nam</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Die als den Enghel ons <rhyme label="a" type="m">bewijst</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ontfanghen vanden heyl'ghen <rhyme label="a" type="m" corresp="#s3:l1">Gheest</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End van Maria Maghet <rhyme label="b" type="m">tzaert</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Christ Godt en Mensch ghebooren <rhyme label="b" type="m" corresp="#s3:l3">wert</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">End ons door <rhyme label="c" type="m">t'Euangelion</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Des Vaders Naem heeft condt <rhyme label="c" type="m" corresp="#s3:l5">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Heeft selvest hem ten Offer <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Aent Cruys voor onse <rhyme label="a" type="m" corresp="#s4:l1">Missedaet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gheoffert willich met <rhyme label="b" type="m">ghedult</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Dat hy betaelden onse <rhyme label="b" type="m" corresp="#s4:l3">Schult</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">End ons verloosd' wt aller <rhyme label="c" type="m">Noot</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Door syn Bloet end onschuldig' <rhyme label="c" type="m" corresp="#s4:l5">Doot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="+-">Ghestorven end gheleyt int <rhyme label="a" type="m">Graff</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ghevaren in die Hell' hen <rhyme label="a" type="m" corresp="#s5:l1">aff</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Des Duyvels macht end werck <rhyme label="b" type="m">verstoort</rhyme></l>
        <l id="s5:l4" met="4" type="-+">End hem met Ketten bonden <rhyme label="b" type="m" corresp="#s5:l3">hart</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dat hy ons niet meer schaden <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Dien, die Christo ghehooren <rhyme label="c" type="m" corresp="#s5:l5">an</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="+-">Des derden Daeghs van Doodt op <rhyme label="a" type="m">stond'</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dien hy gheweldich <rhyme label="a" type="m" corresp="#s6:l1">Overwond'</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">End daer nae op ten HEmel <rhyme label="b" type="m">steech</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Soo Heerlick hy Victory <rhyme label="b" type="m" corresp="#s6:l3">kreech</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ons nu in syner <rhyme label="c" type="m">Majestaet</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Verbidden end beschudden <rhyme label="c" type="m" corresp="#s6:l5">gaet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="+-">Van daer hy comen sal <rhyme label="a" type="m">subijt</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Met groote Macht ter laetsten <rhyme label="a" type="m" corresp="#s7:l1">tijdt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Te Rechten dat Mensch'lijck <rhyme label="b" type="m">Geslecht</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">End elcken een Beloonen <rhyme label="b" type="m" corresp="#s7:l3">recht</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Die G'loovighen met Hemels <rhyme label="c" type="m">vreucht</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Die Godloosen met Helles <rhyme label="c" type="m" corresp="#s7:l5">leydt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="+-">Mijn G'looff houdt aen den Heyligen <rhyme label="a" type="m">Gheest</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Die ons de Waerheyt Godes <rhyme label="a" type="m" corresp="#s8:l1">wijst</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Van Vader en den Soon wt <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Leert die Propheten ons te <rhyme label="b" type="m" corresp="#s8:l3">baet</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Jae onsen Gheest oock Tuychnis <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Dat ons Godt als die Kinder <rhyme label="c" type="m" corresp="#s8:l5">Liefft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="+-">Ick gelooff een Christelijck <rhyme label="a" type="m">Gemeen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Daer Christus is dat Hooft <rhyme label="a" type="m" corresp="#s9:l1">alleen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Daer hy oock blijft tot aen den <rhyme label="b" type="m">Eyndt</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Door Godes Woort end <rhyme label="b" type="m" corresp="#s9:l3">Sacrament</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Die door hem heeft ghemeynschap <rhyme label="c" type="m">vroet</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Aller Geestlijcker gaven <rhyme label="c" type="m" corresp="#s9:l5">goet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="+-">Ick Ghelooff dat onse Sond' end <rhyme label="a" type="m">Schult</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Vergeven syn door Christus <rhyme label="a" type="m" corresp="#s10:l1">Huld'</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">End dat wy hebben <rhyme label="b" type="m">staedichlijck</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Die Sleutelen van <rhyme label="b" type="m" corresp="#s10:l3">t'Hemelrijck</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Ten Troost die Boetveerdich <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Den Rouw'loosen tot Eewig' <rhyme label="c" type="m" corresp="#s10:l5">Pijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="+-">Ick gelooff dat die verstorven <rhyme label="a" type="m">Lijff</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Niet altijdts in den Doode <rhyme label="a" type="m" corresp="#s11:l1">blijff</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Maer sal noch wederom <rhyme label="b" type="m">Opstaen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">End in dat Eewich Leven <rhyme label="b" type="m" corresp="#s11:l3">gaen</rhyme>.</l>
        <l id="s11:l5" met="4" type="-+">Daer wy met Godt in <rhyme label="c" type="m">Eewicheyt</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Wel varen sonder eenich <rhyme label="c" type="m" corresp="#s11:l5">Leydt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="5" type="+-">Wie dit Gelooff waerachtich <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Die selffd' in GOds Ghenade <rhyme label="a" type="m" corresp="#s12:l1">leeft</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">End sal met goeden Wandel <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Wel Vruchtbaer int Gheloove <rhyme label="b" type="m" corresp="#s12:l3">zijn</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Des help ons HEEre Jesu <rhyme label="c" type="m">Christ</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Want ghy des G'looves Veste <rhyme label="c" type="m" corresp="#s12:l5">bist</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
