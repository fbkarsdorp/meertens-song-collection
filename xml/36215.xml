      <tag>devout song</tag>
    </tags>
    <melody>Sarabande Pinel</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Mijnen schepper hoogh <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Mijnen Godt <rhyme label="b" type="m">ghebenedijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ghy hebt hier mijn hert <rhyme label="a" type="f" corresp="#s1:l1">ghegheven</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Heerschappy ter deser <rhyme label="b" type="m" corresp="#s1:l2">tijdt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Over al dat heeft het <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Op de aerde, Zee en <rhyme label="d" type="m">locht</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">K'sou die gern <rhyme label="c" type="f" corresp="#s1:l5">overgheven</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Dat ick sulcx door u <rhyme label="d" type="m" corresp="#s1:l6">vermocht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Siet ick heb soo langh' op <rhyme label="a" type="f">erden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dese heerschappy <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En om daer in te <rhyme label="a" type="f" corresp="#s2:l1">volherden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Sulcx mijn hert niet goet en <rhyme label="b" type="m" corresp="#s2:l2">vindt</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Ick wil u alleen <rhyme label="c" type="f">aenveerden</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Maeckt my t'werelts ruyse <rhyme label="d" type="m">quijt</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Want ten is van gheender <rhyme label="c" type="f" corresp="#s2:l5">weerden</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Dat ghy selve niet en <rhyme label="d" type="m" corresp="#s2:l6">zijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Daerom wil ick t'swerelts <rhyme label="a" type="f">saeken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Altemael verfoeyen <rhyme label="b" type="m">nu</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Jae ick wil die heel <rhyme label="a" type="f" corresp="#s3:l1">versaecken</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Maer k'en cant niet sonder <rhyme label="b" type="m" corresp="#s3:l2">u</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Wilt my hier bequaem toe <rhyme label="c" type="f">maecken</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Dat ick sulcx volbringhen <rhyme label="d" type="m">mach</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Laet my desen troost hier <rhyme label="c" type="f" corresp="#s3:l5">smaecken</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Dien ick soecke nacht en <rhyme label="d" type="m" corresp="#s3:l6">dach</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Twee goey vleughelen my <rhyme label="a" type="f">faelen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Om de werelt te <rhyme label="b" type="m">ontvlien</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Waer sal ick die nu gaen <rhyme label="a" type="f" corresp="#s4:l1">haelen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Ick en vind' die by gheen <rhyme label="b" type="m" corresp="#s4:l2">lie'n</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Sulcke moeten van u <rhyme label="c" type="f">daelen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">En ghy gheeft die, die ghy <rhyme label="d" type="m">wilt</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Hierom comt met uwe <rhyme label="c" type="f" corresp="#s4:l5">straelen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Gheeft my die, o ghever <rhyme label="d" type="m" corresp="#s4:l6">mildt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gheeft my van een duyf de <rhyme label="a" type="f">pennen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Om te vlieghen uyt dit <rhyme label="b" type="m">dal</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">K'sal hier me my selven <rhyme label="a" type="f" corresp="#s5:l1">wennen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">vlieghen boven t'schepsel <rhyme label="b" type="m" corresp="#s5:l2">al</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Gheeft my dat ick u mach <rhyme label="c" type="f">kennen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Wie ghy syt, o eeuwigh <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Dan sal ick my by u <rhyme label="c" type="f" corresp="#s5:l5">mennen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Al door u ghenaede <rhyme label="d" type="m" corresp="#s5:l6">soet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">K'sal hier me, uyt al mijn <rhyme label="a" type="f">crachten</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Schauwen al dat u <rhyme label="b" type="m">mishaeght</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En oock doen uyt mijn <rhyme label="a" type="f" corresp="#s6:l1">ghedachten</rhyme></l>
        <l id="s6:l4" met="4" type="+-">T'ghene dat van u <rhyme label="b" type="m" corresp="#s6:l2">vertraeght</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Ick sal altijdt voorder <rhyme label="c" type="f">trachten</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Naer u Goddelijck <rhyme label="d" type="m">aenschijn</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">En by daeghen, en by <rhyme label="c" type="f" corresp="#s6:l5">nachten</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Altijt u ghetrauwe <rhyme label="d" type="m" corresp="#s6:l6">syn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Orlof Godt mijns herts <rhyme label="a" type="f">verlanghen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Wilt versaeden mijnen <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Laet my mijn begheert <rhyme label="a" type="f" corresp="#s7:l1">ontfanghen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Stelt mijn hertjen eens <rhyme label="b" type="m" corresp="#s7:l2">gherust</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Wilt mijn lyf, en ziel <rhyme label="c" type="f">aen-hanghen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Vleughels van een duyfken <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Op dat ick u eens mach <rhyme label="c" type="f" corresp="#s7:l5">vanghen</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Boven in des Hemels <rhyme label="d" type="m" corresp="#s7:l6">throon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
