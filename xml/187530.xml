<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187530</id>
    <date>
      <year-start>1693</year-start>
      <year-end>1693</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O zalig heilig Bethlehem</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Heere Godt van machten <rhyme label="a" type="m">groot</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">Wiens lof niet is om te <rhyme label="b" type="f">volprijsen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Gy die verlost uyt angst en <rhyme label="a" type="m" corresp="#s1:l1">noot</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Alle die u eere <rhyme label="b" type="f" corresp="#s1:l2">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Siet wy gaen nu ter Zeewaert <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Om goede handel te <rhyme label="b" type="f">plegen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Geeft doch Heer voorspoet en <rhyme label="a" type="m" corresp="#s2:l1">gewin</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En leyt ons in bequame <rhyme label="b" type="f" corresp="#s2:l2">wegen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy die nu kruyssen heen en <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Over golven en <rhyme label="b" type="f">zee-baren</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wilt onsen Leyts-man wesen <rhyme label="a" type="m" corresp="#s3:l1">Heer</rhyme>!</l>
        <l id="s3:l4" met="4" type="-+">En door u stercke hand <rhyme label="b" type="f" corresp="#s3:l2">bewaren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Want sonder u Heer zijn wy <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Daerom wilt onws bystandigh <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Op dat wy in noot en <rhyme label="a" type="m" corresp="#s4:l1">verdriet</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Mogen u aenmercken met <rhyme label="b" type="f" corresp="#s4:l2">vreesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Donder en den Blixem <rhyme label="a" type="m">fel</rhyme>!</l>
        <l id="s5:l2" met="4" type="-+">Die gy ons dickwils stelt voor <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Om dat wy niet zouden <rhyme label="a" type="m" corresp="#s5:l1">rebel</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Zijn, maer na sijnen wil ons gaen <rhyme label="b" type="f" corresp="#s5:l2">bogen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wy die somtijts in het <rhyme label="a" type="m">onweer</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Gebruycken wel treuren en <rhyme label="b" type="f">weenen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En roepen met een groot <rhyme label="a" type="m" corresp="#s6:l1">begeer</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Heer wilt ons u hulpe <rhyme label="b" type="f" corresp="#s6:l2">verleenen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Soo haest de Heer ons dan <rhyme label="a" type="m">verhoort</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En dat de Zee staet geheel <rhyme label="b" type="f">stille</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Soo doen wy niet al na sijn <rhyme label="a" type="m" corresp="#s7:l1">woort</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Maer volgen onsen eygen <rhyme label="b" type="f" corresp="#s7:l2">wille</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Och, â€™t is tijd om te beklagen <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Dat wy hier aldus sondigh <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daer den tijd vlieght henen seer <rhyme label="a" type="m" corresp="#s8:l1">snel</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Die God ons niet weder sal <rhyme label="b" type="f" corresp="#s8:l2">geven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Oorlof laet dit nu zijn <rhyme label="a" type="m">bedacht</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En gebruyckt doch den tijd <rhyme label="b" type="f">bequame</rhyme></l>
        <l id="s9:l3" met="4" type="-+">En vreest den Heer dagh ende <rhyme label="a" type="m" corresp="#s9:l1">nacht</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Al is het weer heel <rhyme label="b" type="f" corresp="#s9:l2">aengename</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
