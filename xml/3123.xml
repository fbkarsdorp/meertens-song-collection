  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">PRins Cardinael van <rhyme label="a" type="f">Spaengjen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Is nu uwen moet <rhyme label="b" type="m">ghedaen</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Dat ghy den Prins d' <rhyme label="a" type="f" corresp="#s1:l1">Oraengjen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Niet en soeckt om op te <rhyme label="b" type="m" corresp="#s1:l2">slaen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Of heeft hy te stercken <rhyme label="c" type="f">haven</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Doen graven doen <rhyme label="c" type="f" corresp="#s1:l5">graven</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Dat ghy 't niet en vint <rhyme label="b" type="m" corresp="#s1:l2">geraen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Lest quaemt ghy tot <rhyme label="a" type="f">Rijs-bergen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Sloegh u Leger daer ter <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Geen aen-val en dorst ghy <rhyme label="a" type="f" corresp="#s2:l1">vergen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Want wy pasten op <rhyme label="b" type="m" corresp="#s2:l2">Geweer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">By Nacht en by dagh in 't <rhyme label="c" type="f">Wapen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">In 't Wapen. En 't <rhyme label="c" type="f" corresp="#s2:l5">slapen</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Lieten wy voor 's Princen <rhyme label="b" type="m" corresp="#s2:l2">eer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wast niet om te <rhyme label="a" type="f">begecken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Dat ghy door u Trom <rhyme label="b" type="m">af-riep</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat ghy nu wout <rhyme label="a" type="f" corresp="#s3:l1">vertrecken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Die u sien wou haestigh <rhyme label="b" type="m" corresp="#s3:l2">liep</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En ginght op straet <rhyme label="c" type="f">vertoonen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Vertoonen, tot <rhyme label="c" type="f" corresp="#s3:l5">loonen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Maer sijn Hoogheyt niet en <rhyme label="b" type="m" corresp="#s3:l2">sliep</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dus komt nu selfs eens <rhyme label="a" type="f">kijcken</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Na de brave Stadt <rhyme label="b" type="m">Breda</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Die nu gansch wil <rhyme label="a" type="f" corresp="#s4:l1">bezwijcken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Want wy komen haer te <rhyme label="b" type="m" corresp="#s4:l2">na</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En hoort hoe de <rhyme label="c" type="f">schaly-daken</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Door 't kraken, door 't <rhyme label="c" type="f" corresp="#s4:l5">kraken</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Tuygen over groote <rhyme label="b" type="m" corresp="#s4:l2">scha</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Want ons Canons <rhyme label="a" type="f">gedonder</rhyme></l>
        <l id="s5:l2" met="4" type="+-">(Als een vreeselijck <rhyme label="b" type="m">gebral</rhyme>)</l>
        <l id="s5:l3" met="3" type="-+">Is voor Paep-Jan een <rhyme label="a" type="f" corresp="#s5:l1">wonder</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Door het spelen op de <rhyme label="b" type="m" corresp="#s5:l2">Wal</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En 't werpen van de <rhyme label="c" type="f">Garnaten</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Ontlaten de <rhyme label="c" type="f" corresp="#s5:l5">Straten</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Dat hy maeckt een groot <rhyme label="b" type="m" corresp="#s5:l2">geschal</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hebt ghy door <rhyme label="a" type="f">moedigheden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">(Tot u Goveneur sijn <rhyme label="b" type="m">eer</rhyme>)</l>
        <l id="s6:l3" met="3" type="-+">De Wallen <rhyme label="a" type="f" corresp="#s6:l1">door-gesneden</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">En 't Geschut doen sincken <rhyme label="b" type="m" corresp="#s6:l2">neer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Om ons Galdery te <rhyme label="c" type="f">stuyten</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">En sluyten ons <rhyme label="c" type="f" corresp="#s6:l5">buyten</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Maer u stoutheyt gelt niet <rhyme label="b" type="m" corresp="#s6:l2">meer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Twee van de naest' <rhyme label="a" type="f">Bat'rijen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Schieten met sulcken <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dat onse <rhyme label="a" type="f" corresp="#s7:l1">Gaelderijen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Is de Reijs haest <rhyme label="b" type="m" corresp="#s7:l2">in-gebracht</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">De Bies-brugh en andere <rhyme label="c" type="f">practycken</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Af-wijcken, <rhyme label="c" type="f" corresp="#s7:l5">af-wijcken</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Om te leggen in de <rhyme label="b" type="m" corresp="#s7:l2">Gracht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Het Vier-wercken dat gestelt <rhyme label="a" type="f">is</rhyme></l>
        <l id="s8:l2" met="4" type="+-">'t Geen de Hant-granaten <rhyme label="b" type="m">schiet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Een teycken wat gewelt <rhyme label="a" type="f" corresp="#s8:l1">is</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Datmen op den Spaengjaert <rhyme label="b" type="m" corresp="#s8:l2">biet</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wilt ghy nu den Storm vol <rhyme label="c" type="f">krachten</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Met machten <rhyme label="c" type="f" corresp="#s8:l5">verwachten</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Vry van Paep, en Monn'ken <rhyme label="b" type="m" corresp="#s8:l2">vliedt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Het Hoeren-werck zijn <rhyme label="a" type="f">gronden</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Is gemest met Spaengjaerts <rhyme label="b" type="m">bloedt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Jebt ghy doen niet <rhyme label="a" type="f" corresp="#s9:l1">bevonden</rhyme></l>
        <l id="s9:l4" met="4" type="+-">d' Edel Prins sijn kloecken <rhyme label="b" type="m" corresp="#s9:l2">moet</rhyme>?</l>
        <l id="s9:l5" met="3" type="-+">Doen vreeslijck de Mijnen <rhyme label="c" type="f">sprongen</rhyme></l>
        <l id="s9:l6" met="2" type="-+">Hoe songen, en <rhyme label="c" type="f" corresp="#s9:l5">sprongen</rhyme></l>
        <l id="s9:l7" met="4" type="+-">Doen u Wals en Spaensch <rhyme label="b" type="m" corresp="#s9:l2">gebroedt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dus wil ick u wat <rhyme label="a" type="f">raden</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Prince Cardinael <rhyme label="b" type="m">Infant</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Treckt uyt u <rhyme label="a" type="f" corresp="#s10:l1">Krijghs-gewaden</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Reyst weer na u Spaensche <rhyme label="b" type="m" corresp="#s10:l2">Landt</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En draeght daer de <rhyme label="c" type="f">Geestelijckheden</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Haer kleeden, met <rhyme label="c" type="f" corresp="#s10:l5">vreeden</rhyme>.</l>
        <l id="s10:l7" met="4" type="+-">Die u beter staen ter <rhyme label="b" type="m" corresp="#s10:l2">handt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ghy Papen en <rhyme label="a" type="f">Bagijnen</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Hanght de Kap nu op den <rhyme label="b" type="m">Tuyn</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Minne-broers en <rhyme label="a" type="f" corresp="#s11:l1">Capecijnen</rhyme></l>
        <l id="s11:l4" met="4" type="+-">Scheert niet meer den kale <rhyme label="b" type="m" corresp="#s11:l2">Cruyn</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Broer Jacus en Pater <rhyme label="c" type="f">Symen</rhyme></l>
        <l id="s11:l6" met="2" type="-+">Die swijmen, die <rhyme label="c" type="f" corresp="#s11:l5">zwijmen</rhyme></l>
        <l id="s11:l7" met="4" type="+-">Want haer Klap-mus staet te <rhyme label="b" type="m" corresp="#s11:l2">schuyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
