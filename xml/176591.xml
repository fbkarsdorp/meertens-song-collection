        <lon>52.4570766</lon>
        <lat>5.0373214</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Schoonste herderin</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">TRuytje soete <rhyme label="a" type="f">maatje</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Warom toont gy u so <rhyme label="b" type="m">straf</rhyme>?</l>
        <l id="s1:l3" met="3" type="+-">Seg dog <rhyme label="a" type="f" corresp="#s1:l1">Cameratje</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Sult gy niet eens laten <rhyme label="b" type="m" corresp="#s1:l2">af</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Of is Krelis 't liefste <rhyme label="c" type="m">kint</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Die je meer als my <rhyme label="c" type="m" corresp="#s1:l5">bemint</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Och kon ik u <rhyme label="a" type="f">krijgen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Truytje-maat ik segje <rhyme label="b" type="m">dat</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Soete Koeck en <rhyme label="a" type="f" corresp="#s2:l1">vijgen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Gaf ik jou te eeten <rhyme label="b" type="m" corresp="#s2:l2">sat</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Oesters Tongen en <rhyme label="c" type="m">Pladdijs</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dus soo nemt me benje <rhyme label="c" type="m" corresp="#s2:l5">wijs</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Soud'je Krelis <rhyme label="a" type="f">Meesen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Neemen, dat 's een droncken <rhyme label="b" type="m">bloet</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Ik sal Nugter <rhyme label="a" type="f" corresp="#s3:l1">weesen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Lieve Truytje 'k ben so <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ook so ben ik stijf en <rhyme label="c" type="m">stout</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Krelis-beur is slap en <rhyme label="c" type="m" corresp="#s3:l5">oudt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wel ik segje <rhyme label="a" type="f">Truytje</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Laet loopen dan ouden <rhyme label="b" type="m">Grijn</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Komt en weest mijn <rhyme label="a" type="f" corresp="#s4:l1">Bruytje</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'k Sal u geven bier en <rhyme label="b" type="m" corresp="#s4:l2">Wijn</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Leck're Bout, ook pos en <rhyme label="c" type="m">Baars</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Lieve Truytje 'k word schier <rhyme label="c" type="m" corresp="#s4:l5">aars</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Soete <rhyme label="a" type="f">Smodder-muyltje</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Benje niet mijn Botter <rhyme label="b" type="m">doos</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">En mijn eygen <rhyme label="a" type="f" corresp="#s5:l1">Uyltje</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dien ik voor mijn lief <rhyme label="b" type="m" corresp="#s5:l2">verkoos</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Truytje 'k word soo nieu, soo <rhyme label="c" type="m">groen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Soete Schaapje laat het <rhyme label="c" type="m" corresp="#s5:l5">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Mogt ik jou eens <rhyme label="a" type="f">Meysje</rhyme>:</l>
        <l id="s6:l2" met="4" type="+-">(Ay je weet wel ik <rhyme label="b" type="m">mien</rhyme>)</l>
        <l id="s6:l3" met="3" type="+-">Vatten by u <rhyme label="a" type="f" corresp="#s6:l1">vleysje</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Je soud soete keure <rhyme label="b" type="m" corresp="#s6:l2">sien</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Ja ik soud jou 't best uyt <rhyme label="c" type="m">lijf</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Geven, alsje waart mijn <rhyme label="c" type="m" corresp="#s6:l5">Wijf</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Og mijn blaas van <rhyme label="a" type="f">binnen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Lieve truy je brant als <rhyme label="b" type="m">vier</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Dus wilt my weer <rhyme label="a" type="f" corresp="#s7:l1">minne</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Komt mijn soete Venus <rhyme label="b" type="m" corresp="#s7:l2">dier</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Komt dog lieve Truytje <rhyme label="c" type="m">dra</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Komt dog geeft my 't woortje <rhyme label="c" type="m" corresp="#s7:l5">ja</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
