<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22429</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Herr Christ der einig Gottes Sohn</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Christe O eenich <rhyme label="a" type="f">Sone</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Godt Vaders in <rhyme label="b" type="m">eewicheyt</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Wt synes hertens <rhyme label="a" type="f" corresp="#s1:l1">throne</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ghesproten in der <rhyme label="b" type="m" corresp="#s1:l2">waerheyt</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ghy zijt die morghen <rhyme label="c" type="f">Sterre</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dijnen glants die schijnt so <rhyme label="c" type="f" corresp="#s1:l5">verre</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Bouen anderen Sterren <rhyme label="d" type="m">claerheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Voor ons een mensche <rhyme label="a" type="f">gheboren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In den laetsten tijt <rhyme label="b" type="m">bereyt</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Wiens Moeder niet heeft <rhyme label="a" type="f" corresp="#s2:l1">verloren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Haer maechdelijcke <rhyme label="b" type="m" corresp="#s2:l2">cuyscheyt</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Den doot heeft hy <rhyme label="c" type="f">ghebroken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Den Hemel voor ons <rhyme label="c" type="f" corresp="#s2:l5">ontloken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En dat leuen ons <rhyme label="d" type="m">toegheseyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laet ons in dijne <rhyme label="a" type="f">liefde</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ende kennisse wassen <rhyme label="b" type="m">seer</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Int ghelooue <rhyme label="a" type="f" corresp="#s3:l1">onghegriefde</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met den Geest u dienende <rhyme label="b" type="m" corresp="#s3:l2">meer</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Dat wy hier moeghen <rhyme label="c" type="f">beeruen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Int herte u soete <rhyme label="c" type="f" corresp="#s3:l5">conseruen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ende dorsten nae dijne <rhyme label="d" type="m">leer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy Schepper aller <rhyme label="a" type="f">dinghen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghy Vaderlijcke <rhyme label="b" type="m">cracht</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Ghy en zijt niet om <rhyme label="a" type="f" corresp="#s4:l1">bedwinghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Sterck synde met eewighe <rhyme label="b" type="m" corresp="#s4:l2">macht</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Laet ons herten u <rhyme label="c" type="f">ghewinnen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En bewaert al onse <rhyme label="c" type="f" corresp="#s4:l5">sinnen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van den dolenden sondighen <rhyme label="d" type="m">nacht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Doot in ons door <rhyme label="a" type="f">ghenade</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Den ouden Adam <rhyme label="b" type="m">cranck</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Verweckt eenen nieuwen <rhyme label="a" type="f" corresp="#s5:l1">gherade</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die daer leeft nae dijnen <rhyme label="b" type="m" corresp="#s5:l2">danck</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Wel hem op deser <rhyme label="c" type="f">eerden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die u van herten <rhyme label="c" type="f" corresp="#s5:l5">aenueerden</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En hebben nae u <rhyme label="d" type="m">verlanck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
