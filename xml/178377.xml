<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178377</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Wie schön leuchtet der Morgenstern</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Hoe schoon licht ons de <rhyme label="a" type="m">Morgenster</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">d'Oranie Zon schiet ook van <rhyme label="a" type="m" corresp="#s1:l1">ver</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Zyn straalen in ons <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Hier schitterd ook 't Oranje <rhyme label="c" type="m">licht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Zeer heerlyk in ons <rhyme label="c" type="m" corresp="#s1:l4">aangezicht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En flonkerd op de <rhyme label="b" type="f" corresp="#s1:l3">boogen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Dit meld, <rhyme label="d" type="m">verteld</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Blyde dingen, om te <rhyme label="e" type="f">zingen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">d' Eer <rhyme label="f" type="f">t'ontvouwen</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Van Wilhelmus van <rhyme label="f" type="f" corresp="#s1:l9">Nassouwen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'t Is nu de Heugelyke <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Toen Neerlands Vorst eerst 't Zonligt <rhyme label="a" type="m" corresp="#s2:l1">zag</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En voor ons werd <rhyme label="b" type="f">gebooren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Hoe helder scheen d'Oranje <rhyme label="c" type="m">Zon</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Hoe vol was toen die Vreugde <rhyme label="c" type="m" corresp="#s2:l4">Bron</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dit klonk alom in <rhyme label="b" type="f" corresp="#s2:l3">d'ooren</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Hoe wier, toen <rhyme label="d" type="m">hier</rhyme></l>
        <l id="s2:l8" met="4" type="+-">'t Opperweezen; hoog <rhyme label="e" type="f">gepreezen</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Die <rhyme label="f" type="f">vereerden</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Ons, 't geen wy van hem <rhyme label="f" type="f" corresp="#s2:l9">begeerden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dus vinden wy ons duur <rhyme label="a" type="m">verplicht</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Om heeden by dit vreugde <rhyme label="a" type="m" corresp="#s3:l1">licht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Des Heeren lof te <rhyme label="b" type="f">uiten</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Daar Hy Prins Willem weer <rhyme label="c" type="m">verjaard</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Zyn dierbre Echtgenoot <rhyme label="c" type="m" corresp="#s3:l4">bewaard</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Met haar Oranje <rhyme label="b" type="f" corresp="#s3:l3">Spruiten</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Lang moog leeven, God wil <rhyme label="e" type="f">geeven</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">'t Weelrig <rhyme label="f" type="f">bloeijen</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Dat Oranjes Stam mag <rhyme label="f" type="f" corresp="#s3:l9">groeijen</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zoo zal nog lang d'Oranje <rhyme label="a" type="m">gloed</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Waar by het goud verdooven <rhyme label="a" type="m" corresp="#s4:l1">moet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Haar glorie af doen <rhyme label="b" type="f">schynen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Zoo zal nog lang dit heerlyk <rhyme label="c" type="m">ligt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dat hier geduurig word <rhyme label="c" type="m" corresp="#s4:l4">gesticht</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Door Nevels nooit <rhyme label="b" type="f" corresp="#s4:l3">verdwynen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Zoo zal, dit <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Ons tot zingen, staag <rhyme label="e" type="f">aandringen</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Van <rhyme label="f" type="f">Nassouwen</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Dat wy hem nog lang <rhyme label="f" type="f" corresp="#s4:l9">aanschouwen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy blyf nog lang tot heil <rhyme label="a" type="m">cieraad</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Van Neerlands Kerk en <rhyme label="a" type="m" corresp="#s5:l1">Burgerstaat</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gelyk in vroeger <rhyme label="b" type="f">tyden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">God schenkt hem Saalmons wys <rhyme label="c" type="m">beleid</rhyme></l>
        <l id="s5:l5" met="4" type="-+">En Davids moet en <rhyme label="c" type="m" corresp="#s5:l4">dapperheid</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Een Gidion ten <rhyme label="b" type="f" corresp="#s5:l3">strydee</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">God zy, na <rhyme label="d" type="m">by</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Met zyn zeegen, t'ajler <rhyme label="e" type="f">weegen</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Voor <rhyme label="f" type="f">gevaaren</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Wil hy Neerlands Vorst <rhyme label="f" type="f" corresp="#s5:l9">bewaaren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Och dat men eens die blyde <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Van Vreede, Liefd' en Eendragt <rhyme label="a" type="m" corresp="#s6:l1">zag</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Voor Neerlands echte <rhyme label="b" type="f">Zoonen</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Zoo leefden wy weer Vry en <rhyme label="c" type="m">Bly</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">o Groote Vreede-Vorst, wilt <rhyme label="c" type="m" corresp="#s6:l4">Gy</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hier meede ons <rhyme label="b" type="f" corresp="#s6:l3">bekroonen</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Gy zyt, <rhyme label="d" type="m">altyd</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Ons in noode, by <rhyme label="e" type="f">gevloode</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-">Wilt in <rhyme label="f" type="f">deezen</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Ook nog Neerlands Redder <rhyme label="f" type="f" corresp="#s6:l9">weezen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wees onze Vorst een <rhyme label="a" type="m">Toeverlaat</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Behoed hem voor all' hoon en <rhyme label="a" type="m" corresp="#s7:l1">smaad</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Van hun die op hem <rhyme label="b" type="f">smaalen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Beziel hem met een <rhyme label="c" type="m">heldenmoed</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Op dat hy daar Gy hem <rhyme label="c" type="m" corresp="#s7:l4">behoed</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Nog eens mag <rhyme label="b" type="f" corresp="#s7:l3">zegepraalen</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Uw macht, en <rhyme label="d" type="m">kracht</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Moet hem schraagen, t'allen <rhyme label="e" type="f">daagen</rhyme>,</l>
        <l id="s7:l9" met="2" type="+-">En <rhyme label="f" type="f">verzellen</rhyme>,</l>
        <l id="s7:l10" met="4" type="+-">Wilt zyn luister weer <rhyme label="f" type="f" corresp="#s7:l9">herstellen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Laat nog ons dierbaar Vorstlyk <rhyme label="a" type="m">Huis</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Bevryd van alle ramp en <rhyme label="a" type="m" corresp="#s8:l1">kruis</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">In voorspoed weeldrig <rhyme label="b" type="f">bloejen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Bescherm het zelve door Uw <rhyme label="c" type="m">macht</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Laat tot in 't laatste <rhyme label="c" type="m" corresp="#s8:l4">Nageslacht</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">d'Oranje Stamboom <rhyme label="b" type="f" corresp="#s8:l3">groejen</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Zoo doch, zal <rhyme label="d" type="m">noch</rhyme></l>
        <l id="s8:l8" met="4" type="+-">'s Prinsen leeven, stoffen <rhyme label="e" type="f">geeven</rhyme>,</l>
        <l id="s8:l9" met="2" type="+-">U te <rhyme label="f" type="f">looven</rhyme>,</l>
        <l id="s8:l10" met="4" type="+-">Schenkt ons zoo dit heil van <rhyme label="f" type="f" corresp="#s8:l9">booven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Laat weer de liefd' en <rhyme label="a" type="m">Eendrachtsband</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Met Vree en Vryheid hand aan <rhyme label="a" type="m" corresp="#s9:l1">hand</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Elkander bly <rhyme label="b" type="f">ontmoeten</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Zoo zullen wy weer hier ter <rhyme label="c" type="m">Stee</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Met handgeklap en bly <rhyme label="c" type="m" corresp="#s9:l4">Hoezée</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Vorst Willem vaak <rhyme label="b" type="f" corresp="#s9:l3">begroeten</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">En hem, met <rhyme label="d" type="m">Stem</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Lofschal geeven, lang moog <rhyme label="e" type="f">leeven</rhyme>,</l>
        <l id="s9:l9" met="2" type="+-">Van <rhyme label="f" type="f">Nassouwen</rhyme>,</l>
        <l id="s9:l10" met="4" type="+-">God doe ons U lang <rhyme label="f" type="f" corresp="#s9:l9">aanschouwen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O Vorst! o Neerlands lust en <rhyme label="a" type="m">vreugd</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Gy zyt het die ons hart <rhyme label="a" type="m" corresp="#s10:l1">verheugd</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Reeds al van vroeger <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Gy zyt geteeld uit Neerlands <rhyme label="c" type="m">Bloed</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Uit zulken, die hun lyf en <rhyme label="c" type="m" corresp="#s10:l4">goed</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Voor 't Vaderland dorst'n <rhyme label="b" type="f" corresp="#s10:l3">waagen</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Hun eer, moet <rhyme label="d" type="m">weer</rhyme></l>
        <l id="s10:l8" met="4" type="+-">U beklyven, zoo zal 't <rhyme label="e" type="f">blyven</rhyme>,</l>
        <l id="s10:l9" met="2" type="+-">In <rhyme label="f" type="f">gedagte</rhyme>,</l>
        <l id="s10:l10" met="4" type="+-">Tot in 't laatste <rhyme label="f" type="f" corresp="#s10:l9">Nageslagte</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
