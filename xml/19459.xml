<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19459</id>
    <date>
      <year-start>1642</year-start>
      <year-end>1642</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Polyphemus aan de stranden</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AL de ghene die my <rhyme label="a" type="f">quellen</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Om te <rhyme label="a" type="f" corresp="#s1:l1">stellen</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Dit of dat in <rhyme label="b" type="m">Rymery</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En voor al in Bruyt-lofs <rhyme label="c" type="f">zaken</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Sullen <rhyme label="c" type="f" corresp="#s1:l4">maken</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Dat ik raak tot <rhyme label="b" type="m" corresp="#s1:l3">hooveirdy</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Slechte Jaap sou moghen <rhyme label="a" type="f">gissen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">'t Kan niet <rhyme label="a" type="f" corresp="#s2:l1">missen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dit verstant is bol en <rhyme label="b" type="m">dik</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Geest lijk beest'lijk, pleyten, <rhyme label="c" type="f">plechten</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Ja de <rhyme label="c" type="f" corresp="#s2:l4">Rechten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Moet hy weten op een <rhyme label="b" type="m" corresp="#s2:l3">prik</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hoor ik dit, soo moet ik <rhyme label="a" type="f">treden</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Wyde <rhyme label="a" type="f" corresp="#s3:l1">schreden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Gooffen Nul komt Voor den <rhyme label="b" type="m">dach</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Plompert, Esel, Buffel, <rhyme label="c" type="f">Bloetje</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Licht u <rhyme label="c" type="f" corresp="#s3:l4">hoetje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dit's de man die veel <rhyme label="b" type="m" corresp="#s3:l3">vermach</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ja ik moet 't ghesicht <rhyme label="a" type="f">verkeeren</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Ende <rhyme label="a" type="f" corresp="#s4:l1">leeren</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Yemant aansien van ter <rhyme label="b" type="m">zy</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Met een wesen stuer en <rhyme label="c" type="f">statich</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Heel <rhyme label="c" type="f" corresp="#s4:l4">besatich</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Wie en schroomt fich niet voor <rhyme label="b" type="m" corresp="#s4:l3">my</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wil my yemant van dees' <rhyme label="a" type="f">leken</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Nodigh <rhyme label="a" type="f" corresp="#s5:l1">spreken</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Als icK over straten <rhyme label="b" type="m">ga</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Soud ick stil staen om syn <rhyme label="c" type="f">reden</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Of <rhyme label="c" type="f" corresp="#s5:l4">gebeden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Hy moet bloots hoofts volgen <rhyme label="b" type="m" corresp="#s5:l3">na</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ik als 't hooft vol muyse <rhyme label="a" type="f">nesten</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Mach ten <rhyme label="a" type="f" corresp="#s6:l1">lesten</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Seggen fijn - man zijt <rhyme label="b" type="m">gedekt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Wilt u woorden kort <rhyme label="c" type="f">beleggen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Al dit <rhyme label="c" type="f" corresp="#s6:l4">seggen</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Dunkt my narghens toe en <rhyme label="b" type="m" corresp="#s6:l3">strekt</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Vaart hy voort dan met sijn <rhyme label="a" type="f">klagen</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Raat te <rhyme label="a" type="f" corresp="#s7:l1">vragen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Daar ik weynigh op <rhyme label="b" type="m">versta</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Wil ik gaan als in <rhyme label="c" type="f">gedachten</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Op sijn <rhyme label="c" type="f" corresp="#s7:l4">klachten</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Mach ik somtijts seggen <rhyme label="b" type="m" corresp="#s7:l3">ja</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Daar by noch als wel <rhyme label="a" type="f">verstaande</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s8:l1">angaande</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Nu te weten, wel, ja <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">t Recht bestaat in goe <rhyme label="c" type="f">getuygen</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Joris <rhyme label="c" type="f" corresp="#s8:l4">Huygen</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Is een fray geleert <rhyme label="b" type="m" corresp="#s8:l3">gesel</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">'t Slot sal wesen van mijn <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Weest te <rhyme label="a" type="f" corresp="#s9:l1">vreden</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">'k Heb nu wat gewichtighs <rhyme label="b" type="m">voor</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dekt u, wilt u hoet op <rhyme label="c" type="f">setten</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Men sal <rhyme label="c" type="f" corresp="#s9:l4">letten</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Op u saken na <rhyme label="b" type="m" corresp="#s9:l3">behoor</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Maar om wel in alle <rhyme label="a" type="f">delen</rhyme></l>
        <l id="s10:l2" met="2" type="+-">Na te <rhyme label="a" type="f" corresp="#s10:l1">spelen</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Den bespraakten <rhyme label="b" type="m">Cicero</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Salik met uytheymsche <rhyme label="c" type="f">woorden</rhyme></l>
        <l id="s10:l5" met="2" type="+-">'t Duyts <rhyme label="c" type="f" corresp="#s10:l4">vermoorden</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Vive la <rhyme label="b" type="m" corresp="#s10:l3">grandissima</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">'k Heb al wat Latijn gaan <rhyme label="a" type="f">soeken</rhyme></l>
        <l id="s11:l2" met="2" type="+-">Inde <rhyme label="a" type="f" corresp="#s11:l1">hoeken</rhyme></l>
        <l id="s11:l3" met="4" type="+-">'k Ben soo slecht niet als een <rhyme label="b" type="m">lam</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Tibi Bestia <rhyme label="c" type="f">Civitatis</rhyme>,</l>
        <l id="s11:l5" met="2" type="+-">Qui in staat <rhyme label="c" type="f" corresp="#s11:l4">is</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Amabit <rhyme label="b" type="m" corresp="#s11:l3">Pecuniam</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Potten, Ketels, <rhyme label="a" type="f">Meule-swengels</rhyme>,</l>
        <l id="s12:l2" met="2" type="+-">Schots en <rhyme label="a" type="f" corresp="#s12:l1">Engels</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Greecx, Hebreus, Latijn, en <rhyme label="b" type="m">Wals</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Al dat goetje meng ik <rhyme label="c" type="f">t'samen</rhyme>,</l>
        <l id="s12:l5" met="2" type="+-">ik Sou my <rhyme label="c" type="f" corresp="#s12:l4">schamen</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Dat ik niet en kost van <rhyme label="b" type="m" corresp="#s12:l3">als</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Spieghel u an mijn <rhyme label="a" type="f">Studeren</rhyme>,</l>
        <l id="s13:l2" met="2" type="+-">Al die <rhyme label="a" type="f" corresp="#s13:l1">leeren</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Kost het Bier of koele <rhyme label="b" type="m">Wijn</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Maken bonne <rhyme label="c" type="f">Studiosis</rhyme>,</l>
        <l id="s13:l5" met="2" type="+-">'t Kallef <rhyme label="c" type="f" corresp="#s13:l4">Mosis</rhyme></l>
        <l id="s13:l6" met="4" type="+-">Sou haast Doctor Iuris <rhyme label="b" type="m" corresp="#s13:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Philosophen en <rhyme label="a" type="f">Poëten</rhyme></l>
        <l id="s14:l2" met="2" type="+-">Mochtmen <rhyme label="a" type="f" corresp="#s14:l1">meten</rhyme></l>
        <l id="s14:l3" met="4" type="+-">Met de groote Bonsche <rhyme label="b" type="m">maat</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Op-gehoopt met <rhyme label="c" type="f">af-gestreken</rhyme>,</l>
        <l id="s14:l5" met="2" type="+-">Eele <rhyme label="c" type="f" corresp="#s14:l4">weken</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Hoe waar dan het landt <rhyme label="b" type="m" corresp="#s14:l3">verzaat</rhyme>,</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Hoe sou 't Haspel-tuychje <rhyme label="a" type="f">schryven</rhyme></l>
        <l id="s15:l2" met="2" type="+-">Ende <rhyme label="a" type="f" corresp="#s15:l1">vryven</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Yeder een op syn <rhyme label="b" type="m">manier</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Vande Goden en <rhyme label="c" type="f">Godinnen</rhyme>,</l>
        <l id="s15:l5" met="2" type="+-">Van het <rhyme label="c" type="f" corresp="#s15:l4">minnen</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">Maar wat raat om wit <rhyme label="b" type="m" corresp="#s15:l3">Papier</rhyme>?</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">'k Mach mijn hoost niet langer <rhyme label="a" type="f">breken</rhyme></l>
        <l id="s16:l2" met="2" type="+-">Met dit <rhyme label="a" type="f" corresp="#s16:l1">preken</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Ik verwar in dese <rhyme label="b" type="m">les</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Dit mach ik den Danker <rhyme label="c" type="f">danken</rhyme>,</l>
        <l id="s16:l5" met="2" type="+-">Voor <rhyme label="c" type="f" corresp="#s16:l4">twee-blanken</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">En het Schaapjen inden <rhyme label="b" type="m" corresp="#s16:l3">Nes</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Die zyn oorsaak van dit <rhyme label="a" type="f">brodden</rhyme>,</l>
        <l id="s17:l2" met="2" type="+-">'k Smyt de <rhyme label="a" type="f" corresp="#s17:l1">vodden</rhyme></l>
        <l id="s17:l3" met="4" type="+-">Om een haver stro int <rhyme label="b" type="m">vuyt</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Of de vrinden niet en <rhyme label="c" type="f">weten</rhyme></l>
        <l id="s17:l5" met="2" type="+-">Dat het <rhyme label="c" type="f" corresp="#s17:l4">eeten</rhyme></l>
        <l id="s17:l6" met="4" type="+-">En de Boter is soo <rhyme label="b" type="m" corresp="#s17:l3">duyr</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Altijt weer an Trouwen <rhyme label="a" type="f">Houwen</rhyme>,</l>
        <l id="s18:l2" met="2" type="+-">'k Weet gheen <rhyme label="a" type="f" corresp="#s18:l1">mouwen</rhyme></l>
        <l id="s18:l3" met="4" type="+-">Meer te setten an dit <rhyme label="b" type="m">Kleet</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Van dit Dichten, van dit <rhyme label="c" type="f">Rymen</rhyme></l>
        <l id="s18:l5" met="2" type="+-">Soumen <rhyme label="c" type="f" corresp="#s18:l4">swymen</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">Datmen sich daar van <rhyme label="b" type="m" corresp="#s18:l3">besuykerde</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">'t Is gheen tijt om tijt te <rhyme label="a" type="f">spillen</rhyme></l>
        <l id="s19:l2" met="2" type="+-">Met dees <rhyme label="a" type="f" corresp="#s19:l1">grillen</rhyme>,</l>
        <l id="s19:l3" met="4" type="+-">Wie onachtsaam tijt <rhyme label="b" type="m">verslijt</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Smaakt ook tydeloose <rhyme label="c" type="f">vruchten</rhyme>,</l>
        <l id="s19:l5" met="2" type="+-">Vele <rhyme label="c" type="f" corresp="#s19:l4">zuchten</rhyme></l>
        <l id="s19:l6" met="4" type="+-">Dikwils t'onty om de <rhyme label="b" type="m" corresp="#s19:l3">tyt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Staach de tijt met boertsche <rhyme label="a" type="f">dinghen</rhyme></l>
        <l id="s20:l2" met="2" type="+-">Deur te <rhyme label="a" type="f" corresp="#s20:l1">bringhen</rhyme>,</l>
        <l id="s20:l3" met="4" type="+-">Dat is wel een sotte <rhyme label="b" type="m">daat</rhyme>,</l>
        <l id="s20:l4" met="4" type="+-">Maar 't is wijsheyt die te <rhyme label="c" type="f">pleghen</rhyme>,</l>
        <l id="s20:l5" met="2" type="+-">Als 't ter <rhyme label="c" type="f" corresp="#s20:l4">deghen</rhyme></l>
        <l id="s20:l6" met="4" type="+-">Wort gebruykt op stond en <rhyme label="b" type="m" corresp="#s20:l3">maat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
