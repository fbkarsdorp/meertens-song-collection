    <melody>Psalm 033 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Gheloeft soo sy des Heeren <rhyme label="a" type="f">Name</rhyme>,,</l>
        <l id="s1:l2" met="4" type="-+">Van nu tot in der <rhyme label="b" type="m">eewicheyt</rhyme>,,</l>
        <l id="s1:l3" met="4" type="-+">Van der Sonnen opganck <rhyme label="a" type="f" corresp="#s1:l1">bequame</rhyme>,,</l>
        <l id="s1:l4" met="4" type="-+">Tot daer den nederganck <rhyme label="b" type="m" corresp="#s1:l2">verbreyt</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Geeft hem prijs en <rhyme label="c" type="f">eere</rhyme>,,</l>
        <l id="s1:l6" met="3" type="+-">Want goet is de <rhyme label="c" type="f" corresp="#s1:l5">Heere</rhyme>,,</l>
        <l id="s1:l7" met="2" type="+-">Al van hier <rhyme label="d" type="f">boven</rhyme>,,</l>
        <l id="s1:l8" met="3" type="+-">Op zijn <rhyme label="e" type="f">wonderwercken</rhyme>,,</l>
        <l id="s1:l9" met="3" type="+-">Wilt alle wel <rhyme label="e" type="f" corresp="#s1:l8">mercken</rhyme>,,</l>
        <l id="s1:l10" met="2" type="-+">En wilt hem <rhyme label="d" type="f" corresp="#s1:l7">loven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al dat de Heer ons Godt <rhyme label="a" type="f">almachtich</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Hier ordineert, en ons <rhyme label="b" type="m">ghebiet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Is goet: des sullen wijt' <rhyme label="a" type="f" corresp="#s2:l1">eendrachtich</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Navolghen, en vergheten <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Tot <rhyme label="c" type="f">ghedachtenisse</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Dat wy zijn <rhyme label="c" type="f" corresp="#s2:l5">ghewisse</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Christi <rhyme label="d" type="f">Erven</rhyme>.</l>
        <l id="s2:l8" met="3" type="+-">Liet hy ons een <rhyme label="e" type="f">spijse</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Seer hooghe van <rhyme label="e" type="f" corresp="#s2:l8">prijse</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Recht voor zijn <rhyme label="d" type="f" corresp="#s2:l7">sterven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Moyses en heeft niet <rhyme label="a" type="f">ghegheven</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Den synen t'rechte Hemels <rhyme label="b" type="m">Broot</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Maer Christus die daer is het <rhyme label="a" type="f" corresp="#s3:l1">leven</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Gheeft Spyse voor d'eewighe <rhyme label="b" type="m" corresp="#s3:l2">doot</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Het lichaem seer <rhyme label="c" type="f">heylich</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Twelck den wech maeckt <rhyme label="c" type="f" corresp="#s3:l5">veylich</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Tot den <rhyme label="d" type="f">Vader</rhyme>:</l>
        <l id="s3:l8" met="3" type="+-">Sijn Bloet wort <rhyme label="e" type="f">ghedroncken</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Verdienst wort <rhyme label="e" type="f" corresp="#s3:l8">gheschoncken</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Ons <rhyme label="d" type="f" corresp="#s3:l7">allegader</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Vaders hebben al <rhyme label="a" type="f">ghegheten</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Een Gheestelicke spijs met <rhyme label="b" type="m">danck</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Uut de Steenrootse hooch <rhyme label="a" type="f" corresp="#s4:l1">gheseten</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Dronckense Gheestelicken <rhyme label="b" type="m" corresp="#s4:l2">dranck</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">Der Steenrootsen <rhyme label="c" type="f">claerheyt</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Was Christus in <rhyme label="c" type="f" corresp="#s4:l5">waerheyt</rhyme></l>
        <l id="s4:l7" met="2" type="+-"><rhyme label="d" type="f">Gheestelijcke</rhyme>:</l>
        <l id="s4:l8" met="3" type="+-">Soo hy hier ter <rhyme label="e" type="f">stede</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Ons allen spijst <rhyme label="e" type="f" corresp="#s4:l8">mede</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Tot synen <rhyme label="d" type="f" corresp="#s4:l7">Rijcke</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Het Broot dat wy als nu hier <rhyme label="a" type="f">breken</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Is de ghemeynschap inder <rhyme label="b" type="m">daet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Des Lichaems Christi, wie can <rhyme label="a" type="f" corresp="#s5:l1">spreken</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Hoe wonderlick dat dit toe <rhyme label="b" type="m" corresp="#s5:l2">gaet</rhyme>?</l>
        <l id="s5:l5" met="3" type="+-">Effen door den <rhyme label="c" type="f">Beker</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Worden wy oock <rhyme label="c" type="f" corresp="#s5:l5">seker</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Groot en <rhyme label="d" type="f">cleyne</rhyme>:</l>
        <l id="s5:l8" met="3" type="+-">T'bloet voor ons <rhyme label="e" type="f">vergoten</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">T'lijden <rhyme label="e" type="f" corresp="#s5:l8">onverdroten</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Wort ons <rhyme label="d" type="f" corresp="#s5:l7">ghemeyne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Lof sy dy Christe t'allen <rhyme label="a" type="f">tijden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die ons spijst door dijn vlees en <rhyme label="b" type="m">bloet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ende door dijne bitter <rhyme label="a" type="f" corresp="#s6:l1">lijden</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Ons hebt bereyt t'eewighe <rhyme label="b" type="m" corresp="#s6:l2">goet</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">Die hier zijt int <rhyme label="c" type="f">midden</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Om den Gheest wy <rhyme label="c" type="f" corresp="#s6:l5">bidden</rhyme></l>
        <l id="s6:l7" met="2" type="+-">Al te <rhyme label="d" type="f">samen</rhyme>:</l>
        <l id="s6:l8" met="3" type="+-">Die t'gheloof <rhyme label="e" type="f">vermeere</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">En ons liefde <rhyme label="e" type="f" corresp="#s6:l8">leere</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">In vreden, <rhyme label="d" type="f" corresp="#s6:l7">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
