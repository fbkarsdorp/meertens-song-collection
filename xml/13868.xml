<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13868</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
    </tags>
    <melody>De stille nacht genaakt</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ALs Phoebus heldre <rhyme label="a" type="m">Kruyn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Verdreef de Wolcken <rhyme label="a" type="m" corresp="#s1:l1">bruyn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Door ‘t flick’ren van zijn <rhyme label="b" type="f">stralen</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Quam Philis met haer <rhyme label="c" type="m">Vee</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Tot aen d’Oever van de <rhyme label="c" type="m" corresp="#s1:l4">Zee</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met haer Beesjes eensaem <rhyme label="b" type="f" corresp="#s1:l3">dwaelen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Door de hitte <rhyme label="a" type="m">gheport</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Heeft sy ‘t Keursje <rhyme label="a" type="m" corresp="#s2:l1">gheschort</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Tot ruym boven haer <rhyme label="b" type="f">Knietjes</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En laet haer wolligh <rhyme label="c" type="m">Vee</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Gaen inde woeste <rhyme label="c" type="m" corresp="#s2:l4">Zee</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Daer de bracke <rhyme label="b" type="f" corresp="#s2:l3">Water-vlietjes</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Door Auraas kracht op <rhyme label="a" type="m">drongh</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Tot aen haer Borsjes <rhyme label="a" type="m" corresp="#s3:l1">sprongh</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Sy is te rugh <rhyme label="b" type="f">gheloopen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met overgrooten <rhyme label="c" type="m">haest</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En doet gheheel <rhyme label="c" type="m" corresp="#s3:l4">verbaest</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Haer natte Boesem <rhyme label="b" type="f" corresp="#s3:l3">open</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Tyter die komt op ‘t <rhyme label="a" type="m">slagh</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En riep: noyt blijder <rhyme label="a" type="m" corresp="#s4:l1">dagh</rhyme>!</l>
        <l id="s4:l3" met="3" type="-+">Och! mocht ick in mijn <rhyme label="b" type="f">Armen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De poeselighe <rhyme label="c" type="m">Le’en</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En Borsjes, wel <rhyme label="c" type="m" corresp="#s4:l4">besneen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van mijn Lief Philis <rhyme label="b" type="f" corresp="#s4:l3">warmen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Haer Kaeckje kreegh een <rhyme label="a" type="m">bloos</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Veel rooder dan een <rhyme label="a" type="m" corresp="#s5:l1">Roos</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Door de schaemte <rhyme label="b" type="f">ontsteecken</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Sy sey, waer ick <rhyme label="c" type="m">Diaen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Het leet aen my <rhyme label="c" type="m" corresp="#s5:l4">ghedaen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sou ick, als sy deed, <rhyme label="b" type="f" corresp="#s5:l3">wreecken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick hoop niet dat ghy ‘t <rhyme label="a" type="m">mient</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sey hy, heb ick ‘t <rhyme label="a" type="m" corresp="#s6:l1">verdient</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Of so ghy een Hart wilt <rhyme label="b" type="f">plaghen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">So wil ick garen ‘t <rhyme label="c" type="m">mijn</rhyme></l>
        <l id="s6:l5" met="3" type="-+">(Kan dit voldoeningh <rhyme label="c" type="m" corresp="#s6:l4">zijn</rhyme>)</l>
        <l id="s6:l6" met="3" type="-+">V tot een Offer <rhyme label="b" type="f" corresp="#s6:l3">draghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
