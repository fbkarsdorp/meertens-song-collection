<song>
  <meta>
    <id>13910</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AStrea, waerde <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Vliedt niet die u <rhyme label="a" type="m" corresp="#s1:l1">nae-jaeght</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En wilt hem niet <rhyme label="b" type="f">bedroeven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Maer wilt hier een weynigh <rhyme label="b" type="f" corresp="#s1:l3">toeven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ay! wilt soo hardt niet <rhyme label="a" type="m">vlien</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Neen saft, laet my <rhyme label="a" type="m" corresp="#s2:l1">betien</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En laet my in mijn <rhyme label="b" type="f">vreden</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Want ick acht gheen Minnaers <rhyme label="b" type="f" corresp="#s2:l3">Eeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Eeden van die <rhyme label="a" type="m">mint</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die werden van de <rhyme label="a" type="m" corresp="#s3:l1">windt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Als krachteloos <rhyme label="b" type="f">verdreven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En als op een Stroom <rhyme label="b" type="f" corresp="#s3:l3">gheschreven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Want als ghy nu <rhyme label="a" type="m">ghenoot</rhyme></l>
        <l id="s4:l2" met="3" type="-+">De weelde van <rhyme label="a" type="m" corresp="#s4:l1">mijnSchoot</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zoo soudt ghy van my <rhyme label="b" type="f">vluchten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En laten my, Maeght, in <rhyme label="b" type="f" corresp="#s4:l3">suchten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Astrea, neen, ick <rhyme label="a" type="m">zweer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat ick gheen aer <rhyme label="a" type="m" corresp="#s5:l1">begheer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dan u: dus ô mijn <rhyme label="b" type="f">Schoone</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Wilt my toch met we’er-min <rhyme label="b" type="f" corresp="#s5:l3">loonen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Eer sal Diana <rhyme label="a" type="m">self</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Door ‘s Hemels blaeuw <rhyme label="a" type="m" corresp="#s6:l1">ghewelf</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Haer Broeders Waghen <rhyme label="b" type="f">nemen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Eer dat ick u Eer sou <rhyme label="b" type="f" corresp="#s6:l3">schennen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Eer val de <rhyme label="a" type="m">Zon</rhyme></l>
        <l id="s7:l2" met="3" type="-+">In’t diepst van <rhyme label="a" type="m" corresp="#s7:l1">Acheron</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Eer ick u sal <rhyme label="b" type="f">verlaten</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Dus wilt my niet meerder <rhyme label="b" type="f" corresp="#s7:l3">haten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Astrea daer’s mijn <rhyme label="a" type="m">Handt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En Trouw, tot <rhyme label="a" type="m" corresp="#s8:l1">onderpandt</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Dees Kus die sal <rhyme label="b" type="f">ghetuyghen</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Dat ick my nae u wil <rhyme label="b" type="f" corresp="#s8:l3">buyghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Coridon boet nu <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s9:l2" met="3" type="-+">V lieve lust aen <rhyme label="a" type="m" corresp="#s9:l1">my</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Nu ghy u Trouw te <rhyme label="b" type="f">vooren</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Zoo duur hebt aen my <rhyme label="b" type="f" corresp="#s9:l3">ghezwooren</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Met Coridon <rhyme label="a" type="m">toeschoot</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En heeft haer Borst <rhyme label="a" type="m" corresp="#s10:l1">onbloot</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Kussende die teere <rhyme label="b" type="f">Leden</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Voelde hy voort nae <rhyme label="b" type="f" corresp="#s10:l3">beneden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Coridon niet te <rhyme label="a" type="m">veer</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Neen saft, dat krenckt mijn <rhyme label="a" type="m" corresp="#s11:l1">Eer</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Ay, bid’ ick, houdt u <rhyme label="b" type="f">handen</rhyme>:</l>
        <l id="s11:l4" met="4" type="+-">Want ick voel mijn Hartje <rhyme label="b" type="f" corresp="#s11:l3">branden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Iupijn die sou het <rhyme label="a" type="m">sien</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En klappen ‘t aen de <rhyme label="a" type="m" corresp="#s12:l1">Lie’n</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En segghen tot mijn <rhyme label="b" type="f">schanden</rhyme>.</l>
        <l id="s12:l4" met="4" type="+-">Stucken zijn Astreaes <rhyme label="b" type="f" corresp="#s12:l3">banden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Sorght niet, mijn Lief <rhyme label="a" type="m">Astre</rhyme>’,</l>
        <l id="s13:l2" met="3" type="-+">Iupijn die de’e het <rhyme label="a" type="m" corresp="#s13:l1">me’e</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Vandt onder ‘t lieflijck <rhyme label="b" type="f">speelen</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Gaet zijn lieven Ionghen <rhyme label="b" type="f" corresp="#s13:l3">streelen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Hier me’e soo zweegh sy <rhyme label="a" type="m">stil</rhyme>.</l>
        <l id="s14:l2" met="3" type="-+">Coridon de’e zijn <rhyme label="a" type="m" corresp="#s14:l1">wil</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">En pluckten ‘t <rhyme label="b" type="f">Maeghden-Roosje</rhyme>:</l>
        <l id="s14:l4" met="4" type="+-">Maer Astrea kreegh een <rhyme label="b" type="f" corresp="#s14:l3">bloosje</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
