    <melody>Viva roep al gelijk</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'t Schijnt of den <rhyme label="a" type="m">Zegenprael</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">den Pruys is <rhyme label="b" type="f">opgedragen</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Hy gaet zijn leven <rhyme label="b" type="f" corresp="#s1:l2">wagen</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Door donder Vuur en <rhyme label="a" type="m" corresp="#s1:l1">stael</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hy trekt door veel <rhyme label="c" type="f">Rijkslande</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Rukt in't Boheemse <rhyme label="d" type="m">Rijke</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">En gaet daer zoo <rhyme label="c" type="f" corresp="#s1:l5">aenrande</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In Steden en op <rhyme label="c" type="f" corresp="#s1:l5">Landen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Poolen en <rhyme label="d" type="m" corresp="#s1:l6">Oostenrijk</rhyme>.</l>
        <l id="s1:l10" met="3" type="-+">Poolen en <rhyme label="d" type="m" corresp="#s1:l6">Oostenrijk</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die door een loos <rhyme label="a" type="m">verraet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Door listen en met <rhyme label="b" type="f">reden</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Te brengen Rijk en <rhyme label="b" type="f" corresp="#s2:l2">steden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En ook de Kerk en <rhyme label="a" type="m" corresp="#s2:l1">Staet</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Van onze <rhyme label="c" type="f">Protestanten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Te brengen in 't <rhyme label="d" type="m">verdriet</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Wilt nu aen alle <rhyme label="c" type="f" corresp="#s2:l5">kanten</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Den Oorlogs Stam daer <rhyme label="c" type="f" corresp="#s2:l5">planten</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">En schroomt dog Vrankrijk <rhyme label="d" type="m" corresp="#s2:l6">niet</rhyme></l>
        <l id="s2:l10" met="3" type="-+">En schroomt dog Vrankrijk <rhyme label="d" type="m" corresp="#s2:l6">niet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zeventien Hondert <rhyme label="a" type="m">jaer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Nog Vijftig en de <rhyme label="b" type="f">Zeven</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En nog daer by <rhyme label="b" type="f" corresp="#s3:l2">geschreven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Aprillus twintig <rhyme label="a" type="m" corresp="#s3:l1">klaer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Toen trok Brunswijk te <rhyme label="c" type="f">Velde</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Tegene zijn Vyand <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met twintig Duyzent <rhyme label="c" type="f" corresp="#s3:l5">Helde</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Zoo ons de krant <rhyme label="c" type="f" corresp="#s3:l5">melde</rhyme>;</l>
        <l id="s3:l9" met="3" type="-+">Sloeg Dertig Duyzent <rhyme label="d" type="m" corresp="#s3:l6">Man</rhyme>.</l>
        <l id="s3:l10" met="3" type="-+">Sloeg Dertig Duyzent <rhyme label="d" type="m" corresp="#s3:l6">Man</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die haer twee uuren <rhyme label="a" type="m">lang</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Veel dapperlijk haer <rhyme label="b" type="f">kweten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die moeste haest wilt <rhyme label="b" type="f" corresp="#s4:l2">weten</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Afdijnze 't viel haer <rhyme label="a" type="m" corresp="#s4:l1">bang</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">De pruize heel <rhyme label="c" type="f">manmoedig</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">joegen gaer agter <rhyme label="d" type="m">na</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Zy wierden haer zeer <rhyme label="c" type="f" corresp="#s4:l5">spoedig</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">De slag die was zeer <rhyme label="c" type="f" corresp="#s4:l5">bloedig</rhyme>:</l>
        <l id="s4:l9" met="3" type="-+">Looft God voor zijn <rhyme label="d" type="m" corresp="#s4:l6">gena</rhyme>.</l>
        <l id="s4:l10" met="3" type="-+">Looft God voor zijn <rhyme label="d" type="m" corresp="#s4:l6">gena</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Slaet op de <rhyme label="a" type="m">Keteltrom</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laet de Kanonne <rhyme label="b" type="f">Raeze</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">En de Trompette <rhyme label="b" type="f" corresp="#s5:l2">blazen</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">En Zwiert de Vendels <rhyme label="a" type="m" corresp="#s5:l1">om</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Looft God met hert en <rhyme label="c" type="f">monde</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hem zy de <rhyme label="d" type="m">Heerlijkheid</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Wilt Godes Eer <rhyme label="c" type="f" corresp="#s5:l5">verkonde</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ia alle uur en <rhyme label="c" type="f" corresp="#s5:l5">stonden</rhyme>:</l>
        <l id="s5:l9" met="3" type="-+">Tot In der <rhyme label="d" type="m" corresp="#s5:l6">Eeuwigheid</rhyme>.</l>
        <l id="s5:l10" met="3" type="-+">Tot In der <rhyme label="d" type="m" corresp="#s5:l6">Eeuwigheid</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Neemt heer in uw <rhyme label="a" type="m">gely</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ons and're <rhyme label="b" type="f">Legerbende</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wilt ook uw zegen <rhyme label="b" type="f" corresp="#s6:l2">wende</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">verhoort o heere <rhyme label="a" type="m" corresp="#s6:l1">my</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Wilt onze lieve <rhyme label="c" type="f">Koning</rhyme></l>
        <l id="s6:l6" met="3" type="-+">O Heer behoeden <rhyme label="d" type="m">dog</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Hem met zeegen kroone <rhyme label="c" type="f" corresp="#s6:l5">wilt</rhyme>:</l>
        <l id="s6:l8" met="3" type="-+">Die in't velt heeft zijn <rhyme label="c" type="f" corresp="#s6:l5">woning</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Gunt ons de Vreede <rhyme label="d" type="m" corresp="#s6:l6">nog</rhyme>.</l>
        <l id="s6:l10" met="3" type="-+">Gunt ons de Vreede <rhyme label="d" type="m" corresp="#s6:l6">nog</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ik neem nu mijn <rhyme label="a" type="m">besluyt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Uw zy de lof en <rhyme label="b" type="f">Eere</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Ã´ Grote God en <rhyme label="b" type="f" corresp="#s7:l2">Heere</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Van alle <rhyme label="a" type="m" corresp="#s7:l1">Heerlijkheid</rhyme>;</l>
        <l id="s7:l5" met="3" type="-+">Gy kan 's Vyands <rhyme label="c" type="f">gewelde</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Verbreken door uw <rhyme label="d" type="m">kragt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Die haer daer tegen <rhyme label="c" type="f" corresp="#s7:l5">stelde</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Gy haer ter neder <rhyme label="c" type="f" corresp="#s7:l5">velde</rhyme>;</l>
        <l id="s7:l9" met="3" type="-+">Beneemt alzo haar <rhyme label="d" type="m" corresp="#s7:l6">magt</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Beneemt alzo haar <rhyme label="d" type="m" corresp="#s7:l6">magt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
