  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WY wenschen u geluck, Vrouw <rhyme label="a" type="m">Bruydt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En u, Heer <rhyme label="b" type="m">Bruydegom</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Want, ô Reaeltje, ghy munt <rhyme label="a" type="m" corresp="#s1:l1">uyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Voor u lof stae ick <rhyme label="b" type="m" corresp="#s1:l2">stom</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">En ghy, ô Rombouts, waert het <rhyme label="c" type="m">waerdt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat ick met reden <rhyme label="d" type="m">sprack</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Dat ghy, na uw' verdiensten <rhyme label="c" type="m" corresp="#s1:l5">paert</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">'t Geen u alleen <rhyme label="d" type="m" corresp="#s1:l6">ontbrack</rhyme>.</l>
      </lg>
    </lg>
  </text>
