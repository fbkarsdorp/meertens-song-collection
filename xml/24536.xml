<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24536</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="praise song">loflied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ghelauet systu <rhyme label="a" type="f">cederboem</rhyme></l>
        <l id="s1:l2" met="3" type="-+">du hoge gelaefde <rhyme label="b" type="m">holt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">wanttu heuest <rhyme label="c" type="f">gedragen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">den edellen vorsten <rhyme label="b" type="m" corresp="#s1:l2">stolt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick meyne Jhesus <rhyme label="a" type="f">christus</rhyme></l>
        <l id="s2:l2" met="3" type="-+">syn naem is wyde ende <rhyme label="b" type="m">breyt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">die hem in synen herten <rhyme label="c" type="f">draget</rhyme></l>
        <l id="s2:l4" met="3" type="-+">hi benemet hem alle syn <rhyme label="b" type="m" corresp="#s2:l2">leyt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O du suete <rhyme label="a" type="f">Jhesus</rhyme></l>
        <l id="s3:l2" met="3" type="-+">du edelle vorste <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">geuet my dat ic v <rhyme label="c" type="f">drage</rhyme></l>
        <l id="s3:l4" met="3" type="-+">al inden herten <rhyme label="b" type="m" corresp="#s3:l2">myn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Als die here heuet <rhyme label="a" type="f">gehangen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">aen enen cruce <rhyme label="b" type="m">breyt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">daer di dyn mylde <rhyme label="c" type="f">herte</rhyme></l>
        <l id="s4:l4" met="3" type="-+">een scarp speer doer <rhyme label="b" type="m" corresp="#s4:l2">sneet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Op mynes lieues <rhyme label="a" type="f">houet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">daer steet een <rhyme label="b" type="m">crenselkyn</rhyme></l>
        <l id="s5:l3" met="3" type="-+">dat crensellyn is <rhyme label="c" type="f">bedouwet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">mitten edellen bloede <rhyme label="b" type="m" corresp="#s5:l2">syn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Och waer myn herte een <rhyme label="a" type="f">gaerden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">van edellen <rhyme label="b" type="m">blomekyn</rhyme></l>
        <l id="s6:l3" met="3" type="-+">soe wolde ic daer in <rhyme label="c" type="f">planten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">myns lieuen <rhyme label="b" type="m" corresp="#s6:l2">crensellyn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die bloemkens die ic <rhyme label="a" type="f">meyne</rhyme></l>
        <l id="s7:l2" met="3" type="-+">die heyten <rhyme label="b" type="m">ommylitas</rhyme></l>
        <l id="s7:l3" met="3" type="-+">die andere selue <rhyme label="c" type="f">bloemen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">die heyten spes fydes <rhyme label="b" type="m" corresp="#s7:l2">caritas</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wt mynes lieues <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">daer sprynget een <rhyme label="b" type="m">bornekyn</rhyme></l>
        <l id="s8:l3" met="3" type="-+">een ryuierken wil ic <rhyme label="c" type="f">leyden</rhyme></l>
        <l id="s8:l4" met="3" type="-+">al inden gaerden <rhyme label="b" type="m" corresp="#s8:l2">myn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O Jhesus <rhyme label="a" type="f">gaerdenere</rhyme></l>
        <l id="s9:l2" met="3" type="-+">du ware  <rhyme label="b" type="m">ackerman</rhyme></l>
        <l id="s9:l3" met="3" type="-+">woldestu myns gaerdes <rhyme label="c" type="f">plegen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">soe worde hi <rhyme label="b" type="m" corresp="#s9:l2">lauesam</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Mynes lieues <rhyme label="a" type="f">armen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">staen wyde <rhyme label="b" type="m">wtgebreyt</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Och mocht ic daer in <rhyme label="c" type="f">rusten</rhyme></l>
        <l id="s10:l4" met="3" type="-+">soe vergete ic alle myn <rhyme label="b" type="m" corresp="#s10:l2">leyt</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Hy heeft tot my <rhyme label="a" type="f">geneyget</rhyme></l>
        <l id="s11:l2" met="3" type="-+">synen edellen roder <rhyme label="b" type="m">mont</rhyme></l>
        <l id="s11:l3" met="3" type="-+">och mocht ic en <rhyme label="c" type="f">cussen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">myne ziele die worde <rhyme label="b" type="m" corresp="#s11:l2">gesont</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Soe ic hem <rhyme label="a" type="f">aenschouwe</rhyme></l>
        <l id="s12:l2" met="3" type="-+">den vorsten <rhyme label="b" type="m">hogebaren</rhyme></l>
        <l id="s12:l3" met="3" type="-+">die mynne heeft ham <rhyme label="c" type="f">verwonnen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">syne verwe heeft hi <rhyme label="b" type="m" corresp="#s12:l2">verlaren</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Aen mynes lieues <rhyme label="a" type="f">syden</rhyme></l>
        <l id="s13:l2" met="3" type="-+">daer  licht   een gulden <rhyme label="b" type="m">screen</rhyme></l>
        <l id="s13:l3" met="3" type="-+">och waer ic daer in <rhyme label="c" type="f">beslaten</rhyme></l>
        <l id="s13:l4" met="3" type="-+">al naeden willen <rhyme label="b" type="m" corresp="#s13:l2">myn</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Ick en kan daer niet in <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">du en leydes my <rhyme label="b" type="m">daerin</rhyme></l>
        <l id="s14:l3" met="3" type="-+">wanttu heues <rhyme label="c" type="f">gespraken</rhyme></l>
        <l id="s14:l4" met="3" type="-+">sonder my en moegdi niet <rhyme label="b" type="m" corresp="#s14:l2">doen</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Tot mynes lieuen <rhyme label="a" type="f">voeten</rhyme></l>
        <l id="s15:l2" met="3" type="-+">daer steet een <rhyme label="b" type="m">boemekyn</rhyme></l>
        <l id="s15:l3" met="3" type="-+">mocht ic daer onder <rhyme label="c" type="f">spacieren</rhyme></l>
        <l id="s15:l4" met="3" type="-+">ick vergete oec alle myn <rhyme label="b" type="m" corresp="#s15:l2">pyn</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Wanneer ic myn lief <rhyme label="a" type="f">verliese</rhyme></l>
        <l id="s16:l2" met="3" type="-+">des dages ende oec des <rhyme label="b" type="m">nachtes</rhyme></l>
        <l id="s16:l3" met="3" type="-+">soe mach icken weder <rhyme label="c" type="f">vynden</rhyme></l>
        <l id="s16:l4" met="3" type="-+">al aen des cruces <rhyme label="b" type="m" corresp="#s16:l2">ast</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Die mynne heeft hem <rhyme label="a" type="f">gebonden</rhyme></l>
        <l id="s17:l2" met="3" type="-+">die hilige hande <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s17:l3" met="3" type="-+">al aen des cruces <rhyme label="c" type="f">aste</rhyme></l>
        <l id="s17:l4" met="3" type="-+">myt plompen <rhyme label="b" type="m" corresp="#s17:l2">nagel</rhyme></l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Sie ic hem aen die <rhyme label="a" type="f">voeten</rhyme></l>
        <l id="s18:l2" met="3" type="-+">den lieuen heren <rhyme label="b" type="m">myn</rhyme></l>
        <l id="s18:l3" met="3" type="-+">hi steet soe vaste <rhyme label="c" type="f">genagelt</rhyme></l>
        <l id="s18:l4" met="3" type="-+">hi en wil ons niet <rhyme label="b" type="m" corresp="#s18:l2">ontvlien</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Och alre liefste <rhyme label="a" type="f">here</rhyme></l>
        <l id="s19:l2" met="3" type="-+">hoe dicke ic di <rhyme label="b" type="m">ontflie</rhyme></l>
        <l id="s19:l3" met="3" type="-+">om mynre sunden <rhyme label="c" type="f">wille</rhyme></l>
        <l id="s19:l4" met="3" type="-+">is my myn herte soe <rhyme label="b" type="m" corresp="#s19:l2">wee</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Ghedencke here der <rhyme label="a" type="f">reden</rhyme></l>
        <l id="s20:l2" met="3" type="-+">die van di gescreuen <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s20:l3" met="3" type="-+">wanneer ic verhoget <rhyme label="c" type="f">weerde</rhyme></l>
        <l id="s20:l4" met="3" type="-+">alle dynck wil ic nae my <rhyme label="b" type="m" corresp="#s20:l2">tien</rhyme></l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Soe bid ic v lieue <rhyme label="a" type="f">here</rhyme></l>
        <l id="s21:l2" met="3" type="-+">doer dynre lieuer <rhyme label="b" type="m">cracht</rhyme></l>
        <l id="s21:l3" met="3" type="-+">trecket mynwylde <rhyme label="c" type="f">herte</rhyme></l>
        <l id="s21:l4" met="3" type="-+">al aen des cruces <rhyme label="b" type="m" corresp="#s21:l2">cracht</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Dat myn herte moete <rhyme label="a" type="f">rusten</rhyme></l>
        <l id="s22:l2" met="3" type="-+">al inden wonden <rhyme label="b" type="m">dyn</rhyme></l>
        <l id="s22:l3" met="3" type="-+">al tusschen dinen <rhyme label="c" type="f">borsten</rhyme></l>
        <l id="s22:l4" met="3" type="-+">als een merren <rhyme label="b" type="m" corresp="#s22:l2">bondekyn</rhyme></l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">regiere my lieue <rhyme label="a" type="f">here</rhyme></l>
        <l id="s23:l2" met="3" type="-+">dach ende nacht tot alre <rhyme label="b" type="m">stont</rhyme></l>
        <l id="s23:l3" met="3" type="-+">dat ick di lief <rhyme label="c" type="f">gewynne</rhyme></l>
        <l id="s23:l4" met="3" type="-+">van alle myns hertsen <rhyme label="b" type="m" corresp="#s23:l2">gront</rhyme></l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Wal op mynre zielen <rhyme label="a" type="f">crachten</rhyme></l>
        <l id="s24:l2" met="3" type="-+">ende maket v sneel al <rhyme label="b" type="m">hier</rhyme></l>
        <l id="s24:l3" met="3" type="-+">ende dient den heren mit <rhyme label="c" type="f">vlyte</rhyme></l>
        <l id="s24:l4" met="3" type="-+">dat is al myn <rhyme label="b" type="m" corresp="#s24:l2">begeren</rhyme></l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Dat hi ons niet en <rhyme label="a" type="f">ontwyke</rhyme></l>
        <l id="s25:l2" met="3" type="-+">aen hem licht alle myn <rhyme label="b" type="m">troest</rhyme></l>
        <l id="s25:l3" met="3" type="-+">altoes als ic hem niet en <rhyme label="c" type="f">geuoele</rhyme></l>
        <l id="s25:l4" met="3" type="-+">myn ziele en wort niet <rhyme label="b" type="m" corresp="#s25:l2">verloest</rhyme></l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Soe bid ic v lieue <rhyme label="a" type="f">here</rhyme></l>
        <l id="s26:l2" met="3" type="-+">doer dine sware <rhyme label="b" type="m">pyne</rhyme></l>
        <l id="s26:l3" met="3" type="-+">soe verenige my in dinre <rhyme label="c" type="f">mynnen</rhyme></l>
        <l id="s26:l4" met="3" type="-+">soe en mach my nvmmermeer bet <rhyme label="b" type="m" corresp="#s26:l2">geschien</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
