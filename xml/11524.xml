<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11524</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Al hebben de prinsen haar wens</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SChoon of mijn stem als een <rhyme label="a" type="m">Basuyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Klonck over alder bergen <rhyme label="a" type="m" corresp="#s1:l1">kruyn</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En of de zang-Goddinnetjes <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dijn lof elck neuriden te <rhyme label="b" type="m" corresp="#s1:l3">moedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Soo was nochtans al haer <rhyme label="a" type="m">geschal</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Tot u waerdye niet met <rhyme label="a" type="m" corresp="#s2:l1">al</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Veel meerder eer met eenen <rhyme label="b" type="m">strael</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Verdiendy, als sy <rhyme label="b" type="m" corresp="#s2:l3">altemael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ghy maer eensjes van ter <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Werpt uwe teedere licht op <rhyme label="a" type="m" corresp="#s3:l1">my</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">'t Schijnt of Iupijn sijn wolckjes <rhyme label="b" type="m">dael</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Laet rollen, en my opwaerts <rhyme label="b" type="m" corresp="#s3:l3">hael</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Nectar niet so fris mijn <rhyme label="a" type="m">voedt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Als 't kussen van u lipjes <rhyme label="a" type="m" corresp="#s4:l1">soet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Noch alder Hemelen <rhyme label="b" type="m">krioel</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Tot u mijn lusjes niet <rhyme label="b" type="m" corresp="#s4:l3">verkoel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick puffe Iupijns gulde <rhyme label="a" type="m">zael</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En 't Goden selschap <rhyme label="a" type="m" corresp="#s5:l1">altemael</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wanneer ick maer 't Min-stovent <rhyme label="b" type="m">licht</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Bestaren mach van u <rhyme label="b" type="m" corresp="#s5:l3">gesicht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O schoonheyt schoon! wiens leetjes <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Puffen de blinckende <rhyme label="a" type="m" corresp="#s6:l1">Galathee</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Koningin van Paphos <rhyme label="b" type="m">swijckt</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Voor u, die geen Goddinnen <rhyme label="b" type="m" corresp="#s6:l3">wijckt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Het lieffelijck blos van u <rhyme label="a" type="m">gelaet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Bedroeft de goude <rhyme label="a" type="m" corresp="#s7:l1">dageraet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Sy went haer wel geswind'lijck <rhyme label="b" type="m">om</rhyme></l>
        <l id="s7:l4" met="4" type="-+">So ghy haer eensjes groeten <rhyme label="b" type="m" corresp="#s7:l3">kom</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Goddinnetje, die de schoone <rhyme label="a" type="m">bruyt</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Van Thiton steeckt de ooghjes <rhyme label="a" type="m" corresp="#s8:l1">uyt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">'t Is wonder hy u niet <rhyme label="b" type="m">beloert</rhyme></l>
        <l id="s8:l4" met="4" type="-+">En voor Aurora mede <rhyme label="b" type="m" corresp="#s8:l3">voert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
