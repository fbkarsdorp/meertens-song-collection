    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEljae, wel wat vremder <rhyme label="a" type="f">dinghen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wat of dit beduyden <rhyme label="b" type="m">wil</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">M’hoort naeu hier een deuntje <rhyme label="a" type="f" corresp="#s1:l1">singen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hoe of ‘t volckjen is dus <rhyme label="b" type="m" corresp="#s1:l2">stil</rhyme>?</l>
        <l id="s1:l5" met="4" type="-+">Iae wel, jae wel, ick gis, ick <rhyme label="c" type="m">gis</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Dat het niet van, dat het niet <rhyme label="d" type="m">van</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dat het niet van schaemten <rhyme label="c" type="m" corresp="#s1:l5">is</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ey dan lustigh gaet u <rhyme label="a" type="f">ganghe</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Yder brenght wat kluchjes <rhyme label="b" type="m">voort</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Sooje wist van Hase <rhyme label="a" type="f" corresp="#s2:l1">vanghe</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">‘T was by-na dan soo ‘t <rhyme label="b" type="m" corresp="#s2:l2">behoort</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Ey gut, ey gut, hoe fijn, hoe <rhyme label="c" type="m">fijn</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Soudt dan wesen, soudt dan <rhyme label="d" type="m">wesen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Soudt dan wesen, reck soudt <rhyme label="c" type="m" corresp="#s2:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Kon ik nu maer wat <rhyme label="a" type="f">versinne</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Waer me dat ick dede <rhyme label="b" type="m">Feest</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Kon vermaken, ‘k sou <rhyme label="a" type="f" corresp="#s3:l1">beginne</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">‘K deed’ het met een luchte <rhyme label="b" type="m" corresp="#s3:l2">gheest</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Maer siet, maer siet, ick weet, ick <rhyme label="c" type="m">weet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Lieve vrienden, lieve <rhyme label="d" type="m">vrienden</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Lieve vrienden niet een <rhyme label="c" type="m" corresp="#s3:l5">beet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ey maer speelt doch niet het <rhyme label="a" type="f">stommetje</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sou men hier niet vrolick <rhyme label="b" type="m">zijn</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Drinckt eens liever op een <rhyme label="a" type="f" corresp="#s4:l1">sommetje</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Vrolickheydt past by de <rhyme label="b" type="m" corresp="#s4:l2">Wijn</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Nu tsa, nu tsa, wel aen, wel <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Laet doch yemandt, laet doch <rhyme label="d" type="m">yemandt</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Laet doch yemandt wat <rhyme label="c" type="m" corresp="#s4:l5">om-gaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer, Speelnoot, ghy moet <rhyme label="a" type="f">ghehenghe</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dat ick, onder ‘t singhen, <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Al-te-met een kusje <rhyme label="a" type="f" corresp="#s5:l1">menghe</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Kusjes trotsent Nectar <rhyme label="b" type="m" corresp="#s5:l2">soet</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Nu dan, nu dan, wel aen, wel <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Laet dat soo eens, laet dat soo <rhyme label="d" type="m">eens</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Laet dat soo eens rondom <rhyme label="c" type="m" corresp="#s5:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laet ons nu eens rondom <rhyme label="a" type="f">drincken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Op de Bruydegom en <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Haer ghesontheyt nu wilt <rhyme label="a" type="f" corresp="#s6:l1">schincken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En dat moet wat schoontjes <rhyme label="b" type="m" corresp="#s6:l2">uyt</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">En als ‘t is uyt, ey dan, ey <rhyme label="c" type="m">dan</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Singht een veersje, singht een <rhyme label="d" type="m">veersje</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Singht een veersje datje <rhyme label="c" type="m" corresp="#s6:l5">kan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Nu, Speelnoot, ick maeck my <rhyme label="a" type="f">vaerdigh</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Denckt vast watje singhen <rhyme label="b" type="m">wil</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Al ist juyst niet even <rhyme label="a" type="f" corresp="#s7:l1">aerdigh</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Helpt my, keunje, swijght niet <rhyme label="b" type="m" corresp="#s7:l2">stil</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Nu drinckt, nu drinckt. Ist uyt, soo <rhyme label="c" type="m">klinght</rhyme>:</l>
        <l id="s7:l6" met="4" type="+-">Nu het veersje, nu het <rhyme label="d" type="m">veersje</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Mu het veersje luchtigh <rhyme label="c" type="m" corresp="#s7:l5">singht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
