    <melody>Rosbol wou laatst buizen</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CVpido Venus <rhyme label="a" type="f">Boefken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Fielken, maeckt u pack, en <rhyme label="b" type="m">sack</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Want <rhyme label="b" type="m" corresp="#s1:l2">ongemack</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Noyt en <rhyme label="b" type="m" corresp="#s1:l2">onbrack</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Aen die inde Liefde <rhyme label="b" type="m" corresp="#s1:l2">stack</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Van hier, Ã´ vuylste <rhyme label="a" type="f" corresp="#s1:l1">Slofken</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Met al u <rhyme label="c" type="m">sotterny</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Die gy <rhyme label="c" type="m" corresp="#s1:l7">my</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Brenghden <rhyme label="c" type="m" corresp="#s1:l7">by</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">k' Wil u gaen stellen aen een <rhyme label="c" type="m" corresp="#s1:l7">sy</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">k' Was eens by-naer <rhyme label="a" type="f">gevangen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En getrest met u wen <rhyme label="b" type="m">Schicht</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Klijn Venus <rhyme label="b" type="m" corresp="#s2:l2">wicht</rhyme></l>
        <l id="s2:l4" met="2" type="-+">'t Verstants <rhyme label="b" type="m" corresp="#s2:l2">gesicht</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Doen verblindt, is nu <rhyme label="b" type="m" corresp="#s2:l2">verlicht</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Want k' gaen nu ander <rhyme label="a" type="f" corresp="#s2:l1">gangen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En lach u schichten <rhyme label="c" type="m">uyt</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Klijnen <rhyme label="c" type="m" corresp="#s2:l7">Guyt</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Van my <rhyme label="c" type="m" corresp="#s2:l7">bruyt</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Off ick bril u voor den <rhyme label="c" type="m" corresp="#s2:l7">snuyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Waer vondtmen snooder <rhyme label="a" type="f">plaegen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Als gebruyt sijn met een <rhyme label="b" type="m">Wyff</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Die ons seer <rhyme label="b" type="m" corresp="#s3:l2">styff</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Naer lanck <rhyme label="b" type="m" corresp="#s3:l2">gekyff</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Rammelt, sammelt op het <rhyme label="b" type="m" corresp="#s3:l2">lijff</rhyme></l>
        <l id="s3:l6" met="3" type="-+">'tIs niet om te <rhyme label="a" type="f" corresp="#s3:l1">verdraegen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dat eenen goeden <rhyme label="c" type="m">Bloet</rhyme></l>
        <l id="s3:l8" met="2" type="+-">Duycken <rhyme label="c" type="m" corresp="#s3:l7">moet</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Met <rhyme label="c" type="m" corresp="#s3:l7">oytmoet</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Vallen sijne Vrouw te <rhyme label="c" type="m" corresp="#s3:l7">voet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ma foy ick moet <rhyme label="a" type="f">beklaegen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Onsen armen Duyvel <rhyme label="b" type="m">Jan</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Die ons hier <rhyme label="b" type="m" corresp="#s4:l2">van</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Ten vollen <rhyme label="b" type="m" corresp="#s4:l2">kan</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Geven thoon den armen <rhyme label="b" type="m" corresp="#s4:l2">Man</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Syn VVyff vat by de <rhyme label="a" type="f" corresp="#s4:l1">kraege</rhyme></l>
        <l id="s4:l7" met="3" type="-+">O Duyvels wat een <rhyme label="c" type="m">Hel</rhyme>!</l>
        <l id="s4:l8" met="2" type="+-">Smeert sijn <rhyme label="c" type="m" corresp="#s4:l7">vel</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Wonder <rhyme label="c" type="m" corresp="#s4:l7">fel</rhyme></l>
        <l id="s4:l10" met="4" type="-+">k' Heb geenen troef in sulcken <rhyme label="c" type="m" corresp="#s4:l7">spel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Voor my ick sau haer <rhyme label="a" type="f">schencken</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Voor present, oft een <rhyme label="b" type="m">Nieuw-Jaer</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Eer ick door <rhyme label="b" type="m" corresp="#s5:l2">haer</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Quam in 't <rhyme label="b" type="m" corresp="#s5:l2">gevaer</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'tWaer voor den Duyvel just een <rhyme label="b" type="m" corresp="#s5:l2">paer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">s'En hoefd' het niet te <rhyme label="a" type="f" corresp="#s5:l1">dencken</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Al waer sy noch soo <rhyme label="c" type="m">kloeck</rhyme>;</l>
        <l id="s5:l8" met="2" type="+-">Ick de <rhyme label="c" type="m" corresp="#s5:l7">Broeck</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Sy den <rhyme label="c" type="m" corresp="#s5:l7">Doeck</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">k'Sou haer planten inden <rhyme label="c" type="m" corresp="#s5:l7">hoeck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
