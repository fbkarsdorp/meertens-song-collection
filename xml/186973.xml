  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Meend gy Vader, dat my 't <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Heeft <rhyme label="a" type="f" corresp="#s1:l1">begeven</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Door mijn ziekte en zwakke <rhyme label="b" type="m">leen</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Staak uw oordeel: want dat <rhyme label="c" type="f">gissen</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Kon vry <rhyme label="c" type="f" corresp="#s1:l4">missen</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Mijne dood heeft andre <rhyme label="b" type="m" corresp="#s1:l3">reen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wijl ik was PoÃ«et <rhyme label="a" type="f">geboren</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Heeft het <rhyme label="a" type="f" corresp="#s2:l1">hooren</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van Antonides zijn <rhyme label="b" type="m">dood</rhyme></l>
        <l id="s2:l4" met="4" type="+-">My van bittren rouw doen <rhyme label="c" type="f">sterven</rhyme></l>
        <l id="s2:l5" met="2" type="+-">'t Graf doen <rhyme label="c" type="f" corresp="#s2:l4">erven</rhyme></l>
        <l id="s2:l6" met="4" type="+-">En u van uw Zoon <rhyme label="b" type="m" corresp="#s2:l3">ontbloot</rhyme>.</l>
      </lg>
    </lg>
  </text>
