  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HOudt gy nog langer van <rhyme label="a" type="f">wagten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Schoonste kom doet mijn <rhyme label="b" type="f">begeer</rhyme>,</l>
        <l id="s1:l3" met="5" type="+-">De min, de min, die moet men niet <rhyme label="a" type="f" corresp="#s1:l1">veragte</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">De min geeft soo veel <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Dat hij ons leeve <rhyme label="c" type="m" corresp="#s1:l4">doet</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">De liefde geeft al <rhyme label="c" type="m" corresp="#s1:l4">soet</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">Dat wy begrijpen met <rhyme label="a" type="f" corresp="#s1:l1">gedagten</rhyme>,</l>
        <l id="s1:l8" met="5" type="+-">De min, de min die moet men niet <rhyme label="a" type="f" corresp="#s1:l1">veragten</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">De min geeft soo veel <rhyme label="c" type="m" corresp="#s1:l4">soet</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Dat hy ons leeve <rhyme label="c" type="m" corresp="#s1:l4">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
