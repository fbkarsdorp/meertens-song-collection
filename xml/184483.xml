  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Hooft, myn Koning en myn <rhyme label="a" type="m">God</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Myn Kroon, myn Loon, myn <rhyme label="b" type="f">Alles</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Om wien ik liever schand en <rhyme label="a" type="m" corresp="#s1:l1">spot</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dan alle vrucht des <rhyme label="b" type="f" corresp="#s1:l2">Valles</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dan aller weereld vreugd' en <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Goed, gaave, roem en <rhyme label="d" type="f">eere</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">'t Welk door de dood word <rhyme label="c" type="m" corresp="#s1:l5">uitgebluscht</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">t'Ontfangen hier <rhyme label="d" type="f" corresp="#s1:l6">begeere</rhyme>.</l>
      </lg>
    </lg>
