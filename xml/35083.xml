<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35083</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1700</year-end>
    </date>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wie sal, wie kan de minne <rhyme label="a" type="f">kragten</rhyme></l>
        <l id="s1:l2" met="4" type="-+">beteuglen, wie is oijt soo <rhyme label="b" type="m">stout</rhyme></l>
        <l id="s1:l3" met="4" type="-+">dat hij vrouw venus durft <rhyme label="a" type="f" corresp="#s1:l1">misachten</rhyme></l>
        <l id="s1:l4" met="4" type="-+">en niet bevreest is dat t'hem <rhyme label="b" type="m" corresp="#s1:l2">rouwt</rhyme>?</l>
        <l id="s1:l5" met="2" type="-+">terght haer dogh <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s1:l6" met="2" type="-+">haer hoogh <rhyme label="c" type="m" corresp="#s1:l5">gebied</rhyme></l>
        <l id="s1:l7" met="4" type="-+">is gewoon alles te <rhyme label="a" type="f" corresp="#s1:l1">verzachten</rhyme></l>
        <l id="s1:l8" met="4" type="-+">daer sij maer selfs een lonck en <rhyme label="b" type="m" corresp="#s1:l2">schiet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Indien sij kan door enkle <rhyme label="a" type="f">loncken</rhyme></l>
        <l id="s2:l2" met="4" type="-+">gelijck sij dicwils heeft <rhyme label="b" type="m">getoont</rhyme></l>
        <l id="s2:l3" met="4" type="-+">het wederspanninghst hert <rhyme label="a" type="f" corresp="#s2:l1">ontvoncken</rhyme></l>
        <l id="s2:l4" met="4" type="-+">dat in het kille noorden <rhyme label="b" type="m" corresp="#s2:l2">woont</rhyme></l>
        <l id="s2:l5" met="2" type="-+">ah wat sal <rhyme label="c" type="m">t'sijn</rhyme></l>
        <l id="s2:l6" met="2" type="-+">soo sij <rhyme label="c" type="m" corresp="#s2:l5">verschijnt</rhyme></l>
        <l id="s2:l7" met="4" type="-+">met al de nacht aen haer <rhyme label="a" type="f" corresp="#s2:l1">geschoncken</rhyme></l>
        <l id="s2:l8" met="4" type="-+">om ons te brengen inde <rhyme label="b" type="m" corresp="#s2:l2">pijn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">haer onweerstaenelicke <rhyme label="a" type="f">lichten</rhyme></l>
        <l id="s3:l2" met="4" type="-+">veroveren alles t'is <rhyme label="b" type="m">gewis</rhyme></l>
        <l id="s3:l3" met="4" type="-+">en haer soons scherp gesleepe <rhyme label="a" type="f" corresp="#s3:l1">schichten</rhyme></l>
        <l id="s3:l4" met="4" type="-+">en schieten nimmermeer eens <rhyme label="b" type="m" corresp="#s3:l2">mis</rhyme></l>
        <l id="s3:l5" met="2" type="-+">noijt wierd hun <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s3:l6" met="2" type="-+">van een <rhyme label="c" type="m" corresp="#s3:l5">misacht</rhyme></l>
        <l id="s3:l7" met="4" type="-+">die net als hij die dacht te <rhyme label="a" type="f" corresp="#s3:l1">swichten</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Het  eerst ten onder wierd <rhyme label="b" type="m" corresp="#s3:l2">gebraght</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Siet men niet daegelijckx <rhyme label="a" type="f">geschieden</rhyme></l>
        <l id="s4:l2" met="4" type="-+">dat selfs monarchen hoogh van <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">die d'halfve weerelt schier <rhyme label="a" type="f" corresp="#s4:l1">gebieden</rhyme></l>
        <l id="s4:l4" met="4" type="-+">sigh buijghen voor een soet <rhyme label="b" type="m" corresp="#s4:l2">gelaet</rhyme></l>
        <l id="s4:l5" met="2" type="-+">de kracht der <rhyme label="c" type="m">min</rhyme></l>
        <l id="s4:l6" met="2" type="-+">heeft wonder <rhyme label="c" type="m" corresp="#s4:l5">in</rhyme></l>
        <l id="s4:l7" met="4" type="-+">soo wie de selve pooght <rhyme label="a" type="f" corresp="#s4:l1">t'ontvlieden</rhyme></l>
        <l id="s4:l8" met="4" type="-+">vergroot haer segen en <rhyme label="b" type="m" corresp="#s4:l2">gewin</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">haer listen die sijn <rhyme label="a" type="f">meenighvuldigh</rhyme></l>
        <l id="s5:l2" met="4" type="-+">haer kracht kan niemant tegen <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">elck een is haer tribuijt <rhyme label="a" type="f" corresp="#s5:l1">verschuldight</rhyme></l>
        <l id="s5:l4" met="4" type="-+">soo haest hij t'leven heeft <rhyme label="b" type="m" corresp="#s5:l2">ontfaen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">ons ouders <rhyme label="c" type="m">min</rhyme></l>
        <l id="s5:l6" met="2" type="-+">stort ons sulcx <rhyme label="c" type="m" corresp="#s5:l5">in</rhyme></l>
        <l id="s5:l7" met="4" type="-+">van ons gewilligh en <rhyme label="a" type="f" corresp="#s5:l1">verduldigh</rhyme></l>
        <l id="s5:l8" met="4" type="-+">te schicken naer haer wet en <rhyme label="b" type="m" corresp="#s5:l2">sin</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Den eersten mensch ons aller <rhyme label="a" type="f">vader</rhyme></l>
        <l id="s6:l2" met="4" type="-+">gaf door sijn doen hier van <rhyme label="b" type="m">bericht</rhyme></l>
        <l id="s6:l3" met="4" type="-+">hij heeft met t'leven in ons <rhyme label="a" type="f" corresp="#s6:l1">aders</rhyme></l>
        <l id="s6:l4" met="4" type="-+">gestort die drift waer voor t'al <rhyme label="b" type="m" corresp="#s6:l2">zwicht</rhyme></l>
        <l id="s6:l5" met="2" type="-+">en eerst <rhyme label="c" type="m">verlicht</rhyme></l>
        <l id="s6:l6" met="2" type="-+">den grond <rhyme label="c" type="m" corresp="#s6:l5">gesticht</rhyme></l>
        <l id="s6:l7" met="4" type="-+">waerdoor dat wij sijn <rhyme label="a" type="f" corresp="#s6:l1">allegader</rhyme></l>
        <l id="s6:l8" met="4" type="-+">de weerelt nazaeten <rhyme label="b" type="m" corresp="#s6:l2">verplicht</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
