<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6870</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>emmerick</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Beata immaculata</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Laet ons al t’samen <rhyme label="a" type="f">loven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met sanck en snaren <rhyme label="b" type="m">spel</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">De Borghers van hier <rhyme label="a" type="f" corresp="#s1:l1">boven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Want sij’t verdienen <rhyme label="b" type="m" corresp="#s1:l2">wel</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Singt met <rhyme label="c" type="m" corresp="#s1:l5">jolijt</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s1:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer onder veele <rhyme label="a" type="f">diversse</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soo wil ick mijn <rhyme label="b" type="m">Patroon</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En mijne <rhyme label="a" type="f" corresp="#s2:l1">Patronersse</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Singen een lof <rhyme label="b" type="m" corresp="#s2:l2">ydoon</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Singt met <rhyme label="c" type="m" corresp="#s2:l5">jolijt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s2:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O soet Patroon <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">O Patronersse <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wat soud’ ick u best <rhyme label="a" type="f" corresp="#s3:l1">geven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat u sou wesen <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme>?</l>
        <l id="s3:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Singt met <rhyme label="c" type="m" corresp="#s3:l5">jolijt</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s3:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wilt ghy ons wel <rhyme label="a" type="f">vereeren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die onsen naeme <rhyme label="b" type="m">draecht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Voor allen, wilt dit <rhyme label="a" type="f" corresp="#s4:l1">leeren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">V wel van sonden <rhyme label="b" type="m" corresp="#s4:l2">vaecht</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Singt met <rhyme label="c" type="m" corresp="#s4:l5">jolijt</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s4:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daer naer so moet ghy <rhyme label="a" type="f">talen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Naer onse deuchden <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Kondt ghy die <rhyme label="a" type="f" corresp="#s5:l1">achter-halen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Weest seker ’s Hemels <rhyme label="b" type="m" corresp="#s5:l2">kroon</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Weest al <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Singt met <rhyme label="c" type="m" corresp="#s5:l5">jolijt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Maer eerst maeckt u de sonden <rhyme label="c" type="m" corresp="#s5:l5">quijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
