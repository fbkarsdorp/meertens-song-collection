  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TReet vlugge voetjes <rhyme label="a" type="m">treet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Treet vlugge voetjes <rhyme label="a" type="m" corresp="#s1:l1">treet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Op dat ick klaegh mijn <rhyme label="a" type="m" corresp="#s1:l1">leet</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Aen mijn <rhyme label="b" type="m">Goddin</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Terwijl de stille <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hier heelt mijn droeve <rhyme label="c" type="m" corresp="#s1:l5">klaght</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Voegh ick my <rhyme label="d" type="m">veur</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Ach Philida, Philida, u weerde <rhyme label="d" type="m" corresp="#s1:l7">deur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ontwaeckt Goddin <rhyme label="a" type="m">ontwaeckt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ontwaeckt Goddin <rhyme label="a" type="m" corresp="#s2:l1">ontwaeckt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En 'tsoete droomen <rhyme label="a" type="m" corresp="#s2:l1">staeckt</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">En sluymert <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Terwijl u Dienaer <rhyme label="c" type="m">sucht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En met bedroeft <rhyme label="c" type="m" corresp="#s2:l5">gerucht</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Zijn dienst u <rhyme label="d" type="m">bied</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Ach Philida, Philida, verberght u <rhyme label="d" type="m" corresp="#s2:l7">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Pronck van ons gantsche <rhyme label="a" type="m">Stad</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Pronck van ons gantsche <rhyme label="a" type="m" corresp="#s3:l1">Stad</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Waerder dan al de <rhyme label="a" type="m" corresp="#s3:l1">Schadt</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Waer oyt den <rhyme label="b" type="m">Mensch</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Heeft vlytigh <rhyme label="c" type="m">na-gejaeght</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En listigh <rhyme label="c" type="m" corresp="#s3:l5">na-gelaeght</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">Meerder ick <rhyme label="d" type="m">u</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Steets viere en diene, ach waerom noch <rhyme label="d" type="m" corresp="#s3:l7">schu</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hoe meught ghy langer <rhyme label="a" type="m">zien</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hoe meught ghy langer <rhyme label="a" type="m" corresp="#s4:l1">zien</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Het nedrigh zoet <rhyme label="a" type="m" corresp="#s4:l1">aenbien</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Van mynen <rhyme label="b" type="m">dienst</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hoe komt ghy <rhyme label="c" type="m">Celadon</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Zyn licht, zijn tweede <rhyme label="c" type="m" corresp="#s4:l5">zon</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Onthouden, <rhyme label="d" type="m">och</rhyme>!</l>
        <l id="s4:l8" met="4" type="-+">Neen Philida, Philida, 'khout voor <rhyme label="d" type="m" corresp="#s4:l7">bedrog</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Opent vry <rhyme label="a" type="m">'tIngewant</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Opent vry <rhyme label="a" type="m" corresp="#s5:l1">'tIngewant</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">'t Welck staet in lichten <rhyme label="a" type="m" corresp="#s5:l1">brand</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">En smeuckt soo <rhyme label="b" type="m">voort</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Want waer ick gae of <rhyme label="c" type="m">stae</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daer volght my stadigh <rhyme label="c" type="m" corresp="#s5:l5">na</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Dit zoet <rhyme label="d" type="m">gewoel</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Maer Philida, Philida, blijft even <rhyme label="d" type="m" corresp="#s5:l7">koel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Schoon of u zoet <rhyme label="a" type="m">gemoet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Schoon of u zoet <rhyme label="a" type="m" corresp="#s6:l1">gemoet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">My noch te haten <rhyme label="a" type="m" corresp="#s6:l1">spoet</rhyme>:</l>
        <l id="s6:l4" met="2" type="-+">Nochtans ick <rhyme label="b" type="m">zweer</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Dat mijne trouwe <rhyme label="c" type="m">Min</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Gegrond in't binnenst' <rhyme label="c" type="m" corresp="#s6:l5">in</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Mijn Ziele <rhyme label="d" type="m">leeft</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Tot Celadon Philida op aerd' <rhyme label="d" type="m" corresp="#s6:l7">begeeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
