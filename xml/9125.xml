<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9125</id>
    <date>
      <year-start>1676</year-start>
      <year-end>1676</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Mensen onvroedig</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VLugtige <rhyme label="a" type="f">jaren</rhyme>!</l>
        <l id="s1:l2" met="3" type="+-">Die daar henen <rhyme label="b" type="f">glijden</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Als een wevers <rhyme label="c" type="m">spoel</rhyme>;</l>
        <l id="s1:l4" met="3" type="+-">Wat <rhyme label="a" type="f" corresp="#s1:l1">wedervaren</rhyme></l>
        <l id="s1:l5" met="3" type="+-">doet gy ons al <rhyme label="b" type="f" corresp="#s1:l2">lijden</rhyme></l>
        <l id="s1:l6" met="3" type="+-">In des werelds <rhyme label="c" type="m" corresp="#s1:l3">woel</rhyme>?</l>
        <l id="s1:l7" met="5" type="+-">Soo wy niet mickten na des Hemels <rhyme label="c" type="m" corresp="#s1:l3">doel</rhyme>.</l>
        <l id="s1:l8" met="3" type="+-">Voorspoeden sijn <rhyme label="d" type="m">dog</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Niet dan <rhyme label="e" type="f">ydelheden</rhyme>:</l>
        <l id="s1:l10" met="3" type="+-">En <rhyme label="d" type="m" corresp="#s1:l8">tegenspoeden</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Waren haast <rhyme label="e" type="f" corresp="#s1:l9">geleden</rhyme>:</l>
        <l id="s1:l12" met="3" type="+-">Oh! dat eeuwig <rhyme label="f" type="m">Al</rhyme>!</l>
        <l id="s1:l13" met="3" type="+-">Oh! dat eeuwig <rhyme label="f" type="m" corresp="#s1:l12">Al</rhyme>!</l>
        <l id="s1:l14" met="5" type="+-">Is het Een dat ick be-oogen <rhyme label="f" type="m" corresp="#s1:l12">sal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Al de <rhyme label="a" type="f">vermaken</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Die de wereld <rhyme label="b" type="f">aanbood</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Wt haar vuylen <rhyme label="c" type="m">poel</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Moet ick nu <rhyme label="a" type="f" corresp="#s2:l1">laacken</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Die der sielen <rhyme label="b" type="f" corresp="#s2:l2">aanstoot</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Levendig <rhyme label="c" type="m" corresp="#s2:l3">gevoel</rhyme>:</l>
        <l id="s2:l7" met="5" type="+-">Nu ick met hert stel na des Hemels <rhyme label="c" type="m" corresp="#s2:l3">doel</rhyme>.</l>
        <l id="s2:l8" met="3" type="+-">Onsinnig mensch! <rhyme label="d" type="m">hoort</rhyme>!</l>
        <l id="s2:l9" met="3" type="+-">Wat u schijnt een <rhyme label="e" type="f">luyster</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Siet een na <rhyme label="d" type="m" corresp="#s2:l8">boven</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Alles wert u <rhyme label="e" type="f" corresp="#s2:l9">duyster</rhyme>.</l>
        <l id="s2:l12" met="3" type="+-">Oh! dat eeuwig' <rhyme label="f" type="m">Al</rhyme>!</l>
        <l id="s2:l13" met="3" type="+-">Oh! dat eeuwig' <rhyme label="f" type="m" corresp="#s2:l12">Al</rhyme>!</l>
        <l id="s2:l14" met="5" type="+-">Is het Een dat ick be-oogen <rhyme label="f" type="m" corresp="#s2:l12">sal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ah trouwen <rhyme label="a" type="f">Heyland</rhyme>!</l>
        <l id="s3:l2" met="3" type="+-">Die noyt cont <rhyme label="b" type="f">versmaden</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Die sig 'tuwaarts <rhyme label="c" type="m">voegt</rhyme>;</l>
        <l id="s3:l4" met="3" type="+-">Voert my op t' <rhyme label="a" type="f" corresp="#s3:l1">Wey-land</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Van uw vette <rhyme label="b" type="f" corresp="#s3:l2">genaden</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Daar mijn siel na <rhyme label="c" type="m" corresp="#s3:l3">swoegt</rhyme>:</l>
        <l id="s3:l7" met="5" type="+-">So is mijn siel in u eeuwig <rhyme label="c" type="m" corresp="#s3:l3">vergenoegt</rhyme>.</l>
        <l id="s3:l8" met="3" type="+-">Al 's werelds lust <rhyme label="d" type="m">gaat</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Met de tijd <rhyme label="e" type="f">verdwijnen</rhyme>.</l>
        <l id="s3:l10" met="3" type="+-">Ey! 's hemels vier <rhyme label="d" type="m" corresp="#s3:l8">laat</rhyme></l>
        <l id="s3:l11" met="3" type="+-">In de plaats <rhyme label="e" type="f" corresp="#s3:l9">verschijnen</rhyme>!</l>
        <l id="s3:l12" met="3" type="+-">Oh! dat eeuwig <rhyme label="f" type="m">al</rhyme>!</l>
        <l id="s3:l13" met="3" type="+-">Oh! dat eeuwig <rhyme label="f" type="m" corresp="#s3:l12">al</rhyme>!</l>
        <l id="s3:l14" met="5" type="+-">Is het Een dat ick be-oogen <rhyme label="f" type="m" corresp="#s3:l12">sal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">In dees mijn <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Waarlijck levens <rhyme label="b" type="f">nagten</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Als den tijt <rhyme label="c" type="m">ontgaat</rhyme>:</l>
        <l id="s4:l4" met="3" type="+-">Laat u <rhyme label="a" type="f" corresp="#s4:l1">behagen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Alle mijn <rhyme label="b" type="f" corresp="#s4:l2">gedagten</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Woorden, wil, en <rhyme label="c" type="m" corresp="#s4:l3">daad</rhyme>;</l>
        <l id="s4:l7" met="5" type="+-">En in my groejen het woort dat levend <rhyme label="c" type="m" corresp="#s4:l3">saad</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Op dat ick nog <rhyme label="d" type="m">in</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Dees onvrugtbre <rhyme label="e" type="f">dage'</rhyme></l>
        <l id="s4:l10" met="3" type="+-">U sy ten dienst, <rhyme label="d" type="m" corresp="#s4:l8">en</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Heyl'ge vrugten <rhyme label="e" type="f" corresp="#s4:l9">drage</rhyme>.</l>
        <l id="s4:l12" met="3" type="+-">Oh! dat eeuwig <rhyme label="f" type="m">al</rhyme>!</l>
        <l id="s4:l13" met="3" type="+-">Oh! dat eeuwig <rhyme label="f" type="m" corresp="#s4:l12">al</rhyme></l>
        <l id="s4:l14" met="5" type="+-">Is het Een dat ick be-oogen <rhyme label="f" type="m" corresp="#s4:l12">sal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Oneyndig <rhyme label="a" type="f">heylig</rhyme>!</l>
        <l id="s5:l2" met="3" type="+-">Onbegrepen <rhyme label="b" type="f">heerlijck</rhyme>!</l>
        <l id="s5:l3" met="3" type="+-">On-toe-ganckelijck <rhyme label="c" type="m">ligt</rhyme>!</l>
        <l id="s5:l4" met="3" type="+-">Tot U men <rhyme label="a" type="f" corresp="#s5:l1">veylig</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Gaat, want al 't <rhyme label="b" type="f" corresp="#s5:l2">begeerlijck</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Is in uw <rhyme label="c" type="m" corresp="#s5:l3">gesigt</rhyme>.</l>
        <l id="s5:l7" met="5" type="+-">Was maar ons hert na u evenbeeld <rhyme label="c" type="m" corresp="#s5:l3">gestigt</rhyme>!</l>
        <l id="s5:l8" met="3" type="+-">Oh! dat de <rhyme label="d" type="m">heyligheyd</rhyme></l>
        <l id="s5:l9" met="3" type="+-">Ciersel van uw <rhyme label="e" type="f">throone</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">In dees mijn <rhyme label="d" type="m" corresp="#s5:l8">vreemdelingschap</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Sy mijn hooft ten <rhyme label="e" type="f" corresp="#s5:l9">croone</rhyme>!</l>
        <l id="s5:l12" met="3" type="+-">Tot het eeuwig' <rhyme label="f" type="m">Al</rhyme></l>
        <l id="s5:l13" met="3" type="+-">Tot het eeuwig' <rhyme label="f" type="m" corresp="#s5:l12">Al</rhyme></l>
        <l id="s5:l14" met="5" type="+-">My om Iesus wil volmaken <rhyme label="f" type="m" corresp="#s5:l12">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
