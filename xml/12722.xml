<song>
  <meta>
    <id>12722</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Polyphemus aan de stranden</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Siet, dus gaen de Iaren <rhyme label="a" type="f">strijcken</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Van ons <rhyme label="a" type="f" corresp="#s1:l1">wijcken</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">’t Eene voor en ’t ander <rhyme label="b" type="m">na</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Even vluchtigh werdt ons <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Heen <rhyme label="c" type="f" corresp="#s1:l4">gedreven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Daer ick vaeck op stille <rhyme label="b" type="m" corresp="#s1:l3">sta</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">’t Is te wonder, wy <rhyme label="a" type="f">verkouden</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Door ’t <rhyme label="a" type="f" corresp="#s2:l1">verouden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En veracht’ren van de <rhyme label="b" type="m">Deught</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Sonden die men moest <rhyme label="c" type="f">besnoeyen</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Staen en <rhyme label="c" type="f" corresp="#s2:l4">bloeyen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Als in ’t heetste van de <rhyme label="b" type="m" corresp="#s2:l3">jeught</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">’t Quade dat men hoort te <rhyme label="a" type="f">mijden</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Af te <rhyme label="a" type="f" corresp="#s3:l1">snijden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Gaet noch even hard’ in <rhyme label="b" type="m">swangh</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Selve die door Christi <rhyme label="c" type="f">leden</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Heen gaen <rhyme label="c" type="f" corresp="#s3:l4">treden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Singen noch haer oude <rhyme label="b" type="m" corresp="#s3:l3">sang</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Stemmigh komen sy en <rhyme label="a" type="f">statigh</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Seer <rhyme label="a" type="f" corresp="#s4:l1">gelatigh</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om te hooren Godes <rhyme label="b" type="m">Woort</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Maer ’t is nau’lijck <rhyme label="c" type="f">uyt-gesproken</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-"><rhyme label="c" type="f" corresp="#s4:l4">Afghebroken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Of sy gaen weer sondigh <rhyme label="b" type="m" corresp="#s4:l3">voort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">O! wat siet men groote <rhyme label="a" type="f">hoopen</rhyme>?</l>
        <l id="s5:l2" met="2" type="+-">Sondaeghs <rhyme label="a" type="f" corresp="#s5:l1">loopen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Die ons heyligh hoord’ te <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Na de Bier-banck, daer sy <rhyme label="c" type="f">kroegen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Datse <rhyme label="c" type="f" corresp="#s5:l4">swoegen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Tot sy worden als een <rhyme label="b" type="m" corresp="#s5:l3">Swijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">’S morgens moet men dan noch <rhyme label="a" type="f">roemen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">En gaen <rhyme label="a" type="f" corresp="#s6:l1">noemen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Hoe en wat men heeft <rhyme label="b" type="m">gedaen</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">O! seyt d’een, ick was soo <rhyme label="c" type="f">droncken</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s6:l4">beschoncken</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat ick qualijck konde <rhyme label="b" type="m" corresp="#s6:l3">gaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Weer een ander seyt het <rhyme label="a" type="f">sijne</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Ick voor ’t <rhyme label="a" type="f" corresp="#s7:l1">mijne</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Was soo dick gelijckeen <rhyme label="b" type="m">Ton</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat men my na huys moest’ <rhyme label="c" type="f">leyden</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Tusschen <rhyme label="c" type="f" corresp="#s7:l4">beyden</rhyme>!</l>
        <l id="s7:l6" met="4" type="+-">Want ick heen noch weer en <rhyme label="b" type="m" corresp="#s7:l3">kon</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dat zijn Helden, dat sijn <rhyme label="a" type="f">Geesten</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Dicht aen <rhyme label="a" type="f" corresp="#s8:l1">Beesten</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dat zijn Mannen kloeck en <rhyme label="b" type="m">sneegh</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Dat zijn uyt-genomen <rhyme label="c" type="f">Basen</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Die de <rhyme label="c" type="f" corresp="#s8:l4">Glasen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Tot de grondt toe maken <rhyme label="b" type="m" corresp="#s8:l3">leegh</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Kon men soo den Hemel <rhyme label="a" type="f">winnen</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">’k Woud’ <rhyme label="a" type="f" corresp="#s9:l1">beginnen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Om die wegh oock in te <rhyme label="b" type="m">slaen</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Maer, o! Gieters, Suyvers, <rhyme label="c" type="f">Gluypers</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Slempers, <rhyme label="c" type="f" corresp="#s9:l4">Suypers</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dronckaerts sullen buyten <rhyme label="b" type="m" corresp="#s9:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Menigh heb ick wegh sien <rhyme label="a" type="f">halen</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s10:l1">Neder-dalen</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">In de Dronckenschap <rhyme label="b" type="m">versmoort</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">En wie soud’ het wonder <rhyme label="c" type="f">geven</rhyme>?</l>
        <l id="s10:l5" met="2" type="+-">Dat g’u <rhyme label="c" type="f" corresp="#s10:l4">leven</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Nu of dan oock soo <rhyme label="b" type="m" corresp="#s10:l3">verloort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
