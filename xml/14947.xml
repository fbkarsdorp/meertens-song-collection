    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>My mistress sings no other song</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IC weet wel wat den Duytser <rhyme label="a" type="m">schort</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dat hy so by zyn selver <rhyme label="a" type="m" corresp="#s1:l1">mort</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En vloect nu vast, al ist maar <rhyme label="b" type="m">spel</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Hans is in <rhyme label="c" type="f">ly’en</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Om syn mal <rhyme label="c" type="f" corresp="#s1:l4">vryen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Hans is in <rhyme label="c" type="f" corresp="#s1:l4">ly’en</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Dat ist <rhyme label="b" type="m" corresp="#s1:l3">ghequel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hy wouw het Meysjen soo hy <rhyme label="a" type="m">zeyt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Maar voelen of zy Knecht oft <rhyme label="a" type="m" corresp="#s2:l1">Meyt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Was, maar de afgherechte <rhyme label="b" type="m">Feecx</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Ginck hem so <rhyme label="c" type="f">groeten</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Zijn lust te <rhyme label="c" type="f" corresp="#s2:l4">boeten</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Ginck hem soo <rhyme label="c" type="f" corresp="#s2:l4">groeten</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Dat met haar <rhyme label="b" type="m" corresp="#s2:l3">Reecks</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nu krauw’t hy vast zyn buyl’ghe <rhyme label="a" type="m">kop</rhyme></l>
        <l id="s3:l2" met="4" type="-+">En voelter noch de Sleutels <rhyme label="a" type="m" corresp="#s3:l1">op</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Gaat Jong (seght hy) haalt mijn <rhyme label="b" type="m">gheweer</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ick wilze <rhyme label="c" type="f">wonden</rhyme></l>
        <l id="s3:l5" met="2" type="-+">By gantsch <rhyme label="c" type="f" corresp="#s3:l4">wonden</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Ick wilse <rhyme label="c" type="f" corresp="#s3:l4">wonden</rhyme></l>
        <l id="s3:l7" met="2" type="-+">De loose <rhyme label="b" type="m" corresp="#s3:l3">Meer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy peurt haar met den Degen <rhyme label="a" type="m">na</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Maar ‘twas al weer tot zynder <rhyme label="a" type="m" corresp="#s4:l1">scha</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Want het volck hem de huyt so <rhyme label="b" type="m">murf</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Sloeghen, en <rhyme label="c" type="f">beuckten</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Waar het hem <rhyme label="c" type="f" corresp="#s4:l4">jeuckten</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Sloeghen en <rhyme label="c" type="f" corresp="#s4:l4">beuckten</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Dat hy schier <rhyme label="b" type="m" corresp="#s4:l3">sturf</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
