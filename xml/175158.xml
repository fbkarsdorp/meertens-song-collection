    <melody>Belgica loof de heer</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="groups">groepen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Belgica looft den <rhyme label="a" type="m">Here</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">diet alles heeft <rhyme label="b" type="f">begrepen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dyn vyant sterck op <rhyme label="a" type="m" corresp="#s1:l1">meer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">met zyne groote <rhyme label="b" type="f" corresp="#s1:l2">scheepen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">en hoogen trotsen <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">leyt heel onder de <rhyme label="c" type="m" corresp="#s1:l5">voet</rhyme></l>
        <l id="s1:l7" met="2" type="-+">als nu <rhyme label="d" type="m">vertreen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Door zynen arm <rhyme label="d" type="m" corresp="#s1:l7">alleen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De groote Spaensche <rhyme label="a" type="m">vloot</rhyme></l>
        <l id="s2:l2" met="3" type="-+">met meenich duijsent <rhyme label="b" type="f">knechten</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En Parma die zoo <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">hem stelde om te <rhyme label="b" type="f" corresp="#s2:l2">vechten</rhyme></l>
        <l id="s2:l5" met="3" type="-+">zyn gewoorden <rhyme label="c" type="m">vertsaecht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">aleer haer yemant <rhyme label="c" type="m" corresp="#s2:l5">Jaecht</rhyme></l>
        <l id="s2:l7" met="2" type="-+">gods stercke <rhyme label="d" type="m">hant</rhyme></l>
        <l id="s2:l8" met="3" type="-+">heeft haer gebracht tot <rhyme label="d" type="m" corresp="#s2:l7">Schant</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghelyck als Faro <rhyme label="a" type="m">Fel</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Int roo meer is <rhyme label="b" type="f">gebleven</rhyme></l>
        <l id="s3:l3" met="3" type="-+">en Senachrip <rhyme label="a" type="m" corresp="#s3:l1">snel</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Jerusalem heeft <rhyme label="b" type="f" corresp="#s3:l2">begeven</rhyme></l>
        <l id="s3:l5" met="3" type="-+">zoo is de Spaensche <rhyme label="c" type="m">hont</rhyme></l>
        <l id="s3:l6" met="3" type="-+">oock verdreven <rhyme label="c" type="m" corresp="#s3:l5">terstont</rhyme></l>
        <l id="s3:l7" met="2" type="-+">zijnen bloet <rhyme label="d" type="m">dorst</rhyme></l>
        <l id="s3:l8" met="3" type="-+">is met water <rhyme label="d" type="m" corresp="#s3:l7">geschorst</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den Prins die Leyden <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s4:l2" met="3" type="-+">verloste uijt haer <rhyme label="b" type="f">lijden</rhyme></l>
        <l id="s4:l3" met="3" type="-+">heeft bij Belgica <rhyme label="a" type="m" corresp="#s4:l1">schier</rhyme></l>
        <l id="s4:l4" met="3" type="-+">oock willen met <rhyme label="b" type="f" corresp="#s4:l2">bevryden</rhyme></l>
        <l id="s4:l5" met="3" type="-+">looft zingt met luijder <rhyme label="c" type="m">stem</rhyme></l>
        <l id="s4:l6" met="3" type="-+">danckt eert prijst alleen <rhyme label="c" type="m" corresp="#s4:l5">hem</rhyme></l>
        <l id="s4:l7" met="2" type="-+">en maeckt <rhyme label="d" type="m">bekent</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat lieff is <rhyme label="d" type="m" corresp="#s4:l7">t'Fondament</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
