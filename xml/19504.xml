        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>farewell song</tag>
      <tag>love song</tag>
    </tags>
    <melody>Ik voer al over zee</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAastmaal als Philis <rhyme label="a" type="m">sliep</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Laastmael als Philis sliep, ja <rhyme label="a" type="m" corresp="#s1:l1">sliep</rhyme></l>
        <l id="s1:l3" met="7" type="+-">Onder d'else boompjes quickme hier, quickme <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Al droomende sy <rhyme label="a" type="m" corresp="#s1:l1">riep</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Komt Titer koelt u <rhyme label="a" type="m">lust</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Komt Titer koelt u lust, ja <rhyme label="a" type="m" corresp="#s2:l1">lust</rhyme>,</l>
        <l id="s2:l3" met="7" type="+-">Met quam hy aengevloge, quickme hier, quickme <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En heeft haer mondt <rhyme label="a" type="m" corresp="#s2:l1">gekust</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy nam haer in sijn <rhyme label="a" type="m">arm</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hy nam haer in sijn arm, ja <rhyme label="a" type="m" corresp="#s3:l1">arm</rhyme>,</l>
        <l id="s3:l3" met="7" type="+-">Toen vlogen al sijn lusjens quickme hier, quickme <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Gelijck een <rhyme label="a" type="m" corresp="#s3:l1">Bye-swarm</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy soo door min <rhyme label="a" type="m">vervoert</rhyme>;</l>
        <l id="s4:l2" met="4" type="-+">Hy soo door min vervoert, ja <rhyme label="a" type="m" corresp="#s4:l1">voert</rhyme>,</l>
        <l id="s4:l3" met="7" type="+-">Heeft met sijn radde ving'ren, quickme hier quickme <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Haer borsjens <rhyme label="a" type="m" corresp="#s4:l1">aengeroert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Toen prickte hem de <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Toen prickte hem de min, ja <rhyme label="a" type="m" corresp="#s5:l1">min</rhyme></l>
        <l id="s5:l3" met="7" type="+-">Soo krachtig, hy sijn lusjens quickme hier, quickme <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Niet meer kost toomen <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dies quam hy 'er heel <rhyme label="a" type="m">na</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dies quam hy 'er heel na, ja <rhyme label="a" type="m" corresp="#s6:l1">na</rhyme>,</l>
        <l id="s6:l3" met="7" type="+-">Sy liet al droevend' slippen quickme hier quickme <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met suchjens 't woortje <rhyme label="a" type="m" corresp="#s6:l1">ja</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy sprack niet langer 'k <rhyme label="a" type="m">toef</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Hy sprack niet langer 'k toef, ja <rhyme label="a" type="m" corresp="#s7:l1">toef</rhyme>,</l>
        <l id="s7:l3" met="7" type="+-">Maer met gewenste lusjens quickme hier quickme <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Hy in haer schoot sich <rhyme label="a" type="m" corresp="#s7:l1">groef</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
