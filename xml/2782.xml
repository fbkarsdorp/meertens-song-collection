    <melody>Een meisje van hart fijn</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wie wil hoeren een nieu <rhyme label="a" type="f">liet</rhyme></l>
        <l id="s1:l2" met="4" type="-+">al van een meisie van herten <rhyme label="b" type="m">fyn</rhyme></l>
        <l id="s1:l3" met="4" type="-+">sy kon haer selven niet <rhyme label="a" type="f" corresp="#s1:l1">bedwingen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">en sy sprack tot haer vaeder <rhyme label="b" type="m" corresp="#s1:l2">fyn</rhyme></l>
        <l id="s1:l5" met="4" type="-+">en sel Ick altyt gaen met de <rhyme label="c" type="m">hul</rhyme></l>
        <l id="s1:l6" met="3" type="+-">wech wech ick wil <rhyme label="d" type="f">hylicken</rhyme></l>
        <l id="s1:l7" met="4" type="-+">geeft myn een man of ick wordt <rhyme label="c" type="m" corresp="#s1:l5">dul</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al die myn nu dat hylieke <rhyme label="a" type="f">laeke</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Wens ick allegaer den <rhyme label="b" type="m">droes</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Ick kan soe wel myn bedt op <rhyme label="a" type="f" corresp="#s2:l1">maeken</rhyme></l>
        <l id="s2:l4" met="4" type="-+">en koeken aer goedt <rhyme label="b" type="m" corresp="#s2:l2">waaremoes</rhyme></l>
        <l id="s2:l5" met="4" type="-+">en koeken schelleuis ende <rhyme label="c" type="m">gul</rhyme></l>
        <l id="s2:l6" met="3" type="+-">wech wech ick wil <rhyme label="d" type="f">hylicken</rhyme></l>
        <l id="s2:l7" met="4" type="-+">geeft myn een man of ick wordt <rhyme label="c" type="m" corresp="#s2:l5">dul</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy sal met myn niet sin <rhyme label="a" type="f">voorlegen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ick kan dat huis al syn <rhyme label="b" type="m">manier</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Wasse boene myn vloer aen <rhyme label="a" type="f" corresp="#s3:l1">veegen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">en boete daer oook een goedt <rhyme label="b" type="m" corresp="#s3:l2">vier</rhyme></l>
        <l id="s3:l5" met="4" type="-+">van turf van kluiten en oeck van <rhyme label="c" type="m">mul</rhyme></l>
        <l id="s3:l6" met="3" type="+-">wech wech ick wil <rhyme label="d" type="f">hylicken</rhyme></l>
        <l id="s3:l7" met="4" type="-+">geeft myn een man of ick wordt <rhyme label="c" type="m" corresp="#s3:l5">dul</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Deese Prinsesse woent buiten <rhyme label="a" type="f">leyden</rhyme></l>
        <l id="s4:l2" met="4" type="-+">en haer naem Is geheeten <rhyme label="b" type="m">mat</rhyme></l>
        <l id="s4:l3" met="4" type="-+">hoe sy huilde en hoe sy <rhyme label="a" type="f" corresp="#s4:l1">schreide</rhyme></l>
        <l id="s4:l4" met="4" type="-+">sy kon krygen geene <rhyme label="b" type="m" corresp="#s4:l2">man</rhyme></l>
        <l id="s4:l5" met="4" type="-+">en wat sy badt en sy kreech <rhyme label="c" type="m">nul</rhyme></l>
        <l id="s4:l6" met="3" type="+-">wech wech ick wil <rhyme label="d" type="f">hylicken</rhyme></l>
        <l id="s4:l7" met="4" type="-+">geeft myn een man of ick wordt <rhyme label="c" type="m" corresp="#s4:l5">dul</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
