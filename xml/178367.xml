<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178367</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Ha ha zie zo</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Vivat deeze agtste <rhyme label="a" type="m">Maart</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Dat Willem weer <rhyme label="a" type="m" corresp="#s1:l1">verjaardt</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Dien braaven Prins, wiens taay <rhyme label="b" type="m">geduld</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Der braaven Burg'ren hart <rhyme label="b" type="m" corresp="#s1:l3">vervuld</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Met vreugd, met <rhyme label="c" type="m">vreugd</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">Dat Voorbeeld van de <rhyme label="c" type="m" corresp="#s1:l5">Deugd</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Laat vry het snoode <rhyme label="a" type="m">Rot</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Dat Vaderland en <rhyme label="a" type="m" corresp="#s2:l1">God</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En alles wat naar Waarheid <rhyme label="b" type="m">Zweeft</rhyme></l>
        <l id="s2:l4" met="4" type="-+">De laatste trap op 't harte <rhyme label="b" type="m" corresp="#s2:l3">geeft</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Maar woen, maar <rhyme label="c" type="m">woen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">God zal den Vorst <rhyme label="c" type="m" corresp="#s2:l5">behoen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Geen saemgeruiden <rhyme label="a" type="m">hoop</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van Roovers stuit den <rhyme label="a" type="m" corresp="#s3:l1">loop</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Die Willems Glorie hebben <rhyme label="b" type="m">zal</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Geen Vrycorps stelt Oranje <rhyme label="b" type="m" corresp="#s3:l3">pal</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Zyn Naam, zyn <rhyme label="c" type="m">naam</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Herleeve op Neerlands <rhyme label="c" type="m" corresp="#s3:l5">Faam</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy die de Prins <rhyme label="a" type="m">verdoemt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En Landverrader <rhyme label="a" type="m" corresp="#s4:l1">noemdt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Of voor een dwingeland <rhyme label="b" type="m">erkendt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die Neerlands oude Vryheid <rhyme label="b" type="m" corresp="#s4:l3">schendt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Die zal, die <rhyme label="c" type="m">zal</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Haast nad'ren tot zyn <rhyme label="c" type="m" corresp="#s4:l5">val</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Geen Eerlyk <rhyme label="a" type="m">Batavier</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Stoor zig aan 't <rhyme label="a" type="m" corresp="#s5:l1">Monsterdier</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dat Monster dat 's Lands Eendragt <rhyme label="b" type="m">rooft</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En aan de Hel zyn Ziel <rhyme label="b" type="m" corresp="#s5:l3">belooft</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Brengt hy, brengt <rhyme label="c" type="m">hy</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ons eens in <rhyme label="c" type="m" corresp="#s5:l5">slaaverny</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ons Leeven, Goed en <rhyme label="a" type="m">bloed</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Staan vyl, in dezen <rhyme label="a" type="m" corresp="#s6:l1">gloed</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Van Stookebranden, die door <rhyme label="b" type="m">schyn</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Van voor het Vaderland te <rhyme label="b" type="m" corresp="#s6:l3">zyn</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Niet doen, niet <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dan op de Vryheid <rhyme label="c" type="m" corresp="#s6:l5">woÃªn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Zo lang Oranje <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En 't roer der Staaten <rhyme label="a" type="m" corresp="#s7:l1">heeft</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Zo lang Vorst Willem leeven <rhyme label="b" type="m">mag</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Gedenken wy aan deezen <rhyme label="b" type="m" corresp="#s7:l3">Dag</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Tot spyt, tot <rhyme label="c" type="m">spyt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Van hem die ons <rhyme label="c" type="m" corresp="#s7:l5">benydt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Zo lang aan 't <rhyme label="a" type="m">Firmament</rhyme></l>
        <l id="s8:l2" met="3" type="-+">De dagtoors is <rhyme label="a" type="m" corresp="#s8:l1">bekendt</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Zo lang dees aarde, de aarde <rhyme label="b" type="m">zy</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Al kwam heel Utrechts <rhyme label="b" type="m" corresp="#s8:l3">Burgerey</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Geen nood, geen <rhyme label="c" type="m">nood</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Wy vreezen Hel nog <rhyme label="c" type="m" corresp="#s8:l5">Dood</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dit Leyden, dat <rhyme label="a" type="m">weleer</rhyme>.</l>
        <l id="s9:l2" met="3" type="-+">De Spanjaard sloeg ter <rhyme label="a" type="m" corresp="#s9:l1">neer</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Zal nimmer dulden dat den <rhyme label="b" type="m">Vorst</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Die zo getrouw 's Lands zaaken <rhyme label="b" type="m" corresp="#s9:l3">torst</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Door haat. door <rhyme label="c" type="m">haat</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Geschopt word uit den <rhyme label="c" type="m" corresp="#s9:l5">raadt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Gryp. dappere Prins! Gryp <rhyme label="a" type="m">moed</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Vreest geen Armyns <rhyme label="a" type="m" corresp="#s10:l1">gebroed</rhyme>;</l>
        <l id="s10:l3" met="4" type="-+">Vreest geen Tirannen van het <rhyme label="b" type="m">land</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Zo lang hier Helden moet <rhyme label="b" type="m" corresp="#s10:l3">ontbrandt</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Zyt gy, zyt <rhyme label="c" type="m">gy</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Voor zulke snoodaarts <rhyme label="c" type="m" corresp="#s10:l5">vry</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
