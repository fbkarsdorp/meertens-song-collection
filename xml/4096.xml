        <l id="s3:l3" met="4" type="+-">Christum regem <rhyme label="a" type="m" corresp="#s3:l1">dominum</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Sine <rhyme label="b" type="f">viro</rhyme></l>
        <l id="s3:l5" met="5" type="+-">Ut vellus sub rore / modo <rhyme label="b" type="f" corresp="#s3:l4">miro</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Virgo mater <rhyme label="a" type="m">nescia</rhyme></l>
