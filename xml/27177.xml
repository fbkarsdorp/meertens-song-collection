  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O nabeur <rhyme label="a" type="m">robrecht</rhyme></l>
        <l id="s1:l2" met="3" type="-+">myn hert is vol van <rhyme label="b" type="m">pyn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">O nabeur <rhyme label="c" type="m">roelant</rhyme></l>
        <l id="s1:l4" met="3" type="-+">waerom mach dat toch <rhyme label="b" type="m" corresp="#s1:l2">syn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Jan koster lieft <rhyme label="d" type="m">margeriette</rhyme></l>
        <l id="s1:l6" met="3" type="-+">ende dat is my een <rhyme label="e" type="m">smert</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Weest te vreden goede <rhyme label="f" type="m">roulant</rhyme></l>
        <l id="s1:l8" met="3" type="-+">het is toch maer int <rhyme label="e" type="m" corresp="#s1:l6">scherts</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sy is met hem opt <rhyme label="a" type="m">kerchoff</rhyme></l>
        <l id="s2:l2" met="3" type="-+">o wee ende wat schaet <rhyme label="b" type="m">dat</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Sy vryen daer ick vruchte <rhyme label="c" type="m">my</rhyme></l>
        <l id="s2:l4" met="3" type="-+">sy doen ick weet niet <rhyme label="b" type="m" corresp="#s2:l2">wat</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Weest te vreden goede <rhyme label="d" type="m">Roulant</rhyme></l>
        <l id="s2:l6" met="3" type="-+">en volcht toch myn <rhyme label="e" type="m">gebodt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Siet hoe sy tsaemen <rhyme label="f" type="m">lopen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">nu is myn hert schier <rhyme label="e" type="m" corresp="#s2:l6">doot</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Licht neder en versteeckt v <rhyme label="a" type="m">doch</rhyme></l>
        <l id="s3:l2" met="3" type="-+">en hoort toch wat sy <rhyme label="b" type="m">sagen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Neen neen maer ick wil <rhyme label="c" type="m">opstaen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">en wil den koster wech <rhyme label="b" type="m" corresp="#s3:l2">jaghen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Soo verlaet ick v goede <rhyme label="d" type="m">roulant</rhyme></l>
        <l id="s3:l6" met="3" type="-+">o robrecht hoe syt ghy <rhyme label="e" type="m">gesint</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Myn cloeckheyt sal v <rhyme label="f" type="m">helpen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">ick slae den koster <rhyme label="e" type="m" corresp="#s3:l6">blint</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat faelt v liefste <rhyme label="a" type="m">margeriette</rhyme></l>
        <l id="s4:l2" met="3" type="-+">dat ghy my so vremt <rhyme label="b" type="m">aensiet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ick geloue ghy my niet lieff en <rhyme label="c" type="m">hebt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">vaet toch wel myn <rhyme label="b" type="m" corresp="#s4:l2">bediet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hebt ghy verlaeten <rhyme label="d" type="m">roulant</rhyme></l>
        <l id="s4:l6" met="3" type="-+">dat is al langh <rhyme label="e" type="m">geschiet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Soo wil ick v dan <rhyme label="f" type="m">trouwen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">twaer beter dat ghyt <rhyme label="e" type="m" corresp="#s4:l6">liet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Godt groet v lieue <rhyme label="a" type="m">margeriette</rhyme></l>
        <l id="s5:l2" met="3" type="-+">ick brenghe v boses <rhyme label="b" type="m">nys</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Wat is dat goede <rhyme label="c" type="m">roberecht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">maeckt ghy my niet wat <rhyme label="b" type="m" corresp="#s5:l2">wys</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Vwen Roulant is <rhyme label="d" type="m">gestoruen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">o robrecht wie heeftet <rhyme label="e" type="m">gedaen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Om dat ghy lieft een <rhyme label="f" type="m">koster</rhyme></l>
        <l id="s5:l8" met="3" type="-+">o laet hem toch wech <rhyme label="e" type="m" corresp="#s5:l6">ghaen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Siet doch hoe hy daer <rhyme label="a" type="m">ligget</rhyme></l>
        <l id="s6:l2" met="3" type="-+">die v heeft wt ver <rhyme label="b" type="m">weelt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Vergeuet my lieue <rhyme label="c" type="m">roulant</rhyme></l>
        <l id="s6:l4" met="3" type="-+">myn smert my toch <rhyme label="b" type="m" corresp="#s6:l2">verveelt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">O robrecht het gerouwet <rhyme label="d" type="m">my</rhyme></l>
        <l id="s6:l6" met="3" type="-+">het is met v maer <rhyme label="e" type="m">spodt</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Compt tot my en lieuet <rhyme label="f" type="m">my</rhyme></l>
        <l id="s6:l8" met="3" type="-+">ick slae ju koster <rhyme label="e" type="m" corresp="#s6:l6">doot</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O onbarmhertige <rhyme label="a" type="m">margeriette</rhyme></l>
        <l id="s7:l2" met="3" type="-+">hebt ghy hem bracht omt <rhyme label="b" type="m">leuen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Soete liefste verlaet toch <rhyme label="c" type="m">hem</rhyme></l>
        <l id="s7:l4" met="3" type="-+">ick sal v slaeghen <rhyme label="b" type="m" corresp="#s7:l2">geuen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Om niet is al v <rhyme label="d" type="m">treuren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">compt margeriette gaet met <rhyme label="e" type="m">my</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Treuren mach roulant niet <rhyme label="f" type="m">helpen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">compt koster ick gae met <rhyme label="e" type="m" corresp="#s7:l6">dy</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Myn hoocheyt sal van daeghe <rhyme label="a" type="m">syn</rhyme></l>
        <l id="s8:l2" met="3" type="-+">v wille hebt ghy aen <rhyme label="b" type="m">my</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Maer hoort toch nabeur <rhyme label="c" type="m">koster</rhyme></l>
        <l id="s8:l4" met="3" type="-+">een woort heb ick aen <rhyme label="b" type="m" corresp="#s8:l2">dy</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Leeft noch myn lieue <rhyme label="d" type="m">roelant</rhyme></l>
        <l id="s8:l6" met="3" type="-+">dat is my grote <rhyme label="e" type="m">pyn</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Gaet ter kercken en luyt die <rhyme label="f" type="m">klocken</rhyme></l>
        <l id="s8:l8" met="3" type="-+">nu sal myn hoocheyt <rhyme label="e" type="m" corresp="#s8:l6">syn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick begere geen ander als <rhyme label="a" type="m">roulant</rhyme></l>
        <l id="s9:l2" met="3" type="-+">ghy koster gaet toch <rhyme label="b" type="m">wech</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Wilt margeriette my <rhyme label="c" type="m">verlaeten</rhyme></l>
        <l id="s9:l4" met="3" type="-+">ghy syt een arghe <rhyme label="b" type="m" corresp="#s9:l2">gheck</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Lichtveerdige jonckvrouwen en trou ick <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s9:l6" met="3" type="-+">gaet maeckt ghy koster een <rhyme label="e" type="m">graff</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Nu is margeriette <rhyme label="f" type="m">roelants</rhyme></l>
        <l id="s9:l8" met="3" type="-+">soo syt ghy nu daer <rhyme label="e" type="m" corresp="#s9:l6">aff</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
