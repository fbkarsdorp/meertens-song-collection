    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Een Venus' dier houdt mij in het bestier</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een dier een aerdich <rhyme label="a" type="m">dier</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hout my in haer <rhyme label="a" type="m" corresp="#s1:l1">bestier</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Alzo vaste <rhyme label="b" type="f">bedwonghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Vrau venus <rhyme label="a" type="m" corresp="#s1:l1">fier</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Doet my zo menghen <rhyme label="c" type="m">gru</rhyme></l>
        <l id="s1:l6" met="3" type="-+">zoe valt my <rhyme label="a" type="m" corresp="#s1:l1">putertier</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Te my waert also <rhyme label="c" type="m" corresp="#s1:l5">schu</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Wie zal ict claghen <rhyme label="c" type="m" corresp="#s1:l5">nv</rhyme></l>
        <l id="s1:l9" met="3" type="-+">want trueren valt my zo <rhyme label="c" type="m" corresp="#s1:l5">schu</rhyme></l>
        <l id="s1:l10" met="3" type="-+">van myne lieue <rhyme label="b" type="f" corresp="#s1:l3">ghezonghen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haer wesen <rhyme label="a" type="m">eerbaer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">So vriendelic <rhyme label="a" type="m" corresp="#s2:l1">eenpaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Zonder <rhyme label="b" type="f">cesseren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Haer bruyn oochsken <rhyme label="a" type="m" corresp="#s2:l1">claer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">verhueghen mynen zin en <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Al myn lyden <rhyme label="a" type="m" corresp="#s2:l1">zwaer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dat is by haer <rhyme label="c" type="m" corresp="#s2:l5">gheschent</rhyme></l>
        <l id="s2:l8" met="3" type="-+">[..] haeren vpsschen <rhyme label="c" type="m" corresp="#s2:l5">mont</rhyme></l>
        <l id="s2:l9" met="3" type="-+">doet in vruechden zoet  [..] therte <rhyme label="c" type="m" corresp="#s2:l5">bloet</rhyme></l>
        <l id="s2:l10" met="3" type="-+">In vruechden <rhyme label="b" type="f" corresp="#s2:l3">verjubileren</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">So wye dat spyt of <rhyme label="a" type="m">deert</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Schoon lief bem ic v <rhyme label="a" type="m" corresp="#s3:l1">weert</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Zyt my goet ende <rhyme label="b" type="f">ghetrauwe</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Bem ict v <rhyme label="a" type="m" corresp="#s3:l1">weert</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Myn liefste mondeken <rhyme label="c" type="m">roodt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Troost my nv tes <rhyme label="a" type="m" corresp="#s3:l1">noodt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die my therte <rhyme label="c" type="m" corresp="#s3:l5">duerschoot</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Ende dat zegghic v <rhyme label="c" type="m" corresp="#s3:l5">bloot</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Schoon lief bem icx v <rhyme label="c" type="m" corresp="#s3:l5">weert</rhyme></l>
        <l id="s3:l10" met="3" type="-+">zyt my goed ende <rhyme label="b" type="f" corresp="#s3:l3">ghetrauwe</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een dier een princelic <rhyme label="a" type="m">dier</rhyme></l>
        <l id="s4:l2" met="3" type="-+">hout my in haer <rhyme label="a" type="m" corresp="#s4:l1">bestier</rhyme></l>
        <l id="s4:l3" met="3" type="-+">zo vast in vrau venus <rhyme label="b" type="f">quale</rhyme></l>
        <l id="s4:l4" met="3" type="-+">mochten wy <rhyme label="a" type="m" corresp="#s4:l1">twee</rhyme></l>
        <l id="s4:l5" met="3" type="-+">tsamen ghenougen <rhyme label="c" type="m">zyn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">deze nyders schoon <rhyme label="a" type="m" corresp="#s4:l1">aenschyn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Quae tonghen zyn valsch <rhyme label="c" type="m" corresp="#s4:l5">fenyn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ic claecht de liefste <rhyme label="c" type="m" corresp="#s4:l5">myn</rhyme></l>
        <l id="s4:l9" met="3" type="-+">In droufheyt ende <rhyme label="c" type="m" corresp="#s4:l5">pyn</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Moet ligghen vast in Venus <rhyme label="b" type="f" corresp="#s4:l3">quale</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
