<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>12739</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pronkje van de maagden</melody>
  <author>C. Rhijnenburgh</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Ruyter gaet ghy <rhyme label="a" type="f">henen</rhyme>?</l>
        <l id="s1:l2" met="3" type="+-">Godt wil u <rhyme label="b" type="f">geleyden</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">En gheluck <rhyme label="a" type="f" corresp="#s1:l1">verleenen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Op Neltunus <rhyme label="b" type="f" corresp="#s1:l2">weyden</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">‘k Sal u we’er <rhyme label="b" type="f" corresp="#s1:l2">verbeyden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Behouwen hier te <rhyme label="c" type="f">Ree</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Vaert spoedigh // Zijt <rhyme label="d" type="f">moedigh</rhyme></l>
        <l id="s1:l8" met="2" type="-+">In ’t draven // Brenght <rhyme label="e" type="f">Slaven</rhyme></l>
        <l id="s1:l9" met="2" type="+-">En Prijsen <rhyme label="c" type="f" corresp="#s1:l6">mee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Laet u witte <rhyme label="a" type="f">Zeylen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Na beneden <rhyme label="b" type="f">vallen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Om Neptuyn te <rhyme label="a" type="f" corresp="#s2:l1">peylen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Langhs de Spaense <rhyme label="b" type="f" corresp="#s2:l2">wallen</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">En ’t Drie-manigh <rhyme label="b" type="f" corresp="#s2:l2">brallen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Te dempen na u <rhyme label="c" type="f">best</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">U kielen // <rhyme label="d" type="f">Vernielen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">De Roovers En <rhyme label="e" type="f">Stroovers</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Van dat <rhyme label="c" type="f" corresp="#s2:l6">gewest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Turcken ’t sal u <rhyme label="a" type="f">gelden</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Hy wil ruymte <rhyme label="b" type="f">maken</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Dat u bracke <rhyme label="a" type="f" corresp="#s3:l1">Velden</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Sullen staen te <rhyme label="b" type="f" corresp="#s3:l2">blaken</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">En u boorden <rhyme label="b" type="f" corresp="#s3:l2">kraken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Als hy lest heeft <rhyme label="c" type="f">gedaen</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">De Guyters Mijns <rhyme label="d" type="f">Ruyters</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Sijn Trompen Die <rhyme label="e" type="f">dompen</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Recht op u <rhyme label="c" type="f" corresp="#s3:l6">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Met Metale <rhyme label="a" type="f">Vogels</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Sal hy u <rhyme label="b" type="f">ontmoeten</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En met sware <rhyme label="a" type="f" corresp="#s4:l1">Kogels</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Kneppels en <rhyme label="b" type="f" corresp="#s4:l2">Koe-voeten</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">U soo wel <rhyme label="b" type="f" corresp="#s4:l2">begroeten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat als u moedt en <rhyme label="c" type="f">lust</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Sal sacken // <rhyme label="d" type="f">Verswacken</rhyme></l>
        <l id="s4:l8" met="2" type="-+">U Schepen // Hy <rhyme label="e" type="f">slepen</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Sal na mijn <rhyme label="c" type="f" corresp="#s4:l6">kust</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Hemel-Heer en <rhyme label="a" type="f">Koningh</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Siet doch op hem <rhyme label="b" type="f">neder</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Uyt u hooge <rhyme label="a" type="f" corresp="#s5:l1">Wooningh</rhyme>:</l>
        <l id="s5:l4" met="3" type="+-">Laet mijn Ruyter <rhyme label="b" type="f" corresp="#s5:l2">weder</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Keeren, rijck en <rhyme label="b" type="f" corresp="#s5:l2">breeder</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met menigh Christen <rhyme label="c" type="f">Mensch</rhyme>;</l>
        <l id="s5:l7" met="2" type="-+">Sijn Degen // Heb <rhyme label="d" type="f">Zegen</rhyme>;</l>
        <l id="s5:l8" met="2" type="-+">Sijn Stucken // <rhyme label="e" type="f">Gelucken</rhyme></l>
        <l id="s5:l9" met="2" type="+-">Hem na sijn <rhyme label="c" type="f" corresp="#s5:l6">wensch</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
