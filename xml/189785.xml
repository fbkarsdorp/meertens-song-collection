  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Lief bemind ter goeder <rhyme label="a" type="m">uur</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ha minnegoodjes ha wat <rhyme label="a" type="m" corresp="#s1:l1">vuur</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sluypt in myn <rhyme label="b" type="m">ingewand</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wen ik haar poesle <rhyme label="b" type="m" corresp="#s1:l3">hand</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Tot dankerkentenisse <rhyme label="c" type="m">druk</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Ã´ Gadeloos <rhyme label="c" type="m" corresp="#s1:l5">geluk</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zy is geen vyandinne <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Der zuivre min, gelyk <rhyme label="a" type="m" corresp="#s2:l1">weleer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Neen, neen haar killig <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Is in de min <rhyme label="b" type="m" corresp="#s2:l3">verwart</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Zy noemt my altoos <rhyme label="c" type="m">wellekom</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Haar schat haar <rhyme label="c" type="m" corresp="#s2:l5">Bruidegom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Myn zielsvoogdes myn <rhyme label="a" type="m">hartvriendin</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ontsluyt voor my de deur der <rhyme label="a" type="m" corresp="#s3:l1">min</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En gun my het <rhyme label="b" type="m">genot</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van 't allerzaaligst <rhyme label="b" type="m" corresp="#s3:l3">lot</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat ooit een sterveling <rhyme label="c" type="m">ervaart</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Op 't groote vlak der <rhyme label="c" type="m" corresp="#s3:l5">aart</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
