<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>111811</id>
    <date>
      <year-start>1745</year-start>
      <year-end>1745</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Wat is er groter vreugde op aarde</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs ik u kuische <rhyme label="a" type="m">Reen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s1:l1">Carileen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Laet in myn gedachten <rhyme label="b" type="f">koomen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dan zoo pord de hoop my <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Om naer u myn Lief te <rhyme label="c" type="m" corresp="#s1:l4">gaen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Maer u koelheyt doet my <rhyme label="b" type="f" corresp="#s1:l3">schroomen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ach bedroefde uur en <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Wat een <rhyme label="a" type="m" corresp="#s2:l1">spyt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Deet gy my lest aen door 't <rhyme label="b" type="f">scheyden</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Waerom buyten Mins <rhyme label="c" type="m">bestek</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dus alleenig op 't <rhyme label="c" type="m" corresp="#s2:l4">vertrek</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Was 't niet beter met ons <rhyme label="b" type="f" corresp="#s2:l3">beyden</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Daer ik met myn hart en <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">U <rhyme label="a" type="m" corresp="#s3:l1">bemin</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En beminnen zal als <rhyme label="b" type="f">eygen</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Hoop en Vreeze voeren <rhyme label="c" type="m">stryt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Tot de aengenaeme <rhyme label="c" type="m" corresp="#s3:l4">tyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Eens u gunst tot my zal <rhyme label="b" type="f" corresp="#s3:l3">neygen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als ik s'nagts door slaepen <rhyme label="a" type="m">koom</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">In een <rhyme label="a" type="m" corresp="#s4:l1">droom</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Uw zoet Weezen my <rhyme label="b" type="f">verbeelden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Schyn ik met de soetste <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">In zoo aengenaeme <rhyme label="c" type="m" corresp="#s4:l4">Rust</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Swemmend in een Zee van <rhyme label="b" type="f" corresp="#s4:l3">Weelden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Roep u Naem met schel <rhyme label="a" type="m">geluyt</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Droomend <rhyme label="a" type="m" corresp="#s5:l1">uyt</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">d'Echo schynd my nae te <rhyme label="b" type="f">roepen</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Mogt ik van die lieve <rhyme label="c" type="m">mond</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Die myn hardt zoo dikwils <rhyme label="c" type="m" corresp="#s5:l4">wond</rhyme>;</l>
        <l id="s5:l6" met="4" type="+-">Steelswys nu een kusje <rhyme label="b" type="f" corresp="#s5:l3">snoepen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Zie uw deugd en schoone <rhyme label="a" type="m">leen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s6:l1">Carileen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">'k Wil hierom standvastig <rhyme label="b" type="f">blyven</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">En storten myn liefde in uw <rhyme label="c" type="m">schoot</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Al is wangunst nog zoo <rhyme label="c" type="m" corresp="#s6:l4">groot</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Niemand zal my van u <rhyme label="b" type="f" corresp="#s6:l3">dryven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
