  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">BLaas al de kaarzen <rhyme label="a" type="m">uit</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En speel hier op de <rhyme label="a" type="m" corresp="#s1:l1">fluit</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">â Super <rhyme label="b" type="m">nagulum</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Nam est is <rhyme label="b" type="m" corresp="#s1:l3">wellekom</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Bene <rhyme label="c" type="f">bitatis</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Et <rhyme label="c" type="f" corresp="#s1:l5">commedatis</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Op tat, Op <rhyme label="d" type="m">tat</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Fota <rhyme label="e" type="f">Compania</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nous cellebron <rhyme label="a" type="m">ici</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Festum <rhyme label="a" type="m" corresp="#s2:l1">palatici</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Vidons, Vidons le <rhyme label="b" type="m">pos</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In dulci <rhyme label="b" type="m" corresp="#s2:l3">jubilo</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Bene <rhyme label="c" type="f">bitatis</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Et <rhyme label="c" type="f" corresp="#s2:l5">Commedatis</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Op tat, Op <rhyme label="d" type="m">tat</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Fota <rhyme label="e" type="f">Compania</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Io <rhyme label="a" type="m">Caliopé</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Venus nous <rhyme label="a" type="m" corresp="#s3:l1">assiste</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">De danse bon <rhyme label="b" type="m">banqué</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Spring op van vreugde <rhyme label="b" type="m" corresp="#s3:l3">meé</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Bene <rhyme label="c" type="f">bitatis</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Et <rhyme label="c" type="f" corresp="#s3:l5">Commedatis</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Op tat, Op <rhyme label="d" type="m">tat</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Fota <rhyme label="e" type="f">Compania</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'t Is Exelente <rhyme label="a" type="m">wyn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Laat ons dog vrolyk <rhyme label="a" type="m" corresp="#s4:l1">zyn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Buvon á la <rhyme label="b" type="m">rondom</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Laudatis <rhyme label="b" type="m" corresp="#s4:l3">bromium</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Bene <rhyme label="c" type="f">bitatis</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Et <rhyme label="c" type="f" corresp="#s4:l5">Commedatis</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Op tat, Op <rhyme label="d" type="m">tat</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Fota <rhyme label="e" type="f">Compania</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ne pargne pas le <rhyme label="a" type="m">vin</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Monsieur jusque á de <rhyme label="a" type="m" corresp="#s5:l1">main</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Den hospes is zeer <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die 't al betalen <rhyme label="b" type="m" corresp="#s5:l3">moet</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Bene <rhyme label="c" type="f">bitatis</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Et <rhyme label="c" type="f" corresp="#s5:l5">Commedatis</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Op tat, Op <rhyme label="d" type="m">tat</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Fota <rhyme label="e" type="f">Compania</rhyme>.</l>
      </lg>
    </lg>
  </text>
