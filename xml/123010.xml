  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zoet vermaak van onze <rhyme label="a" type="m">jeugd</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Duur ei duur zo lang wy <rhyme label="b" type="f">leeven</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Liefde wil ons nooit <rhyme label="b" type="f" corresp="#s1:l2">begeeven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Maar vermeerder on-ze <rhyme label="a" type="m" corresp="#s1:l1">vreugd</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Bachchus mag de min <rhyme label="c" type="f">verzaaken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Liefde alleen kan ons <rhyme label="c" type="f" corresp="#s1:l5">vermaaken</rhyme>.</l>
      </lg>
    </lg>
