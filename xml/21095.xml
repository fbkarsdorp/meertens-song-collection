<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21095</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Questa dolce sirena</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEyl, welvaert en <rhyme label="a" type="m">geluck</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wenschen wy dat door <rhyme label="a" type="m" corresp="#s1:l1">druck</rhyme></l>
        <l id="s1:l3" met="4" type="+-">'t Jonge paer, met <rhyme label="b" type="m">malkaer</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Mach <rhyme label="c" type="f">genieten</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hier tot een <rhyme label="a" type="m" corresp="#s1:l1">Bruylofs-stuck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat is nu op der <rhyme label="a" type="m">aerdt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Het aldermeeste <rhyme label="a" type="m" corresp="#s2:l1">waerdt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als een mensch, heel sijn <rhyme label="b" type="m">wensch</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Soud <rhyme label="c" type="f">verkrijgen</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Ey, ons dit eens <rhyme label="a" type="m" corresp="#s2:l1">verklaert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ist Rijckdom, gelt of <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat ons hier luckighst <rhyme label="a" type="m" corresp="#s3:l1">doet</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Heel verheught, met <rhyme label="b" type="m">geneught</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Stadigh <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En vrolijck van <rhyme label="a" type="m" corresp="#s3:l1">gemoet</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Of doet hoogheyt en <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hier toe de meeste <rhyme label="a" type="m" corresp="#s4:l1">baet</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Om in lust, vreught en <rhyme label="b" type="m">rust</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Hier te <rhyme label="c" type="f">blijven</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Heel vry van alle <rhyme label="a" type="m" corresp="#s4:l1">quaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Neen, neen, dat ist noch <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat droefheyt en <rhyme label="a" type="m" corresp="#s5:l1">verdriet</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Heel en vert, uyt ons <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s5:l4" met="2" type="+-">Kan <rhyme label="c" type="f">verjagen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Gelijck men dickwilt <rhyme label="a" type="m" corresp="#s5:l1">siet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat heeft dan d'hooghste <rhyme label="a" type="m">stee</rhyme>?</l>
        <l id="s6:l2" met="3" type="-+">Maer liefd', eendracht, en <rhyme label="a" type="m" corresp="#s6:l1">vree</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Want het is, waer en <rhyme label="b" type="m">wis</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Datse <rhyme label="c" type="f">brengen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Gewenste leven <rhyme label="a" type="m" corresp="#s6:l1">mee</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Genooden, wie ghy <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wenscht met my dat <rhyme label="a" type="m" corresp="#s7:l1">altijdt</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Liefdens kracht, met <rhyme label="b" type="m">eendracht</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">Ende <rhyme label="c" type="f">vreede</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dees liefjens maeckt <rhyme label="a" type="m" corresp="#s7:l1">verblijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
