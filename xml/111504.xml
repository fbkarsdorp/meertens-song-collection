    <melody>J'avais jur√© que l'amour</melody>
    <categories>
      <category translation="children">kinderen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'k SWoer dat nooit Cupido's <rhyme label="a" type="m">Magt</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Op myn Hert iets sou <rhyme label="b" type="f">vermogen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ondertusschen Dag en <rhyme label="a" type="m" corresp="#s1:l1">Nagt</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Kwyn ik om Climene's <rhyme label="b" type="f" corresp="#s1:l2">Oogen</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Wyl de tyd my merken <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat met vroeg of laat moet <rhyme label="d" type="f">minnen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Wyl de tyd my merken <rhyme label="c" type="m" corresp="#s1:l5">doet</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Dat men niets versweeren <rhyme label="c" type="m" corresp="#s1:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Trots op <rhyme label="a" type="m">onverschilligheid</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Dorst ik d'Alderschoonste <rhyme label="b" type="f">tarten</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En ik maakte <rhyme label="a" type="m" corresp="#s2:l1">Vaniteit</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Vry te zyn van <rhyme label="b" type="f" corresp="#s2:l2">minnesmarten</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Maar de tyd my merken <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat men vroeg of laat moet <rhyme label="d" type="f">minnen</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Maar de tyd my merken <rhyme label="c" type="m" corresp="#s2:l5">doet</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Dat men niets versweeren <rhyme label="c" type="m" corresp="#s2:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Meende 't was een <rhyme label="a" type="m">dolligheid</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dat ik thans noem ware <rhyme label="b" type="f">liefde</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En ik schatte 't <rhyme label="a" type="m" corresp="#s3:l1">malligheid</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dat een Schoone my <rhyme label="b" type="f" corresp="#s3:l2">doorgriefde</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Maar de tyd my merken <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat men vroeg of laat moet <rhyme label="d" type="f">minnen</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Maar de tyd my merken <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Dat men niets versweeren <rhyme label="c" type="m" corresp="#s3:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Van 't begin dat ik <rhyme label="a" type="m">aansag</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Myn Climene's Schoone <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Bukte ik voor haar <rhyme label="a" type="m" corresp="#s4:l1">gesag</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En ik erkende haar <rhyme label="b" type="f" corresp="#s4:l2">vermogen</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Want de tyd my merken <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat men vroeg of laat moet <rhyme label="d" type="f">minnen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Want de tyd my merken <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Dat men niets versweeren <rhyme label="c" type="m" corresp="#s4:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Wondert my niet dat <rhyme label="a" type="m">Climeen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">My in Liefde kan <rhyme label="b" type="f">ontsteken</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En door haar <rhyme label="a" type="m" corresp="#s5:l1">Bevallighe'en</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Myn verstaalde Hert <rhyme label="b" type="f" corresp="#s5:l2">verbreken</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Wyl de tyd my merke <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dat men vroeg of laat moet <rhyme label="d" type="f">minnen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Wyl de tyd my merken <rhyme label="c" type="m" corresp="#s5:l5">doet</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Dat men niets versweeren <rhyme label="c" type="m" corresp="#s5:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Schoonste die myn Ziel <rhyme label="a" type="m">doorwond</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Door uw zoete <rhyme label="b" type="f">minneligten</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Reyk my eens uw lieven <rhyme label="a" type="m" corresp="#s6:l1">mond</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">Uwe Min kan my <rhyme label="b" type="f" corresp="#s6:l2">verpligten</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Wyl de tyd my merken <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat men vroeg of laat moet <rhyme label="d" type="f">minnen</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Wyl de tyd my merken <rhyme label="c" type="m" corresp="#s6:l5">doet</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Dat men niets versweeren <rhyme label="c" type="m" corresp="#s6:l5">moet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
