    <melody>Écoutez bergers la musique angélique</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Verliefde <rhyme label="a" type="m">Maagd</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Meend gy met <rhyme label="b" type="f">lonken</rhyme></l>
        <l id="s1:l3" met="1" type="-+">t’ <rhyme label="b" type="f" corresp="#s1:l2">Ontfonken</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mijn hert wel eer <rhyme label="a" type="m" corresp="#s1:l1">belaagd</rhyme>?</l>
        <l id="s1:l5" met="2" type="-+">Neen, ‘k stel de <rhyme label="c" type="m">min</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Gantsch uit mijn <rhyme label="c" type="m" corresp="#s1:l5">zin</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">‘k Wil altoos met mijn vryheid <rhyme label="b" type="f" corresp="#s1:l2">pronken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Weg malle <rhyme label="a" type="m">prey</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Met al uw <rhyme label="b" type="f">snappen</rhyme></l>
        <l id="s2:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s2:l2">klappen</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Ik laat u in de <rhyme label="a" type="m" corresp="#s2:l1">ley</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Schoon gy mijn <rhyme label="c" type="m">ziel</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Wel eer <rhyme label="c" type="m" corresp="#s2:l5">geviel</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Zult gy my nu nooit weer <rhyme label="b" type="f" corresp="#s2:l2">betrappen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dat komt, <rhyme label="a" type="m">zottin</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Van ’t naa te <rhyme label="b" type="f">loopen</rhyme></l>
        <l id="s3:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s3:l2">hoopen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Op uwe trouwe <rhyme label="a" type="m" corresp="#s3:l1">Min</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Neen, ‘k lag u <rhyme label="c" type="m">uit</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Gewaande <rhyme label="c" type="m" corresp="#s3:l5">Bruit</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">‘k Sal voor mijn geld wel schoonder <rhyme label="b" type="f" corresp="#s3:l2">koopen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Het blinkent <rhyme label="a" type="m">Gout</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Heeft groot <rhyme label="b" type="f">vermoogen</rhyme></l>
        <l id="s4:l3" met="1" type="-+">In d’ <rhyme label="b" type="f" corresp="#s4:l2">oogen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Van Jufffers, jong en <rhyme label="a" type="m" corresp="#s4:l1">oudt</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Beklaag dan <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Uw swaar <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Gy hebt u zelfs te loos <rhyme label="b" type="f" corresp="#s4:l2">bedroogen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Als ik de <rhyme label="a" type="m">wijn</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Zie in den <rhyme label="b" type="f">beker</rhyme>,</l>
        <l id="s5:l3" met="1" type="-+">’t Is <rhyme label="b" type="f" corresp="#s5:l2">seeker</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Die baart my <rhyme label="a" type="m" corresp="#s5:l1">Medicyn</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Die voed de <rhyme label="c" type="m">vreugd</rhyme></l>
        <l id="s5:l6" met="2" type="-+">En doet my <rhyme label="c" type="m" corresp="#s5:l5">deugd</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dies hoef ik noit geen <rhyme label="b" type="f" corresp="#s5:l2">Apotheeker</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Weg dart'le <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Jou regte <rhyme label="b" type="f">sotje</rhyme>!</l>
        <l id="s6:l3" met="1" type="-+">‘k <rhyme label="b" type="f" corresp="#s6:l2">Bespotje</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">‘k Verban u uit mijn <rhyme label="a" type="m" corresp="#s6:l1">zin</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Schoon ik wel <rhyme label="c" type="m">schijn</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Uw slaaf te <rhyme label="c" type="m" corresp="#s6:l5">zijn</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">’t Is maar geveynst en ik <rhyme label="b" type="f" corresp="#s6:l2">bedotje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Ik haat de <rhyme label="a" type="m">Wijn</rhyme></l>
        <l id="s7:l2" met="2" type="-+">En al die <rhyme label="b" type="f">prullen</rhyme></l>
        <l id="s7:l3" met="1" type="-+">Die <rhyme label="b" type="f" corresp="#s7:l2">pullen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En dag'lijks dronken <rhyme label="a" type="m" corresp="#s7:l1">zijn</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">Mijn Hert en <rhyme label="c" type="m">zin</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Leeft door de <rhyme label="c" type="m" corresp="#s7:l5">Min</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Dies sal myn Bacchus noit meer <rhyme label="b" type="f" corresp="#s7:l2">lullen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
