<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9192</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatst was hier een reizer</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">IEn Vryer uyt <rhyme label="a" type="f">vrye</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Teegh, en aen sen <rhyme label="a" type="f" corresp="#s1:l1">sye</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Droech hy sijn Deeghen <rhyme label="b" type="m">mee</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Ierst ging hy by <rhyme label="c" type="f">Haesje</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">By Luytje en <rhyme label="c" type="f" corresp="#s1:l4">Claesje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Maer hy en houde gien <rhyme label="b" type="m" corresp="#s1:l3">stee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">By Wybrecht en <rhyme label="a" type="f">Trijntje</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">By Duysje en <rhyme label="a" type="f" corresp="#s2:l1">Stijntje</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">By Swaentje, Ursel, en <rhyme label="b" type="m">Griet</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">By Brechje, by <rhyme label="c" type="f">Baertje</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">By Aeltje, by <rhyme label="c" type="f" corresp="#s2:l4">Claertje</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En vond' hy sijn gaedingh <rhyme label="b" type="m" corresp="#s2:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">By Wayn, by <rhyme label="a" type="f">Annetje</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">By Lijs, by <rhyme label="a" type="f" corresp="#s3:l1">Tannetje</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">By Eva, by <rhyme label="b" type="m">Hillegont</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">By Beeltie, by <rhyme label="c" type="f">Lijntje</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Noch by <rhyme label="c" type="f" corresp="#s3:l4">Jacomijntje</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hy oock sen slach niet en <rhyme label="b" type="m" corresp="#s3:l3">vont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">By Truy, by <rhyme label="a" type="f">Sannetje</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Marry en <rhyme label="a" type="f" corresp="#s4:l1">Jannetje</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">By Krelisje noch by <rhyme label="b" type="m">Tiet</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">By Raeghel, by <rhyme label="c" type="f">Sijtje</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">By Barbel, by <rhyme label="c" type="f" corresp="#s4:l4">Fijtje</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hy mee een blauwe Scheen <rhyme label="b" type="m" corresp="#s4:l3">stiet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">By Joosje, by <rhyme label="a" type="f">Kniertje</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">By Dieuwer, by <rhyme label="a" type="f" corresp="#s5:l1">Giertje</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">By Lobbetje, en by <rhyme label="b" type="m">Saer</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">By Machtel, by <rhyme label="c" type="f">Lutje</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">By Hayltje, by <rhyme label="c" type="f" corresp="#s5:l4">Jutje</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En was het heylick niet <rhyme label="b" type="m" corresp="#s5:l3">claer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">By Elsje, by <rhyme label="a" type="f">Aefje</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">By Cely, by <rhyme label="a" type="f" corresp="#s6:l1">Baesje</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En by Maynsje al te <rhyme label="b" type="m">met</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">By Niesje, by <rhyme label="c" type="f">Jopje</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Pietertje, en <rhyme label="c" type="f" corresp="#s6:l4">Sopje</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En Ariaentje was sen <rhyme label="b" type="m" corresp="#s6:l3">tret</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">By Jaepje, by <rhyme label="a" type="f">Soetje</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">By Hester, by <rhyme label="a" type="f" corresp="#s7:l1">Oetje</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">By Lenoor, en <rhyme label="b" type="m">Magdaleen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">By Geesje, by <rhyme label="c" type="f">Baycke</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">By Enghel, by <rhyme label="c" type="f" corresp="#s7:l4">Maycke</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En Ysje was hy <rhyme label="b" type="m" corresp="#s7:l3">ghemeen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">By Heleen, by <rhyme label="a" type="f">Fransje</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">By Neel, by <rhyme label="a" type="f" corresp="#s8:l1">Constansje</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">BY Isebeel, by <rhyme label="b" type="m">Ding'na</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">By Ytje, by <rhyme label="c" type="f">Teuntje</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">By Vroutje, by <rhyme label="c" type="f" corresp="#s8:l4">Pleuntje</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Quam hy praten t'savents <rhyme label="b" type="m" corresp="#s8:l3">spae</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Princesse dit <rhyme label="a" type="f">maetje</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Dit layde een <rhyme label="a" type="f" corresp="#s9:l1">praetje</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Op ien avent by  heur <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Dit waren sy <rhyme label="c" type="f">even</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Als ick <rhyme label="c" type="f" corresp="#s9:l4">hebeschreven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Drie vijf-en-twintich is <rhyme label="b" type="m" corresp="#s9:l3">'tal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
