    <melody>La fille de madame Angot</melody>
    <categories>
      <category translation="amusement">vermaak</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Door ieder <rhyme label="a" type="f">aangebeden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Werd steeds Madam <rhyme label="b" type="m">Angot</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Om honderdduizend <rhyme label="a" type="f" corresp="#s1:l1">reden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vereerde men haar <rhyme label="b" type="m" corresp="#s1:l2">zoo</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Maar was het Zon of <rhyme label="c" type="f">feestdag</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En kwam men haar te <rhyme label="d" type="m">na</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dan had, als men haar <rhyme label="c" type="f" corresp="#s1:l5">aanzag</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Men reeds genoeg, ha, <rhyme label="d" type="m" corresp="#s1:l6">ha</rhyme>!</l>
        <l id="s1:l9" met="4" type="+-">Z' is aanminnig, doch zeer <rhyme label="e" type="f">vinnig</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">daarom mint men haar ook <rhyme label="f" type="m">zoo</rhyme>;</l>
        <l id="s1:l11" met="4" type="+-">Niet zeer zedig, maartoch <rhyme label="g" type="f">snedig</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Zie, zoo is Madam <rhyme label="f" type="m" corresp="#s1:l10">Angot</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">En was er iets aan 't <rhyme label="a" type="f">handje</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dan was zij steeds <rhyme label="b" type="m">vooraan</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Vooral bij een groot <rhyme label="a" type="f" corresp="#s2:l1">standje</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Kreeg men haar niet <rhyme label="b" type="m" corresp="#s2:l2">vandaan</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Dan smeet zij naar <rhyme label="c" type="f">politie</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met wat zij krijgen <rhyme label="d" type="m">kon</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Hierdoor kreeg elk <rhyme label="c" type="f" corresp="#s2:l5">ambitie</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Zij schitterde als de <rhyme label="d" type="m" corresp="#s2:l6">zon</rhyme>.</l>
        <l id="s2:l9" met="4" type="+-">Z' is aanminnig, doch zeer <rhyme label="e" type="f">vinnig</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Daarom mint men haar ook <rhyme label="f" type="m">zoo</rhyme>;</l>
        <l id="s2:l11" met="4" type="+-">Niet zeer zedig, maar toch <rhyme label="g" type="f">snedig</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Zie, zoo is madam <rhyme label="f" type="m" corresp="#s2:l10">Angot</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Eens was zij <rhyme label="a" type="f">opgestegen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hoog in een <rhyme label="b" type="m">luchtballon</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zij reisde <rhyme label="a" type="f" corresp="#s3:l1">allerwegen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En gaf ook nergens <rhyme label="b" type="m" corresp="#s3:l2">om</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Te Malabar in <rhyme label="c" type="f">handen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wou men, het was wel <rhyme label="d" type="m">naar</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Haar zoetjes gaan <rhyme label="c" type="f" corresp="#s3:l5">verbranden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dat is de mode <rhyme label="d" type="m" corresp="#s3:l6">daar</rhyme>.</l>
        <l id="s3:l9" met="4" type="+-">Storm en zwarten durft ze <rhyme label="e" type="f">tarten</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Zij is in 't gevaar niet <rhyme label="f" type="m">bloô</rhyme>;</l>
        <l id="s3:l11" met="4" type="+-">Zelfs de donder, o wat <rhyme label="g" type="f">wonder</rhyme>!</l>
        <l id="s3:l12" met="4" type="+-">Hindert niet madam <rhyme label="f" type="m" corresp="#s3:l10">Angot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In Rusland en in <rhyme label="a" type="f">Zweden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Heeft zij <rhyme label="b" type="m">gevovageerd</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Vooral in 't Turksche <rhyme label="a" type="f" corresp="#s4:l1">Eden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Werd zij zeer hoog <rhyme label="b" type="m" corresp="#s4:l2">vereerd</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Vergetend al zijn <rhyme label="c" type="f">vrouwen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ontvlamd in <rhyme label="d" type="m">liefdevuur</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Kwam haar gezelschap <rhyme label="c" type="f" corresp="#s4:l5">houën</rhyme></l>
        <l id="s4:l8" met="3" type="-+">De Sultan, menig <rhyme label="d" type="m" corresp="#s4:l6">uur</rhyme>.</l>
        <l id="s4:l9" met="4" type="+-">Zeer aanminnig, <rhyme label="e" type="f">dubbelzinnig</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Daarom minde hij haar <rhyme label="f" type="m">zoo</rhyme>:</l>
        <l id="s4:l11" met="4" type="+-">Niet te zedig, vrij en <rhyme label="g" type="f">snedig</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Was voor hem madam <rhyme label="f" type="m" corresp="#s4:l10">Angot</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Op Java, en in <rhyme label="a" type="f">Spanje</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Waar zij ook immer <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ja, zelfs in 't rijk <rhyme label="a" type="f" corresp="#s5:l1">Kokanje</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Of bij het zwarte <rhyme label="b" type="m" corresp="#s5:l2">ras</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Waar zij zich maar <rhyme label="c" type="f">vertoonde</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Aan Ganges of aan <rhyme label="d" type="m">Po</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Daar waar een sterfling <rhyme label="c" type="f" corresp="#s5:l5">woonde</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Vereerde men <rhyme label="d" type="m" corresp="#s5:l6">Angot</rhyme>.</l>
        <l id="s5:l9" met="4" type="+-">Wel aanminnig, <rhyme label="e" type="f">dubbelzinnig</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Daarom minde men haar <rhyme label="f" type="m">zoo</rhyme>;</l>
        <l id="s5:l11" met="4" type="+-">Niet te zedig, vrij en <rhyme label="g" type="f">snedig</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">Zie, zoo is Madam <rhyme label="f" type="m" corresp="#s5:l10">Angot</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Bij 't volkje der <rhyme label="a" type="f">Mormonen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Fijnproevers door en <rhyme label="b" type="m">door</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Kwam zij zich pas <rhyme label="a" type="f" corresp="#s6:l1">vertoonen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Of dra begon een <rhyme label="b" type="m" corresp="#s6:l2">koor</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Zoodat hun vrouwen <rhyme label="c" type="f">meenden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dat manlief nu niet <rhyme label="d" type="m">meer</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Aan haar een gunst <rhyme label="c" type="f" corresp="#s6:l5">verleende</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Zooals zoo menig <rhyme label="d" type="m" corresp="#s6:l6">keer</rhyme>.</l>
        <l id="s6:l9" met="4" type="+-">Maar die luidjes met hun <rhyme label="e" type="f">spruitjes</rhyme></l>
        <l id="s6:l10" met="4" type="+-">Hadden niet veel <rhyme label="f" type="m">splintio</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Hier geen fratsen en geen <rhyme label="g" type="f">spatsen</rhyme>,</l>
        <l id="s6:l12" met="4" type="+-">Zei ronduit Madam <rhyme label="f" type="m" corresp="#s6:l10">Angot</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
