        <l id="s1:l1" met="3" type="-+">DE wijle wy zijn <rhyme label="a" type="f">hiere</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Makende goede <rhyme label="a" type="f" corresp="#s1:l1">chiere</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Niet meer dan v lust <rhyme label="b" type="m">drinckt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maeckt in v t'minnen <rhyme label="a" type="f" corresp="#s1:l1">viere</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">In uwen tijt seer <rhyme label="a" type="f" corresp="#s1:l1">diere</rhyme></l>
        <l id="s1:l6" met="3" type="-+">D'een d'ander tot vreucht <rhyme label="b" type="m" corresp="#s1:l3">bringt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Stelt v hartekens tot <rhyme label="c" type="m">rust</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">V lief voedt v met goe <rhyme label="a" type="f" corresp="#s1:l1">manieren</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Soo langhet v beyen <rhyme label="c" type="m" corresp="#s1:l7">lust</rhyme>.</l>
