      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Clauwaert, <rhyme label="a" type="m">Clauwaert</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hoet u wel van den <rhyme label="a" type="m" corresp="#s1:l1">Lelyaert</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Gaet ghi niet te <rhyme label="a" type="m" corresp="#s1:l1">Ghendtwaert</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ghi laetter uwen <rhyme label="a" type="m" corresp="#s1:l1">tabbaert</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Al waerdi noch soe seere <rhyme label="a" type="m" corresp="#s1:l1">ghebaert</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Sy sullen u maken <rhyme label="a" type="m" corresp="#s1:l1">vervaert</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">O Clauwaert, <rhyme label="a" type="m" corresp="#s1:l1">Clauwaert</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Wacht u voere den <rhyme label="a" type="m" corresp="#s1:l1">Lelyaert</rhyme>.</l>
      </lg>
    </lg>
