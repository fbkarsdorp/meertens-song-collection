  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Eb 'k ik maijn <rhyme label="a" type="f">fleske</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Door fraykes ooyt   <rhyme label="b" type="m">geveegd</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Was nau men <rhyme label="a" type="f" corresp="#s1:l1">teske</rhyme></l>
        <l id="s1:l4" met="3" type="-+">So pestig niet <rhyme label="b" type="m" corresp="#s1:l2">geleegd</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">'k En sau gien ontsvod <rhyme label="c" type="m">sain</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van nog ien potte <rhyme label="c" type="m" corresp="#s1:l5">waijn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Door 't kielgat een te <rhyme label="d" type="f">stauwen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Maar men en wil 's aan <rhyme label="c" type="m" corresp="#s1:l5">maijn</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Te kwaad niet <rhyme label="d" type="f" corresp="#s1:l7">auwen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ba sinte <rhyme label="a" type="f">stieven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">'k Ik pays, ik oor nog <rhyme label="b" type="m">klank</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">Doar es nog <rhyme label="a" type="f" corresp="#s2:l1">lieven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Doar 's nog een betske <rhyme label="b" type="m" corresp="#s2:l2">drank</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">A, edel schoene <rhyme label="c" type="m">waijn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Oe sterkt de kragt mijn <rhyme label="c" type="m" corresp="#s2:l5">braijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gay elpt mijn ert aan 't <rhyme label="d" type="f">springen</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Kom 'k sal iens vray van <rhyme label="c" type="m" corresp="#s2:l5">payn</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Ien airke <rhyme label="d" type="f" corresp="#s2:l7">singen</rhyme>.</l>
      </lg>
    </lg>
  </text>
