  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">O <rhyme label="a" type="f">Maria</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Ghy boven alle <rhyme label="b" type="f">vrouwen</rhyme></l>
        <l id="s1:l3" met="2" type="-+"><rhyme label="c" type="m">Ghebenedijdt</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">O <rhyme label="a" type="f" corresp="#s1:l1">Maria</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Op u is ons <rhyme label="b" type="f" corresp="#s1:l2">betrouwen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Tot <rhyme label="c" type="m" corresp="#s1:l3">aldertijdt</rhyme>!</l>
        <l id="s1:l7" met="5" type="+-">Dus wy comen tot u Moeder <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Een <rhyme label="e" type="f">Patronersse</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+"><rhyme label="e" type="f" corresp="#s1:l8">Advocatersse</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">En <rhyme label="e" type="f" corresp="#s1:l8">Middelersse</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Ghy voor ons al wilt <rhyme label="f" type="m">zijn</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Wilt zijn, Wilt <rhyme label="f" type="m" corresp="#s1:l11">zijn</rhyme>,</l>
        <l id="s1:l13" met="4" type="+-">O Maria weest <rhyme label="d" type="m" corresp="#s1:l7">gegroet</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">O <rhyme label="a" type="f">Maria</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">V lief <rhyme label="b" type="f">gebenedijde</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Schoon soete <rhyme label="c" type="m">Vrucht</rhyme>!</l>
        <l id="s2:l4" met="2" type="+-">O <rhyme label="a" type="f" corresp="#s2:l1">Maria</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Ons hier al seer <rhyme label="b" type="f" corresp="#s2:l2">verblijde</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Met groot <rhyme label="c" type="m" corresp="#s2:l3">genucht</rhyme>,</l>
        <l id="s2:l7" met="5" type="+-">Moeder wy u vallen nu te <rhyme label="d" type="m">voet</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Wilt al ons <rhyme label="e" type="f">klaghen</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Tot Iesum <rhyme label="e" type="f" corresp="#s2:l8">draghen</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Om te <rhyme label="e" type="f" corresp="#s2:l8">behaghen</rhyme>!</l>
        <l id="s2:l11" met="3" type="+-">Want ghy met hem <rhyme label="f" type="m">gemeen</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Gemeen, <rhyme label="f" type="m" corresp="#s2:l11">gemeen</rhyme>,</l>
        <l id="s2:l13" met="4" type="+-">Ontfanght uwe Engelsch <rhyme label="d" type="m" corresp="#s2:l7">Groet</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">O <rhyme label="a" type="f">Maria</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Heyligh en uyt <rhyme label="b" type="f">gelesen</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Moeder van <rhyme label="c" type="m">Godt</rhyme></l>
        <l id="s3:l4" met="2" type="+-">O <rhyme label="a" type="f" corresp="#s3:l1">Maria</rhyme>!</l>
        <l id="s3:l5" met="3" type="-+">Bidt voor ons om <rhyme label="b" type="f" corresp="#s3:l2">genesen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Die sijn <rhyme label="c" type="m" corresp="#s3:l3">Gebodt</rhyme></l>
        <l id="s3:l7" met="5" type="+-">Niet en hebben willen <rhyme label="d" type="m">onderstaen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Dat wy <rhyme label="e" type="f">sondaren</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Sonder <rhyme label="e" type="f" corresp="#s3:l8">beswaeren</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Nu laten <rhyme label="e" type="f" corresp="#s3:l8">varen</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Ons sonden voor <rhyme label="f" type="m">altijdt</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Altijdt, <rhyme label="f" type="m" corresp="#s3:l11">altijdt</rhyme>,</l>
        <l id="s3:l13" met="4" type="+-">Nemt ons bidden Moeder <rhyme label="d" type="m" corresp="#s3:l7">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">O <rhyme label="a" type="f">Maria</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">Berght ons voor alle <rhyme label="b" type="f">tijen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">In uwen <rhyme label="c" type="m">schoot</rhyme>!</l>
        <l id="s4:l4" met="2" type="+-">O <rhyme label="a" type="f" corresp="#s4:l1">Maria</rhyme>!</l>
        <l id="s4:l5" met="3" type="-+">Wilt ons doch helpen <rhyme label="b" type="f" corresp="#s4:l2">strijen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">In dâ€™uer der <rhyme label="c" type="m" corresp="#s4:l3">doodt</rhyme></l>
        <l id="s4:l7" met="5" type="+-">Onse Zielen Maghet door <rhyme label="d" type="m">ootmoedt</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Wilt dan <rhyme label="e" type="f">ontfanghen</rhyme>:</l>
        <l id="s4:l9" met="2" type="-+">Want sy seer <rhyme label="e" type="f" corresp="#s4:l8">banghen</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Naer u <rhyme label="e" type="f" corresp="#s4:l8">verlanghen</rhyme></l>
        <l id="s4:l11" met="3" type="+-">O Moeder vol <rhyme label="f" type="m">genaed</rhyme></l>
        <l id="s4:l12" met="2" type="-+">Genaed! <rhyme label="f" type="m" corresp="#s4:l11">genaed</rhyme>!</l>
        <l id="s4:l13" met="4" type="+-">Ons hulp weest met alle <rhyme label="d" type="m" corresp="#s4:l7">spoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
