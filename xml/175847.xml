  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat sullen wy nu gaen <rhyme label="a" type="f">beginnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Roomsche Catholijcken <rhyme label="b" type="m">al</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Wy sijn half uyt ons <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En worden noch wel <rhyme label="b" type="m" corresp="#s1:l2">mal</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Want 't is nu over al de <rhyme label="c" type="m">leus</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Den Bosch sal haest werden <rhyme label="c" type="m" corresp="#s1:l5">Geus</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die maeckt ons een lange <rhyme label="c" type="m" corresp="#s1:l5">Neus</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wie sou doch van sijn <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De ghedachten hebben <rhyme label="b" type="m">ghehadt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat de Prins sou hebben <rhyme label="a" type="f" corresp="#s2:l1">geslagen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sijn Leger voor de <rhyme label="b" type="m" corresp="#s2:l2">Stad</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">'T is nochtans eylaes <rhyme label="c" type="m">geschiet</rhyme>!</l>
        <l id="s2:l6" met="4" type="+-">Wy achten 't eerst min als <rhyme label="c" type="m" corresp="#s2:l5">niet</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Maer 't is nu een groot <rhyme label="c" type="m" corresp="#s2:l5">verdriet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy meende 't sou niet lange <rhyme label="a" type="f">dueren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Daer hy daer sou leggen <rhyme label="b" type="m">veur</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Het water sou hem doen <rhyme label="a" type="f" corresp="#s3:l1">trueren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En met schande weer jagen <rhyme label="b" type="m" corresp="#s3:l2">deur</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Maer sy maeckten met <rhyme label="c" type="m">pracktijck</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Daer eenen Holantschen <rhyme label="c" type="m" corresp="#s3:l5">Dijck</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Die het water deed nemen <rhyme label="c" type="m" corresp="#s3:l5">wijck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy riepen dat de <rhyme label="a" type="f">gronden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Waren weeck en vol <rhyme label="b" type="m">Moras</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Maer sy hebben onder <rhyme label="a" type="f" corresp="#s4:l1">vonden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Datter een goe Sant-gront <rhyme label="b" type="m" corresp="#s4:l2">was</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Oock hebben sy 't geluck <rhyme label="c" type="m">gehadt</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Datter viel seer weynigh <rhyme label="c" type="m" corresp="#s4:l5">nat</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Jnt voor-jaer rontom de <rhyme label="c" type="m" corresp="#s4:l5">Stadt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daer na quam onsen <rhyme label="a" type="f">Helde</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Graef Hendrick van den <rhyme label="b" type="m">Bergh</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">By 't Leger oock te <rhyme label="a" type="f" corresp="#s5:l1">Velde</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met een voornemen  <rhyme label="b" type="m" corresp="#s5:l2">ergh</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Om de Geusen op te <rhyme label="c" type="m">slaen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En te jagen daer van <rhyme label="c" type="m" corresp="#s5:l5">daen</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Maer 't is hem niet wel <rhyme label="c" type="m" corresp="#s5:l5">vergaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Doen waren wy int <rhyme label="a" type="f">vertrouwen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Den Bos sou haest sijn <rhyme label="b" type="m">ontset</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Men ginck ons oock <rhyme label="a" type="f" corresp="#s6:l1">verflouwen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat ons Lief-vrou <rhyme label="b" type="m" corresp="#s6:l2">altemet</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Had gheroepen op de <rhyme label="c" type="m">Vest</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Laet de Geus maer doen sijn <rhyme label="c" type="m" corresp="#s6:l5">best</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Hy moet vluchten uyt dit <rhyme label="c" type="m" corresp="#s6:l5">gewest</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Doen gingen wy wedden en <rhyme label="a" type="f">koopen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Met de Geusen om Gelt en <rhyme label="b" type="m">schat</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dat den Prins sou moeten <rhyme label="a" type="f" corresp="#s7:l1">verloopen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En verlaten de schoone <rhyme label="b" type="m" corresp="#s7:l2">Stadt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Wy gavender veel gelt om <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">'t Was of een kroon ons was een <rhyme label="c" type="m" corresp="#s7:l5">duyt</rhyme></l>
        <l id="s7:l7" met="4" type="+-">Maer nu kijcken wy op ons <rhyme label="c" type="m" corresp="#s7:l5">snuyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wat wy nemen te <rhyme label="a" type="f">vooren</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Tot deeses Stad <rhyme label="b" type="m">ontset</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">'T is al moeyte <rhyme label="a" type="f" corresp="#s8:l1">verloren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">'t Komt uyt, of wort <rhyme label="b" type="m" corresp="#s8:l2">belet</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">'t Schijnt wel dat Godt met haer <rhyme label="c" type="m">is</rhyme></l>
        <l id="s8:l6" met="4" type="+-">En heel tegen ons <rhyme label="c" type="m" corresp="#s8:l5">gewis</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Want ons baet geen biegt noch <rhyme label="c" type="m" corresp="#s8:l5">Mis</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Graef Hendrick had <rhyme label="a" type="f">voor-genomen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Den dommel te sterken <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Maer 't is strax <rhyme label="a" type="f" corresp="#s9:l1">uyt-gekomen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En daer quamen om in 't <rhyme label="b" type="m" corresp="#s9:l2">getreur</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Twee Boeren die 't meynden te <rhyme label="c" type="m">doen</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Die in 't Princen Leger <rhyme label="c" type="m" corresp="#s9:l5">koen</rhyme></l>
        <l id="s9:l7" met="4" type="+-">Sijn gehangen op dat <rhyme label="c" type="m" corresp="#s9:l5">saysoen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Graef Hendrick edel <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Maeckten dickwils oock <rhyme label="b" type="m">alarm</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Om de Geusen te <rhyme label="a" type="f" corresp="#s10:l1">verveere</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Maer ten holp ons niet ach <rhyme label="b" type="m" corresp="#s10:l2">arm</rhyme>!</l>
        <l id="s10:l5" met="4" type="-+">Want sy hielden goede <rhyme label="c" type="m">wacht</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Soo wel by dagen als by <rhyme label="c" type="m" corresp="#s10:l5">nacht</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">En sy hebben daerom <rhyme label="c" type="m" corresp="#s10:l5">gelacht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die van binnen met <rhyme label="a" type="f">kouragie</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Vielen oock wel somtijts <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Om te soecken <rhyme label="a" type="f" corresp="#s11:l1">avontagie</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">Maer sy kregen slagen tot <rhyme label="b" type="m" corresp="#s11:l2">buyt</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">De Geuysen dreven haer weer <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Soo dat sy sonder <rhyme label="c" type="m" corresp="#s11:l5">gewin</rhyme></l>
        <l id="s11:l7" met="4" type="+-">Moesten vluchten  tegen haer <rhyme label="c" type="m" corresp="#s11:l5">sin</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Graef Hendrick van den <rhyme label="a" type="f">Bergen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Heeft oock een versoeck <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Om de Geusen wat te <rhyme label="a" type="f" corresp="#s12:l1">tergen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En hy viel op het Leger <rhyme label="b" type="m" corresp="#s12:l2">aen</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Hy meynde daer ses hondert <rhyme label="c" type="m">man</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Jn de Stad te brengen <rhyme label="c" type="m" corresp="#s12:l5">an</rhyme>,</l>
        <l id="s12:l7" met="4" type="+-">Maer hy moester met schande <rhyme label="c" type="m" corresp="#s12:l5">van</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Hy stormde wel op vier <rhyme label="a" type="f">hoecken</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Maer rechten gantsch niet <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">De Geusen weerden hen <rhyme label="a" type="f" corresp="#s13:l1">kloecken</rhyme></l>
        <l id="s13:l4" met="3" type="-+">En sloegense op de <rhyme label="b" type="m" corresp="#s13:l2">huyt</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Daer blever wel vier hondert <rhyme label="c" type="m">doot</rhyme></l>
        <l id="s13:l6" met="4" type="+-">De reste met angst en schande <rhyme label="c" type="m" corresp="#s13:l5">vloot</rhyme>,</l>
        <l id="s13:l7" met="4" type="+-">'t Welck Graef Hendrick seer <rhyme label="c" type="m" corresp="#s13:l5">verdroot</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Doen bedacht men een nieuwe <rhyme label="a" type="f">streke</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Om haer den Hollantschen <rhyme label="b" type="m">Dijck</rhyme></l>
        <l id="s14:l3" met="3" type="-+">By nacht te doen <rhyme label="a" type="f" corresp="#s14:l1">door-steken</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Door veel swemmers met <rhyme label="b" type="m" corresp="#s14:l2">practijck</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Maer 't volck die 't souden <rhyme label="c" type="m">bestaen</rhyme></l>
        <l id="s14:l6" met="4" type="+-">En dit werck souden <rhyme label="c" type="m" corresp="#s14:l5">aengaen</rhyme></l>
        <l id="s14:l7" met="4" type="+-">Brachten sulcks den Prince <rhyme label="c" type="m" corresp="#s14:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Soo loopt het al in der <rhyme label="a" type="f">asschen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Wat wy peysen of wat wy <rhyme label="b" type="m">doen</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Wy willen de Geusen <rhyme label="a" type="f" corresp="#s15:l1">verrasschen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Maer sy sijn ons te loos en <rhyme label="b" type="m" corresp="#s15:l2">koen</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Oock komen wy veel te <rhyme label="c" type="m">laet</rhyme></l>
        <l id="s15:l6" met="4" type="+-">Want sy sijn in de <rhyme label="c" type="m" corresp="#s15:l5">voor-baet</rhyme></l>
        <l id="s15:l7" met="4" type="+-">Dies werden wy meest <rhyme label="c" type="m" corresp="#s15:l5">desperaet</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Wy hadden ons meest <rhyme label="a" type="f">vertrouwen</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Op de groote en kleyne <rhyme label="b" type="m">Schans</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Dat wy die wel souden <rhyme label="a" type="f" corresp="#s16:l1">houwen</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Maer 't was al verlooren <rhyme label="b" type="m" corresp="#s16:l2">kans</rhyme></l>
        <l id="s16:l5" met="4" type="-+">Wy sijnse in twee dagen <rhyme label="c" type="m">tijdt</rhyme></l>
        <l id="s16:l6" met="4" type="+-">Alle-bey geworden <rhyme label="c" type="m" corresp="#s16:l5">quijdt</rhyme>,</l>
        <l id="s16:l7" met="4" type="+-">'t Welck de Geusen maeckt <rhyme label="c" type="m" corresp="#s16:l5">verblijt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">De Franschen wonnen de <rhyme label="a" type="f">groote</rhyme></l>
        <l id="s17:l2" met="3" type="-+">En d'Engelschen kregen de <rhyme label="b" type="m">kleyn</rhyme></l>
        <l id="s17:l3" met="3" type="-+">En hebben d'onse <rhyme label="a" type="f" corresp="#s17:l1">verstooten</rhyme></l>
        <l id="s17:l4" met="3" type="-+">En verjaeght met al haer <rhyme label="b" type="m" corresp="#s17:l2">treyn</rhyme>,</l>
        <l id="s17:l5" met="4" type="-+">Dat sy voor vrees namen de <rhyme label="c" type="m">vlucht</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">Tot onder de Poort van de <rhyme label="c" type="m" corresp="#s17:l5">Vucht</rhyme></l>
        <l id="s17:l7" met="4" type="+-">Dat kost ons meenigen <rhyme label="c" type="m" corresp="#s17:l5">sucht</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Een dagh of twee te <rhyme label="a" type="f">voren</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Js Graef Hendrick <rhyme label="b" type="m">op-geruckt</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Om Genep te <rhyme label="a" type="f" corresp="#s18:l1">verstoren</rhyme>:</l>
        <l id="s18:l4" met="3" type="-+">Maer d'aenslagh is <rhyme label="b" type="m" corresp="#s18:l2">misluckt</rhyme>,</l>
        <l id="s18:l5" met="4" type="-+">Hy trock na Nimmegen <rhyme label="c" type="m">bly</rhyme></l>
        <l id="s18:l6" met="4" type="+-">Met veel Cornetten <rhyme label="c" type="m" corresp="#s18:l5">Ruytery</rhyme>,</l>
        <l id="s18:l7" met="4" type="+-">Maer met schande verliepen <rhyme label="c" type="m" corresp="#s18:l5">sy</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Nu heeft den Prins <rhyme label="a" type="f">gewonnen</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">Den Bos die sijn wy <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Och had hy noyt <rhyme label="a" type="f" corresp="#s19:l1">begonnen</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">'T is ons een groote <rhyme label="b" type="m" corresp="#s19:l2">spijt</rhyme></l>
        <l id="s19:l5" met="4" type="-+">Dat den Geus daer nu treckt <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s19:l6" met="4" type="+-">Ten is niet wel na onse <rhyme label="c" type="m" corresp="#s19:l5">sin</rhyme></l>
        <l id="s19:l7" met="4" type="+-">Want sy is nu den Princen <rhyme label="c" type="m" corresp="#s19:l5">min</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Waer blijven wy te <rhyme label="a" type="f">hoopen</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">Papen, Munnicken <rhyme label="b" type="m">allegaere</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Waer sullen wy nu <rhyme label="a" type="f" corresp="#s20:l1">loopen</rhyme>?</l>
        <l id="s20:l4" met="3" type="-+">Dit valt ons al te <rhyme label="b" type="m" corresp="#s20:l2">swaer</rhyme>,</l>
        <l id="s20:l5" met="4" type="-+">Ons vette Keucken is <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s20:l6" met="4" type="+-">Wy mogen nu wel bevaert <rhyme label="c" type="m" corresp="#s20:l5">gaen</rhyme></l>
        <l id="s20:l7" met="4" type="+-">Om ons Lief-vrou te nemen <rhyme label="c" type="m" corresp="#s20:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Oorlof o Bos <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Oorlof wy scheyden <rhyme label="b" type="m">nu</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">Oorlof voor al ons <rhyme label="a" type="f" corresp="#s21:l1">leven</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Blijven wy van u <rhyme label="b" type="m" corresp="#s21:l2">schu</rhyme>,</l>
        <l id="s21:l5" met="4" type="-+">U hooghdy die sijt ghy <rhyme label="c" type="m">quijt</rhyme>,</l>
        <l id="s21:l6" met="4" type="+-">Den Prins regeert nu ter <rhyme label="c" type="m" corresp="#s21:l5">tijd</rhyme></l>
        <l id="s21:l7" met="4" type="+-">'T is voor ons een groote <rhyme label="c" type="m" corresp="#s21:l5">spijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
