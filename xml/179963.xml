<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179963</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Engelse fa la la</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Och! Lies, ich heb doe her√∂m <rhyme label="a" type="m">gezocht</rhyme>,</l>
        <l id="s1:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Woo, Hans, wat heitst doe doar <rhyme label="a" type="m" corresp="#s1:l1">toegebrogt</rhyme>?</l>
        <l id="s1:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s1:l2">la</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat schol ik doe kond doan, hupscher <rhyme label="a" type="m">Deirn</rhyme>,</l>
        <l id="s2:l2" met="9" type="+-">Faldera, la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Vertel 't mich dan bald, ich hoar doe dan <rhyme label="a" type="m" corresp="#s2:l1">geirn</rhyme>,</l>
        <l id="s2:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s2:l2">la</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mien smoddemuil, 'k bin op joe <rhyme label="a" type="m">verleifd</rhyme>,</l>
        <l id="s3:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Loap, likst mich de moarze Hans, as doe <rhyme label="a" type="m" corresp="#s3:l1">bleifd</rhyme>,</l>
        <l id="s3:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s3:l2">la</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat meinst nich, 'k geef doe veil liever ein <rhyme label="a" type="m">toet</rhyme>,</l>
        <l id="s4:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Doe scholster wol wieken mit dynen <rhyme label="a" type="m" corresp="#s4:l1">snoet</rhyme>,</l>
        <l id="s4:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s4:l2">la</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Och, liebster maike, wie bist doe zoo <rhyme label="a" type="m">kwoad</rhyme>?</l>
        <l id="s5:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Om dat doe nich als von narre spul <rhyme label="a" type="m" corresp="#s5:l1">proad</rhyme>,</l>
        <l id="s5:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s5:l2">la</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Woo, 't is joo gein gekheid, ik vry joe <rhyme label="a" type="m">oprecht</rhyme>,</l>
        <l id="s6:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Doe meinstet dan Hans, wel wat ofs doe <rhyme label="a" type="m" corresp="#s6:l1">zegt</rhyme>,</l>
        <l id="s6:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s6:l2">la</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Joa, schatke, doar hebst doe ein sjeen <rhyme label="a" type="m">perzent</rhyme>,</l>
        <l id="s7:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Wat doan ik doar mit, doar steikt in dyn <rhyme label="a" type="m" corresp="#s7:l1">krent</rhyme>,</l>
        <l id="s7:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s7:l2">la</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Kiek, 't is ein koukerke rondom mit <rhyme label="a" type="m">gold</rhyme>,</l>
        <l id="s8:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Wat brust mich dat dink, 'k zeg datst doe 't moar <rhyme label="a" type="m" corresp="#s8:l1">hold</rhyme>,</l>
        <l id="s8:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s8:l2">la</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Doe kenst 'er dyn noalden in doan <rhyme label="a" type="m">vryndin</rhyme>,</l>
        <l id="s9:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Loap, steikster dyn eigen noalden dan <rhyme label="a" type="m" corresp="#s9:l1">in</rhyme>,</l>
        <l id="s9:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s9:l2">la</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Doe weitst wol dat ein knegtke nich <rhyme label="a" type="m">noait</rhyme>,</l>
        <l id="s10:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Ziet datst doe dan doar ein ander mit <rhyme label="a" type="m" corresp="#s10:l1">poait</rhyme>,</l>
        <l id="s10:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s10:l2">la</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Doe Meume zei, dats doe 't wol nemmen <rhyme label="a" type="m">schol</rhyme>,</l>
        <l id="s11:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Mien Maur weit nich dat doe schortst in dyn <rhyme label="a" type="m" corresp="#s11:l1">bol</rhyme>,</l>
        <l id="s11:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s11:l2">la</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Och, Leveke, 'k bidst doe, dat die mich <rhyme label="a" type="m">hoard</rhyme>,</l>
        <l id="s12:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Goad hin nar, ond likt het zwienshinder <rhyme label="a" type="m" corresp="#s12:l1">poart</rhyme>,</l>
        <l id="s12:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s12:l2">la</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ich load joe toch nich alleinig heir <rhyme label="a" type="m">stoan</rhyme>,</l>
        <l id="s13:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s13:l3" met="4" type="-+">Ich wol, Hans, want ich wol bald hennen <rhyme label="a" type="m" corresp="#s13:l1">goan</rhyme>.</l>
        <l id="s13:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s13:l2">la</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Mien troeteldierke, ei, bleifst doe noch <rhyme label="a" type="m">wat</rhyme>,</l>
        <l id="s14:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s14:l3" met="4" type="-+">Nein, Hans, het mueske dat loapt voar de <rhyme label="a" type="m" corresp="#s14:l1">kat</rhyme>,</l>
        <l id="s14:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s14:l2">la</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Och ze goat hen; moar ik volg hoar <rhyme label="a" type="m">noar</rhyme>,</l>
        <l id="s15:l2" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m">la</rhyme>;</l>
        <l id="s15:l3" met="4" type="-+">Want 't koukerke hoard moar allienig an <rhyme label="a" type="m" corresp="#s15:l1">hoar</rhyme>,</l>
        <l id="s15:l4" met="9" type="+-">Faldera la, la, la, la, la, la, <rhyme label="b" type="m" corresp="#s15:l2">la</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
