  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Tanneken komt nu aan den <rhyme label="a" type="m">dans</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Het is kermis <rhyme label="b" type="f">avond</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Grietjen die heeft den oven <rhyme label="c" type="m">geheet</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Koeken en taarten zyn <rhyme label="c" type="m" corresp="#s1:l3">gereed</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Pieter Lenaart, Dries en <rhyme label="a" type="m" corresp="#s1:l1">Hans</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die zullen komen <rhyme label="b" type="f" corresp="#s1:l2">t'avond</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Jacq en Jan die loopen <rhyme label="a" type="m">sterk</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Om de klok te <rhyme label="b" type="f">luyen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Peere die bewaart den <rhyme label="c" type="m">tap</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Grammere maakt den <rhyme label="c" type="m" corresp="#s2:l3">ryspap</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Een ider die heeft zyn <rhyme label="a" type="m" corresp="#s2:l1">werk</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En Cathryn wilt gaan <rhyme label="b" type="f" corresp="#s2:l2">huywen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hespe spek en terwen <rhyme label="a" type="m">brood</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hutspot smeert maar <rhyme label="b" type="f">gasten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Eet maar toe en smeert braaf <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wy zullen dan van 't brouwers <rhyme label="c" type="m" corresp="#s3:l3">sop</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Lustig drinken <rhyme label="a" type="m" corresp="#s3:l1">sakkerloot</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En staan pal als <rhyme label="b" type="f" corresp="#s3:l2">masten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Speelman die doet zyn <rhyme label="a" type="m">best</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En laat de snaaren <rhyme label="b" type="f">klinken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met den zoeten kermis <rhyme label="c" type="m">tyd</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Een ieder is <rhyme label="c" type="m" corresp="#s4:l3">verblyd</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Maar de klugt komt op het <rhyme label="a" type="m" corresp="#s4:l1">lest</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat zal margo <rhyme label="b" type="f" corresp="#s4:l2">gedinken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Springt en danst sa op <rhyme label="a" type="m">Marjan</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laat de beentjes <rhyme label="b" type="f">zwieren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Op Marjanneken nog een <rhyme label="c" type="m">keer</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Morgen komt de Speelman <rhyme label="c" type="m" corresp="#s5:l3">weer</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Elk doet hier wat hy <rhyme label="a" type="m" corresp="#s5:l1">kan</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Volgens zyn <rhyme label="b" type="f" corresp="#s5:l2">manieren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dikwils op een kermis <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dan komt Sinte <rhyme label="b" type="f">Maarten</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En hy roept bouw, der bou der de <rhyme label="c" type="m">bou</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Daar naar singt het myske <rhyme label="c" type="m" corresp="#s6:l3">tou</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Den rysap heeft haar <rhyme label="a" type="m" corresp="#s6:l1">verleyd</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Met de koeken en <rhyme label="b" type="f" corresp="#s6:l2">taarten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Voor 't lest nog een kermis <rhyme label="a" type="m">sprong</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Op sa sa <rhyme label="b" type="f">margootje</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Soo krygt 't kermissen een <rhyme label="c" type="m">end</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">'t meysjen gaat met een <rhyme label="c" type="m" corresp="#s7:l3">present</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En met eenen groote <rhyme label="a" type="m" corresp="#s7:l1">vonk</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Van dat zoet <rhyme label="b" type="f" corresp="#s7:l2">Pierootje</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
