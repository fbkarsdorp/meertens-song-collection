<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184849</id>
    <date>
      <year-start>1639</year-start>
      <year-end>1639</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ULiedt nu, vliedt nu jonge <rhyme label="a" type="m">Ieught</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Yd'le <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want het streckt tot groot <rhyme label="b" type="f">bederven</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Neemt den Gulden tijdt nu <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Die hier <rhyme label="c" type="m" corresp="#s1:l4">naer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Liever leven wil, dan <rhyme label="b" type="f" corresp="#s1:l3">sterven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Serven wil elck wel <rhyme label="a" type="m">ontgaen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Maer met <rhyme label="a" type="m" corresp="#s2:l1">waen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Denckt nu menigh tot sijn <rhyme label="b" type="f">schade</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Al zijn wel mijn sonden <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Bloedigh <rhyme label="c" type="m" corresp="#s2:l4">root</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Snee-wit maeckt my Gods <rhyme label="b" type="f" corresp="#s2:l3">ghenade</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gods ghenade den <rhyme label="a" type="m">Sondaer</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Niet een <rhyme label="a" type="m" corresp="#s3:l1">hayr</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Voordeel doet in 't sondigh <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Wie hier sondight niet <rhyme label="c" type="m">behoort</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Christus, <rhyme label="c" type="m" corresp="#s3:l4">voort</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Wordt hy van Sathan <rhyme label="b" type="f" corresp="#s3:l3">ghedreven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sathan loert met aller <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Dagh en <rhyme label="a" type="m" corresp="#s4:l1">nacht</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om ons zielen te <rhyme label="b" type="f">verpletten</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Singt soet als den <rhyme label="c" type="m">Voghelaer</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Lacy! <rhyme label="c" type="m" corresp="#s4:l4">maer</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Om ons vanghen in sijn <rhyme label="b" type="f" corresp="#s4:l3">netten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Van sijn netten zijn <rhyme label="a" type="m">gheplant</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Op het <rhyme label="a" type="m" corresp="#s5:l1">Landt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Over-al, in alle <rhyme label="b" type="f">weghen</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Dies een yeder waer hy <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Ofte <rhyme label="c" type="m" corresp="#s5:l4">gaet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Mach hem wel voorsien te <rhyme label="b" type="f" corresp="#s5:l3">deghen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Elck te deghen nu de <rhyme label="a" type="m">wacht</rhyme></l>
        <l id="s6:l2" met="2" type="+-">Neemt in <rhyme label="a" type="m" corresp="#s6:l1">acht</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wat al vreught ghy voort siet <rhyme label="b" type="f">stellen</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Spreeckt u hater <rhyme label="c" type="m">vriendelick</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">'t Is een <rhyme label="c" type="m" corresp="#s6:l4">strick</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Om daer meed' u neer te <rhyme label="b" type="f" corresp="#s6:l3">vellen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Neder-vellen was sijn <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s7:l2" met="2" type="+-">In 't <rhyme label="a" type="m" corresp="#s7:l1">begin</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Hy is hier in niet <rhyme label="b" type="f">bezweken</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Dies een yeder wacht hem <rhyme label="c" type="m">doch</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Voor 't <rhyme label="c" type="m" corresp="#s7:l4">bedroch</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Van des Sathans loose <rhyme label="b" type="f" corresp="#s7:l3">treken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Wilt uyt-trecken 't sondigh <rhyme label="a" type="m">vleysch</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Naer den <rhyme label="a" type="m" corresp="#s8:l1">eysch</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En uyt Sodoma nu <rhyme label="b" type="f">trachten</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Is der Engelen <rhyme label="c" type="m">bediedt</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Wel <rhyme label="c" type="m" corresp="#s8:l4">gheschiedt</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Wilt dan Gods Woort niet <rhyme label="b" type="f" corresp="#s8:l3">verachten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Acht (doen al de werelt <rhyme label="a" type="m">was</rhyme></l>
        <l id="s9:l2" met="2" type="+-">Als een <rhyme label="a" type="m" corresp="#s9:l1">plas</rhyme>,)</l>
        <l id="s9:l3" met="4" type="+-">Zijnder maer over <rhyme label="b" type="f">ghebleven</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">'t Sal soo zijn van 's Heeren <rhyme label="c" type="m">Dagh</rhyme></l>
        <l id="s9:l5" met="2" type="+-">Alst doen <rhyme label="c" type="m" corresp="#s9:l4">plagh</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dies mach schricken elck, en <rhyme label="b" type="f" corresp="#s9:l3">beven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Schrick'lijck Godt verschijnen <rhyme label="a" type="m">sal</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">En ten <rhyme label="a" type="m" corresp="#s10:l1">val</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Brenghen al 't ongodlijck <rhyme label="b" type="f">wesen</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Denckt hoe veel meer zijn <rhyme label="c" type="m">verdaen</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">Als <rhyme label="c" type="m" corresp="#s10:l4">gegaen</rhyme></l>
        <l id="s10:l6" met="4" type="+-">In 't Beloofde Landt voor <rhyme label="b" type="f" corresp="#s10:l3">desen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Desen schoon dierbaren <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s11:l2" met="2" type="+-">Gaen wy <rhyme label="a" type="m" corresp="#s11:l1">quijt</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Dat een yeder dit <rhyme label="b" type="f">besinde</rhyme>:</l>
        <l id="s11:l4" met="4" type="+-">Dat den tijdt oock naerdert <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s11:l5" met="2" type="+-">Daer de <rhyme label="c" type="m" corresp="#s11:l4">Heer</rhyme></l>
        <l id="s11:l6" met="4" type="+-">Van mistrouwt, Gheloof te <rhyme label="b" type="f" corresp="#s11:l3">vinden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Weynigh Goudt te vinden <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s12:l2" met="2" type="+-">Is <rhyme label="a" type="m" corresp="#s12:l1">gewhis</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">'t Druppel by een vloedt is <rhyme label="b" type="f">kleynigh</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">'s Levens Padt is nauw en <rhyme label="c" type="m">smal</rhyme>.</l>
        <l id="s12:l5" met="2" type="+-">En 't <rhyme label="c" type="m" corresp="#s12:l4">ghetal</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Die bewandelt, is seer <rhyme label="b" type="f" corresp="#s12:l3">weynigh</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Oorlof weynigh (voor een <rhyme label="a" type="m">slot</rhyme>)</l>
        <l id="s13:l2" met="2" type="+-">Vreesen <rhyme label="a" type="m" corresp="#s13:l1">Godt</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Veel met 's werelts lust <rhyme label="b" type="f">versellen</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Gaen den ruymen wegh die <rhyme label="c" type="m">leydt</rhyme></l>
        <l id="s13:l5" met="2" type="+-">(Christus <rhyme label="c" type="m" corresp="#s13:l4">seydt</rhyme>)</l>
        <l id="s13:l6" met="4" type="+-">Tot d'eeuwighe pijn der <rhyme label="b" type="f" corresp="#s13:l3">Hellen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
