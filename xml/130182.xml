<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>130182</id>
    <date>
      <year-start>1710</year-start>
      <year-end>1710</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="love song">liefdeslied</tag>
    </tags>
    <melody>Hele winterse nachten</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Och mijn Engel vol <rhyme label="a" type="f">waerde</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Mijn troost op der <rhyme label="a" type="f" corresp="#s1:l1">Aarde</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Gunt my het Ja <rhyme label="b" type="m">woord</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Mijn smeken <rhyme label="b" type="m" corresp="#s1:l3">verhoord</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Geeft mijn doch u <rhyme label="c" type="m">Trouw</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Of ik sterve van <rhyme label="c" type="m" corresp="#s1:l5">rouw</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Och help mijn, Och help <rhyme label="d" type="f">mijn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Over schoon <rhyme label="c" type="m" corresp="#s1:l5">Jonkvrouw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Heel Winterse <rhyme label="a" type="f">nachten</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Heb ik na u staen <rhyme label="a" type="f" corresp="#s2:l1">wachten</rhyme></l>
        <l id="s2:l3" met="2" type="-+">In den hagel ende <rhyme label="b" type="m">wind</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Mijn oogen <rhyme label="b" type="m" corresp="#s2:l3">verblind</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Mijn kaken bennen <rhyme label="c" type="m">blouw</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Begrieselt van de <rhyme label="c" type="m" corresp="#s2:l5">kouw</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">En zie, dat om uwent <rhyme label="d" type="f">wille</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Over schoon <rhyme label="c" type="m" corresp="#s2:l5">Jonkvrouw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">De Vensters en <rhyme label="a" type="f">deuren</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Die schijnen te <rhyme label="a" type="f" corresp="#s3:l1">scheuren</rhyme>;</l>
        <l id="s3:l3" met="2" type="-+">Sy tieren en zy <rhyme label="b" type="m">razen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">De vensters en de <rhyme label="b" type="m" corresp="#s3:l3">glazen</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">En, och Maegd zonder <rhyme label="c" type="m">genaed</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Uwen Dienaer die <rhyme label="c" type="m" corresp="#s3:l5">vergaet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">En die hier zoo alle <rhyme label="d" type="f">nachten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Voor u deurtje <rhyme label="c" type="m" corresp="#s3:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Aenschout eens mijn <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">- Hoe kond gy't <rhyme label="a" type="f" corresp="#s4:l1">gedogen</rhyme> -</l>
        <l id="s4:l3" met="2" type="-+">Die voor u <rhyme label="b" type="m">leecken</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Als twee <rhyme label="b" type="m" corresp="#s4:l3">water-beecken</rhyme>!</l>
        <l id="s4:l5" met="3" type="-+">Mijn Jeugt die <rhyme label="c" type="m">verdort</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Door de traenen die ik <rhyme label="c" type="m" corresp="#s4:l5">stort</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Maer gy, ô wrede <rhyme label="d" type="f">Vrouwe</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Niet beweegt en <rhyme label="c" type="m" corresp="#s4:l5">word</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">En al wou gy mijn <rhyme label="a" type="f">geven</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Een Koninks <rhyme label="a" type="f" corresp="#s5:l1">leven</rhyme></l>
        <l id="s5:l3" met="2" type="-+">En om u te <rhyme label="b" type="m">derven</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Veel liever wil ik <rhyme label="b" type="m" corresp="#s5:l3">sterven</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Als van u te zijn <rhyme label="c" type="m">ontbloot</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Mijn liefde is te <rhyme label="c" type="m" corresp="#s5:l5">groot</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">En och troost myn, En och troost <rhyme label="d" type="f">mijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Eer dat ik sterf de <rhyme label="c" type="m" corresp="#s5:l5">dood</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Mijn voeten en mijn <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Van liefde staeg <rhyme label="a" type="f" corresp="#s6:l1">branden</rhyme></l>
        <l id="s6:l3" met="2" type="-+">En mijn hert dat is <rhyme label="b" type="m">ontstelt</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Over mijn schoone <rhyme label="b" type="m" corresp="#s6:l3">Belt</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Want u oogen als <rhyme label="c" type="m">Christal</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">U stem verciert het <rhyme label="c" type="m" corresp="#s6:l5">al</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En montje net vol <rhyme label="d" type="f">presen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Met een zoete <rhyme label="c" type="m" corresp="#s6:l5">taal</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Mijn Lief, gy doet mijn <rhyme label="a" type="f">sterve</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Als ik niet <rhyme label="a" type="f" corresp="#s7:l1">beërve</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Uw blanke Borsjes <rhyme label="b" type="m">rond</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Met uwe zoete <rhyme label="b" type="m" corresp="#s7:l3">mond</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Uw Kaken <rhyme label="c" type="m">Lely-wit</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En uw Tanden boven <rhyme label="c" type="m" corresp="#s7:l5">dit</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Een Lichaam net <rhyme label="d" type="f">volpresen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Dat een Ziel <rhyme label="c" type="m" corresp="#s7:l5">bezit</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Juno sal <rhyme label="a" type="f">getuygen</rhyme>:</l>
        <l id="s8:l2" met="2" type="-+">De Godinnen moeten <rhyme label="a" type="f" corresp="#s8:l1">buygen</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Van uw volmaektheyt <rhyme label="b" type="m">schoon</rhyme></l>
        <l id="s8:l4" met="2" type="-+">Tot spijt van al de <rhyme label="b" type="m" corresp="#s8:l3">Goon</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Ey lieve, hoort mijn <rhyme label="c" type="m">klacht</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">Mijn Lief, ik word <rhyme label="c" type="m" corresp="#s8:l5">verkragt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">De Geest die moet ik <rhyme label="d" type="f">geven</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Cupido met mijn <rhyme label="c" type="m" corresp="#s8:l5">lacht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Met smeeken en <rhyme label="a" type="f">vleyen</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Kan ik niet <rhyme label="a" type="f" corresp="#s9:l1">bedeyen</rhyme>.</l>
        <l id="s9:l3" met="2" type="-+">Ik sterve door <rhyme label="b" type="m">smart</rhyme></l>
        <l id="s9:l4" met="2" type="-+">Van liefden in mijn <rhyme label="b" type="m" corresp="#s9:l3">hart</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Mijn leven is <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Na 't Graf zo moet ik <rhyme label="c" type="m" corresp="#s9:l5">gaen</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Adieu, mijn Lief <rhyme label="d" type="f">gepresen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Die als een Rots moet <rhyme label="c" type="m" corresp="#s9:l5">staen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Tot eer van myn <rhyme label="a" type="f">Beminde</rhyme></l>
        <l id="s10:l2" met="2" type="-+">Soo sal zy <rhyme label="a" type="f" corresp="#s10:l1">bevinden</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Hoe dat ik uyt een <rhyme label="b" type="m">steen</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Heb doen houwen <rhyme label="b" type="m" corresp="#s10:l3">alleen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Haar volmaakte leeden <rhyme label="c" type="m">net</rhyme></l>
        <l id="s10:l6" met="3" type="-+">En by mijn graf <rhyme label="c" type="m" corresp="#s10:l5">gezet</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Daer mijn dorre leden <rhyme label="d" type="f">rusten</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Als mijn Lief daer op <rhyme label="c" type="m" corresp="#s10:l5">let</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
