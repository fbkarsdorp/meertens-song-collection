        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>O Flora</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Johanna de Gavre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O gheluckighen <rhyme label="a" type="m">nacht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gheluck soo lanck <rhyme label="a" type="m" corresp="#s1:l1">verwacht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ons nu wordt <rhyme label="b" type="f">ghegheven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dit kindt groot van <rhyme label="a" type="m" corresp="#s1:l1">macht</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Waer d’Enghelen over <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Singhen met soet <rhyme label="c" type="m" corresp="#s1:l5">gheschal</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Gheluckigh is den <rhyme label="d" type="m">Mensch</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die vre soeckt boven <rhyme label="c" type="m" corresp="#s1:l5">al</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wonderlijck is dit <rhyme label="a" type="m">werck</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Soo ick t’wel <rhyme label="a" type="m" corresp="#s2:l1">aenmerck</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat een Maeght wordt <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Sonder pijn oft <rhyme label="a" type="m" corresp="#s2:l1">smert</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Waer om d’Enghelen <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Singhen met soet <rhyme label="c" type="m" corresp="#s2:l5">gheschal</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gheluckigh is den <rhyme label="d" type="m">Mensch</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die vre soeckt boven <rhyme label="c" type="m" corresp="#s2:l5">al</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">In Betlehem die <rhyme label="a" type="m">ste</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">T’kindt en de Moeder <rhyme label="a" type="m" corresp="#s3:l1">me</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Sullen wy gaen <rhyme label="b" type="f">soecken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Mijn Ziel weest te <rhyme label="a" type="m" corresp="#s3:l1">vre</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En hooren d’Enghelen <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Singhen met soet <rhyme label="c" type="m" corresp="#s3:l5">gheschal</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Gheluckigh is den <rhyme label="d" type="m">Mensch</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Die vre soeckt boven <rhyme label="c" type="m" corresp="#s3:l5">al</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O soete lieve <rhyme label="a" type="m">kindt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">T’welck mijn herte <rhyme label="a" type="m" corresp="#s4:l1">bemindt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hoe licht ghy hier <rhyme label="b" type="f">verworpen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Soo naeckt inden <rhyme label="a" type="m" corresp="#s4:l1">windt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy komt uyt s’Hemels <rhyme label="c" type="m">Hof</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">O kleyn kindt grooten <rhyme label="c" type="m" corresp="#s4:l5">Godt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wilt rusten onder <rhyme label="d" type="m">ons</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die niet en zijn dan <rhyme label="c" type="m" corresp="#s4:l5">stof</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt dan menschen <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Komen naer desen <rhyme label="a" type="m" corresp="#s5:l1">stal</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wy sullen daer <rhyme label="b" type="f">aenbidden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat kindt Godt van <rhyme label="a" type="m" corresp="#s5:l1">al</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En sien <rhyme label="c" type="m">d’oodtmoedigheydt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van dese <rhyme label="c" type="m" corresp="#s5:l5">Majesteyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die wordt op wat <rhyme label="d" type="m">stroy</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">In een Kribbe <rhyme label="c" type="m" corresp="#s5:l5">gheleydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
