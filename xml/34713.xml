    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MOedighe Nimphe van veele <rhyme label="a" type="f">vercoren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Sijdy tot mijne elende <rhyme label="a" type="f" corresp="#s1:l1">gheboren</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dat ick moet lijden dit swaer <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Dat ghy ghestadigh voor my <rhyme label="b" type="m" corresp="#s1:l3">vliet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Mijn jeught helaes moet gaen <rhyme label="a" type="f" corresp="#s1:l1">verloren</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Soo ghy my weygert u <rhyme label="b" type="m" corresp="#s1:l3">gheniet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Trodtst ghy Diana in suyvere <rhyme label="a" type="f">leden</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Daer om en hebdy in 't minste gheen <rhyme label="a" type="f" corresp="#s2:l1">reden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat ghy u thoonet soo straf en <rhyme label="b" type="m">stuer</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Sijt ghy een vrouwe van <rhyme label="b" type="m" corresp="#s2:l3">natuer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Soo thoont aen my oock vrouwe <rhyme label="a" type="f" corresp="#s2:l1">seden</rhyme></l>
        <l id="s2:l6" met="4" type="-+">En maeckt my 'tminnen niet soo <rhyme label="b" type="m" corresp="#s2:l3">suer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Anders soo souden mijn jeughdighe <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">d'Walen en dolen door t'vierigh <rhyme label="a" type="f" corresp="#s3:l1">beminnen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Want door de stralen van u <rhyme label="b" type="m">ghesicht</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Is in mijn ziel den Brandt <rhyme label="b" type="m" corresp="#s3:l3">ghesticht</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Soo dat ick niet en can <rhyme label="a" type="f" corresp="#s3:l1">beghinnen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Ten sy ghy mijnen druck <rhyme label="b" type="m" corresp="#s3:l3">verlicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sien ick u lieffelijck mondeken <rhyme label="a" type="f">blosen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Schoonder dan verssche ontluijckende <rhyme label="a" type="f" corresp="#s4:l1">roosen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">En veel soet dauwender als den <rhyme label="b" type="m">dach</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Met een seer vrindelijck <rhyme label="b" type="m" corresp="#s4:l3">ghelach</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dan denck ick t'gheen ick heb <rhyme label="a" type="f" corresp="#s4:l1">verkosen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Is weert dees tranen en <rhyme label="b" type="m" corresp="#s4:l3">gheclach</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sien ick u vlammende lichterkens <rhyme label="a" type="f">branden</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Druck ick u sachte sneeu-wittighe <rhyme label="a" type="f" corresp="#s5:l1">handen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Merck ick u blakende borstiens <rhyme label="b" type="m">ront</rhyme></l>
        <l id="s5:l4" met="4" type="-+">En u schoon hayr goutstralich <rhyme label="b" type="m" corresp="#s5:l3">blont</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Waer in mijn ziel met soete <rhyme label="a" type="f" corresp="#s5:l1">banden</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Leyt slavich tot der doodt <rhyme label="b" type="m" corresp="#s5:l3">ghewont</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Denckende mocht ick dees schoonheyt <rhyme label="a" type="f">genieten</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Lijden noch moeyte en sou my <rhyme label="a" type="f" corresp="#s6:l1">verdrieten</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Ick soude my als Leander <rhyme label="b" type="m">spoen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Om u ghetrouwen dienst te <rhyme label="b" type="m" corresp="#s6:l3">doen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Al waert mijn bloet oock te <rhyme label="a" type="f" corresp="#s6:l1">vergieten</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Als ick mocht crijghen eenen <rhyme label="b" type="m" corresp="#s6:l3">soen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Princerse laet my dees jonste <rhyme label="a" type="f">verwerven</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Anders soo moet ick al levende <rhyme label="a" type="f" corresp="#s7:l1">sterven</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Weyrt van u ziele dees droeve <rhyme label="b" type="m">schandt</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Doet uwe slaef eens <rhyme label="b" type="m" corresp="#s7:l3">onderstandt</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Wilt niet sijn ziel soo vroegh <rhyme label="a" type="f" corresp="#s7:l1">onterven</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Die stadich in u liefde <rhyme label="b" type="m" corresp="#s7:l3">Brandt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
