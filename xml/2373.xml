  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Laet myn ziele spreken <rhyme label="a" type="m">nu</rhyme> /</l>
        <l id="s1:l2" met="4" type="-+">Heere myn herte schinck ick <rhyme label="a" type="m" corresp="#s1:l1">u</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">Van traenen myn oogen <rhyme label="b" type="f">leken</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Aensiende myn sondige <rhyme label="b" type="f" corresp="#s1:l3">gebreken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">U gracie heere die in my <rhyme label="a" type="m">wracht</rhyme>/</l>
        <l id="s2:l2" met="4" type="-+">Heeft my tot kennissen <rhyme label="a" type="m" corresp="#s2:l1">ghebracht</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Daeromme wil ick u <rhyme label="b" type="f">loven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Myn opperste lief <rhyme label="b" type="f" corresp="#s2:l3">daerboven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nu ick ghenaede hebbe <rhyme label="a" type="m">ontfaen</rhyme>/</l>
        <l id="s3:l2" met="4" type="-+">Doet my doch voort in deuchden <rhyme label="a" type="m" corresp="#s3:l1">gaen</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Ende wilt my gracie <rhyme label="b" type="f">geven</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Om naer uwen wille te <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick wil u heere nu dancbaer <rhyme label="a" type="m">wesen</rhyme> /</l>
        <l id="s4:l2" met="4" type="-+">Dat ghij myn ziele hebt <rhyme label="a" type="m" corresp="#s4:l1">genesen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Door uwen crachtigen <rhyme label="b" type="f">bloede</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Neemptse in u <rhyme label="b" type="f" corresp="#s4:l3">behoede</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Int gheloove ende inden <rhyme label="a" type="m">gheest</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Wil ick u aenbidden <rhyme label="a" type="m" corresp="#s5:l1">aldermeest</rhyme> /</l>
        <l id="s5:l3" met="3" type="-+">Ende luysteren met myn <rhyme label="b" type="f">ooren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Om u Goddelyck woort te <rhyme label="b" type="f" corresp="#s5:l3">hooren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die dit Liedeken heeft <rhyme label="a" type="m">ghemaeckt</rhyme> /</l>
        <l id="s6:l2" met="4" type="-+">Haer selven heeft zy ierst <rhyme label="a" type="m" corresp="#s6:l1">gheraect</rhyme>/</l>
        <l id="s6:l3" met="3" type="-+">Daernae al die dit <rhyme label="b" type="f">lesen</rhyme> /</l>
        <l id="s6:l4" met="3" type="-+">Elck wille zijn ghebreken <rhyme label="b" type="f" corresp="#s6:l3">ghenesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
