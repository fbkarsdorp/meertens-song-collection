  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Hett jar doe ich ain oltt wieff <rhyme label="a" type="f">nam</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Sie was <rhyme label="b" type="m">berompen</rhyme>.</l>
        <l id="s1:l3" met="3" type="+-">Ich konndtt or nichtt <rhyme label="a" type="f" corresp="#s1:l1">ghelffen</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Der older <rhyme label="b" type="m" corresp="#s1:l2">trumpe</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ich gennck hen to <rhyme label="a" type="f">karchen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Ich klagde Got mien <rhyme label="b" type="m">noett</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Helptt, reicher Got fann <rhyme label="a" type="f" corresp="#s2:l1">hemel</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Wer datt oltt wieff <rhyme label="b" type="m" corresp="#s2:l2">doett</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Enn doe ich weder fan karchen <rhyme label="a" type="f">kwam</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Datt olde wieff was <rhyme label="b" type="m">doett</rhyme>!</l>
        <l id="s3:l3" met="3" type="+-">Help, reiche Gott fann <rhyme label="a" type="f" corresp="#s3:l1">hemel</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Verwonnen ist al mien <rhyme label="b" type="m" corresp="#s3:l2">no</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ich spande ann mienen <rhyme label="a" type="f">waegen</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Wol 25 <rhyme label="b" type="m">ros</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">Darmitt so gennck ich <rhyme label="a" type="f" corresp="#s4:l1">jagen</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Al nach den <rhyme label="b" type="m" corresp="#s4:l2">karchoff</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Enn doe ich auff den karchoff <rhyme label="a" type="f">kwam</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Datt graff <rhyme label="b" type="m">bereytt</rhyme>.</l>
        <l id="s5:l3" met="3" type="+-">Und soltt ich darumb <rhyme label="a" type="f" corresp="#s5:l1">truuren</rhyme>?</l>
        <l id="s5:l4" met="2" type="+-">Es was my jo niet <rhyme label="b" type="m" corresp="#s5:l2">leitt</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
