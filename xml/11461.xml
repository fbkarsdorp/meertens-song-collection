    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Het daagt in de oosten</melody>
    <categories>
      <category translation="miscellaneous">liefde en seks</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IN 't diepste van myn <rhyme label="a" type="f">dromen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Soo dochtme dat ick <rhyme label="b" type="m">sach</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Een Lecker tot my <rhyme label="a" type="f" corresp="#s1:l1">komen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Doch maeckten gheen <rhyme label="b" type="m" corresp="#s1:l2">gewach</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Het stelde nae mijn <rhyme label="c" type="f">wese</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Strackx sijn <rhyme label="c" type="f" corresp="#s1:l5">pesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn koetsje vloogh hy <rhyme label="a" type="f">over</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hy waende dat ick <rhyme label="b" type="m">sliep</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Och! docht ick, guytsche <rhyme label="a" type="f" corresp="#s2:l1">rover</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe wel ick 't niet en <rhyme label="b" type="m" corresp="#s2:l2">riep</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy swierde gins en <rhyme label="c" type="f">weder</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Op en <rhyme label="c" type="f" corresp="#s2:l5">neder</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy gingh in 't <rhyme label="a" type="f">achter-hoeckjen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">By 't groen gardyntjen <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hy kreech soo menigh <rhyme label="a" type="f" corresp="#s3:l1">vloeckjen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Doch t'zieltjen was <rhyme label="b" type="m" corresp="#s3:l2">belaen</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">'k Swoer by sijn moeders <rhyme label="c" type="f">banden</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Dat mijn <rhyme label="c" type="f" corresp="#s3:l5">handen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hem mochten eens <rhyme label="a" type="f">vermannen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ick sou dat kleyne <rhyme label="b" type="m">Wicht</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Sijn boochjen haest <rhyme label="a" type="f" corresp="#s4:l1">ontspannen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En breecken sijnen <rhyme label="b" type="m" corresp="#s4:l2">schicht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat hy om pijns <rhyme label="c" type="f">vernuwen</rhyme></l>
        <l id="s4:l6" met="2" type="+-">My sou <rhyme label="c" type="f" corresp="#s4:l5">schuwen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sijn vleugeltjens die <rhyme label="a" type="f">bloncken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Vergult op vreemde <rhyme label="b" type="m">aert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Het worde soo <rhyme label="a" type="f" corresp="#s5:l1">broo-droncken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">'t Heeft veel onrust <rhyme label="b" type="m" corresp="#s5:l2">gebaert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Dat ick eerst nae het <rhyme label="c" type="f">swetsen</rhyme></l>
        <l id="s5:l6" met="2" type="+-">Voelden 't <rhyme label="c" type="f" corresp="#s5:l5">quetsen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De borst waer my <rhyme label="a" type="f">inwendigh</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Niet anders als een <rhyme label="b" type="m">gloet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Doe schaterend' 't <rhyme label="a" type="f" corresp="#s6:l1">behendigh</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vloogh wegh, en riep: o <rhyme label="b" type="m" corresp="#s6:l2">bloet</rhyme>!</l>
        <l id="s6:l5" met="3" type="-+">Wilt ghy mijn noch wat <rhyme label="c" type="f">tarten</rhyme>?</l>
        <l id="s6:l6" met="2" type="+-">Voelt u <rhyme label="c" type="f" corresp="#s6:l5">smarten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Vaeck verliet mijn <rhyme label="a" type="f">Ooghen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mijn kamer ick <rhyme label="b" type="m">besocht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick vond my recht <rhyme label="a" type="f" corresp="#s7:l1">bedroghen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Maer niet in 't gheen <rhyme label="b" type="m" corresp="#s7:l2">gewrocht</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Was, in mijn borst <rhyme label="c" type="f">gedreven</rhyme></l>
        <l id="s7:l6" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s7:l5">gegeven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">'k En docht noyt om het <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">'k En dacht aen geen <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">'k En kon het niet <rhyme label="a" type="f" corresp="#s8:l1">versinnen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Waerom dit was <rhyme label="b" type="m" corresp="#s8:l2">geschiet</rhyme>?</l>
        <l id="s8:l5" met="3" type="-+">Doch 't ooghje gluurlijck <rhyme label="c" type="f">ooghde</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">Vaerdigh <rhyme label="c" type="f" corresp="#s8:l5">pooghde</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Te lieven een <rhyme label="a" type="f">Iuffrouwe</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Nae 't schynen een <rhyme label="b" type="m">Goddin</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ick kreegh voor druck en <rhyme label="a" type="f" corresp="#s9:l1">rouwe</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Een Meysjen nae mijn <rhyme label="b" type="m" corresp="#s9:l2">sin</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Die ick sal eeuwich <rhyme label="c" type="f">dienen</rhyme>,</l>
        <l id="s9:l6" met="2" type="+-">'t Hart <rhyme label="c" type="f" corresp="#s9:l5">verlienen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Cupido, niet <rhyme label="a" type="f">ghedencken</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Wilt mijn schempich <rhyme label="b" type="m">verwijt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">'k Sal u offrande <rhyme label="a" type="f" corresp="#s10:l1">schencken</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Heel kostlijck; als de <rhyme label="b" type="m" corresp="#s10:l2">tijt</rhyme></l>
        <l id="s10:l5" met="3" type="-+">My geeft eenich <rhyme label="c" type="f">beheering</rhyme></l>
        <l id="s10:l6" met="2" type="+-">In <rhyme label="c" type="f" corresp="#s10:l5">regeering</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
