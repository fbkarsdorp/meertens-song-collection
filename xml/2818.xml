  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Een maegh seer <rhyme label="a" type="m">schoen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">hout my In haer <rhyme label="b" type="m">bestier</rhyme></l>
        <l id="s1:l3" met="2" type="-+">fraey van <rhyme label="a" type="m" corresp="#s1:l1">persoen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">en oeck fraey van <rhyme label="b" type="m" corresp="#s1:l2">manier</rhyme></l>
        <l id="s1:l5" met="2" type="-+">haer aenschin <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">schint my te weesen <rhyme label="d" type="m">klaer</rhyme></l>
        <l id="s1:l7" met="2" type="-+">fraey van <rhyme label="c" type="m" corresp="#s1:l5">gemoet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">seer klouck ende <rhyme label="d" type="m" corresp="#s1:l6">eerbaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Guibydoe <rhyme label="a" type="m">fel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">heeft myn Ionck hert <rhyme label="b" type="m">duerwont</rhyme></l>
        <l id="s2:l3" met="2" type="-+">en sy seer <rhyme label="a" type="m" corresp="#s2:l1">wel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">kan maeke myn <rhyme label="b" type="m" corresp="#s2:l2">gesont</rhyme></l>
        <l id="s2:l5" met="2" type="-+">als Iunoe <rhyme label="c" type="m">schoen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">soe Is sy oeck <rhyme label="d" type="m">bekent</rhyme></l>
        <l id="s2:l7" met="2" type="-+">In tscheemels <rhyme label="c" type="m" corresp="#s2:l5">troen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">daer liefde Is <rhyme label="d" type="m" corresp="#s2:l6">tfondaement</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Haer kaeckens <rhyme label="a" type="m">roet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">haer oechiens swart <rhyme label="b" type="m">koerael</rhyme></l>
        <l id="s3:l3" met="2" type="-+">haer borsies <rhyme label="a" type="m" corresp="#s3:l1">bloet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">voerhuecht het <rhyme label="b" type="m" corresp="#s3:l2">altemael</rhyme></l>
        <l id="s3:l5" met="2" type="-+">och mocht Ick <rhyme label="c" type="m">koen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">eens weesen haer <rhyme label="d" type="m">party</rhyme></l>
        <l id="s3:l7" met="2" type="-+">In dit <rhyme label="c" type="m" corresp="#s3:l5">saesoen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">twaer myn een <rhyme label="d" type="m" corresp="#s3:l6">meloedy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Soe waer myn <rhyme label="a" type="m">smert</rhyme></l>
        <l id="s4:l2" met="3" type="-+">voerkeert In meeder <rhyme label="b" type="m">geluck</rhyme></l>
        <l id="s4:l3" met="2" type="-+">en myn hert <rhyme label="a" type="m" corresp="#s4:l1">beschermt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">voer meerder <rhyme label="b" type="m" corresp="#s4:l2">druck</rhyme></l>
        <l id="s4:l5" met="2" type="-+">maer sy en acht <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">alle myn lyde <rhyme label="d" type="m">groet</rhyme></l>
        <l id="s4:l7" met="2" type="-+">duer swaer <rhyme label="c" type="m" corresp="#s4:l5">voerdriet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">moet Ick sterue den <rhyme label="d" type="m" corresp="#s4:l6">doet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Prinsesse <rhyme label="a" type="m">fier</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Laet u geebeeden <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s5:l3" met="2" type="-+">uwen prins <rhyme label="a" type="m" corresp="#s5:l1">hier</rhyme></l>
        <l id="s5:l4" met="3" type="-+">te helpen uit syn <rhyme label="b" type="m" corresp="#s5:l2">pyn</rhyme></l>
        <l id="s5:l5" met="2" type="-+">laet u <rhyme label="c" type="m">hoechmoet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">sincken toch eens Int <rhyme label="d" type="m">ent</rhyme></l>
        <l id="s5:l7" met="2" type="-+">siet wat gy <rhyme label="c" type="m" corresp="#s5:l5">doet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">teegen u lief <rhyme label="d" type="m" corresp="#s5:l6">bekent</rhyme>.</l>
      </lg>
    </lg>
  </text>
