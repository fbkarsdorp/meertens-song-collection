  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Onder allen vrouwen vijnt men <rhyme label="a" type="m">gheen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Mijns liefs <rhyme label="b" type="m">ghelijck</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Och mocht my heyl van Haer <rhyme label="a" type="m" corresp="#s1:l1">schyen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Soe waer ic vrolic <rhyme label="b" type="m" corresp="#s1:l2">ewelijc</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">O edel Schoen, int hemels <rhyme label="c" type="m">throon</rhyme></l>
        <l id="s1:l6" met="4" type="-+">draechstu die croon, van gouden <rhyme label="d" type="m">fijn</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Dijn dienre wil ic <rhyme label="d" type="m" corresp="#s1:l6">sijn</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O, ghepeyns van Haer heeft my <rhyme label="a" type="m">bevanghen</rhyme>!</l>
        <l id="s2:l2" met="4" type="-+">Van Haer doechden wil ic <rhyme label="b" type="m">scriven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Mit recht mach my nae Haer <rhyme label="a" type="m" corresp="#s2:l1">verlanghen</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Graci heeft Sy, boven alle <rhyme label="b" type="m" corresp="#s2:l2">wiven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Enghel <rhyme label="c" type="m">Gabriel</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">doe hij brocht snel die boetscap <rhyme label="d" type="m">fijn</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Dijn dienre wil ic <rhyme label="d" type="m" corresp="#s2:l6">sijn</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Alre bloemen <rhyme label="a" type="m">Spieghelglas</rhyme>:</l>
        <l id="s3:l2" met="4" type="-+">Uut Dij soe sproot een edel <rhyme label="b" type="m">greyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die beste vrucht die ye <rhyme label="a" type="m" corresp="#s3:l1">was</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Hadstu bevaen in Dij <rhyme label="b" type="m" corresp="#s3:l2">certeyn</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">O edel Goet, o balsem <rhyme label="c" type="m">Soet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Du geefste moet dat herte <rhyme label="d" type="m">mijn</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Dijn dienre wil ic <rhyme label="d" type="m" corresp="#s3:l6">sijn</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Lichter dan die sterren <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Schijnt ons die son int <rhyme label="b" type="m">openbaer</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Noch claere is Sij sonder <rhyme label="a" type="m" corresp="#s4:l1">ghetal</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Dat doet die Schrift ons wel <rhyme label="b" type="m" corresp="#s4:l2">ghewaer</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">O grote Licht, dat niet en <rhyme label="c" type="m">sicht</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">an Dij soe hicht ic myn <rhyme label="d" type="m">termijn</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Dijn dienre wil ic <rhyme label="d" type="m" corresp="#s4:l6">sijn</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Lelij wit, reyn sonder <rhyme label="a" type="m">smit</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gheen beter bloem men ye en <rhyme label="b" type="m">vant</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Want Ghij stadich sit, voer ons <rhyme label="a" type="m" corresp="#s5:l1">bidt</rhyme>;</l>
        <l id="s5:l4" met="4" type="-+">Des moet wij allen sijn <rhyme label="b" type="m" corresp="#s5:l2">becant</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Ende dancken U seer, mit grooter <rhyme label="c" type="m">gheer</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">mit Onsen Heer U <rhyme label="d" type="m">kijndekijn</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">U dienre wil ic <rhyme label="d" type="m" corresp="#s5:l6">sijn</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Arke van Noe, o kostel <rhyme label="a" type="m">Vat</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Van onsen alre <rhyme label="b" type="m">behoudenisse</rhyme>!</l>
        <l id="s6:l3" met="4" type="-+">Doer Dij hebben wij vercreghen te <rhyme label="a" type="m" corresp="#s6:l1">bat</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Dat onse scult vergouden <rhyme label="b" type="m" corresp="#s6:l2">is</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">O onse Hoep, tot Dij ic <rhyme label="c" type="m">loop</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">'ghenade!' ic roep, schoen <rhyme label="d" type="m">Vrouwe</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Mijn:Dijn dienre wil ic <rhyme label="d" type="m" corresp="#s6:l6">sijn</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Riker Vrou, wilt niet <rhyme label="a" type="m">vergheten</rhyme></l>
        <l id="s7:l2" met="4" type="-+">U arme dienre in sijn <rhyme label="b" type="m">noot</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">Want ic myn tijt heb qualijc <rhyme label="a" type="m" corresp="#s7:l1">versleten</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Des mach ic claghen jammer <rhyme label="b" type="m" corresp="#s7:l2">groot</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Vol van ghenaden, staet my in <rhyme label="c" type="m">staden</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">wilt my ontladen van <rhyme label="d" type="m">allenpijn</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Dijn dienre wil ic <rhyme label="d" type="m" corresp="#s7:l6">sijn</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Te recht is Dij groot lof <rhyme label="a" type="m">bereijt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Schoen Koningine van allen <rhyme label="b" type="m">rijken</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Want Du hebste in Dijn scoot <rhyme label="a" type="m" corresp="#s8:l1">gheleyt</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Die al die werelt niet en mach <rhyme label="b" type="m" corresp="#s8:l2">begrijpen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">O soet van smake, laet Dij <rhyme label="c" type="m">ghenaken</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">die dit maken, een <rhyme label="d" type="m">cranselijn</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Dijn dienre wil ic <rhyme label="d" type="m" corresp="#s8:l6">sijn</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
