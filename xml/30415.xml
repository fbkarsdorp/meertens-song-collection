<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>30415</id>
    <date>
      <year-start>1576</year-start>
      <year-end>1600</year-end>
    </date>
    <location>
      <orginal-name>tongeren</orginal-name>
      <standard-name>Tongeren, Belgium</standard-name>
      <coordinates>
        <lon>50.7810151</lon>
        <lat>5.4648131</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>O Venus der minne godin</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Venus der minnen <rhyme label="a" type="f">goddinne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wat hebdij nu in uwen <rhyme label="a" type="f" corresp="#s1:l1">sinne</rhyme></l>
        <l id="s1:l3" met="3" type="-+">dat ick die ick <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme></l>
        <l id="s1:l4" met="2" type="-+">niet en <rhyme label="a" type="f" corresp="#s1:l1">gewinne</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Cupido god <rhyme label="a" type="f">loyale</rhyme></l>
        <l id="s2:l2" met="3" type="-+">haest v met uwen minnen <rhyme label="a" type="f" corresp="#s2:l1">strale</rhyme></l>
        <l id="s2:l3" met="3" type="-+">soe waer ic alte <rhyme label="a" type="f" corresp="#s2:l1">male</rhyme></l>
        <l id="s2:l4" met="2" type="-+">beurijt van <rhyme label="a" type="f" corresp="#s2:l1">quale</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy heeft myn ionck hertken <rhyme label="a" type="f">beuanghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">na haer staet alle mijn <rhyme label="a" type="f" corresp="#s3:l1">verlangen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">dus singhe ic drouuen <rhyme label="a" type="f" corresp="#s3:l1">sangen</rhyme></l>
        <l id="s3:l4" met="2" type="-+">lieff <rhyme label="a" type="f" corresp="#s3:l1">onbeuanghe</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Schoen lieff aenhoort myn bitter <rhyme label="a" type="f">kermen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">wilt in genaden mijns <rhyme label="a" type="f" corresp="#s4:l1">onfermen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">in uwen smertsen <rhyme label="a" type="f" corresp="#s4:l1">eermen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">wilt my <rhyme label="a" type="f" corresp="#s4:l1">verwermen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">wilt my met uwen troest <rhyme label="a" type="f">bedouwen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">op v staet alle myn <rhyme label="a" type="f" corresp="#s5:l1">betrouwen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ghy syt die fleur van <rhyme label="a" type="f" corresp="#s5:l1">vrouwen</rhyme></l>
        <l id="s5:l4" met="2" type="-+">lieff int <rhyme label="a" type="f" corresp="#s5:l1">aenschouwen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
