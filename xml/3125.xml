    <melody>Wanneer de zon zijn paarden ment</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VVAnneer de Son sijn Paerden <rhyme label="a" type="m">Ment</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wt Oosten West-waert <rhyme label="b" type="m">op</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">En als sijn Jacht in 't Zuyen <rhyme label="a" type="m" corresp="#s1:l1">rent</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dan zweeter Ydaes <rhyme label="b" type="m" corresp="#s1:l2">top</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Dan stilt de loop der <rhyme label="c" type="f">revieren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dan waeyen geen loov'ren <rhyme label="d" type="m">of</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Dan gaender de Nimphjes <rhyme label="c" type="f" corresp="#s1:l5">plaeysieren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In 't schauduw' van 't Lommere <rhyme label="d" type="m" corresp="#s1:l6">Lof</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Soo beurden 't op een <rhyme label="a" type="m">midden-dagh</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Op 't heetste van de <rhyme label="b" type="m">Son</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat ick mijn dorst te lesschen <rhyme label="a" type="m" corresp="#s2:l1">sagh</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wt Beeck, of koele <rhyme label="b" type="m" corresp="#s2:l2">Bron</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">So hoord' ick in 't lommer der <rhyme label="c" type="f">Boompjes</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Het ruysen van een <rhyme label="d" type="m">Fonteyn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ick spoeyde my na de <rhyme label="c" type="f" corresp="#s2:l5">stroompjes</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Daer vond ick een Nimphjen <rhyme label="d" type="m" corresp="#s2:l6">alleyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die sadt met hare Leden <rhyme label="a" type="m">naeckt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Om spoelen in de <rhyme label="b" type="m">Vloedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En 't scheen sy was in slaep <rhyme label="a" type="m" corresp="#s3:l1">geraeckt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat docht my och! soo <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Want geene Diana <rhyme label="c" type="f">g'leeckse</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Noch ick geen <rhyme label="d" type="m">Actoen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">'k Bleef stille staen en <rhyme label="c" type="f" corresp="#s3:l5">bekeeckse</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Veel vreughden dat ick <rhyme label="d" type="m" corresp="#s3:l6">bevon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daer sag ick aen haer gout-geel <rhyme label="a" type="m">haer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">'t Geen krinckelde krullent <rhyme label="b" type="m">vast</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Haer voor-hooft als een <rhyme label="a" type="m" corresp="#s4:l1">pronck-Altaer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Soo wit als een <rhyme label="b" type="m" corresp="#s4:l2">Albast</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Haer Neusje soo net <rhyme label="c" type="f">besneden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Haer Mondetjen bloosen <rhyme label="d" type="m">root</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Haer Kaeckjens en andere <rhyme label="c" type="f" corresp="#s4:l5">Leden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Die maeckten my levende <rhyme label="d" type="m" corresp="#s4:l6">doot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Noch sagh ick dit <rhyme label="a" type="m">Godinnetje</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van Lidt tot Lidt in <rhyme label="b" type="m">als</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Haer ront-gekloofde <rhyme label="a" type="m" corresp="#s5:l1">Kinnetje</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Haer poesel blancken <rhyme label="b" type="m" corresp="#s5:l2">Hals</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Doen sagh ick noch wat <rhyme label="c" type="f">lager</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ick wasser tot kijcken <rhyme label="d" type="m">gesint</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Mijn lusjes die worden veel <rhyme label="c" type="f" corresp="#s5:l5">grager</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En worden door 't kijcken schier <rhyme label="d" type="m" corresp="#s5:l6">blindt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doch voort ick op haer Boesen <rhyme label="a" type="m">sagh</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Twee Memmetjes, wit en <rhyme label="b" type="m">bol</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Doorstraelt met blauwe Aders, <rhyme label="a" type="m" corresp="#s6:l1">ach</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Met Melck, en, Honingh <rhyme label="b" type="m" corresp="#s6:l2">vol</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Haer Tepeltjes <rhyme label="c" type="f">root-koraligh</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Daer drupten den Nectar <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">O Gooden! wie isser soo <rhyme label="c" type="f" corresp="#s6:l5">saligh</rhyme></l>
        <l id="s6:l8" met="3" type="-+">'t Genieten van sulcken <rhyme label="d" type="m" corresp="#s6:l6">Bruyt</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Mijn Oogh dat mijn tot Minnen <rhyme label="a" type="m">dreef</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat sagh haer Buyckje <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En straelde na de <rhyme label="a" type="m" corresp="#s7:l1">Middel-schreef</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Daer vont ick noch wat <rhyme label="b" type="m" corresp="#s7:l2">staen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Dat was: ick en druft niet <rhyme label="c" type="f">segghen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En 't singen dat is <rhyme label="d" type="m">gemeen</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">Ick gingh my wat by haer <rhyme label="c" type="f" corresp="#s7:l5">legghen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Doen waecktense soo het <rhyme label="d" type="m" corresp="#s7:l6">scheen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maer ick omhelsden haer <rhyme label="a" type="m">terstont</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En greep haer in mijn <rhyme label="b" type="m">Arm</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">En kusten daer de lieve <rhyme label="a" type="m" corresp="#s8:l1">Mondt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Cupido blies <rhyme label="b" type="m" corresp="#s8:l2">allarm</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Haer bruyne Oogjens die <rhyme label="c" type="f">lonckte</rhyme></l>
        <l id="s8:l6" met="3" type="-+">En sagen my lachende <rhyme label="d" type="m">aen</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Mijn bloedt in Aders <rhyme label="c" type="f" corresp="#s8:l5">ontfonkte</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Een suchtje heeft syder <rhyme label="d" type="m" corresp="#s8:l6">gedaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
