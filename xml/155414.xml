<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155414</id>
    <date>
      <year-start>1700</year-start>
      <year-end>1700</year-end>
    </date>
    <tags/>
    <melody>De Vlaamse meisjes</melody>
  <author>Pieter de Vos</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Celedon schoon <rhyme label="a" type="m">Jonkvrou</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Waer drijft gy uwe <rhyme label="b" type="f">Schaepjes</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Soo vroeg al door den <rhyme label="a" type="m" corresp="#s1:l1">dou</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ey dog een weynig stille <rhyme label="c" type="m">houd</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Want soo vroeg in den <rhyme label="d" type="m">morgen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Js het nog te <rhyme label="c" type="m" corresp="#s1:l4">kout</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wel Harder ik wil mijn <rhyme label="a" type="m">spoen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Om mijn gewolde <rhyme label="b" type="f">Schaepjes</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Jn 't grazig veld te <rhyme label="a" type="m" corresp="#s2:l1">voen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Het Vee dat is geheel <rhyme label="c" type="m">verblijd</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Nu het is van de <rhyme label="d" type="m">stal</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Jn dese Lente <rhyme label="c" type="m" corresp="#s2:l4">tijd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wel Celedon ik <rhyme label="a" type="m">sal</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mijn vee van stal gaen <rhyme label="b" type="f">drijven</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ook in het Groene <rhyme label="a" type="m" corresp="#s3:l1">dal</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Mijn overschoonste <rhyme label="c" type="m">Herderin</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Laet ons te samen gaen <rhyme label="d" type="m">treden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Soo te Velden <rhyme label="c" type="m" corresp="#s3:l4">in</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Lisander ik ben <rhyme label="a" type="m">bereyd</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Om met u te <rhyme label="b" type="f">spanseeren</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Jn 't Jeugdig veld <rhyme label="a" type="m" corresp="#s4:l1">Tapijt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ons vee dat hippelt en dat <rhyme label="c" type="m">springt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Terwijl het pluym <rhyme label="d" type="m">gevogelt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Soet van vreugde <rhyme label="c" type="m" corresp="#s4:l4">zingt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn liefste <rhyme label="a" type="m">Celedon</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En Harders kind <rhyme label="b" type="f">geboren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Laet ons by dese <rhyme label="a" type="m" corresp="#s5:l1">bron</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Rusten, en spreken van de <rhyme label="c" type="m">Min</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En Liefde, <rhyme label="d" type="m">uytverkoren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Schoonste <rhyme label="c" type="m" corresp="#s5:l4">Harderin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O Harder u <rhyme label="a" type="m">gevley</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van minne en Lief <rhyme label="b" type="f">koosen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat steekt vol <rhyme label="a" type="m" corresp="#s6:l1">veynsery</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Gelijk de steedse knapen <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Door schoone woorden <rhyme label="d" type="m">brengen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">meenig maegt ten <rhyme label="c" type="m" corresp="#s6:l4">val</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Eer mijn getrouwe <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Jn veynse sal <rhyme label="b" type="f">verkeeren</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">O schoonste <rhyme label="a" type="m" corresp="#s7:l1">Harderin</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Eer sal de Sterre, Son en <rhyme label="c" type="m">Maen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ja Hemel en <rhyme label="d" type="m">aerden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Door het vier <rhyme label="c" type="m" corresp="#s7:l4">vergaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Lisander wilt u <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met Celedon nu <rhyme label="b" type="f">boeten</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Jk ben u <rhyme label="a" type="m" corresp="#s8:l1">Harderin</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En gy mijn Harder soet van <rhyme label="c" type="m">aert</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Jk sal u min <rhyme label="d" type="m">beloonen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Laet ons zijn <rhyme label="c" type="m" corresp="#s8:l4">gepaert</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Mijn Schaepjes weest <rhyme label="a" type="m">verheugd</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En vrolijk met u <rhyme label="b" type="f">Harder</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Vermengelt u met <rhyme label="a" type="m" corresp="#s9:l1">vreugd</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Onder 't Vee van mijn <rhyme label="c" type="m">Harderin</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Celedon en <rhyme label="d" type="m">Lisander</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Leven saem door <rhyme label="c" type="m" corresp="#s9:l4">Min</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Mijn Harder ik sal seer <rhyme label="a" type="m">net</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">U Jeugdig hooft <rhyme label="b" type="f">vercieren</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Met Narcis en <rhyme label="a" type="m" corresp="#s10:l1">Phiolet</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">mijn Schaepjes juygd de vreugde <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Nou dat u <rhyme label="d" type="m">Harderinne</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Js een Harders <rhyme label="c" type="m" corresp="#s10:l4">Bruyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
