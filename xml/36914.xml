  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">O Myn alder <rhyme label="a" type="f">schoonste</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Schoonheydts schoonste <rhyme label="a" type="f" corresp="#s1:l1">woonste</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Uwen <rhyme label="b" type="m">Roosen-mondt</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Heeft myn hert' <rhyme label="b" type="m" corresp="#s1:l3">ghewondt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Uw tipkens’ Uw <rhyme label="c" type="f">lipkens</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Uw ivoore <rhyme label="d" type="f">tanden</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Uw <rhyme label="e" type="m">coraletongh'</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Ded' m'in liefde <rhyme label="d" type="f" corresp="#s1:l6">branden</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Doen sy lest-mael <rhyme label="e" type="m" corresp="#s1:l7">songh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Als ghy uw' <rhyme label="a" type="f">ivooren</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Stemmeken laet <rhyme label="a" type="f" corresp="#s2:l1">hooren</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Swyght den <rhyme label="b" type="m">Nachtegael</rhyme></l>
        <l id="s2:l4" met="3" type="+-">En hoort naer uw' <rhyme label="b" type="m" corresp="#s2:l3">[...]ael</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Uw' reden’ Uw' <rhyme label="c" type="f">zeden</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Uw' Deught weert om <rhyme label="d" type="f">loven</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">En uw' <rhyme label="e" type="m">eerbaerheydt</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Gaen noch verr' te <rhyme label="d" type="f" corresp="#s2:l6">boven</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Uw' <rhyme label="e" type="m" corresp="#s2:l7">bevalligheydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Schoon Hair en wel <rhyme label="a" type="f">singhen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Zyn twee broose <rhyme label="a" type="f" corresp="#s3:l1">dinghen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Schoonheydt die <rhyme label="b" type="m">vergaet</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Maer de Deught die <rhyme label="b" type="m" corresp="#s3:l3">staet</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Myn' sinnen’ <rhyme label="c" type="f">Beminnen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Uw' Deugt die ten thoon <rhyme label="d" type="f">leyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">En veel schoonder <rhyme label="e" type="m">thoont</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Als sy in de <rhyme label="d" type="f" corresp="#s3:l6">schoonheydt</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Van schoon leden <rhyme label="e" type="m" corresp="#s3:l7">woont</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
