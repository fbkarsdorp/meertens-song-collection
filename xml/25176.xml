  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Durch liebe manger wirt zu <rhyme label="a" type="f">kinde</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">der doch der jare is <rhyme label="b" type="m">kund</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">was ich der selber kinde <rhyme label="a" type="f" corresp="#s1:l1">vinde</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">die durch die liebe dore <rhyme label="b" type="m" corresp="#s1:l2">sint</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">durch liebe wirt des vil <rhyme label="c" type="m">gedain</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">das sele inde ere beide <rhyme label="d" type="f">krenkit</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">wie sie durch liebe in sunden <rhyme label="d" type="f" corresp="#s1:l6">senkit</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">der mois durch liebe lieber <rhyme label="c" type="m" corresp="#s1:l5">lain</rhyme>.</l>
      </lg>
    </lg>
