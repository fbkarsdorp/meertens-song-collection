<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184370</id>
    <date>
      <year-start>1785</year-start>
      <year-end>1785</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Malbrough s'en va-t-en guerre</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Daar heeft de klok <rhyme label="a" type="f">geslagen</rhyme></l>
        <l id="s1:l2" met="2" type="+-">
          <rhyme label="b" type="m">Mironton</rhyme>
        </l>
        <l id="s1:l3" met="3" type="-+">Daar heeft de klok <rhyme label="a" type="f" corresp="#s1:l1">geslagen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">De Bengel luidt wy <rhyme label="c" type="m">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Myn paardje! gaa nu <rhyme label="a" type="f">stappen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Myn paardje! gaa nu <rhyme label="a" type="f" corresp="#s2:l1">stappen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Terwyl uw jaager <rhyme label="c" type="m">zingt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Men denkt dat aan dit <rhyme label="a" type="f">jaagen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Men denkt dat aan dit <rhyme label="a" type="f" corresp="#s3:l1">jaagen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Niet veel gelegen <rhyme label="c" type="m">is</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">En 'k wil ook wel <rhyme label="a" type="f">bekennen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">En 'k wil ook wel <rhyme label="a" type="f" corresp="#s4:l1">bekennen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat het niet moeilyk <rhyme label="c" type="m">is</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maar zyn myn winsten <rhyme label="a" type="f">mager</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Maar zyn myn winsten <rhyme label="a" type="f" corresp="#s5:l1">mager</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ik pas toch op myn <rhyme label="c" type="m">zaak</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">'k Geef ieder een <rhyme label="a" type="f">genoegen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">'k Geef ieder een <rhyme label="a" type="f" corresp="#s6:l1">genoegen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Zo veel my meuglyk <rhyme label="c" type="m">is</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Zou ik de menschen <rhyme label="a" type="f">plaagen</rhyme></l>
        <l id="s7:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Zou ik de menschen <rhyme label="a" type="f" corresp="#s7:l1">plaagen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Door traag te ryden, <rhyme label="c" type="m">neen</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maar 't beest ook of te <rhyme label="a" type="f">beulen</rhyme></l>
        <l id="s8:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Maar 't beest ook of te <rhyme label="a" type="f" corresp="#s8:l1">beulen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Om een geringe <rhyme label="c" type="m">fooi</rhyme>;</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dat 's niet van my te <rhyme label="a" type="f">krygen</rhyme></l>
        <l id="s9:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s9:l3" met="3" type="-+">Dat 's niet van my te <rhyme label="a" type="f" corresp="#s9:l1">krygen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">En niemand haat my <rhyme label="c" type="m">toch</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">By menschen en by <rhyme label="a" type="f">beesten</rhyme></l>
        <l id="s10:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">By menschen en by <rhyme label="a" type="f" corresp="#s10:l1">beesten</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Is kleine Hein <rhyme label="c" type="m">gezien</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Als ik een paard kom <rhyme label="a" type="f">haalen</rhyme>,</l>
        <l id="s11:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Als ik een paard kom <rhyme label="a" type="f" corresp="#s11:l1">haalen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Wil ieder met my <rhyme label="c" type="m">mee</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">In 't land dan loopen ze <rhyme label="a" type="f">allen</rhyme></l>
        <l id="s12:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s12:l3" met="3" type="-+">In 't land dan loopen ze <rhyme label="a" type="f" corresp="#s12:l1">allen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">My krek als hondjes <rhyme label="c" type="m">naa</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">En spreek ik van de <rhyme label="a" type="f">menschen</rhyme>,</l>
        <l id="s13:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s13:l3" met="3" type="-+">En spreek ik van de <rhyme label="a" type="f" corresp="#s13:l1">menschen</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Die houden ook van <rhyme label="c" type="m">my</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Is dit die goeye <rhyme label="a" type="f">jaager</rhyme>?</l>
        <l id="s14:l2" met="2" type="+-">
          <rhyme label="b" type="m">Mironton</rhyme>
        </l>
        <l id="s14:l3" met="3" type="-+">Is dit die goeye <rhyme label="a" type="f" corresp="#s14:l1">jaager</rhyme>?</l>
        <l id="s14:l4" met="3" type="-+">Zoo vroeg 'er strak een <rhyme label="c" type="m">kind</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Jaa! dat 's die braave <rhyme label="a" type="f">jongen</rhyme>.</l>
        <l id="s15:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s15:l3" met="3" type="-+">Jaa! dat 's die braave <rhyme label="a" type="f" corresp="#s15:l1">jongen</rhyme>.</l>
        <l id="s15:l4" met="3" type="-+">Was 't antwoord van een <rhyme label="c" type="m">vrouw</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Toen sprong ik als een <rhyme label="a" type="f">Ruiter</rhyme>;</l>
        <l id="s16:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">Toen sprong ik als een <rhyme label="a" type="f" corresp="#s16:l1">Ruiter</rhyme>;</l>
        <l id="s16:l4" met="3" type="-+">Zo moedig op myn <rhyme label="c" type="m">paard</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Dat zel van daag weêr <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s17:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s17:l3" met="3" type="-+">Dat zel van daag weêr <rhyme label="a" type="f" corresp="#s17:l1">wezen</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">De poen in Heintjes <rhyme label="c" type="m">zak</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Wat zel myn moeder <rhyme label="a" type="f">lachen</rhyme>,</l>
        <l id="s18:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s18:l3" met="3" type="-+">Wat zel myn moeder <rhyme label="a" type="f" corresp="#s18:l1">lachen</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Als zy myn winst weêr <rhyme label="c" type="m">krygt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Maar lang genoeg <rhyme label="a" type="f">gezongen</rhyme>,</l>
        <l id="s19:l2" met="2" type="+-"><rhyme label="b" type="m">Mironton</rhyme>.</l>
        <l id="s19:l3" met="3" type="-+">Maar lang genoeg <rhyme label="a" type="f" corresp="#s19:l1">gezongen</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Wy zyn aan 't tolhek <rhyme label="c" type="m">reeds</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
