    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Le printemps rappelle aux armes</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laat ons Dames, laat ons <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s1:l2" met="2" type="-+">De Borst nu <rhyme label="a" type="f" corresp="#s1:l1">smeeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Laat ons ook eens <rhyme label="a" type="f" corresp="#s1:l1">kwinkeleeren</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">'t Past by de <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Wy moeten ons <rhyme label="a" type="f" corresp="#s1:l1">Diverteeren</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Als wy by elkander <rhyme label="b" type="m" corresp="#s1:l4">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ieder vult dan eens zyn <rhyme label="a" type="f">Glaasje</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Met goede <rhyme label="a" type="f" corresp="#s2:l1">gratie</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ieder vult dan eens zyn <rhyme label="a" type="f" corresp="#s2:l1">Glaasje</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Met koele <rhyme label="b" type="m">Wyn</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Zonder langer <rhyme label="a" type="f" corresp="#s2:l1">Protestatie</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Moet dit Glas geledigt <rhyme label="b" type="m" corresp="#s2:l4">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want het geld onze <rhyme label="a" type="f">Inclinatie</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Wiens schoone <rhyme label="a" type="f" corresp="#s3:l1">facie</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Meriteerd <rhyme label="a" type="f" corresp="#s3:l1">d'Evacuatie</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Van een vol <rhyme label="b" type="m">Glas</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Hebt gy voor haar <rhyme label="a" type="f" corresp="#s3:l1">Veneratie</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Heeren leegt het dan wat <rhyme label="b" type="m" corresp="#s3:l4">ras</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wat een zoete <rhyme label="a" type="f">alteratie</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Geeft my dat <rhyme label="a" type="f" corresp="#s4:l1">Glaasje</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ik voel dat het myn <rhyme label="a" type="f" corresp="#s4:l1">Inclinatie</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Wel deeglyk <rhyme label="b" type="m">geld</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Ik voel een zoete <rhyme label="a" type="f" corresp="#s4:l1">recreatie</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die my zulks inwendig <rhyme label="b" type="m" corresp="#s4:l4">meld</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Als ik U een Kus mag <rhyme label="a" type="f">geeven</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Myn tweede <rhyme label="a" type="f" corresp="#s5:l1">Leeven</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als ik U een Kus mag <rhyme label="a" type="f" corresp="#s5:l1">geeven</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Ben ik <rhyme label="b" type="m">verheugd</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Wil hier dan niet <rhyme label="a" type="f" corresp="#s5:l1">tegenstreeven</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Anders stoort gy wis myn <rhyme label="b" type="m" corresp="#s5:l4">vreugdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
