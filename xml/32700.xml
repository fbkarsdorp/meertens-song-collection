    <melody>Een kindje is ons geboren</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Een kindeken is ons geboren,, Godt lof en <rhyme label="a" type="m">danck</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="a" type="m" corresp="#s1:l1">sanck</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">TE Nazareth was <rhyme label="b" type="f">besloten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dees bloem in haeren <rhyme label="c" type="m">bodt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die nu is <rhyme label="b" type="f" corresp="#s1:l3">uytghesproten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Seer schoon verciert van <rhyme label="c" type="m" corresp="#s1:l4">Godt</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Te Bethlehem wordt <rhyme label="d" type="f">ghevonden</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Dit <rhyme label="e" type="m">Hemels-broodt</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Dat ons gheneest van <rhyme label="d" type="f" corresp="#s1:l7">sonden</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">En doodt de <rhyme label="e" type="m" corresp="#s1:l8">doodt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Den schijn van 's Vaders <rhyme label="a" type="m">glori'</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Sijns wesens <rhyme label="a" type="m" corresp="#s2:l1">Even-beelt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wt sijn vruchtbaer <rhyme label="b" type="f">memori'</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Eerst zonde vrouw <rhyme label="c" type="m">gheteelt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Is van een maeght <rhyme label="b" type="f" corresp="#s2:l3">gheboren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Oock sonder <rhyme label="c" type="m" corresp="#s2:l4">man</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Dit vat heeft hy <rhyme label="d" type="f">verkoren</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Die wonder <rhyme label="e" type="m">kan</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">Een kindeken is ons geboren,, Godt lof en <rhyme label="d" type="f" corresp="#s2:l7">danck</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s2:l8">sanck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Maer siet, hoe dat onthaelt <rhyme label="a" type="m">wordt</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Godts Soon, Godts Woort, Godts <rhyme label="a" type="m" corresp="#s3:l1">schijn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hoe quaelijck dat betaelt <rhyme label="b" type="f">wort</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sijn min, sijn jonst, sijn <rhyme label="c" type="m">pijn</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Hy gaet tot sijne <rhyme label="b" type="f" corresp="#s3:l3">vrinden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Soeckt over <rhyme label="c" type="m" corresp="#s3:l4">al</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Gheen plaets en kan hy <rhyme label="d" type="f">vinden</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Als inden <rhyme label="e" type="m">stal</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">Een kindeken is ons geboren,, Godt lof en <rhyme label="d" type="f" corresp="#s3:l7">danck</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s3:l8">sanck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Comt alle <rhyme label="a" type="m">creaturen</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Dient die u 't wesen <rhyme label="a" type="m" corresp="#s4:l1">gheeft</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Comt Enghels sijn <rhyme label="b" type="f">ghebueren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die door hem eeuwigh <rhyme label="c" type="m">leeft</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Comt menschen tot u <rhyme label="b" type="f" corresp="#s4:l3">broeder</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Soo kleyn, soo <rhyme label="c" type="m" corresp="#s4:l4">groot</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wie u met sijn <rhyme label="d" type="f">Moeder</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Treckt uyt de <rhyme label="e" type="m">doodt</rhyme>.</l>
        <l id="s4:l9" met="3" type="+-">Een kindeken is ons geboren,, Godt lof en <rhyme label="d" type="f" corresp="#s4:l7">danck</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s4:l8">sanck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Comt Branders nu <rhyme label="a" type="m">beneden</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Met al u <rhyme label="a" type="m" corresp="#s5:l1">minne-vier</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Comt <rhyme label="b" type="f">Volle-wetentheden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">V Meester die is <rhyme label="c" type="m">hier</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Comt Throonen hem <rhyme label="b" type="f" corresp="#s5:l3">ontfanghen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daer ghy in <rhyme label="c" type="m" corresp="#s5:l4">rust</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Des wereldts groot <rhyme label="d" type="f">verlanghen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Des Hemels <rhyme label="e" type="m">lust</rhyme>.</l>
        <l id="s5:l9" met="3" type="+-">Een kindeken is ons geboren,, Godt lof en <rhyme label="d" type="f" corresp="#s5:l7">danck</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s5:l8">sanck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Comt hooghe <rhyme label="a" type="m">Heerschappijen</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Comt alle Cracht, en <rhyme label="a" type="m" corresp="#s6:l1">Macht</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Comt Princen hem <rhyme label="b" type="f">bevrijen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van't haetich Ioods <rhyme label="c" type="m">gheslacht</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Comt hier Arch-enghels <rhyme label="b" type="f" corresp="#s6:l3">vlieghen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Met soet <rhyme label="c" type="m" corresp="#s6:l4">gheschal</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Comt Engelkens hem <rhyme label="d" type="f">wieghen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">In desen <rhyme label="e" type="m">stal</rhyme>.</l>
        <l id="s6:l9" met="3" type="+-">Een kindeken is ons geboren,, Godt lof en <rhyme label="d" type="f" corresp="#s6:l7">danck</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s6:l8">sanck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Maer boven al, ghy <rhyme label="a" type="m">menschen</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Comt dienen uwen <rhyme label="a" type="m" corresp="#s7:l1">Heer</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Die u gheeft uwe <rhyme label="b" type="f">wenschen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En noch belooft veel <rhyme label="c" type="m">meer</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Comt hier van alle <rhyme label="b" type="f" corresp="#s7:l3">kanten</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">O Adams <rhyme label="c" type="m" corresp="#s7:l4">bloet</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Dit kindt comt in u <rhyme label="d" type="f">planten</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Het eeuwigh <rhyme label="e" type="m">goet</rhyme>.</l>
        <l id="s7:l9" met="3" type="+-">Een kindeken is ons geboren,, Godt lof en <rhyme label="d" type="f" corresp="#s7:l7">danck</rhyme>,</l>
        <l id="s7:l10" met="2" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s7:l8">sanck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
