  <author>Philippus Jennyn</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GEen naemen zijnder op der <rhyme label="a" type="m">eird'</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Sulck' eere <rhyme label="a" type="m" corresp="#s1:l1">weird</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Sulck' eere <rhyme label="a" type="m" corresp="#s1:l1">weird</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Als IESVS <rhyme label="b" type="m">Naem'</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Die alle naemen gaet te <rhyme label="b" type="m" corresp="#s1:l4">boven</rhyme>;</l>
        <l id="s1:l6" met="2" type="-+">Laet ons <rhyme label="c" type="f">eersaem</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Laet ons <rhyme label="b" type="m" corresp="#s1:l4">eersaem</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Dien hooghsten <rhyme label="b" type="m" corresp="#s1:l4">Naem</rhyme>.</l>
        <l id="s1:l9" met="2" type="-+">Den soetsten <rhyme label="b" type="m" corresp="#s1:l4">Naem</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Van IESVS <rhyme label="c" type="f" corresp="#s1:l6">loven</rhyme>;</l>
        <l id="s1:l11" met="4" type="+-">Want IESVS eyghen wordt <rhyme label="d" type="m">gheseyt</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">Want IESVS eyghen wordt <rhyme label="d" type="m" corresp="#s1:l11">gheseyt</rhyme>,</l>
        <l id="s1:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s1:l11">saligheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den naeme van <rhyme label="a" type="m">Emmanuel</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Betaemde <rhyme label="a" type="m" corresp="#s2:l1">wel</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Betaemde <rhyme label="a" type="m" corresp="#s2:l1">wel</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Maer al veel <rhyme label="b" type="m">meer</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Den Naem van IESVS hem <rhyme label="b" type="m" corresp="#s2:l4">ghegheven</rhyme>;</l>
        <l id="s2:l6" met="2" type="-+">Laet ons den <rhyme label="c" type="f">Heer</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Laet ons den <rhyme label="b" type="m" corresp="#s2:l4">Heer</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Bewijsen <rhyme label="b" type="m" corresp="#s2:l4">eer'</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Wiens Naeme <rhyme label="b" type="m" corresp="#s2:l4">IESVS</rhyme></l>
        <l id="s2:l10" met="2" type="-+">wordt <rhyme label="c" type="f" corresp="#s2:l6">gheschreven</rhyme>;</l>
        <l id="s2:l11" met="4" type="+-">Heer IESVS eyghen is <rhyme label="d" type="m">gheseyt</rhyme>,</l>
        <l id="s2:l12" met="4" type="-+">Heer IESVS eyghen is <rhyme label="d" type="m" corresp="#s2:l11">gheseyt</rhyme>,</l>
        <l id="s2:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s2:l11">saligheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een olye ghegoten <rhyme label="a" type="m">uyt</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Naemt hem de <rhyme label="a" type="m" corresp="#s3:l1">Bruydt</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Naemt hem de <rhyme label="a" type="m" corresp="#s3:l1">Bruydt</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">En 'tis daer <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat hem de dochterkens <rhyme label="b" type="m" corresp="#s3:l4">beminnen</rhyme>;</l>
        <l id="s3:l6" met="2" type="-+">O <rhyme label="c" type="f">Bruydegom</rhyme> !</l>
        <l id="s3:l7" met="2" type="-+">O <rhyme label="b" type="m" corresp="#s3:l4">Bruydegom</rhyme> !</l>
        <l id="s3:l8" met="2" type="-+">O <rhyme label="b" type="m" corresp="#s3:l4">Jesse-blom</rhyme> !</l>
        <l id="s3:l9" met="2" type="-+">Kom (IESV) <rhyme label="b" type="m" corresp="#s3:l4">vloeyt</rhyme></l>
        <l id="s3:l10" met="2" type="-+">in onse <rhyme label="c" type="f" corresp="#s3:l6">sinnen</rhyme>;</l>
        <l id="s3:l11" met="4" type="+-">Want ghy zijt eyghentlijck <rhyme label="d" type="m">gheseyt</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">Want ghy zijt eyghentlijck <rhyme label="d" type="m" corresp="#s3:l11">gheseyt</rhyme>,</l>
        <l id="s3:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s3:l11">saligheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Adona√Æ hiet hy <rhyme label="a" type="m">andermael</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">In Jodsche <rhyme label="a" type="m" corresp="#s4:l1">tael</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">In Jodsche <rhyme label="a" type="m" corresp="#s4:l1">tael</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Men was <rhyme label="b" type="m">bevreest</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Den naem Iehova te <rhyme label="b" type="m" corresp="#s4:l4">vermonden</rhyme>;</l>
        <l id="s4:l6" met="2" type="-+">Door Godes <rhyme label="c" type="f">Gheest</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Door Godes <rhyme label="b" type="m" corresp="#s4:l4">Gheest</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Voor minst en <rhyme label="b" type="m" corresp="#s4:l4">meest</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Is IESVS <rhyme label="b" type="m" corresp="#s4:l4">Naem</rhyme></l>
        <l id="s4:l10" met="2" type="-+">ons <rhyme label="c" type="f" corresp="#s4:l6">af-ghesonden</rhyme>;</l>
        <l id="s4:l11" met="4" type="+-">Want IESVS eyghen is <rhyme label="d" type="m">gheseyt</rhyme>,</l>
        <l id="s4:l12" met="4" type="-+">Want IESVS eyghen is <rhyme label="d" type="m" corresp="#s4:l11">gheseyt</rhyme>,</l>
        <l id="s4:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s4:l11">saligheyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Naem van IESVS is soo <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Dat sy ons <rhyme label="a" type="m" corresp="#s5:l1">voedt</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Dat sy ons <rhyme label="a" type="m" corresp="#s5:l1">voedt</rhyme></l>
        <l id="s5:l4" met="2" type="-+">In waere <rhyme label="b" type="m">deught</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En weet ghesontheyt by te <rhyme label="b" type="m" corresp="#s5:l4">bringhen</rhyme>;</l>
        <l id="s5:l6" met="2" type="-+">Laet ons <rhyme label="c" type="f">verheught</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Laet ons <rhyme label="b" type="m" corresp="#s5:l4">verheught</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Met volle <rhyme label="b" type="m" corresp="#s5:l4">vreught</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Den Lof van <rhyme label="b" type="m" corresp="#s5:l4">IESVS</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Naeme <rhyme label="c" type="f" corresp="#s5:l6">singhen</rhyme>:</l>
        <l id="s5:l11" met="4" type="+-">Want IESVS eyghen is <rhyme label="d" type="m">gheseyt</rhyme>,</l>
        <l id="s5:l12" met="4" type="-+">Want IESVS eyghen is <rhyme label="d" type="m" corresp="#s5:l11">gheseyt</rhyme>,</l>
        <l id="s5:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s5:l11">saligheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Heer IESVS Naem te boven <rhyme label="a" type="m">gaet</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Den <rhyme label="a" type="m" corresp="#s6:l1">honigh-raet</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Den <rhyme label="a" type="m" corresp="#s6:l1">honigh-raet</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">In <rhyme label="b" type="m">leckerheyt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Die Godes wtverkoren <rhyme label="b" type="m" corresp="#s6:l4">smaken</rhyme>;</l>
        <l id="s6:l6" met="2" type="-+">In IESVS <rhyme label="c" type="f">leyt</rhyme></l>
        <l id="s6:l7" met="2" type="-+">In IESVS <rhyme label="b" type="m" corresp="#s6:l4">leyt</rhyme></l>
        <l id="s6:l8" met="2" type="-+">De <rhyme label="b" type="m" corresp="#s6:l4">soetigheyt</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Die kan <rhyme label="b" type="m" corresp="#s6:l4">alleen'</rhyme></l>
        <l id="s6:l10" met="2" type="-+">de ziel <rhyme label="c" type="f" corresp="#s6:l6">vermaken</rhyme>:</l>
        <l id="s6:l11" met="4" type="+-">Want IESVS eyghen is <rhyme label="d" type="m">gheseyt</rhyme>,</l>
        <l id="s6:l12" met="4" type="-+">Want IESVS eyghen is <rhyme label="d" type="m" corresp="#s6:l11">gheseyt</rhyme>,</l>
        <l id="s6:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s6:l11">saligheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Laet ons wt al ons herte <rhyme label="a" type="m">grondt</rhyme></l>
        <l id="s7:l2" met="2" type="-+">De wereldt <rhyme label="a" type="m" corresp="#s7:l1">rondt</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">De wereldt <rhyme label="a" type="m" corresp="#s7:l1">rondt</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Met bly <rhyme label="b" type="m">ghemoet</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Den Naem' van IESVS vry <rhyme label="b" type="m" corresp="#s7:l4">belijden</rhyme>;</l>
        <l id="s7:l6" met="2" type="-+">Met vleesch en <rhyme label="c" type="f">bloedt</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Met vleesch en <rhyme label="b" type="m" corresp="#s7:l4">bloedt</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">En al ons <rhyme label="b" type="m" corresp="#s7:l4">goedt</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Laet ons voor <rhyme label="b" type="m" corresp="#s7:l4">IESVS</rhyme></l>
        <l id="s7:l10" met="2" type="-+">Naeme <rhyme label="c" type="f" corresp="#s7:l6">strijden</rhyme>.</l>
        <l id="s7:l11" met="4" type="+-">Want IESVS eyghen is <rhyme label="d" type="m">gheseyt</rhyme>,</l>
        <l id="s7:l12" met="4" type="-+">Want IESVS eyghen is <rhyme label="d" type="m" corresp="#s7:l11">gheseyt</rhyme>,.</l>
        <l id="s7:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s7:l11">saligheyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dat IESVS Naem' <rhyme label="a" type="m">ghebenedijdt</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Tot allen <rhyme label="a" type="m" corresp="#s8:l1">tijdt</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Tot allen <rhyme label="a" type="m" corresp="#s8:l1">tijdt</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Aen't gulde <rhyme label="b" type="m">raem</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Gheschildert zy van yder <rhyme label="b" type="m" corresp="#s8:l4">ziele</rhyme>;</l>
        <l id="s8:l6" met="2" type="-+">O IESVS <rhyme label="c" type="f">Naem</rhyme> !</l>
        <l id="s8:l7" met="2" type="-+">O IESVS <rhyme label="b" type="m" corresp="#s8:l4">Naem</rhyme> !</l>
        <l id="s8:l8" met="2" type="-+">Maeckt my <rhyme label="b" type="m" corresp="#s8:l4">bequaem</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Dat ick van <rhyme label="b" type="m" corresp="#s8:l4">uwe</rhyme></l>
        <l id="s8:l10" met="2" type="-+">Liefde <rhyme label="c" type="f" corresp="#s8:l6">kriele</rhyme>;</l>
        <l id="s8:l11" met="4" type="+-">En gheeft my als mijn ziele <rhyme label="d" type="m">scheydt</rhyme>,</l>
        <l id="s8:l12" met="4" type="-+">En gheeft my als mijn ziele <rhyme label="d" type="m" corresp="#s8:l11">scheydt</rhyme>,</l>
        <l id="s8:l13" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s8:l11">saligheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
