  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">OCh! of ghy my een <rhyme label="a" type="f">Broeder</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En van mijn Maegschap <rhyme label="b" type="m">waert</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">En Borsten myner <rhyme label="a" type="f" corresp="#s1:l1">Moeder</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Van die my heeft <rhyme label="b" type="m" corresp="#s1:l2">gebaert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Gesint waert om te <rhyme label="c" type="f">suygë</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En ick op straet u <rhyme label="d" type="m">vont</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">(Des Godt is mijn <rhyme label="c" type="f" corresp="#s1:l5">getuyge</rhyme>!)</l>
        <l id="s1:l8" met="3" type="-+">Ick kust' u met mijn <rhyme label="d" type="m" corresp="#s1:l6">Mont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Soo kan ick oock niet <rhyme label="a" type="f">dencken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat yemant vande <rhyme label="b" type="m">Liên</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">My daerom soude <rhyme label="a" type="f" corresp="#s2:l1">krencken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al had hy 't al <rhyme label="b" type="m" corresp="#s2:l2">gesien</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Want, Liefde toe te <rhyme label="c" type="f">dragen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sijn eygen Bloet, en <rhyme label="d" type="m">Hert</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Gaet immers sonder <rhyme label="c" type="f" corresp="#s2:l5">vragen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En seer gepresen <rhyme label="d" type="m" corresp="#s2:l6">wert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick soud' u spoedich <rhyme label="a" type="f">leyden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In mynes Moeders <rhyme label="b" type="m">huys</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daer wouwd' ick dan <rhyme label="a" type="f" corresp="#s3:l1">verbeyden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gelijck een <rhyme label="b" type="m" corresp="#s3:l2">Open-Sluys</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">De Vruchten van u <rhyme label="c" type="f">Lippen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">De Stroomen van u <rhyme label="d" type="m">Woort</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En niet een laten <rhyme label="c" type="f" corresp="#s3:l5">glippen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Van 't geen ick had <rhyme label="d" type="m" corresp="#s3:l6">gehoort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daer sal ick u in <rhyme label="a" type="f">schincken</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">Een delikaten <rhyme label="b" type="m">Wijn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Daer sal, dat ghy sult <rhyme label="a" type="f" corresp="#s4:l1">drincken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">'t Sap van Granaten <rhyme label="b" type="m" corresp="#s4:l2">sijn</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Daer sal ick Vruchten <rhyme label="c" type="f">dragen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Soo velen in <rhyme label="d" type="m">getal</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Waer uyt een wel <rhyme label="c" type="f" corresp="#s4:l5">behagen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Een yeder scheppen <rhyme label="d" type="m" corresp="#s4:l6">sal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Och! wilt my doch <rhyme label="a" type="f">omvangen</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">U Slinkx, en <rhyme label="b" type="m">Rechter-hant</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">(Och! och! ick wort soo <rhyme label="a" type="f" corresp="#s5:l1">bange</rhyme>!)</l>
        <l id="s5:l4" met="3" type="-+">Sy my, tot <rhyme label="b" type="m" corresp="#s5:l2">onderstant</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Ierusalems <rhyme label="c" type="f">Vriendinnen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Speel-noten, hout u <rhyme label="d" type="m">stil</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En weckt, diẽ wy <rhyme label="c" type="f" corresp="#s5:l5">beminnen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Niet eer, voor dat hy <rhyme label="d" type="m" corresp="#s5:l6">wil</rhyme>.</l>
      </lg>
    </lg>
  </text>
