<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181887</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hou voerman van de hel</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ontwaekt myn Lief, <rhyme label="a" type="m">ontwaekt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wil uit u venster <rhyme label="b" type="f">kyken</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Want ik doe myn <rhyme label="c" type="m">beklag</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In 't naerste van de <rhyme label="c" type="m" corresp="#s1:l3">nagt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Geen Tyger: Leeuw of <rhyme label="d" type="m">Beer</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">Die my kan doen <rhyme label="b" type="f" corresp="#s1:l2">verschrikken</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Geen hagel wind of <rhyme label="e" type="m">weer</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die my kan doen <rhyme label="e" type="m" corresp="#s1:l7">afkeer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer 'k doe hier myn <rhyme label="a" type="m">beklag</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">Over uw wreede <rhyme label="b" type="f">vonken</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Die gy Ã´ schoone <rhyme label="c" type="m">Maegd</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Uan dag tot dag toe <rhyme label="c" type="m" corresp="#s2:l3">draegd</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Maer ik bid u Liefje <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wil my maer eens <rhyme label="b" type="f" corresp="#s2:l2">toelonken</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">En haest u schoon <rhyme label="e" type="m">Ionkvrouwe</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Of ik versmelt van <rhyme label="e" type="m" corresp="#s2:l7">rouw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe lang is 't wel <rhyme label="a" type="m">geleen</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Myn Lief teld maer de <rhyme label="b" type="f">Weken</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Dat ik kwam voor u <rhyme label="c" type="m">deur</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met oogen vol <rhyme label="c" type="m" corresp="#s3:l3">getreur</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Door hagel, sneeuw en <rhyme label="d" type="m">wind</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Om u myn Lief te <rhyme label="b" type="f" corresp="#s3:l2">spreken</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Maer gy blyft wreed <rhyme label="e" type="m">verhart</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En houd myn Min <rhyme label="e" type="m" corresp="#s3:l7">verwart</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik wensch dat ik myn <rhyme label="a" type="m">bloed</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Het zou my niet <rhyme label="b" type="f">verdrieten</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Uoor u mogt storten <rhyme label="c" type="m">uit</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En dat op staende <rhyme label="c" type="m" corresp="#s4:l3">voet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En dat gy my dan <rhyme label="d" type="m">maer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Uw trouwe Dienaer <rhyme label="b" type="f" corresp="#s4:l2">hieten</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Uw slaef of uwen <rhyme label="e" type="m">knegt</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">Ik diende u <rhyme label="e" type="m" corresp="#s4:l7">opregt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ik wenste dat myn <rhyme label="a" type="m">bloed</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Door dwang van 's Hemels <rhyme label="b" type="f">Goden</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Op dat u wreede <rhyme label="c" type="m">Ziel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Gedwongen wierd met <rhyme label="c" type="m" corresp="#s5:l3">min</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Of dat uw Ieugdig <rhyme label="d" type="m">Hert</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">My eens tot Liefde <rhyme label="b" type="f" corresp="#s5:l2">nooden</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Zoo vloog myn Hert en <rhyme label="e" type="m">Zin</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Met u ten Hemel <rhyme label="e" type="m" corresp="#s5:l7">in</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer ik zie wel wreede <rhyme label="a" type="m">Maegd</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat ik u niet kan <rhyme label="b" type="f">bewegen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met myn trouwe Minnaers <rhyme label="c" type="m">hart</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">En door een zoet <rhyme label="c" type="m" corresp="#s6:l3">gevley</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Zo moet ik treuren <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Tot het einde van myn <rhyme label="b" type="f" corresp="#s6:l2">Leven</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Adieu ik vertek van <rhyme label="e" type="m">hier</rhyme>:</l>
        <l id="s6:l8" met="3" type="-+">Ik wensch u veel <rhyme label="e" type="m" corresp="#s6:l7">pleysier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wat hoor ik voor <rhyme label="a" type="m">Geluid</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Wie of my hier komt <rhyme label="b" type="f">wekken</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Uit mynen zoeten <rhyme label="c" type="m">slaep</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Wie of het wesen <rhyme label="c" type="m" corresp="#s7:l3">mag</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">Ik moet op zyn <rhyme label="d" type="m">afscheid</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Eens uit myn Uenster <rhyme label="b" type="f" corresp="#s7:l2">kyken</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">My dunkt zyn stem <rhyme label="e" type="m">verlaet</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">'t Is of hy henen <rhyme label="e" type="m" corresp="#s7:l7">gaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Gy trouwe Minnaer <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En wilt eens weder <rhyme label="b" type="f">keeren</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">De liefste daer gy om <rhyme label="c" type="m">treurt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die lag vast in haer <rhyme label="c" type="m" corresp="#s8:l3">rust</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Ik droomde eenen <rhyme label="d" type="m">droom</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat myn Ionk-hert doet <rhyme label="b" type="f" corresp="#s8:l2">treuren</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Als ik hem heen liet <rhyme label="e" type="m">gaen</rhyme>:</l>
        <l id="s8:l8" met="3" type="-+">Dus blyft een weinig <rhyme label="e" type="m" corresp="#s8:l7">staen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ontfangt van my den <rhyme label="a" type="m">Trouw</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Myn Engel vol van <rhyme label="b" type="f">waerden</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Die voor zyn zoetste <rhyme label="c" type="m">Lief</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Zyn lyf en bloed steld <rhyme label="c" type="m" corresp="#s9:l3">bloot</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">De Goden uit den <rhyme label="d" type="m">Throon</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die roep ik tot <rhyme label="b" type="f" corresp="#s9:l2">getuigen</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Dat ik zal zyn <rhyme label="e" type="m">Getrouw</rhyme>:</l>
        <l id="s9:l8" met="3" type="-+">Als eenen Man zyn <rhyme label="e" type="m" corresp="#s9:l7">Urouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
