<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8148</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">Geestelijk lied</tag>
    </tags>
    <melody>Laatstmaal ging ik ter jacht</melody>
    <categories/>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">OP eenen morghen <rhyme label="a" type="m">vroegh</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Als het begon te <rhyme label="b" type="f">lichten</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Het peynsen soo veel <rhyme label="a" type="m" corresp="#s1:l1">woegh</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat ick begon te <rhyme label="b" type="f" corresp="#s1:l2">dichten</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Bysonder aen de jeucht dit <rhyme label="c" type="m">liedt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Waerom sy soo weynigh <rhyme label="c" type="m" corresp="#s1:l5">vliet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Van de werelt vol <rhyme label="c" type="m" corresp="#s1:l5">verdriet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O lieve jonghe <rhyme label="a" type="m">jeught</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Versint het wel te <rhyme label="b" type="f">deghen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Het schijnt voor u wel <rhyme label="a" type="m" corresp="#s2:l1">vreucht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maer ghy gaet sware <rhyme label="b" type="f" corresp="#s2:l2">weghen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Ick bid u gaet doch niet soo <rhyme label="c" type="m">voort</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Recht aen teghen Godes <rhyme label="c" type="m" corresp="#s2:l5">woordt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">In u sonden niet <rhyme label="c" type="m" corresp="#s2:l5">smoort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Christus toont ons <rhyme label="a" type="m">ghereedt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Twee weghen <rhyme label="b" type="f">onghelijcken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">D'een smal en d'ander <rhyme label="a" type="m" corresp="#s3:l1">breet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Den breeden moet men <rhyme label="b" type="f" corresp="#s3:l2">wijcken</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Want sy geeft ons een bitter <rhyme label="c" type="m">loon</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Voor al onse diensten <rhyme label="c" type="m" corresp="#s3:l5">schoon</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die wy deden <rhyme label="c" type="m" corresp="#s3:l5">ydoon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O Wereldt vol <rhyme label="a" type="m">onkruyt</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">Wie soud' hem aen u <rhyme label="b" type="f">houwen</rhyme>?</l>
        <l id="s4:l3" met="3" type="-+">Hy komt bedroghen <rhyme label="a" type="m" corresp="#s4:l1">uyt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die op u set <rhyme label="b" type="f" corresp="#s4:l2">betrouwen</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">'Tis maer voor eenen schoonen <rhyme label="c" type="m">schijn</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Aen te sien, maer vol <rhyme label="c" type="m" corresp="#s4:l5">fenijn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Sult ghy bevonden <rhyme label="c" type="m" corresp="#s4:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hierom ghy aerden <rhyme label="a" type="m">Clomp</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Laet u doch niet <rhyme label="b" type="f">verleyen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Doet ghy 't is al te <rhyme label="a" type="m" corresp="#s5:l1">plomp</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hier na sal hy u <rhyme label="b" type="f" corresp="#s5:l2">payen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Met knerssinghe der tanden <rhyme label="c" type="m">fel</rhyme></l>
        <l id="s5:l6" met="4" type="+-">In een brant, die dueren <rhyme label="c" type="m" corresp="#s5:l5">sel</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Met eeuwigheyts <rhyme label="c" type="m" corresp="#s5:l5">ghequel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wy zijn maer hoy of <rhyme label="a" type="m">gras</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Konden wy 't slechts <rhyme label="b" type="f">versinnen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat ons soo snel en <rhyme label="a" type="m" corresp="#s6:l1">ras</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die doodt hier comt <rhyme label="b" type="f" corresp="#s6:l2">bespringhen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Wy souden ons wel wachten <rhyme label="c" type="m">bet</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Van die wereldt, want sy <rhyme label="c" type="m" corresp="#s6:l5">set</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Veel stricken tot <rhyme label="c" type="m" corresp="#s6:l5">belet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O Werelt! die u <rhyme label="a" type="m">dient</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hem naeckt een swaer <rhyme label="b" type="f">ellende</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Quaet loon krijght uwen <rhyme label="a" type="m" corresp="#s7:l1">vriendt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Och die u noyt en <rhyme label="b" type="f" corresp="#s7:l2">kenden</rhyme>!</l>
        <l id="s7:l5" met="4" type="-+">Mijn vleys wil ick nu cruysen <rhyme label="c" type="m">saen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Want ick sie ten mach niet <rhyme label="c" type="m" corresp="#s7:l5">gaen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Te slepen achter <rhyme label="c" type="m" corresp="#s7:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Adieu wereldt <rhyme label="a" type="m">verblint</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ick wil u varen <rhyme label="b" type="f">laten</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">V diend' ick <rhyme label="a" type="m" corresp="#s8:l1">pertinent</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Maer nu wil ick u <rhyme label="b" type="f" corresp="#s8:l2">haten</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Alle mijn nettigheyt met <rhyme label="c" type="m">vlijt</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Wil ick laten tot u <rhyme label="c" type="m" corresp="#s8:l5">spijt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Want 'tis toch niet een <rhyme label="c" type="m" corresp="#s8:l5">mijt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick voel ick heb <rhyme label="a" type="m">ghedwaelt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Heere, van uwe <rhyme label="b" type="f">paden</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Mijn gangh die heeft <rhyme label="a" type="m" corresp="#s9:l1">ghefaelt</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Och neemt my in <rhyme label="b" type="f" corresp="#s9:l2">ghenade</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Laet my u vruchten smaecken <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Van uws gheests Fonteyne <rhyme label="c" type="m" corresp="#s9:l5">soet</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Heere, soo krijgh ick <rhyme label="c" type="m" corresp="#s9:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Helpt my voort en <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Op dat ick niet <rhyme label="b" type="f">beswijcke</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Sterckt my met uwen <rhyme label="a" type="m" corresp="#s10:l1">woordt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En neemt van mijn, gheen <rhyme label="b" type="f" corresp="#s10:l2">wijcke</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Want ick geeren noch verder <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Daer de straten blincken <rhyme label="c" type="m" corresp="#s10:l5">klaer</rhyme></l>
        <l id="s10:l7" met="3" type="-+">By u Hemelsche <rhyme label="c" type="m" corresp="#s10:l5">schaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O soete <rhyme label="a" type="m">melody</rhyme>!</l>
        <l id="s11:l2" met="3" type="-+">Wanneer sal ick u <rhyme label="b" type="f">smaecken</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Haelt my terstont tot <rhyme label="a" type="m" corresp="#s11:l1">dy</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Ick wilt hier al <rhyme label="b" type="f" corresp="#s11:l2">versaecken</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Ende keeren hier mijn <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Om dan gaen ten Hemel <rhyme label="c" type="m" corresp="#s11:l5">in</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Niet beter ick en <rhyme label="c" type="m" corresp="#s11:l5">vin</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Och waer ick in de <rhyme label="a" type="m">vreucht</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ick soud' niet weder <rhyme label="b" type="f">keeren</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Want daer is soet <rhyme label="a" type="m" corresp="#s12:l1">geneucht</rhyme></l>
        <l id="s12:l4" met="3" type="-+">En hier is groot <rhyme label="b" type="f" corresp="#s12:l2">verseeren</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Die wereldt wil beletter <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Maer ick moeter tegen <rhyme label="c" type="m" corresp="#s12:l5">strijn</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Heere Godt helpt <rhyme label="c" type="m" corresp="#s12:l5">mijn</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Prince Vader <rhyme label="a" type="m">ydoon</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Laet het ons doch <rhyme label="b" type="f">erlanghen</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Dat wy van u de <rhyme label="a" type="m" corresp="#s13:l1">kroon</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Wt genade <rhyme label="b" type="f" corresp="#s13:l2">ontfanghen</rhyme>;</l>
        <l id="s13:l5" met="4" type="-+">En dan singhen dat nieuwe <rhyme label="c" type="m">liedt</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Op den bergh Syon <rhyme label="c" type="m" corresp="#s13:l5">siet</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Daer Esdras van <rhyme label="c" type="m" corresp="#s13:l5">bediet</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Neemt toch in danck dit <rhyme label="a" type="m">liedt</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Ghy jonghe jeucht te <rhyme label="b" type="f">saemen</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Hier vrij een bodt nae <rhyme label="a" type="m" corresp="#s14:l1">biedt</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Draecht rechte Christen <rhyme label="b" type="f" corresp="#s14:l2">naemen</rhyme>.</l>
        <l id="s14:l5" met="4" type="-+">Als wy ter rechter sijden <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Dan worden genomen <rhyme label="c" type="m" corresp="#s14:l5">aen</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Om in de vreucht te <rhyme label="c" type="m" corresp="#s14:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Scheyden moet ick van <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Maer daer sal 't eeuwich <rhyme label="b" type="f">dueren</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">En in dat helsche <rhyme label="a" type="m" corresp="#s15:l1">vier</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Of daer men vreucht <rhyme label="b" type="f" corresp="#s15:l2">labueren</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Altijdt versteken sijn van <rhyme label="c" type="m">Godt</rhyme></l>
        <l id="s15:l6" met="4" type="+-">Oft t'ontfanghen 't beste <rhyme label="c" type="m" corresp="#s15:l5">lot</rhyme>.</l>
        <l id="s15:l7" met="3" type="-+">Siet toe, dat is het <rhyme label="c" type="m" corresp="#s15:l5">slot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
