  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">In een palleys quam ic <rhyme label="a" type="m">ghegaen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In een palleys quam ic <rhyme label="a" type="m" corresp="#s1:l1">ghegaen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">In een palleys quam ic <rhyme label="a" type="m" corresp="#s1:l1">ghegaen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Int soetste vande <rhyme label="b" type="m">Mey</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Quam ick mijn Liefken <rhyme label="b" type="m" corresp="#s1:l4">by</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mijn docht ic was int <rhyme label="a" type="m">Paradijs</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Mijn docht ic was int <rhyme label="a" type="m" corresp="#s2:l1">Paradijs</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Mijn docht ic was int <rhyme label="a" type="m" corresp="#s2:l1">Paradijs</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Elck voghelken op een <rhyme label="b" type="m">rijs</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Sy zonghen daerom <rhyme label="b" type="m" corresp="#s2:l4">prijs</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">By een fonteyn van water <rhyme label="a" type="m">claer</rhyme>.</l>
        <l id="s3:l2" met="4" type="-+">By een fonteyn van water <rhyme label="a" type="m" corresp="#s3:l1">claer</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">By een fonteyn van water <rhyme label="a" type="m" corresp="#s3:l1">claer</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Veel bloemkens vant ick <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Het was de tijt van <rhyme label="b" type="m" corresp="#s3:l4">t'Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die Nachtegael lustich van <rhyme label="a" type="m">sanck</rhyme>.</l>
        <l id="s4:l2" met="4" type="-+">Die Nachtegael lustich van <rhyme label="a" type="m" corresp="#s4:l1">sanck</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Die Nachtegael lustich van <rhyme label="a" type="m" corresp="#s4:l1">sanck</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Mijn Liefs keelken <rhyme label="b" type="m">blanck</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Het was so soeten <rhyme label="b" type="m" corresp="#s4:l4">sang</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mijn Liefken was daer in <rhyme label="a" type="m">verhuecht</rhyme>.</l>
        <l id="s5:l2" met="4" type="-+">Mijn Liefken was daer in <rhyme label="a" type="m" corresp="#s5:l1">verhuecht</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Mijn Liefken was daer in <rhyme label="a" type="m" corresp="#s5:l1">verhuecht</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Al door mijn liefkens <rhyme label="b" type="m">iuecht</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Was ick daer in <rhyme label="b" type="m" corresp="#s5:l4">verhuecht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Princesse ick was daer in <rhyme label="a" type="m">verblijt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Princesse ick was daer in <rhyme label="a" type="m" corresp="#s6:l1">verblijt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Princesse ick was daer in <rhyme label="a" type="m" corresp="#s6:l1">verblijt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ghy alleen die liefste <rhyme label="b" type="m">sijt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Al wort het ons <rhyme label="b" type="m" corresp="#s6:l4">benijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
