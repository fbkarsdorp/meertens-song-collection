  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">MYn ziels <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Mijn herts <rhyme label="a" type="f" corresp="#s1:l1">bezinde</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Hoe is uw <rhyme label="b" type="f">poogen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Dus gestaig naa 't <rhyme label="b" type="f" corresp="#s1:l3">hooge</rhyme>?</l>
        <l id="s1:l5" met="3" type="+-">Hoogte die <rhyme label="c" type="m">vergaat</rhyme>.</l>
        <l id="s1:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s1:l7" met="3" type="+-">Kiez een vaster <rhyme label="c" type="m" corresp="#s1:l5">staat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ai! Stel uw <rhyme label="a" type="f">luste</rhyme></l>
        <l id="s2:l2" met="2" type="-+">En al uw <rhyme label="a" type="f" corresp="#s2:l1">ruste</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Op my uw <rhyme label="b" type="f">Herder</rhyme>.</l>
        <l id="s2:l4" met="3" type="+-">Ai! En loop niet <rhyme label="b" type="f" corresp="#s2:l3">verder</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Koom hier dog by <rhyme label="c" type="m">my</rhyme></l>
        <l id="s2:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s2:l7" met="3" type="+-">Staak uw' <rhyme label="c" type="m" corresp="#s2:l5">woelery</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">O lief! Waar zijt <rhyme label="a" type="f">Gy</rhyme>?</l>
        <l id="s3:l2" met="2" type="-+">Uw roep verblijd <rhyme label="a" type="f" corresp="#s3:l1">my</rhyme>.</l>
        <l id="s3:l3" met="2" type="-+">'k Gaa U vast <rhyme label="b" type="f">zoeken</rhyme></l>
        <l id="s3:l4" met="3" type="+-">In boschaadj' en <rhyme label="b" type="f" corresp="#s3:l3">hoeken</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">In mijn <rhyme label="c" type="m">zinlijkheid</rhyme>.</l>
        <l id="s3:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s3:l7" met="3" type="+-">Waar is 't dat Gy <rhyme label="c" type="m" corresp="#s3:l5">weid</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Hier in dit <rhyme label="a" type="f">laage</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Van willig <rhyme label="a" type="f" corresp="#s4:l1">draagen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Van kruis en <rhyme label="b" type="f">lijden</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Dat my quam <rhyme label="b" type="f" corresp="#s4:l3">bestrijden</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Hier in diep <rhyme label="c" type="m">geduld</rhyme>.</l>
        <l id="s4:l6" met="3" type="+-">[O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s4:l7" met="3" type="+-">Gy my vinden <rhyme label="c" type="m" corresp="#s4:l5">zult</rhyme>.]</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Hoe daar <rhyme label="a" type="f">beneeden</rhyme>?</l>
        <l id="s5:l2" met="2" type="-+">Zoo laag <rhyme label="a" type="f" corresp="#s5:l1">vertreeden</rhyme></l>
        <l id="s5:l3" met="2" type="-+">In elks <rhyme label="b" type="f">versmaaden</rhyme>?</l>
        <l id="s5:l4" met="3" type="+-">'t Zoud mijn aan-zien <rhyme label="b" type="f" corresp="#s5:l3">schaaden</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Gaf ik my zoo <rhyme label="c" type="m">laag</rhyme>:</l>
        <l id="s5:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s5:l7" met="3" type="+-">En dat hiel ik <rhyme label="c" type="m" corresp="#s5:l5">graag</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Gy zyt <rhyme label="a" type="f">bedroogen</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Door schijn en <rhyme label="a" type="f" corresp="#s6:l1">loogen</rhyme>.</l>
        <l id="s6:l3" met="2" type="-+">Door ligt <rhyme label="b" type="f">gelooven</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Laat Gy u <rhyme label="b" type="f" corresp="#s6:l3">verdooven</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Dat gy heel <rhyme label="c" type="m">verblind</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s6:l7" met="3" type="+-">Schand voor eer <rhyme label="c" type="m" corresp="#s6:l5">bemind</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Hoe kan dat <rhyme label="a" type="f">weezen</rhyme>?</l>
        <l id="s7:l2" met="2" type="-+">'k Word hier <rhyme label="a" type="f" corresp="#s7:l1">gepreezen</rhyme>.</l>
        <l id="s7:l3" met="2" type="-+">'k Zit hier in <rhyme label="b" type="f">eeren</rhyme>.</l>
        <l id="s7:l4" met="3" type="+-">'k Gaa mijn schat <rhyme label="b" type="f" corresp="#s7:l3">vermeeren</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">'t Gaat in alles <rhyme label="c" type="m">wel</rhyme>.</l>
        <l id="s7:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s7:l7" met="3" type="+-">'k Ken nog ramp nog <rhyme label="c" type="m" corresp="#s7:l5">quel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Ai! Zoud 't daar wel <rhyme label="a" type="f">zijn</rhyme></l>
        <l id="s8:l2" met="2" type="-+">En vry van quel <rhyme label="a" type="f" corresp="#s8:l1">zijn</rhyme>?</l>
        <l id="s8:l3" met="2" type="-+">D'wijl op de <rhyme label="b" type="f">bergen</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Die den Heemel <rhyme label="b" type="f" corresp="#s8:l3">tergen</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">'t Staadig stormd en <rhyme label="c" type="m">raazd</rhyme>.</l>
        <l id="s8:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s8:l7" met="3" type="+-">Gy zijt heel <rhyme label="c" type="m" corresp="#s8:l5">verdwaazd</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Hebt gy by <rhyme label="a" type="f">tijden</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Een schijn <rhyme label="a" type="f" corresp="#s9:l1">verblijden</rhyme>?</l>
        <l id="s9:l3" met="2" type="-+">Voor uw <rhyme label="b" type="f">verheffen</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Zal u 't vallen <rhyme label="b" type="f" corresp="#s9:l3">treffen</rhyme>.</l>
        <l id="s9:l5" met="3" type="+-">Hoe u 't hoog <rhyme label="c" type="m">toe-lagt</rhyme>.</l>
        <l id="s9:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s9:l7" met="3" type="+-">'t Eind is rouw en <rhyme label="c" type="m" corresp="#s9:l5">klagt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Tot U te <rhyme label="a" type="f">keeren</rhyme></l>
        <l id="s10:l2" met="2" type="-+">In diep <rhyme label="a" type="f" corresp="#s10:l1">verneeren</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Daar ik zoud <rhyme label="b" type="f">wagten</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Spot en elks <rhyme label="b" type="f" corresp="#s10:l3">veragten</rhyme>;</l>
        <l id="s10:l5" met="3" type="+-">Dat is ook geen <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s10:l7" met="3" type="+-">Voor mijn blijde <rhyme label="c" type="m" corresp="#s10:l5">jeugd</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Maar zoo 'k 't <rhyme label="a" type="f">vermaaken</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Dat ik hier <rhyme label="a" type="f" corresp="#s11:l1">smaake</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Sta'ig mogt <rhyme label="b" type="f">behouden</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Voor gewis ik <rhyme label="b" type="f" corresp="#s11:l3">zoude</rhyme></l>
        <l id="s11:l5" met="3" type="+-">Haastig zijn <rhyme label="c" type="m">bene'en</rhyme>.</l>
        <l id="s11:l6" met="3" type="+-">O Herder! o <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s11:l7" met="3" type="+-">Kan dat zijn <rhyme label="c" type="m" corresp="#s11:l5">gele'en</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Neen all' die <rhyme label="a" type="f">dingen</rhyme></l>
        <l id="s12:l2" met="2" type="-+">Moest gy <rhyme label="a" type="f" corresp="#s12:l1">ontspringen</rhyme>.</l>
        <l id="s12:l3" met="2" type="-+">Schijn vreugds <rhyme label="b" type="f">bedrijven</rhyme></l>
        <l id="s12:l4" met="3" type="+-">Moet by d' hoogheid <rhyme label="b" type="f" corresp="#s12:l3">blijven</rhyme>.</l>
        <l id="s12:l5" met="3" type="+-">'t Waare goed en <rhyme label="c" type="m">waan</rhyme></l>
        <l id="s12:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s12:l7" met="3" type="+-">Kan niet zaamen <rhyme label="c" type="m" corresp="#s12:l5">gaan</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="2" type="-+">Want d' aardze <rhyme label="a" type="f">hoogheid</rhyme>.</l>
        <l id="s13:l2" met="2" type="-+">Hoe zoet zy 't ook <rhyme label="a" type="f" corresp="#s13:l1">vleid</rhyme>.</l>
        <l id="s13:l3" met="2" type="-+">Zoud u <rhyme label="b" type="f">besmetten</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">En uw loop <rhyme label="b" type="f" corresp="#s13:l3">beletten</rhyme>:</l>
        <l id="s13:l5" met="3" type="+-">Want de liefd tot 't <rhyme label="c" type="m">aards</rhyme></l>
        <l id="s13:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s13:l7" met="3" type="+-">Wil niet <rhyme label="c" type="m" corresp="#s13:l5">Heemel-waards</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="2" type="-+">Wel nu <rhyme label="a" type="f">genoomen</rhyme></l>
        <l id="s14:l2" met="2" type="-+">'k Zoud tot U <rhyme label="a" type="f" corresp="#s14:l1">koomen</rhyme>;</l>
        <l id="s14:l3" met="2" type="-+">D'wijl <rhyme label="b" type="f">onder-weegen</rhyme></l>
        <l id="s14:l4" met="3" type="+-">Is een leeuw <rhyme label="b" type="f" corresp="#s14:l3">geleegen</rhyme>,</l>
        <l id="s14:l5" met="3" type="+-">Hoe op wat <rhyme label="c" type="m">manier</rhyme></l>
        <l id="s14:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s14:l7" met="3" type="+-">Ging ik best van <rhyme label="c" type="m" corresp="#s14:l5">hier</rhyme>?</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="2" type="-+">Snel maar uw <rhyme label="a" type="f">gangen</rhyme>,</l>
        <l id="s15:l2" met="2" type="-+">'k Zal u <rhyme label="a" type="f" corresp="#s15:l1">ontfangen</rhyme>;</l>
        <l id="s15:l3" met="2" type="-+">'k Zal u <rhyme label="b" type="f">ontmoeten</rhyme>;</l>
        <l id="s15:l4" met="3" type="+-">'k Zal u leet <rhyme label="b" type="f" corresp="#s15:l3">verzoeten</rhyme>.</l>
        <l id="s15:l5" met="3" type="+-">U en zal niet <rhyme label="c" type="m">scha'en</rhyme>,</l>
        <l id="s15:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s15:l7" met="3" type="+-">Koom maar vierig <rhyme label="c" type="m" corresp="#s15:l5">aan</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="2" type="-+">Ik koom <rhyme label="a" type="f">geloopen</rhyme>,</l>
        <l id="s16:l2" met="2" type="-+">Gestijfd door <rhyme label="a" type="f" corresp="#s16:l1">hoope</rhyme>.</l>
        <l id="s16:l3" met="2" type="-+">'k Hebt al <rhyme label="b" type="f">verlaaten</rhyme>.</l>
        <l id="s16:l4" met="3" type="+-">'k Laat de haaters <rhyme label="b" type="f" corresp="#s16:l3">praaten</rhyme>.</l>
        <l id="s16:l5" met="3" type="+-">'k Hebt met U <rhyme label="c" type="m">door-stre'en</rhyme>.</l>
        <l id="s16:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s16:l7" met="3" type="+-">Waar leid Gy my <rhyme label="c" type="m" corresp="#s16:l5">heen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="2" type="-+">Welkoom mijn <rhyme label="a" type="f">schoone</rhyme>.</l>
        <l id="s17:l2" met="2" type="-+">Ik zal u <rhyme label="a" type="f" corresp="#s17:l1">toonen</rhyme>,</l>
        <l id="s17:l3" met="2" type="-+">Hoe mijn <rhyme label="b" type="f">versmaadheid</rhyme></l>
        <l id="s17:l4" met="3" type="+-">U tot d' hoogste staat <rhyme label="b" type="f" corresp="#s17:l3">leid</rhyme>,</l>
        <l id="s17:l5" met="3" type="+-">En de rijkste <rhyme label="c" type="m">schat</rhyme>,</l>
        <l id="s17:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s17:l7" met="3" type="+-">Die noit hert <rhyme label="c" type="m" corresp="#s17:l5">bevat</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="2" type="-+">Hier onder 't <rhyme label="a" type="f">lommer</rhyme>,</l>
        <l id="s18:l2" met="2" type="-+">Gants vrij van <rhyme label="a" type="f" corresp="#s18:l1">kommer</rhyme>,</l>
        <l id="s18:l3" met="2" type="-+">Aan deeze <rhyme label="b" type="f">bronne</rhyme>,</l>
        <l id="s18:l4" met="3" type="+-">Uit de brand der <rhyme label="b" type="f" corresp="#s18:l3">zonne</rhyme>,</l>
        <l id="s18:l5" met="3" type="+-">In dit lustig <rhyme label="c" type="m">dal</rhyme>,</l>
        <l id="s18:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s18:l7" met="3" type="+-">Ik u leiden <rhyme label="c" type="m" corresp="#s18:l5">zal</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="2" type="-+">By deeze <rhyme label="a" type="f">stroomen</rhyme></l>
        <l id="s19:l2" met="2" type="-+">En will'ge <rhyme label="a" type="f" corresp="#s19:l1">boomen</rhyme>,</l>
        <l id="s19:l3" met="2" type="-+">Hier in de <rhyme label="b" type="f">roozen</rhyme></l>
        <l id="s19:l4" met="3" type="+-">Zult gy u <rhyme label="b" type="f" corresp="#s19:l3">verpoozen</rhyme>,</l>
        <l id="s19:l5" met="3" type="+-">Met de hoogste <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s19:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s19:l7" met="3" type="+-">In volmaakte <rhyme label="c" type="m" corresp="#s19:l5">rust</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="2" type="-+">O lief! uw <rhyme label="a" type="f">goedheid</rhyme></l>
        <l id="s20:l2" met="2" type="-+">Vol zoete <rhyme label="a" type="f" corresp="#s20:l1">zoetheid</rhyme>,</l>
        <l id="s20:l3" met="2" type="-+">Uw lieflijk <rhyme label="b" type="f">weezen</rhyme></l>
        <l id="s20:l4" met="3" type="+-">Zal mijn ziel <rhyme label="b" type="f" corresp="#s20:l3">geneezen</rhyme>.</l>
        <l id="s20:l5" met="3" type="+-">Ag! hoe is 't <rhyme label="c" type="m">gemoed</rhyme></l>
        <l id="s20:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s20:l7" met="3" type="+-">Met uw lief <rhyme label="c" type="m" corresp="#s20:l5">ontgloed</rhyme>!</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="2" type="-+">Geen dood nog <rhyme label="a" type="f">helle</rhyme>,</l>
        <l id="s21:l2" met="2" type="-+">Geen angstig <rhyme label="a" type="f" corresp="#s21:l1">quellen</rhyme></l>
        <l id="s21:l3" met="2" type="-+">Zal my <rhyme label="b" type="f">verleiden</rhyme>,</l>
        <l id="s21:l4" met="3" type="+-">Nog my van U <rhyme label="b" type="f" corresp="#s21:l3">scheiden</rhyme>.</l>
        <l id="s21:l5" met="3" type="+-">'t Rust gerust en <rhyme label="c" type="m">stil</rhyme></l>
        <l id="s21:l6" met="3" type="+-">O Herder! <rhyme label="d" type="f">Herder</rhyme>!</l>
        <l id="s21:l7" met="3" type="+-">In uw zoete <rhyme label="c" type="m" corresp="#s21:l5">wil</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="2" type="-+">23.Uw wil zal mijn <rhyme label="a" type="f">zijn</rhyme>,</l>
        <l id="s22:l2" met="2" type="-+">De mijn zal dijn <rhyme label="a" type="f" corresp="#s22:l1">zijn</rhyme>.</l>
        <l id="s22:l3" met="2" type="-+">En uit dat <rhyme label="b" type="f">leeven</rhyme></l>
        <l id="s22:l4" met="3" type="+-">Zult gy zijn <rhyme label="b" type="f" corresp="#s22:l3">verheeven</rhyme>,</l>
        <l id="s22:l5" met="3" type="+-">Door my, tot een <rhyme label="c" type="m">hoog't</rhyme>,</l>
        <l id="s22:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s22:l7" met="3" type="+-">Die noit oog <rhyme label="c" type="m" corresp="#s22:l5">geoogd</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="2" type="-+">Daar duizend <rhyme label="a" type="f">Eng'len</rhyme></l>
        <l id="s23:l2" met="2" type="-+">Mijn troon <rhyme label="a" type="f" corresp="#s23:l1">om-henglen</rhyme>,</l>
        <l id="s23:l3" met="2" type="-+">Daar vreugd op vreugd <rhyme label="b" type="f">is</rhyme>,</l>
        <l id="s23:l4" met="3" type="+-">Daar gestaage jeugd <rhyme label="b" type="f" corresp="#s23:l3">is</rhyme>,</l>
        <l id="s23:l5" met="3" type="+-">Daar geen dood ons <rhyme label="c" type="m">scheid</rhyme></l>
        <l id="s23:l6" met="3" type="+-">O Agar! <rhyme label="d" type="f">Agar</rhyme>!</l>
        <l id="s23:l7" met="3" type="+-">In der <rhyme label="c" type="m" corresp="#s23:l5">eeuwigheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
