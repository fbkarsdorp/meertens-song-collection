    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Op die wensch, o Kloris,   <rhyme label="a" type="m">voegd</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Dat wy tonen hoe <rhyme label="a" type="m" corresp="#s1:l1">vernoegd</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Onse herten sig <rhyme label="b" type="f">bevinden</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Om gewillig en <rhyme label="c" type="m">beryd</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Tot u dienst zig te <rhyme label="b" type="f" corresp="#s1:l3">verbinden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Met een altoos ware <rhyme label="c" type="m" corresp="#s1:l4">vlyd</rhyme>.</l>
      </lg>
    </lg>
