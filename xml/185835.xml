<song>
  <meta>
    <id>185835</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De tijd is hier</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Dit is den <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die God den Heer der <rhyme label="b" type="f">Heeren</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Sonder <rhyme label="a" type="m" corresp="#s1:l1">verdrach</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghemaeckt heeft ons <rhyme label="c" type="m">profijt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Laet ons vruecht <rhyme label="b" type="f" corresp="#s1:l2">hanteren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En vrolick <rhyme label="b" type="f" corresp="#s1:l2">jubileren</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Met groot <rhyme label="c" type="m" corresp="#s1:l4">jolijt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Sijn inden gheest <rhyme label="c" type="m" corresp="#s1:l4">verblijt</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Den Tempel <rhyme label="c" type="m" corresp="#s1:l4">wijt</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Tot de hoornen vant <rhyme label="d" type="m">outaer</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Met meyen wilt <rhyme label="e" type="f">vercieren</rhyme></l>
        <l id="s1:l12" met="3" type="-+">En lustich helpen <rhyme label="e" type="f" corresp="#s1:l11">vieren</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Dees feest <rhyme label="d" type="m" corresp="#s1:l10">eerbaer</rhyme></l>
        <l id="s1:l14" met="4" type="-+">Der verrijsenissen <rhyme label="d" type="m" corresp="#s1:l10">claer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ons Paechlam <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is Christus onsen <rhyme label="b" type="f">Heeren</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Opgheoffert <rhyme label="a" type="m" corresp="#s2:l1">pleyn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Voor ons, tis <rhyme label="c" type="m">openbaer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Laet ons dan <rhyme label="b" type="f" corresp="#s2:l2">expurgeeren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Den ouden heeft, met <rhyme label="b" type="f" corresp="#s2:l2">eeren</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Houden <rhyme label="c" type="m" corresp="#s2:l4">eenpaer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Des Heeren feest <rhyme label="c" type="m" corresp="#s2:l4">eerbaer</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Maer niet <rhyme label="c" type="m" corresp="#s2:l4">voorwaer</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Int oude deesem <rhyme label="d" type="m">suyr</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Maer ongheheefde <rhyme label="e" type="f">broode</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Der waerheyt tis van <rhyme label="e" type="f" corresp="#s2:l11">noode</rhyme></l>
        <l id="s2:l13" met="2" type="-+">Suyver en <rhyme label="d" type="m" corresp="#s2:l10">puyr</rhyme></l>
        <l id="s2:l14" met="4" type="-+">Als t’gout ghepuyrt door <rhyme label="d" type="m" corresp="#s2:l10">tvuyr</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">O princelijck <rhyme label="a" type="m">greyn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Boven alle Princen <rhyme label="b" type="f">verheven</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Die ons <rhyme label="a" type="m" corresp="#s3:l1">certeyn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Den dach seer langh <rhyme label="c" type="m">verwacht</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Hebt weder <rhyme label="b" type="f" corresp="#s3:l2">ghegheven</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Den duyster nacht <rhyme label="b" type="f" corresp="#s3:l2">verdreven</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Wter sonden <rhyme label="c" type="m" corresp="#s3:l4">gracht</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Verweckt onsen <rhyme label="c" type="m" corresp="#s3:l4">ghedacht</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Ws gheests <rhyme label="c" type="m" corresp="#s3:l4">cracht</rhyme></l>
        <l id="s3:l10" met="4" type="+-">In onsen herten <rhyme label="d" type="m">plant</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Om t’aertsche te <rhyme label="e" type="f">versaecken</rhyme></l>
        <l id="s3:l12" met="3" type="-+">En t’hemelsche te <rhyme label="e" type="f" corresp="#s3:l11">smaecken</rhyme></l>
        <l id="s3:l13" met="2" type="-+">By Christum <rhyme label="d" type="m" corresp="#s3:l10">triumphant</rhyme></l>
        <l id="s3:l14" met="4" type="-+">Aen s’vaders <rhyme label="d" type="m" corresp="#s3:l10">rechterhant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
