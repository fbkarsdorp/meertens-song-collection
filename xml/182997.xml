<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182997</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Die weldoet smaakt alleen het zoet</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Die Oranje mind die smaekt 't <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van waere <rhyme label="b" type="m">vrolykheid</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Die in Antipaeters <rhyme label="a" type="m" corresp="#s1:l1">tegenspoed</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Geduldig zig <rhyme label="b" type="m" corresp="#s1:l2">verbleid</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Oranjes wandel is een <rhyme label="c" type="m">schild</rhyme>/</l>
        <l id="s1:l6" met="3" type="-+">Waer op de Antipaeters <rhyme label="d" type="m">neid</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Haer gifte pylen vrugteloos <rhyme label="c" type="m" corresp="#s1:l5">spild</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">Als de Oranje glans <rhyme label="d" type="m" corresp="#s1:l6">verspreid</rhyme> /</l>
        <l id="s1:l9" met="4" type="-+">Dus is de pligt van ieder <rhyme label="e" type="m">een</rhyme> /</l>
        <l id="s1:l10" met="3" type="-+">Oranje minnen en wel te <rhyme label="e" type="m" corresp="#s1:l9">vreen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gelyk de koesterende <rhyme label="a" type="m">zon</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">Die de oorsprong is van 't <rhyme label="b" type="m">licht</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">Zo is Oranje ons waere <rhyme label="a" type="m" corresp="#s2:l1">bron</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">En wy Broeders aen hem <rhyme label="b" type="m" corresp="#s2:l2">verplicht</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Gelyk de zon aen plaat en <rhyme label="c" type="m">boom</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Aen 't veld zyn zegen <rhyme label="d" type="m">geeft</rhyme> /</l>
        <l id="s2:l7" met="4" type="-+">Zo is Oranje ons liefde <rhyme label="c" type="m" corresp="#s2:l5">stroom</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">Die van zegen is voort <rhyme label="d" type="m" corresp="#s2:l6">gestreeft</rhyme> /</l>
        <l id="s2:l9" met="4" type="-+">Dat waer dat zy zig werkzaem <rhyme label="e" type="m">toond</rhyme> /</l>
        <l id="s2:l10" met="3" type="-+">Al die Agiethoofels <rhyme label="e" type="m" corresp="#s2:l9">loond</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gelukkig dan zo een <rhyme label="a" type="m">Broederschap</rhyme>/</l>
        <l id="s3:l2" met="3" type="-+">Die Oranjes Stam trouw <rhyme label="b" type="m">eerd</rhyme></l>
        <l id="s3:l3" met="4" type="-+">'t Is Vorst Willems deugd van trap tot <rhyme label="a" type="m" corresp="#s3:l1">trap</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Die het ons nog dagelyks <rhyme label="b" type="m" corresp="#s3:l2">leerd</rhyme> /</l>
        <l id="s3:l5" met="4" type="-+">Hy haet geveinsde <rhyme label="c" type="m">vlyjery</rhyme>;</l>
        <l id="s3:l6" met="3" type="-+">Maer mind <rhyme label="d" type="m">getrouwigheid</rhyme> /</l>
        <l id="s3:l7" met="4" type="-+">Opregte deugd verkiest Oranjes <rhyme label="c" type="m" corresp="#s3:l5">zy</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">Die haeter is van valsche <rhyme label="d" type="m" corresp="#s3:l6">neid</rhyme>;</l>
        <l id="s3:l9" met="4" type="-+">Dus weest getrouw / aenhoord en <rhyme label="e" type="m">ziet</rhyme> /</l>
        <l id="s3:l10" met="3" type="-+">Het is Oranje die u <rhyme label="e" type="m" corresp="#s3:l9">gebied</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
