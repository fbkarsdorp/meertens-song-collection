  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">O Ghy doot <rhyme label="a" type="f">cruele</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Ghy haudt my te <rhyme label="a" type="f" corresp="#s1:l1">veele</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Langhdurich <rhyme label="b" type="m">ghepraempt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Al door u <rhyme label="c" type="f">bedryven</rhyme></l>
        <l id="s1:l5" met="3" type="+-">K'sie dat ick moet <rhyme label="c" type="f" corresp="#s1:l4">blyven</rhyme></l>
        <l id="s1:l6" met="3" type="+-">K'sie dat ick moet <rhyme label="c" type="f" corresp="#s1:l4">blyven</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Noch in u <rhyme label="b" type="m" corresp="#s1:l3">ghepraemt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wat hebt ghy toch <rhyme label="a" type="f">voren</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Hebt ghy dan <rhyme label="a" type="f" corresp="#s2:l1">ghesworen</rhyme></l>
        <l id="s2:l3" met="3" type="+-">T'hauden my voor <rhyme label="b" type="m">gast</rhyme>?</l>
        <l id="s2:l4" met="3" type="+-">Dat ghy hier soo <rhyme label="c" type="f">langhe</rhyme></l>
        <l id="s2:l5" met="3" type="+-">En daer by soo <rhyme label="c" type="f" corresp="#s2:l4">stranghe</rhyme></l>
        <l id="s2:l6" met="3" type="+-">En daer by soo <rhyme label="c" type="f" corresp="#s2:l4">stranghe</rhyme></l>
        <l id="s2:l7" met="3" type="+-">My besluyt soo <rhyme label="b" type="m" corresp="#s2:l3">vast</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Ben ick u <rhyme label="a" type="f">ghegheven</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Om in u te <rhyme label="a" type="f" corresp="#s3:l1">leven</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Naer des Hemels <rhyme label="b" type="m">raedt</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">T'is om met <rhyme label="c" type="f">vercloeken</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Mijnen Godt te <rhyme label="c" type="f" corresp="#s3:l4">soecken</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Mijnen Godt te <rhyme label="c" type="f" corresp="#s3:l4">soecken</rhyme>;</l>
        <l id="s3:l7" met="3" type="+-">Niet dat u <rhyme label="b" type="m" corresp="#s3:l3">gaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Laet my dan <rhyme label="a" type="f">uyt-breken</rhyme></l>
        <l id="s4:l2" met="3" type="+-">En met soete <rhyme label="a" type="f" corresp="#s4:l1">treke</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Soecken naer den <rhyme label="b" type="m">gheest</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Mijnen Heer der <rhyme label="c" type="f">Heeren</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Die ick moet <rhyme label="c" type="f" corresp="#s4:l4">ver-eeren</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Die ick moet <rhyme label="c" type="f" corresp="#s4:l4">ver-eeren</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Met een liefde <rhyme label="b" type="m" corresp="#s4:l3">meest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Want voorwaer door <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Can ick eeuwich <rhyme label="a" type="f" corresp="#s5:l1">wesen</rhyme></l>
        <l id="s5:l3" met="3" type="+-">En syn rijck <rhyme label="b" type="m">ontfaen</rhyme>:</l>
        <l id="s5:l4" met="3" type="+-">Bleef ick op u <rhyme label="c" type="f">kijcken</rhyme></l>
        <l id="s5:l5" met="3" type="+-">K'sou daer door <rhyme label="c" type="f" corresp="#s5:l4">beswijcken</rhyme></l>
        <l id="s5:l6" met="3" type="+-">K'sou daer door <rhyme label="c" type="f" corresp="#s5:l4">beswijcken</rhyme></l>
        <l id="s5:l7" met="3" type="+-">En met u <rhyme label="b" type="m" corresp="#s5:l3">vergaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Soo dat ghy sult <rhyme label="a" type="f">moeten</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Stellen my op <rhyme label="a" type="f" corresp="#s6:l1">voeten</rhyme></l>
        <l id="s6:l3" met="3" type="+-">In my <rhyme label="b" type="m">vryicheydt</rhyme>:</l>
        <l id="s6:l4" met="3" type="+-">Om op dese <rhyme label="c" type="f">paelen</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Sien te <rhyme label="c" type="f" corresp="#s6:l4">achter:haelen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Sien te <rhyme label="c" type="f" corresp="#s6:l4">achter:haelen</rhyme></l>
        <l id="s6:l7" met="3" type="+-">Mijnw <rhyme label="b" type="m" corresp="#s6:l3">salicheydt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Lof mijnen <rhyme label="a" type="f">beminden</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Die't al cont <rhyme label="a" type="f" corresp="#s7:l1">ontbinden</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Ontbindt mijn <rhyme label="b" type="m">lichaem</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Dat ick my mach <rhyme label="c" type="f">voeghen</rhyme></l>
        <l id="s7:l5" met="3" type="+-">Om u te <rhyme label="c" type="f" corresp="#s7:l4">ghenoeghen</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Om u te <rhyme label="c" type="f" corresp="#s7:l4">ghenoeghen</rhyme></l>
        <l id="s7:l7" met="3" type="+-">En te syn <rhyme label="b" type="m" corresp="#s7:l3">bequaem</rhyme>.</l>
      </lg>
    </lg>
  </text>
