  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Jan. Ach <rhyme label="a" type="m">bitterheyt</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Ach! ach! waer vlieden <rhyme label="b" type="m">wy</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Wel! waerom dus <rhyme label="a" type="m" corresp="#s1:l1">geschreyt</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">J. 'tIs vande <rhyme label="b" type="m" corresp="#s1:l2">tyranny</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Die ick, Met schrick, Hoor <rhyme label="c" type="m">aen</rhyme>,,</l>
        <l id="s1:l6" met="2" type="-+">Maraen,, Doet <rhyme label="d" type="m">spaen</rhyme>,,</l>
        <l id="s1:l7" met="4" type="-+">'tVolc levend' onder <rhyme label="e" type="m">d'aerd</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Haer joncheyt hy niet <rhyme label="e" type="m" corresp="#s1:l7">spaert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Een Maegt, die <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Was in Gods woord <rhyme label="b" type="m">geschickt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wort van de Spaensche <rhyme label="a" type="m" corresp="#s2:l1">schaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In d'aerden muyl <rhyme label="b" type="m" corresp="#s2:l2">verstickt</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Hoor doch,, <rhyme label="c" type="m">'tgekroch</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">Wat och! Heeft <rhyme label="d" type="m">noch</rhyme></l>
        <l id="s2:l7" met="4" type="-+">'tBedrog,, 'tSchoffieren en <rhyme label="e" type="m">'tgeschend</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">End' dit gemoord geen <rhyme label="e" type="m" corresp="#s2:l7">end</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">De <rhyme label="a" type="m">Cardinael</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Bewyst hier door <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat hy ons al te <rhyme label="a" type="m" corresp="#s3:l1">mael</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Soud' dooden, kond hy <rhyme label="b" type="m" corresp="#s3:l2">maer</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Doch 'tsal,, Niet <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">U mensch,, naer wensch, <rhyme label="d" type="m">Vergaen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Wanneer ghy seer <rhyme label="e" type="m">belaen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Sult voor Gods oordeel <rhyme label="e" type="m" corresp="#s3:l7">staen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
