  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Reyn maecht eerbaer / aenhoort myn claeghen <rhyme label="a" type="m">snel</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Weest niet <rhyme label="a" type="m" corresp="#s1:l1">rebel</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Myn hert voorwaer / dat is in groot <rhyme label="a" type="m" corresp="#s1:l1">gequel</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Door dit <rhyme label="a" type="m" corresp="#s1:l1">opstel</rhyme></l>
        <l id="s1:l5" met="5" type="-+">Ghy weet toch wel / om v ben ick in <rhyme label="b" type="m">noodt</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Troost my goddinne / eer ic <rhyme label="c" type="f">ontsinne</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Door het lyden <rhyme label="b" type="m" corresp="#s1:l5">groot</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Want niemant niet in swerelts <rhyme label="a" type="m">pleyn</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Ick seght <rhyme label="a" type="m" corresp="#s2:l1">certeyn</rhyme></l>
        <l id="s2:l3" met="5" type="-+">Die myn verdriet can bluschen o lieff <rhyme label="a" type="m" corresp="#s2:l1">reyn</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Dan ghy <rhyme label="a" type="m" corresp="#s2:l1">alleyn</rhyme></l>
        <l id="s2:l5" met="5" type="-+">O weertste greyn myn smeken <rhyme label="b" type="m">aenhoort</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Off ick moet steruen laet my eens <rhyme label="c" type="f">eruen</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Troost en <rhyme label="b" type="m" corresp="#s2:l5">conffoort</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Lyff ende goet sal ick in swerelts <rhyme label="a" type="m">troon</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Stellen te <rhyme label="a" type="m" corresp="#s3:l1">loon</rhyme></l>
        <l id="s3:l3" met="5" type="-+">Vlees ende bloet sal ick waeghen om v <rhyme label="a" type="m" corresp="#s3:l1">persoon</rhyme></l>
        <l id="s3:l4" met="2" type="-+">O liefste <rhyme label="a" type="m" corresp="#s3:l1">schoon</rhyme></l>
        <l id="s3:l5" met="5" type="-+">Iae duysent doon sal ick steruen voor <rhyme label="b" type="m">dyn</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Mocht ick aenschouwe schone <rhyme label="c" type="f">jonckurouwe</rhyme></l>
        <l id="s3:l7" met="2" type="+-">V blyd <rhyme label="b" type="m" corresp="#s3:l5">aenschyn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Apollo ydone in liefde heeft <rhyme label="a" type="m">geblaeckt</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Om daphne <rhyme label="a" type="m" corresp="#s4:l1">gewaeckt</rhyme></l>
        <l id="s4:l3" met="5" type="-+">So is myn hert o schone tot v <rhyme label="a" type="m" corresp="#s4:l1">geraect</rhyme></l>
        <l id="s4:l4" met="2" type="-+">'T welck na v <rhyme label="a" type="m" corresp="#s4:l1">haeckt</rhyme></l>
        <l id="s4:l5" met="5" type="-+">O lieff vermaeckt v bedruckte <rhyme label="b" type="m">dienaer</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Die gaet al treuren en moet <rhyme label="c" type="f">beseuren</rhyme></l>
        <l id="s4:l7" met="2" type="+-">Groot lyden <rhyme label="b" type="m" corresp="#s4:l5">swaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Princes myn Ieucht door v maechdelyck <rhyme label="a" type="m">saet</rhyme></l>
        <l id="s5:l2" met="2" type="-+">En thert <rhyme label="a" type="m" corresp="#s5:l1">vergaet</rhyme></l>
        <l id="s5:l3" met="5" type="-+">O ghy die meucht my nu comen te <rhyme label="a" type="m" corresp="#s5:l1">baet</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Ende my <rhyme label="a" type="m" corresp="#s5:l1">verlaet</rhyme></l>
        <l id="s5:l5" met="5" type="-+">Heel desolaet ben ick sonder <rhyme label="b" type="m">respyt</rhyme></l>
        <l id="s5:l6" met="4" type="-+">O lieff verheuen wilt toch troost <rhyme label="c" type="f">geuen</rhyme></l>
        <l id="s5:l7" met="2" type="+-">Tis meer dan <rhyme label="b" type="m" corresp="#s5:l5">tyt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
