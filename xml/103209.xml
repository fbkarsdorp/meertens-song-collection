    <melody>Grande bourrée</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOe blijft mijn waerde <rhyme label="a" type="m">Rosemond</rhyme> ?</l>
        <l id="s1:l2" met="3" type="-+">Soo wars van weder <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die my door d'eerst <rhyme label="a" type="m" corresp="#s1:l1">Minne-wont</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ontroofde Ziel en <rhyme label="b" type="m" corresp="#s1:l2">zin</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">En hielt mijn <rhyme label="c" type="m">Hert</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Heel <rhyme label="c" type="m" corresp="#s1:l5">verwert</rhyme>:</l>
        <l id="s1:l7" met="5" type="+-">In dien strick, die my soo diere <rhyme label="d" type="m">staet</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Door dien u af-keer <rhyme label="e" type="m">nu</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Van Minne-sugjes <rhyme label="e" type="m" corresp="#s1:l8">schuw</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Mijn Liefde <rhyme label="c" type="m" corresp="#s1:l5">haet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sal altoos u bevroore <rhyme label="a" type="m">Borst</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Voor my beslooten <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En met ondooybaer Ys <rhyme label="a" type="m" corresp="#s2:l1">beschorst</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op dat ick eeuwigh <rhyme label="b" type="m" corresp="#s2:l2">quijn</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">En 't <rhyme label="c" type="m">Minne-wigt</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Schigt by <rhyme label="c" type="m" corresp="#s2:l5">Schigt</rhyme>,</l>
        <l id="s2:l7" met="5" type="+-">Daer op spil, gun liever dat ick <rhyme label="d" type="m">sterf</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En eenmael in u <rhyme label="e" type="m">schoot</rhyme></l>
        <l id="s2:l9" met="3" type="-+">(Voor duysentmael de <rhyme label="e" type="m" corresp="#s2:l8">Doot</rhyme>)</l>
        <l id="s2:l10" met="2" type="-+">Een doot <rhyme label="c" type="m" corresp="#s2:l5">verwerf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">U schoot sal een Caneele <rhyme label="a" type="m">vier</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ghy selfs het Altaer <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ick 't eenich en vermoede <rhyme label="a" type="m" corresp="#s3:l1">Dier</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Volleeft in Minne <rhyme label="b" type="m" corresp="#s3:l2">pijn</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Sal door de <rhyme label="c" type="m">vonck</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Van een <rhyme label="c" type="m" corresp="#s3:l5">lonck</rhyme>:</l>
        <l id="s3:l7" met="5" type="+-">Aengestigt, myn Offer voeren <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Op dat (gelijck ick <rhyme label="e" type="m">was</rhyme></l>
        <l id="s3:l9" met="3" type="-+">In't Minnen) uyt mijn <rhyme label="e" type="m" corresp="#s3:l8">Ass</rhyme>:</l>
        <l id="s3:l10" met="2" type="-+">Een Penicx <rhyme label="c" type="m" corresp="#s3:l5">spruyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dees troosteloose <rhyme label="a" type="m">Levens-Boey</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Baert my maer <rhyme label="b" type="m">droeffenis</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Dewijl ick als een AEtna <rhyme label="a" type="m" corresp="#s4:l1">gloey</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En u mijn Zielson <rhyme label="b" type="m" corresp="#s4:l2">min</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">En meer en <rhyme label="c" type="m">meer</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Steets <rhyme label="c" type="m" corresp="#s4:l5">verleer</rhyme>,</l>
        <l id="s4:l7" met="5" type="+-">In dien gloet, die my het Hert <rhyme label="d" type="m">doorsiet</rhyme>:</l>
        <l id="s4:l8" met="3" type="-+">Want ghy mijn Levens <rhyme label="e" type="m">ligt</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Den brandt van u <rhyme label="e" type="m" corresp="#s4:l8">gestigt</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Geen voedsel <rhyme label="c" type="m" corresp="#s4:l5">hebt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
