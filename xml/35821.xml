  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">MEt groot <rhyme label="a" type="m">ootmoet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Maria wy u <rhyme label="b" type="f">smeecken</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Krijght ons in onse <rhyme label="c" type="m">noodt</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Troost veel en <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Voor Klercken ende <rhyme label="b" type="f" corresp="#s1:l2">Leecken</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">En dat wy sonden <rhyme label="c" type="m" corresp="#s1:l3">groot</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Schouwen als de <rhyme label="c" type="m" corresp="#s1:l3">doodt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En dat wy Godes <rhyme label="d" type="f">Wetten</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Voldoen oprecht en <rhyme label="e" type="m">geheel</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En haten Satans <rhyme label="d" type="f" corresp="#s1:l8">netten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">O Moeder <rhyme label="a" type="m">Godts</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wilt voor ons al <rhyme label="b" type="f">ontfanghen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van Jesu onsen <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s2:l4" met="2" type="-+">In d'uur des <rhyme label="a" type="m" corresp="#s2:l1">doodts</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat geen van d'Helsche <rhyme label="b" type="f" corresp="#s2:l2">slanghen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ons hind'ren of doen <rhyme label="c" type="m" corresp="#s2:l3">seer</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Noch na ons <rhyme label="c" type="m" corresp="#s2:l3">begeer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Versoent ons aen uw <rhyme label="d" type="f">Soone</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Door u gebedt en <rhyme label="e" type="m">voorspraeck</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Dat hy ons wilt <rhyme label="d" type="f" corresp="#s2:l8">verschoone</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
