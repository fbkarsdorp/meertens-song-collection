  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VRiendelick <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Sijn seer seltsam, en dubbelt van <rhyme label="b" type="f">vermeugen</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Sy doen mijn smert <rhyme label="a" type="f" corresp="#s1:l1">ghedoghen</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">En wederom heel hartelijck <rhyme label="b" type="f" corresp="#s1:l2">verheughen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Wanneer als <rhyme label="c" type="m">ghy</rhyme></l>
        <l id="s1:l6" met="2" type="-+">V ooghjens <rhyme label="c" type="m" corresp="#s1:l5">bly</rhyme></l>
        <l id="s1:l7" met="3" type="-+">My toe schiet van der <rhyme label="d" type="f">syen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Dan can ick <rhyme label="e" type="f">voelen</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Myn hart cryiekend' <rhyme label="e" type="f" corresp="#s1:l8">woelen</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Doort <rhyme label="d" type="f" corresp="#s1:l7">verblyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer als ghy die gaet <rhyme label="a" type="f">stueren</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Wt lust, of vrees, afkerich tot een <rhyme label="b" type="f">ander</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">En veynst voor u <rhyme label="a" type="f" corresp="#s2:l1">ghebueren</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Of ghy niet saegt hoe treurich dat ick <rhyme label="b" type="f" corresp="#s2:l2">wander</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Laes dan tuyght <rhyme label="c" type="m">myn</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Bedroef <rhyme label="c" type="m" corresp="#s2:l5">aenschijn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Myn heymelijcke <rhyme label="d" type="f">smarte</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Die ick by <rhyme label="e" type="f">vlaeghen</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Soo gantsch vermont moet <rhyme label="e" type="f" corresp="#s2:l8">draghen</rhyme></l>
        <l id="s2:l10" met="2" type="+-">In mijn <rhyme label="d" type="f" corresp="#s2:l7">herte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als ghy my laest <rhyme label="a" type="f">ghemoeten</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Kost ick van vreucht mijn wesen niet <rhyme label="b" type="f">bedwinghen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">In plaets van u te <rhyme label="a" type="f" corresp="#s3:l1">groeten</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Begost myn tong van blyschap schier te <rhyme label="b" type="f" corresp="#s3:l2">singhen</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Ick hadt dien <rhyme label="c" type="m">tijdt</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Soo harden <rhyme label="c" type="m" corresp="#s3:l5">strijt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ick dorst u niet <rhyme label="d" type="f">aenranden</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Myn hart met <rhyme label="e" type="f">lusteb</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Soo seer smaeckmondent <rhyme label="e" type="f" corresp="#s3:l8">kusten</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Uwe <rhyme label="d" type="f" corresp="#s3:l7">handen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">V Kars blos roode <rhyme label="a" type="f">lippen</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Ontsloot ghy lief soo Lieffelijck t'was <rhyme label="b" type="f">wonder</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ick sach u oock <rhyme label="a" type="f" corresp="#s4:l1">ontslippen</rhyme></l>
        <l id="s4:l4" met="5" type="-+">De soete gunst, die daer noch schuylden <rhyme label="b" type="f" corresp="#s4:l2">onder</rhyme></l>
        <l id="s4:l5" met="2" type="-+">V goede <rhyme label="c" type="m">luym</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Myn plomp <rhyme label="c" type="m" corresp="#s4:l5">versuymt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Doet my in myn selfs <rhyme label="d" type="f">praeten</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Wrocken en <rhyme label="e" type="f">eerkauwen</rhyme>!</l>
        <l id="s4:l9" met="3" type="-+">Die gaet myn siel <rhyme label="e" type="f" corresp="#s4:l8">benauwen</rhyme></l>
        <l id="s4:l10" met="2" type="+-">Wter <rhyme label="d" type="f" corresp="#s4:l7">maten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
