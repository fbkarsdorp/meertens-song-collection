<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>176820</id>
    <date>
      <year-start>1683</year-start>
      <year-end>1683</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Myn ziel maeckt groot den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Looft hem toch altijt <rhyme label="a" type="m" corresp="#s1:l1">meer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wilt sijnen naem hoogh <rhyme label="b" type="f">prijsen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Looft hem met Loff-Sangh <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Met haer sin en <rhyme label="c" type="m" corresp="#s1:l4">ghemoet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Soo wilt hem eer <rhyme label="b" type="f" corresp="#s1:l3">bewijsen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In lijdene en <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In al wat u <rhyme label="a" type="m" corresp="#s2:l1">gheschiet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En wilt niet <rhyme label="b" type="f">murmureeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ghedenckt u eyghen <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Op Godes wille <rhyme label="c" type="m" corresp="#s2:l4">siet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Soo moetet haest <rhyme label="b" type="f" corresp="#s2:l3">verkeeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Al valt de wegh wat <rhyme label="a" type="m">langh</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door uwen <rhyme label="a" type="m" corresp="#s3:l1">ondergangh</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En wilt daerom niet <rhyme label="b" type="f">klaghen</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Looft den Heer met <rhyme label="c" type="m">ghesangh</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hy sal selve eer <rhyme label="c" type="m" corresp="#s3:l4">langh</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Voor u dat Kruyce <rhyme label="b" type="f" corresp="#s3:l3">draghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy treet die Pars <rhyme label="a" type="m">alleen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Want daer is anders <rhyme label="a" type="m" corresp="#s4:l1">gheen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die daer sal <rhyme label="b" type="f">overwinnen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dus wordt toch in hem <rhyme label="c" type="m">een</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Want daer hy is <rhyme label="c" type="m" corresp="#s4:l4">ghemeen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Gheeft hy lijtsame <rhyme label="b" type="f" corresp="#s4:l3">sinnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">In alle <rhyme label="a" type="m">teghenheyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Des Heeren Lof <rhyme label="a" type="m" corresp="#s5:l1">verbreydt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Het dient u al ten <rhyme label="b" type="f">besten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Want door lief en door <rhyme label="c" type="m">leydt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Maeckt hy u eerst <rhyme label="c" type="m" corresp="#s5:l4">bereydt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dit voeltmen eerst ten <rhyme label="b" type="f" corresp="#s5:l3">lesten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dus looft hem in sijn <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Looft hem in u <rhyme label="a" type="m" corresp="#s6:l1">ghedacht</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Looft hem in al sijn <rhyme label="b" type="f">wercken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Looft hem dagh ende <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Looft hem met gantscher <rhyme label="c" type="m" corresp="#s6:l4">macht</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Looft hem in allen <rhyme label="b" type="f" corresp="#s6:l3">percken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Lof o Heer <rhyme label="a" type="m">Sebaoth</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Lof Prince stercke <rhyme label="a" type="m" corresp="#s7:l1">Godt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Lof zy u Heer <rhyme label="b" type="f">ghesonghen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Lof zy u <rhyme label="c" type="m">heerlijckheyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Lof in der <rhyme label="c" type="m" corresp="#s7:l4">eeuwicheyt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Looft hem ghy nieuwe <rhyme label="b" type="f" corresp="#s7:l3">Tonghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
