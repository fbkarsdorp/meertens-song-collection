  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wi willen Gode <rhyme label="a" type="f">loven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Mit grooter <rhyme label="b" type="m">waerdicheit</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Hi heeft ons <rhyme label="c" type="f">ghetoghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Doer Sijn <rhyme label="b" type="m" corresp="#s1:l2">oetmoedicheit</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Soe wie mit Jhesus ledich <rhyme label="b" type="m" corresp="#s1:l2">staet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die ghebruuct <rhyme label="d" type="f">vreden</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s1:l6">Vreden</rhyme>:</l>
        <l id="s1:l8" met="4" type="-+">Die God bemint in deser <rhyme label="e" type="m">tijt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f">leven</rhyme>.</l>
        <l id="s1:l10" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s1:l6">Vreden</rhyme>:</l>
        <l id="s1:l11" met="4" type="+-">Die God bemint in deser <rhyme label="e" type="m" corresp="#s1:l8">tijt</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f" corresp="#s1:l9">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die menschen suchten <rhyme label="a" type="f">seere</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Al in dit [...] <rhyme label="b" type="m">likedal</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om alsoe corter <rhyme label="c" type="f">wijlen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat hijt ghebruken <rhyme label="b" type="m" corresp="#s2:l2">sal</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Daer om soe willen wijt laten <rhyme label="b" type="m" corresp="#s2:l2">al</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ende ghebruken <rhyme label="d" type="f">vreden</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s2:l6">Vreden</rhyme>:</l>
        <l id="s2:l8" met="4" type="-+">Die God bemint in deser <rhyme label="e" type="m">tijt</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f">leven</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s2:l6">Vrede</rhyme>:</l>
        <l id="s2:l11" met="4" type="+-">Die God bemint in deser <rhyme label="e" type="m" corresp="#s2:l8">tijt</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f" corresp="#s2:l9">leven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ic was in grooter <rhyme label="a" type="f">vare</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Doen ic my keerden <rhyme label="b" type="m">of</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van alle <rhyme label="c" type="f">creatueren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dien ic te minnen <rhyme label="b" type="m" corresp="#s3:l2">plach</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Doen ic my tot Gode <rhyme label="b" type="m" corresp="#s3:l2">gaef</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Doen ghebruucte ic <rhyme label="d" type="f">vrede</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s3:l6">Vrede</rhyme>:</l>
        <l id="s3:l8" met="4" type="-+">Die God bemint in deser <rhyme label="e" type="m">tijt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f">leven</rhyme>.</l>
        <l id="s3:l10" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s3:l6">Vrede</rhyme>:</l>
        <l id="s3:l11" met="4" type="+-">Die God bemint in deser <rhyme label="e" type="m" corresp="#s3:l8">tijt</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f" corresp="#s3:l9">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die fonteynen sijnder <rhyme label="a" type="f">vijve</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Si sijn ons <rhyme label="b" type="m">opghedaen</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Daer alle suver <rhyme label="c" type="f">harten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Uut drincken moeghen <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ende bliven inder minnen <rhyme label="b" type="m" corresp="#s4:l2">staen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ende gebruken <rhyme label="d" type="f">vreden</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s4:l6">Vrede</rhyme>:</l>
        <l id="s4:l8" met="4" type="-+">Die God bemint in deser <rhyme label="e" type="m">tijt</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f">leven</rhyme>.</l>
        <l id="s4:l10" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s4:l6">Vrede</rhyme>:</l>
        <l id="s4:l11" met="4" type="+-">Die God bemint in deser <rhyme label="e" type="m" corresp="#s4:l8">tijt</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f" corresp="#s4:l9">leven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'Nu drincket, drincket, <rhyme label="a" type="f">siele</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van desen gueden <rhyme label="b" type="m">wijn</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Drincket also <rhyme label="c" type="f">langhe</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Tot dat ghi droncken <rhyme label="b" type="m" corresp="#s5:l2">sijt</rhyme>!</l>
        <l id="s5:l5" met="4" type="-+">Drincket, drincket, siele <rhyme label="b" type="m" corresp="#s5:l2">mijn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ende ghebruuket <rhyme label="d" type="f">vrede</rhyme>.'</l>
        <l id="s5:l7" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s5:l6">Vreden</rhyme>:</l>
        <l id="s5:l8" met="4" type="-+">Die God bemint in deser <rhyme label="e" type="m">tijt</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f">leven</rhyme>.</l>
        <l id="s5:l10" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s5:l6">Vrede</rhyme>:</l>
        <l id="s5:l11" met="4" type="+-">Die God bemint in deser <rhyme label="e" type="m" corresp="#s5:l8">tijt</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f" corresp="#s5:l9">leven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat sellen wy den gueden Waert <rhyme label="a" type="f">scenken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Voer Sinen gueden <rhyme label="b" type="m">dranck</rhyme>?</l>
        <l id="s6:l3" met="3" type="-+">Dancbaerheit ende <rhyme label="c" type="f">eere</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Tot eenen sekeren <rhyme label="b" type="m" corresp="#s6:l2">pant</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">Ende bliven inder minnen <rhyme label="b" type="m" corresp="#s6:l2">brant</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Ende ghebruken <rhyme label="d" type="f">vreden</rhyme>.</l>
        <l id="s6:l7" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s6:l6">Vreden</rhyme>:</l>
        <l id="s6:l8" met="4" type="-+">Die God bemint in deser <rhyme label="e" type="m">tijt</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f">leven</rhyme>.</l>
        <l id="s6:l10" met="2" type="-+">Vre- Vre- <rhyme label="d" type="f" corresp="#s6:l6">Vreden</rhyme>:</l>
        <l id="s6:l11" met="4" type="+-">Die God bemint in deser <rhyme label="e" type="m" corresp="#s6:l8">tijt</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Die heeft sijn ewich <rhyme label="f" type="f" corresp="#s6:l9">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
