        <l id="s1:l4" met="4" type="+-">Indien gy my staag kwamt <rhyme label="a" type="f" corresp="#s1:l1">verheugen</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Ha! ha, <rhyme label="c" type="m">ha</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Dan zouje eerst <rhyme label="a" type="f" corresp="#s1:l1">deugen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Meer als het schoonst' <rhyme label="b" type="m" corresp="#s1:l2">juweel</rhyme>.</l>
