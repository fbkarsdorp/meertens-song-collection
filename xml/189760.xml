<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>189760</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ô WReede Goon! waarom wort <rhyme label="a" type="m">ik</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zoo vreedelyk <rhyme label="b" type="m">mislyd</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Myn schoone vlucht, ô Angst! ô <rhyme label="a" type="m" corresp="#s1:l1">Schrik</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">ô Harteleet! ô <rhyme label="b" type="m" corresp="#s1:l2">Spyt</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">Se ontvlucht myn oog waar dat se <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Des ben ik een verloren <rhyme label="c" type="m" corresp="#s1:l5">Man</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ach Laura! Laura! Laura <rhyme label="a" type="m">Ach</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Hoe kan het mooglyk <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat uwen haat zoo veel <rhyme label="a" type="m" corresp="#s2:l1">vermag</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat ik in angst en <rhyme label="b" type="m" corresp="#s2:l2">pyn</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Verzinkt! ô barenswee! ô <rhyme label="c" type="m">smart</rhyme>!</l>
        <l id="s2:l6" met="4" type="-+">De doot zit my in 't <rhyme label="c" type="m" corresp="#s2:l5">hart</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Verlaat gy my, die eer uw <rhyme label="a" type="m">vreucht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En zuivre wellust <rhyme label="b" type="m">won</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">Verlaat gy my, die in zyn <rhyme label="a" type="m" corresp="#s3:l1">jeugt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">ô Onbesmetbre <rhyme label="b" type="m" corresp="#s3:l2">zon</rhyme>?</l>
        <l id="s3:l5" met="4" type="-+">U schonk, met onbesweken <rhyme label="c" type="m">moedt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Een onuitblusbre <rhyme label="c" type="m" corresp="#s3:l5">liefdegloedt</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Herdenk die aangename <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Myn hoogste vreucht myn <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hoe dikwerf ik geheel <rhyme label="a" type="m" corresp="#s4:l1">verblyd</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Door wellust <rhyme label="b" type="m" corresp="#s4:l2">afgemat</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">My spiegelde in 't heerlyk <rhyme label="c" type="m">licht</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Van uw bekoorlyk <rhyme label="c" type="m" corresp="#s4:l5">aangezicht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Herdenk noch meer met welk <rhyme label="a" type="m">gezoen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik dikwerf keer op <rhyme label="b" type="m">keer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">U wierp op 't malsche <rhyme label="a" type="m" corresp="#s5:l1">klavergroen</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">En hoe ik dan weer <rhyme label="b" type="m" corresp="#s5:l2">teer</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">U weder opbeurde aan u <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Wat vuur sloop in myn <rhyme label="c" type="m" corresp="#s5:l5">ingewand</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wat vreucht genoot ik in myn <rhyme label="a" type="m">ziel</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Wen ik uw boesem <rhyme label="b" type="m">kust</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En my als Olm gekluystert <rhyme label="a" type="m" corresp="#s6:l1">hiel</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">In 't minst van smart <rhyme label="b" type="m" corresp="#s6:l2">bewust</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Aan uwe lieve tengre <rhyme label="c" type="m">leen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">ô Vreucht! vol lieve <rhyme label="c" type="m" corresp="#s6:l5">godlykheen</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ja denkt hoe gy door min <rhyme label="a" type="m">vervult</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">My noemde <rhyme label="b" type="m">Bruydegom</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En ik, geheel van vreucht <rhyme label="a" type="m" corresp="#s7:l1">doorzult</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">U Bruit, en schoone <rhyme label="b" type="m" corresp="#s7:l2">blom</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Maar Ach! die blyde tyd is <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">ô Wrange daagen van <rhyme label="c" type="m" corresp="#s7:l5">geween</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maar Ach! die altewreede <rhyme label="a" type="m">Maagt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Is geen verliefde <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Zy die myn hart en oog <rhyme label="a" type="m" corresp="#s8:l1">behaagt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Is tans niet zagt en <rhyme label="b" type="m" corresp="#s8:l2">teer</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Maar als een wreede <rhyme label="c" type="m">tygerin</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Trotseert zy myne lieve <rhyme label="c" type="m" corresp="#s8:l5">min</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Zy spot en lagt met al het <rhyme label="a" type="m">geen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dat onlangs is <rhyme label="b" type="m">geschiet</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">ô Nacht! van nare <rhyme label="a" type="m" corresp="#s9:l1">bitterheen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ach waarom worde ik <rhyme label="b" type="m" corresp="#s9:l2">niet</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Daar my de spyt het hart byt <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Niet neer geblixemt in het <rhyme label="c" type="m" corresp="#s9:l5">graf</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Vaar wel myn Laura vaar vaar <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Vaar eeuwig wel ik <rhyme label="b" type="m">ga</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">In plaas van lieflyk <rhyme label="a" type="m" corresp="#s10:l1">minnespel</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">In bosschen vroeg en <rhyme label="b" type="m" corresp="#s10:l2">spa</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Myn min betreuren tot ik <rhyme label="c" type="m">wort</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">In 't akelige graf <rhyme label="c" type="m" corresp="#s10:l5">gestort</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Want ik begeer, in 't minst noch <rhyme label="a" type="m">meest</rhyme>:</l>
        <l id="s11:l2" met="3" type="-+">ô Maagt! het leven <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">De doot maakt my niet meer <rhyme label="a" type="m" corresp="#s11:l1">bevreest</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Als onlangs is <rhyme label="b" type="m" corresp="#s11:l2">geschiet</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">ô Neen! neen, neen, ik lacht er <rhyme label="c" type="m">om</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">De Doot is by my <rhyme label="c" type="m" corresp="#s11:l5">wellekom</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Maar vrees de worm, die u <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Te bitter knagen <rhyme label="b" type="m">zal</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Dan smaakt gy in de plaas van <rhyme label="a" type="m" corresp="#s12:l1">zoet</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Een wrang en bitter <rhyme label="b" type="m" corresp="#s12:l2">gal</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Dan zult gy roepen vroeg en <rhyme label="c" type="m">spa</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Tot Kloris, en tot <rhyme label="c" type="m" corresp="#s12:l5">Lelia</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Speelnootjes leer van my, ei <rhyme label="a" type="m">leer</rhyme>!</l>
        <l id="s13:l2" met="3" type="-+">Leer, uit myn <rhyme label="b" type="m">zielverdriet</rhyme>:</l>
        <l id="s13:l3" met="4" type="-+">Dat gy Lieftalge <rhyme label="a" type="m" corresp="#s13:l1">Nimmermeer</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Spot met uw minnaar <rhyme label="b" type="m" corresp="#s13:l2">niet</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Op dat de wanhoop scherp van <rhyme label="c" type="m">tand</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">U nimmer knaagt in 't <rhyme label="c" type="m" corresp="#s13:l5">ingewand</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
