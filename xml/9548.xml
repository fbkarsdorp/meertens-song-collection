<song>
  <meta>
    <id>9548</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LOf ruste, die en goedt <rhyme label="a" type="f">is</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En goedt is, sonder <rhyme label="b" type="m">end'</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Spreeck yder die behoedt <rhyme label="a" type="f" corresp="#s1:l1">is</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En boven sijn <rhyme label="b" type="m" corresp="#s1:l2">ellend'</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Maer in de rust <rhyme label="c" type="m">gherust</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Te werden, door <rhyme label="d" type="f">vergissen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Doet hinderen de <rhyme label="c" type="m" corresp="#s1:l5">lust</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En rustes-ghever <rhyme label="d" type="f" corresp="#s1:l6">missen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">verss. 1   Ick heb in mijn <rhyme label="a" type="f">ruste</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hem, op het bed' <rhyme label="b" type="m">ghesocht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Na wien de Ziele <rhyme label="a" type="f" corresp="#s2:l1">luste</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maer was te laet <rhyme label="b" type="m" corresp="#s2:l2">bedocht</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Ick sochte met <rhyme label="c" type="m">verdriet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van mijne <rhyme label="d" type="f">bangigheden</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Doch ick en vandt hem <rhyme label="c" type="m" corresp="#s2:l5">niet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hy was al <rhyme label="d" type="f" corresp="#s2:l6">wegh-getreden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'k Sprack sonder <rhyme label="a" type="f">onder-laten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Nu wil ick, <rhyme label="b" type="m">op-gestaen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">De wijcken ende <rhyme label="a" type="f" corresp="#s3:l1">straten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Der Stadt, al' <rhyme label="b" type="m" corresp="#s3:l2">omme-gaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En die in 's herten <rhyme label="c" type="m">grondt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">My leyt, sal ick gaen <rhyme label="d" type="f">soecken</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maer ick hem niet en <rhyme label="c" type="m" corresp="#s3:l5">vond'</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In al de Werelts <rhyme label="d" type="f" corresp="#s3:l6">hoecken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doe vonden my de <rhyme label="a" type="f">Wachters</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Mijn vraegh was; Lieve <rhyme label="b" type="m">lie'n</rhyme>!</l>
        <l id="s4:l3" met="3" type="-+">Ghy trouwe <rhyme label="a" type="f" corresp="#s4:l1">Vreed'-betrachters</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hebt ghy mijn Lief <rhyme label="b" type="m" corresp="#s4:l2">gesien</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">Het uytstel was te <rhyme label="c" type="m">langh</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ick gingh een weynigh <rhyme label="d" type="f">verder</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En na des herts <rhyme label="c" type="m" corresp="#s4:l5">verlangh</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Ick vond' den goeden <rhyme label="d" type="f" corresp="#s4:l6">Herder</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick hieu en hield' hem <rhyme label="a" type="f">vaste</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ick hem niet gaen en <rhyme label="b" type="m">liet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Tot dat hy my <rhyme label="a" type="f" corresp="#s5:l1">ontlaste</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Van druck die was <rhyme label="b" type="m" corresp="#s5:l2">geschiet</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Tot dat hy was <rhyme label="c" type="m">gebracht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In 's moeders <rhyme label="d" type="f">binnen-Kamer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En had' ghenomen <rhyme label="c" type="m" corresp="#s5:l5">acht</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Op mijnen Zielen <rhyme label="d" type="f" corresp="#s5:l6">jamer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">'k Sprack, Dochters ick <rhyme label="a" type="f">besweere</rhyme></l>
        <l id="s6:l2" met="3" type="-+">By hinden op het <rhyme label="b" type="m">velt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">By goet, by soet, by <rhyme label="a" type="f" corresp="#s6:l1">eere</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat ghy geen liefde <rhyme label="b" type="m" corresp="#s6:l2">quelt</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Dat ghy geen liefde <rhyme label="c" type="m">steurt</rhyme>;</l>
        <l id="s6:l6" met="3" type="-+">Dat ghy'se laet <rhyme label="d" type="f">gheworden</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Dat ghy geen liefde <rhyme label="c" type="m" corresp="#s6:l5">scheurt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Maer laet'se vast <rhyme label="d" type="f" corresp="#s6:l6">begorden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wie komt'er <rhyme label="a" type="f">op-gebroocken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van uyt de <rhyme label="b" type="m">wildernis</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Met Mirre, met <rhyme label="a" type="f" corresp="#s7:l1">wieroocken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die op-getoghen <rhyme label="b" type="m" corresp="#s7:l2">is</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Ghelijck een <rhyme label="c" type="m">roock-Pylaer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Met specery <rhyme label="d" type="f">gevoedert</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Met reuck-werck, in het <rhyme label="c" type="m" corresp="#s7:l5">hayr</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Der Kruydeniers <rhyme label="d" type="f" corresp="#s7:l6">gepoedert</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick ben het, siet de <rhyme label="a" type="f">plaetse</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van mijnen <rhyme label="b" type="m">Bruydegom</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Hoe heerlijcken <rhyme label="a" type="f" corresp="#s8:l1">staet'se</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Met Engelen <rhyme label="b" type="m" corresp="#s8:l2">rondtom</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Ver boven <rhyme label="c" type="m">Salomon</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Wiens bed' maer tsestigh <rhyme label="d" type="f">Helden</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Van Israel <rhyme label="c" type="m" corresp="#s8:l5">bevon</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Die sich in orden <rhyme label="d" type="f" corresp="#s8:l6">stelden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Die met gestaelde <rhyme label="a" type="f">Sweerden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Ter Oorlogh wel <rhyme label="b" type="m">gheleert</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Van sijne Koetse <rhyme label="a" type="f" corresp="#s9:l1">keerden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Het geen de ruste <rhyme label="b" type="m" corresp="#s9:l2">weert</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Wiens koets maer was <rhyme label="c" type="m">gebout</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Bruydse-bedd' het <rhyme label="d" type="f">alderbeste</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Van geurigh <rhyme label="c" type="m" corresp="#s9:l5">Ceder-hout</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Uyt Libanons <rhyme label="d" type="f" corresp="#s9:l6">gheweste</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De Pilaers maer van <rhyme label="a" type="f">Silver</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">De Vloer met Goudt <rhyme label="b" type="m">gespreydt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">'t Gordijn van Oest'ren <rhyme label="a" type="f" corresp="#s10:l1">schilver</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Het Bedde-kleedt <rhyme label="b" type="m" corresp="#s10:l2">genaeyd</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Met Docht'ren <rhyme label="c" type="m">liefde-naeldt</rhyme>:</l>
        <l id="s10:l6" met="3" type="-+">Maer alle dit <rhyme label="d" type="f">geboeysel</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Seer ver werdt <rhyme label="c" type="m" corresp="#s10:l5">over-haeldt</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Van 't sonder hand' <rhyme label="d" type="f" corresp="#s10:l6">geschoeysel</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ghy Sions <rhyme label="a" type="f">Maeghde-vrouwen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Besiet de <rhyme label="b" type="m">Bruyloft-kroon</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Van Salomonis <rhyme label="a" type="f" corresp="#s11:l1">trouwen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En heerlijckheyts <rhyme label="b" type="m" corresp="#s11:l2">vertoon</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Maer boven dat <rhyme label="c" type="m">geheucht</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Het lustigh rustigh <rhyme label="d" type="f">leven</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">En d'ongesteurde <rhyme label="c" type="m" corresp="#s11:l5">vreucht</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Den saligen <rhyme label="d" type="f" corresp="#s11:l6">ghegeven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
