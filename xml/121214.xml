    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Maar ach <rhyme label="a" type="m">helaas</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Waar ben ik toe <rhyme label="b" type="f">gebooren</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Op een zoo Jonge <rhyme label="c" type="m">maagt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">die ik zoo heb <rhyme label="b" type="f" corresp="#s1:l2">verkooren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zy is zoo wonder <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Spant boven al de <rhyme label="d" type="m" corresp="#s1:l5">kroon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Was op een avond <rhyme label="a" type="m">laat</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het Maantje scheen met <rhyme label="b" type="f">stralen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">dat ik dagt aan myn <rhyme label="c" type="m">lief</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Myn hert kwam <rhyme label="b" type="f" corresp="#s2:l2">openbaren</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Maar ach Ã´ Goden <rhyme label="d" type="m">ach</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Wat antwoord zy my <rhyme label="d" type="m" corresp="#s2:l5">gaf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'t Was op een avond <rhyme label="a" type="m">laat</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ik zag een aardig <rhyme label="b" type="f">Meisje</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zy was zoo wonder <rhyme label="c" type="m">schoon</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Zoo poezel blank van <rhyme label="b" type="f" corresp="#s3:l2">vleisje</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Ik dagt wel in myn <rhyme label="d" type="m">zin</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gy zyt myn <rhyme label="d" type="m" corresp="#s3:l5">ziels-vrindin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik ben nog veel te <rhyme label="a" type="m">Jong</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En daar toe niet <rhyme label="b" type="f">genegen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Om my dog alzo <rhyme label="c" type="m">vroeg</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">In het Slaverney te <rhyme label="b" type="f" corresp="#s4:l2">geeven</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Jonkman dat is een <rhyme label="d" type="m">last</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die voor my niet en <rhyme label="d" type="m" corresp="#s4:l5">past</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maar ach <rhyme label="a" type="m">helaas</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Waar toe ben ik <rhyme label="b" type="f">geboren</rhyme>?</l>
        <l id="s5:l3" met="3" type="-+">Om een zoo Jonge <rhyme label="c" type="m">maagd</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die ik zoo heb <rhyme label="b" type="f" corresp="#s5:l2">verkoren</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zy is zoo wonder <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Spant boven al de <rhyme label="d" type="m" corresp="#s5:l5">kroon</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ik bid u liefje <rhyme label="a" type="m">zoet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Laat my dog troost <rhyme label="b" type="f">verwerven</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Eer ik nog voor u <rhyme label="c" type="m">deur</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Van droefheid kom te <rhyme label="b" type="f" corresp="#s6:l2">sterven</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Verandert dog u <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ziet dat ik u <rhyme label="d" type="m" corresp="#s6:l5">bemin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ik meende dat 't <rhyme label="a" type="m">Swaard</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Myn boezem kwam <rhyme label="b" type="f">doorsteeken</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Doen ik myn zoete <rhyme label="c" type="m">lief</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die woorden hoorden <rhyme label="b" type="f" corresp="#s7:l2">spreeken</rhyme>;</l>
        <l id="s7:l5" met="3" type="-+">Ik dogt nu moet ik <rhyme label="d" type="m">heen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En blyven in 't <rhyme label="d" type="m" corresp="#s7:l5">geween</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Vertrek Jonkman <rhyme label="a" type="m">vertrek</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Hoe staat gy zoo te <rhyme label="b" type="f">gapen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">De nagt is naast <rhyme label="c" type="m">voorby</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">En ik moet nog wat <rhyme label="b" type="f" corresp="#s8:l2">slapen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Gaat zoekt een ander <rhyme label="d" type="m">maagd</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die u Jonkhert <rhyme label="d" type="m" corresp="#s8:l5">behaagt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Nu doen ik myn <rhyme label="a" type="m">beklagt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Aan alle myne <rhyme label="b" type="f">Gooden</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">In 't diepste van de <rhyme label="c" type="m">nagt</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Kom ik tot u <rhyme label="b" type="f" corresp="#s9:l2">gevlooden</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Als sterre, zon en <rhyme label="d" type="m">maan</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die aan den Hemel <rhyme label="d" type="m" corresp="#s9:l5">staan</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Nu heb ik myn hert <rhyme label="a" type="m">verligt</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Door Bachus <rhyme label="b" type="f">Medecynen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Ik ben nu uit den <rhyme label="c" type="m">nood</rhyme>!</l>
        <l id="s10:l4" met="3" type="-+">En buyten alle <rhyme label="b" type="f" corresp="#s10:l2">pynen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Altyd op Bas, <rhyme label="d" type="m">Fiool</rhyme>;</l>
        <l id="s10:l6" met="3" type="-+">Maar nooit op Venus <rhyme label="d" type="m" corresp="#s10:l5">school</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Gy Jonkmans wie gy <rhyme label="a" type="m">zyt</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Kunt gy geen troost <rhyme label="b" type="f">verwerven</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Van een zoo trosse <rhyme label="c" type="m">maagd</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Wild van geen droefheid <rhyme label="b" type="f" corresp="#s11:l2">sterven</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Maar ach volg myn <rhyme label="d" type="m">raad</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Kiest Bachus voor u <rhyme label="d" type="m" corresp="#s11:l5">maat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
