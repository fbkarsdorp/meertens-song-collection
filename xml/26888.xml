  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Hett ich siebenn w nschenn jn meiner <rhyme label="a" type="m">gwaltt</rhyme> /</l>
        <l id="s1:l2" met="4" type="-+">hann ich recht oder hab ich <rhyme label="b" type="m">vnrecht</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">So woldt ich w nschenn werdenn nimmer <rhyme label="a" type="m" corresp="#s1:l1">altt</rhyme> /</l>
        <l id="s1:l4" met="4" type="-+">Sagtt mir hab ich rechtt / oder hab ich <rhyme label="b" type="m" corresp="#s1:l2">vnrechtt</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die ander w nsch die sall <rhyme label="a" type="m">sein</rhyme> /</l>
        <l id="s2:l2" met="4" type="-+">hab ich vnrechtt oder hab ich <rhyme label="b" type="m">recht</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">das nymmer sehell  soll khomenn jn <rhyme label="a" type="m" corresp="#s2:l1">pein</rhyme> /</l>
        <l id="s2:l4" met="4" type="-+">Sacht mir hab ich <rhyme label="b" type="m" corresp="#s2:l2">vnrechtt</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die dritte w nsche die is <rhyme label="a" type="m">also</rhyme> /</l>
        <l id="s3:l2" met="4" type="-+">hab ich <rhyme label="b" type="m">vnrechtt</rhyme> /</l>
        <l id="s3:l3" met="4" type="-+">Nemantz schuldig / altzeitt geltes <rhyme label="a" type="m" corresp="#s3:l1">genoch</rhyme> /</l>
        <l id="s3:l4" met="4" type="-+">Sagtt mir hab ich  vnrechtt / <rhyme label="b" type="m" corresp="#s3:l2">oder</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die vierte w nsch die sall <rhyme label="a" type="m">sein</rhyme> /</l>
        <l id="s4:l2" met="4" type="-+">hab ich <rhyme label="b" type="m">vnrechtt</rhyme> /</l>
        <l id="s4:l3" met="4" type="-+">Altzeitt frolich bei h bschenn Junckfrawenn <rhyme label="a" type="m" corresp="#s4:l1">sein</rhyme> /</l>
        <l id="s4:l4" met="4" type="-+">Sagtt mir hab vnrecht / <rhyme label="b" type="m" corresp="#s4:l2">oder</rhyme> /</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die f nffte w nsch die is <rhyme label="a" type="m">also</rhyme> /</l>
        <l id="s5:l2" met="4" type="-+">hab ich <rhyme label="b" type="m">vnrecht</rhyme> /</l>
        <l id="s5:l3" met="4" type="-+">Nimmer tra rich altzeitt <rhyme label="a" type="m" corresp="#s5:l1">fro</rhyme> /</l>
        <l id="s5:l4" met="4" type="-+">Sagtt mir hab ich vnrechtt / <rhyme label="b" type="m" corresp="#s5:l2">oder</rhyme> /</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die sechste w nsche die sall <rhyme label="a" type="m">sein</rhyme></l>
        <l id="s6:l2" met="4" type="-+">hab ich <rhyme label="b" type="m">vnrecht</rhyme> /</l>
        <l id="s6:l3" met="4" type="-+">fridlich bei sein fr ndekin / vnnd ich bei der <rhyme label="a" type="m" corresp="#s6:l1">mein</rhyme> /</l>
        <l id="s6:l4" met="4" type="-+">Sagtt mir hab ich rechtt / <rhyme label="b" type="m" corresp="#s6:l2">oder</rhyme> /</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die sebende w nsch der ist die <rhyme label="a" type="m">best</rhyme> /</l>
        <l id="s7:l2" met="4" type="-+">hab ich <rhyme label="b" type="m">vnrechtt</rhyme> /</l>
        <l id="s7:l3" met="4" type="-+">Das nymmer kleffer irgens ab en w <rhyme label="a" type="m" corresp="#s7:l1">st</rhyme> /</l>
        <l id="s7:l4" met="4" type="-+">Sagtt mir hab ich vnrechtt oder hab ich <rhyme label="b" type="m" corresp="#s7:l2">rechtt</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
