<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182656</id>
    <date>
      <year-start>1759</year-start>
      <year-end>1759</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie hoe gelukkig zijn ze dan</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Waereld! Vol van <rhyme label="a" type="m">Overdaad</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wat baand gy open <rhyme label="b" type="f">wegen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Tot alderhande <rhyme label="a" type="m" corresp="#s1:l1">kwaad</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Tot Wraak, verderf en <rhyme label="c" type="m">Moord</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Een zaak noyt meer <rhyme label="c" type="m" corresp="#s1:l4">gehoord</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Zal ik u brengen <rhyme label="c" type="m" corresp="#s1:l4">voord</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In Zeeland is die Feyt <rhyme label="a" type="m">geschied</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ten teyd doen Hertog <rhyme label="b" type="f">Karel</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hier voerden groot <rhyme label="a" type="m" corresp="#s2:l1">gebied</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Zijn Gouverneur <rhyme label="c" type="m">bekend</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Heeft hem tot kwaad <rhyme label="c" type="m" corresp="#s2:l4">gewend</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Maar kreeg een droevig <rhyme label="c" type="m" corresp="#s2:l4">end</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy sloeg een geyl en dertel <rhyme label="a" type="m">Oog</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Op een Edele <rhyme label="b" type="f">Vrouwe</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">'t Geen hem op 't laast <rhyme label="a" type="m" corresp="#s3:l1">bedroog</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">En sprak mijn <rhyme label="c" type="m">Herten-lust</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zal nimmer zijn <rhyme label="c" type="m" corresp="#s3:l4">gerust</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Voor gy mijn Vlamme <rhyme label="c" type="m" corresp="#s3:l4">blust</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zy als een eerbaar kuysche <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Sprak Heer zou ik <rhyme label="b" type="f">verbreeken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met u mijn Egte <rhyme label="a" type="m" corresp="#s4:l1">Trouw</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Zou ik mijn lieve <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Doen zulken gruwel <rhyme label="c" type="m" corresp="#s4:l4">an</rhyme>?</l>
        <l id="s4:l6" met="3" type="-+">Och! Spreekt daar nimmer <rhyme label="c" type="m" corresp="#s4:l4">van</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy met een opstinaat <rhyme label="a" type="m">gemoed</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ging hem na Huys toe <rhyme label="b" type="f">keeren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Heel toornig en <rhyme label="a" type="m" corresp="#s5:l1">verwoed</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hy liet van stonden <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zijn Dienaars heenen <rhyme label="c" type="m" corresp="#s5:l4">gaan</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En nam haar Man <rhyme label="c" type="m" corresp="#s5:l4">gevaan</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">U Man (sprak hy) wou 't Land <rhyme label="a" type="m">verraan</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ik sal hem door <rhyme label="b" type="f">Beuls-handen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Het Hoofd van 't Lighaam <rhyme label="a" type="m" corresp="#s6:l1">slaan</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Maar by mijn magt ik <rhyme label="c" type="m">zweer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Voldoet gy mijn <rhyme label="c" type="m" corresp="#s6:l4">begeer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Gy krijgt u Man dan <rhyme label="c" type="m" corresp="#s6:l4">weer</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De Zond werd hem vergeven <rhyme label="a" type="m">dra</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Voldoet gy mijn <rhyme label="b" type="f">lusten</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zo krijgt u Man <rhyme label="a" type="m" corresp="#s7:l1">gena</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Dus kiest in dit <rhyme label="c" type="m">geval</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Mijn Wil te doen voor <rhyme label="c" type="m" corresp="#s7:l4">al</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Of ik hem straffen <rhyme label="c" type="m" corresp="#s7:l4">zal</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De kuysche Vrouwe toog <rhyme label="a" type="m">mitsdien</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Zeer bitterlijk aan 't <rhyme label="b" type="f">schryen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Zal ik mijn Man dan <rhyme label="a" type="m" corresp="#s8:l1">zien</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">'t Hoofd vallen voor hem <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Of af staan van mijn <rhyme label="c" type="m" corresp="#s8:l4">Eer</rhyme>?</l>
        <l id="s8:l6" met="3" type="-+">En doen na zijn <rhyme label="c" type="m" corresp="#s8:l4">begeer</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Mijn lieve Man is my zo <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Als eenig Schat op <rhyme label="b" type="f">Aarden</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Of als mijn Eer my <rhyme label="a" type="m" corresp="#s9:l1">doet</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Mijn Eer mijn <rhyme label="c" type="m">Waardste-pand</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Mijn Man mijn <rhyme label="c" type="m" corresp="#s9:l4">Regter-hand</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Moet een van by van <rhyme label="c" type="m" corresp="#s9:l4">kant</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Den Booswigt greep haar by de <rhyme label="a" type="m">Hand</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">In 't midden van haar <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Wierp haar op 't <rhyme label="a" type="m" corresp="#s10:l1">ledikant</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En heeft zoo <rhyme label="c" type="m">ongerust</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Zijn geyl en dertele <rhyme label="c" type="m" corresp="#s10:l4">lust</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Tot walgens toe <rhyme label="c" type="m" corresp="#s10:l4">geblust</rhyme>,</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Doen sprak hy Vrouwe gaat nu <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Op Morgen moogt gy <rhyme label="b" type="f">halen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Uw Man uyt het <rhyme label="a" type="m" corresp="#s11:l1">geween</rhyme>;</l>
        <l id="s11:l4" met="3" type="-+">Bedroef is zy <rhyme label="c" type="m">gegaan</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Hy liet van stonden <rhyme label="c" type="m" corresp="#s11:l4">aan</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Haar Man het Hoofd <rhyme label="c" type="m" corresp="#s11:l4">af-slaan</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Des Morgens opend' zy haar <rhyme label="a" type="m">Deur</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En ging vrymoedig <rhyme label="b" type="f">treden</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Regt na den <rhyme label="a" type="m" corresp="#s12:l1">Gouverneur</rhyme>;</l>
        <l id="s12:l4" met="3" type="-+">En sprak Groot-mogend <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Geeft my mijn Man nu <rhyme label="c" type="m" corresp="#s12:l4">weer</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Voor mijn geschonden <rhyme label="c" type="m" corresp="#s12:l4">Eer</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Hy riep twee Dienaars voor den <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Die bragten haar ter <rhyme label="b" type="f">plaatze</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Daar zy haar Man aan <rhyme label="a" type="m" corresp="#s13:l1">zag</rhyme>;</l>
        <l id="s13:l4" met="3" type="-+">En lag daar zonder <rhyme label="c" type="m">Hoofd</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Door 't Bloedig-zwaard <rhyme label="c" type="m" corresp="#s13:l4">verdooft</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Van het leven geheel <rhyme label="c" type="m" corresp="#s13:l4">berooft</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Zy viel op 't doode Lighaam <rhyme label="a" type="m">neer</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">En kusten zijne <rhyme label="b" type="f">wangen</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">En schreyden om haar <rhyme label="a" type="m" corresp="#s14:l1">Eer</rhyme>:</l>
        <l id="s14:l4" met="3" type="-+">Trok 't Hayr uyt de <rhyme label="c" type="m">Harssen-pan</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">En riep: ô sno <rhyme label="c" type="m" corresp="#s14:l4">Tyran</rhyme>!</l>
        <l id="s14:l6" met="3" type="-+">Vermoord gy zo mijn <rhyme label="c" type="m" corresp="#s14:l4">Man</rhyme>,</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Zy liep als zinloos en <rhyme label="a" type="m">verwoed</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">En klaagden het aan haar <rhyme label="b" type="f">Vrienden</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Haar aldernaaste <rhyme label="a" type="m" corresp="#s15:l1">Bloed</rhyme>:</l>
        <l id="s15:l4" met="3" type="-+">Den Gouverneur ô <rhyme label="c" type="m">schand</rhyme>!</l>
        <l id="s15:l5" met="3" type="-+">Heeft my mijn Eer <rhyme label="c" type="m" corresp="#s15:l4">ontmand</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Mijn Man geleyd in 't <rhyme label="c" type="m" corresp="#s15:l4">Zand</rhyme>,</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Haar Vrienden spraken: Nigt, wel <rhyme label="a" type="m">hoe</rhyme>!</l>
        <l id="s16:l2" met="3" type="-+">Laat ons van Zeeland <rhyme label="b" type="f">reyzen</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Terstond na Holland <rhyme label="a" type="m" corresp="#s16:l1">toe</rhyme>;</l>
        <l id="s16:l4" met="3" type="-+">Beklagen daar dit <rhyme label="c" type="m">kwaad</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">En gruwelijke <rhyme label="c" type="m" corresp="#s16:l4">daad</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Aan d'Hertog hoog van <rhyme label="c" type="m" corresp="#s16:l4">Staat</rhyme>,</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Men kwam zeer haast en <rhyme label="a" type="m">ongemeen</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Tot Delft binnen <rhyme label="b" type="f">Reyden</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Men ging ten Hove <rhyme label="a" type="m" corresp="#s17:l1">treen</rhyme>:</l>
        <l id="s17:l4" met="3" type="-+">Zy sprak den Hertog <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Deed hem de Zaak <rhyme label="c" type="m" corresp="#s17:l4">verstaan</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Hoe 't al was toe <rhyme label="c" type="m" corresp="#s17:l4">gegaan</rhyme>,</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Ach! wreek de Dood van mijne <rhyme label="a" type="m">Man</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Wreek mijn geschonden <rhyme label="b" type="f">Eere</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Zo ymand wreeken <rhyme label="a" type="m" corresp="#s18:l1">kan</rhyme>;</l>
        <l id="s18:l4" met="3" type="-+">U valschen <rhyme label="c" type="m">Gouverneur</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Die braght my in't <rhyme label="c" type="m" corresp="#s18:l4">getreur</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Bestelt hem straf daer <rhyme label="c" type="m" corresp="#s18:l4">veur</rhyme>,</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Den Hertogh sprack u wel <rhyme label="a" type="m">versint</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">en let wel op u <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Hy is mijn een goet <rhyme label="a" type="m" corresp="#s19:l1">vrindt</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Dus seght in dit <rhyme label="c" type="m">geval</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">De suyvere waerheyt <rhyme label="c" type="m" corresp="#s19:l4">al</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">'k Sweer ick hem staffen <rhyme label="c" type="m" corresp="#s19:l4">sal</rhyme>,</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Maer soo ick dees saek onwaer <rhyme label="a" type="m">bevint</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Soo sal ick doen <rhyme label="b" type="f">smaken</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">De doodt die u <rhyme label="a" type="m" corresp="#s20:l1">verslint</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Ick daegh hem voor het <rhyme label="c" type="m">recht</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">Dus hier in <rhyme label="c" type="m" corresp="#s20:l4">ovelegt</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Bedenckt wel wat gy <rhyme label="c" type="m" corresp="#s20:l4">segt</rhyme>,</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Zy sprack mijn Heer is dit <rhyme label="a" type="m">onwaer</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Soo laet mijn lichaem <rhyme label="b" type="f">sagen</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">Aen stucken van <rhyme label="a" type="m" corresp="#s21:l1">malkaer</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Of helpt my onder de <rhyme label="c" type="m">aerdt</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Door een scherp-rechters <rhyme label="c" type="m" corresp="#s21:l4">zwaert</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">De doot my niet <rhyme label="c" type="m" corresp="#s21:l4">vervaert</rhyme>,</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Den Hertogh liet den breeden <rhyme label="a" type="m">Raet</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">Terstont ten Hoof <rhyme label="b" type="f">vergaren</rhyme>:</l>
        <l id="s22:l3" met="3" type="-+">Om dit vervloeckte <rhyme label="a" type="m" corresp="#s22:l1">quaet</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">Te straffen <rhyme label="c" type="m">ongemeen</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Men deed de vrouw <rhyme label="c" type="m" corresp="#s22:l4">alleen</rhyme></l>
        <l id="s22:l6" met="3" type="-+">Al in een kamer <rhyme label="c" type="m" corresp="#s22:l4">treen</rhyme>;</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Toen quam den Booswicht voor den <rhyme label="a" type="m">dagh</rhyme>,</l>
        <l id="s23:l2" met="3" type="-+">Voor d'hoogen Raedt <rhyme label="b" type="f">verschijnen</rhyme>,</l>
        <l id="s23:l3" met="3" type="-+">En sprack met bly <rhyme label="a" type="m" corresp="#s23:l1">gelagh</rhyme>:</l>
        <l id="s23:l4" met="3" type="-+">Wat 's d' oorsaeck doch mijn <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Dat ick om u <rhyme label="c" type="m" corresp="#s23:l4">begeer</rhyme></l>
        <l id="s23:l6" met="3" type="-+">Kom reysen hier dus <rhyme label="c" type="m" corresp="#s23:l4">veer</rhyme>?</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Den Hertog sprak hem vreesselijk <rhyme label="a" type="m">an</rhyme>,</l>
        <l id="s24:l2" met="3" type="-+">Gy eer-vergeten <rhyme label="b" type="f">Schelm</rhyme>,</l>
        <l id="s24:l3" met="3" type="-+">Wat quaedt hebt gy <rhyme label="a" type="m" corresp="#s24:l1">gedaen</rhyme>?</l>
        <l id="s24:l4" met="3" type="-+">Een eerb're vrou <rhyme label="c" type="m">verkracht</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Haer man ter doodt <rhyme label="c" type="m" corresp="#s24:l4">gebracht</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">Denckt dat ghy straf <rhyme label="c" type="m" corresp="#s24:l4">verwacht</rhyme>,</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Hy sprack, wie leyt my dit te <rhyme label="a" type="m">last</rhyme>?</l>
        <l id="s25:l2" met="3" type="-+">Het zijn verdichte <rhyme label="b" type="f">loogens</rhyme>,</l>
        <l id="s25:l3" met="3" type="-+">O Vorst! daer 's niet aen <rhyme label="a" type="m" corresp="#s25:l1">vast</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">Stelt mijn party ten <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">Ick zweer u by de <rhyme label="c" type="m" corresp="#s25:l4">Goon</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">Hy krijgt verdiende <rhyme label="c" type="m" corresp="#s25:l4">loon</rhyme>,</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Den Hertogh sprack, O <rhyme label="a" type="m">Gouverneur</rhyme>,</l>
        <l id="s26:l2" met="3" type="-+">'k Sal oprechte blijck u <rhyme label="b" type="f">toonen</rhyme>:</l>
        <l id="s26:l3" met="3" type="-+">Men opende de <rhyme label="a" type="m" corresp="#s26:l1">deur</rhyme>,</l>
        <l id="s26:l4" met="3" type="-+">Toen wiert sijn haert <rhyme label="c" type="m">belaen</rhyme>,</l>
        <l id="s26:l5" met="3" type="-+">Hy sagh de Vrouwe <rhyme label="c" type="m" corresp="#s26:l4">aen</rhyme>,</l>
        <l id="s26:l6" met="3" type="-+">Bedroeft daer voor hem <rhyme label="c" type="m" corresp="#s26:l4">staen</rhyme>,</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Don Carel wiert als <rhyme label="a" type="m">opstinaet</rhyme>,</l>
        <l id="s27:l2" met="3" type="-+">Sprack, kendt gy wel dees <rhyme label="b" type="f">vrouwe</rhyme>?</l>
        <l id="s27:l3" met="3" type="-+">Die hier voor u <rhyme label="a" type="m" corresp="#s27:l1">staet</rhyme>?</l>
        <l id="s27:l4" met="3" type="-+">Dit is u <rhyme label="c" type="m">vyandin</rhyme>?</l>
        <l id="s27:l5" met="3" type="-+">Die gy door geyle <rhyme label="c" type="m" corresp="#s27:l4">min</rhyme>,</l>
        <l id="s27:l6" met="3" type="-+">Dwongh naer u lust en <rhyme label="c" type="m" corresp="#s27:l4">sin</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Hy viel van schaemt' ter Aerden <rhyme label="a" type="m">neer</rhyme>,</l>
        <l id="s28:l2" met="3" type="-+">Den Hertogh sprack met <rhyme label="b" type="f">reden</rhyme>:</l>
        <l id="s28:l3" met="3" type="-+">Hersteld dees vrouw haer <rhyme label="a" type="m" corresp="#s28:l1">eer</rhyme>;</l>
        <l id="s28:l4" met="3" type="-+">Ick wil tot straffens <rhyme label="c" type="m">schuldt</rhyme>,</l>
        <l id="s28:l5" met="3" type="-+">Dat gy haer met <rhyme label="c" type="m" corresp="#s28:l4">gedult</rhyme></l>
        <l id="s28:l6" met="3" type="-+">Terstondt hier echten <rhyme label="c" type="m" corresp="#s28:l4">sult</rhyme>,</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Don Carel vraegde aen dees <rhyme label="a" type="m">Vrouw</rhyme></l>
        <l id="s29:l2" met="3" type="-+">Of sy haer wou <rhyme label="b" type="f">begeven</rhyme></l>
        <l id="s29:l3" met="3" type="-+">met hem in d'Echte <rhyme label="a" type="m" corresp="#s29:l1">trouw</rhyme>,</l>
        <l id="s29:l4" met="3" type="-+">Sy sprack o vorst ick <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s29:l5" met="3" type="-+">In d'echt niet nemen <rhyme label="c" type="m" corresp="#s29:l4">an</rhyme></l>
        <l id="s29:l6" met="3" type="-+">Den moorder van mijn <rhyme label="c" type="m" corresp="#s29:l4">man</rhyme>,</l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="4" type="-+">Hy sprack 't sal u niet schadelrijck <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s30:l2" met="3" type="-+">Gy sult oock wraeck <rhyme label="b" type="f">genieten</rhyme>,</l>
        <l id="s30:l3" met="3" type="-+">Dus doet den raad van <rhyme label="a" type="m" corresp="#s30:l1">mijn</rhyme>;</l>
        <l id="s30:l4" met="3" type="-+">Dus voort door Vrienden <rhyme label="c" type="m">raad</rhyme>,</l>
        <l id="s30:l5" met="3" type="-+">En de Hertogs zoete <rhyme label="c" type="m" corresp="#s30:l4">praat</rhyme>,</l>
        <l id="s30:l6" met="3" type="-+">Zy 't Ia-woord geven <rhyme label="c" type="m" corresp="#s30:l4">gaat</rhyme>,</l>
      </lg>
      <lg id="s31" type="stanza">
        <l id="s31:l1" met="4" type="-+">Dus wierd d'Weduw' weer de <rhyme label="a" type="m">Bruyd</rhyme>,</l>
        <l id="s31:l2" met="3" type="-+">En d'Huwelijks <rhyme label="b" type="f">Voorwaarden</rhyme>,</l>
        <l id="s31:l3" met="3" type="-+">Wierd daar geschreven <rhyme label="a" type="m" corresp="#s31:l1">uyt</rhyme>:</l>
        <l id="s31:l4" met="3" type="-+">Al ley zy nooyt geen <rhyme label="c" type="m">Kraam</rhyme>,</l>
        <l id="s31:l5" met="3" type="-+">En hy te sterven <rhyme label="c" type="m" corresp="#s31:l4">kwaam</rhyme>,</l>
        <l id="s31:l6" met="3" type="-+">Zo bleef zy <rhyme label="c" type="m" corresp="#s31:l4">Erfgenaam</rhyme>,</l>
      </lg>
      <lg id="s32" type="stanza">
        <l id="s32:l1" met="4" type="-+">Dus wierd de Priester voort <rhyme label="a" type="m">gehaald</rhyme>,</l>
        <l id="s32:l2" met="3" type="-+">En 't Huwelijk <rhyme label="b" type="f">bevestigt</rhyme>,</l>
        <l id="s32:l3" met="3" type="-+">En voor den Raad <rhyme label="a" type="m" corresp="#s32:l1">bepaald</rhyme>;</l>
        <l id="s32:l4" met="3" type="-+">Den Gouverneur sprak: <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s32:l5" met="3" type="-+">Wy bedanken d'Hertog <rhyme label="c" type="m" corresp="#s32:l4">zeer</rhyme>,</l>
        <l id="s32:l6" met="3" type="-+">Voor dees genoten <rhyme label="c" type="m" corresp="#s32:l4">Eer</rhyme>,</l>
      </lg>
      <lg id="s33" type="stanza">
        <l id="s33:l1" met="4" type="-+">Don Karel vraagde, zijt gy t' <rhyme label="a" type="m">vreen</rhyme>?</l>
        <l id="s33:l2" met="3" type="-+">Ia Heer, en wy <rhyme label="b" type="f">vertrekken</rhyme></l>
        <l id="s33:l3" met="3" type="-+">Met alle <rhyme label="a" type="m" corresp="#s33:l1">dankbaarheen</rhyme>;</l>
        <l id="s33:l4" met="3" type="-+">Zo sprak hy onder <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s33:l5" met="3" type="-+">Mijn Bruylofts-Feest die <rhyme label="c" type="m" corresp="#s33:l4">zal</rhyme></l>
        <l id="s33:l6" met="3" type="-+">Ook volgen na <rhyme label="c" type="m" corresp="#s33:l4">geval</rhyme>,</l>
      </lg>
      <lg id="s34" type="stanza">
        <l id="s34:l1" met="4" type="-+">Neen, sprak den Vorst; na mijn <rhyme label="a" type="m">vermoen</rhyme>,</l>
        <l id="s34:l2" met="3" type="-+">Zo moet gy de <rhyme label="b" type="f">Iustitie</rhyme></l>
        <l id="s34:l3" met="3" type="-+">En mijn ook eerst <rhyme label="a" type="m" corresp="#s34:l1">voldoen</rhyme>;</l>
        <l id="s34:l4" met="3" type="-+">Za knield daar voor het <rhyme label="c" type="m">Zand</rhyme>,</l>
        <l id="s34:l5" met="3" type="-+">Op dat gy door <rhyme label="c" type="m" corresp="#s34:l4">Beuls-hand</rhyme></l>
        <l id="s34:l6" met="3" type="-+">Gestraft word weer met <rhyme label="c" type="m" corresp="#s34:l4">schand</rhyme>,</l>
      </lg>
      <lg id="s35" type="stanza">
        <l id="s35:l1" met="4" type="-+">Hy bad den Vorst om <rhyme label="a" type="m">Lijfs-gena</rhyme>,</l>
        <l id="s35:l2" met="3" type="-+">Maar 't smeken was <rhyme label="b" type="f">verloren</rhyme>;</l>
        <l id="s35:l3" met="3" type="-+">Den Hertog sprak: komt <rhyme label="a" type="m" corresp="#s35:l1">za</rhyme></l>
        <l id="s35:l4" met="3" type="-+">Scherpregter, volg uw <rhyme label="c" type="m">last</rhyme>;</l>
        <l id="s35:l5" met="3" type="-+">En geeft den snode <rhyme label="c" type="m" corresp="#s35:l4">Gast</rhyme>,</l>
        <l id="s35:l6" met="3" type="-+">Het loon hem toe <rhyme label="c" type="m" corresp="#s35:l4">gepast</rhyme>,</l>
      </lg>
      <lg id="s36" type="stanza">
        <l id="s36:l1" met="4" type="-+">Men sloeg hem 't Hoofd af met een <rhyme label="a" type="m">slag</rhyme>,</l>
        <l id="s36:l2" met="3" type="-+">Het welk de Ionge <rhyme label="b" type="f">Weduw</rhyme>,</l>
        <l id="s36:l3" met="3" type="-+">Met blijdschap als aan <rhyme label="a" type="m" corresp="#s36:l1">zag</rhyme>;</l>
        <l id="s36:l4" met="3" type="-+">Dus was den <rhyme label="c" type="m">Gouverneur</rhyme>,</l>
        <l id="s36:l5" met="3" type="-+">Zijn end en droef <rhyme label="c" type="m" corresp="#s36:l4">getreur</rhyme>,</l>
        <l id="s36:l6" met="3" type="-+">Een groote Staat voor <rhyme label="c" type="m" corresp="#s36:l4">heur</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
