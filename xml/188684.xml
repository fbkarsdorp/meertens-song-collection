<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188684</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Un matin brusquement</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom zingen wij nu <rhyme label="a" type="m">blij</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Bataven buiten <rhyme label="b" type="f">vreezen</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Ons Vaderland is <rhyme label="a" type="m" corresp="#s1:l1">vrij</rhyme>!</l>
        <l id="s1:l4" met="5" type="-+">Nu kent gij, o Belg! geen <rhyme label="a" type="m" corresp="#s1:l1">dwingelandij</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Hoe schoon is het vrij te <rhyme label="b" type="f" corresp="#s1:l2">wezen</rhyme>!</l>
        <l id="s1:l6" met="4" type="+-">Dank zij de onverwinbren <rhyme label="c" type="m">Gal</rhyme>!</l>
        <l id="s1:l7" met="4" type="+-">Hij sloot met ons 't <rhyme label="d" type="m">Vreeverbond</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Eeuwig zij zijn deugd <rhyme label="b" type="f" corresp="#s1:l2">geprezen</rhyme>.</l>
        <l id="s1:l9" met="4" type="+-">Hij sloot met ons 't <rhyme label="d" type="m" corresp="#s1:l7">Vreeverbond</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Dat op Vrijheid werd <rhyme label="d" type="m" corresp="#s1:l7">gegrond</rhyme>.</l>
        <l id="s1:l11" met="4" type="+-">Dat op Vrijheid werd <rhyme label="d" type="m" corresp="#s1:l7">gegrond</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gij meugd, o <rhyme label="a" type="m">Albion</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">De Vrijheid ons <rhyme label="b" type="f">benijden</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Vreez ons geducht <rhyme label="a" type="m" corresp="#s2:l1">Canon</rhyme>!</l>
        <l id="s2:l4" met="5" type="-+">De Gal, die kwam, zag en <rhyme label="a" type="m" corresp="#s2:l1">overwon</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Zal met Bato's telgen <rhyme label="b" type="f" corresp="#s2:l2">strijden</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">Zoo word eens de vrije <rhyme label="c" type="m">Zee</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Uit der Britten boei <rhyme label="d" type="m">geslaakt</rhyme>;</l>
        <l id="s2:l8" met="4" type="+-">Zoo ziet Zeevaart zich <rhyme label="b" type="f" corresp="#s2:l2">bevrijden</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Uit der Britten boei <rhyme label="d" type="m" corresp="#s2:l7">geslaakt</rhyme>;</l>
        <l id="s2:l10" met="4" type="+-">Aarde en water vrij <rhyme label="d" type="m" corresp="#s2:l7">gemaakt</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Aarde en water vrij <rhyme label="d" type="m" corresp="#s2:l7">gemaakt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Koom, dappre <rhyme label="a" type="m">heldenstoet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'k Zie reeds uw oogen <rhyme label="b" type="f">branden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Van edlen <rhyme label="a" type="m" corresp="#s3:l1">heldengloed</rhyme>.</l>
        <l id="s3:l4" met="5" type="-+">O wat heil beloost ons uwen <rhyme label="a" type="m" corresp="#s3:l1">moed</rhyme>!</l>
        <l id="s3:l5" met="4" type="+-">Ruiter joeg langs Chattams <rhyme label="b" type="f" corresp="#s3:l2">stranden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">'t Britsche roversrot naar <rhyme label="c" type="m">huis</rhyme>.</l>
        <l id="s3:l7" met="4" type="+-">Volgen wij dat helden <rhyme label="d" type="m">spoor</rhyme>!</l>
        <l id="s3:l8" met="4" type="+-">Vrijheid leve in alle <rhyme label="b" type="f" corresp="#s3:l2">Landen</rhyme>!</l>
        <l id="s3:l9" met="4" type="+-">Volgen wij dat helden <rhyme label="d" type="m" corresp="#s3:l7">spoor</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Naar het glinstrend <rhyme label="d" type="m" corresp="#s3:l7">gloriekoor</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Naar het glinstrend <rhyme label="d" type="m" corresp="#s3:l7">gloriekoor</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
