    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Jonkvrouw mijn hart mijn ziel mijn zin</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">STerr' van de zee ghegroet ghy <rhyme label="a" type="m">zyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gods Moeder sonder <rhyme label="b" type="m">blaem'</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En oock maeghd on-be-vleckt <rhyme label="a" type="m" corresp="#s1:l1">al-tydt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Des hemels poort <rhyme label="b" type="m" corresp="#s1:l2">be-quaem</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Die hebt van Gabriel <rhyme label="c" type="m">ont-faen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">De alder-soetste <rhyme label="d" type="m">groet</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">In vrede doet ons blyven <rhyme label="c" type="m" corresp="#s1:l5">staen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ver-andert Ev√¢s <rhyme label="d" type="m" corresp="#s1:l6">boet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ont-bind der sonden swaeren <rhyme label="a" type="m">bast</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Licht aen de blinde <rhyme label="b" type="m">langht</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Van alle quaeden ons <rhyme label="a" type="m" corresp="#s2:l1">ont-last</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met alle goed <rhyme label="b" type="m" corresp="#s2:l2">om-vanght</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Toont dat ghy Moeder zyt <rhyme label="c" type="m">ghewis</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Dat ons door u <rhyme label="d" type="m">ver-hoor'</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Die ons van u gheboren <rhyme label="c" type="m" corresp="#s2:l5">is</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Uw alder-liefste <rhyme label="d" type="m" corresp="#s2:l6">oor</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maeghd boven alle Maeghden <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En saechtste van <rhyme label="b" type="m">ghemoed</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dus vry van schulden groot en <rhyme label="a" type="m" corresp="#s3:l1">kleyn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Maeckt suyver ende <rhyme label="b" type="m" corresp="#s3:l2">goed</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Ver-leent daer een reyn leven <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En onsen wegh <rhyme label="d" type="m">be-vryd</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Dat wy hier naer-maels Jesum <rhyme label="c" type="m" corresp="#s3:l5">bly</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Sien in syn vreughd <rhyme label="d" type="m" corresp="#s3:l6">al-tyd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Vader God in 't Hemelsch <rhyme label="a" type="m">hof</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En hier, en over <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wy bieden prys, en eer', en <rhyme label="a" type="m" corresp="#s4:l1">lof</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met sangh, en soet <rhyme label="b" type="m" corresp="#s4:l2">gheschal</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Lof, prys, en glori' zy den <rhyme label="c" type="m">Soon</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Den heyl'ghen Gheest zy <rhyme label="d" type="m">eer'</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Aen dry persoonen in den <rhyme label="c" type="m" corresp="#s4:l5">throon</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">Maer eenen God, en <rhyme label="d" type="m" corresp="#s4:l6">Heer'</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
