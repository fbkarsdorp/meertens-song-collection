<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160476</id>
    <date>
      <year-start>1700</year-start>
      <year-end>1700</year-end>
    </date>
    <tags/>
    <melody>In de min vind ik geneugten</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Mjn is soet en vol <rhyme label="a" type="f">genugten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Daer op stel ik al mijn <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daer in roep ik soete <rhyme label="a" type="f" corresp="#s1:l1">vrugten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">'t Js een nimphje die ik <rhyme label="b" type="m" corresp="#s1:l2">min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Blank van vel en net <rhyme label="a" type="f">besneden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Vriend'lijk is haer roder <rhyme label="b" type="m">mond</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Soet van stem en rat van <rhyme label="a" type="f" corresp="#s2:l1">leden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Zy maekt mijn Jonk hert <rhyme label="b" type="m" corresp="#s2:l2">gesond</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Goon kan ik wel beter <rhyme label="a" type="f">wensen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Van de Min is al mijn <rhyme label="b" type="m">sang</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Daer na haeken Jonge <rhyme label="a" type="f" corresp="#s3:l1">mensen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Altijd met een groot <rhyme label="b" type="m" corresp="#s3:l2">verlang</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wel wie sou de Min <rhyme label="a" type="f">veragten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Daer den mens werd door <rhyme label="b" type="m">geteelt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Liefde is vol wonder <rhyme label="a" type="f" corresp="#s4:l1">kragten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Want zy al mijn smerte <rhyme label="b" type="m" corresp="#s4:l2">heelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Selfs hoe groot of kleyne <rhyme label="a" type="f">dieren</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Beer, of Leeuw of <rhyme label="b" type="m">Oliphant</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ossen, Schapen kleyne <rhyme label="a" type="f" corresp="#s5:l1">Mieren</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Het is al vol <rhyme label="b" type="m" corresp="#s5:l2">Minne-brand</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Vogels vliegend' of die <rhyme label="a" type="f">ganen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Arend, Pauw, of <rhyme label="b" type="m">Nagtegael</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Mosse Duyven, Hoenders, <rhyme label="a" type="f" corresp="#s6:l1">Swanen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">'t Leeft door Liefde <rhyme label="b" type="m" corresp="#s6:l2">altemael</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Al het kruyt en fruyt en <rhyme label="a" type="f">bloemen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Dat des Somers lief'lijk <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Wat gewas men ook mag <rhyme label="a" type="f" corresp="#s7:l1">noemen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Het schiet door de Min zijn <rhyme label="b" type="m" corresp="#s7:l2">zaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Jn de stille water <rhyme label="a" type="f">stroomen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">'t Zy hoe groot of kleyne <rhyme label="b" type="m">Vis</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Of wat monsters daer uyt <rhyme label="a" type="f" corresp="#s8:l1">komen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">'t Leeft door Min al watter <rhyme label="b" type="m" corresp="#s8:l2">is</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Jn de Min seer hoog <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Sal ik dan volherden, <rhyme label="b" type="m">want</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Het moet wel een Drogaert <rhyme label="a" type="f" corresp="#s9:l1">wesen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Die niet voelt de <rhyme label="b" type="m" corresp="#s9:l2">Minne-brant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
