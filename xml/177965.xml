<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>177965</id>
    <date>
      <year-start>1837</year-start>
      <year-end>1837</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wenn man nicht zu Mädchen gehen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Als gij wilt op schaatsen <rhyme label="a" type="f">rijden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Rijd dan vlugtig langs de <rhyme label="b" type="m">baan</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wilt gij uw voor ramp <rhyme label="c" type="f">bewaren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Rijd niet wilt maar zagtjes <rhyme label="b" type="m" corresp="#s1:l2">aan</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Want fluks valt gij na <rhyme label="d" type="m">beneen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En breekt haastig arm of <rhyme label="d" type="m" corresp="#s1:l5">been</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dus niet haastig maar <rhyme label="e" type="m">bedaard</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Niet te driftig <rhyme label="e" type="m" corresp="#s1:l7">onervaard</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dan lijd gij ook geen <rhyme label="f" type="m">gevaar</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Voor een wilde <rhyme label="f" type="m" corresp="#s1:l9">lepelaar</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt gij schuiven op de <rhyme label="a" type="f">plassen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Neem dan een lief meisje <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Maar pas dan ook op uwe <rhyme label="c" type="f">krassen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Als een zeilschuit, zag uw <rhyme label="b" type="m" corresp="#s2:l2">slee</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Want fluks word gij <rhyme label="d" type="m">omgekeerd</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En gij en de meid <rhyme label="d" type="m" corresp="#s2:l5">bezeerd</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Dus voorzigtig en <rhyme label="e" type="m">bedaard</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Niet te driftig, <rhyme label="e" type="m" corresp="#s2:l7">onvervaard</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Dan lijd gij nog geen <rhyme label="f" type="m">gevaar</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Voor een wilde <rhyme label="f" type="m" corresp="#s2:l9">zeilenaar</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Heb gij dan een eind <rhyme label="a" type="f">gereden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Leg dan ook eens deftig <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Neem u meisje uit de <rhyme label="c" type="f">slede</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Laat zij in de tent nu <rhyme label="b" type="m" corresp="#s3:l2">gaan</rhyme></l>
        <l id="s3:l5" met="4" type="+-">En fluks word <rhyme label="d" type="m">gepresenteerd</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Wijn of punsch wat men <rhyme label="d" type="m" corresp="#s3:l5">begeert</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Drinkt dan lustig maar <rhyme label="e" type="m">bedaard</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Niet te driftig <rhyme label="e" type="m" corresp="#s3:l7">onvervaard</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Dan lijd gij ook geen <rhyme label="f" type="m">gevaar</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Van te horen <rhyme label="f" type="m" corresp="#s3:l9">Gierigaar</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Keert gij dan weer stadwaarts <rhyme label="a" type="f">henen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Met uw slede en uw <rhyme label="b" type="m">paard</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wees voorzigtig op de <rhyme label="c" type="f">steenen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat zij uw geen onheil <rhyme label="b" type="m" corresp="#s4:l2">baard</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Fluks het meisje uit de <rhyme label="d" type="m">slee</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Gij gaat ook dan vrolijk <rhyme label="d" type="m" corresp="#s4:l5">mee</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">In de kamer en <rhyme label="e" type="m">bedaard</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Niet te driftig <rhyme label="e" type="m" corresp="#s4:l7">onvervaard</rhyme>;</l>
        <l id="s4:l9" met="4" type="+-">Zoo krijgd gij het naar uw <rhyme label="f" type="m">zin</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Meisjes, gunst, en hand en <rhyme label="f" type="m" corresp="#s4:l9">min</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Als gij zit dan in de <rhyme label="a" type="f">kamer</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Bij de kachel, of den <rhyme label="b" type="m">haard</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Is er ook dan iets <rhyme label="c" type="f">bekwamer</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dat uw liefde <rhyme label="b" type="m" corresp="#s5:l2">openbaart</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Fluks vat 't meisje van haar <rhyme label="d" type="m">hand</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">De zegelring als <rhyme label="d" type="m" corresp="#s5:l5">onderpand</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Neem die zagtkens en <rhyme label="e" type="m">bedaard</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Niet te driftig <rhyme label="e" type="m" corresp="#s5:l7">onvervaard</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Maar beproefd eens hare <rhyme label="f" type="m">zin</rhyme>:</l>
        <l id="s5:l10" met="4" type="+-">Of zij heelt opregte <rhyme label="f" type="m" corresp="#s5:l9">min</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Want de meisjes al te <rhyme label="a" type="f">gader</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Minnen die wat voor haár <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wagt tot morgen, komt dan <rhyme label="c" type="f">nader</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Ziet hoe zij u dan <rhyme label="b" type="m" corresp="#s6:l2">ontmoet</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Fluks word gij daar heen <rhyme label="d" type="m">geleid</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Daar 't geluk u steeds <rhyme label="d" type="m" corresp="#s6:l5">verbeid</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Dan courasie, stout tree <rhyme label="e" type="m">nader</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Niet bevreesd maar <rhyme label="e" type="m" corresp="#s6:l7">onversaagd</rhyme>:</l>
        <l id="s6:l9" met="4" type="+-">Zoo hebt gij uw doel <rhyme label="f" type="m">bereikt</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Spoedig in het <rhyme label="f" type="m" corresp="#s6:l9">huwelijk</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
