  <meta>
    <id>191539</id>
    <date>
      <year-start>1736</year-start>
      <year-end>1736</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
