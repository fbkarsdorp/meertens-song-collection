      </coordinates>
    </location>
    <tags>
      <tag standard-name="kluchtlied" translation="humorous song">kluchtlied</tag>
    </tags>
    <melody>Fortune que tu m'importunes</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+">HEt <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Dat myn jonghe <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Eertydts heeft <rhyme label="b" type="m">vervoert</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Heeft my aen sich we'erom <rhyme label="b" type="m" corresp="#s1:l3">gesnoert</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">En laet my <rhyme label="c" type="f">voelen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Een hittigh <rhyme label="c" type="f" corresp="#s1:l5">woelen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Niet om te <rhyme label="c" type="f" corresp="#s1:l5">koelen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Als door eene <rhyme label="b" type="m" corresp="#s1:l3">boert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+">Schoon <rhyme label="a" type="f">schoonheydt</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Die de kracht uws throon <rhyme label="a" type="f" corresp="#s2:l1">spreydt</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Over mensch, en <rhyme label="b" type="m">Go'on</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Stelt ghy u soetheydt my ten <rhyme label="b" type="m" corresp="#s2:l3">toon</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Op dat het <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s2:l6" met="2" type="-+">My sou <rhyme label="c" type="f" corresp="#s2:l5">begeven</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">En eer te <rhyme label="c" type="f" corresp="#s2:l5">zweven</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Naer het ryck der <rhyme label="b" type="m" corresp="#s2:l3">do'on</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+">'t Jongh <rhyme label="a" type="f">bloetjen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Dat van handt en <rhyme label="a" type="f" corresp="#s3:l1">voetjen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Loopt myn lichaem <rhyme label="b" type="m">door</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wilt ghy dat ick daer in <rhyme label="b" type="m" corresp="#s3:l3">versmoor</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">En dat ick <rhyme label="c" type="f">draghe</rhyme></l>
        <l id="s3:l6" met="2" type="-+">In 't eynd' myns <rhyme label="c" type="f" corresp="#s3:l5">daghen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Vergheefs het <rhyme label="c" type="f" corresp="#s3:l5">plaghen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Van dees <rhyme label="b" type="m" corresp="#s3:l3">minne-boor</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="1" type="-+">Myn <rhyme label="a" type="f">voeten</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Willen u <rhyme label="a" type="f" corresp="#s4:l1">ontmoeten</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Waer ick gaen oft <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En schoon my 't graf al doet <rhyme label="b" type="m" corresp="#s4:l3">vermaen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Noch wil ick <rhyme label="c" type="f">planten</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Op syne <rhyme label="c" type="f" corresp="#s4:l5">kanten</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Voor mins <rhyme label="c" type="f" corresp="#s4:l5">trawanten</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Eene <rhyme label="b" type="m" corresp="#s4:l3">Myrtus-laen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="1" type="-+">Schoon <rhyme label="a" type="f">beeltjen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Dat myn bloem en <rhyme label="a" type="f" corresp="#s5:l1">steeltjen</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Op een nieuw <rhyme label="b" type="m">verquickt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ontfangt u minnaer <rhyme label="b" type="m" corresp="#s5:l3">on-verschrickt</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">En wilt niet <rhyme label="c" type="f">doemen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Syn kerck-hof <rhyme label="c" type="f" corresp="#s5:l5">bloemen</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Maer laet-se <rhyme label="c" type="f" corresp="#s5:l5">roemen</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Dat ghy zyt <rhyme label="b" type="m" corresp="#s5:l3">verstrickt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
