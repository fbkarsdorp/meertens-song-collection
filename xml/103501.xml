  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VVEest niet <rhyme label="a" type="m">verbaest</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Waar heen Nimphje dus <rhyme label="a" type="m" corresp="#s1:l1">haast</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Wilt gy vluchten <rhyme label="b" type="m">alleen</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Ik bid aan hoort mijn <rhyme label="b" type="m" corresp="#s1:l3">reen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En klachten die ik <rhyme label="c" type="m">stort</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Eer dat de doot mijn leven <rhyme label="c" type="m" corresp="#s1:l5">kort</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Maar't schijnt dat gy <rhyme label="d" type="m">veracht</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Al dese droeve <rhyme label="d" type="m" corresp="#s1:l7">klacht</rhyme></l>
        <l id="s1:l9" met="4" type="-+">En suchten daar ick in <rhyme label="d" type="m" corresp="#s1:l7">versmacht</rhyme></l>
        <l id="s1:l10" met="5" type="-+">Waerom dus wreet tegen u trouwen <rhyme label="e" type="m">vrient</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Wanneer heb ik dit oyt <rhyme label="e" type="m" corresp="#s1:l10">verdient</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ik heb <rhyme label="a" type="m">altijt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">U mijn dienst <rhyme label="a" type="m" corresp="#s2:l1">toegewijt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En heb mijn <rhyme label="b" type="m">verstout</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Aan eentch Nimph int <rhyme label="b" type="m" corresp="#s2:l3">Wout</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Te tonen sulcken <rhyme label="c" type="m">vlam</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Die uyt een oprecht herte <rhyme label="c" type="m" corresp="#s2:l5">quam</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Waerom wilt gy dan <rhyme label="d" type="m">niet</rhyme>?</l>
        <l id="s2:l8" met="3" type="-+">Dat onder u <rhyme label="d" type="m" corresp="#s2:l7">gebiet</rhyme></l>
        <l id="s2:l9" met="4" type="-+">U Slaef mach eynden zyn <rhyme label="d" type="m" corresp="#s2:l7">verdriet</rhyme>,</l>
        <l id="s2:l10" met="5" type="-+">Die u bemint boven al daatter <rhyme label="e" type="m">leeft</rhyme></l>
        <l id="s2:l11" met="4" type="+-">En staag tot u dienst sugh <rhyme label="e" type="m" corresp="#s2:l10">begeeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
