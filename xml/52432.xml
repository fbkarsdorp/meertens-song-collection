<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>52432</id>
    <date>
      <year-start>1830</year-start>
      <year-end>1840</year-end>
    </date>
    <tags>
      <tag standard-name="politiek lied" translation="political song">politiek lied</tag>
      <tag standard-name="oranjelied" translation="orangist song">oranjelied</tag>
    </tags>
    <melody>Wilhelmus</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Was onder 't grievendst <rhyme label="b" type="m">leed</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat ooit hen mogt <rhyme label="a" type="f" corresp="#s1:l1">benauwen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Der Vadren leus en <rhyme label="b" type="m" corresp="#s1:l2">kreet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Wat oorlog hen deed <rhyme label="c" type="f">leijen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Wat muitzucht derst <rhyme label="d" type="m">bestaan</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Men zag aan 't hoofd der <rhyme label="c" type="f" corresp="#s1:l5">reijen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Een telg van Nassou <rhyme label="d" type="m" corresp="#s1:l6">staan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zou ons de moed <rhyme label="a" type="f">ontzinken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Nu weer het oproer <rhyme label="b" type="m">woede</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Nu wij het staal zien <rhyme label="a" type="f" corresp="#s2:l1">blinken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat dorst naar Hollands <rhyme label="b" type="m" corresp="#s2:l2">bloed</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Nu BelgiÃ« <rhyme label="c" type="f">verwaten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Zich in haar naaktheid <rhyme label="d" type="m">toont</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En 't schuim der <rhyme label="c" type="f" corresp="#s2:l5">onderzaten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Den eedlen WILLEM <rhyme label="d" type="m" corresp="#s2:l6">hoont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De band is <rhyme label="a" type="f">losgereten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die Bat en Belg <rhyme label="b" type="m">verbond</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wee die 't zich dorst <rhyme label="a" type="f" corresp="#s3:l1">vermeten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En heilige eeden <rhyme label="b" type="m" corresp="#s3:l2">schond</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Oud Neerlands dappre <rhyme label="c" type="f">telgen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Verachten 't wust <rhyme label="d" type="m">gebroed</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Zij toonen lage <rhyme label="c" type="f" corresp="#s3:l5">Belgen</rhyme>!</l>
        <l id="s3:l8" met="3" type="-+">U hunne trouw en <rhyme label="d" type="m" corresp="#s3:l6">moed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Gij blijft wat ons <rhyme label="b" type="m">verbeid</rhyme></l>
        <l id="s4:l3" met="3" type="-+">De burg van ons <rhyme label="a" type="f" corresp="#s4:l1">vertrouwen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De star die ons <rhyme label="b" type="m" corresp="#s4:l2">geleidt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wij achten geen <rhyme label="c" type="f">gevaren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wij duchten geen <rhyme label="d" type="m">verraad</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Zoolang ge aan 't hoofd der <rhyme label="c" type="f" corresp="#s4:l5">scharen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Van 't oude Neerland <rhyme label="d" type="m" corresp="#s4:l6">staat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
