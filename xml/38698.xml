    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">Wel ach schoon Goddin o! roem van all' die <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Hoe staen ick <rhyme label="b" type="m">verstelt</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Wat loff sal ick u <rhyme label="a" type="f" corresp="#s1:l1">gheven</rhyme>?</l>
        <l id="s1:l4" met="2" type="-+">Wat soet <rhyme label="b" type="m" corresp="#s1:l2">ghewelt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Doet u albasten <rhyme label="b" type="m" corresp="#s1:l2">beldt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Com siet hier eens <rhyme label="c" type="m">Pymaglion</rhyme>?</l>
        <l id="s1:l7" met="5" type="-+">Haer ooghen die op onse herten <rhyme label="d" type="f">dalen</rhyme></l>
        <l id="s1:l8" met="1" type="+-">
          <rhyme label="d" type="f" corresp="#s1:l7">stralen</rhyme>
        </l>
        <l id="s1:l9" met="2" type="+-">Als een <rhyme label="c" type="m" corresp="#s1:l6">Son</rhyme>:</l>
        <l id="s1:l10" met="2" type="-+">Soo dat ick <rhyme label="e" type="m">schier</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Versengh in't <rhyme label="e" type="m" corresp="#s1:l10">minne-vier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Wat ick roep Princers ghy stopt voor my u <rhyme label="a" type="f">ooren</rhyme>?</l>
        <l id="s2:l2" met="3" type="+-">Denckt niet dat ick <rhyme label="b" type="m">veys</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Mijn jeucht en krachten <rhyme label="a" type="f" corresp="#s2:l1">smooren</rhyme></l>
        <l id="s2:l4" met="2" type="-+">In't mins <rhyme label="b" type="m" corresp="#s2:l2">forneys</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wins schoonheydt alle <rhyme label="b" type="m" corresp="#s2:l2">reys</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Van my soo seer aenbeden <rhyme label="c" type="m">wert</rhyme>:</l>
        <l id="s2:l7" met="5" type="-+">Ey toont weer-min aen die u soo <rhyme label="d" type="f">beminden</rhyme>?</l>
        <l id="s2:l8" met="1" type="+-">
          <rhyme label="d" type="f" corresp="#s2:l7">Inden</rhyme>
        </l>
        <l id="s2:l9" met="2" type="+-">Grondt sijns <rhyme label="c" type="m" corresp="#s2:l6">hert</rhyme>:</l>
        <l id="s2:l10" met="2" type="-+">Soo salt <rhyme label="e" type="m">ghesucht</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Verkeeren in <rhyme label="e" type="m" corresp="#s2:l10">ghenucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
