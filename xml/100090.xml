<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100090</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zal ik nog langer in hete tranen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VVAnneer mijn waerde Ziel <rhyme label="a" type="f">Clorinde</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Vermoeyt van al het Jagen <rhyme label="b" type="m">was</rhyme></l>
        <l id="s1:l3" met="4" type="-+">En dat haer Hertien inde <rhyme label="a" type="f" corresp="#s1:l1">linde</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Tot rusten seer genegen <rhyme label="b" type="m" corresp="#s1:l2">was</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wanneer de Son // al ree begon // te dalen <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Ley spint Groen haer <rhyme label="d" type="m">neer</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Dicht aende kant, der kille <rhyme label="c" type="m" corresp="#s1:l5">Water-bron</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nu ick die eer hadt te <rhyme label="a" type="f">geleyden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Haer Zieltgen naer en van de <rhyme label="b" type="m">Jacht</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Geboot de vest van ons te <rhyme label="a" type="f" corresp="#s2:l1">scheyden</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Om rust te soecken voor de <rhyme label="b" type="m" corresp="#s2:l2">nacht</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dies voecht den Prins sich by haer <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">En 't Venus dier // dat leyt sich voor <rhyme label="d" type="m">playsier</rhyme> //</l>
        <l id="s2:l7" met="5" type="-+">Nu voor haer schoot daer weder daer en <rhyme label="c" type="m" corresp="#s2:l5">hier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En als haer Ooghjens <rhyme label="a" type="f">toegeloken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Haer Lipjens vast geslooten <rhyme label="b" type="m">hadt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En door de groote vaeck <rhyme label="a" type="f" corresp="#s3:l1">gedoocken</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Met 't hooft schier op mijn Boesem <rhyme label="b" type="m" corresp="#s3:l2">sat</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Soo streelt mijn hant het poesel wit, haer Borsjes <rhyme label="c" type="m">ront</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Die u door vermaeck <rhyme label="d" type="m">aenbaet</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">En schier duysentmael haer lieve <rhyme label="c" type="m" corresp="#s3:l5">Mont</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
