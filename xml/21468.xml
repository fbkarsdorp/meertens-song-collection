<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21468</id>
    <date>
      <year-start>1558</year-start>
      <year-end>1558</year-end>
    </date>
    <location>
      <orginal-name>emden]</orginal-name>
      <standard-name>Emden, Germany</standard-name>
      <coordinates>
        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag>prayer song</tag>
    </tags>
    <melody>Als het al rust mans en vrouwen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk roep wt swaer <rhyme label="a" type="f">benouwen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Mijns gheests sonder <rhyme label="b" type="f">vermijen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Tot v o God vol <rhyme label="a" type="f" corresp="#s1:l1">trouwen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat ghy ons wilt <rhyme label="b" type="f" corresp="#s1:l2">beurijen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Uan alle die ons <rhyme label="b" type="f" corresp="#s1:l2">benijen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Bewaert ons tot elcken <rhyme label="c" type="f">keere</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Laet ons met v <rhyme label="b" type="f" corresp="#s1:l2">verblijen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">U goetheyt wilt <rhyme label="c" type="f" corresp="#s1:l6">vermeeren</rhyme></l>
        <l id="s1:l9" met="2" type="-+">O ghenadighe <rhyme label="c" type="f" corresp="#s1:l6">Heere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">En laet my niet te <rhyme label="a" type="f">schanden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Comen om mijn <rhyme label="b" type="f">misdaden</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En wilt dwerck uwer <rhyme label="a" type="f" corresp="#s2:l1">handen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">O Heere niet <rhyme label="b" type="f" corresp="#s2:l2">versmaden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Uerlost ons vanden <rhyme label="b" type="f" corresp="#s2:l2">quaden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Door v ghenade <rhyme label="c" type="f">pure</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Ontfanct doch in <rhyme label="b" type="f" corresp="#s2:l2">ghenaden</rhyme></l>
        <l id="s2:l8" met="3" type="-+">U selfs <rhyme label="c" type="f" corresp="#s2:l6">creature</rhyme></l>
        <l id="s2:l9" met="2" type="-+">In elcker <rhyme label="c" type="f" corresp="#s2:l6">vre</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick hebbe buyter <rhyme label="a" type="f">maten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghedwaelt tot allen <rhyme label="b" type="f">stonden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sheeren wech heb ick <rhyme label="a" type="f" corresp="#s3:l1">verlaten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Gheuallen ben ick in <rhyme label="b" type="f" corresp="#s3:l2">sonden</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Mijn misdaet is quaet om <rhyme label="b" type="f" corresp="#s3:l2">gronden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die ick dickmael heb <rhyme label="c" type="f">bedreuen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Twaer schande om <rhyme label="b" type="f" corresp="#s3:l2">vermonden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Ons boose sondich <rhyme label="c" type="f" corresp="#s3:l6">leuen</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Heere willet <rhyme label="c" type="f" corresp="#s3:l6">vergheuen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O God wilt niet <rhyme label="a" type="f">ghedencken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ons misdaet daer wy in <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wilt ons v gracie <rhyme label="a" type="f" corresp="#s4:l1">schencken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Door v goetheyt <rhyme label="b" type="f" corresp="#s4:l2">ghepresen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Oft anders so wy <rhyme label="b" type="f" corresp="#s4:l2">lesen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sullen wy gantsch <rhyme label="c" type="f">bederuen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wilt ghy ons niet <rhyme label="b" type="f" corresp="#s4:l2">ghenesen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">So moeten wy <rhyme label="c" type="f" corresp="#s4:l6">verweruen</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Een eewich <rhyme label="c" type="f" corresp="#s4:l6">steruen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wie sal staende <rhyme label="a" type="f">blijuen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Al voor Gods aenschijn <rhyme label="b" type="f">reene</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En hy ons wilt toe <rhyme label="a" type="f" corresp="#s5:l1">schrijuen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ons misdaden groot oft <rhyme label="b" type="f" corresp="#s5:l2">cleene</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Uoorwaer daer en is niet <rhyme label="b" type="f" corresp="#s5:l2">eene</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Sy en zijn al vol <rhyme label="c" type="f">ghebreken</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Ia sondich al <rhyme label="b" type="f" corresp="#s5:l2">ghemeene</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En van der waerheyt <rhyme label="c" type="f" corresp="#s5:l6">gheweken</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Na Dauids <rhyme label="c" type="f" corresp="#s5:l6">spreken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">God sach van shemels <rhyme label="a" type="f">throone</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In al des weerelts <rhyme label="b" type="f">hoecken</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Offer yemant in <rhyme label="a" type="f" corresp="#s6:l1">persoone</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Was, die God wilde <rhyme label="b" type="f" corresp="#s6:l2">soecken</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Men sietse nv <rhyme label="b" type="f" corresp="#s6:l2">vercloecken</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Tot sonden is elck <rhyme label="c" type="f">gheneghen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Sy zijn in duyster <rhyme label="b" type="f" corresp="#s6:l2">broecken</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ghedwaelt al van den <rhyme label="c" type="f" corresp="#s6:l6">weghen</rhyme></l>
        <l id="s6:l9" met="2" type="-+">Int quaet ter <rhyme label="c" type="f" corresp="#s6:l6">deghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Prince God der <rhyme label="a" type="f">heyrscharen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wiens macht blijft sonder <rhyme label="b" type="f">enden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">U schepsel wilt nv <rhyme label="a" type="f" corresp="#s7:l1">sparen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dwelck verdient heeft swaer <rhyme label="b" type="f" corresp="#s7:l2">amenden</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Wilt v tot ons waert <rhyme label="b" type="f" corresp="#s7:l2">wenden</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ons boosheyt en wilt niet <rhyme label="c" type="f">mercken</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Maer wilt ons uwen gheest <rhyme label="b" type="f" corresp="#s7:l2">senden</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Op dat hy ons mach <rhyme label="c" type="f" corresp="#s7:l6">stercken</rhyme></l>
        <l id="s7:l9" met="2" type="-+">Tot goede <rhyme label="c" type="f" corresp="#s7:l6">wercken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
