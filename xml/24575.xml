<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24575</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="devotional song">devoot lied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ene moele ic bouwen <rhyme label="a" type="m">wil</rhyme></l>
        <l id="s1:l2" met="3" type="+-">here god wost ic waer <rhyme label="b" type="f">mede</rhyme></l>
        <l id="s1:l3" met="3" type="+-">had ic holt <rhyme label="b" type="f" corresp="#s1:l2">gerede</rhyme></l>
        <l id="s1:l4" met="2" type="+-">ende woste war van <rhyme label="c" type="m">dan</rhyme></l>
        <l id="s1:l5" met="4" type="+-">soe wolde houwen <rhyme label="c" type="m" corresp="#s1:l4">aen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Toe holte wil ic varen <rhyme label="a" type="m">heen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">dat wolt en is my niet <rhyme label="b" type="f">vere</rhyme></l>
        <l id="s2:l3" met="3" type="+-">hulpe had ic <rhyme label="b" type="f" corresp="#s2:l2">geerne</rhyme></l>
        <l id="s2:l4" met="2" type="+-">die wosten <rhyme label="c" type="m">wal</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Hoe men boemen vellen <rhyme label="c" type="m" corresp="#s2:l4">sal</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dat wolt dat heytet <rhyme label="a" type="m">lybanus</rhyme></l>
        <l id="s3:l2" met="3" type="+-">daer wassen cedrus <rhyme label="b" type="f">schiere</rhyme></l>
        <l id="s3:l3" met="3" type="+-">sypressen ende <rhyme label="b" type="f" corresp="#s3:l2">ryuieren</rhyme></l>
        <l id="s3:l4" met="2" type="+-">ende palmen <rhyme label="c" type="m">stolt</rhyme></l>
        <l id="s3:l5" met="4" type="+-">oliua dat walnutte <rhyme label="c" type="m" corresp="#s3:l4">holt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Och meyster hoech van costen <rhyme label="a" type="m">ryck</rhyme></l>
        <l id="s4:l2" met="3" type="+-">du moges my synne <rhyme label="b" type="f">geuen</rhyme></l>
        <l id="s4:l3" met="3" type="+-">houwen snoeyen ende <rhyme label="b" type="f" corresp="#s4:l2">euen</rhyme></l>
        <l id="s4:l4" met="2" type="+-">ende voegen dat <rhyme label="c" type="m">slicht</rhyme></l>
        <l id="s4:l5" met="4" type="+-">soe wort die moelen wal <rhyme label="c" type="m" corresp="#s4:l4">bericht</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Moyses nv wees du daer <rhyme label="a" type="m">by</rhyme></l>
        <l id="s5:l2" met="3" type="+-">den ouersten steen <rhyme label="b" type="f">berichte</rhyme></l>
        <l id="s5:l3" met="3" type="+-">dat hi ligge <rhyme label="b" type="f" corresp="#s5:l2">dichte</rhyme></l>
        <l id="s5:l4" met="2" type="+-">soe draget hi <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s5:l5" met="4" type="+-">die olde ewe die meyn ic <rhyme label="c" type="m" corresp="#s5:l4">daer</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die nye ewe den ouersten <rhyme label="a" type="m">steen</rhyme></l>
        <l id="s6:l2" met="3" type="+-">den legge ic op den <rhyme label="b" type="f">olden</rhyme></l>
        <l id="s6:l3" met="3" type="+-">dat hi loepe <rhyme label="b" type="f" corresp="#s6:l2">bolde</rhyme></l>
        <l id="s6:l4" met="2" type="+-">nae meysters <rhyme label="c" type="m">const</rhyme></l>
        <l id="s6:l5" met="4" type="+-">den dryuet des hiligen geestes <rhyme label="c" type="m" corresp="#s6:l4">gonst</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Jheronymus <rhyme label="a" type="m">Ambrosius</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Gregorius <rhyme label="b" type="f">Augustynus</rhyme></l>
        <l id="s7:l3" met="3" type="+-">bewaert gi ons die <rhyme label="b" type="f" corresp="#s7:l2">moelen</rhyme></l>
        <l id="s7:l4" met="2" type="+-">ende dat kamer <rhyme label="c" type="m">raet</rhyme></l>
        <l id="s7:l5" met="4" type="+-">mit kammen wal berichtet <rhyme label="c" type="m" corresp="#s7:l4">dat</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ghi tweelf apostelen gaet hier <rhyme label="a" type="m">voer</rhyme></l>
        <l id="s8:l2" met="3" type="+-">maeket gi die moelen <rhyme label="b" type="f">gaende</rhyme></l>
        <l id="s8:l3" met="3" type="+-">dat sy niet en blyue <rhyme label="b" type="f" corresp="#s8:l2">staende</rhyme></l>
        <l id="s8:l4" met="2" type="+-">gy syt gesant te <rhyme label="c" type="m">maelen</rhyme></l>
        <l id="s8:l5" met="4" type="+-">auer al dat <rhyme label="c" type="m" corresp="#s8:l4">lant</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Ghigon physon <rhyme label="a" type="m">eufrates</rhyme></l>
        <l id="s9:l2" met="3" type="+-">tygrus gi vloeden <rhyme label="b" type="f">viere</rhyme></l>
        <l id="s9:l3" met="3" type="+-">ghi edel stolte <rhyme label="b" type="f" corresp="#s9:l2">ryuieren</rhyme></l>
        <l id="s9:l4" met="2" type="+-">gi hebt waters <rhyme label="c" type="m">genoech</rhyme></l>
        <l id="s9:l5" met="4" type="+-">geuet der moelen oer <rhyme label="c" type="m" corresp="#s9:l4">geuoech</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Een Joncfrouwe had een <rhyme label="a" type="m">sackelkyn</rhyme></l>
        <l id="s10:l2" met="3" type="+-">mit weyte wal <rhyme label="b" type="f">gebonden</rhyme></l>
        <l id="s10:l3" met="3" type="+-">al totter seluer <rhyme label="b" type="f" corresp="#s10:l2">stonden</rhyme></l>
        <l id="s10:l4" met="2" type="+-">ter moelen <rhyme label="c" type="m">quam</rhyme></l>
        <l id="s10:l5" met="4" type="+-">een prophete dat <rhyme label="c" type="m" corresp="#s10:l4">vernam</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Ysayas lange te voeren hadde <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s11:l2" met="3" type="+-">oec voel van <rhyme label="b" type="f">gescreuen</rhyme></l>
        <l id="s11:l3" met="3" type="+-">hoe ons worde <rhyme label="b" type="f" corresp="#s11:l2">gegeuen</rhyme></l>
        <l id="s11:l4" met="2" type="+-">een Joncfrouwe <rhyme label="c" type="m">weert</rhyme></l>
        <l id="s11:l5" met="4" type="+-">die eens kyndes hadde <rhyme label="c" type="m" corresp="#s11:l4">gebaert</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Syn naem die is <rhyme label="a" type="m">emanuel</rhyme></l>
        <l id="s12:l2" met="3" type="+-">den  sulle wi alle <rhyme label="b" type="f">lauen</rhyme></l>
        <l id="s12:l3" met="3" type="+-">guetlic van hier <rhyme label="b" type="f" corresp="#s12:l2">bauen</rhyme></l>
        <l id="s12:l4" met="2" type="+-">hier tot ons <rhyme label="c" type="m">quam</rhyme></l>
        <l id="s12:l5" met="4" type="+-">des vrouwen sich vrouwen ende <rhyme label="c" type="m" corresp="#s12:l4">man</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Der ander propheten is so <rhyme label="a" type="m">voel</rhyme></l>
        <l id="s13:l2" met="3" type="+-">die daer van hebben <rhyme label="b" type="f">gesongen</rhyme></l>
        <l id="s13:l3" met="3" type="+-">het is oec wal <rhyme label="b" type="f" corresp="#s13:l2">gelongen</rhyme></l>
        <l id="s13:l4" met="2" type="+-">ende <rhyme label="c" type="m">vollenbracht</rhyme></l>
        <l id="s13:l5" met="4" type="+-">dat scach tot eenre <rhyme label="c" type="m" corresp="#s13:l4">myddernacht</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Doe die nacht die corte <rhyme label="a" type="m">nam</rhyme></l>
        <l id="s14:l2" met="3" type="+-">die dach ontfenck die <rhyme label="b" type="f">lengde</rhyme></l>
        <l id="s14:l3" met="3" type="+-">der duysternisse <rhyme label="b" type="f" corresp="#s14:l2">dwynge</rhyme></l>
        <l id="s14:l4" met="2" type="+-">een eynde <rhyme label="c" type="m">nam</rhyme></l>
        <l id="s14:l5" met="4" type="+-">Och god des bystu <rhyme label="c" type="m" corresp="#s14:l4">lauesam</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Die dinre soe lange gebeydet  <rhyme label="a" type="m">hadden</rhyme></l>
        <l id="s15:l2" met="3" type="+-">quamestu soe <rhyme label="b" type="f">sachte</rhyme></l>
        <l id="s15:l3" met="3" type="+-">der vrouden sullen wy <rhyme label="b" type="f" corresp="#s15:l2">becrachten</rhyme></l>
        <l id="s15:l4" met="2" type="+-">ende wesen des <rhyme label="c" type="m">wys</rhyme></l>
        <l id="s15:l5" met="4" type="+-">dat gades soen gebaren <rhyme label="c" type="m" corresp="#s15:l4">is</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Ghi ewangelisten alle <rhyme label="a" type="m">vier</rhyme></l>
        <l id="s16:l2" met="3" type="+-">gi moget daer wal op <rhyme label="b" type="f">crachten</rhyme></l>
        <l id="s16:l3" met="3" type="+-">dat gy wyselicke <rhyme label="b" type="f" corresp="#s16:l2">wachten</rhyme></l>
        <l id="s16:l4" met="2" type="+-">dat <rhyme label="c" type="m">sackelkyn</rhyme></l>
        <l id="s16:l5" met="4" type="+-">dat v bracht een Jonfer <rhyme label="c" type="m" corresp="#s16:l4">fyn</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Matheus siet du op den <rhyme label="a" type="m">giet</rhyme></l>
        <l id="s17:l2" met="3" type="+-">op in gades <rhyme label="b" type="f">namen</rhyme></l>
        <l id="s17:l3" met="3" type="+-">ende leert ons alte <rhyme label="b" type="f" corresp="#s17:l2">samen</rhyme></l>
        <l id="s17:l4" met="2" type="+-">du biste <rhyme label="c" type="m">geleert</rhyme></l>
        <l id="s17:l5" met="4" type="+-">hoe gades soen mensche <rhyme label="c" type="m" corresp="#s17:l4">waert</rhyme></l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Lucas ryt den sack <rhyme label="a" type="m">ontwee</rhyme></l>
        <l id="s18:l2" met="3" type="+-">giet op die moelen laet <rhyme label="b" type="f">wryuen</rhyme></l>
        <l id="s18:l3" met="3" type="+-">du kanste ons wal <rhyme label="b" type="f" corresp="#s18:l2">bescryuen</rhyme></l>
        <l id="s18:l4" met="2" type="+-">dat offer <rhyme label="c" type="m">groet</rhyme></l>
        <l id="s18:l5" met="4" type="+-">hoe gades soen leet den <rhyme label="c" type="m" corresp="#s18:l4">doet</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Marcus sterke lewe <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s19:l2" met="3" type="+-">giet op die moelen laet <rhyme label="b" type="f">scraden</rhyme></l>
        <l id="s19:l3" met="3" type="+-">hoe god opstont vanden <rhyme label="b" type="f" corresp="#s19:l2">dode</rhyme></l>
        <l id="s19:l4" met="2" type="+-">hoe dat <rhyme label="c" type="m">gescach</rhyme></l>
        <l id="s19:l5" met="4" type="+-">dat riepestu aenden Jersten <rhyme label="c" type="m" corresp="#s19:l4">nacht</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Johannes een aern van hoger <rhyme label="a" type="m">vlucht</rhyme></l>
        <l id="s20:l2" met="3" type="+-">du kanste ons wal <rhyme label="b" type="f">geleren</rhyme></l>
        <l id="s20:l3" met="3" type="+-">die hemeluaert ons <rhyme label="b" type="f" corresp="#s20:l2">heren</rhyme></l>
        <l id="s20:l4" met="2" type="+-">al <rhyme label="c" type="m">apenbaer</rhyme></l>
        <l id="s20:l5" met="4" type="+-">help ons dat wy comen <rhyme label="c" type="m" corresp="#s20:l4">daer</rhyme></l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Die moelen goet se is <rhyme label="a" type="m">bereyt</rhyme></l>
        <l id="s21:l2" met="3" type="+-">die nv soe willen <rhyme label="b" type="f">maelen</rhyme></l>
        <l id="s21:l3" met="3" type="+-">die  sullen daer wal <rhyme label="b" type="f" corresp="#s21:l2">haelen</rhyme></l>
        <l id="s21:l4" met="2" type="+-">syn coernken <rhyme label="c" type="m">reyn</rhyme></l>
        <l id="s21:l5" met="4" type="+-">soe wordet hem ghemaelen <rhyme label="c" type="m" corresp="#s21:l4">cleyn</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Pawes keyser <rhyme label="a" type="m">predicker</rhyme></l>
        <l id="s22:l2" met="3" type="+-">waert ghi die moelen <rhyme label="b" type="f">euen</rhyme></l>
        <l id="s22:l3" met="3" type="+-">dat sy moghe <rhyme label="b" type="f" corresp="#s22:l2">geuen</rhyme></l>
        <l id="s22:l4" met="2" type="+-">mele ende <rhyme label="c" type="m">molt</rhyme></l>
        <l id="s22:l5" met="4" type="+-">daer van soe wort v ryke <rhyme label="c" type="m" corresp="#s22:l4">stolt</rhyme></l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Die syne ziele spysen <rhyme label="a" type="m">wyl</rhyme></l>
        <l id="s23:l2" met="3" type="+-">die sal sich hier nae <rhyme label="b" type="f">snellen</rhyme></l>
        <l id="s23:l3" met="3" type="+-">tot deser moelen <rhyme label="b" type="f" corresp="#s23:l2">vellen</rhyme></l>
        <l id="s23:l4" met="2" type="+-">hi is des <rhyme label="c" type="m">wys</rhyme></l>
        <l id="s23:l5" met="4" type="+-">sy malet ende molftert <rhyme label="c" type="m" corresp="#s23:l4">niet</rhyme></l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Die dese moelen gebouwet <rhyme label="a" type="m">heeft</rhyme></l>
        <l id="s24:l2" met="3" type="+-">den moet god <rhyme label="b" type="f">geleyden</rhyme></l>
        <l id="s24:l3" met="3" type="+-">wanneer hi van hier sal <rhyme label="b" type="f" corresp="#s24:l2">scheyden</rhyme></l>
        <l id="s24:l4" met="2" type="+-">in engels <rhyme label="c" type="m">wijs</rhyme></l>
        <l id="s24:l5" met="4" type="+-">soe voer hem god in dat <rhyme label="c" type="m" corresp="#s24:l4">paradys</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
