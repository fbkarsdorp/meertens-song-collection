    <melody>Baise-moi ma Jeanneton</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Cloris die myn sinnen, en <rhyme label="a" type="m">ghemoet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Naer u altydt verlanghen <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Neemt my voor slaef <rhyme label="b" type="f">beminde</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Al de <rhyme label="c" type="m">vreucht</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En lust van myne <rhyme label="c" type="m" corresp="#s1:l4">jeucht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Kan ick in u <rhyme label="b" type="f" corresp="#s1:l3">bevinden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">U soet aenschyn, en lieffelycken <rhyme label="a" type="m">mont</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Heeft door een lach myn hert <rhyme label="a" type="m" corresp="#s2:l1">doorwont</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">'tGhesicht met lieve <rhyme label="b" type="f">lonckxkens</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Heeft in my <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">On-steken eene <rhyme label="c" type="m" corresp="#s2:l4">smert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van duysent minne <rhyme label="b" type="f" corresp="#s2:l3">vonckxkens</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Als u myn mont, een soete soentjen <rhyme label="a" type="m">gheeft</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Myn ziel, in lust, en vreuchden <rhyme label="a" type="m" corresp="#s3:l1">leeft</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">'tGeswil van uwe <rhyme label="b" type="f">borsten</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Meer smaeck my <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Als al den honinck <rhyme label="c" type="m" corresp="#s3:l4">soet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wie sou daer naer niet <rhyme label="b" type="f" corresp="#s3:l3">dorsten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Gunt my dan d'eer, myn liefste maghet <rhyme label="a" type="m">teer</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat ick myn diensten tot u <rhyme label="a" type="m" corresp="#s4:l1">keer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Op d'aerde gheen kan <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Daer ick myn <rhyme label="c" type="m">trou</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En liefde liever <rhyme label="c" type="m" corresp="#s4:l4">sou</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Als aen u schoonheyt <rhyme label="b" type="f" corresp="#s4:l3">gheven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
