<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4562</id>
    <date>
      <year-start>1667</year-start>
      <year-end>1667</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="drinking song">drinklied</tag>
    </tags>
    <melody>Courante simple</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VOlmaeckte Phillis, soetste <rhyme label="a" type="m">zoet</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Ick zie wel dat ick u verlaten <rhyme label="a" type="m" corresp="#s1:l1">moet</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Vermits ghy my valt veel te <rhyme label="b" type="m">straf</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Daerom laet ick van het minnen <rhyme label="b" type="m" corresp="#s1:l3">af</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Voorwaer, voorwaer het is mijn <rhyme label="a" type="m">leedt</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Volmaeckte Phillis! dat ghy zijt soo <rhyme label="a" type="m" corresp="#s2:l1">wreet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">U Minnaer was u wel <rhyme label="b" type="m">getrouw</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Maer ghy voor hem te straffen <rhyme label="b" type="m" corresp="#s2:l3">Vrouw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn dunckt dat ick tot mijn <rhyme label="a" type="m">geluck</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Ontslagen ben van dese <rhyme label="a" type="m" corresp="#s3:l1">minne-druck</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Want ick heb wel aen u <rhyme label="b" type="m">beproeft</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat soo te minnen, is <rhyme label="b" type="m" corresp="#s3:l3">bedroeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Min en heeft geen zoetheit <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Als zy niet is verselt met <rhyme label="a" type="m" corresp="#s4:l1">weder-min</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Daerom geef ick de Min de <rhyme label="b" type="m">schop</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En zet'er liefst een glaesjen <rhyme label="b" type="m" corresp="#s4:l3">op</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick houw nu met de min de <rhyme label="a" type="m">spot</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">En God Cupido voor een <rhyme label="a" type="m" corresp="#s5:l1">minne-sot</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ick houw 't met Bacchus en sijn <rhyme label="b" type="m">stoet</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die my veel meerder vreugt aen <rhyme label="b" type="m" corresp="#s5:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doen ick op Philis was <rhyme label="a" type="m">verlieft</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Doen was mijn hart met enk'le rouw <rhyme label="a" type="m" corresp="#s6:l1">doorgrieft</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maer nu soo leef ick sonder <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">By 't glaesjen, en by 't <rhyme label="b" type="m" corresp="#s6:l3">Ticktack-bert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nu leef ick stadigh <rhyme label="a" type="m">onbevreest</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Met vernoegen, en een blyen <rhyme label="a" type="m" corresp="#s7:l1">geest</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">De wijn die zet mijn hert <rhyme label="b" type="m">gerust</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Die my den dorst, en liefde <rhyme label="b" type="m" corresp="#s7:l3">blust</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ick kom in geen Zaletten <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">Ick schijt eens in die schoone Juffers <rhyme label="a" type="m" corresp="#s8:l1">eer</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Mijn Minnares is glas, en <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Daer krijg ick beter voetzel <rhyme label="b" type="m" corresp="#s8:l3">van</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Gezegent zy de tijdt, en <rhyme label="a" type="m">uur</rhyme></l>
        <l id="s9:l2" met="5" type="-+">Dat ick verlaten heb het <rhyme label="a" type="m" corresp="#s9:l1">minne-vuur</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Nu leef ick altijdt als een <rhyme label="b" type="m">Vorst</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En smeer gestaegh mijn droge <rhyme label="b" type="m" corresp="#s9:l3">borst</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">t'Za waert langt ons een glaesje <rhyme label="a" type="m">wijn</rhyme>,</l>
        <l id="s10:l2" met="5" type="-+">Ick moet van daeg weer hallef nobis <rhyme label="a" type="m" corresp="#s10:l1">zijn</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">Indien het maer mijn Noodlot <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Ick wens te sterven by het <rhyme label="b" type="m" corresp="#s10:l3">glas</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
