<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35787</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>holy song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Luca! Gesprooten uyt edel <rhyme label="a" type="m">bloet</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Bidt voor ons <rhyme label="b" type="f">Patroone</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Beminder van Godt ende Hemels <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Krijght van Godes <rhyme label="b" type="f" corresp="#s1:l2">Zoone</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dat ons hart als d'Arent in d' Hemel <rhyme label="c" type="m">woont</rhyme>;</l>
        <l id="s1:l6" met="4" type="-+">Dat niemant van 't Hemels hem <rhyme label="c" type="m" corresp="#s1:l5">verschoont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als Tobias ghy zijt spiegel der <rhyme label="a" type="m">jeught</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Bidt voor ons ô <rhyme label="b" type="f">Vader</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">Als Isaac gehoorsaem, en zee vol <rhyme label="a" type="m" corresp="#s2:l1">deught</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Krijght ons <rhyme label="b" type="f" corresp="#s2:l2">allegader</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dat Goods medragende juck ons valt <rhyme label="c" type="m">licht</rhyme>;</l>
        <l id="s2:l6" met="4" type="-+">Dat yder sijn Eve-naeste <rhyme label="c" type="m" corresp="#s2:l5">sticht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O Sonne-blom! Die na Godt altijdt <rhyme label="a" type="m">dreyt</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Bidt voor ons de <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Geen swaert, geen vyer van Sint Paulus u <rhyme label="a" type="m" corresp="#s3:l1">scheyt</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Krijght na ons <rhyme label="b" type="f" corresp="#s3:l2">begeere</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat Godes wil ons in alles <rhyme label="c" type="m">behaeght</rhyme>;</l>
        <l id="s3:l6" met="4" type="-+">En yder sorgh der droevige <rhyme label="c" type="m" corresp="#s3:l5">draeght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Vyge-boom! Die geen end' wasdom <rhyme label="a" type="m">geeft</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Hoort ons <rhyme label="b" type="f">gebeden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Man na Goods hart, die altijdt suyver <rhyme label="a" type="m" corresp="#s4:l1">leeft</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Verkrijght voor ons <rhyme label="b" type="f" corresp="#s4:l2">mede</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Suyverheyt van harte en al de <rhyme label="c" type="m">le'en</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Dat Godt in ons leeft; en anders <rhyme label="c" type="m" corresp="#s4:l5">geen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O lastdragende Palm! Die breeckt noch <rhyme label="a" type="m">scheurt</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Bidt voor onse <rhyme label="b" type="f">chooren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">O verduldighe Job! Die klaeght noch <rhyme label="a" type="m" corresp="#s5:l1">treurt</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Krijght, o <rhyme label="b" type="f" corresp="#s5:l2">uytverkooren</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Euangelist! Dat wy staen stijf en <rhyme label="c" type="m">vast</rhyme>.</l>
        <l id="s5:l6" met="4" type="-+">Als Godt ons proeft, met plagen aen <rhyme label="c" type="m" corresp="#s5:l5">tast</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O simpel duyfje met Elias <rhyme label="a" type="m">geest</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Bidt voor Klerck als <rhyme label="b" type="f">leecken</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">U Euangely prijst de minst als <rhyme label="a" type="m" corresp="#s6:l1">meest</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Krijght door uw' <rhyme label="b" type="f" corresp="#s6:l2">voorspreecken</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Dat traegheyt, noch bedrogh in ons krijght <rhyme label="c" type="m">voet</rhyme>;</l>
        <l id="s6:l6" met="4" type="-+">Dat Godt onse daden keurt <rhyme label="c" type="m" corresp="#s6:l5">voorgoet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Pronck van Gods Kerck en ysere <rhyme label="a" type="m">Kalom</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Bidt voor ons <rhyme label="b" type="f">Patroone</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Karbunkel, en Samson van 't <rhyme label="a" type="m" corresp="#s7:l1">Heydendom</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Krijght van Godes <rhyme label="b" type="f" corresp="#s7:l2">Zoone</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Te blyven levendigh lidtmaten <rhyme label="c" type="m">Godts</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Dat wy winnen die sijn blint of <rhyme label="c" type="m" corresp="#s7:l5">boos</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Als een Engel ghy leeft, in Sondaers <rhyme label="a" type="m">schijn</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Bidt voor ons, ô <rhyme label="b" type="f">Vader</rhyme>!</l>
        <l id="s8:l3" met="4" type="-+">Zeyl-steen der Sondaren, en <rhyme label="a" type="m" corresp="#s8:l1">Medecijn</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Krijght ons <rhyme label="b" type="f" corresp="#s8:l2">allegader</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Dat in ons blijft Godes beelt heel en <rhyme label="c" type="m">net</rhyme>;</l>
        <l id="s8:l6" met="4" type="-+">Iaeght van ons dat lijf of ziel <rhyme label="c" type="m" corresp="#s8:l5">besmet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Als Abram ghy wandelt voor Goods <rhyme label="a" type="m">aenschijn</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Bidt voor ons de <rhyme label="b" type="f">Heere</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">O Thebaidsche Bisschop, en <rhyme label="a" type="m" corresp="#s9:l1">Seraphijn</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Krijght na ons <rhyme label="b" type="f" corresp="#s9:l2">begeere</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Dat yder Godt en al voor oogen <rhyme label="c" type="m">houdt</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Dat noyt in ons Gods liefde <rhyme label="c" type="m" corresp="#s9:l5">verflout</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O Licht des Werelts! En sout der <rhyme label="a" type="m">aerdt</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Hoort onse <rhyme label="b" type="f">Gebede</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Secretaris van Goods Moeder <rhyme label="a" type="m" corresp="#s10:l1">vermaerdt</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Verkrijght voor ons <rhyme label="b" type="f" corresp="#s10:l2">mede</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Dat Iesus en zijn Moeder ons <rhyme label="c" type="m">bemint</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Dat niet ons hart bederft, of <rhyme label="c" type="m" corresp="#s10:l5">verblint</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Als Iosue en Moyses ghy 't Volck <rhyme label="a" type="m">leyt</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Bidt voor onse <rhyme label="b" type="f">Chooren</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">O Schilder! En Zee van <rhyme label="a" type="m" corresp="#s11:l1">ootmoedigheyt</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Krijght ô <rhyme label="b" type="f" corresp="#s11:l2">Uytverkooren</rhyme>!</l>
        <l id="s11:l5" met="4" type="-+">Dat Godt blijft ons oogh'merck ende zijn <rhyme label="c" type="m">eer</rhyme>.</l>
        <l id="s11:l6" met="4" type="-+">Dat yder hem voeght na Godes <rhyme label="c" type="m" corresp="#s11:l5">leer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">O dooder van lusten, tongh, end' <rhyme label="a" type="m">gehoor</rhyme>!</l>
        <l id="s12:l2" met="3" type="+-">Bidt voor ons <rhyme label="b" type="f">Patroone</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Heldt onuytspreeckelijck, die noyt <rhyme label="a" type="m" corresp="#s12:l1">verloor</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">Krijght van Godes <rhyme label="b" type="f" corresp="#s12:l2">Zoone</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Dat yder zijn lusten quaet <rhyme label="c" type="m">wederstaet</rhyme>;</l>
        <l id="s12:l6" met="4" type="-+">Sijn sinnen versterft; niet sondichs <rhyme label="c" type="m" corresp="#s12:l5">praet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Noyt stilstaende Son, al zijt ghy stock <rhyme label="a" type="m">oudt</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Bidt voor ons ô <rhyme label="b" type="f">Vader</rhyme>!</l>
        <l id="s13:l3" met="4" type="-+">Lely gehangen aen 't <rhyme label="a" type="m" corresp="#s13:l1">Olyven-hout</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">Krijght ons <rhyme label="b" type="f" corresp="#s13:l2">allgader</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Apostel, Martelaer, maeght en <rhyme label="c" type="m">Doctoor</rhyme></l>
        <l id="s13:l6" met="4" type="-+">U lof te singen in 't Hemels <rhyme label="c" type="m" corresp="#s13:l5">Choor</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
