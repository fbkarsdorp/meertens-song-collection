  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laat ons heeden deezen <rhyme label="a" type="m">dag</rhyme> /</l>
        <l id="s1:l2" met="3" type="+-">Bly en Droolyk <rhyme label="b" type="f">weezen</rhyme> /</l>
        <l id="s1:l3" met="4" type="+-">Daar ons niemand stooren <rhyme label="a" type="m" corresp="#s1:l1">mag</rhyme> /</l>
        <l id="s1:l4" met="3" type="+-">Waarom zoud' men <rhyme label="b" type="f" corresp="#s1:l2">vreezen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Drienden voegd uw aan den <rhyme label="c" type="m">disch</rhyme> /</l>
        <l id="s1:l6" met="3" type="+-">Gaa aan tafel <rhyme label="d" type="f">zitten</rhyme> /</l>
        <l id="s1:l7" met="4" type="+-">Tot gebruik van Vleesch en <rhyme label="c" type="m" corresp="#s1:l5">Visch</rhyme> /</l>
        <l id="s1:l8" met="3" type="+-">Roode wyn / of <rhyme label="d" type="f" corresp="#s1:l6">witten</rhyme>.</l>
      </lg>
    </lg>
