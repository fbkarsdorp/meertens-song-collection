<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4534</id>
    <date>
      <year-start>1667</year-start>
      <year-end>1667</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Amarante</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">DIaniere, Ick en viere U niet <rhyme label="a" type="m">meer</rhyme>:</l>
        <l id="s1:l2" met="6" type="+-">'k Acht u loncken, Mijn t' ontvoncken, Veel te <rhyme label="a" type="m" corresp="#s1:l1">teer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">'k Haet het minnen, Dat de sinnen maer <rhyme label="a" type="m">verwart</rhyme>,</l>
        <l id="s2:l2" met="6" type="+-">Wijl de Liefde Noyt door-griefde Eenig <rhyme label="a" type="m" corresp="#s2:l1">hart</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Of de wonden Soeken vonden, Die al <rhyme label="a" type="m">d'eer</rhyme>.</l>
        <l id="s3:l2" met="6" type="+-">Van de Vryers, Noyt bedyers, Rucken <rhyme label="a" type="m" corresp="#s3:l1">neer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Minje matich, 't Is te statich, 't Wort <rhyme label="a" type="m">belacht</rhyme>,</l>
        <l id="s4:l2" met="6" type="+-">Minje flouw'lijck, 't Is te kouw'lijck, 't Wort <rhyme label="a" type="m" corresp="#s4:l1">veracht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Minje trouw'lijk, 't Is te vrouw'lijk, En men <rhyme label="a" type="m">seyt</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Siet de slaven, Eensjes draven, Om de <rhyme label="a" type="m" corresp="#s5:l1">Meyt</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="+-">Daer s'haer pooten Lichtaen stooten Door een <rhyme label="a" type="m">grau</rhyme>.</l>
        <l id="s6:l2" met="6" type="+-">Dat de beenen Voor de scheenen Worden <rhyme label="a" type="m" corresp="#s6:l1">blaeuw</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="+-">Daerom Vrysters, Schoon j'als Lysters, Singen <rhyme label="a" type="m">kont</rhyme>,</l>
        <l id="s7:l2" met="6" type="+-">En het rappe Exters klappen <rhyme label="a" type="m" corresp="#s7:l1">Overwont</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="6" type="+-">'k Sal u laten By u praten, By u <rhyme label="a" type="m">sang</rhyme>,</l>
        <l id="s8:l2" met="6" type="+-">En mijn houwen Sonder vrouwen Buyten <rhyme label="a" type="m" corresp="#s8:l1">dwang</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="6" type="+-">Kom dan Bacchus, Kom Iacchus, Kom <rhyme label="a" type="m">Aoon</rhyme>.</l>
        <l id="s9:l2" met="6" type="+-">Kom Dircaeus, Kom Nicaeus, 'k Acht u <rhyme label="a" type="m" corresp="#s9:l1">schoon</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="6" type="+-">U bemin ick, U besin ick, U <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s10:l2" met="6" type="+-">Salick Vryen, En benyen, Anders <rhyme label="a" type="m" corresp="#s10:l1">geen</rhyme>,</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="6" type="+-">Als die menschen, Die mijn wenschen Van dees <rhyme label="a" type="m">buyt</rhyme></l>
        <l id="s11:l2" met="6" type="+-">Af te keeren Die 'k wil sweeren Voor mijn <rhyme label="a" type="m" corresp="#s11:l1">Bruyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
