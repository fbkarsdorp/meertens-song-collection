<song>
  <meta>
    <id>19176</id>
    <date>
      <year-start>1611</year-start>
      <year-end>1611</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Aanhoor geklaag o bloeiende jeugd</melody>
  <author>Jan Jansz. Orlers</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">NIet isser dat Godt geschapen <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Al vvat hy 't leven op aerden <rhyme label="a" type="m" corresp="#s1:l1">geeft</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Dat sich niet <rhyme label="b" type="m">vergaert</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">End' tsamen <rhyme label="b" type="m" corresp="#s1:l3">paert</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">De wijl het <rhyme label="a" type="m" corresp="#s1:l1">leeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Aensiet de Leeŭ een gryselijck <rhyme label="a" type="m">dier</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Aensiet de dŭyven seer <rhyme label="a" type="m" corresp="#s2:l1">goedertier</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">Die sich by <rhyme label="b" type="m">een</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Voeghen int <rhyme label="b" type="m" corresp="#s2:l3">ghemeen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Na haer <rhyme label="a" type="m" corresp="#s2:l1">manier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dŭs Brŭyd'gom men ŭ prijsen <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dat ghy getroŭt hebt een Ioncvroŭ <rhyme label="a" type="m" corresp="#s3:l1">vroet</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Cathrina <rhyme label="b" type="m">Knijf</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Tot ŭ echt <rhyme label="b" type="m" corresp="#s3:l3">Ionckvvijf</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">In deŭchden <rhyme label="a" type="m" corresp="#s3:l1">goedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoŭdt beyd' in eer den echten <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Bemindt de vrede, en vliedt het <rhyme label="a" type="m" corresp="#s4:l1">qŭaet</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Looft Godt den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Gheeft hem de <rhyme label="b" type="m" corresp="#s4:l3">eer</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Van dees <rhyme label="a" type="m" corresp="#s4:l1">vveldaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
