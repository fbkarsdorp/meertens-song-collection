<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24563</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Nv vervrouwen wy ons tot deser lieuer <rhyme label="a" type="m">stont</rhyme></l>
        <l id="s1:l2" met="4" type="-+">men doet ons groter vrouden <rhyme label="a" type="m" corresp="#s1:l1">cont</rhyme></l>
        <l id="s1:l3" met="4" type="-+">ons is gebaren inder werlde <rhyme label="b" type="m">troest</rhyme></l>
        <l id="s1:l4" met="4" type="-+">van hem is al die werlt <rhyme label="b" type="m" corresp="#s1:l3">verloest</rhyme></l>
        <l id="s1:l5" met="4" type="-+">des sullen wi alle wesen <rhyme label="c" type="m">vroe</rhyme></l>
        <l id="s1:l6" met="4" type="-+">syngen ende spryngen <rhyme label="c" type="m" corresp="#s1:l5">ho</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Gloria in excelcis <rhyme label="c" type="m" corresp="#s1:l5">deo</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Van Jesses wortel <rhyme label="a" type="m">gaerdekyn</rhyme></l>
        <l id="s2:l2" met="4" type="-+">van ons moetstu gelauet <rhyme label="a" type="m" corresp="#s2:l1">syn</rhyme></l>
        <l id="s2:l3" met="4" type="-+">ende oec dat suete <rhyme label="b" type="m">bloemekyn</rhyme></l>
        <l id="s2:l4" met="4" type="-+">dat ons god liet in menschelicken schen <rhyme label="b" type="m" corresp="#s2:l3">schyn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">des sulle wy alle wesen <rhyme label="c" type="m">vroe</rhyme></l>
        <l id="s2:l6" met="4" type="-+">syngen ende spryngen <rhyme label="c" type="m" corresp="#s2:l5">ho</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Gloria in excelcis <rhyme label="c" type="m" corresp="#s2:l5">deo</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waer sach yeman alsulken <rhyme label="a" type="m">meyt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">die sonder wee ende sonder <rhyme label="a" type="m" corresp="#s3:l1">leyt</rhyme></l>
        <l id="s3:l3" met="4" type="-+">der werlt heeft een kynt <rhyme label="b" type="m">gebracht</rhyme></l>
        <l id="s3:l4" met="4" type="-+">dat heuet gedaen die gades <rhyme label="b" type="m" corresp="#s3:l3">cracht</rhyme></l>
        <l id="s3:l5" met="4" type="-+">des sullen wi alle wesen <rhyme label="c" type="m">vroe</rhyme></l>
        <l id="s3:l6" met="4" type="-+">syngen ende spryngen <rhyme label="c" type="m" corresp="#s3:l5">hoe</rhyme></l>
        <l id="s3:l7" met="4" type="-+">gloria in exselcys <rhyme label="c" type="m" corresp="#s3:l5">deo</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
