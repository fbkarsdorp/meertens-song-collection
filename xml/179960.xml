<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179960</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach Phyllis lief mijn hartedief</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Wilt hulde <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Aan 't <rhyme label="a" type="m" corresp="#s1:l1">Herfstzaisoen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">'t Geen ons zoo mild komt <rhyme label="b" type="f">noden</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Ter Boomgaard <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Daar elk zyn <rhyme label="c" type="m" corresp="#s1:l4">zin</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Volöp word <rhyme label="b" type="f" corresp="#s1:l3">aangeboden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">De Druiven <rhyme label="a" type="m">tros</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Met dauw en <rhyme label="a" type="m" corresp="#s2:l1">blos</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ontvonkt het hart in <rhyme label="b" type="f">vreugden</rhyme>;</l>
        <l id="s2:l4" met="2" type="-+">Haar geurig <rhyme label="c" type="m">nat</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Zoo hoog <rhyme label="c" type="m" corresp="#s2:l4">geschat</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Vaak oud en jong <rhyme label="b" type="f" corresp="#s2:l3">verheugden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">ô Herfsttyd! <rhyme label="a" type="m">gy</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Verschaft aan <rhyme label="a" type="m" corresp="#s3:l1">my</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Korhoenders, Haas, <rhyme label="b" type="f">Patryzen</rhyme>;</l>
        <l id="s3:l4" met="2" type="-+">Faizant, <rhyme label="c" type="m">Konyn</rhyme></l>
        <l id="s3:l5" met="2" type="-+">En Snippen <rhyme label="c" type="m" corresp="#s3:l4">zyn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">All' t'saam' uw eedle <rhyme label="b" type="f" corresp="#s3:l3">spyzen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Het Vinke <rhyme label="a" type="m">net</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Word <rhyme label="a" type="m" corresp="#s4:l1">uitgezet</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">De Lysterboog <rhyme label="b" type="f">gehangen</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+">ô Herfst! uw <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Doet onze <rhyme label="c" type="m" corresp="#s4:l4">jeugd</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Naar uwe komst <rhyme label="b" type="f" corresp="#s4:l3">verlangen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
