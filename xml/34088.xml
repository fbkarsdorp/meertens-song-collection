  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LOoft Barbara <rhyme label="a" type="m">ver-blydt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die hier ver-gaedert <rhyme label="a" type="m" corresp="#s1:l1">zyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een Maeghd soet, hoogh <rhyme label="b" type="f">ver-heven</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Al is den naem <rhyme label="c" type="m">barbaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Haer daed ons all' doet <rhyme label="b" type="f" corresp="#s1:l3">leven</rhyme>;</l>
        <l id="s1:l6" met="4" type="-+">Die honigh-soeter is <rhyme label="c" type="m" corresp="#s1:l4">voor-waer</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Die honigh-soeter is <rhyme label="c" type="m" corresp="#s1:l4">voor-waer</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Door hare deughd, dat 's <rhyme label="c" type="m" corresp="#s1:l4">open-baer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haer groote jonst <rhyme label="a" type="m">ver-toont</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Waer med' sy al-tyd <rhyme label="a" type="m" corresp="#s2:l1">loont</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Haer dienaers <rhyme label="b" type="f">uyt-ver-koren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die tot haer zyn <rhyme label="c" type="m">ghesint</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Want niemand gingh <rhyme label="b" type="f" corresp="#s2:l3">ver-loren</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die dese Maeghed heeft <rhyme label="c" type="m" corresp="#s2:l4">ghedint</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Die dese Maeghed heeft <rhyme label="c" type="m" corresp="#s2:l4">ghedint</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Haer dienaers Barbara <rhyme label="c" type="m" corresp="#s2:l4">bemint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vlucht al barbaersch <rhyme label="a" type="m">gheslacht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En wreede Koninghs <rhyme label="a" type="m" corresp="#s3:l1">macht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van Roomen en <rhyme label="b" type="f">Afriecken</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Als wy de soete <rhyme label="c" type="m">deughdt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van Barbara tot <rhyme label="b" type="f" corresp="#s3:l3">slecken</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Met eenen Sangh, die 't hert <rhyme label="c" type="m" corresp="#s3:l4">ver-heught</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Met eenen Sangh, die 't hert <rhyme label="c" type="m" corresp="#s3:l4">ver-heught</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Ver-klaeren tot een ieders <rhyme label="c" type="m" corresp="#s3:l4">vreughdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Want sy ghenoemt moet <rhyme label="a" type="m">zyn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">De soete <rhyme label="a" type="m" corresp="#s4:l1">medecyn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">(Ghelyck het klaer magh <rhyme label="b" type="f">blycken</rhyme>)</l>
        <l id="s4:l4" met="3" type="-+">In allen <rhyme label="c" type="m">teghen-spoedt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Op zee, op 't land, in <rhyme label="b" type="f" corresp="#s4:l3">rycken</rhyme></l>
        <l id="s4:l6" met="4" type="-+">De doode sy her-leven <rhyme label="c" type="m" corresp="#s4:l4">doet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">De doode sy her-leven <rhyme label="c" type="m" corresp="#s4:l4">doet</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Haer dienaers troost, en hulpe <rhyme label="c" type="m" corresp="#s4:l4">goedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dit is oock Aelst <rhyme label="a" type="m">be-kent</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat Barbara <rhyme label="a" type="m" corresp="#s5:l1">ghewent</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Haer dienaers troost te <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En by-stand in den <rhyme label="c" type="m">noodt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Als sy stracx heeft <rhyme label="b" type="f" corresp="#s5:l3">ghenesen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Die daer nu scheen te wesen <rhyme label="c" type="m" corresp="#s5:l4">doodt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Die daer nu scheen te wesen <rhyme label="c" type="m" corresp="#s5:l4">doodt</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Ver-resen door haer vvel-daed <rhyme label="c" type="m" corresp="#s5:l4">groot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dus singhen wy met <rhyme label="a" type="m">vreughd</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En loven uwe <rhyme label="a" type="m" corresp="#s6:l1">deughd</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">O Barbara <rhyme label="b" type="f">ghepresen</rhyme>!</l>
        <l id="s6:l4" met="3" type="-+">Die niet en zyt <rhyme label="c" type="m">Barbaer</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Maer een Maeghd saeght van <rhyme label="b" type="f" corresp="#s6:l3">wesen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Den Roos-maryn houd staen <rhyme label="c" type="m" corresp="#s6:l4">vruchtbaer</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Den Roos-maryn houd staen <rhyme label="c" type="m" corresp="#s6:l4">vruchtbaer</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Tot uwer eer' gheplant <rhyme label="c" type="m" corresp="#s6:l4">voor-waer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
