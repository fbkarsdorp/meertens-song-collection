<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6833</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>emmerick</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag>Easter song</tag>
    </tags>
    <melody>Christ ist erstanden</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Christus is op <rhyme label="a" type="f">gestanden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Al van der marterer <rhyme label="a" type="f" corresp="#s1:l1">handen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Des sullen wy allegaer vrolijck <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Christus sal onser Trooster <rhyme label="b" type="m" corresp="#s1:l3">sijn</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Waer Christus niet <rhyme label="a" type="f">erstanden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Soo waer die werelt <rhyme label="a" type="f" corresp="#s2:l1">vergangen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dâ€™wijl hy nu erstanden <rhyme label="b" type="m">ist</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Soo loven wy den Heeren Iesum <rhyme label="b" type="m" corresp="#s2:l3">Christ</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Het gingen dry heylige <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sy wouden dat graf <rhyme label="a" type="f" corresp="#s3:l1">beschouwen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sy sochten den Heeren Iesum <rhyme label="b" type="m">Christ</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die aller werelt een Trooster <rhyme label="b" type="m" corresp="#s3:l3">is</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wie wentelt ons van des grafs <rhyme label="a" type="f">door</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Den grooten steen die ligt daer <rhyme label="a" type="f" corresp="#s4:l1">voor</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Soo balt sy avers quamen <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Den steen daer van gewentelt <rhyme label="b" type="m" corresp="#s4:l3">waer</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sy gingen in dat graft heen <rhyme label="a" type="f">in</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En sagen daer een Engel <rhyme label="a" type="f" corresp="#s5:l1">fijn</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Int graft sitten ter rechter <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">In een angh en seer schoon <rhyme label="b" type="m" corresp="#s5:l3">gewandt</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sy erschrickten voor dit <rhyme label="a" type="f">gesicht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die Engel sprack, en vreest u <rhyme label="a" type="f" corresp="#s6:l1">nicht</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ghy soeckt den gecruytsten Iesum <rhyme label="b" type="m">Christ</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Die vanden doodt erstanden <rhyme label="b" type="m" corresp="#s6:l3">ist</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daer siet het toe by deser <rhyme label="a" type="f">frist</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die plaets daer hy gelegen <rhyme label="a" type="f" corresp="#s7:l1">ist</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Soo gaet nu heen, en segt <rhyme label="b" type="m">terstont</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Sijn Iongeren, en doet Petro <rhyme label="b" type="m" corresp="#s7:l3">kondt</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">In Galileam doet haer <rhyme label="a" type="f">gaen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Daer sullen sy Christum schouwen <rhyme label="a" type="f" corresp="#s8:l1">aen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Sy gingen snel hen wegh vant <rhyme label="b" type="m">graft</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En deden wat haer was <rhyme label="b" type="m" corresp="#s8:l3">operlagt</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wy dancken u Heer Iesu <rhyme label="a" type="f">Christ</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dat ghy van den doodt erstanden <rhyme label="a" type="f" corresp="#s9:l1">bist</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En hebt verstoort sijn gewalt en <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En ons wederom tot het leven <rhyme label="b" type="m" corresp="#s9:l3">bracht</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Lof sy u o heyliges <rhyme label="a" type="f">cruys</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Behuildt ons arme Christen <rhyme label="a" type="f" corresp="#s10:l1">luydt</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Al van der Ketters valschen <rhyme label="b" type="m">leer</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Dat sy ons nimmer bedroeven <rhyme label="b" type="m" corresp="#s10:l3">meer</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O Iesu Christ beminde <rhyme label="a" type="f">Heer</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Door u seer groote marters <rhyme label="a" type="f" corresp="#s11:l1">eer</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Verleendt ons al een saligh <rhyme label="b" type="m">end</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">En hier na een vrolijck <rhyme label="b" type="m" corresp="#s11:l3">opperstend</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Kyrie <rhyme label="c" type="f">eleison</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
