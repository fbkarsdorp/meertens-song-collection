<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>30100</id>
    <date>
      <year-start>1591</year-start>
      <year-end>1591</year-end>
    </date>
    <location>
      <orginal-name>middelburg</orginal-name>
      <standard-name>Middelburg, The Netherlands</standard-name>
      <coordinates>
        <lon>51.4997302</lon>
        <lat>3.6136962</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>Psalm 141 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk schrey tot dy o Heer <rhyme label="a" type="f">almachtich</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Koim my te hulp, end spoede <rhyme label="b" type="m">dy</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">In mijnen noot: verhoore <rhyme label="b" type="m" corresp="#s1:l2">my</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Als voir dy comt mijn stemme <rhyme label="a" type="f" corresp="#s1:l1">clachtich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laet mijn gebeden <rhyme label="a" type="f">opgeresen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Sijn eenen soeten wieroock <rhyme label="b" type="m">brant</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">End het opsteken mijner <rhyme label="b" type="m" corresp="#s2:l2">handt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Een heylig avontoffer <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">End mijnen mont wel naeu <rhyme label="a" type="f">beware</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Dat ick my nergens in <rhyme label="b" type="m">ontgae</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Een slot voir mijne lippen <rhyme label="b" type="m" corresp="#s3:l2">slae</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Op datter niet quaets uyt en <rhyme label="a" type="f" corresp="#s3:l1">vare</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laet doch mijn hert niet sijn <rhyme label="a" type="f">genegen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Tot saecken onrecht ende <rhyme label="b" type="m">boos</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met dese lieden <rhyme label="b" type="m" corresp="#s4:l2">goddeloos</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Laet my haer wellust oock niet <rhyme label="a" type="f" corresp="#s4:l1">plegen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer so my een vroim man, hardt <rhyme label="a" type="f">raket</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ick acht dat hy my gunste <rhyme label="b" type="m">doet</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Sijn straf is als een balsem <rhyme label="b" type="m" corresp="#s5:l2">soet</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dat geene wond' int hooft en <rhyme label="a" type="f" corresp="#s5:l1">maket</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maer eer veel tijts noch sy <rhyme label="a" type="f">geleden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Sal tgodloos volck selfs sijn in <rhyme label="b" type="m">druck</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">So dat ick, in haer <rhyme label="b" type="m" corresp="#s6:l2">ongeluck</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Tot God sal storten mijn <rhyme label="a" type="f" corresp="#s6:l1">gebeden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Als haere Richters die troer <rhyme label="a" type="f">stieren</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Van klippen sullen storten <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dan salmen sien, dat mijne <rhyme label="b" type="m" corresp="#s7:l2">straf</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Sacht is geweest, end <rhyme label="a" type="f" corresp="#s7:l1">goedertieren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Doch nu gaens' ons gebeente <rhyme label="a" type="f">rucken</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">End stroyent voirt graf hier end <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Als die met eenen hamer <rhyme label="b" type="m" corresp="#s8:l2">swaer</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Steen onder deirde hout <rhyme label="a" type="f" corresp="#s8:l1">ontstucken</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Daeromme staet mijn oog <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Tot dy, Heer: ick heb dy <rhyme label="b" type="m">vertrout</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Op dy is mijne hoip <rhyme label="b" type="m" corresp="#s9:l2">gebout</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Wil doch mijn siele niet bloot <rhyme label="a" type="f" corresp="#s9:l1">geven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Bewaer my, Heer, voir haere <rhyme label="a" type="f">stricken</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Daermed' ick rontom ben <rhyme label="b" type="m">beset</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">End voir tbedriegelijcke <rhyme label="b" type="m" corresp="#s10:l2">net</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Van tvolck die sick tot boosheyt <rhyme label="a" type="f" corresp="#s10:l1">schicken</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Geef dat de boos' in haere <rhyme label="a" type="f">garen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Dwelck was tot mijn verderf <rhyme label="b" type="m">gepast</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Gevangen werden end <rhyme label="b" type="m" corresp="#s11:l2">verrast</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Tot dat ick sy voirby <rhyme label="a" type="f" corresp="#s11:l1">gevaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
