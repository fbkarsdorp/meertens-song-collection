  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als Ick moet scheiden <rhyme label="a" type="f">serteine</rhyme></l>
        <l id="s1:l2" met="3" type="-+">van u oe maeget <rhyme label="a" type="f" corresp="#s1:l1">reine</rhyme></l>
        <l id="s1:l3" met="3" type="-+">soe sal Ick noch dickmaels <rhyme label="a" type="f" corresp="#s1:l1">weine</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Om u <rhyme label="a" type="f" corresp="#s1:l1">alleine</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Eedel sit gij wel schoen <rhyme label="a" type="f">kersouwe</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Gij spandt de kroene der <rhyme label="a" type="f" corresp="#s2:l1">vrouwe</rhyme></l>
        <l id="s2:l3" met="3" type="-+">op u staet sonder <rhyme label="a" type="f" corresp="#s2:l1">flouwe</rhyme></l>
        <l id="s2:l4" met="2" type="-+">al mijn <rhyme label="a" type="f" corresp="#s2:l1">betrouwe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Eerelick liefste liefie <rhyme label="a" type="f">gepreesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">en oeck van schoenheit uut <rhyme label="a" type="f" corresp="#s3:l1">geleesen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">hondert duisende Int <rhyme label="a" type="f" corresp="#s3:l1">weesen</rhyme></l>
        <l id="s3:l4" met="2" type="-+">geen schoender <rhyme label="a" type="f" corresp="#s3:l1">voereesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Lief sonder u mach mijn <rhyme label="a" type="f">genougen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">noch goude noch sillever schoen In <rhyme label="a" type="f" corresp="#s4:l1">doege</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Als quaement als pijlen uit <rhyme label="a" type="f" corresp="#s4:l1">boegen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">mijn toe <rhyme label="a" type="f" corresp="#s4:l1">gevloegen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mach Ick dan geeren troest <rhyme label="a" type="f">voerwerven</rhyme></l>
        <l id="s5:l2" met="3" type="-+">van rou soe sal Ick moeten <rhyme label="a" type="f" corresp="#s5:l1">sterven</rhyme></l>
        <l id="s5:l3" met="3" type="-+">en van u schoenheit niet <rhyme label="a" type="f" corresp="#s5:l1">erven</rhyme></l>
        <l id="s5:l4" met="2" type="-+">maer moet <rhyme label="a" type="f" corresp="#s5:l1">bederven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Prinselicke liefie <rhyme label="a" type="f">gepreesen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">mocht ick noch eens u dienaer <rhyme label="a" type="f" corresp="#s6:l1">weesen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">soe waer al mijn druck <rhyme label="a" type="f" corresp="#s6:l1">geneesen</rhyme></l>
        <l id="s6:l4" met="2" type="-+">rein blom <rhyme label="a" type="f" corresp="#s6:l1">gepreesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
