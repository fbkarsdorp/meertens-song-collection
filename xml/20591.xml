      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ceu sate viever no son</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">,,O Fortuna <rhyme label="a" type="m">vvanckelbaer</rhyme></l>
        <l id="s1:l2" met="4" type="+-">,,Wree lichtveerdighe <rhyme label="b" type="f">Goddinne</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">,,Ghy en Cupido, te <rhyme label="a" type="m" corresp="#s1:l1">gaer</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">,,Rooft den mensch van Siel, en <rhyme label="b" type="f" corresp="#s1:l2">sinne</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">,,Die u <rhyme label="c" type="m">mint</rhyme></l>
        <l id="s1:l6" met="2" type="+-">,,Eert, en <rhyme label="c" type="m" corresp="#s1:l5">dint</rhyme></l>
        <l id="s1:l7" met="2" type="+-">,,D'onrust <rhyme label="c" type="m" corresp="#s1:l5">vint</rhyme></l>
        <l id="s1:l8" met="2" type="-+">,,In schijn van <rhyme label="b" type="f" corresp="#s1:l2">minne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Tvvee vervolghen my <rhyme label="a" type="m">ghelijck</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">D'een hout maerten, en veel <rhyme label="b" type="f">knapen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Is van Vee, en vruchten <rhyme label="a" type="m" corresp="#s2:l1">rijck</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Daer der veel ghenucht in <rhyme label="b" type="f" corresp="#s2:l2">rapen</rhyme>:</l>
        <l id="s2:l5" met="2" type="+-">Dander <rhyme label="c" type="m">recht</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Een goet <rhyme label="c" type="m" corresp="#s2:l5">knecht</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Trouvv', en <rhyme label="c" type="m" corresp="#s2:l5">slecht</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Meest by sijn <rhyme label="b" type="f" corresp="#s2:l2">schapen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wie van tvvee ick best <rhyme label="a" type="m">aen-cleef</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sijn gheduerich mijn <rhyme label="b" type="f">ghedachten</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Soo ick my tot rijckdom <rhyme label="a" type="m" corresp="#s3:l1">geef</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Moet ick schimp, en smaet <rhyme label="b" type="f" corresp="#s3:l2">vervvachten</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Als den <rhyme label="c" type="m">lust</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Is <rhyme label="c" type="m" corresp="#s3:l5">gheblust</rhyme>:</l>
        <l id="s3:l7" met="2" type="+-">Neen naer <rhyme label="c" type="m" corresp="#s3:l5">rust</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Wil ick best <rhyme label="b" type="f" corresp="#s3:l2">trachten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Soud' ick om een luttel <rhyme label="a" type="m">Vee</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Laten my versuft <rhyme label="b" type="f">bepraten</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Soud' ick om een schaep, oft <rhyme label="a" type="m" corresp="#s4:l1">tvvee</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Eenen trouvven herder <rhyme label="b" type="f" corresp="#s4:l2">haten</rhyme>:</l>
        <l id="s4:l5" met="2" type="+-">Neen, neen <rhyme label="c" type="m">al</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Woudt' <rhyme label="c" type="m" corresp="#s4:l5">gheval</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Soo en <rhyme label="c" type="m" corresp="#s4:l5">sal</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Ick hem niet <rhyme label="b" type="f" corresp="#s4:l2">laten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">,,Rijckdom, vveld', en hooghen <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">,,Can den mensch t'gemoet doen <rhyme label="b" type="f">vvroeghen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">,,Beter is de middel <rhyme label="a" type="m" corresp="#s5:l1">maet</rhyme></l>
        <l id="s5:l4" met="4" type="+-">,,Alsser is rust, en <rhyme label="b" type="f" corresp="#s5:l2">vernoeghen</rhyme>:</l>
        <l id="s5:l5" met="2" type="+-">,,Suer en <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">,,Quaet, en <rhyme label="c" type="m" corresp="#s5:l5">goet</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">,,Liefde <rhyme label="c" type="m" corresp="#s5:l5">doet</rhyme></l>
        <l id="s5:l8" met="2" type="-+">,,Tot al ons <rhyme label="b" type="f" corresp="#s5:l2">voeghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
