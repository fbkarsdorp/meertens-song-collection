  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Met blijscap laet god loouen <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="4" type="-+">wanneer goeds macht in swerels <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme></l>
        <l id="s1:l3" met="4" type="-+">om ons te helpen vuijt den <rhyme label="a" type="m" corresp="#s1:l1">val</rhyme></l>
        <l id="s1:l4" met="4" type="-+">maect sijn dienstmaecht coempt sonder <rhyme label="a" type="m" corresp="#s1:l1">gal</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maria nu ontfangen <rhyme label="a" type="m">is</rhyme></l>
        <l id="s2:l2" met="4" type="-+">des wij prijsen gods wijsheijt <rhyme label="a" type="m" corresp="#s2:l1">wis</rhyme></l>
        <l id="s2:l3" met="4" type="-+">sij coempt t'onser <rhyme label="a" type="m" corresp="#s2:l1">behoodenis</rhyme></l>
        <l id="s2:l4" met="4" type="-+">vuijt Conincks saet, teer maecht <rhyme label="a" type="m" corresp="#s2:l1">gewis</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gebooren van een maechde <rhyme label="a" type="m">reijn</rhyme></l>
        <l id="s3:l2" met="4" type="-+">v zij met vaer glorij <rhyme label="a" type="m" corresp="#s3:l1">certeijn</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Oeck ben v geest trooster <rhyme label="a" type="m" corresp="#s3:l1">gemeijn</rhyme></l>
        <l id="s3:l4" met="4" type="-+">behoet ons in dees weerels <rhyme label="a" type="m" corresp="#s3:l1">pleijn</rhyme></l>
      </lg>
    </lg>
  </text>
