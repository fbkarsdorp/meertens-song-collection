<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182543</id>
    <date>
      <year-start>1793</year-start>
      <year-end>1793</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Sinjeur de Schelde</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoe kan, hoe kan het <rhyme label="a" type="f">weesen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat zo een jonge <rhyme label="b" type="m">meid</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">O liefde nooit <rhyme label="a" type="f" corresp="#s1:l1">volpreesen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Schoon gy myn hert <rhyme label="b" type="m" corresp="#s1:l2">doorsnyd</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Niemand die myn komt <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Of spreeken van de <rhyme label="d" type="m">min</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Myn dragt mag hem <rhyme label="c" type="f" corresp="#s1:l5">mishagen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dat ik ben een <rhyme label="d" type="m" corresp="#s1:l6">boerin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zien, ik zien een <rhyme label="a" type="f">ander</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ionkmans en dogters <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wandelen met <rhyme label="a" type="f" corresp="#s2:l1">malkander</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En ik moet in de <rhyme label="b" type="m" corresp="#s2:l2">stal</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Melken geyt en <rhyme label="c" type="f">koeye</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat voor een klein <rhyme label="d" type="m">gewin</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">De Ionkmans myn <rhyme label="c" type="f" corresp="#s2:l5">verfoeyen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Om dat ik ben een <rhyme label="d" type="m" corresp="#s2:l6">Boerin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">In plaets, in plaets van <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Of met myn lief te <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zo haest dat gaet <rhyme label="a" type="f" corresp="#s3:l1">beginnen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Het krayen van de <rhyme label="b" type="m" corresp="#s3:l2">haen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Moet ik met myne <rhyme label="c" type="f">schapen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Trekken ter velden <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Daer is geen vreugd te <rhyme label="c" type="f" corresp="#s3:l5">rapen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Voor myn eilaes <rhyme label="d" type="m" corresp="#s3:l6">Boerin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men ziet, men ziet dog <rhyme label="a" type="f">zwier</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Veel dogters in de <rhyme label="b" type="m">stee</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die nemen haer <rhyme label="a" type="f" corresp="#s4:l1">plasier</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Op Balle en <rhyme label="b" type="m" corresp="#s4:l2">assamble</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En ik gebeele <rhyme label="c" type="f">dage</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Vroeten als een <rhyme label="d" type="m">slavin</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ik mag myn leet wel <rhyme label="c" type="f" corresp="#s4:l5">klagen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Ag was ik geen <rhyme label="d" type="m" corresp="#s4:l6">boerin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De nieuw, de nieuw <rhyme label="a" type="f">methode</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Waer naer een Ionkman <rhyme label="b" type="m">ziet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Schoentje na de <rhyme label="a" type="f" corresp="#s5:l1">mode</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En ik tot myn <rhyme label="b" type="m" corresp="#s5:l2">verdriet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Kousen en klompen <rhyme label="c" type="f">stinken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Kom ik ten koey stal <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">De min moet my <rhyme label="c" type="f" corresp="#s5:l5">ontsinken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat ik ben een <rhyme label="d" type="m" corresp="#s5:l6">boerin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat moet, wat moet ik <rhyme label="a" type="f">lyden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">In mynen maegden <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Komt my de min <rhyme label="a" type="f" corresp="#s6:l1">bestryden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Eilaes geen troost of <rhyme label="b" type="m" corresp="#s6:l2">baet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Of dat ik zugt of <rhyme label="c" type="f">weene</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ionkmans bevat den <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Ik ben van vlees en <rhyme label="c" type="f" corresp="#s6:l5">beenen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Schoon dat ik ben <rhyme label="d" type="m" corresp="#s6:l6">boerin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Geen troost, geen troost te <rhyme label="a" type="f">winnen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">In alle myn <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Niemand die myn komt <rhyme label="a" type="f" corresp="#s7:l1">minnen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Myn klagt is al om <rhyme label="b" type="m" corresp="#s7:l2">niet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Moet my de Ionkheid <rhyme label="c" type="f">haeten</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daer ik heb trouwens <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ik wil den boer <rhyme label="c" type="f" corresp="#s7:l5">verlaten</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En Kleetsel van <rhyme label="d" type="m" corresp="#s7:l6">Boerin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">In kort, in korte <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Als ik na Brussel my <rhyme label="b" type="m">keer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Zat ik de mode <rhyme label="a" type="f" corresp="#s8:l1">dragen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Maer geen kapotje <rhyme label="b" type="m" corresp="#s8:l2">meer</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En mynen kop <rhyme label="c" type="f">coeffeeren</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat is de jongmans <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Men zal my <rhyme label="c" type="f" corresp="#s8:l5">carasseeren</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Als ik een juffrouw <rhyme label="d" type="m" corresp="#s8:l6">bin</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ik hoop, ik hoop te <rhyme label="a" type="f">wachten</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Met eenen goeden <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">De Ionkman van <rhyme label="a" type="f" corresp="#s9:l1">gedagten</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Of die heeft geld en <rhyme label="b" type="m" corresp="#s9:l2">goed</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Komt hy zyn trouw my <rhyme label="c" type="f">geeven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dan ik zyn gratie <rhyme label="d" type="m">win</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Dan ben ik al myn <rhyme label="c" type="f" corresp="#s9:l5">leven</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Juffrouw maar geen <rhyme label="d" type="m" corresp="#s9:l6">boerin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
