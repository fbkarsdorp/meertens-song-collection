    <melody>O Venus der minne godin</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als ick moet scheyden <rhyme label="a" type="f">certeyne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">van v schoon maechdeken <rhyme label="a" type="f" corresp="#s1:l1">reyne</rhyme></l>
        <l id="s1:l3" met="3" type="-+">soo sal ick noch dickmaels <rhyme label="a" type="f" corresp="#s1:l1">weyne</rhyme></l>
        <l id="s1:l4" met="2" type="-+">om v <rhyme label="a" type="f" corresp="#s1:l1">alleyne</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Edel is die schone <rhyme label="a" type="f">carsouwe</rhyme></l>
        <l id="s2:l2" met="3" type="-+">hy spant oock die croone der <rhyme label="a" type="f" corresp="#s2:l1">vrouwen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">op haer staet sonder <rhyme label="a" type="f" corresp="#s2:l1">flouwen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">al myn <rhyme label="a" type="f" corresp="#s2:l1">betrouwen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Fraey edel is hy van <rhyme label="a" type="f">manieren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">en oock seer lieffelyck int <rhyme label="a" type="f" corresp="#s3:l1">bestieren</rhyme></l>
        <l id="s3:l3" met="3" type="-+">troost my dat bidick v <rhyme label="a" type="f" corresp="#s3:l1">fiere</rhyme></l>
        <l id="s3:l4" met="2" type="-+">o schoon <rhyme label="a" type="f" corresp="#s3:l1">rogiere</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men sach noecht schoonder <rhyme label="a" type="f">figuyre</rhyme></l>
        <l id="s4:l2" met="3" type="-+">noch amoireuser <rhyme label="a" type="f" corresp="#s4:l1">creature</rhyme></l>
        <l id="s4:l3" met="3" type="-+">dan dit beeldeken <rhyme label="a" type="f" corresp="#s4:l1">pure</rhyme></l>
        <l id="s4:l4" met="2" type="-+">teeniger <rhyme label="a" type="f" corresp="#s4:l1">vre</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Eerlyck is die lieffde <rhyme label="a" type="f">gepresen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">en oock van schoonheyt vyt <rhyme label="a" type="f" corresp="#s5:l1">gelesen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">onder duysent int <rhyme label="a" type="f" corresp="#s5:l1">wesen</rhyme></l>
        <l id="s5:l4" met="2" type="-+">noyt schoonder <rhyme label="a" type="f" corresp="#s5:l1">verresen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Niet sonder haer mach myn <rhyme label="a" type="f">genoegen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">noch gout noch siluer schoon in <rhyme label="a" type="f" corresp="#s6:l1">doogen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">al quaemt als pylen wt <rhyme label="a" type="f" corresp="#s6:l1">bogen</rhyme></l>
        <l id="s6:l4" met="2" type="-+">my <rhyme label="a" type="f" corresp="#s6:l1">toegevlogen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Sal ick geenen troost <rhyme label="a" type="f">verweruen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">soo moet ick om v liefken <rhyme label="a" type="f" corresp="#s7:l1">sterven</rhyme></l>
        <l id="s7:l3" met="3" type="-+">en van v schoonheyt niet <rhyme label="a" type="f" corresp="#s7:l1">eruen</rhyme></l>
        <l id="s7:l4" met="2" type="-+">dats myn <rhyme label="a" type="f" corresp="#s7:l1">bederuen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Als ick aensie  v eerbaer <rhyme label="a" type="f">leeuen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">t'geen in myn herte staet <rhyme label="a" type="f" corresp="#s8:l1">geschreuen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">soo moet ick in vreuchden <rhyme label="a" type="f" corresp="#s8:l1">leeuen</rhyme></l>
        <l id="s8:l4" met="2" type="-+">vry sonder <rhyme label="a" type="f" corresp="#s8:l1">sneuen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Het is groote pyn en <rhyme label="a" type="f">verdriet</rhyme></l>
        <l id="s9:l2" met="3" type="-+">die hen van syn lieff verlaeten <rhyme label="a" type="f" corresp="#s9:l1">siet</rhyme></l>
        <l id="s9:l3" met="3" type="-+">maer wel mach hy hem <rhyme label="a" type="f" corresp="#s9:l1">verblyden</rhyme></l>
        <l id="s9:l4" met="2" type="-+">die se mach spreeken sonder <rhyme label="a" type="f" corresp="#s9:l1">myden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Schoon princes liefken vyt <rhyme label="a" type="f">vercoren</rhyme></l>
        <l id="s10:l2" met="3" type="-+">en laet v liefken niet <rhyme label="a" type="f" corresp="#s10:l1">verlooren</rhyme></l>
        <l id="s10:l3" met="3" type="-+">soo sal ick noch vreucht <rhyme label="a" type="f" corresp="#s10:l1">oorboren</rhyme></l>
        <l id="s10:l4" met="2" type="-+">en druck <rhyme label="a" type="f" corresp="#s10:l1">versmoren</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
