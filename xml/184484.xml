    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">DAt is Klaarheit:,: wat zig <rhyme label="a" type="m">openbaart</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Dat is Waarheit, wat den grond <rhyme label="a" type="m" corresp="#s1:l1">bewaart</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat is Eenvoud, zonder <rhyme label="b" type="m">schyn</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Voor den Heer' oprecht te <rhyme label="b" type="m" corresp="#s1:l3">zyn</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">In een stille, zuiv're kinder <rhyme label="a" type="m" corresp="#s1:l1">aart</rhyme>.</l>
      </lg>
    </lg>
