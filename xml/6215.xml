<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6215</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nog heb ik van mijn leven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GAlathea, <rhyme label="a" type="f">Harderinne</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">Galathea, onse <rhyme label="a" type="f" corresp="#s1:l1">Minne</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Hebt ghy te rugh <rhyme label="b" type="m">ghestelt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Door al te geyle <rhyme label="c" type="f">lusten</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Die u niet en lieten <rhyme label="c" type="f" corresp="#s1:l4">rusten</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In't Claver-rijcke <rhyme label="b" type="m" corresp="#s1:l3">Veldt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Doen ick u eerst quam <rhyme label="a" type="f">vryen</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">Ick dacht u <rhyme label="a" type="f" corresp="#s2:l1">slavernyen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Te stellen aen d'een <rhyme label="b" type="m">sy</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">En u een Vrou te <rhyme label="c" type="f">maecken</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Van veel Edel-rijcke <rhyme label="c" type="f" corresp="#s2:l4">saecken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tot lust, en rust van <rhyme label="b" type="m" corresp="#s2:l3">dy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy scheent my toe te <rhyme label="a" type="f">vallen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met hart, met ziel, met <rhyme label="a" type="f" corresp="#s3:l1">allen</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Waer ick quam in u <rhyme label="b" type="m">Wey</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Ghy liet u Schaepjes <rhyme label="c" type="f">grasen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En ghy ginckt mijn hartjen <rhyme label="c" type="f" corresp="#s3:l4">asen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Met Kluchjes <rhyme label="b" type="m" corresp="#s3:l3">veelderley</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy renden met <rhyme label="a" type="f">malkander</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Van d'eene Wey in <rhyme label="a" type="f" corresp="#s4:l1">d'ander</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Om Struycke, Bosch, en <rhyme label="b" type="m">Haegh</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Ghy liet u niet <rhyme label="c" type="f">verdrieten</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Minne-scheutjes te <rhyme label="c" type="f" corresp="#s4:l4">ghenieten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">By nachten en by <rhyme label="b" type="m" corresp="#s4:l3">daegh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick schonck u, <rhyme label="a" type="f">Harderinne</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Een Trou-juweel, uyt <rhyme label="a" type="f" corresp="#s5:l1">Minne</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">En ghy my <rhyme label="b" type="m">wederom</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Seer schoone <rhyme label="c" type="f">Breseletten</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Om mijn Handekens seer <rhyme label="c" type="f" corresp="#s5:l4">nette</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van meen'ge schoone <rhyme label="b" type="m" corresp="#s5:l3">Blom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dees Giften, Lief <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dacht ick ghenoegh te <rhyme label="a" type="f" corresp="#s6:l1">wesen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Tot Trouw <rhyme label="b" type="m">verseeckeringh</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Maer ghy, o lichte <rhyme label="c" type="f">Vrouwe</rhyme>!</l>
        <l id="s6:l5" met="4" type="+-">Breeckt nu vast gebonden <rhyme label="c" type="f" corresp="#s6:l4">Trouwe</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Door liefds <rhyme label="b" type="m" corresp="#s6:l3">veranderingh</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Galathe ghy sult <rhyme label="a" type="f">beklaghen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Al dyne levens <rhyme label="a" type="f" corresp="#s7:l1">daghen</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">D'ontrou die ghy my <rhyme label="b" type="m">doet</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Wanneer ramps <rhyme label="c" type="f">onghelucken</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">V beginnen te <rhyme label="c" type="f" corresp="#s7:l4">verdrucken</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En helpen onder <rhyme label="b" type="m" corresp="#s7:l3">d'voet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Verblint zijt ghy met <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat ghy u gaet <rhyme label="a" type="f" corresp="#s8:l1">besteden</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Aen 't plomp, en Boers <rhyme label="b" type="m">gheslacht</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Daer ghy een Ioncker <rhyme label="c" type="f">eele</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Krijghen mocht tot uwen <rhyme label="c" type="f" corresp="#s8:l4">deele</rhyme>;</l>
        <l id="s8:l6" met="3" type="-+">En worden groot <rhyme label="b" type="m" corresp="#s8:l3">gheacht</rhyme>;</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Of kont ghy niet <rhyme label="a" type="f">verlaten</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Der Boeren hoofsche <rhyme label="a" type="f" corresp="#s9:l1">staten</rhyme>;</l>
        <l id="s9:l3" met="3" type="-+">Vermidts ghy by het <rhyme label="b" type="m">Vee</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ghewoon zijt te <rhyme label="c" type="f">verkeeren</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">En u selven te <rhyme label="c" type="f" corresp="#s9:l4">gheneeren</rhyme>;</l>
        <l id="s9:l6" met="3" type="-+">Ghelijck oyt slaefjen <rhyme label="b" type="m" corresp="#s9:l3">dee</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wel Galathe gaet <rhyme label="a" type="f">heenen</rhyme>;</l>
        <l id="s10:l2" met="3" type="-+">Den Heer wil u <rhyme label="a" type="f" corresp="#s10:l1">verleenen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Met dese Kinckel <rhyme label="b" type="m">grof</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Veel sinnelijcke <rhyme label="c" type="f">lusjes</rhyme>;</l>
        <l id="s10:l5" met="4" type="+-">Dat ghy leven mooght <rhyme label="c" type="f" corresp="#s10:l4">gherusjes</rhyme>;</l>
        <l id="s10:l6" met="3" type="-+">En ick keer weer nae 't <rhyme label="b" type="m" corresp="#s10:l3">Hof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
