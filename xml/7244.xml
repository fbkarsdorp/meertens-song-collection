  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Het waeren twee Koninghs <rhyme label="a" type="f">Kinderen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Zy hadden malkander soo <rhyme label="b" type="m">lief</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Zy konden by malkander niet <rhyme label="c" type="f">comen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Het water was veels te <rhyme label="b" type="m" corresp="#s1:l2">diep</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat stack zy op drie <rhyme label="a" type="f">Keerssen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Drie Keerssen van twaelf in’t <rhyme label="b" type="m">pont</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om daer me te <rhyme label="c" type="f">behouden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">’t Konings soontje van iaren was <rhyme label="b" type="m" corresp="#s2:l2">ionc</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Met een quam daer een <rhyme label="a" type="f">Beste</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een ouwe fenijnde <rhyme label="b" type="m">Bes</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En die blies uyt de <rhyme label="c" type="f">Keerssen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daer verdroncker die Ionge <rhyme label="b" type="m" corresp="#s3:l2">helt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Och moeder seydse <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Mijn hoofje doet mijnder soo <rhyme label="b" type="m">wee</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Mocht icker een kort half <rhyme label="c" type="f">uertje</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Spanceren al langhes die <rhyme label="b" type="m" corresp="#s4:l2">Zee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Och dochter seydse <rhyme label="a" type="f">dochter</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Alleen en meught gy niet <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Weckt op u Ionghste <rhyme label="c" type="f">Suster</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En laeter die met u <rhyme label="b" type="m" corresp="#s5:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mijn alder-ionghste <rhyme label="a" type="f">suster</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Datse alsoo kleynen <rhyme label="b" type="m">kindt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Zy pluckter maer alle die <rhyme label="c" type="f">Roosjes</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die zy in haren weghen <rhyme label="b" type="m" corresp="#s6:l2">vindt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Zy plackter maer alle de <rhyme label="a" type="f">Roosjes</rhyme></l>
        <l id="s7:l2" met="3" type="-+">En die bladeren laet zy <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dan segghen maer alle die <rhyme label="c" type="f">Lieden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat hebben de Konincks kinderen <rhyme label="b" type="m" corresp="#s7:l2">gedaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De moeder ginck nae de <rhyme label="a" type="f">Kercke</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">De dochter ginck haren <rhyme label="b" type="m">ganck</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Zy ginck maer alsoo <rhyme label="c" type="f">veere</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Daer zy haer vaders vissertje <rhyme label="b" type="m" corresp="#s8:l2">vant</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Och Vusscher seydse <rhyme label="a" type="f">Visscher</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Mijn Vaders <rhyme label="b" type="m">Visscherkijn</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wout ghy my een weynichie <rhyme label="c" type="f">vissen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">‘t Sou u wel gheloonet <rhyme label="b" type="m" corresp="#s9:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Hy smeet zijn Netten in’t <rhyme label="a" type="f">water</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">De Looties die ginghen te <rhyme label="b" type="m">grondt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hoe haest was daer <rhyme label="c" type="f">ghevisschet</rhyme></l>
        <l id="s10:l4" met="3" type="-+">’t Konings soontje van iaren was <rhyme label="b" type="m" corresp="#s10:l2">iong</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wat trock zy van haer <rhyme label="a" type="f">hande</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Een Vingerlijck root van <rhyme label="b" type="m">Goudt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Houdt daer mijn Vaders <rhyme label="c" type="f">Visscher</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Daer isser den loon van <rhyme label="b" type="m" corresp="#s11:l2">ion</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Zy nam hem in haer <rhyme label="a" type="f">armen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Zy kusten hem voor zijn <rhyme label="b" type="m">mondt</rhyme>:</l>
        <l id="s12:l3" met="3" type="-+">Och mondeling kost ghy <rhyme label="c" type="f">spreecken</rhyme>!</l>
        <l id="s12:l4" met="3" type="-+">Och hartje waert ghyder <rhyme label="b" type="m" corresp="#s12:l2">ghesont</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Zy nam hem in haer <rhyme label="a" type="f">armen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Zy sproncker mee in de <rhyme label="b" type="m">Zee</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Adieu mijn Vader en <rhyme label="c" type="f">Moeder</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Van u leven siet ghy my niet <rhyme label="b" type="m" corresp="#s13:l2">weer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Adieu mijn Vader en <rhyme label="a" type="f">Moeder</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Mijn Vriendekens alle <rhyme label="b" type="m">ghelijck</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Adieu mijn Suster en <rhyme label="c" type="f">Broeder</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Ick vaerder naer ’t <rhyme label="b" type="m" corresp="#s14:l2">Hemelrijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
