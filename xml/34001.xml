<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>34001</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags>
      <tag>triumph song</tag>
    </tags>
    <melody>Gallas il n'est pas à propos</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Peys-Godinne, die soo <rhyme label="a" type="m">langh</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Verjaecht uyt onse <rhyme label="b" type="f">palen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hebt elders moeten <rhyme label="b" type="f" corresp="#s1:l2">dwalen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Van ons geweecken door <rhyme label="a" type="m" corresp="#s1:l1">bedwangh</rhyme>;</l>
        <l id="s1:l5" met="6" type="+-">Voorwaer ghy zijt // die ons bevrijdt // en <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s1:l6" met="3" type="+-">In de droefste <rhyme label="d" type="f">tijden</rhyme>;</l>
        <l id="s1:l7" met="2" type="-+">Die ’t quaedt in <rhyme label="e" type="m">goedt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En ’t suer in <rhyme label="e" type="m" corresp="#s1:l7">soet</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Verkeeren <rhyme label="e" type="m" corresp="#s1:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy komt beweeght door ons <rhyme label="a" type="m">beklagh</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Geroepen door de <rhyme label="b" type="f">wenschen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van soo veel duysent <rhyme label="b" type="f" corresp="#s2:l2">menschen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En brenght den Vrede voor den <rhyme label="a" type="m" corresp="#s2:l1">dagh</rhyme>.</l>
        <l id="s2:l5" met="6" type="+-">Voorwaer ghy zijt // die ons bevrijdt // en <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s2:l6" met="3" type="+-">In de droefste <rhyme label="d" type="f">tijden</rhyme>;</l>
        <l id="s2:l7" met="2" type="-+">Die ’t quaedt in <rhyme label="e" type="m">goedt</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En ’t suer in <rhyme label="e" type="m" corresp="#s2:l7">soet</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Verkeeren <rhyme label="e" type="m" corresp="#s2:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den Boer die Mars verdreven <rhyme label="a" type="m">heeft</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En sijn verwoeste <rhyme label="b" type="f">velden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ghy weder stelt in <rhyme label="b" type="f" corresp="#s3:l2">weelden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En hem met rust zijn vruchten <rhyme label="a" type="m" corresp="#s3:l1">gheeft</rhyme>.</l>
        <l id="s3:l5" met="6" type="+-">Voorwaer ghy zijt // die ons bevrijdt // en <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s3:l6" met="3" type="+-">In de droefste <rhyme label="d" type="f">tijden</rhyme>;</l>
        <l id="s3:l7" met="2" type="-+">Die ’t quaedt in <rhyme label="e" type="m">goedt</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">En ’t suer in <rhyme label="e" type="m" corresp="#s3:l7">soet</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Verkeeren <rhyme label="e" type="m" corresp="#s3:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Borgher, die noch in sijn <rhyme label="a" type="m">stee</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Noch buyten op sijn <rhyme label="b" type="f">erven</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Gherustheydt kost <rhyme label="b" type="f" corresp="#s4:l2">verwerven</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ghy stelt in vrydom en in <rhyme label="a" type="m" corresp="#s4:l1">vree</rhyme>.</l>
        <l id="s4:l5" met="6" type="+-">Voorwaer ghy zijt // die ons bevrijdt // en <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s4:l6" met="3" type="+-">In de droefste <rhyme label="d" type="f">tijden</rhyme>;</l>
        <l id="s4:l7" met="2" type="-+">Die ’t quaedt in <rhyme label="e" type="m">goedt</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En ’t suer in <rhyme label="e" type="m" corresp="#s4:l7">soet</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Verkeeren <rhyme label="e" type="m" corresp="#s4:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Koopman komt ghy oock te <rhyme label="a" type="m">baet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die met sijn rijcke <rhyme label="b" type="f">Waeren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Al-om kan gaen en <rhyme label="b" type="f" corresp="#s5:l2">vaeren</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Want ’t Landt en Zee hem open <rhyme label="a" type="m" corresp="#s5:l1">staet</rhyme>.</l>
        <l id="s5:l5" met="6" type="+-">Voorwaer ghy zijt // die ons bevrijdt // en <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s5:l6" met="3" type="+-">In de droefste <rhyme label="d" type="f">tijden</rhyme>;</l>
        <l id="s5:l7" met="2" type="-+">Die ’t quaedt in <rhyme label="e" type="m">goedt</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">En ’t suer in <rhyme label="e" type="m" corresp="#s5:l7">soet</rhyme></l>
        <l id="s5:l9" met="2" type="+-">Verkeeren <rhyme label="e" type="m" corresp="#s5:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Den Joncker, die sijn geldt en <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sijn landen ende <rhyme label="b" type="f">gronden</rhyme></l>
        <l id="s6:l3" met="3" type="-+">In d’oorlogh heeft <rhyme label="b" type="f" corresp="#s6:l2">verslonden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ghy stelt op eenen verschen <rhyme label="a" type="m" corresp="#s6:l1">voet</rhyme>.</l>
        <l id="s6:l5" met="6" type="+-">Voorwaer ghy zijt // die ons bevrijdt // en <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s6:l6" met="3" type="+-">In de droefste <rhyme label="d" type="f">tijden</rhyme>;</l>
        <l id="s6:l7" met="2" type="-+">Die ’t quaedt in <rhyme label="e" type="m">goedt</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">En ’t suer in <rhyme label="e" type="m" corresp="#s6:l7">soet</rhyme></l>
        <l id="s6:l9" met="2" type="+-">Verkeeren <rhyme label="e" type="m" corresp="#s6:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Vw’ deught Apollo singhen <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Want ’t soet gheluyt der <rhyme label="b" type="f">Wetten</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Door Trommels en <rhyme label="b" type="f" corresp="#s7:l2">Trompetten</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Verdooft, ghy weder hooren <rhyme label="a" type="m" corresp="#s7:l1">doet</rhyme>.</l>
        <l id="s7:l5" met="6" type="+-">Voorwaer ghy zijt // die ons bevrijdt // en <rhyme label="c" type="m">verblijdt</rhyme></l>
        <l id="s7:l6" met="3" type="+-">In de droefste <rhyme label="d" type="f">tijden</rhyme>;</l>
        <l id="s7:l7" met="2" type="-+">Die ’t quaedt in <rhyme label="e" type="m">goedt</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">En ’t suer in <rhyme label="e" type="m" corresp="#s7:l7">soet</rhyme></l>
        <l id="s7:l9" met="2" type="+-">Verkeeren <rhyme label="e" type="m" corresp="#s7:l7">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
