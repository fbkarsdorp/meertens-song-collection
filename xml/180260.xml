<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180260</id>
    <date>
      <year-start>1783</year-start>
      <year-end>1783</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Van de korporaal</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O! Hemel ik <rhyme label="a" type="m">bespeur</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Dat ik niet meer kan <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">De dood staet voor myn <rhyme label="a" type="m" corresp="#s1:l1">deur</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt myn dog Pardon <rhyme label="b" type="f" corresp="#s1:l2">geven</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Myn Levens-loop is <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Waer mee heb ik 't <rhyme label="c" type="m" corresp="#s1:l5">verbruyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Moet dan een <rhyme label="a" type="m">Muziekant</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">In 't bloeijen van zyn <rhyme label="b" type="f">Leven</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Al sterven hier in 't <rhyme label="a" type="m" corresp="#s2:l1">Land</rhyme>?</l>
        <l id="s2:l4" met="3" type="-+">ô Dood! wilt mijn dog <rhyme label="b" type="f" corresp="#s2:l2">sparen</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">En laet my Leven <rhyme label="c" type="m">hier</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">In vreugden en <rhyme label="c" type="m" corresp="#s2:l5">Playsier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Bent gy een <rhyme label="a" type="m">Muziekant</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Die heb ik ook van <rhyme label="b" type="f">noden</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Ik koom als <rhyme label="a" type="m" corresp="#s3:l1">Afgesant</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gy moet na 't Ryk der <rhyme label="b" type="f" corresp="#s3:l2">doden</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Gy moet terstond met <rhyme label="c" type="m">myn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hier helpt geen <rhyme label="c" type="m" corresp="#s3:l5">Medicyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Moet dan een Krygsman <rhyme label="a" type="m">stout</rhyme>?</l>
        <l id="s4:l2" met="3" type="-+">Een meester der <rhyme label="b" type="f">Zoldaten</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Ruim twintig Iaren <rhyme label="a" type="m" corresp="#s4:l1">oud</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">De Weereld al <rhyme label="b" type="f" corresp="#s4:l2">verlaten</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">ô Dood! verschoont myn <rhyme label="c" type="m">dog</rhyme>!</l>
        <l id="s4:l6" met="3" type="-+">En laet myn Leven <rhyme label="c" type="m" corresp="#s4:l5">nog</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Neen ik verschoon u <rhyme label="a" type="m">niet</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Ik weet van geen <rhyme label="b" type="f">Zoldaten</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gy moet nam myn <rhyme label="a" type="m" corresp="#s5:l1">Gebiet</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">De Weereld al <rhyme label="b" type="f" corresp="#s5:l2">verlaten</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Al bent gy een <rhyme label="c" type="m">Korporael</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ik vrees geen Vuur nog <rhyme label="c" type="m" corresp="#s5:l5">Stael</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wel moet ik dan van <rhyme label="a" type="m">kant</rhyme>?</l>
        <l id="s6:l2" met="3" type="-+">Daer ik in 't kort kan <rhyme label="b" type="f">hopen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De plaets al van een <rhyme label="a" type="m" corresp="#s6:l1">Serjand</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dit staet al voor myn <rhyme label="b" type="f" corresp="#s6:l2">open</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dus bid ik u soo <rhyme label="c" type="m">zeer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Pardon voor desen <rhyme label="c" type="m" corresp="#s6:l5">keer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ik weet van geen <rhyme label="a" type="m">Pardon</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Daer is by God <rhyme label="b" type="f">genaden</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Al waert gy een <rhyme label="a" type="m" corresp="#s7:l1">Baron</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Ik agt geen <rhyme label="b" type="f" corresp="#s7:l2">Kaskenaden</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Gy moet met myn in het <rhyme label="c" type="m">Graf</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En van de Weereld <rhyme label="c" type="m" corresp="#s7:l5">af</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wel Dood in ben soo <rhyme label="a" type="m">Ionk</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">Wat heb ik dog <rhyme label="b" type="f">misdreeven</rhyme>?</l>
        <l id="s8:l3" met="3" type="-+">Wat helpt myn staet of <rhyme label="a" type="m" corresp="#s8:l1">pronk</rhyme>?</l>
        <l id="s8:l4" met="3" type="-+">Als ik niet meer kan <rhyme label="b" type="f" corresp="#s8:l2">Leeven</rhyme>?</l>
        <l id="s8:l5" met="3" type="-+">Gy weet dat ik <rhyme label="c" type="m">omtrent</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Stond op <rhyme label="c" type="m" corresp="#s8:l5">advancement</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Bent gy so <rhyme label="a" type="m">assurand</rhyme>?</l>
        <l id="s9:l2" met="3" type="-+">En praet van <rhyme label="b" type="f">avanceren</rhyme>?</l>
        <l id="s9:l3" met="3" type="-+">Neen, neen gy moet van <rhyme label="a" type="m" corresp="#s9:l1">kant</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Trekt uyt u schone <rhyme label="b" type="f" corresp="#s9:l2">Kleeren</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Legt af u <rhyme label="c" type="m">Zydgeweer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Gy zyt geen Krygsman <rhyme label="c" type="m" corresp="#s9:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">ô Hemel! wat een <rhyme label="a" type="m">smert</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">De Dood komt myn al <rhyme label="b" type="f">nader</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Dat voel ik aen myn <rhyme label="a" type="m" corresp="#s10:l1">hert</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Schryft het dog aen myn <rhyme label="b" type="f" corresp="#s10:l2">Vader</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Schryft dog na het <rhyme label="c" type="m">Hesseland</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Een vliegende <rhyme label="c" type="m" corresp="#s10:l5">Courand</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Zorgt daer in 't minst niet <rhyme label="a" type="m">voor</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Hier is u regte <rhyme label="b" type="f">Haven</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">Hier legt een <rhyme label="a" type="m" corresp="#s11:l1">Gansen-Choor</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Van uwe staet <rhyme label="b" type="f" corresp="#s11:l2">begraven</rhyme>:</l>
        <l id="s11:l5" met="3" type="-+">Daerom begeeft u <rhyme label="c" type="m">maer</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Al op den <rhyme label="c" type="m" corresp="#s11:l5">doden-Baer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Komt Doctor komt <rhyme label="a" type="m">gezwind</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Doet my een ader <rhyme label="b" type="f">open</rhyme>:</l>
        <l id="s12:l3" met="3" type="-+">Eer myn de Dood <rhyme label="a" type="m" corresp="#s12:l1">verslint</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Misschien is nog te <rhyme label="b" type="f" corresp="#s12:l2">hoopen</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Dat ik het Leven <rhyme label="c" type="m">hou</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Komt dog en helpt myn <rhyme label="c" type="m" corresp="#s12:l5">gou</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Voort, voort de Tyd <rhyme label="a" type="m">mankeert</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Hier helpt geen <rhyme label="b" type="f">Aderlaten</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Daer is al <rhyme label="a" type="m" corresp="#s13:l1">gecommandeert</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Een gansch Platon <rhyme label="b" type="f" corresp="#s13:l2">Zoldaten</rhyme>:</l>
        <l id="s13:l5" met="3" type="-+">Die zullen u voor <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Drie Salvoos doen in 't <rhyme label="c" type="m" corresp="#s13:l5">Graf</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Ach! ach! is 't al soo <rhyme label="a" type="m">laet</rhyme>?</l>
        <l id="s14:l2" met="3" type="-+">Dat ik in 't Graf sal <rhyme label="b" type="f">wonen</rhyme>?</l>
        <l id="s14:l3" met="3" type="-+">Dat men de laesten <rhyme label="a" type="m" corresp="#s14:l1">eer</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Sal doen aen my <rhyme label="b" type="f" corresp="#s14:l2">betonen</rhyme>:</l>
        <l id="s14:l5" met="3" type="-+">Moet ik dan met <rhyme label="c" type="m">gewelt</rhyme>?</l>
        <l id="s14:l6" met="3" type="-+">Naer het Americaense <rhyme label="c" type="m" corresp="#s14:l5">Veld</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Adieu dan <rhyme label="a" type="m">Americanen</rhyme>!</l>
        <l id="s15:l2" met="3" type="-+">Ik ga na het Ryk der <rhyme label="b" type="f">doden</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Concert of <rhyme label="a" type="m" corresp="#s15:l1">Opera</rhyme>:</l>
        <l id="s15:l4" met="3" type="-+">Is hier niet meer van <rhyme label="b" type="f" corresp="#s15:l2">noden</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Of andere <rhyme label="c" type="m">vrolykheyt</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Ik word in 't Graf <rhyme label="c" type="m" corresp="#s15:l5">geleyt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Adieu dan al te <rhyme label="a" type="m">saem</rhyme>!</l>
        <l id="s16:l2" met="3" type="-+">Gy Iuffrouws en gy <rhyme label="b" type="f">Heeren</rhyme>;</l>
        <l id="s16:l3" met="3" type="-+">Daer ik soo <rhyme label="a" type="m" corresp="#s16:l1">aengenaem</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Plagt meede te <rhyme label="b" type="f" corresp="#s16:l2">verkeren</rhyme>:</l>
        <l id="s16:l5" met="3" type="-+">Kies maer nu tot U <rhyme label="c" type="m">deel</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Een ander uyt het <rhyme label="c" type="m" corresp="#s16:l5">Kasteel</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Adieu myn <rhyme label="a" type="m">Cammeraets</rhyme>!</l>
        <l id="s17:l2" met="3" type="-+">Nu moet ik van u <rhyme label="b" type="f">treden</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Wilt op u goed <rhyme label="a" type="m" corresp="#s17:l1">Zoldaets</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Myn nu ter aerd <rhyme label="b" type="f" corresp="#s17:l2">besteden</rhyme>:</l>
        <l id="s17:l5" met="3" type="-+">Terwyl nu mijn <rhyme label="c" type="m">Persoon</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Moet van het <rhyme label="c" type="m" corresp="#s17:l5">Bataljoon</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Daer is myn <rhyme label="a" type="m">Zyd-geweer</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Myn Rotting en myn <rhyme label="b" type="f">Kleeren</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Ik kan nu dog niet <rhyme label="a" type="m" corresp="#s18:l1">meer</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Zoldaten <rhyme label="b" type="f" corresp="#s18:l2">excerseren</rhyme>:</l>
        <l id="s18:l5" met="3" type="-+">Draegt myn maer als een <rhyme label="c" type="m">Lijk</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Naer 't eeuwig doden <rhyme label="c" type="m" corresp="#s18:l5">rijk</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Weg Weerelt, weg van <rhyme label="a" type="m">my</rhyme>!</l>
        <l id="s19:l2" met="3" type="-+">Ik gaen myn nu <rhyme label="b" type="f">begeven</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Als uit een <rhyme label="a" type="m" corresp="#s19:l1">Slaverny</rhyme>:</l>
        <l id="s19:l4" met="3" type="-+">En na het eeuwig <rhyme label="b" type="f" corresp="#s19:l2">Leven</rhyme>:</l>
        <l id="s19:l5" met="3" type="-+">Daer vind ik <rhyme label="c" type="m">abondant</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Het ryke <rhyme label="c" type="m" corresp="#s19:l5">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Hier leid een <rhyme label="a" type="m">Korperael</rhyme>?</l>
        <l id="s20:l2" met="3" type="-+">Van Twee-en-twintig <rhyme label="b" type="f">Iaren</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Een Meester van de <rhyme label="a" type="m" corresp="#s20:l1">Bael</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Door Muziek ryk <rhyme label="b" type="f" corresp="#s20:l2">ervaren</rhyme>:</l>
        <l id="s20:l5" met="3" type="-+">Geboren uyt <rhyme label="c" type="m">Hesseland</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Hier leid een <rhyme label="c" type="m" corresp="#s20:l5">Muziekant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
