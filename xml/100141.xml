  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ditez moy quel plaisir avez <rhyme label="a" type="m">vous</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Philis de mestre en <rhyme label="a" type="m" corresp="#s1:l1">courous</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">pensez en vous <rhyme label="b" type="f">mesme</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">&amp; songez, Que vous <rhyme label="c" type="f">desobligez</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Celuy qui vous <rhyme label="b" type="f" corresp="#s1:l3">aime</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vn Amant moins fidelle que <rhyme label="a" type="m">moy</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Abandonnerait a vostre <rhyme label="a" type="m" corresp="#s2:l1">Loy</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Moy tout au <rhyme label="b" type="f">contraire</rhyme></l>
        <l id="s2:l4" met="4" type="+-">j'aime mieux expirer, &amp; <rhyme label="c" type="f">mourir</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Que de vous <rhyme label="b" type="f" corresp="#s2:l3">desplaire</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
