    <melody>O lux beata</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O licht heijlich <rhyme label="a" type="m">drieuuldicheijt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">ende in sonder <rhyme label="a" type="m" corresp="#s1:l1">enicheijt</rhyme></l>
        <l id="s1:l3" met="4" type="-+">die vierich son gaet me van <rhyme label="b" type="m">hier</rhyme></l>
        <l id="s1:l4" met="4" type="-+">stort in ons hart t'lichtende <rhyme label="b" type="m" corresp="#s1:l3">vier</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Smorgens met sange louen <rhyme label="a" type="m">wij</rhyme></l>
        <l id="s2:l2" met="4" type="-+">oeck tsauonts bidden wij v <rhyme label="a" type="m" corresp="#s2:l1">blij</rhyme></l>
        <l id="s2:l3" met="4" type="-+">in oothmoet louen wij v <rhyme label="b" type="m">heer</rhyme></l>
        <l id="s2:l4" met="4" type="-+">ghij sijt ons glorij ende ons <rhyme label="b" type="m" corresp="#s2:l3">eer</rhyme></l>
      </lg>
    </lg>
  </text>
