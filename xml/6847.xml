    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Pange lingua</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Myn tongh wilt met vreugden <rhyme label="a" type="f">klingen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dat geheymnis des lijfs <rhyme label="b" type="m">zart</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En van duyren bloet oock <rhyme label="a" type="f" corresp="#s1:l1">singen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat die lijfs vruchrt edler <rhyme label="b" type="m" corresp="#s1:l2">aert</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Aller werlt dat heyl te <rhyme label="a" type="f" corresp="#s1:l1">brengen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Heeft vergoten <rhyme label="b" type="m" corresp="#s1:l2">onverspaert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ons geboren, ons <rhyme label="a" type="f">gegeven</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Wt een Maget <rhyme label="b" type="m">onbesmet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">In dees werlt was sijn <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Sijn woordt planten hy seer <rhyme label="b" type="m" corresp="#s2:l2">net</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En sijn wandel daer <rhyme label="a" type="f" corresp="#s2:l1">beneven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Heeft hy wonderlijck <rhyme label="b" type="m" corresp="#s2:l2">beset</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">In dat leste avents <rhyme label="a" type="f">eten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Als hy by sijn Ionghers <rhyme label="b" type="m">sat</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat geset wordt niet <rhyme label="a" type="f" corresp="#s3:l1">vergeten</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Doe hy dat Paeschlamken <rhyme label="b" type="m" corresp="#s3:l2">at</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En gaf tot een spijs <rhyme label="a" type="f" corresp="#s3:l1">vermeeten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Sich selfs als een kostlijck <rhyme label="b" type="m" corresp="#s3:l2">schat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">T’woort vleisch sijnde, heeft <rhyme label="a" type="f">waerachtigh</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Door t’woort van’t broot vleysch <rhyme label="b" type="m">gemaeckt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En van Wijn sijn bloet seer <rhyme label="a" type="f" corresp="#s4:l1">crachtigh</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat niet en mach sijn <rhyme label="b" type="m" corresp="#s4:l2">gelaeckt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Want een goet hert <rhyme label="a" type="f" corresp="#s4:l1">twijfelachtigh</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Door t’geloof tot sterckheyt <rhyme label="b" type="m" corresp="#s4:l2">raeckt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Laet ons ootmoedich <rhyme label="a" type="f">aenbeden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Soo een weerdig <rhyme label="b" type="m">Sacrament</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Een voor d’oude Wet met <rhyme label="a" type="f" corresp="#s5:l1">reden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Laet die nieu ons sijn <rhyme label="b" type="m" corresp="#s5:l2">bekent</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Ons verstant moet t’allen <rhyme label="a" type="f" corresp="#s5:l1">steden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Door t’geloof sijn <rhyme label="b" type="m" corresp="#s5:l2">ongeschent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Den Vader en Soon <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Sy lof en <rhyme label="b" type="m">jubilatie</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Saligheydt, eer, cracht <rhyme label="a" type="f" corresp="#s6:l1">mitsdesen</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Haer sy oock <rhyme label="b" type="m" corresp="#s6:l2">benedictie</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Die van beyden compt <rhyme label="a" type="f" corresp="#s6:l1">geresen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Sy gelijcke <rhyme label="b" type="m" corresp="#s6:l2">laudatie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
