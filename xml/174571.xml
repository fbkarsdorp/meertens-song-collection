    <tags>
      <tag translation="New Year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Zing God een nieuw lied met hart en moed</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">Werelt wel onthout, tyt van gods <rhyme label="a" type="m">geboort</rhyme></l>
        <l id="s1:l2" met="5" type="+-">sestien hondert Iaer twintich en <rhyme label="b" type="f">seven</rhyme></l>
        <l id="s1:l3" met="6" type="+-">vreught heeft Ionck en out, by Bethlehem <rhyme label="a" type="m" corresp="#s1:l1">gehoort</rhyme></l>
        <l id="s1:l4" met="5" type="+-">van shemels heyr schaer, d'herders <rhyme label="b" type="f" corresp="#s1:l2">beneven</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Die godt loff prys, hebben <rhyme label="b" type="f" corresp="#s1:l2">gegeven</rhyme></l>
        <l id="s1:l6" met="4" type="+-">om mogen leven, In vreed' op <rhyme label="c" type="m">aerd'</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">elck hart beswaert ,, is vreucht <rhyme label="c" type="m" corresp="#s1:l6">verclaerd'</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">t'woort hooch vermaert ,, <rhyme label="c" type="m" corresp="#s1:l6">geopenbaert</rhyme></l>
        <l id="s1:l9" met="2" type="+-">aen Christi <rhyme label="d" type="f">leden</rhyme></l>
        <l id="s1:l10" met="4" type="+-">T'geen met voordacht ,, lang is <rhyme label="e" type="m">verwacht</rhyme></l>
        <l id="s1:l11" met="2" type="+-">om sien gods <rhyme label="e" type="m" corresp="#s1:l10">kracht</rhyme></l>
        <l id="s1:l12" met="2" type="+-">op den <rhyme label="e" type="m" corresp="#s1:l10">karsmacht</rhyme></l>
        <l id="s1:l13" met="2" type="+-">In <rhyme label="d" type="f" corresp="#s1:l9">nedricheden</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Drije wysen Int oost, die hebben <rhyme label="a" type="m">gesien</rhyme></l>
        <l id="s2:l2" met="5" type="+-">een licht wonder groot om aff te <rhyme label="b" type="f">meeten</rhyme></l>
        <l id="s2:l3" met="6" type="+-">door gelooft tot troost, gods wil most <rhyme label="a" type="m" corresp="#s2:l1">geschien</rhyme></l>
        <l id="s2:l4" met="5" type="+-">t'was teecken van noot ,, by de <rhyme label="b" type="f" corresp="#s2:l2">planeeten</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Gods leytster waer door men mocht <rhyme label="b" type="f" corresp="#s2:l2">weeten</rhyme></l>
        <l id="s2:l6" met="4" type="+-">hoge secreten, uyt shemels <rhyme label="c" type="m">troon</rhyme></l>
        <l id="s2:l7" met="4" type="+-">hoe dat gods soon, doen lach ten <rhyme label="c" type="m" corresp="#s2:l6">toon</rhyme></l>
        <l id="s2:l8" met="4" type="+-">In bethlem schoon, veel <rhyme label="c" type="m" corresp="#s2:l6">ongewoon</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Om te <rhyme label="d" type="f">gelooven</rhyme></l>
        <l id="s2:l10" met="4" type="+-">zo arm van staet, godt schickt voor <rhyme label="e" type="m">raet</rhyme></l>
        <l id="s2:l11" met="2" type="+-">wysen te <rhyme label="e" type="m" corresp="#s2:l10">baet</rhyme>,</l>
        <l id="s2:l12" met="2" type="+-">Die t'godlyck <rhyme label="e" type="m" corresp="#s2:l10">saet</rhyme></l>
        <l id="s2:l13" met="2" type="+-">sochten van <rhyme label="d" type="f" corresp="#s2:l9">boven</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Herodes voluydt, op t'goddelyck <rhyme label="a" type="m">kint</rhyme></l>
        <l id="s3:l2" met="5" type="+-">daer van was geseyt, om te <rhyme label="b" type="f">regeren</rhyme></l>
        <l id="s3:l3" met="6" type="+-">doen In twee Iaer tyt, hy tmoorden <rhyme label="a" type="m" corresp="#s3:l1">begint</rhyme></l>
        <l id="s3:l4" met="5" type="+-">Om t'geen was voorseyt verbreyt te doen <rhyme label="b" type="f" corresp="#s3:l2">cesseren</rhyme></l>
        <l id="s3:l5" met="4" type="+-">kinders hy doot tot straffs <rhyme label="b" type="f" corresp="#s3:l2">vermeeren</rhyme></l>
        <l id="s3:l6" met="4" type="+-">nae t'woort des heeren, gehoort <rhyme label="c" type="m">tgeween</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">gekerst door sneen ,, om vinden <rhyme label="c" type="m" corresp="#s3:l6">een</rhyme></l>
        <l id="s3:l8" met="4" type="+-">onder de geen, Dootlicken <rhyme label="c" type="m" corresp="#s3:l6">vertreen</rhyme></l>
        <l id="s3:l9" met="2" type="+-">tot veel <rhyme label="d" type="f">mishagen</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Ioseph gevlucht ,, voort groot <rhyme label="e" type="m">gerucht</rhyme></l>
        <l id="s3:l11" met="2" type="+-">bewaert de <rhyme label="e" type="m" corresp="#s3:l10">vrucht</rhyme></l>
        <l id="s3:l12" met="2" type="+-">onder de <rhyme label="e" type="m" corresp="#s3:l10">lucht</rhyme></l>
        <l id="s3:l13" met="2" type="+-">Iaeren en <rhyme label="d" type="f" corresp="#s3:l9">dagen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Een winter bedrouft, de Stadt <rhyme label="a" type="m">Bethlehem</rhyme></l>
        <l id="s4:l2" met="5" type="+-">die staegh wort gequelt, van spaensche <rhyme label="b" type="f">draecken</rhyme></l>
        <l id="s4:l3" met="6" type="+-">ons noodich behoeft, te hooren gods <rhyme label="a" type="m" corresp="#s4:l1">stem</rhyme></l>
        <l id="s4:l4" met="5" type="+-">Die met schrick het velt ledich can <rhyme label="b" type="f" corresp="#s4:l2">maecken</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Int nieuw Iaer, bidt voor alle <rhyme label="b" type="f" corresp="#s4:l2">saecken</rhyme></l>
        <l id="s4:l6" met="4" type="+-">vrede te naecken, voor tytlick <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s4:l7" met="4" type="+-">tonnosel bloet, te werden <rhyme label="c" type="m" corresp="#s4:l6">behoet</rhyme></l>
        <l id="s4:l8" met="4" type="+-">voor tegenspoet ,, die mars <rhyme label="c" type="m" corresp="#s4:l6">verwoet</rhyme></l>
        <l id="s4:l9" met="2" type="+-">weet te <rhyme label="d" type="f">bedrijven</rhyme></l>
        <l id="s4:l10" met="4" type="+-">op tnederlant ,, door <rhyme label="e" type="m">misverstant</rhyme></l>
        <l id="s4:l11" met="2" type="+-">staet vast gods <rhyme label="e" type="m" corresp="#s4:l10">hant</rhyme></l>
        <l id="s4:l12" met="2" type="+-">zal den <rhyme label="e" type="m" corresp="#s4:l10">vyant</rhyme></l>
        <l id="s4:l13" met="2" type="+-">voor ons <rhyme label="d" type="f" corresp="#s4:l9">ontlijven</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Den Prins boven all, comt op aerden <rhyme label="a" type="m">loff</rhyme></l>
        <l id="s5:l2" met="5" type="+-">werelt daer op let, tot twist <rhyme label="b" type="f">genegen</rhyme></l>
        <l id="s5:l3" met="6" type="+-">t'lydt samich getal, soet levend van <rhyme label="a" type="m" corresp="#s5:l1">stoff</rhyme></l>
        <l id="s5:l4" met="5" type="+-">prijsen gods wett, en sijnen <rhyme label="b" type="f" corresp="#s5:l2">segen</rhyme></l>
        <l id="s5:l5" met="4" type="+-">maer die gierige t'dient niet <rhyme label="b" type="f" corresp="#s5:l2">verswegen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">comt straffe tegen, voor tlaetste <rhyme label="c" type="m">ent</rhyme></l>
        <l id="s5:l7" met="4" type="+-">van sond u went; die tleven <rhyme label="c" type="m" corresp="#s5:l6">schent</rhyme></l>
        <l id="s5:l8" met="4" type="+-">lieffd' t Fondament, maeckt d'arm <rhyme label="c" type="m" corresp="#s5:l6">betrent</rhyme></l>
        <l id="s5:l9" met="2" type="+-">Die moeten <rhyme label="d" type="f">swerven</rhyme></l>
        <l id="s5:l10" met="4" type="+-">acoley witt, onthout toch <rhyme label="e" type="m">ditt</rhyme></l>
        <l id="s5:l11" met="2" type="+-">Die om hoogh <rhyme label="e" type="m" corresp="#s5:l10">sitt</rhyme></l>
        <l id="s5:l12" met="2" type="+-">weet wie recht <rhyme label="e" type="m" corresp="#s5:l10">bidtt</rhyme></l>
        <l id="s5:l13" met="2" type="+-">om tryck te <rhyme label="d" type="f" corresp="#s5:l9">erven</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
