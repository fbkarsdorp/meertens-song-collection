<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159122</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wees welkom bruidegom</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">KOmt Fama blaest int <rhyme label="a" type="m">openbaer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ter eeren van dit jonge <rhyme label="a" type="m" corresp="#s1:l1">paer</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Komt wenst haer <rhyme label="b" type="m">mee</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Des Heeren heyl'ge <rhyme label="b" type="m" corresp="#s1:l3">vree</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Geluck so wenschen wy te <rhyme label="a" type="m">saem</rhyme></l>
        <l id="s2:l2" met="4" type="-+">U beyde in des Heeren <rhyme label="a" type="m" corresp="#s2:l1">naem</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Geluck jongh <rhyme label="b" type="m">paer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Geluck van Iaer tot <rhyme label="b" type="m" corresp="#s2:l3">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Aenhoort nu wat gy schuldig <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En hoe ghy sult tot aller <rhyme label="a" type="m" corresp="#s3:l1">tijt</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">In d' <rhyme label="b" type="m">Echte-Staet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">U dragen vroegh en <rhyme label="b" type="m" corresp="#s3:l3">laet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy man hooft sult al wat ghy <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">In u geluck of <rhyme label="a" type="m" corresp="#s4:l1">tegenspoet</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">In vreught of <rhyme label="b" type="m">pijn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Niet wreet noch korsel <rhyme label="b" type="m" corresp="#s4:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">U Huysvrou sult gaen te <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">In vrede by haer woonen <rhyme label="a" type="m" corresp="#s5:l1">soet</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">En <rhyme label="b" type="m">onbeswaert</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Haer houden lief en <rhyme label="b" type="m" corresp="#s5:l3">waert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy zijt voortaen u eygen <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Daer is een vast verbont <rhyme label="a" type="m" corresp="#s6:l1">geschiet</rhyme>:</l>
        <l id="s6:l3" met="2" type="-+">Want man en <rhyme label="b" type="m">wijf</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Twee Zieltjes in een <rhyme label="b" type="m" corresp="#s6:l3">Lijf</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">En gy Huysvrou van uwen <rhyme label="a" type="m">man</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Neemt oock u plicht gewilligh <rhyme label="a" type="m" corresp="#s7:l1">an</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Met vreught <rhyme label="b" type="m">betoont</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat Liefde by u <rhyme label="b" type="m" corresp="#s7:l3">woont</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">U man sult ghy gehoorsaem <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Uyt reyne Liefd' en niet in <rhyme label="a" type="m" corresp="#s8:l1">schijn</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">De vreed na <rhyme label="b" type="m">jaecht</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Doe niet dat hem <rhyme label="b" type="m" corresp="#s8:l3">mishaeght</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">In woorden en in daden <rhyme label="a" type="m">kuys</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Sachtmoedig, stil ook in het <rhyme label="a" type="m" corresp="#s9:l1">kruys</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Met heus <rhyme label="b" type="m">gelaet</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Want dit is u <rhyme label="b" type="m" corresp="#s9:l3">cieraet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Laet lichten als de Sonne <rhyme label="a" type="m">klaer</rhyme></l>
        <l id="s10:l2" met="4" type="-+">U beyde deught in 't <rhyme label="a" type="m" corresp="#s10:l1">openbaer</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">U kind'ren <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s10:l4" met="3" type="-+">In Godes vrees <rhyme label="b" type="m" corresp="#s10:l3">op-voet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Leeft 't samen vredigh, vreest den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">In Liefde bloeyt, groeyt meer en <rhyme label="a" type="m" corresp="#s11:l1">meer</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Tot dat de <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s11:l4" met="3" type="-+">U voert in Abrams <rhyme label="b" type="m" corresp="#s11:l3">schoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
