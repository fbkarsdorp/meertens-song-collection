<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184712</id>
    <date>
      <year-start>1634</year-start>
      <year-end>1634</year-end>
    </date>
    <location>
      <orginal-name>den haag</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Vrouwtje is uw man niet thuis</melody>
  <author>Pieter Nootmans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Nymphje wilje so ter <rhyme label="a" type="m">vlucht</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Mits Apolloos <rhyme label="b" type="f">krulsel</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daeldt by Thetis uyt de <rhyme label="a" type="m" corresp="#s1:l1">lucht</rhyme></l>
        <l id="s1:l4" met="3" type="+-">En Diaen haer <rhyme label="b" type="f" corresp="#s1:l2">hulsel</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Nu vol-toyt met naer <rhyme label="a" type="m" corresp="#s1:l1">gesucht</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Tot ds nachts <rhyme label="b" type="f" corresp="#s1:l2">vervulsel</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Harder wat sou my de <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Niet dan droefheyt <rhyme label="b" type="f">geven</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Wie sou voor mijn schaepjes <rhyme label="a" type="m" corresp="#s2:l1">wacht</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">(Die van angste <rhyme label="b" type="f" corresp="#s2:l2">beven</rhyme>)</l>
        <l id="s2:l5" met="4" type="+-">Tegen aller beesten <rhyme label="a" type="m" corresp="#s2:l1">macht</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Stellen sijn jongh <rhyme label="b" type="f" corresp="#s2:l2">leven</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dat sal ick u Titer <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Set u by my <rhyme label="b" type="f">neder</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En in't Bos u schaepjes <rhyme label="a" type="m" corresp="#s3:l1">hoen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Daer ick u van <rhyme label="b" type="f" corresp="#s3:l2">Ceder</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Eene Krans in 't <rhyme label="a" type="m" corresp="#s3:l1">klaver-groen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Vlechten sal dan <rhyme label="b" type="f" corresp="#s3:l2">weder</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Of het schijnssel vande <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Harder my eens <rhyme label="b" type="f">kusten</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Sylvia ten waer geen <rhyme label="a" type="m" corresp="#s4:l1">noodt</rhyme>:</l>
        <l id="s4:l4" met="3" type="+-">Slaep hier vry na <rhyme label="b" type="f" corresp="#s4:l2">lusten</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En wilt veyligh in mijn <rhyme label="a" type="m" corresp="#s4:l1">schoot</rhyme></l>
        <l id="s4:l6" met="3" type="+-">By u Titer <rhyme label="b" type="f" corresp="#s4:l2">rusten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Of ons dan een wreede <rhyme label="a" type="m">Leeu</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Listigh quam <rhyme label="b" type="f">betrapen</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Dan sal ick met hel <rhyme label="a" type="m" corresp="#s5:l1">geschreeuw</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Wecken al de <rhyme label="b" type="f" corresp="#s5:l2">Knapen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Die met reckingh en <rhyme label="a" type="m" corresp="#s5:l1">gegeeuw</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Hoeden hare <rhyme label="b" type="f" corresp="#s5:l2">Schapen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Titer nu ghy om mijn <rhyme label="a" type="m">min</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Hebt dus langh <rhyme label="b" type="f">gehengelt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En ghy met u <rhyme label="a" type="m" corresp="#s6:l1">Harderin</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Ziel in Ziel <rhyme label="b" type="f" corresp="#s6:l2">vermengelt</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Soo wert u dan na mijn <rhyme label="a" type="m" corresp="#s6:l1">sin</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Echtens Krans <rhyme label="b" type="f" corresp="#s6:l2">gestrengelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Sylvia als ghy my <rhyme label="a" type="m">kroont</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Met de roose <rhyme label="b" type="f">Bloemen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Sal ick gaen daerPhyllis <rhyme label="a" type="m" corresp="#s7:l1">woont</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">En ons Bruyloft <rhyme label="b" type="f" corresp="#s7:l2">roemen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Om dat ghy met minne <rhyme label="a" type="m" corresp="#s7:l1">loont</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Vwen Naem haer <rhyme label="b" type="f" corresp="#s7:l2">noemen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
