<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24579</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Gheselleken nv laet ons vrolic <rhyme label="a" type="m">syn</rhyme></l>
        <l id="s1:l2" met="4" type="-+">ende laet ons roesken <rhyme label="b" type="f">lesen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">die enghel heeft ons die bosscop <rhyme label="a" type="m" corresp="#s1:l1">gebracht</rhyme></l>
        <l id="s1:l4" met="4" type="-+">wie sal daer droeuich moegen  <rhyme label="b" type="f" corresp="#s1:l2">wesen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Och wonder bauen alle <rhyme label="a" type="m">wonder</rhyme></l>
        <l id="s2:l2" met="4" type="-+">mer dat wy hebben <rhyme label="b" type="f">vernaemen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">hoe dat des edellen gades <rhyme label="a" type="m" corresp="#s2:l1">soen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">wtten hemel is neder <rhyme label="b" type="f" corresp="#s2:l2">gecoemen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat heeft gedaen een reyne <rhyme label="a" type="m">maget</rhyme></l>
        <l id="s3:l2" met="4" type="-+">van gade wal seer <rhyme label="b" type="f">gepresen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">god heeft sy waerlic wal <rhyme label="a" type="m" corresp="#s3:l1">behaget</rhyme></l>
        <l id="s3:l4" met="4" type="-+">wie sal daer droeuich moegen <rhyme label="b" type="f" corresp="#s3:l2">wesen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">God is gebaren ter <rhyme label="a" type="m">myddernacht</rhyme></l>
        <l id="s4:l2" met="4" type="-+">van eenre maget <rhyme label="b" type="f">gepresen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Marien oetmoet heeft hi <rhyme label="a" type="m" corresp="#s4:l1">bedacht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">dus wilt oetmoedich <rhyme label="b" type="f" corresp="#s4:l2">wesen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">In betlahem is hi <rhyme label="a" type="m">gebaeren</rhyme></l>
        <l id="s5:l2" met="4" type="-+">die Conynck bauen allen <rhyme label="b" type="f">heren</rhyme></l>
        <l id="s5:l3" met="4" type="-+">groet armoede heeft hi <rhyme label="a" type="m" corresp="#s5:l1">seer</rhyme></l>
        <l id="s5:l4" met="4" type="-+">wtuercaren ende ons gelaten ter <rhyme label="b" type="f" corresp="#s5:l2">leren</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Nv merket die stede synre <rhyme label="a" type="m">weerdicheit</rhyme></l>
        <l id="s6:l2" met="4" type="-+">een crybbe wast daer hi in <rhyme label="b" type="f">rusten</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Och bedden noch plumen waeren daer <rhyme label="a" type="m" corresp="#s6:l1">bereyt</rhyme></l>
        <l id="s6:l4" met="4" type="-+">dus schuwet mit hem <rhyme label="b" type="f" corresp="#s6:l2">wallusten</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Een os ende oec een <rhyme label="a" type="m">eselkyn</rhyme></l>
        <l id="s7:l2" met="4" type="-+">al byder crybben <rhyme label="b" type="f">stonden</rhyme></l>
        <l id="s7:l3" met="4" type="-+">sy verwarmden dat suete <rhyme label="a" type="m" corresp="#s7:l1">kyndekyn</rhyme></l>
        <l id="s7:l4" met="4" type="-+">daert lach in doekerkyn <rhyme label="b" type="f" corresp="#s7:l2">gewonden</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Drie Conyngen groet wt veren <rhyme label="a" type="m">lande</rhyme></l>
        <l id="s8:l2" met="4" type="-+">sy syn gecoemen ter <rhyme label="b" type="f">steden</rhyme></l>
        <l id="s8:l3" met="4" type="-+">sy brachten hem mynlic hoer <rhyme label="a" type="m" corresp="#s8:l1">offerhande</rhyme></l>
        <l id="s8:l4" met="4" type="-+">wilstu offeren doet dine <rhyme label="b" type="f" corresp="#s8:l2">gebede</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maria syn moeder ende tkynt <rhyme label="a" type="m">gepresen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">in lyden syn sy beyde <rhyme label="b" type="f">gestelt</rhyme></l>
        <l id="s9:l3" met="4" type="-+">laet ons mit hem in liden <rhyme label="a" type="m" corresp="#s9:l1">wesen</rhyme></l>
        <l id="s9:l4" met="4" type="-+">wy werden verloest van des viandes <rhyme label="b" type="f" corresp="#s9:l2">gewelt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
