<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>165890</id>
    <date>
      <year-start>1620</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag>new year song</tag>
    </tags>
    <melody>Naar oostland wil ik varen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Heer laet ons nijet <rhyme label="a" type="f">beroven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">al van u godlick <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s1:l3" met="3" type="-+">maer prysen dancken <rhyme label="a" type="f" corresp="#s1:l1">loven</rhyme></l>
        <l id="s1:l4" met="3" type="-+">u heerlick <rhyme label="b" type="m" corresp="#s1:l2">geboort</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Doen den wijsen met <rhyme label="c" type="f">vreden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">volgden die sterre <rhyme label="d" type="m">naer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Om Christum te <rhyme label="c" type="f" corresp="#s1:l5">aenbeden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In desen nieuwe <rhyme label="d" type="m" corresp="#s1:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vol blijschap ende <rhyme label="a" type="f">vreuchden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Den wijsen met <rhyme label="b" type="m">verstant</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Haer Inde gheest <rhyme label="a" type="f" corresp="#s2:l1">verheuchden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">met giften Inde <rhyme label="b" type="m" corresp="#s2:l2">hant</rhyme></l>
        <l id="s2:l5" met="3" type="-+">De harders opden <rhyme label="c" type="f">velde</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Om scheen gods licht zeer <rhyme label="d" type="m">claer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">t welck zy den volck <rhyme label="c" type="f" corresp="#s2:l5">vertelden</rhyme></l>
        <l id="s2:l8" met="3" type="-+">In desen nieuwe <rhyme label="d" type="m" corresp="#s2:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Herodes zynd <rhyme label="a" type="f">bedrogen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Socht Christum te <rhyme label="b" type="m">verdoen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Twelck nijet was In <rhyme label="a" type="f" corresp="#s3:l1">vermogen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tijrannich nae te <rhyme label="b" type="m" corresp="#s3:l2">spoen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">vervult zijn <rhyme label="c" type="f">prophesyen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Op den geberchte <rhyme label="d" type="m">swaer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">gehoort In raghels <rhyme label="c" type="f" corresp="#s3:l5">tijen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">met desen nieuwe <rhyme label="d" type="m" corresp="#s3:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Bloetdorstich heeft <rhyme label="a" type="f">geboden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">belast met luijder <rhyme label="b" type="m">stem</rhyme></l>
        <l id="s4:l3" met="3" type="-+">de kinderkens te <rhyme label="a" type="f" corresp="#s4:l1">dooden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">alsdoen In <rhyme label="b" type="m" corresp="#s4:l2">Betliem</rhyme></l>
        <l id="s4:l5" met="3" type="-+">de vlecken om <rhyme label="c" type="f">gelegen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Tonnoosel bloet <rhyme label="d" type="m">aldaer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">gedoot sonder <rhyme label="c" type="f" corresp="#s4:l5">bewegen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In desen nieuwe <rhyme label="d" type="m" corresp="#s4:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Den Prins zeer hooch <rhyme label="a" type="f">geseten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Is alle dinck <rhyme label="b" type="m">bekent</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Emanueel <rhyme label="a" type="f" corresp="#s5:l1">geheeten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">want lieffd es <rhyme label="b" type="m" corresp="#s5:l2">t'fondament</rhyme></l>
        <l id="s5:l5" met="3" type="-+">aenden cruijce <rhyme label="c" type="f">bewesen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">voor ons zonden <rhyme label="d" type="m">eenpaer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">van doot en hel <rhyme label="c" type="f" corresp="#s5:l5">genesen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In desen nieuwe <rhyme label="d" type="m" corresp="#s5:l6">Iaer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
