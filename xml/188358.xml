  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">A La fois dans cette <rhyme label="a" type="f">vie</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">L'on seroit <rhyme label="b" type="m">malheureux</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Si l'on craignoit <rhyme label="a" type="f" corresp="#s1:l1">l'envie</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Elle reigne en tout <rhyme label="b" type="m" corresp="#s1:l2">lieu</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s1:l5">rien</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s1:l5">train</rhyme>.</l>
        <l id="s1:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s1:l5">train</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Chacqu'un vous <rhyme label="a" type="f">examine</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Et fait votre <rhyme label="b" type="m">procès</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Par devant bonne <rhyme label="a" type="f" corresp="#s2:l1">mine</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Et par derniere un <rhyme label="b" type="m" corresp="#s2:l2">ses</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s2:l5">rien</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s2:l5">train</rhyme>.</l>
        <l id="s2:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s2:l5">train</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Avez vous des <rhyme label="a" type="f">richesses</rhyme>;</l>
        <l id="s3:l2" met="3" type="-+">Chacun vous fait la <rhyme label="b" type="m">cour</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Tout le monde <rhyme label="a" type="f" corresp="#s3:l1">s'empresse</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">A vous dire <rhyme label="b" type="m" corresp="#s3:l2">bonjour</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s3:l5">rien</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s3:l5">train</rhyme>.</l>
        <l id="s3:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s3:l5">train</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Tel vous voit dans la <rhyme label="a" type="f">rue</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Vous ôter son <rhyme label="b" type="m">chapeau</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Et lorsqu'il vous <rhyme label="a" type="f" corresp="#s4:l1">salue</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Vous souhaite au <rhyme label="b" type="m" corresp="#s4:l2">tombeau</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s4:l5">rien</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s4:l5">train</rhyme>.</l>
        <l id="s4:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s4:l5">train</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Un autre vous <rhyme label="a" type="f">proteste</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">D'être toujours <rhyme label="b" type="m">discrèt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Qui dans un temps <rhyme label="a" type="f" corresp="#s5:l1">funeste</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Publie vos secrets, <rhyme label="b" type="m" corresp="#s5:l2">&amp;c</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s5:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s5:l5">rien</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s5:l5">train</rhyme>.</l>
        <l id="s5:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s5:l5">train</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Un tel dit je vous <rhyme label="a" type="f">aime</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Et vous serre la <rhyme label="b" type="m">main</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Et s'il osoit <rhyme label="a" type="f" corresp="#s6:l1">lui-même</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Sera votre <rhyme label="b" type="m" corresp="#s6:l2">assassin</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s6:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s6:l5">rien</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s6:l5">train</rhyme>.</l>
        <l id="s6:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s6:l5">train</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">D'autres vous font des <rhyme label="a" type="f">offres</rhyme>:</l>
        <l id="s7:l2" met="3" type="-+">D'uger de leur <rhyme label="b" type="m">argent</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Faut-il ouvrir leur <rhyme label="a" type="f" corresp="#s7:l1">coffres</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Il n'y a rien <rhyme label="b" type="m" corresp="#s7:l2">dedans</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s7:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s7:l5">rien</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s7:l5">train</rhyme>.</l>
        <l id="s7:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s7:l5">train</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Si la fortune <rhyme label="a" type="f">tourne</rhyme>;</l>
        <l id="s8:l2" met="3" type="-+">La monde tourne <rhyme label="b" type="m">aussi</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">L'on ne verra <rhyme label="a" type="f" corresp="#s8:l1">personne</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Prendte votre <rhyme label="b" type="m" corresp="#s8:l2">parti</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s8:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s8:l5">rien</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s8:l5">train</rhyme>.</l>
        <l id="s8:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s8:l5">train</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Comment diable dont <rhyme label="a" type="f">faire</rhyme>;</l>
        <l id="s9:l2" met="3" type="-+">Dans ce monde <rhyme label="b" type="m">peril</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Puisqu'on en s'auroit <rhyme label="a" type="f" corresp="#s9:l1">plaire</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Qu'en marchand de <rhyme label="b" type="m" corresp="#s9:l2">travers</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s9:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s9:l5">rien</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s9:l5">train</rhyme>.</l>
        <l id="s9:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s9:l5">train</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Amis veut tu m'en <rhyme label="a" type="f">croire</rhyme>;</l>
        <l id="s10:l2" met="3" type="-+">Et vivre sans <rhyme label="b" type="m">chagrin</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Laissé la monde <rhyme label="a" type="f" corresp="#s10:l1">faire</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Et va toujours ton <rhyme label="b" type="m" corresp="#s10:l2">train</rhyme>.</l>
        <l id="s10:l5" met="2" type="-+">Je vais mon <rhyme label="c" type="m">train</rhyme>,</l>
        <l id="s10:l6" met="5" type="-+">Et sans me metre en peine de <rhyme label="c" type="m" corresp="#s10:l5">rien</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s10:l5">train</rhyme>.</l>
        <l id="s10:l8" met="2" type="-+">Je vais mon <rhyme label="c" type="m" corresp="#s10:l5">train</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
