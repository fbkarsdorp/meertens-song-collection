  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">¶ Als eene scone <rhyme label="a" type="m">dagheraet</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die voer de lichten dah <rhyme label="a" type="m" corresp="#s1:l1">opgaet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">sijt ghi maria <rhyme label="b" type="f">reyne</rhyme></l>
        <l id="s1:l4" met="4" type="-+">mijn onderstant ende al mijn <rhyme label="a" type="m" corresp="#s1:l1">raet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">mijn hoep mijn troest mijn <rhyme label="a" type="m" corresp="#s1:l1">toeuerlaet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">sijt ghi o vrou <rhyme label="b" type="f" corresp="#s1:l3">alleyne</rhyme></l>
        <l id="s1:l7" met="4" type="-+">ic roep aen v <rhyme label="c" type="m">ghenadicheit</rhyme></l>
        <l id="s1:l8" met="4" type="-+">want ghi hebt onse <rhyme label="c" type="m" corresp="#s1:l7">salicheit</rhyme></l>
        <l id="s1:l9" met="1" type="-+">
          <rhyme label="d" type="f">gheuonden</rhyme>
        </l>
        <l id="s1:l10" met="4" type="-+">den bant van ons <rhyme label="c" type="m" corresp="#s1:l7">misdadicheit</rhyme></l>
        <l id="s1:l11" met="4" type="+-">hebt ghi mit v <rhyme label="c" type="m" corresp="#s1:l7">oetmoedicheit</rhyme></l>
        <l id="s1:l12" met="1" type="-+">
          <rhyme label="d" type="f" corresp="#s1:l9">ontbonden</rhyme>
        </l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">¶ Een leli claer scoen ende <rhyme label="a" type="m">wit</rhyme></l>
        <l id="s2:l2" met="4" type="-+">so sijdi maghet <rhyme label="a" type="m" corresp="#s2:l1">onbesmit</rhyme></l>
        <l id="s2:l3" met="3" type="-+">van alle man <rhyme label="b" type="f">ghebleuen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">nye vrou en heeft onfangen <rhyme label="a" type="m" corresp="#s2:l1">dit</rhyme></l>
        <l id="s2:l5" met="4" type="-+">tis bouen der naturen <rhyme label="a" type="m" corresp="#s2:l1">wit</rhyme></l>
        <l id="s2:l6" met="3" type="-+">dit vordeel v <rhyme label="b" type="f" corresp="#s2:l3">ghegeuen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">een kijnt tonfaen ende maecht te <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s2:l8" met="4" type="-+">ende dat te baren sonder <rhyme label="c" type="m" corresp="#s2:l7">pijn</rhyme></l>
        <l id="s2:l9" met="1" type="-+">mit <rhyme label="d" type="f">vroechden</rhyme></l>
        <l id="s2:l10" met="4" type="-+">o suuer waerde <rhyme label="c" type="m" corresp="#s2:l7">maechdekijn</rhyme></l>
        <l id="s2:l11" met="4" type="+-">dit heeft betaemt v <rhyme label="c" type="m" corresp="#s2:l7">kijndekijn</rhyme></l>
        <l id="s2:l12" met="1" type="-+">sijn <rhyme label="d" type="f" corresp="#s2:l9">doechden</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">¶ Lustelic scone <rhyme label="a" type="m">paradijs</rhyme></l>
        <l id="s3:l2" met="4" type="-+">ghi sijt oec aarons bloyende <rhyme label="a" type="m" corresp="#s3:l1">rijs</rhyme></l>
        <l id="s3:l3" met="3" type="-+">moeder der scoonre <rhyme label="b" type="f">mynnen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">nye heeft geweest verstant so <rhyme label="a" type="m" corresp="#s3:l1">wijs</rhyme></l>
        <l id="s3:l5" met="4" type="-+">die v te recht mocht gheuen <rhyme label="a" type="m" corresp="#s3:l1">prijs</rhyme></l>
        <l id="s3:l6" met="3" type="-+">mit tongen of mit <rhyme label="b" type="f" corresp="#s3:l3">synnen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">sij bouen gaet der englen <rhyme label="c" type="m">lof</rhyme></l>
        <l id="s3:l8" met="4" type="-+">so hoech sij di in themels <rhyme label="c" type="m" corresp="#s3:l7">hof</rhyme></l>
        <l id="s3:l9" met="1" type="-+">
          <rhyme label="d" type="f">verheuen</rhyme>
        </l>
        <l id="s3:l10" met="4" type="-+">wat soud wi sonders doch hier <rhyme label="c" type="m" corresp="#s3:l7">of</rhyme></l>
        <l id="s3:l11" met="4" type="+-">die niet en sijn van ass ende <rhyme label="c" type="m" corresp="#s3:l7">stof</rhyme></l>
        <l id="s3:l12" met="1" type="-+">lof <rhyme label="d" type="f" corresp="#s3:l9">gheuen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">¶ Ten is gheweest nye dijns <rhyme label="a" type="m">gelijc</rhyme></l>
        <l id="s4:l2" met="4" type="-+">noch voer noch na op al <rhyme label="a" type="m" corresp="#s4:l1">aertrijc</rhyme></l>
        <l id="s4:l3" met="3" type="-+">een vrou van sulker <rhyme label="b" type="f">waerden</rhyme></l>
        <l id="s4:l4" met="4" type="-+">leli der dalen <rhyme label="a" type="m" corresp="#s4:l1">suuerlijc</rhyme></l>
        <l id="s4:l5" met="4" type="-+">een bloem des velts <rhyme label="a" type="m" corresp="#s4:l1">genoechelijc</rhyme></l>
        <l id="s4:l6" met="3" type="-+">sidi in themels <rhyme label="b" type="f" corresp="#s4:l3">gaerden</rhyme></l>
        <l id="s4:l7" met="4" type="-+">tbesloten hof sidi <rhyme label="c" type="m">alleyn</rhyme></l>
        <l id="s4:l8" met="4" type="-+">ende die gheteikende <rhyme label="c" type="m" corresp="#s4:l7">fonteyn</rhyme></l>
        <l id="s4:l9" met="1" type="-+">
          <rhyme label="d" type="f">vriendinne</rhyme>
        </l>
        <l id="s4:l10" met="4" type="-+">gods bruit gods duif gods suuer <rhyme label="c" type="m" corresp="#s4:l7">reyn</rhyme></l>
        <l id="s4:l11" met="4" type="+-">eens kijnts mit god den vader <rhyme label="c" type="m" corresp="#s4:l7">gemeijn</rhyme></l>
        <l id="s4:l12" met="1" type="-+">sijn <rhyme label="d" type="f" corresp="#s4:l9">minne</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">¶ Rose van ierico seer <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s5:l2" met="4" type="-+">salighe poort des hemels <rhyme label="a" type="m" corresp="#s5:l1">troon</rhyme></l>
        <l id="s5:l3" met="3" type="-+">moeder des hoochsten <rhyme label="b" type="f">heren</rhyme></l>
        <l id="s5:l4" met="4" type="-+">van twalif sterren ene <rhyme label="a" type="m" corresp="#s5:l1">croon</rhyme></l>
        <l id="s5:l5" met="4" type="-+">heeft v bereit v lieue <rhyme label="a" type="m" corresp="#s5:l1">soon</rhyme></l>
        <l id="s5:l6" met="3" type="-+">die ghi nv draecht mit <rhyme label="b" type="f" corresp="#s5:l3">eren</rhyme></l>
        <l id="s5:l7" met="4" type="-+">die sonne heeft v <rhyme label="c" type="m">ombeuaen</rhyme></l>
        <l id="s5:l8" met="4" type="-+">mit vwe voeten heb di die <rhyme label="c" type="m" corresp="#s5:l7">maen</rhyme></l>
        <l id="s5:l9" met="1" type="-+">
          <rhyme label="d" type="f">beschreden</rhyme>
        </l>
        <l id="s5:l10" met="4" type="-+">die englen sijn v <rhyme label="c" type="m" corresp="#s5:l7">onderdaen</rhyme></l>
        <l id="s5:l11" met="4" type="+-">des viants crachten heb di <rhyme label="c" type="m" corresp="#s5:l7">saen</rhyme></l>
        <l id="s5:l12" met="1" type="-+">
          <rhyme label="d" type="f" corresp="#s5:l9">vertreden</rhyme>
        </l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">¶ Voerwaer ghi sijt die <rhyme label="a" type="m">eglentier</rhyme></l>
        <l id="s6:l2" met="4" type="-+">den moyses barnen sach als <rhyme label="a" type="m" corresp="#s6:l1">vier</rhyme></l>
        <l id="s6:l3" met="3" type="-+">ende onghebrant <rhyme label="b" type="f">ghebleuen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">die arck van goude herde <rhyme label="a" type="m" corresp="#s6:l1">dier</rhyme></l>
        <l id="s6:l5" met="4" type="-+">is ons van v o maghet <rhyme label="a" type="m" corresp="#s6:l1">fier</rhyme></l>
        <l id="s6:l6" met="3" type="-+">voer een figur <rhyme label="b" type="f" corresp="#s6:l3">beschreuen</rhyme></l>
        <l id="s6:l7" met="4" type="-+">een ster des meers sidi <rhyme label="c" type="m">gheseit</rhyme></l>
        <l id="s6:l8" met="4" type="-+">een moeder der <rhyme label="c" type="m" corresp="#s6:l7">barmherticheit</rhyme></l>
        <l id="s6:l9" met="1" type="-+">o <rhyme label="d" type="f">reyne</rhyme></l>
        <l id="s6:l10" met="4" type="-+">ghi leit doer v <rhyme label="c" type="m" corresp="#s6:l7">ghenadicheit</rhyme></l>
        <l id="s6:l11" met="4" type="+-">den sondaers totter <rhyme label="c" type="m" corresp="#s6:l7">salicheit</rhyme></l>
        <l id="s6:l12" met="1" type="-+">
          <rhyme label="d" type="f" corresp="#s6:l9">ghemeyne</rhyme>
        </l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">¶ U werdicheit wel <rhyme label="a" type="m">toebehoort</rhyme></l>
        <l id="s7:l2" met="4" type="-+">datmen v lof sal setten <rhyme label="a" type="m" corresp="#s7:l1">voort</rhyme></l>
        <l id="s7:l3" met="3" type="-+">nv ende tot allen <rhyme label="b" type="f">vren</rhyme></l>
        <l id="s7:l4" met="4" type="-+">ezechiels besloten <rhyme label="a" type="m" corresp="#s7:l1">poort</rhyme></l>
        <l id="s7:l5" met="4" type="-+">waer doer dat ghinc des vaders <rhyme label="a" type="m" corresp="#s7:l1">woort</rhyme></l>
        <l id="s7:l6" met="3" type="-+">is een van v <rhyme label="b" type="f" corresp="#s7:l3">figuren</rhyme></l>
        <l id="s7:l7" met="4" type="-+">gedeons vlies betaemt v <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s7:l8" met="4" type="-+">daermen v bi gheliken <rhyme label="c" type="m" corresp="#s7:l7">sel</rhyme></l>
        <l id="s7:l9" met="1" type="-+">mit <rhyme label="d" type="f">waerden</rhyme></l>
        <l id="s7:l10" met="4" type="-+">al daer die hemels dau op <rhyme label="c" type="m" corresp="#s7:l7">vel</rhyme></l>
        <l id="s7:l11" met="4" type="+-">alleyn opt vlies ende nerghent <rhyme label="c" type="m" corresp="#s7:l7">el</rhyme></l>
        <l id="s7:l12" met="1" type="-+">op <rhyme label="d" type="f" corresp="#s7:l9">aerden</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">¶ Tis noch al cleyn dat is <rhyme label="a" type="m">gheseit</rhyme></l>
        <l id="s8:l2" met="4" type="-+">van vwer groter <rhyme label="a" type="m" corresp="#s8:l1">waerdicheit</rhyme></l>
        <l id="s8:l3" met="3" type="-+">hemelsche <rhyme label="b" type="f">keyserinne</rhyme></l>
        <l id="s8:l4" met="4" type="-+">want van v hoghe <rhyme label="a" type="m" corresp="#s8:l1">edelheit</rhyme></l>
        <l id="s8:l5" met="4" type="-+">sich seer verblijt die <rhyme label="a" type="m" corresp="#s8:l1">triniteit</rhyme></l>
        <l id="s8:l6" met="3" type="-+">ende al themels <rhyme label="b" type="f" corresp="#s8:l3">ghesinne</rhyme></l>
        <l id="s8:l7" met="4" type="-+">nv bidden wij <rhyme label="c" type="m">ootmoedelijc</rhyme></l>
        <l id="s8:l8" met="4" type="-+">dat wi mit v ons al <rhyme label="c" type="m" corresp="#s8:l7">ghelijc</rhyme></l>
        <l id="s8:l9" met="1" type="-+">
          <rhyme label="d" type="f">verbliden</rhyme>
        </l>
        <l id="s8:l10" met="4" type="-+">hier bouen in dat soete <rhyme label="c" type="m" corresp="#s8:l7">rijc</rhyme></l>
        <l id="s8:l11" met="4" type="+-">daer vroechd sal wesen <rhyme label="c" type="m" corresp="#s8:l7">ewelijc</rhyme></l>
        <l id="s8:l12" met="1" type="-+">en <rhyme label="d" type="f" corresp="#s8:l9">tiden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
