  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VUilt wt den slaep <rhyme label="a" type="f">ontspringhen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Elck die in sonden <rhyme label="b" type="m">leeft</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En wilt v vleesch <rhyme label="a" type="f" corresp="#s1:l1">bedwinghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Eer v den tijt <rhyme label="b" type="m" corresp="#s1:l2">begheeft</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Den dach die naect, daert al voor <rhyme label="b" type="m" corresp="#s1:l2">beeft</rhyme></l>
        <l id="s1:l6" met="1" type="-+">
          <rhyme label="c" type="f">Alomme</rhyme>
        </l>
        <l id="s1:l7" met="3" type="-+">Siet toe weest op v <rhyme label="d" type="f">hoede</rhyme></l>
        <l id="s1:l8" met="3" type="-+">De wapen des lichts <rhyme label="b" type="m" corresp="#s1:l2">aencleeft</rhyme></l>
        <l id="s1:l9" met="5" type="-+">Eer v den grooten dach met list <rhyme label="c" type="f" corresp="#s1:l6">aencomme</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den dach des Heeren <rhyme label="a" type="f">vierich</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sal comen <rhyme label="b" type="m">onbedacht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ghelijck eenen dief <rhyme label="a" type="f" corresp="#s2:l1">ghierich</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Is comende inder <rhyme label="b" type="m" corresp="#s2:l2">nacht</rhyme></l>
        <l id="s2:l5" met="4" type="-+">In welcke de hemelen sullen met <rhyme label="b" type="m" corresp="#s2:l2">cracht</rhyme></l>
        <l id="s2:l6" met="1" type="-+">
          <rhyme label="c" type="f">Bederuen</rhyme>
        </l>
        <l id="s2:l7" met="3" type="-+">De elementen <rhyme label="d" type="f">sullen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dan smelten seer <rhyme label="b" type="m" corresp="#s2:l2">onsacht</rhyme></l>
        <l id="s2:l9" met="5" type="-+">Door den anxt die elck dan sal <rhyme label="c" type="f" corresp="#s2:l6">verweruen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wanneer dien dach sal <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat Christus comen <rhyme label="b" type="m">sal</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sullen die stercke <rhyme label="a" type="f" corresp="#s3:l1">ruesen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dan schreyen ouer <rhyme label="b" type="m" corresp="#s3:l2">al</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Want desen dach wert een <rhyme label="b" type="m" corresp="#s3:l2">gheschal</rhyme></l>
        <l id="s3:l6" met="1" type="-+">Der <rhyme label="c" type="f">tranen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Eenen dach der <rhyme label="d" type="f">tempeeste</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En eenen duysteren <rhyme label="b" type="m" corresp="#s3:l2">val</rhyme></l>
        <l id="s3:l9" met="5" type="-+">So Iohels prophecien ons <rhyme label="c" type="f" corresp="#s3:l6">vermanen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De menschen sullen <rhyme label="a" type="f">weenen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Door vreese ionck en <rhyme label="b" type="m">oudt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Haer bloet sal door <rhyme label="a" type="f" corresp="#s4:l1">tvereenen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Uerstorten <rhyme label="b" type="m" corresp="#s4:l2">menichfout</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Haer vleesch sal zijn als dreck <rhyme label="b" type="m" corresp="#s4:l2">bedout</rhyme></l>
        <l id="s4:l6" met="1" type="-+">Uan <rhyme label="c" type="f">drucke</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Niet en sal haer <rhyme label="d" type="f">beschermen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Siluer goet nochte <rhyme label="b" type="m" corresp="#s4:l2">gout</rhyme></l>
        <l id="s4:l9" met="5" type="-+">In desen wreeden dach der <rhyme label="c" type="f" corresp="#s4:l6">onghelucke</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Al dees eerdtsche <rhyme label="a" type="f">landouwen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Moeten door tvier <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Sijnder ieloescher <rhyme label="a" type="f" corresp="#s5:l1">trouwen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Uerteert zijn <rhyme label="b" type="m" corresp="#s5:l2">openbaer</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Want hy salt al ten eynde <rhyme label="b" type="m" corresp="#s5:l2">claer</rhyme></l>
        <l id="s5:l6" met="1" type="-+">
          <rhyme label="c" type="f">Beschicken</rhyme>
        </l>
        <l id="s5:l7" met="3" type="-+">Met al die inden <rhyme label="d" type="f">lande</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Leuen hier ende <rhyme label="b" type="m" corresp="#s5:l2">daer</rhyme></l>
        <l id="s5:l9" met="5" type="-+">Door synen grammen moet, en swaer <rhyme label="c" type="f" corresp="#s5:l6">verschricken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nieu hemel en nieu <rhyme label="a" type="f">eerde</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Uerwachten wy <rhyme label="b" type="m">diuijn</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Die hy met grooter <rhyme label="a" type="f" corresp="#s6:l1">weerde</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ons sal aenbiende <rhyme label="b" type="m" corresp="#s6:l2">zijn</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Daeromme lieue broeders <rhyme label="b" type="m" corresp="#s6:l2">fijn</rhyme></l>
        <l id="s6:l6" met="1" type="-+">Wilt <rhyme label="c" type="f">waken</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Op dat ghy wert <rhyme label="d" type="f">beuonden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Al voor zijn wreet <rhyme label="b" type="m" corresp="#s6:l2">aenschijn</rhyme></l>
        <l id="s6:l9" met="5" type="-+">Een wtuercoren vadt in alle <rhyme label="c" type="f" corresp="#s6:l6">saken</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Keert v nv twert v <rhyme label="a" type="f">boetsel</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wt uwen sondighen <rhyme label="b" type="m">ganck</rhyme></l>
        <l id="s7:l3" met="3" type="-+">O ghy onsalich <rhyme label="a" type="f" corresp="#s7:l1">broetsel</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Eer v dat oordeel <rhyme label="b" type="m" corresp="#s7:l2">stranck</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Aencome met een listich <rhyme label="b" type="m" corresp="#s7:l2">beuanck</rhyme></l>
        <l id="s7:l6" met="1" type="-+">Des <rhyme label="c" type="f">Heeren</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Soect hem nv inder <rhyme label="d" type="f">eerden</rhyme></l>
        <l id="s7:l8" met="3" type="-+">De daghen ws leuens <rhyme label="b" type="m" corresp="#s7:l2">lanck</rhyme></l>
        <l id="s7:l9" met="5" type="-+">Op dat ghy moecht ontulien, alsulck <rhyme label="c" type="f" corresp="#s7:l6">onteert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
