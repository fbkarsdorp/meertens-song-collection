<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22710</id>
    <date>
      <year-start>1576</year-start>
      <year-end>1582</year-end>
    </date>
    <tags>
      <tag translation="">gezongen Onze Vader</tag>
    </tags>
    <melody>De lustelijke mei is nu in de tijd</melody>
    <categories/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Onse Vader in die Hemelen <rhyme label="a" type="m">claer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wy arme ellendighe op <rhyme label="b" type="f">Aerden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Roepen tot u wt der herten <rhyme label="a" type="m" corresp="#s1:l1">swaer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Neempt onse gheroep in <rhyme label="b" type="f" corresp="#s1:l2">Waerden</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Ghedenckt die liefde <rhyme label="c" type="m">bloot</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">U ghewaerighe goetheyt <rhyme label="c" type="m" corresp="#s1:l5">ghroot</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Die eewelijck sal duyren sonder <rhyme label="d" type="f">swichten</rhyme>:</l>
        <l id="s1:l8" met="4" type="-+">Laet u aensicht den nederen <rhyme label="d" type="f" corresp="#s1:l7">verlichten</rhyme>.</l>
        <l id="s1:l9" met="4" type="-+">Gheheylicht unde gheeert moet <rhyme label="e" type="m">sijn</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Alleen den Name <rhyme label="e" type="m" corresp="#s1:l9">dijn</rhyme>:</l>
        <l id="s1:l11" met="4" type="+-">Wy moeten altijt Sondaers <rhyme label="f" type="f">blyven</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">Laet den Gheest uwer minnen aen ons <rhyme label="f" type="f" corresp="#s1:l11">beclyven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">U Rijck toekomme ons alle <rhyme label="a" type="m">ghelijck</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">U Heylighen Gheest alleyn Ick <rhyme label="b" type="f">meyne</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">U wil gheschie als in des Hemels <rhyme label="a" type="m" corresp="#s2:l1">Rijck</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">In deser aerden <rhyme label="b" type="f" corresp="#s2:l2">pleyne</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Al en dunckt-es ons (Heer) niet <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Alleenich uwen wille <rhyme label="c" type="m" corresp="#s2:l5">doet</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">Die gheschiedt oock <rhyme label="d" type="f">onghelooghen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Maer t'is wonderlijck voor onsen <rhyme label="d" type="f" corresp="#s2:l7">ooghen</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">Gheheylicht unde gheeert moet <rhyme label="e" type="m">sijn</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Alleen den Name <rhyme label="e" type="m" corresp="#s2:l9">dijn</rhyme>:</l>
        <l id="s2:l11" met="4" type="+-">Wy moeten altijt Sondaers <rhyme label="f" type="f">blyven</rhyme>,</l>
        <l id="s2:l12" met="4" type="-+">Laet den Gheest uwer minnen aen ons <rhyme label="f" type="f" corresp="#s2:l11">beclyven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Heer gheeft ons huyden ons daghelijcks <rhyme label="a" type="m">broot</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Wilt ons voetsel der Zielen <rhyme label="b" type="f">gheven</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Gheeft ons u Gheest, u Woort inder <rhyme label="a" type="m" corresp="#s3:l1">noot</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat is ons spijs, vleysch, Gheest unde <rhyme label="b" type="f" corresp="#s3:l2">leven</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Dat bidden wy alle <rhyme label="c" type="m">ghelijck</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al maecken wy weynich <rhyme label="c" type="m" corresp="#s3:l5">gheblijck</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Soo is het doch ons <rhyme label="d" type="f">begheeren</rhyme>:</l>
        <l id="s3:l8" met="4" type="-+">Welck ghy O Godt moet <rhyme label="d" type="f" corresp="#s3:l7">vermeeren</rhyme>.</l>
        <l id="s3:l9" met="4" type="-+">Gheheylicht unde gheeert moet <rhyme label="e" type="m">sijn</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Alleen den Name <rhyme label="e" type="m" corresp="#s3:l9">dijn</rhyme>:</l>
        <l id="s3:l11" met="4" type="+-">Wy moeten altijt Sondaers <rhyme label="f" type="f">blyven</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">Laet den Gheest uwer minnen aen ons <rhyme label="f" type="f" corresp="#s3:l11">beclyven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vergheeft onse schult, sond' unde <rhyme label="a" type="m">misdaet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die wy oyt hebben <rhyme label="b" type="f">bedreven</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Soo wy doen, Och Heere kompt ons te <rhyme label="a" type="m" corresp="#s4:l1">baet</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die ons misdoen, willen wy <rhyme label="b" type="f" corresp="#s4:l2">vergheven</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Of wy verliesen den <rhyme label="c" type="m">strijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Daer ghy O Heere niet en <rhyme label="c" type="m" corresp="#s4:l5">sijt</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">U handt moet ons stadich <rhyme label="d" type="f">bewaren</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Wilt ons in die becoringhe <rhyme label="d" type="f" corresp="#s4:l7">sparen</rhyme>:</l>
        <l id="s4:l9" met="4" type="-+">Gheheylicht unde gheeert moet <rhyme label="e" type="m">sijn</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Alleen den Name <rhyme label="e" type="m" corresp="#s4:l9">dijn</rhyme>:</l>
        <l id="s4:l11" met="4" type="+-">Wy moeten altijt Sondaers <rhyme label="f" type="f">blyven</rhyme>,</l>
        <l id="s4:l12" met="4" type="-+">Laet den Gheest uwer minnen aen ons <rhyme label="f" type="f" corresp="#s4:l11">beclyven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Heere wilt ons van't vleysch <rhyme label="a" type="m">ontladen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Of blyven in der Werlt <rhyme label="b" type="f">versopen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Verlost ons doch vanden <rhyme label="a" type="m" corresp="#s5:l1">quaden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Wy ellendighe Kinderen tot u <rhyme label="b" type="f" corresp="#s5:l2">ropen</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Unde suchten inder herten <rhyme label="c" type="m">eenpaer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat wy u niet en volghen <rhyme label="c" type="m" corresp="#s5:l5">naer</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Mit die daden unde leven <rhyme label="d" type="f">reyne</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Vader neemt die wille, t'werck is <rhyme label="d" type="f" corresp="#s5:l7">kleyne</rhyme>:</l>
        <l id="s5:l9" met="4" type="-+">Gheheylicht unde gheeert moet <rhyme label="e" type="m">sijn</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Alleen den Name <rhyme label="e" type="m" corresp="#s5:l9">dijn</rhyme>:</l>
        <l id="s5:l11" met="4" type="+-">Wy moeten altijt Sondaers <rhyme label="f" type="f">blyven</rhyme>,</l>
        <l id="s5:l12" met="4" type="-+">Laet den Gheest uwer minnen aen ons <rhyme label="f" type="f" corresp="#s5:l11">beclyven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Prince, lof, eer unde <rhyme label="a" type="m">danckbaerheyt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die ons tot sijn Kinderen heeft <rhyme label="b" type="f">verkooren</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Mit Juchen te roepen sijn wy <rhyme label="a" type="m" corresp="#s6:l1">bereydt</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Want u is dat Rijck unde kraft int <rhyme label="b" type="f" corresp="#s6:l2">hooren</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Lof Heere der ons heeft <rhyme label="c" type="m">ghevrijt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Deur den Soon <rhyme label="c" type="m" corresp="#s6:l5">Ghebenedijt</rhyme>:</l>
        <l id="s6:l7" met="4" type="-+">Looft den Heer der Heerscharen mit <rhyme label="d" type="f">namen</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Looft hem Heerlijckin eewicheyt, <rhyme label="d" type="f" corresp="#s6:l7">Amen</rhyme>.</l>
        <l id="s6:l9" met="4" type="-+">Gheheylicht unde gheeert moet <rhyme label="e" type="m">sijn</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Alleen den Name <rhyme label="e" type="m" corresp="#s6:l9">dijn</rhyme>:</l>
        <l id="s6:l11" met="4" type="+-">Wy moeten altijt Sondaers <rhyme label="f" type="f">blyven</rhyme>,</l>
        <l id="s6:l12" met="4" type="-+">Laet den Gheest uwer minnen aen ons <rhyme label="f" type="f" corresp="#s6:l11">beclyven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
