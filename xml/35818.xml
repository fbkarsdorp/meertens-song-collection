  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">d' ENg'len in 's Hemels <rhyme label="a" type="f">Hoven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Een Godt belijden <rhyme label="b" type="m">sy</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die sy allegaer <rhyme label="a" type="f" corresp="#s1:l1">loven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Persoonen drie daer <rhyme label="b" type="m" corresp="#s1:l2">by</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Soo doen de <rhyme label="c" type="f">Cherubynen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hier toe zijn altijdt <rhyme label="d" type="m">ree</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Soo oock de <rhyme label="c" type="f" corresp="#s1:l5">Seraphynen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Vol liefde sonder <rhyme label="c" type="f" corresp="#s1:l5">schijnen</rhyme></l>
        <l id="s1:l9" met="3" type="-+">d' Andere Chooren <rhyme label="d" type="m" corresp="#s1:l6">mee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Seght ghy, ick sou oock <rhyme label="a" type="f">wenschen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met haer te loven <rhyme label="b" type="m">Godt</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Volbrenght dit alle <rhyme label="a" type="f" corresp="#s2:l1">Menschen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Want 't is u eygen <rhyme label="b" type="m" corresp="#s2:l2">lot</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Om Godt te <rhyme label="c" type="f">benedijden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">U, is verleent een <rhyme label="d" type="m">mont</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">'t Hart om Godt te <rhyme label="c" type="f" corresp="#s2:l5">belijden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wilt u hier door <rhyme label="c" type="f" corresp="#s2:l5">verblijden</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Voldoet dit al <rhyme label="d" type="m" corresp="#s2:l6">terstondt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een Engel heeft <rhyme label="a" type="f">gesonden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Godt, tot Iohan van <rhyme label="b" type="m">Matt</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Drievuldicheyt met <rhyme label="a" type="f" corresp="#s3:l1">monden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">'t Loven als d'waerste <rhyme label="b" type="m" corresp="#s3:l2">Schat</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Dese seer <rhyme label="c" type="f">uytverkooren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Goods wil en wensch <rhyme label="d" type="m">voldoet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met negen Eng'len <rhyme label="c" type="f" corresp="#s3:l5">Chooren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Laet yder dees lof <rhyme label="c" type="f" corresp="#s3:l5">hooren</rhyme>:</l>
        <l id="s3:l9" met="3" type="-+">Dees lof smaeckt veel seer <rhyme label="d" type="m" corresp="#s3:l6">soet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vrinden, wilt al <rhyme label="a" type="f">aenspannen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Met haer, en 's Hemels <rhyme label="b" type="m">Hof</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Ghy Maeght, Vrouw ende <rhyme label="a" type="f" corresp="#s4:l1">Mannen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In dees Heylige <rhyme label="b" type="m" corresp="#s4:l2">lof</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Laet schrijven uwe <rhyme label="c" type="f">namen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hierom in 't <rhyme label="d" type="m">Broederschap</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Ey! vrinden al te <rhyme label="c" type="f" corresp="#s4:l5">samen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En laet u dat niet <rhyme label="c" type="f" corresp="#s4:l5">schamen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Volgen sal groot <rhyme label="d" type="m" corresp="#s4:l6">blijdtschap</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Niemant, die kan <rhyme label="a" type="f">verhalen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Geen tongh is soo <rhyme label="b" type="m">bequaem</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Wat gracy op u <rhyme label="a" type="f" corresp="#s5:l1">dalen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Soo men vindt uwe <rhyme label="b" type="m" corresp="#s5:l2">naem</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">In 't Broederschap <rhyme label="c" type="f">geschreven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Soo ghy 't oock <rhyme label="d" type="m">onderhoudt</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Aflaet ghy krijght in 't <rhyme label="c" type="f" corresp="#s5:l5">leven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Daer na wort ghy <rhyme label="c" type="f" corresp="#s5:l5">verheven</rhyme>;</l>
        <l id="s5:l9" met="3" type="-+">Blincken sult meer al 't <rhyme label="d" type="m" corresp="#s5:l6">Gout</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wilt 's weecx een Kransken <rhyme label="a" type="f">vlechten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van negentigh <rhyme label="b" type="m">glory</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">'t Geloof tusschen tien in 't <rhyme label="a" type="f" corresp="#s6:l1">hechten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Voeght negenmael daer <rhyme label="b" type="m" corresp="#s6:l2">by</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">In 't maecken van des <rhyme label="c" type="f">wercken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Houdt ziel suyver en <rhyme label="d" type="m">net</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Op d'Engelen wilt <rhyme label="c" type="f" corresp="#s6:l5">aenmercken</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Wat sy in 's Hemels <rhyme label="c" type="f" corresp="#s6:l5">kercken</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Doen; spreeckt eerst dit <rhyme label="d" type="m" corresp="#s6:l6">gebedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Godt Vader voor u 'k <rhyme label="a" type="f">buyge</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Sterckt mijn met uwe <rhyme label="b" type="m">macht</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Goods Zoone, laet my <rhyme label="a" type="f" corresp="#s7:l1">suygen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">U wijsheyt dagh en <rhyme label="b" type="m" corresp="#s7:l2">nacht</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Godt heyl'ge geest wilt <rhyme label="c" type="f">toonen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Goetheyt, mijn sondigh <rhyme label="d" type="m">Mensch</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Om een Godt, drie <rhyme label="c" type="f" corresp="#s7:l5">Persoonen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Met d'Engels in 's Hemels <rhyme label="c" type="f" corresp="#s7:l5">throone</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Te loven na mijn <rhyme label="d" type="m" corresp="#s7:l6">wensch</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
