<song>
  <meta>
    <id>165702</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Al door de dalen kwam ik gegaan</melody>
  <author>Roelant de Kater</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Bruydegom ende Bruyd <rhyme label="a" type="m">eerbaar</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Bruydegom ende Bruyd <rhyme label="a" type="m" corresp="#s1:l1">eerbaar</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Ik wensch u veel geluk al met <rhyme label="a" type="m" corresp="#s1:l1">malkaar</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Ik wensch u veel geluk al met <rhyme label="a" type="m" corresp="#s1:l1">malkaar</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En dat gy mogt leven beyde te <rhyme label="a" type="m">gaar</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En dat gy mogt leven beyde te <rhyme label="a" type="m" corresp="#s2:l1">gaar</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">In gerustheyd en stilligheyd <rhyme label="a" type="m" corresp="#s2:l1">voorwaar</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">In gerustheyd en stilligheyd <rhyme label="a" type="m" corresp="#s2:l1">voorwaar</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ik wensch den Bruydegom nog eens <rhyme label="a" type="m">geluk</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ik wensch den Bruydegom nog eens <rhyme label="a" type="m" corresp="#s3:l1">geluk</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Want hy gescheyden is uyt <rhyme label="a" type="m" corresp="#s3:l1">liefdens-druk</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Want hy gescheyden is uyt <rhyme label="a" type="m" corresp="#s3:l1">liefdens-druk</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat heeft hy menige Nagten <rhyme label="a" type="m">daar</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Wat heeft hy menige Nagten <rhyme label="a" type="m" corresp="#s4:l1">daar</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Gezeten met dees zoete Bruyd <rhyme label="a" type="m" corresp="#s4:l1">eerbaar</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Gezeten met dees zoete Bruyd <rhyme label="a" type="m" corresp="#s4:l1">eerbaar</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die koude Wintersche Nagten <rhyme label="a" type="m">lang</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Die koude Wintersche Nagten <rhyme label="a" type="m" corresp="#s5:l1">lang</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Die vielen onzen Bruydegom zo <rhyme label="a" type="m" corresp="#s5:l1">strang</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Die vielen onzen Bruydegom zo <rhyme label="a" type="m" corresp="#s5:l1">strang</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maar nu zal hy voor zijn <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Maar nu zal hy voor zijn <rhyme label="a" type="m" corresp="#s6:l1">verdriet</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Met zijn Lief slapen in zijn armen, <rhyme label="a" type="m" corresp="#s6:l1">ziet</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Met zijn Lief slapen in zijn armen, <rhyme label="a" type="m" corresp="#s6:l1">ziet</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wat heeft ons lieve Bruyd <rhyme label="a" type="m">eerbaar</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Wat heeft ons lieve Bruyd <rhyme label="a" type="m" corresp="#s7:l1">eerbaar</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">Niet geleden om haar Bruydegom <rhyme label="a" type="m" corresp="#s7:l1">voorwaar</rhyme></l>
        <l id="s7:l4" met="5" type="-+">Niet geleden om haar Bruydegom <rhyme label="a" type="m" corresp="#s7:l1">voorwaar</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Menig Nagt geslapen maar een Uur, of <rhyme label="a" type="m">twee</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Menig Nagt geslapen maar een Uur, of <rhyme label="a" type="m" corresp="#s8:l1">twee</rhyme></l>
        <l id="s8:l3" met="5" type="-+">En leed met Dromen ook dikwils <rhyme label="a" type="m" corresp="#s8:l1">onvree</rhyme></l>
        <l id="s8:l4" met="5" type="-+">En leed met Dromen ook dikwils <rhyme label="a" type="m" corresp="#s8:l1">onvree</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Nog had zy dikwils zwaar <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Nog had zy dikwils zwaar <rhyme label="a" type="m" corresp="#s9:l1">verdriet</rhyme>,</l>
        <l id="s9:l3" met="5" type="-+">Wanneer haar Lief voer op de Zee met <rhyme label="a" type="m" corresp="#s9:l1">vliet</rhyme>,</l>
        <l id="s9:l4" met="5" type="-+">Wanneer haar Lief voer op de Zee met <rhyme label="a" type="m" corresp="#s9:l1">vliet</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Maar weest gerust onze lieve <rhyme label="a" type="m">Bruyd</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Maar weest gerust onze lieve <rhyme label="a" type="m" corresp="#s10:l1">Bruyd</rhyme>,</l>
        <l id="s10:l3" met="5" type="-+">Want deze droeve Dagen die zijn <rhyme label="a" type="m" corresp="#s10:l1">uyt</rhyme>,</l>
        <l id="s10:l4" met="5" type="-+">Want deze droeve Dagen die zijn <rhyme label="a" type="m" corresp="#s10:l1">uyt</rhyme>,</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Nu zult gy slapen blijd’ en <rhyme label="a" type="m">gerust</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Nu zult gy slapen blijd’ en <rhyme label="a" type="m" corresp="#s11:l1">gerust</rhyme>,</l>
        <l id="s11:l3" met="5" type="-+">In uw Liefs Armen als u t’zame <rhyme label="a" type="m" corresp="#s11:l1">lust</rhyme>,</l>
        <l id="s11:l4" met="5" type="-+">In uw Liefs Armen als u t’zame <rhyme label="a" type="m" corresp="#s11:l1">lust</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">‘k Wensch u veel Zegens toe, ô lieve <rhyme label="a" type="m">Paar</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">‘k Wensch u veel Zegens toe, ô lieve <rhyme label="a" type="m" corresp="#s12:l1">Paar</rhyme>,</l>
        <l id="s12:l3" met="5" type="-+">En vrugtbaarheyd in ‘t Egte-bed <rhyme label="a" type="m" corresp="#s12:l1">voorwaar</rhyme>,</l>
        <l id="s12:l4" met="5" type="-+">En vrugtbaarheyd in ‘t Egte-bed <rhyme label="a" type="m" corresp="#s12:l1">voorwaar</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">‘k Wensch dat men van u al binnen ‘t <rhyme label="a" type="m">Iaar</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">‘k Wensch dat men van u al binnen ‘t <rhyme label="a" type="m" corresp="#s13:l1">Iaar</rhyme>,</l>
        <l id="s13:l3" met="5" type="-+">Mag zien een Zoontje, of een Dogter <rhyme label="a" type="m" corresp="#s13:l1">klaar</rhyme>,</l>
        <l id="s13:l4" met="5" type="-+">Mag zien een Zoontje, of een Dogter <rhyme label="a" type="m" corresp="#s13:l1">klaar</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Dit wensch ik u Lieden <rhyme label="a" type="m">ydoon</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Dit wensch ik u Lieden <rhyme label="a" type="m" corresp="#s14:l1">ydoon</rhyme>,</l>
        <l id="s14:l3" met="5" type="-+">Tot datje ‘er twaalf ziet in eene <rhyme label="a" type="m" corresp="#s14:l1">kroon</rhyme>,</l>
        <l id="s14:l4" met="5" type="-+">Tot datje ‘er twaalf ziet in eene <rhyme label="a" type="m" corresp="#s14:l1">kroon</rhyme>,</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Speelnootjes ziet deze lieve <rhyme label="a" type="m">twee</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Speelnootjes ziet deze lieve <rhyme label="a" type="m" corresp="#s15:l1">twee</rhyme>,</l>
        <l id="s15:l3" met="5" type="-+">Wat isser vreugden en blijdschap <rhyme label="a" type="m" corresp="#s15:l1">alree</rhyme>,</l>
        <l id="s15:l4" met="5" type="-+">Wat isser vreugden en blijdschap <rhyme label="a" type="m" corresp="#s15:l1">alree</rhyme>,</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Denkt eens wat zal der al vreugde <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Denkt eens wat zal der al vreugde <rhyme label="a" type="m" corresp="#s16:l1">zijn</rhyme>,</l>
        <l id="s16:l3" met="5" type="-+">Als zy zijn beyd’ in haar Slaapkamer <rhyme label="a" type="m" corresp="#s16:l1">fijn</rhyme>,</l>
        <l id="s16:l4" met="5" type="-+">Als zy zijn beyd’ in haar Slaapkamer <rhyme label="a" type="m" corresp="#s16:l1">fijn</rhyme>,</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Denkt het u goed en zijt niet <rhyme label="a" type="m">slegt</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Denkt het u goed en zijt niet <rhyme label="a" type="m" corresp="#s17:l1">slegt</rhyme>,</l>
        <l id="s17:l3" met="5" type="-+">Maar wild ook aanvaarden met u liefden <rhyme label="a" type="m" corresp="#s17:l1">Egt</rhyme>,</l>
        <l id="s17:l4" met="5" type="-+">Maar wild ook aanvaarden met u liefden <rhyme label="a" type="m" corresp="#s17:l1">Egt</rhyme>,</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Verhoord hem dog die met <rhyme label="a" type="m">gebeen</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Verhoord hem dog die met <rhyme label="a" type="m" corresp="#s18:l1">gebeen</rhyme>,</l>
        <l id="s18:l3" met="5" type="-+">U zoekt tot Bruyd en Vrouw, en anders <rhyme label="a" type="m" corresp="#s18:l1">geen</rhyme></l>
        <l id="s18:l4" met="5" type="-+">U zoekt tot Bruyd en Vrouw, en anders <rhyme label="a" type="m" corresp="#s18:l1">geen</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Verblijd hem met een zoet <rhyme label="a" type="m">gelag</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Verblijd hem met een zoet <rhyme label="a" type="m" corresp="#s19:l1">gelag</rhyme>,</l>
        <l id="s19:l3" met="5" type="-+">Op dat hy haast zijn wensch verkrijgen <rhyme label="a" type="m" corresp="#s19:l1">mag</rhyme>,</l>
        <l id="s19:l4" met="5" type="-+">Op dat hy haast zijn wensch verkrijgen <rhyme label="a" type="m" corresp="#s19:l1">mag</rhyme>,</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Oorlof Gezelschap minst ende <rhyme label="a" type="m">meest</rhyme>,</l>
        <l id="s20:l2" met="4" type="-+">Oorlof Gezelschap minst ende <rhyme label="a" type="m" corresp="#s20:l1">meest</rhyme>,</l>
        <l id="s20:l3" met="5" type="-+">Weest dog, al t’zaam vrolijk op deze <rhyme label="a" type="m" corresp="#s20:l1">Feest</rhyme></l>
        <l id="s20:l4" met="5" type="-+">Weest dog, al t’zaam vrolijk op deze <rhyme label="a" type="m" corresp="#s20:l1">Feest</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
