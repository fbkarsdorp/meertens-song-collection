    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HElaes mijn <rhyme label="a" type="m">Goddin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die ick eeuwich <rhyme label="a" type="m" corresp="#s1:l1">bemin</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">VVat een <rhyme label="b" type="m">ly</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Brenght ghy <rhyme label="b" type="m" corresp="#s1:l3">my</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Inde <rhyme label="c" type="m">ziel</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">VVat een <rhyme label="d" type="m">smert</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Voelt mijn <rhyme label="d" type="m" corresp="#s1:l6">hert</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Als u vreedtheyt op my <rhyme label="c" type="m" corresp="#s1:l5">viel</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Ghy siet <rhyme label="e" type="f">beginnen</rhyme></l>
        <l id="s1:l10" met="2" type="-+">De liefd in <rhyme label="f" type="m">trouw</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Die u altyt soeckt te <rhyme label="e" type="f" corresp="#s1:l9">minnen</rhyme></l>
        <l id="s1:l12" met="3" type="+-">Tot een echte <rhyme label="f" type="m" corresp="#s1:l10">vrouw</rhyme>,</l>
        <l id="s1:l13" met="2" type="+-">Maer wat <rhyme label="f" type="m" corresp="#s1:l10">rouw</rhyme>!</l>
        <l id="s1:l14" met="2" type="-+">Ghy acht geen <rhyme label="g" type="f">clagen</rhyme></l>
        <l id="s1:l15" met="2" type="+-">Van myn <rhyme label="h" type="m">pijn</rhyme>,</l>
        <l id="s1:l16" met="4" type="+-">Die my laet mistroostich <rhyme label="h" type="m" corresp="#s1:l15">syn</rhyme>,</l>
        <l id="s1:l17" met="4" type="+-">Maer ghy sult my dese <rhyme label="g" type="f" corresp="#s1:l14">dagen</rhyme>,</l>
        <l id="s1:l18" met="3" type="+-">Naer het graf doen <rhyme label="g" type="f" corresp="#s1:l14">dragen</rhyme>,</l>
        <l id="s1:l19" met="3" type="-+">Doch u soet <rhyme label="h" type="m" corresp="#s1:l15">aenschijn</rhyme></l>
        <l id="s1:l20" met="4" type="+-">Can my haest wesen <rhyme label="h" type="m" corresp="#s1:l15">medecijn</rhyme>,</l>
        <l id="s1:l21" met="2" type="+-">Slaet u <rhyme label="i" type="f">ooghen</rhyme></l>
        <l id="s1:l22" met="2" type="+-">In t <rhyme label="i" type="f" corresp="#s1:l21">ghedoogen</rhyme>,</l>
        <l id="s1:l23" met="4" type="-+">Op die u vrijt Princers met <rhyme label="j" type="m">drift</rhyme>,</l>
        <l id="s1:l24" met="2" type="+-">Thoont hem <rhyme label="k" type="f">genaden</rhyme>,</l>
        <l id="s1:l25" met="2" type="+-">Soete <rhyme label="l" type="m">belt</rhyme>,</l>
        <l id="s1:l26" met="2" type="+-">t'Staet <rhyme label="l" type="m" corresp="#s1:l25">ghestelt</rhyme></l>
        <l id="s1:l27" met="2" type="-+">In u <rhyme label="j" type="m" corresp="#s1:l23">belieft</rhyme>,</l>
        <l id="s1:l28" met="2" type="-+">VVilt hem <rhyme label="k" type="f" corresp="#s1:l24">versaden</rhyme></l>
        <l id="s1:l29" met="1" type="-+">Den <rhyme label="m" type="m">lust</rhyme>,</l>
        <l id="s1:l30" met="2" type="-+">Die <rhyme label="m" type="m" corresp="#s1:l29">ongheblust</rhyme></l>
        <l id="s1:l31" met="2" type="-+">Tot uwe <rhyme label="m" type="m" corresp="#s1:l29">rust</rhyme></l>
        <l id="s1:l32" met="2" type="-+">Sal sijn <rhyme label="j" type="m" corresp="#s1:l23">begift</rhyme>,</l>
        <l id="s1:l33" met="2" type="-+">Compt hem ter <rhyme label="k" type="f" corresp="#s1:l24">staden</rhyme></l>
        <l id="s1:l34" met="1" type="-+">t'Is <rhyme label="n" type="m">noot</rhyme>,</l>
        <l id="s1:l35" met="2" type="-+">Eer dat de <rhyme label="n" type="m" corresp="#s1:l34">doot</rhyme></l>
        <l id="s1:l36" met="2" type="-+">Door bleeken <rhyme label="n" type="m" corresp="#s1:l34">stoot</rhyme></l>
        <l id="s1:l37" met="2" type="-+">Syn leven <rhyme label="j" type="m" corresp="#s1:l23">sift</rhyme>,</l>
        <l id="s1:l38" met="1" type="-+">Ach <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme></l>
        <l id="s1:l39" met="1" type="-+">
          <rhyme label="a" type="m" corresp="#s1:l1">Goddin</rhyme>
        </l>
        <l id="s1:l40" met="3" type="-+">Nempt uwen dienaer <rhyme label="o" type="m">af</rhyme>,</l>
        <l id="s1:l41" met="4" type="-+">De thoomen van syn swaere <rhyme label="o" type="m" corresp="#s1:l40">straf</rhyme>,</l>
        <l id="s1:l42" met="4" type="-+">Eer Lachesis hem rolt int <rhyme label="o" type="m" corresp="#s1:l40">graf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">V belt dach en <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Draeght my in myn <rhyme label="a" type="m" corresp="#s2:l1">gedacht</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Als my <rhyme label="b" type="m">docht</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Dat ick <rhyme label="b" type="m" corresp="#s2:l3">socht</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Mijn <rhyme label="c" type="m">Syringh</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Maer als <rhyme label="d" type="m">Pan</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Ben ick <rhyme label="d" type="m" corresp="#s2:l6">van</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Daer ick met het hert na <rhyme label="c" type="m" corresp="#s2:l5">spring</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">In al myn <rhyme label="e" type="f">kermen</rhyme></l>
        <l id="s2:l10" met="2" type="-+">k En gryp maer <rhyme label="f" type="m">riet</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">VVant als ick u meyn <rhyme label="e" type="f" corresp="#s2:l9">t'ontermen</rhyme>,</l>
        <l id="s2:l12" met="3" type="+-">Ick en vind' u <rhyme label="f" type="m" corresp="#s2:l10">niet</rhyme>,</l>
        <l id="s2:l13" met="2" type="+-">VVat <rhyme label="f" type="m" corresp="#s2:l10">verdriet</rhyme>!</l>
        <l id="s2:l14" met="2" type="-+">Hoe condy <rhyme label="g" type="f">vluchten</rhyme></l>
        <l id="s2:l15" met="2" type="+-">Van u <rhyme label="h" type="m">vrint</rhyme>?</l>
        <l id="s2:l16" met="4" type="+-">Die u uyter herten <rhyme label="h" type="m" corresp="#s2:l15">mint</rhyme>,</l>
        <l id="s2:l17" met="4" type="+-">VVaerom spot ghy met mijn <rhyme label="g" type="f" corresp="#s2:l14">suchten</rhyme>,</l>
        <l id="s2:l18" met="3" type="+-">Schept ghy dan <rhyme label="g" type="f" corresp="#s2:l14">genuchten</rhyme></l>
        <l id="s2:l19" met="3" type="-+">In mijn droeve <rhyme label="h" type="m" corresp="#s2:l15">quael</rhyme>,</l>
        <l id="s2:l20" met="4" type="+-">Goddinne geeft u dienaer <rhyme label="h" type="m" corresp="#s2:l15">tael</rhyme>;</l>
        <l id="s2:l21" met="2" type="+-">VVilt hem <rhyme label="i" type="f">helpen</rhyme></l>
        <l id="s2:l22" met="2" type="+-">Traenen <rhyme label="i" type="f" corresp="#s2:l21">stelpen</rhyme>,</l>
        <l id="s2:l23" met="4" type="-+">In u schep ick alleen <rhyme label="j" type="m">genucht</rhyme>:</l>
        <l id="s2:l24" met="2" type="+-">V <rhyme label="k" type="f">honinck-wesen</rhyme>,</l>
        <l id="s2:l25" met="2" type="+-">Soete <rhyme label="l" type="m">spraeck</rhyme></l>
        <l id="s2:l26" met="2" type="+-">Is <rhyme label="l" type="m" corresp="#s2:l25">t'vermaeck</rhyme></l>
        <l id="s2:l27" met="2" type="-+">Van mijn <rhyme label="j" type="m" corresp="#s2:l23">gesucht</rhyme>,</l>
        <l id="s2:l28" met="2" type="-+">Comt <rhyme label="k" type="f" corresp="#s2:l24">uytgelesen</rhyme></l>
        <l id="s2:l29" met="1" type="-+">Tot <rhyme label="m" type="m">my</rhyme>,</l>
        <l id="s2:l30" met="2" type="-+">En maeckt my <rhyme label="m" type="m" corresp="#s2:l29">bly</rhyme>,</l>
        <l id="s2:l31" met="2" type="-+">VVant ick u <rhyme label="m" type="m" corresp="#s2:l29">vry</rhyme>,</l>
        <l id="s2:l32" met="2" type="-+">Hoe wel ghy <rhyme label="j" type="m" corresp="#s2:l23">vlucht</rhyme>;</l>
        <l id="s2:l33" met="2" type="-+">VVilt my <rhyme label="k" type="f" corresp="#s2:l24">ghenesen</rhyme></l>
        <l id="s2:l34" met="1" type="-+">Dees <rhyme label="n" type="m">wont</rhyme>,</l>
        <l id="s2:l35" met="2" type="-+">Ach rooden <rhyme label="n" type="m" corresp="#s2:l34">mont</rhyme></l>
        <l id="s2:l36" met="2" type="-+">Maeckt my <rhyme label="n" type="m" corresp="#s2:l34">ghesont</rhyme></l>
        <l id="s2:l37" met="2" type="-+">Door soet <rhyme label="j" type="m" corresp="#s2:l23">gerucht</rhyme>,</l>
        <l id="s2:l38" met="1" type="-+">Op <rhyme label="a" type="m" corresp="#s2:l1">dat</rhyme></l>
        <l id="s2:l39" met="1" type="-+">Mijn <rhyme label="a" type="m" corresp="#s2:l1">schat</rhyme></l>
        <l id="s2:l40" met="3" type="-+">Ghy wesen moocht hier <rhyme label="o" type="m">mé</rhyme>,</l>
        <l id="s2:l41" met="4" type="-+">Dus hoop ick naer mijns liefdens <rhyme label="o" type="m" corresp="#s2:l40">bé</rhyme>,</l>
        <l id="s2:l42" met="4" type="-+">Met u te leven inden <rhyme label="o" type="m" corresp="#s2:l40">vré</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
