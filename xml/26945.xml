        <l id="s2:l4" met="4" type="-+">Spandtt jr jm hertzen der mynnenn <rhyme label="b" type="m" corresp="#s2:l2">kron</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Sei steitt jn mein <rhyme label="c" type="f">behagenn</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">Vff ir so r st mein <rhyme label="d" type="m">moitt</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Des will ich ir liebde <rhyme label="c" type="f" corresp="#s2:l5">dragenn</rhyme> /</l>
        <l id="s2:l8" met="4" type="-+">Vnnd schencken ihr meins hertzen <rhyme label="d" type="m" corresp="#s2:l6">bloitt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
