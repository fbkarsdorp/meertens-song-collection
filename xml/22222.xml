    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Godt swijcht niet, noch en zijt niet <rhyme label="a" type="m">stil</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En beydt niet lang, dats mijnen <rhyme label="a" type="m" corresp="#s1:l1">wil</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">U vyanden rasen, en <rhyme label="b" type="f">dullen</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">En die u haeten die steken <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Met valsche pracktijcken, haeren <rhyme label="c" type="m" corresp="#s1:l4">cop</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En teghen u volck sy <rhyme label="b" type="f" corresp="#s1:l3">brullen</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Sy bestormden gheren met liste <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Die onder u bescherminghe <rhyme label="d" type="m" corresp="#s1:l7">sijn</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">Maer ghy sult die heymlijck <rhyme label="e" type="f">beurijen</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Welaen segghen sy: herwaerts doch <rhyme label="f" type="m">comt</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Wy willense wtroeyen, tot op den <rhyme label="f" type="m" corresp="#s1:l10">grondt</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Op datse gheen volck meer en <rhyme label="e" type="f" corresp="#s1:l9">sijen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En dat niemandt en ghedencke <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Aen Israel, sy rotten hun <rhyme label="a" type="m" corresp="#s2:l1">seer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Eenen raet hebben sy <rhyme label="b" type="f">ghesloten</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Sy maken een verbondt teghen <rhyme label="c" type="m">u</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die Edomiter die rotten <rhyme label="c" type="m" corresp="#s2:l4">nu</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ismael comter toe met <rhyme label="b" type="f" corresp="#s2:l3">vloten</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Moabiter, <rhyme label="d" type="m">Hagarener</rhyme>, (?)</l>
        <l id="s2:l8" met="4" type="-+">Gebal, Ammon, <rhyme label="d" type="m" corresp="#s2:l7">Palestiner</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Amaleck, Titus, <rhyme label="e" type="f">Aser</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Hun heeft vergadert dese <rhyme label="f" type="m">rodt</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Sy zijn gelijck geworden den kinderen van <rhyme label="f" type="m" corresp="#s2:l10">Loth</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Straftse glijck Jabin aen <rhyme label="e" type="f" corresp="#s2:l9">dwater</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Verworchtse glijck de <rhyme label="a" type="m">Madianiter</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Glijck Sisaram, en <rhyme label="a" type="m" corresp="#s3:l1">Endorner</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">So wordense al tot <rhyme label="b" type="f">slijcke</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Hun Princen die maeckt ghelijck <rhyme label="c" type="m">Oreb</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Zalmana, en ghelijck <rhyme label="c" type="m" corresp="#s3:l4">Seeb</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En die daer segghen al <rhyme label="b" type="f" corresp="#s3:l3">ghlijcke</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Wy willen innemen Godes <rhyme label="d" type="m">huys</rhyme>?</l>
        <l id="s3:l8" met="4" type="-+">Maeckt die doch Heer alsoo <rhyme label="d" type="m" corresp="#s3:l7">confuys</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Glijck een omloopende <rhyme label="e" type="f">wile</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Glijck stroo dat vanden winde <rhyme label="f" type="m">breeckt</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Glijck tvier dat een groot Bosch <rhyme label="f" type="m" corresp="#s3:l10">aensteeckt</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Als vlammen die de berghen <rhyme label="e" type="f" corresp="#s3:l9">vernielen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wiltse alsoo veruolghen <rhyme label="a" type="m">fel</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Met u onweder, ras en <rhyme label="a" type="m" corresp="#s4:l1">snel</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Maeckt hun aenghesicht te <rhyme label="b" type="f">schande</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Op datz o Heer doch soecken <rhyme label="c" type="m">u</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Nae u vraghen, en hun schamen <rhyme label="c" type="m" corresp="#s4:l4">nu</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Verschricktse in alle <rhyme label="b" type="f" corresp="#s4:l3">landen</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Laetse doch worden <rhyme label="d" type="m">eewelijck</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Beschaemt, en vergaen alle <rhyme label="d" type="m" corresp="#s4:l7">ghelijck</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Op datse eens leeren <rhyme label="e" type="f">weten</rhyme>:</l>
        <l id="s4:l10" met="4" type="-+">Dat ghy alleen Heer zijt <rhyme label="f" type="m">ghewis</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">En dat uwen name alleen <rhyme label="f" type="m" corresp="#s4:l10">is</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">De hoochst ouer deertrijck <rhyme label="e" type="f" corresp="#s4:l9">gheseten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
