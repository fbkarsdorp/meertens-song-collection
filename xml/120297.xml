<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>120297</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>farewell song</tag>
    </tags>
    <melody>Welaan rechtschapen vaderlanders</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vaart wel myn Vaderland en <rhyme label="a" type="f">Vrinden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wy gaan nu na een ander <rhyme label="b" type="m">Oord</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Te zien om ons geluk te <rhyme label="a" type="f" corresp="#s1:l1">vinden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En wat ons daar ook meer <rhyme label="b" type="m" corresp="#s1:l2">bekoord</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">’t Is of wy gaan naar zwarte <rhyme label="c" type="f">Menschen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Maar mooglyk zyn zy in haar <rhyme label="d" type="m">hart</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Veel meer bevryd van alle <rhyme label="d" type="m" corresp="#s1:l6">smart</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Dat menig Europeesch zou <rhyme label="c" type="f" corresp="#s1:l5">wenschen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Adieu dan <rhyme label="e" type="m">Vaderland</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Wy steeken van u <rhyme label="e" type="m" corresp="#s1:l9">strand</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Adieu wy dienen met <rhyme label="f" type="m">plyzier</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">En vinden u weer <rhyme label="f" type="m" corresp="#s1:l11">hier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Tot uw beminnelyke <rhyme label="a" type="f">Sexe</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Zeg ik uw voor het laatst <rhyme label="b" type="m">vaarwel</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">By uw genoot ik meenig <rhyme label="a" type="f" corresp="#s2:l1">reisje</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Het lief en aangenaam <rhyme label="b" type="m" corresp="#s2:l2">minnespel</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Maar laat het uw tog niet <rhyme label="c" type="f">verdrieten</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dat wy thans van u scheiden <rhyme label="d" type="m">gaan</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Gy waard met ons tog niet <rhyme label="d" type="m" corresp="#s2:l6">voldaan</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Gy kunt van ons niet meer <rhyme label="c" type="f" corresp="#s2:l5">genieten</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Adieu dan <rhyme label="e" type="m">Vaderland</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Wy steeken van u <rhyme label="e" type="m" corresp="#s2:l9">strand</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Adieu wy dienen met <rhyme label="f" type="m">plyzier</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">En vinden u weer <rhyme label="f" type="m" corresp="#s2:l11">hier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nu zyn alle onze <rhyme label="a" type="f">wenschen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Om een spoedige <rhyme label="b" type="m">Overtogt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En dan door de woeste <rhyme label="a" type="f" corresp="#s3:l1">Baaren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Gezond te zyn aan land <rhyme label="b" type="m" corresp="#s3:l2">gebragt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Om daar met nut en vreugd te <rhyme label="c" type="f">dienen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Voor ons lieve <rhyme label="d" type="m">Vaderland</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Dat is en blyft ons dierbaar <rhyme label="d" type="m" corresp="#s3:l6">pand</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Daar wy geen rampen voor <rhyme label="c" type="f" corresp="#s3:l5">ontzienen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Adieu dan <rhyme label="e" type="m">Vaderland</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Wy steeken van u <rhyme label="e" type="m" corresp="#s3:l9">strand</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Adieu wy dienen met <rhyme label="f" type="m">plyzier</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">En vinden u weer <rhyme label="f" type="m" corresp="#s3:l11">hier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Nog weinig tyds door woeste <rhyme label="a" type="f">Baaren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Zien wy ons goed en <rhyme label="b" type="m">welgetroost</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Bevryd van alle die <rhyme label="a" type="f" corresp="#s4:l1">gevaaren</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Voordeelig in de West of <rhyme label="b" type="m" corresp="#s4:l2">Oost</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Daar weet geen mensch van meer te <rhyme label="c" type="f">treuren</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Daar is het vrolyk <rhyme label="d" type="m">vergenoegd</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">En ’t geen men hier in arbeid <rhyme label="d" type="m" corresp="#s4:l6">zwoegd</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Zien wy door vreugd daar opwaards <rhyme label="c" type="f" corresp="#s4:l5">beuren</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Adieu dan <rhyme label="e" type="m">Vaderland</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Wy steeken van u <rhyme label="e" type="m" corresp="#s4:l9">strand</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Adieu wy dienen met <rhyme label="f" type="m">plyzier</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">En vinden u weer <rhyme label="f" type="m" corresp="#s4:l11">hier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daar wagten ons de beste <rhyme label="a" type="f">Vrugten</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Met matigheid van elk <rhyme label="b" type="m">geplukt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Is daar geen onheil van te <rhyme label="a" type="f" corresp="#s5:l1">dugten</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dat heeft zo menig een <rhyme label="b" type="m" corresp="#s5:l2">gelukt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Daar wagten ons de Zwarte <rhyme label="c" type="f">Meisjes</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Zo poeslig als het blankste <rhyme label="d" type="m">vel</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Zy weeten daar van vreugd ook <rhyme label="d" type="m" corresp="#s5:l6">wel</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Kom doe met ons dit vrolyk <rhyme label="c" type="f" corresp="#s5:l5">reisje</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Adieu dan <rhyme label="e" type="m">Vaderland</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Wy steeken van u <rhyme label="e" type="m" corresp="#s5:l9">strand</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Adieu wy dienen met <rhyme label="f" type="m">plyzier</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">En vinden u weer <rhyme label="f" type="m" corresp="#s5:l11">hier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Uit Oost en West ziet gy ons <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Met blydschap in ons <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Na dat de tyd ons wat kan <rhyme label="a" type="f" corresp="#s6:l1">leeren</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dan bieden wy een elk de <rhyme label="b" type="m" corresp="#s6:l2">hand</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Dan wagten ons met open <rhyme label="c" type="f">armen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">De lieve Meisjes wel te <rhyme label="d" type="m">vree</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">De vreugde keerd met ons ook <rhyme label="d" type="m" corresp="#s6:l6">mee</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Hoe dat de Meisjes nu ook <rhyme label="c" type="f" corresp="#s6:l5">kermen</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Adieu dan <rhyme label="e" type="m">Vaderland</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Wy steeken van uw <rhyme label="e" type="m" corresp="#s6:l9">strand</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Wy dienen met <rhyme label="f" type="m">plyzier</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">En vinden u weer <rhyme label="f" type="m" corresp="#s6:l11">hier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
