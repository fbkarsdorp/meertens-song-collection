<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179969</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het was laatst op een morgen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik zag laatst in het <rhyme label="a" type="f">dalen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van 't koestrend <rhyme label="b" type="m">zonnelicht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een jonge Veldnimf <rhyme label="a" type="f" corresp="#s1:l1">dwalen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Des ik my vond <rhyme label="b" type="m" corresp="#s1:l2">verplicht</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Die schoonheid zoo vol <rhyme label="c" type="m">glans</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Eer de Avond 's Hemels <rhyme label="c" type="m" corresp="#s1:l5">trans</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Bezwalkt had bruin en <rhyme label="d" type="m">naar</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Te redden in 't <rhyme label="d" type="m" corresp="#s1:l7">gevaar</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zag zy was <rhyme label="a" type="f">verlegen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Toen ik haar bood myn <rhyme label="b" type="m">hulp</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Doch wierd wel haast <rhyme label="a" type="f" corresp="#s2:l1">genegen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met my naar myne <rhyme label="b" type="m" corresp="#s2:l2">stulp</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">In veiligheid te <rhyme label="c" type="m">gaan</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Ik bood myn dwaalster <rhyme label="c" type="m" corresp="#s2:l5">aan</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat zy tot de <rhyme label="d" type="m">ochtendzon</rhyme></l>
        <l id="s2:l8" met="3" type="-+">By my vertoeven <rhyme label="d" type="m" corresp="#s2:l7">kon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy nam daar in <rhyme label="a" type="f">genoegen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Want zy was <rhyme label="b" type="m">afgemat</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Des ik haar toe ging <rhyme label="a" type="f" corresp="#s3:l1">voegen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Het eêlste dat ik <rhyme label="b" type="m" corresp="#s3:l2">had</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">'k Verkwikte haar met <rhyme label="c" type="m">Room</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zoo goed als <rhyme label="c" type="m" corresp="#s3:l5">Melkertoom</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ooit van zyn bonte <rhyme label="d" type="m">Blaêr</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Schepte in het best' van 't <rhyme label="d" type="m" corresp="#s3:l7">jaar</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Zy scheen myn Room te <rhyme label="a" type="f">lusten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wyl zy zoo smaaklyk <rhyme label="b" type="m">at</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En eerder niet wouw <rhyme label="a" type="f" corresp="#s4:l1">rusten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Voor ik niet meerder <rhyme label="b" type="m" corresp="#s4:l2">had</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Myn Roomkruik was <rhyme label="c" type="m">geleegd</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En schoontjes <rhyme label="c" type="m" corresp="#s4:l5">uitgeveegd</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Doch, 't was my groot <rhyme label="d" type="m">vermaak</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dat zy zoo at met <rhyme label="d" type="m" corresp="#s4:l7">smaak</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'k Had voor haar in een <rhyme label="a" type="f">hoekje</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Noch een klein potje <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Waar van zy 't witte <rhyme label="a" type="f" corresp="#s5:l1">doekje</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Terstond heeft <rhyme label="b" type="m" corresp="#s5:l2">afgedaan</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Zy dronk het byna <rhyme label="c" type="m">leeg</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Zoo smaaklyk dat zy <rhyme label="c" type="m" corresp="#s5:l5">heeg</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">'t Was melk, die ik haar <rhyme label="d" type="m">gaf</rhyme>;</l>
        <l id="s5:l8" met="3" type="-+">Doch 't Roompje was 'er <rhyme label="d" type="m" corresp="#s5:l7">af</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wy brachten met ons <rhyme label="a" type="f">beiden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Vernoegt het nachtje <rhyme label="b" type="m">door</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Ik ging haar vroeg <rhyme label="a" type="f" corresp="#s6:l1">geleiden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Tot op het rechte <rhyme label="b" type="m" corresp="#s6:l2">spoor</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Toen ging zy van my <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En zei: 'k ben wel te <rhyme label="c" type="m" corresp="#s6:l5">vreên</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Met zulk een Room <rhyme label="d" type="m">onthaal</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Als ik eens weder <rhyme label="d" type="m" corresp="#s6:l7">dwaal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
