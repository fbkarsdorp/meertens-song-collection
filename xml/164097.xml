    <melody>Ik stort nu mijn klachten</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Wat stelt gy in 't <rhyme label="a" type="f">paren</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">O Jeugd, uwe <rhyme label="b" type="m">lust</rhyme>!</l>
        <l id="s1:l3" met="3" type="+-">Besteed uwe <rhyme label="a" type="f" corresp="#s1:l1">jaren</rhyme></l>
        <l id="s1:l4" met="3" type="+-">In meer vreugd en <rhyme label="b" type="m" corresp="#s1:l2">rust</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Geen vrolijker <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s1:l6" met="3" type="+-">(Volg vry dese <rhyme label="d" type="m">raad</rhyme>)</l>
        <l id="s1:l7" met="3" type="+-">Kan imand u <rhyme label="c" type="f" corresp="#s1:l5">geven</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Als de vrye <rhyme label="d" type="m" corresp="#s1:l6">staat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Geen blinkende <rhyme label="a" type="f">kronen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Geen rijkdom nog <rhyme label="b" type="m">goed</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Geen heerschende <rhyme label="a" type="f" corresp="#s2:l1">throonen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Geen agtbare <rhyme label="b" type="m" corresp="#s2:l2">soet</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Waar op het <rhyme label="c" type="f">begeeren</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Haar oogstralen <rhyme label="d" type="m">slaat</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Kan u meer <rhyme label="c" type="f" corresp="#s2:l5">vereeren</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Als de vrye <rhyme label="d" type="m" corresp="#s2:l6">staat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Geen blosende <rhyme label="a" type="f">kaken</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Nog ogen vol <rhyme label="b" type="m">gloed</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Hoe fel sy doen <rhyme label="a" type="f" corresp="#s3:l1">blaken</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Een verliefd <rhyme label="b" type="m" corresp="#s3:l2">gemoed</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Verrukken de <rhyme label="c" type="f">sinnen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Ooit in hooger <rhyme label="d" type="m">graad</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Niets waarder te <rhyme label="c" type="f" corresp="#s3:l5">minnen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Als de vrye <rhyme label="d" type="m" corresp="#s3:l6">staat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wat vreugd kan u <rhyme label="a" type="f">geven</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Met een slaafse <rhyme label="b" type="m">band</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Te zijn u gants <rhyme label="a" type="f" corresp="#s4:l1">leven</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Aan een vrou <rhyme label="b" type="m" corresp="#s4:l2">verpand</rhyme>!</l>
        <l id="s4:l5" met="3" type="+-">Slijt liever uwe <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Bevrijd van dat <rhyme label="d" type="m">kwaad</rhyme>:</l>
        <l id="s4:l7" met="3" type="+-">Laat niets u <rhyme label="c" type="f" corresp="#s4:l5">behagen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Als de vrye <rhyme label="d" type="m" corresp="#s4:l6">staat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Bevryd dan u <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Soo lang als gy <rhyme label="b" type="m">leefd</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Voor al de <rhyme label="a" type="f" corresp="#s5:l1">smerten</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Die de liefden <rhyme label="b" type="m" corresp="#s5:l2">geefd</rhyme>;</l>
        <l id="s5:l5" met="3" type="+-">Wild nimmer <rhyme label="c" type="f">gelooven</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Hoe schoon dat men <rhyme label="d" type="m">praat</rhyme>:</l>
        <l id="s5:l7" met="3" type="+-">Laat noit u <rhyme label="c" type="f" corresp="#s5:l5">berooven</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Van de vrye <rhyme label="d" type="m" corresp="#s5:l6">staat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
