<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186468</id>
    <date>
      <year-start>1793</year-start>
      <year-end>1793</year-end>
    </date>
    <tags/>
    <melody>Vrolijke uurtjes</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">FEest <rhyme label="a" type="f">Gezinden</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Braave <rhyme label="a" type="f" corresp="#s1:l1">Vrinden</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Juicht ter eer' van 't lieve <rhyme label="b" type="m">paar</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat elkander lang <rhyme label="a" type="f" corresp="#s1:l1">beminde</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Leef gelukkig, jaar op <rhyme label="b" type="m" corresp="#s1:l3">jaar</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Laat ons <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s2:l1">Feestelingen</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Oop'nen wy ons hart tot <rhyme label="b" type="m">vreugd</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hier moet ons geen smart <rhyme label="a" type="f" corresp="#s2:l1">omringen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Daar men 't oog houd op de <rhyme label="b" type="m" corresp="#s2:l3">deugd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Vol de <rhyme label="a" type="f">glazen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Laat de <rhyme label="a" type="f" corresp="#s3:l1">Dwaazen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">'t Raazen, 't schreeuwen 't schelden <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">'t Kan geen liefd'ryk hart <rhyme label="a" type="f" corresp="#s3:l1">verbaazen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Door de vreugd groeit <rhyme label="b" type="m" corresp="#s3:l3">Poëzy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Leef veel <rhyme label="a" type="f">jaaren</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Van <rhyme label="a" type="f" corresp="#s4:l1">gevaaren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Van de druk en 't leed <rhyme label="b" type="m">bevryd</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Lang moeit u de hemel <rhyme label="a" type="f" corresp="#s4:l1">spaaren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Leef gezegend, leef <rhyme label="b" type="m" corresp="#s4:l3">verblyd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Ouders, <rhyme label="a" type="f">Vrinden</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s5:l1">Deugdgezinden</rhyme>!</l>
        <l id="s5:l3" met="4" type="+-">Ziet veel blydschap aan deez' <rhyme label="b" type="m">twee</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Lang moogt ge allen <rhyme label="a" type="f" corresp="#s5:l1">ondervinden</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Welzyn, voorspoed, heil en <rhyme label="b" type="m" corresp="#s5:l3">vreê</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
