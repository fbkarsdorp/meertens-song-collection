  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">‘Onder 't lommer van dees <rhyme label="a" type="f">elsen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Kan ik u vernoegt <rhyme label="a" type="f" corresp="#s1:l1">omhelsen</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Dit jeugdig <rhyme label="b" type="m">groen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Zal U geen hinder <rhyme label="b" type="m" corresp="#s1:l3">doen</rhyme>.</l>
        <l id="s1:l5" met="1" type="-+"><rhyme label="c" type="m">Verviel</rhyme>,</l>
        <l id="s1:l6" met="1" type="-+">Mijn <rhyme label="c" type="m" corresp="#s1:l5">Ziel</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Door uw <rhyme label="d" type="f">bekoorlijkheeden</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Tot de <rhyme label="e" type="m">min</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-"><rhyme label="e" type="m" corresp="#s1:l8">Engelin</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Waarom my so lang <rhyme label="d" type="f" corresp="#s1:l7">bestreden</rhyme>.</l>
        <l id="s1:l11" met="2" type="+-">'t Waerdste   <rhyme label="f" type="m">soet</rhyme></l>
        <l id="s1:l12" met="2" type="+-">Van die <rhyme label="f" type="m" corresp="#s1:l11">gloed</rhyme></l>
        <l id="s1:l13" met="4" type="+-">Is dat gy uw lust <rhyme label="f" type="m" corresp="#s1:l11">voldoet</rhyme>.’</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">‘Onder 't lommer van dees <rhyme label="a" type="f">Elsen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Laat ik my so niet <rhyme label="a" type="f" corresp="#s2:l1">omhelsen</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">Dit jeugdig <rhyme label="b" type="m">groen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sou my self hinder <rhyme label="b" type="m" corresp="#s2:l3">doen</rhyme>.</l>
        <l id="s2:l5" met="1" type="-+">
          <rhyme label="c" type="m">Verviel</rhyme>
        </l>
        <l id="s2:l6" met="1" type="-+">Mijn <rhyme label="c" type="m" corresp="#s2:l5">Ziel</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Door uw <rhyme label="d" type="f">gedienstigheden</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Tot de <rhyme label="e" type="m">min</rhyme>?</l>
        <l id="s2:l9" met="2" type="+-">'k Weet mijn <rhyme label="e" type="m" corresp="#s2:l8">sin</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Och, te buygen na de <rhyme label="d" type="f" corresp="#s2:l7">reeden</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Waarom 't <rhyme label="f" type="m">soet</rhyme></l>
        <l id="s2:l12" met="2" type="+-">Van die <rhyme label="f" type="m" corresp="#s2:l11">gloed</rhyme></l>
        <l id="s2:l13" met="4" type="+-">Noyt so ligt uw lust <rhyme label="f" type="m" corresp="#s2:l11">voldoet</rhyme>.’</l>
      </lg>
    </lg>
  </text>
