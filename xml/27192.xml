  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">In mynen sin heb ick vercoren <rhyme label="a" type="m">saen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Een excelent / seer jent <rhyme label="b" type="m">bekent</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Blomken seer wel <rhyme label="a" type="m" corresp="#s1:l1">gedaen</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Op v goddin so gryp ick moet <rhyme label="a" type="m" corresp="#s1:l1">voertaen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Als ick my went / present / <rhyme label="b" type="m" corresp="#s1:l2">ontrent</rhyme></l>
        <l id="s1:l6" met="3" type="-+">V schoon vrouwelyck <rhyme label="a" type="m" corresp="#s1:l1">graen</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Om v so moet ick my <rhyme label="c" type="f">verblyden</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Oeck so doet ghy my vele <rhyme label="c" type="f" corresp="#s1:l7">lyden</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Ghy verheucht myn <rhyme label="d" type="m">hert</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Oock doet ghy my groote <rhyme label="d" type="m" corresp="#s1:l9">smert</rhyme></l>
        <l id="s1:l11" met="4" type="-+">Om haer / maeck ick vreuchde <rhyme label="e" type="m">voorwaer</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Want voor en naer / eenpaer / seer <rhyme label="e" type="m" corresp="#s1:l11">claer</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Syt ghy die liefste <rhyme label="e" type="m" corresp="#s1:l11">caer</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Seer swaer / oock met veel druckx <rhyme label="e" type="m" corresp="#s1:l11">gebaer</rhyme></l>
        <l id="s1:l15" met="4" type="-+">Treurt met veruaer / voorwaer / al t <rhyme label="e" type="m" corresp="#s1:l11">iaer</rhyme></l>
        <l id="s1:l16" met="3" type="-+">V getrouwe <rhyme label="e" type="m" corresp="#s1:l11">dienaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Aen dit schoon beelt heb ick geuonden <rhyme label="a" type="m">rust</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Dat druck verjaecht / en laecht <rhyme label="b" type="m">behaecht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Oock al myns herten <rhyme label="a" type="m" corresp="#s2:l1">lust</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Ghy die my stelt het herte met <rhyme label="a" type="m" corresp="#s2:l1">onrust</rhyme></l>
        <l id="s2:l5" met="4" type="-+">T welck droeuich waecht / versaecht / en <rhyme label="b" type="m" corresp="#s2:l2">claecht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dattet niet wert <rhyme label="a" type="m" corresp="#s2:l1">geblust</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Gelyck Veenus Paris <rhyme label="c" type="f">beloofde</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Helena die hy wt griecken <rhyme label="c" type="f" corresp="#s2:l7">roofde</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Hoop ick dat dit <rhyme label="d" type="m">juweel</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Sal syn myn beste <rhyme label="d" type="m" corresp="#s2:l9">deel</rhyme></l>
        <l id="s2:l11" met="4" type="-+">Seer reyn / myn alderliefste <rhyme label="e" type="m">greyn</rhyme></l>
        <l id="s2:l12" met="4" type="-+">Ghy syt certeyn / int pleyn / <rhyme label="e" type="m" corresp="#s2:l11">alleyn</rhyme></l>
        <l id="s2:l13" met="3" type="-+">Die schoonste <rhyme label="e" type="m" corresp="#s2:l11">rosemareyn</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Geweyn / en met suchten <rhyme label="e" type="m" corresp="#s2:l11">gemeyn</rhyme></l>
        <l id="s2:l15" met="4" type="-+">Heb ick doleyn / onreyn / niet <rhyme label="e" type="m" corresp="#s2:l11">cleyn</rhyme></l>
        <l id="s2:l16" met="3" type="-+">Geleden als <rhyme label="e" type="m" corresp="#s2:l11">villayn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">V wesen fier v amoreux <rhyme label="a" type="m">gesicht</rhyme></l>
        <l id="s3:l2" met="4" type="-+">V net persoon idoon seer <rhyme label="b" type="m">schoon</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Heeft myn jonckhert <rhyme label="a" type="m" corresp="#s3:l1">verlicht</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Door tsoet bestier der liefden brandt <rhyme label="a" type="m" corresp="#s3:l1">gewicht</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Spant sonder hoon die croon ten <rhyme label="b" type="m" corresp="#s3:l2">toon</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ghy schone venus <rhyme label="a" type="m" corresp="#s3:l1">nicht</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Schoonder noyt myn oghen <rhyme label="c" type="f">aensaeghen</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Dan die my na haer doet <rhyme label="c" type="f" corresp="#s3:l7">jaegen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Want v schoon <rhyme label="d" type="m">aenschyn</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Blinck als aurora <rhyme label="d" type="m" corresp="#s3:l9">fyn</rhyme></l>
        <l id="s3:l11" met="4" type="-+">Valyant goddinne <rhyme label="e" type="m">tryumphant</rhyme></l>
        <l id="s3:l12" met="4" type="-+">Ghy hebt geplant een brant <rhyme label="e" type="m" corresp="#s3:l11">playsant</rhyme></l>
        <l id="s3:l13" met="3" type="-+">In myn o <rhyme label="e" type="m" corresp="#s3:l11">dyamant</rhyme></l>
        <l id="s3:l14" met="3" type="-+">T'verstant my oeck vermindert <rhyme label="e" type="m" corresp="#s3:l11">want</rhyme></l>
        <l id="s3:l15" met="4" type="-+">Heel ruynant ter schant int <rhyme label="e" type="m" corresp="#s3:l11">sant</rhyme></l>
        <l id="s3:l16" met="3" type="-+">Daer om reyckt my v <rhyme label="e" type="m" corresp="#s3:l11">hant</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Syt my getrou princesse <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Want tcleyn gewin daer in v <rhyme label="b" type="m">sin</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Bedroeuen myn <rhyme label="a" type="m" corresp="#s4:l1">gemoet</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Suyuer kersou schone matresse <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Stelt toch v sin en min van <rhyme label="b" type="m" corresp="#s4:l2">bin</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Nen myn juechelyck <rhyme label="a" type="m" corresp="#s4:l1">bloet</rhyme></l>
        <l id="s4:l7" met="4" type="+-">O godt hadt gy tgenoecht <rhyme label="c" type="f">bequaeme</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Dat wy als bykens sworuen <rhyme label="c" type="f" corresp="#s4:l7">tsaemen</rhyme></l>
        <l id="s4:l9" met="3" type="-+">So waer myn pyn een <rhyme label="d" type="m">endt</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Daer ick om lyde <rhyme label="d" type="m" corresp="#s4:l9">torment</rhyme></l>
        <l id="s4:l11" met="4" type="-+">Verhoort o liefste met <rhyme label="e" type="m">accoort</rhyme></l>
        <l id="s4:l12" met="4" type="-+">Die leyt versmoort bekoort <rhyme label="e" type="m" corresp="#s4:l11">vermoort</rhyme></l>
        <l id="s4:l13" met="3" type="-+">In liefde <rhyme label="e" type="m" corresp="#s4:l11">rechteuoort</rhyme></l>
        <l id="s4:l14" met="3" type="-+">T' ja woort geeft hem toch van nu <rhyme label="e" type="m" corresp="#s4:l11">voort</rhyme></l>
        <l id="s4:l15" met="4" type="-+">Want hy orboort t behoort <rhyme label="e" type="m" corresp="#s4:l11">antwoort</rhyme></l>
        <l id="s4:l16" met="3" type="-+">T' ontfanghen met <rhyme label="e" type="m" corresp="#s4:l11">confoort</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
