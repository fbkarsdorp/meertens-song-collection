<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24547</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag>Easter song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Des mergens vroe by <rhyme label="a" type="f">tyden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">een luttel voer den <rhyme label="b" type="m">dagen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">doe quam Maria <rhyme label="c" type="f">magdaleena</rhyme></l>
        <l id="s1:l4" met="3" type="-+">tot onses heren <rhyme label="b" type="m" corresp="#s1:l2">graue</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Doe sy tot synen graue <rhyme label="a" type="f">quam</rhyme></l>
        <l id="s2:l2" met="3" type="-+">ende sy hem daer niet en <rhyme label="b" type="m">vant</rhyme></l>
        <l id="s2:l3" met="3" type="-+">den heren dien sy <rhyme label="c" type="f">sochte</rhyme></l>
        <l id="s2:l4" met="3" type="-+">sy weende oec alte <rhyme label="b" type="m" corresp="#s2:l2">hant</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ende doe sy mitten <rhyme label="a" type="f">rouwe</rhyme></l>
        <l id="s3:l2" met="3" type="-+">dus seer al was <rhyme label="b" type="m">beuaen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">daer vant sy enen <rhyme label="c" type="f">ackerman</rhyme></l>
        <l id="s3:l4" met="3" type="-+">in oeren wege <rhyme label="b" type="m" corresp="#s3:l2">staen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat ist dat gi <rhyme label="a" type="f">sucket</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Maria <rhyme label="b" type="m">magdalena</rhyme></l>
        <l id="s4:l3" met="3" type="-+">ic sueke mynen <rhyme label="c" type="f">heren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Jhesus van <rhyme label="b" type="m" corresp="#s4:l2">nasarenen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sy hoerdent aen synen <rhyme label="a" type="f">woerden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">ende oec aen synre <rhyme label="b" type="m">spraken</rhyme></l>
        <l id="s5:l3" met="3" type="-+">syndy dit gebenedide <rhyme label="c" type="f">troest</rhyme></l>
        <l id="s5:l4" met="3" type="-+">die my van sunden heeft <rhyme label="b" type="m" corresp="#s5:l2">verloest</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Jhesus mit uwen brunen <rhyme label="a" type="f">oghen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">gi steelt my myne <rhyme label="b" type="m">synnen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">ic wilt marien <rhyme label="c" type="f">clagen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">dat ick beroeuet <rhyme label="b" type="m" corresp="#s6:l2">byn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Claegdy dat mynre <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s7:l2" met="3" type="-+">dat wil ic aen v <rhyme label="b" type="m">wreken</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ic wyl v alsoe doen <rhyme label="c" type="f">mynnen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">v herte sal v te <rhyme label="b" type="m" corresp="#s7:l2">breken</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Brecty my myn <rhyme label="a" type="f">herte</rhyme></l>
        <l id="s8:l2" met="3" type="-+">soe nemet my myne <rhyme label="b" type="m">ziele</rhyme></l>
        <l id="s8:l3" met="3" type="-+">soe wil ick mit v <rhyme label="c" type="f">woenen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">soe en schieden my nye soe <rhyme label="b" type="m" corresp="#s8:l2">lieue</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Die wech is alsoe <rhyme label="a" type="f">suete</rhyme></l>
        <l id="s9:l2" met="3" type="-+">ten hemelryke <rhyme label="b" type="m">wart</rhyme></l>
        <l id="s9:l3" met="3" type="-+">van lelyen ende oec van <rhyme label="c" type="f">roesen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">my verlanget nader <rhyme label="b" type="m" corresp="#s9:l2">vaert</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Van lelyen ende oec van <rhyme label="a" type="f">roesen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">ende oec van <rhyme label="b" type="m">akeleyen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">ende daer  heer Jhesus <rhyme label="c" type="f">woenet</rhyme></l>
        <l id="s10:l4" met="3" type="-+">daer bloeyet al van <rhyme label="b" type="m" corresp="#s10:l2">beyden</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die wyn is <rhyme label="a" type="f">opgesteken</rhyme></l>
        <l id="s11:l2" met="3" type="-+">dat vat is <rhyme label="b" type="m">opgedaen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">daer lopen vyf <rhyme label="c" type="f">ryuieren</rhyme></l>
        <l id="s11:l4" met="3" type="-+">wy willen dryncken <rhyme label="b" type="m" corresp="#s11:l2">gaen</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Die wyn die daer wt <rhyme label="a" type="f">vloeyet</rhyme></l>
        <l id="s12:l2" met="3" type="-+">die is van sulker <rhyme label="b" type="m">aert</rhyme></l>
        <l id="s12:l3" met="3" type="-+">die doet die droefheit <rhyme label="c" type="f">wyken</rhyme></l>
        <l id="s12:l4" met="3" type="-+">die ons die vroude <rhyme label="b" type="m" corresp="#s12:l2">gaeft</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
