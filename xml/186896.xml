<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186896</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Ha ha zie zo</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ZA Mannen kruyt en <rhyme label="a" type="m">loot</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Schiet nu met schoot op <rhyme label="a" type="m" corresp="#s1:l1">schoot</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">De Trotsche Britten na haer <rhyme label="b" type="m">gat</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Daer men voor lang al regt tos <rhyme label="b" type="m" corresp="#s1:l3">had</rhyme>:</l>
        <l id="s1:l5" met="2" type="+-">Schiet toe, schiet <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wy zyn haer trosheid <rhyme label="c" type="m" corresp="#s1:l5">moe</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Herdenkt hoe dat wel <rhyme label="a" type="m">eer</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">De Ruyter keer op <rhyme label="a" type="m" corresp="#s2:l1">keer</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Hun groeten dorst uyt zyn <rhyme label="b" type="m">metael</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Volgt dien held in zegen <rhyme label="b" type="m" corresp="#s2:l3">prael</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Schiet toe, schiet <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wy zyn het nog niet <rhyme label="c" type="m" corresp="#s2:l5">moe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Waegt nu de vrye <rhyme label="a" type="m">kop</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Set al voor vryheid <rhyme label="a" type="m" corresp="#s3:l1">op</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Voor lieve vryheid die vol <rhyme label="b" type="m">moet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wierd gekogt door vryheids <rhyme label="b" type="m" corresp="#s3:l3">bloet</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Schiet toe, schiet <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wy zyn de Britten <rhyme label="c" type="m" corresp="#s3:l5">moe</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Neen trosaert nog een <rhyme label="a" type="m">keer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Schiet gy, wy schicte <rhyme label="a" type="m" corresp="#s4:l1">weer</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Leg aen geeft vuur dat door zyn <rhyme label="b" type="m">huyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Dat op de Chattams puynhoop <rhyme label="b" type="m" corresp="#s4:l3">stuyt</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Schiet toe, schiet <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wy zyn uw trosheid <rhyme label="c" type="m" corresp="#s4:l5">moe</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Triumpheert hy strykt de <rhyme label="a" type="m">vlag</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Soo wint men slag op <rhyme label="a" type="m" corresp="#s5:l1">slag</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Viva kourazie dit geeft <rhyme label="b" type="m">moet</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Daer men de Brit verzuypen <rhyme label="b" type="m" corresp="#s5:l3">doet</rhyme>;</l>
        <l id="s5:l5" met="2" type="+-">Schiet toe, schiet <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wy zyn het nog niet <rhyme label="c" type="m" corresp="#s5:l5">moe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
