<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>63057</id>
    <date>
      <year-start>1813</year-start>
      <year-end>1813</year-end>
    </date>
    <location>
      <orginal-name>amsterdam (achter de nieuwe kerk bij de dam)</orginal-name>
      <standard-name>Nieuwekerk, Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3738044</lon>
        <lat>4.8918198</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="vaderlands lied" translation="patriotic song">vaderlands lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>God save the king</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">God! zegen Neêrlands <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die 's Lands belangen <rhyme label="a" type="m" corresp="#s1:l1">torscht</rhyme>!</l>
        <l id="s1:l3" met="2" type="-+">God! zegen <rhyme label="b" type="m">hem</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">Dat hy ons lang <rhyme label="c" type="m">regeer'</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Den vyand van ons <rhyme label="c" type="m" corresp="#s1:l4">keer'</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En ons geluk <rhyme label="c" type="m" corresp="#s1:l4">vermeer'</rhyme>!</l>
        <l id="s1:l7" met="2" type="-+">God! zegen <rhyme label="b" type="m" corresp="#s1:l3">hem</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Op dien doorluchten <rhyme label="a" type="m">Stam</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die 't hoog bewind <rhyme label="a" type="m" corresp="#s2:l1">bekwam</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Stort zegen <rhyme label="b" type="m">uit</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Dat hy steeds groei' en <rhyme label="c" type="m">bloei'</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En ons door liefde <rhyme label="c" type="m" corresp="#s2:l4">boei'</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Dat ieders harte <rhyme label="c" type="m" corresp="#s2:l4">gloei'</rhyme></l>
        <l id="s2:l7" met="2" type="-+">En niets dit <rhyme label="b" type="m" corresp="#s2:l3">stuit'</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Euroop' was heel <rhyme label="a" type="m">verneêrd</rhyme>;</l>
        <l id="s3:l2" met="3" type="-+">Ons Vaderland <rhyme label="a" type="m" corresp="#s3:l1">verheerd</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Door loos <rhyme label="b" type="m">verraad</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Een trotsche <rhyme label="c" type="m">Dwingeland</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Verbrekende orde en <rhyme label="c" type="m" corresp="#s3:l4">band</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Heerschte over ons, ô <rhyme label="c" type="m" corresp="#s3:l4">schand'</rhyme>!</l>
        <l id="s3:l7" met="2" type="-+">Met wrev'len <rhyme label="b" type="m" corresp="#s3:l3">haat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Rus, de Brit, de <rhyme label="a" type="m">Zweed</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De Duitscher hief een' <rhyme label="a" type="m" corresp="#s4:l1">kreet</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Van felle <rhyme label="b" type="m">wraak</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">God en der volken <rhyme label="c" type="m">regt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Verwonnen in 't <rhyme label="c" type="m" corresp="#s4:l4">gevecht</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Thans is de twist <rhyme label="c" type="m" corresp="#s4:l4">beslecht</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Met felle <rhyme label="b" type="m" corresp="#s4:l3">wraak</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ook Neêrlands moed <rhyme label="a" type="m">herleeft</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En 't fransche rot reeds <rhyme label="a" type="m" corresp="#s5:l1">beeft</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Voor 't wrekend <rhyme label="b" type="m">staal</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">De vlaggen wapp'ren <rhyme label="c" type="m">weêr</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zoo lustig als <rhyme label="c" type="m" corresp="#s5:l4">weleer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Voor Neêrlands Vorst en <rhyme label="c" type="m" corresp="#s5:l4">Heer</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">In <rhyme label="b" type="m" corresp="#s5:l3">zegepraal</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Niets blyve in 's vyands <rhyme label="a" type="m">hand</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van 't lieve <rhyme label="a" type="m" corresp="#s6:l1">Vaderland</rhyme>.</l>
        <l id="s6:l3" met="2" type="-+">Op, op ten <rhyme label="b" type="m">stryd</rhyme>!</l>
        <l id="s6:l4" met="3" type="-+">God zelf treedt voor ons <rhyme label="c" type="m">uit</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Niets is er dat ons <rhyme label="c" type="m" corresp="#s6:l4">stuit</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Roep wyd en zyd en <rhyme label="c" type="m" corresp="#s6:l4">luid</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Op, op ten <rhyme label="b" type="m" corresp="#s6:l3">stryd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Verdryf des Dwinglands <rhyme label="a" type="m">magt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Beroof hem van zyn <rhyme label="a" type="m" corresp="#s7:l1">kracht</rhyme>.</l>
        <l id="s7:l3" met="2" type="-+">Ten stryd - op! <rhyme label="b" type="m">op</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">Verdelg hem van deze <rhyme label="c" type="m">aard</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Die 't bloed niet heeft <rhyme label="c" type="m" corresp="#s7:l4">gespaard</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ofschoon uw wraat <rhyme label="c" type="m" corresp="#s7:l4">onwaard</rhyme>.</l>
        <l id="s7:l7" met="2" type="-+">Verplet zyn' <rhyme label="b" type="m" corresp="#s7:l3">kop</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ontvang myn hulde en <rhyme label="a" type="m">groet</rhyme>;</l>
        <l id="s8:l2" met="3" type="-+">Uit grooten willems <rhyme label="a" type="m" corresp="#s8:l1">bloed</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Gy, die <rhyme label="b" type="m">ontsproot</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">U zy dit lied <rhyme label="c" type="m">gewyd</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">God zelf heeft U <rhyme label="c" type="m" corresp="#s8:l4">geleid</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">God zelf heeft ons <rhyme label="c" type="m" corresp="#s8:l4">bevryd</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Uit onzen <rhyme label="b" type="m" corresp="#s8:l3">nood</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dank, lof en prys <rhyme label="a" type="m">ontvang'</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Gy, die ons van den <rhyme label="a" type="m" corresp="#s9:l1">dwang</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Verloste, <rhyme label="b" type="m">Heer</rhyme>!</l>
        <l id="s9:l4" met="3" type="-+">Geef ons een dankbaar <rhyme label="c" type="m">hart</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">O Heer! verligt de <rhyme label="c" type="m" corresp="#s9:l4">smart</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die velen nog <rhyme label="c" type="m" corresp="#s9:l4">benart</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">O God! o <rhyme label="b" type="m" corresp="#s9:l3">Heer</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Behoed steeds <rhyme label="a" type="m">Nederland</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Voor een hernieuwde <rhyme label="a" type="m" corresp="#s10:l1">schand'</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Nooit bukk' het <rhyme label="b" type="m">weêr</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Gy zelf hebt ons <rhyme label="c" type="m">bevryd</rhyme></l>
        <l id="s10:l5" met="3" type="-+">En heel het land <rhyme label="c" type="m" corresp="#s10:l4">verblyd</rhyme>.</l>
        <l id="s10:l6" met="3" type="-+">Op, Neêrlands jeugd! ten <rhyme label="c" type="m" corresp="#s10:l4">stryd</rhyme>!</l>
        <l id="s10:l7" met="2" type="-+">Nooitbukk' men <rhyme label="b" type="m" corresp="#s10:l3">weêr</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
