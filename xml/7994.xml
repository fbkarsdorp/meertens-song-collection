    <melody>Psalm 008 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">O Onse God en Heer seer hoogh <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Hoe heerlijck moet doch uwen Name <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Over d'aertrijck strect u heerlijckheyt <rhyme label="b" type="m">schoon</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Ia wijder dan daer gaet des Hemels <rhyme label="b" type="m" corresp="#s1:l3">Throon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Men siet alsins uwer cracht veel <rhyme label="a" type="f">ghetuyghen</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Selfs in den mondt der kinderen die <rhyme label="a" type="f" corresp="#s2:l1">suyghen</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Daer door maeckt ghy tot niet ende <rhyme label="b" type="m">beschaemt</rhyme></l>
        <l id="s2:l4" met="5" type="-+">V vyanden, door u cracht seer <rhyme label="b" type="m" corresp="#s2:l3">vernaemt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Maer als ick wil aensien ende <rhyme label="a" type="f">bemercken</rhyme>.</l>
        <l id="s3:l2" met="5" type="-+">De Hemelen, Heer uwer handen <rhyme label="a" type="f" corresp="#s3:l1">wercken</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">De Sterren, de Mane, die ghy door 't <rhyme label="b" type="m">Woordt</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Maeckt ende stelt een yder op zijn <rhyme label="b" type="m" corresp="#s3:l3">oort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Als dan spreeck ick by my verwondert <rhyme label="a" type="f">seere</rhyme>:</l>
        <l id="s4:l2" met="5" type="-+">Wat ist doch van den armen Mensch, O <rhyme label="a" type="f" corresp="#s4:l1">Heere</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Dat ghy zijner alsoo ghedachtigh <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">End over hem sorghe draeght t'aller <rhyme label="b" type="m" corresp="#s4:l3">tijd</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Ghy maeckt hem dat hy God schier zy <rhyme label="a" type="f">gelijcke</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Want ghy maeckt hem overvloedigh en <rhyme label="a" type="f" corresp="#s5:l1">rijcke</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Van heerlijckheyt die doch naeckt is en <rhyme label="b" type="m">bloot</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Ghy maeckt hem vol met veel goederen <rhyme label="b" type="m" corresp="#s5:l3">groot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Ghy laet hem zijn over 'twerck uwer <rhyme label="a" type="f">handen</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Als een Heer der selven in allen <rhyme label="a" type="f" corresp="#s6:l1">landen</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Sonder uytnemen, alles in't <rhyme label="b" type="m">gemeen</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Hebt ghy hem onderdaen gemaeckt met <rhyme label="b" type="m" corresp="#s6:l3">een</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Ossen, Schapen, haer wolle, end' haer <rhyme label="a" type="f">vellen</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Die ghy op de Bergen voedt sonder <rhyme label="a" type="f" corresp="#s7:l1">quellen</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">En op dat velt weyden doet over <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">In bossen, berghen, en in menigh <rhyme label="b" type="m" corresp="#s7:l3">dal</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">De vliegende vogelen die wel <rhyme label="a" type="f">singhen</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">De visschen des Meyrs, en oock alle <rhyme label="a" type="f" corresp="#s8:l1">dinghen</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">Dien ghy haer wesen en den adem <rhyme label="b" type="m">gheeft</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">Maeckt ghy hem onderdaen, Ia al dat <rhyme label="b" type="m" corresp="#s8:l3">leeft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">O onse God en Heer seer hoogh <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s9:l2" met="5" type="-+">Ten rechten moet uwen naem heerlijck <rhyme label="a" type="f" corresp="#s9:l1">wesen</rhyme>,</l>
        <l id="s9:l3" met="5" type="-+">Vwes Naems heerlijckheyt in <rhyme label="b" type="m">overvloet</rhyme>,</l>
        <l id="s9:l4" met="5" type="-+">Streckt veel wijder dan den Aertbodem <rhyme label="b" type="m" corresp="#s9:l3">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
