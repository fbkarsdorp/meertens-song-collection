    <melody>Kom laat ons in het gemeen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een Droevich liet <rhyme label="a" type="m">serteyn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wil ick nu In dit <rhyme label="a" type="m" corresp="#s1:l1">pleyn</rhyme></l>
        <l id="s1:l3" met="4" type="-+">versinnen gaen wilt Dit <rhyme label="b" type="m">voorstaen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die mij te minnen <rhyme label="c" type="m">placht</rhyme></l>
        <l id="s1:l5" met="2" type="-+">nacht ende <rhyme label="c" type="m" corresp="#s1:l4">Dach</rhyme></l>
        <l id="s1:l6" met="3" type="-+">heeft mijn voorlaeten <rhyme label="b" type="m" corresp="#s1:l3">saen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoen woorden gaf sij <rhyme label="a" type="m">mij</rhyme></l>
        <l id="s2:l2" met="3" type="-+">geveinst van herten <rhyme label="a" type="m" corresp="#s2:l1">vrij</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Om mij Int net, dat <rhyme label="b" type="m">venes</rhyme></l>
        <l id="s2:l4" met="3" type="-+">te brengen met <rhyme label="c" type="m">bedroch</rhyme></l>
        <l id="s2:l5" met="2" type="-+">eylaesy <rhyme label="c" type="m" corresp="#s2:l4">doch</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ick merck wel wie het <rhyme label="b" type="m" corresp="#s2:l3">let</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wacht u van sulcken <rhyme label="a" type="m">daet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">gij die om minnen <rhyme label="a" type="m" corresp="#s3:l1">gaet</rhyme></l>
        <l id="s3:l3" met="4" type="-+">want snoeder sij sijn, dan quat <rhyme label="b" type="m">fenin</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Der slangen klein of <rhyme label="c" type="m">groet</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Dies In groeten <rhyme label="c" type="m" corresp="#s3:l4">noet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">nu Is dat herte <rhyme label="b" type="m" corresp="#s3:l3">mijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den rom die hij nu <rhyme label="a" type="m">draecht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">sal dickwils sijn <rhyme label="a" type="m" corresp="#s4:l1">beclaecht</rhyme></l>
        <l id="s4:l3" met="4" type="-+">beroemers quaet, Ick altijt <rhyme label="b" type="m">haet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">mer eylaesy daer om <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s4:l5" met="2" type="-+">en es <rhyme label="c" type="m" corresp="#s4:l4">geschiet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">van mij aen hem <rhyme label="b" type="m" corresp="#s4:l3">misdaet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">haerom gij voogel <rhyme label="a" type="m">licht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">vliet gij uit mij(n) <rhyme label="a" type="m" corresp="#s5:l1">gesicht</rhyme></l>
        <l id="s5:l3" met="4" type="-+">hebt gij gedacht dat In u <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">mijn liefie was <rhyme label="c" type="m">geheel</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Tis groet <rhyme label="c" type="m" corresp="#s5:l4">gescheel</rhyme></l>
        <l id="s5:l6" met="3" type="-+">naer eer Is dat Ick <rhyme label="b" type="m" corresp="#s5:l3">tracht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
