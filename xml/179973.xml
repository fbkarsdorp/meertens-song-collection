    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Cloris lief uw schone ogen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wyfje, 'k wouw je wel reis <rhyme label="a" type="f">zoenen</rhyme>;</l>
        <l id="s1:l2" met="4" type="+-">Maar je ziet me noch zoo <rhyme label="b" type="m">kwaad</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Of je my de huid wouwd <rhyme label="a" type="f" corresp="#s1:l1">boenen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Met de beezem die daar <rhyme label="b" type="m" corresp="#s1:l2">staat</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">'k Weet wel 'k heb het wat <rhyme label="c" type="m">verbrust</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Maar, myn Engel wees <rhyme label="c" type="m" corresp="#s1:l5">gerust</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">'k Zal nooit weer tot 's morgens <rhyme label="d" type="m">vroeg</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Blyven zuipen in de <rhyme label="d" type="m" corresp="#s1:l7">kroeg</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Geldverkwister, <rhyme label="a" type="f">hoerenjager</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Nachtrinkinker, dronke <rhyme label="b" type="m">beest</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Valschespeelder, <rhyme label="a" type="f" corresp="#s2:l1">wyvenplager</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Waar ben jy nu weÃªr <rhyme label="b" type="m" corresp="#s2:l2">geweest</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">'k Wouw wel luizebosch dat <rhyme label="c" type="m">jy</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Op de Buitenplaats aan 't <rhyme label="c" type="m" corresp="#s2:l5">Y</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Hoog en droog te kyken <rhyme label="d" type="m">zat</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Op een stoeltje zonder <rhyme label="d" type="m" corresp="#s2:l7">mat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vrouwtje lief, gy doet my <rhyme label="a" type="f">zuchten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Omdat jy me noch zoo <rhyme label="b" type="m">scheld</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Meid, wy hadden zulke <rhyme label="a" type="f" corresp="#s3:l1">kluchten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Dubbelwaard te zyn <rhyme label="b" type="m" corresp="#s3:l2">verteld</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">'k Wenschte tienmaal in myn <rhyme label="c" type="m">geest</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat jy by my waard <rhyme label="c" type="m" corresp="#s3:l5">geweest</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Want jy had je loof ik <rhyme label="d" type="m">schier</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Slap gelagchen om 't <rhyme label="d" type="m" corresp="#s3:l7">plaizier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Lichtmis, 'k vraag of jy zult <rhyme label="a" type="f">zwygen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'k Hoor geen nachtrinkinkers <rhyme label="b" type="m">praat</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'k Zal dien beezem aanstonds <rhyme label="a" type="f" corresp="#s4:l1">krygen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Zo je niet aan 't werken <rhyme label="b" type="m" corresp="#s4:l2">gaat</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Allerliefste lieve <rhyme label="c" type="m">wyf</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">'k Houw zoo veel noch van myn <rhyme label="c" type="m" corresp="#s4:l5">lyf</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Dat ik liever heen ga <rhyme label="d" type="m">kind</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Tot jy beter bent <rhyme label="d" type="m" corresp="#s4:l7">gezint</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
