<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191621</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hoor Kees mijn vrijer</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">MArritje <rhyme label="a" type="f">Joosten</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">'k Heb jou zo lang <rhyme label="b" type="m">gevreyt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Dog zonder <rhyme label="a" type="f" corresp="#s1:l1">troosten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Of eenig goed <rhyme label="b" type="m" corresp="#s1:l2">bescheyt</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Wel Meid wat scheelt 'er <rhyme label="c" type="m">aan</rhyme>?</l>
        <l id="s1:l6" met="3" type="-+">Of sta ik jou niet <rhyme label="c" type="m" corresp="#s1:l5">aan</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Zeg zoete lieve <rhyme label="d" type="f">bekje</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Waarom zie jy zo <rhyme label="e" type="m">bars</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Myn hart dat <rhyme label="d" type="f" corresp="#s1:l7">trekje</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">'k Ben ommers <rhyme label="a" type="f">netjes</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van 't hoofje tot myn <rhyme label="b" type="m">teen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Heel glad en <rhyme label="a" type="f" corresp="#s2:l1">vetjes</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van alles wel <rhyme label="b" type="m" corresp="#s2:l2">besneen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Kyk hoe myn haarje <rhyme label="c" type="m">krult</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Myn zuyk're Marry <rhyme label="c" type="m" corresp="#s2:l5">zult</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En kyk eens na myn <rhyme label="d" type="f">kuyten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ze drillen alle <rhyme label="e" type="m">bey</rhyme></l>
        <l id="s2:l9" met="2" type="-+">'t En zyn geen <rhyme label="d" type="f" corresp="#s2:l7">fluyten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Neen Lieve <rhyme label="a" type="f">Maatje</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat is het alles <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Je weet het <rhyme label="a" type="f" corresp="#s3:l1">gaatje</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Nog d'hiele inhoud <rhyme label="b" type="m" corresp="#s3:l2">niet</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">Maar ouwe <rhyme label="c" type="m">Peterneel</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die is het by me <rhyme label="c" type="m" corresp="#s3:l5">keel</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die ons dit komt te <rhyme label="d" type="f">brouwen</rhyme>;</l>
        <l id="s3:l8" met="3" type="-+">Myn zoete <rhyme label="e" type="m">Leendertje</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Dat wy niet <rhyme label="d" type="f" corresp="#s3:l7">Trouwen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wanneer ik <rhyme label="a" type="f">vrage</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Of zy het toe wil <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">'k Ken niet <rhyme label="a" type="f" corresp="#s4:l1">verdragen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Roept zy gestadig <rhyme label="b" type="m" corresp="#s4:l2">aan</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat zulke Snog're <rhyme label="c" type="m">Mayt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Als jy myn Marritje <rhyme label="c" type="m" corresp="#s4:l5">zyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">wil paren met zoo'n <rhyme label="d" type="f">Jorden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Als Leender Dirks <rhyme label="e" type="m">is</rhyme></l>
        <l id="s4:l9" met="2" type="-+">'k Zou gek haast <rhyme label="d" type="f" corresp="#s4:l7">worden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Wel zoete <rhyme label="a" type="f">Liefje</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Zo 't anders niet en <rhyme label="b" type="m">is</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Myn Harte <rhyme label="a" type="f" corresp="#s5:l1">Diefje</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wed ik wel oor <rhyme label="b" type="m" corresp="#s5:l2">gewis</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Als jy maar eerst zeyd <rhyme label="c" type="m">ja</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Dan zal ik 't ook wel <rhyme label="c" type="m" corresp="#s5:l5">dra</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">By Peetje zo gaan <rhyme label="d" type="f">klaren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat zy het ons toe <rhyme label="e" type="m">staat</rhyme></l>
        <l id="s5:l9" met="2" type="-+">En wy dan <rhyme label="d" type="f" corresp="#s5:l7">Paren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Als je 't kan <rhyme label="a" type="f">doenen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat zy het ons <rhyme label="b" type="m">toestaat</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Je zelt zes <rhyme label="a" type="f" corresp="#s6:l1">zoenen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dan hebben beste <rhyme label="b" type="m" corresp="#s6:l2">Maat</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Maar zo ze zeit van <rhyme label="c" type="m">neen</rhyme>:</l>
        <l id="s6:l6" met="3" type="-+">Zo bruy vry aanstonds <rhyme label="c" type="m" corresp="#s6:l5">heen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Of anders mogtje <rhyme label="d" type="f">vrezen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Zo jy quam op de <rhyme label="e" type="m">Werf</rhyme></l>
        <l id="s6:l9" met="2" type="-+">Ze zouje <rhyme label="d" type="f" corresp="#s6:l7">peezen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Draag maar geen <rhyme label="a" type="f">zorg</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Myn aller beste <rhyme label="b" type="m">Mayt</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Ik blyf jou <rhyme label="a" type="f" corresp="#s7:l1">borg</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Zo als ik heb <rhyme label="b" type="m" corresp="#s7:l2">gezayt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">'k Laat jou zo lang <rhyme label="c" type="m">allier</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En Stap zo dadelyk <rhyme label="c" type="m" corresp="#s7:l5">hier</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Myn lieve <rhyme label="d" type="f">Smodder-Smuiltje</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Geef my een mokkel <rhyme label="e" type="m">eerst</rhyme></l>
        <l id="s7:l9" met="2" type="-+">Myn zoete <rhyme label="d" type="f" corresp="#s7:l7">Zuiltje</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Gans bloet dat <rhyme label="a" type="f">smaaktme</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van zulke bolle <rhyme label="b" type="m">Mayt</rhyme>!</l>
        <l id="s8:l3" met="2" type="-+">Kedaar je <rhyme label="a" type="f" corresp="#s8:l1">raaktme</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Zo schielyk nog niet <rhyme label="b" type="m" corresp="#s8:l2">quyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">'k Moet hebben eerst een <rhyme label="c" type="m">Zoen</rhyme>?</l>
        <l id="s8:l6" met="3" type="-+">Nou Leen ik wil 't niet <rhyme label="c" type="m" corresp="#s8:l5">doen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Dag zoete Marritje <rhyme label="d" type="f">Joosten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Dag Leendert <rhyme label="e" type="m">Derkse</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Dag Troost der <rhyme label="d" type="f" corresp="#s8:l7">Troosten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
