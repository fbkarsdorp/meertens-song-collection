    <tags>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Malbrough s'en va-t-en guerre</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOort hier al in het <rhyme label="a" type="m">ronde</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoord hier al in het <rhyme label="a" type="m" corresp="#s1:l1">ronde</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wat ik zal zingen <rhyme label="c" type="m">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al van een zeer <rhyme label="a" type="m">ontrouwe</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Al van een zeer <rhyme label="a" type="m" corresp="#s2:l1">ontrouwe</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die zyn baas heeft <rhyme label="c" type="m">verraân</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoord toe hy is <rhyme label="a" type="m">gekoomen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hoord toe hy is <rhyme label="a" type="m" corresp="#s3:l1">gekoomen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al met een graauwe <rhyme label="c" type="m">Jas</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Met vyf-en-twintig <rhyme label="a" type="m">lappen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met vyf-en-twintig <rhyme label="a" type="m" corresp="#s4:l1">lappen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die hy had op zyn <rhyme label="c" type="m">bast</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hy had geen brood te <rhyme label="a" type="m">vreeten</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hy had geen brood te <rhyme label="a" type="m" corresp="#s5:l1">vreeten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">In zyn slegt <rhyme label="c" type="m">moffeland</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maar nu gaat hy <rhyme label="a" type="m">verkeeren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maar nu gaat hy <rhyme label="a" type="m" corresp="#s6:l1">verkeeren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En loopt nu met een <rhyme label="c" type="m">stok</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ja ook met groote <rhyme label="a" type="m">heeren</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ja ook met groote <rhyme label="a" type="m" corresp="#s7:l1">heeren</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">En met een swarte <rhyme label="c" type="m">rok</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Toen is hy hier <rhyme label="a" type="m">gekomen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Toen is hy hier <rhyme label="a" type="m" corresp="#s8:l1">gekomen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Bragt hy zyn dogter <rhyme label="c" type="m">meê</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En dat met ee <rhyme label="a" type="m">paarklompen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En dat met een paar <rhyme label="a" type="m" corresp="#s9:l1">klompen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En een boerinne <rhyme label="c" type="m">jak</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Maar nu gaat zy met <rhyme label="a" type="m">strikken</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Maar nu gaat zy met <rhyme label="a" type="m" corresp="#s10:l1">strikken</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En zoekt nu naar een <rhyme label="c" type="m">man</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die haar dan kan <rhyme label="a" type="m">borduuren</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Die haar dan kan <rhyme label="a" type="m" corresp="#s11:l1">borduuren</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Al op het <rhyme label="c" type="m">Venus-bed</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Dat lelyk mottig <rhyme label="a" type="m">meisje</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Dat lelyk mottig <rhyme label="a" type="m" corresp="#s12:l1">meisje</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Is op haar manier nog <rhyme label="c" type="m">net</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ik hou nu op met <rhyme label="a" type="m">digten</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontyne</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Ik hou nu op met <rhyme label="a" type="m" corresp="#s13:l1">digten</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Ze zyne niet meer <rhyme label="c" type="m">waard</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
