  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Schoen blomien uit <rhyme label="a" type="f">voerkoeren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick badt u eens te <rhyme label="a" type="f" corresp="#s1:l1">voeren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat gij sout willen <rhyme label="a" type="f" corresp="#s1:l1">hoeren</rhyme></l>
        <l id="s1:l4" met="3" type="+-">die u met herten <rhyme label="b" type="m">bemindt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dus en wilt mijn niet <rhyme label="a" type="f" corresp="#s1:l1">voersmaeden</rhyme></l>
        <l id="s1:l6" met="3" type="+-">voersint eer gij <rhyme label="b" type="m" corresp="#s1:l4">begint</rhyme></l>
        <l id="s1:l7" met="4" type="-+">genaedich sit gestaedich <rhyme label="c" type="m">lijt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Om u rein veenes <rhyme label="b" type="m" corresp="#s1:l4">kint</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mocht Ick eens bij u <rhyme label="a" type="f">weesen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">op een plaetse sonder <rhyme label="a" type="f" corresp="#s2:l1">vreesen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">soe waer mijn hert <rhyme label="a" type="f" corresp="#s2:l1">geneesen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Daer gij weesen sult voer <rhyme label="b" type="m">al</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En gij mij kuent <rhyme label="a" type="f" corresp="#s2:l1">geneesen</rhyme></l>
        <l id="s2:l6" met="3" type="+-">tan dit lijden en swaer <rhyme label="b" type="m" corresp="#s2:l4">misval</rhyme></l>
        <l id="s2:l7" met="4" type="-+">tsal sin sekreet sorcht voer geen <rhyme label="c" type="m">leet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Want daer niemant of weeten en <rhyme label="b" type="m" corresp="#s2:l4">sal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Prinses Ick sal u <rhyme label="a" type="f">trackteeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">den tijt die sal ons <rhyme label="a" type="f" corresp="#s3:l1">leeren</rhyme></l>
        <l id="s3:l3" met="3" type="-+">gij en sult geen beter <rhyme label="a" type="f" corresp="#s3:l1">begeeren</rhyme></l>
        <l id="s3:l4" met="3" type="+-">als wij bij den anderen <rhyme label="b" type="m">sin</rhyme></l>
        <l id="s3:l5" met="3" type="-+">u sal geschien veel <rhyme label="a" type="f" corresp="#s3:l1">eere</rhyme></l>
        <l id="s3:l6" met="3" type="+-">In duechde geloeft het <rhyme label="b" type="m" corresp="#s3:l4">vrij</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Dus haest u drae secht toch eens <rhyme label="c" type="m">Jae</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Oe alder liefste <rhyme label="b" type="m" corresp="#s3:l4">mijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
