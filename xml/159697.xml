  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Kon een minnaar door het <rhyme label="a" type="f">kussen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Sijne heete vlammen <rhyme label="a" type="f" corresp="#s1:l1">blussen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">'k Sag haast   mijn <rhyme label="b" type="m">wond</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Geheelt, o <rhyme label="b" type="m" corresp="#s1:l3">Rosemond</rhyme>.</l>
        <l id="s1:l5" met="1" type="-+">Maar <rhyme label="c" type="m">ag</rhyme>,</l>
        <l id="s1:l6" met="1" type="-+">Ik <rhyme label="c" type="m" corresp="#s1:l5">sag</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Daar door mijn brand nooyt <rhyme label="d" type="f">koelen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Maar veel <rhyme label="e" type="m">eer</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Dat ik <rhyme label="e" type="m" corresp="#s1:l8">meer</rhyme></l>
        <l id="s1:l10" met="4" type="-+">My kan fel ontsteken   <rhyme label="d" type="f" corresp="#s1:l7">voelen</rhyme></l>
        <l id="s1:l11" met="2" type="+-">En <rhyme label="f" type="m">ontrust</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Als vol <rhyme label="f" type="m" corresp="#s1:l11">lust</rhyme></l>
        <l id="s1:l13" met="4" type="+-">Ik uw kaakjes had <rhyme label="f" type="m" corresp="#s1:l11">gekust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Egter wensch ik <rhyme label="a" type="f">duysendmalen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Uw met kusjes te <rhyme label="a" type="f" corresp="#s2:l1">onthalen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Dewijl dat <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Mijn hert verheugen <rhyme label="b" type="m" corresp="#s2:l3">doet</rhyme>.</l>
        <l id="s2:l5" met="1" type="-+">Ik <rhyme label="c" type="m">blaak</rhyme></l>
        <l id="s2:l6" met="1" type="-+">En <rhyme label="c" type="m" corresp="#s2:l5">haak</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Na uwe malsche <rhyme label="d" type="f">lippen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Want als <rhyme label="e" type="m">gy</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Laat aan <rhyme label="e" type="m" corresp="#s2:l8">my</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Uw een soete kus <rhyme label="d" type="f" corresp="#s2:l7">ontglippen</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Vlied de <rhyme label="f" type="m">smert</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Van mijn <rhyme label="f" type="m" corresp="#s2:l11">hert</rhyme>,</l>
        <l id="s2:l13" met="4" type="+-">Schoon 't daar door ontsteken <rhyme label="f" type="m" corresp="#s2:l11">werd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gund my dan dat ik uw <rhyme label="a" type="f">kaken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Met mijn lippen mag <rhyme label="a" type="f" corresp="#s3:l1">genaken</rhyme></l>
        <l id="s3:l3" met="2" type="-+">En door die <rhyme label="b" type="m">smaak</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Myn eenig ziels <rhyme label="b" type="m" corresp="#s3:l3">vermaak</rhyme>,</l>
        <l id="s3:l5" met="1" type="-+">Myn <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s3:l6" met="1" type="-+">Van <rhyme label="c" type="m" corresp="#s3:l5">smert</rhyme></l>
        <l id="s3:l7" met="3" type="-+">En pijnen mag <rhyme label="d" type="f">bevryden</rhyme></l>
        <l id="s3:l8" met="2" type="+-">Die uw <rhyme label="e" type="m">min</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-"><rhyme label="e" type="m" corresp="#s3:l8">Engelin</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">My wel eer soo fel deed <rhyme label="d" type="f" corresp="#s3:l7">lijden</rhyme></l>
        <l id="s3:l11" met="2" type="+-">En my <rhyme label="f" type="m">streel</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Tot ik <rhyme label="f" type="m" corresp="#s3:l11">heel</rhyme></l>
        <l id="s3:l13" met="4" type="+-">Uw verkryge tot mijn <rhyme label="f" type="m" corresp="#s3:l11">deel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
