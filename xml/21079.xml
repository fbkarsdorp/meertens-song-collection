    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Phyllis kwam Philander tegen</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">FEest-genooten laet ons <rhyme label="a" type="f">loven</rhyme></l>
        <l id="s1:l2" met="4" type="+-">'t Heyligh <rhyme label="b" type="m">over-oud'-verbondt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat in d'alder-eelste <rhyme label="a" type="f" corresp="#s1:l1">hoven</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Tusschen d'eerste twee <rhyme label="b" type="m" corresp="#s1:l2">begond</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Die den Hemel, in dien <rhyme label="c" type="m">standt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Gelucksaligh t'samen <rhyme label="c" type="m" corresp="#s1:l5">bandt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wel geluckigh zijn de <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Die soo worden t'saem <rhyme label="b" type="m">gebrocht</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die door Geestelijcke <rhyme label="a" type="f" corresp="#s2:l1">banden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">d'Harten zijn in een <rhyme label="b" type="m" corresp="#s2:l2">verknocht</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Die geen noodt of ramp, hoe <rhyme label="c" type="m">swaer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Kunnen scheyden van <rhyme label="c" type="m" corresp="#s2:l5">elck-aer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Wensch dat hier soo zijn <rhyme label="a" type="f">vergadert</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Twee lichamen in den <rhyme label="b" type="m">Echt</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Maer dat heynder sijn <rhyme label="a" type="f" corresp="#s3:l1">genadert</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Twee gemoeden, die <rhyme label="b" type="m" corresp="#s3:l2">gehecht</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Maken door oprechte <rhyme label="c" type="m">Min</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Vyt twee harten eenen <rhyme label="c" type="m" corresp="#s3:l5">zin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Wensch de harten altijdt <rhyme label="a" type="f">blaken</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Door de reyne <rhyme label="b" type="m">Liefdes-bandt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Vol van 't opperste <rhyme label="a" type="f" corresp="#s4:l1">vermaken</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Van een sael'ge <rhyme label="b" type="m" corresp="#s4:l2">Echten-stant</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Dat ghy t'saem in dese <rhyme label="c" type="m">vreught</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Veele jaren tellen <rhyme label="c" type="m" corresp="#s4:l5">meught</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Wensch dat uwe Echt mach <rhyme label="a" type="f">teelen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Een soet lieffelijck <rhyme label="b" type="m">geslacht</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daer de Vader mee mach <rhyme label="a" type="f" corresp="#s5:l1">speelen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Daer de Moeder mede <rhyme label="b" type="m" corresp="#s5:l2">lacht</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Die als Olyf-spruytjes <rhyme label="c" type="m">frisch</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Rondom cieren uwen <rhyme label="c" type="m" corresp="#s5:l5">disch</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'k Wensch dat uwe deughden <rhyme label="a" type="f">bloeyen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat des Heeren heylsaem <rhyme label="b" type="m">woordt</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Meer en meer in u mach <rhyme label="a" type="f" corresp="#s6:l1">groeyen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En brengh rijcke vruchten <rhyme label="b" type="m" corresp="#s6:l2">voort</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Dat de toortze van u <rhyme label="c" type="m">plight</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Veel in duysternissen <rhyme label="c" type="m" corresp="#s6:l5">licht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dat gedurigh 's Heeren <rhyme label="a" type="f">vrede</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Woone onder u <rhyme label="b" type="m">gebouw</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dat ghy Bruygom voor mooght <rhyme label="a" type="f" corresp="#s7:l1">treden</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Tot een voorbeelt aen u <rhyme label="b" type="m" corresp="#s7:l2">Vrouw</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">En ghy Bruydt u Man en <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Mooght bewijsen liefd' en <rhyme label="c" type="m" corresp="#s7:l5">eer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">O! wel-gelucksaligh <rhyme label="a" type="f">paren</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">d'Hemel zy met uwe <rhyme label="b" type="m">trou</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En wil u gestaegh <rhyme label="a" type="f" corresp="#s8:l1">bewaren</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Voor verdriet, voor druck, voor <rhyme label="b" type="m" corresp="#s8:l2">rouw</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Wil u gunnen dese <rhyme label="c" type="m">bee</rhyme>:</l>
        <l id="s8:l6" met="4" type="+-">Feest-genoten wenscht dit <rhyme label="c" type="m" corresp="#s8:l5">mee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
