<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9191</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La Dauphine</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IOngh' Kees Neeltje Korsen <rhyme label="a" type="m">seun</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Die is verlieft op Tietjes <rhyme label="a" type="m" corresp="#s1:l1">Teun</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Te bijster // te <rhyme label="b" type="f">bijster</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Men hoorde noyt soo vreemde <rhyme label="a" type="m" corresp="#s1:l1">deun</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van Vryer noch van <rhyme label="b" type="f" corresp="#s1:l3">Vrijster</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hoort ientjes van dit moye <rhyme label="a" type="m">paer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ellick is goelicker as <rhyme label="a" type="m" corresp="#s2:l1">aer</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Deus Liefjes // deus <rhyme label="b" type="f">Liefjes</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vryde al over seven <rhyme label="a" type="m" corresp="#s2:l1">Jaer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Doe se woonde tot <rhyme label="b" type="f" corresp="#s2:l3">Jefjes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De liefd'en is noch niet <rhyme label="a" type="m">vergaen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Maer se begint op nieu weer <rhyme label="a" type="m" corresp="#s3:l1">aen</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Te brangde // te <rhyme label="b" type="f">brangde</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Soo sy my corts gaf te <rhyme label="a" type="m" corresp="#s3:l1">verstaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Tot haer oneer en <rhyme label="b" type="f" corresp="#s3:l3">schangde</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy sey het was tot Jefs <rhyme label="a" type="m">voorwint</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Jongh Kees had' mee toe diest <rhyme label="a" type="m" corresp="#s4:l1">emint</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">En Aertje // en <rhyme label="b" type="f">Aertje</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die maeckte mee int ierst met <rhyme label="a" type="m" corresp="#s4:l1">kint</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En Kees die worden't <rhyme label="b" type="f" corresp="#s4:l3">Vaertje</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daynckt iensjes om de slechte <rhyme label="a" type="m">Boer</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Hy brochtet strick strack by sen <rhyme label="a" type="m" corresp="#s5:l1">moer</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">O moertje // o <rhyme label="b" type="f">moertje</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Kijck iensjes sayd' hy wat ick <rhyme label="a" type="m" corresp="#s5:l1">voer</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ien Seuntje 'tis gien <rhyme label="b" type="f" corresp="#s5:l3">Boertje</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Neel say ick weet niet by Sint <rhyme label="a" type="m">Jan</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Mee dunckt dit is al staegh weer <rhyme label="a" type="m" corresp="#s6:l1">an</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Jou guytje // jou <rhyme label="b" type="f">guytje</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Je meucht sien datier raeckt hier <rhyme label="a" type="m" corresp="#s6:l1">van</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En van jou nieuwe <rhyme label="b" type="f" corresp="#s6:l3">Bruytje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy het het in Waterlant <rhyme label="a" type="m">besteedt</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Ick weet niet hoe hy soo veul <rhyme label="a" type="m" corresp="#s7:l1">weet</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Het bloetje // het <rhyme label="b" type="f">bloetje</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">'tKijnt weer te halen hy <rhyme label="a" type="m" corresp="#s7:l1">vergheet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Nou vrijt Teun hem om <rhyme label="b" type="f" corresp="#s7:l3">'tgoetje</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ick hebje soo lief sayde <rhyme label="a" type="m">hy</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Teun aers noch aers ghelooft mee <rhyme label="a" type="m" corresp="#s8:l1">vry</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">As 't Kuycke // as <rhyme label="b" type="f">'tKuycke</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Sen jongties doet, soo doe ick <rhyme label="a" type="m" corresp="#s8:l1">dy</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Mocht ickje maer <rhyme label="b" type="f" corresp="#s8:l3">ghebruycke</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wel Prince secht is dit gien <rhyme label="a" type="m">liefd'</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Se vryen meekaer hiel <rhyme label="a" type="m" corresp="#s9:l1">andiefd</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">'k Wet Teuntje // 'k wet <rhyme label="b" type="f">Teuntje</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Met ien nachje ien aer <rhyme label="a" type="m" corresp="#s9:l1">gheriefd'</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Wel soo krijght Kees weer 't <rhyme label="b" type="f" corresp="#s9:l3">Seuntje</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
