    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Schoon lief gij zijt prijs waard alleen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">BOven al zijt ghy mijn lief <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hier in dit s'Weerelts <rhyme label="b" type="m">pleyn</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Mijn smert cont ghy alleen <rhyme label="a" type="f" corresp="#s1:l1">ghenesen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghy zijt mijn liefste <rhyme label="b" type="m" corresp="#s1:l2">greyn</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Mijn liefste lief <rhyme label="b" type="m" corresp="#s1:l2">alleyn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Wilt v van niet <rhyme label="c" type="f">weeren</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Na v lief staet al mijn <rhyme label="c" type="f" corresp="#s1:l6">begeeren</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Want ick het v moet clagen <rhyme label="b" type="m" corresp="#s1:l2">certeyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt my O lief nu niet <rhyme label="a" type="f">begeven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Al ben ick <rhyme label="b" type="m">desolaet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ick ben ten eynde van mijn <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ist dat ghy my <rhyme label="b" type="m" corresp="#s2:l2">ontgaet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Mocht ick seer <rhyme label="b" type="m" corresp="#s2:l2">delicaet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Int houwelijck met v <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Zo sou vreuchde in my zijn <rhyme label="c" type="f" corresp="#s2:l6">gheresen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">ghy zijt schoon lief daer al mijn troost <rhyme label="b" type="m" corresp="#s2:l2">opstaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Seer net staen lief uwe <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ghy zijt seer wel <rhyme label="b" type="m">gheraeckt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wel gemaniert seer jent van <rhyme label="a" type="f" corresp="#s3:l1">seden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer toe seer wel <rhyme label="b" type="m" corresp="#s3:l2">bespraeckt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Mijn vreucht is heel <rhyme label="b" type="m" corresp="#s3:l2">gestaeckt</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Om v mijn lief <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ghy brengt my nu geheel in <rhyme label="c" type="f" corresp="#s3:l6">sneven</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Hoe sou mijn vreuchde dan ooc zijn <rhyme label="b" type="m" corresp="#s3:l2">volmaect</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Princes ghy zijt in my <rhyme label="a" type="f">gheseten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Boven Helena <rhyme label="b" type="m">playsant</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ghy hebt mijn hart also <rhyme label="a" type="f" corresp="#s4:l1">doorreten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Het is Godt wel <rhyme label="b" type="m" corresp="#s4:l2">becant</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy zijt seer <rhyme label="b" type="m" corresp="#s4:l2">abondant</rhyme> //</l>
        <l id="s4:l6" met="3" type="-+">wilt my doch niet <rhyme label="c" type="f">verdrijven</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Laet mijn liefde in v <rhyme label="c" type="f" corresp="#s4:l6">verstijven</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Helpt my schoon lief in desen <rhyme label="b" type="m" corresp="#s4:l2">stant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
