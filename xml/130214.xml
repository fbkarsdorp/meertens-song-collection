  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vlied, snelle winden, haest   <rhyme label="a" type="m">voorheen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Na <rhyme label="a" type="m" corresp="#s1:l1">Carileen</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Bruyst, schommelt zoetjes, bracke <rhyme label="b" type="m">stroom</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Met <rhyme label="c" type="f">zuydervlaagjes</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Dan hoog, dan <rhyme label="c" type="f" corresp="#s1:l4">laagies</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Tuygt dat ik <rhyme label="b" type="m" corresp="#s1:l3">koom</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O soete golfjes van de <rhyme label="a" type="m">Meer</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Spoelt op en <rhyme label="a" type="m" corresp="#s2:l1">neer</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">Y, blauwe Tye, drijft u <rhyme label="b" type="m">vloet</rhyme></l>
        <l id="s2:l4" met="2" type="-+">En d'Eyke <rhyme label="c" type="f">Boomen</rhyme></l>
        <l id="s2:l5" met="2" type="-+">En snelle <rhyme label="c" type="f" corresp="#s2:l4">stroomen</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Eens ruisen <rhyme label="b" type="m" corresp="#s2:l3">doet</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Spoelt zagjes tegen het zwacke <rhyme label="a" type="m">riet</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Tot dat sy <rhyme label="a" type="m" corresp="#s3:l1">siet</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Mijn uitgespannen zeiltjes <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Door gunst der <rhyme label="c" type="f">stroomen</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Voorwind <rhyme label="c" type="f" corresp="#s3:l4">afkomen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Met Min <rhyme label="b" type="m" corresp="#s3:l3">belaen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wanneer ik kom daar het oog <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Sal sien na <rhyme label="a" type="m" corresp="#s4:l1">mijn</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Soo stiert mijn tong, O God <rhyme label="b" type="m">Jupyn</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Dat sy met <rhyme label="c" type="f">reeden</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Mag <rhyme label="c" type="f" corresp="#s4:l4">aangebeeden</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Werden van <rhyme label="b" type="m" corresp="#s4:l3">myn</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Eerbiedig val ik haer te <rhyme label="a" type="m">voet</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Met heusse <rhyme label="a" type="m" corresp="#s5:l1">groet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">En klagten van mijn sware <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Opdat, met <rhyme label="c" type="f">lusjes</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Drie soete <rhyme label="c" type="f" corresp="#s5:l4">kusjes</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Mijn welkom <rhyme label="b" type="m" corresp="#s5:l3">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
