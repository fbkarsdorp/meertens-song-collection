    <melody>Wee mij ik mag wel klagen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Aedieu oe stat van <rhyme label="a" type="f">Leyden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Vermaert breet ende <rhyme label="b" type="m">wyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">aedieu Ick moet nu <rhyme label="a" type="f" corresp="#s1:l1">scheiden</rhyme></l>
        <l id="s1:l4" met="4" type="+-">voertrecken op dees <rhyme label="b" type="m" corresp="#s1:l2">tyt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">quit soe syn Ick nu eenpaer, <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">myn vruecht ende <rhyme label="b" type="m" corresp="#s1:l2">Ioelyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Aedieu te speelen <rhyme label="a" type="f">ryden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">aedieu veel vruechde <rhyme label="b" type="m">goet</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">aedieu myn te gaen <rhyme label="a" type="f" corresp="#s2:l1">vlyen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">by haer die myn moet, <rhyme label="b" type="m" corresp="#s2:l2">doet</rhyme></l>
        <l id="s2:l5" met="4" type="+-">branden geheel seer fel, <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s2:l6" met="3" type="-+">al Inder minnen <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Aedieu te <rhyme label="a" type="f">leyden.buiten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">u myn lief <rhyme label="b" type="m">tryonfant</rhyme></l>
        <l id="s3:l3" met="3" type="-+">aedieu liefelyck <rhyme label="a" type="f" corresp="#s3:l1">uiten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">om troest en onder stant, <rhyme label="b" type="m" corresp="#s3:l2">want</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Ick moet nu belaen, <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">laten u thart te <rhyme label="b" type="m" corresp="#s3:l2">pant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Adieu prinsesse <rhyme label="a" type="f">kersouwe</rhyme></l>
        <l id="s4:l2" met="3" type="-+">adieu lief <rhyme label="b" type="m">deelyckaet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ick bidt u fluer der <rhyme label="a" type="f" corresp="#s4:l1">vrouwen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">toont myn doch doer genaet, <rhyme label="b" type="m" corresp="#s4:l2">baet</rhyme></l>
        <l id="s4:l5" met="4" type="+-">eer Ick doer druck en <rhyme label="c" type="m">vaer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">swaer sterf seer <rhyme label="b" type="m" corresp="#s4:l2">desoelaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
