<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>34043</id>
    <date>
      <year-start>1654</year-start>
      <year-end>1654</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>biblical history song</tag>
      <tag>praise</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Ik stond op hoge bergen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAer is Salomons <rhyme label="a" type="f">Troone</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Daer hy in heeft <rhyme label="b" type="m">gepraelt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In welke Goodes <rhyme label="a" type="f" corresp="#s1:l1">Soone</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van d' Heemel was <rhyme label="b" type="m" corresp="#s1:l2">gedaelt</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Waer is de gulde <rhyme label="a" type="f">Arke</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van 't Nieuwe <rhyme label="b" type="m">Testament</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Waer meed' sijn <rhyme label="a" type="f" corresp="#s2:l1">wonder-warke</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Godt heeft gemaekt <rhyme label="b" type="m" corresp="#s2:l2">bekent</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Den Emmer van sijn <rhyme label="a" type="f">goude</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Waer in het Heemels <rhyme label="b" type="m">Mann'</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wiert al-an goet <rhyme label="a" type="f" corresp="#s3:l1">gehoude</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Waer magh die weesen <rhyme label="b" type="m" corresp="#s3:l2">dan</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Waer is de <rhyme label="a" type="f">onbedorven</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Reyn Maeght, en Moeder <rhyme label="b" type="m">Goodts</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Is sy al meed' <rhyme label="a" type="f" corresp="#s4:l1">gestorven</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Door 't strengh gebiet des <rhyme label="b" type="m" corresp="#s4:l2">doodts</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sy is al meed' <rhyme label="a" type="f">gestorven</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Begraven <rhyme label="b" type="m">eerelijk</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Maer sy is niet <rhyme label="a" type="f" corresp="#s5:l1">bedorven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Nogh niet vergaen tot <rhyme label="b" type="m" corresp="#s5:l2">slijk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">d' Apostolen te <rhyme label="a" type="f">samen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Door maghtigh Goodts <rhyme label="b" type="m">bestuer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met nogh meer ander <rhyme label="a" type="f" corresp="#s6:l1">quamen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vergadert op dees <rhyme label="b" type="m" corresp="#s6:l2">uer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die Psalmen en <rhyme label="a" type="f">Lof-sangen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Songen <rhyme label="b" type="m">devotelijk</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Men hoorden daer ook <rhyme label="a" type="f" corresp="#s7:l1">lange</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Der Engelen <rhyme label="b" type="m" corresp="#s7:l2">Musijk</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Als sy ten derden <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Eer sy wegh wilden <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Haer nogh eens geerne <rhyme label="a" type="f" corresp="#s8:l1">sagen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Was sy daer al van <rhyme label="b" type="m" corresp="#s8:l2">daen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Sy is na 's Heemels <rhyme label="a" type="f">Troone</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Van d' Engelen <rhyme label="b" type="m">gebraght</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Tot haren lieven <rhyme label="a" type="f" corresp="#s9:l1">Soone</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Die haer langh had <rhyme label="b" type="m" corresp="#s9:l2">verwaght</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">In 't Heemels <rhyme label="a" type="f">Tabernakel</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Most d' Arke sijn <rhyme label="b" type="m">gestelt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Dit wonder groot <rhyme label="a" type="f" corresp="#s10:l1">mirakel</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Most niet staen bloot in 't <rhyme label="b" type="m" corresp="#s10:l2">velt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Niet by de <rhyme label="a" type="f">Philistijne</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Niet by <rhyme label="b" type="m">Obed-Edom</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Ook niet in de <rhyme label="a" type="f" corresp="#s11:l1">Woestijne</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Niet by 't gedierte <rhyme label="b" type="m" corresp="#s11:l2">stom</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Maer David liet se <rhyme label="a" type="f">haelen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Van 's Weerelts lange <rhyme label="b" type="m">reys</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En broght se in de <rhyme label="a" type="f" corresp="#s12:l1">Saelen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Van 't Heemelsche <rhyme label="b" type="m" corresp="#s12:l2">Palleys</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Men speelt op <rhyme label="a" type="f">instrumenten</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Men songh seer schoon <rhyme label="b" type="m">Musijk</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Soo dat des Heemels <rhyme label="a" type="f" corresp="#s13:l1">Tenten</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Deur-dreunden <rhyme label="b" type="m" corresp="#s13:l2">soetelijk</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Hy gingh se selfs <rhyme label="a" type="f">t'gemoete</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Hy toonde bly <rhyme label="b" type="m">gelaet</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Sijn liefde wonder <rhyme label="a" type="f" corresp="#s14:l1">soete</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Hy toonde met der <rhyme label="b" type="m" corresp="#s14:l2">daet</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">De schoone <rhyme label="a" type="f">Betsabeë</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Quam by haer lieven <rhyme label="b" type="m">Soon</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Sy wiert met alle <rhyme label="a" type="f" corresp="#s15:l1">reë</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Geset in eenen <rhyme label="b" type="m" corresp="#s15:l2">Troon</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Sy wierdt ter rechter <rhyme label="a" type="f">sijden</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Van Salomon <rhyme label="b" type="m">gestelt</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Strax al die hem <rhyme label="a" type="f" corresp="#s16:l1">benijden</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Die sijn ter neer <rhyme label="b" type="m" corresp="#s16:l2">gevelt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Nu is Salomons <rhyme label="a" type="f">Rijke</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Bevestight om en <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Sijn Vyant moet nu <rhyme label="a" type="f" corresp="#s17:l1">wijke</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Sijn nijders worden <rhyme label="b" type="m" corresp="#s17:l2">stom</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">O groote <rhyme label="a" type="f">Koninginne</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Van Heemel en van <rhyme label="b" type="m">Aerdt</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Voor u de <rhyme label="a" type="f" corresp="#s18:l1">Seraphinne</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Haer neygen met <rhyme label="b" type="m" corresp="#s18:l2">eerwaerdt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Wy Catholijken <rhyme label="a" type="f">meede</rhyme></l>
        <l id="s19:l2" met="3" type="-+">U bieden alle <rhyme label="b" type="m">eer</rhyme>;</l>
        <l id="s19:l3" met="3" type="-+">Wilt ons door u <rhyme label="a" type="f" corresp="#s19:l1">gebeede</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Versoenen met den <rhyme label="b" type="m" corresp="#s19:l2">Heer</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">O Heylige <rhyme label="a" type="f">Marye</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">Reyn Maeght en Moeder <rhyme label="b" type="m">Goodts</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Wilt ons van quaet <rhyme label="a" type="f" corresp="#s20:l1">bevrye</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Nu en in d' uer des <rhyme label="b" type="m" corresp="#s20:l2">doodts</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
