    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">LIeven Godt wat <rhyme label="a" type="m">tijt</rhyme>:</l>
        <l id="s1:l2" met="3" type="+-">Yder een die <rhyme label="a" type="m" corresp="#s1:l1">vrijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">t'Is al Bruygom ende <rhyme label="b" type="m">Bruyt</rhyme>:</l>
        <l id="s1:l4" met="3" type="+-">Ieder soeckt sijn <rhyme label="c" type="m">paer</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Met het nieuwe <rhyme label="c" type="m" corresp="#s1:l4">jaer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als de jeught wilt seffens <rhyme label="b" type="m" corresp="#s1:l3">uyt</rhyme>:</l>
        <l id="s1:l7" met="3" type="+-">Het is Venus <rhyme label="d" type="m">smert</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Die soo menich <rhyme label="d" type="m" corresp="#s1:l7">hert</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Door den <rhyme label="e" type="m">schicht</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Heeft verlicht,, Tot den <rhyme label="f" type="m">bant</rhyme>:</l>
        <l id="s1:l11" met="3" type="+-">Soo dat Ionck en <rhyme label="g" type="m">out</rhyme></l>
        <l id="s1:l12" met="3" type="+-">Soeckt te sijn <rhyme label="g" type="m" corresp="#s1:l11">ghetrout</rhyme>,</l>
        <l id="s1:l13" met="4" type="+-">Heel de weerelt is in <rhyme label="f" type="m" corresp="#s1:l10">brant</rhyme>,</l>
      </lg>
