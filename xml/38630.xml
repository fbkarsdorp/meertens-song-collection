  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Soo langh de blond' <rhyme label="a" type="m">Aurôor</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den bruynen nacht doet <rhyme label="b" type="m">vliên</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verweck ick en <rhyme label="a" type="m" corresp="#s1:l1">bekoor</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met liefde alle <rhyme label="b" type="m" corresp="#s1:l2">liên</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Gheen macht en <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Weer-staen mijn soet <rhyme label="d" type="m">ghestreel</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Noch vrouw noch <rhyme label="c" type="m" corresp="#s1:l5">man</rhyme>:</l>
        <l id="s1:l8" met="2" type="-+">Noch vrouw noch <rhyme label="c" type="m" corresp="#s1:l5">man</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">En koos oyt ander <rhyme label="d" type="m" corresp="#s1:l6">deel</rhyme>.</l>
        <l id="s1:l10" met="3" type="-+">En koos oyt ander <rhyme label="d" type="m" corresp="#s1:l6">deel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den schilt oft t'swert van <rhyme label="a" type="m">Mars</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En acht ick niet een <rhyme label="b" type="m">sier</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Als ick zijn herte <rhyme label="a" type="m" corresp="#s2:l1">pars</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Doort pramen van mijn <rhyme label="b" type="m" corresp="#s2:l2">vier</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Jae Godt <rhyme label="c" type="m">Iupijn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met zijnen <rhyme label="d" type="m">donderslach</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Moet vluchtich <rhyme label="c" type="m" corresp="#s2:l5">sijn</rhyme>:</l>
        <l id="s2:l8" met="2" type="-+">Moet vluchtich <rhyme label="c" type="m" corresp="#s2:l5">sijn</rhyme>:</l>
        <l id="s2:l9" met="3" type="-+">Alleen voor mijn <rhyme label="d" type="m" corresp="#s2:l6">ghelach</rhyme>.</l>
        <l id="s2:l10" met="3" type="-+">Alleen voor mijn <rhyme label="d" type="m" corresp="#s2:l6">ghelach</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Kan mijn verheven <rhyme label="a" type="m">vlam</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Een godtheyt soo <rhyme label="b" type="m">bevaen</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Als sy ghedreven <rhyme label="a" type="m" corresp="#s3:l1">quam</rhyme></l>
        <l id="s3:l4" met="3" type="-+">By Leda in een <rhyme label="b" type="m" corresp="#s3:l2">swaen</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Hoe veel te <rhyme label="c" type="m">meer</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En sal het mensch <rhyme label="d" type="m">gheslacht</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Mijn groote <rhyme label="c" type="m" corresp="#s3:l5">macht</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Mijn groote <rhyme label="c" type="m" corresp="#s3:l5">macht</rhyme>:</l>
        <l id="s3:l9" met="3" type="-+">Niet houden dan in <rhyme label="d" type="m" corresp="#s3:l6">eer</rhyme>.</l>
        <l id="s3:l10" met="3" type="-+">Niet houden dan in <rhyme label="d" type="m" corresp="#s3:l6">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
