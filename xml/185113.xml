  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VErschrikt, ag Elementen <rhyme label="a" type="m">al</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Ag davert Hemelen, Aerdsche <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Og wat sal hier <rhyme label="b" type="f">gebeuren</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">O ziel, dit is om <rhyme label="c" type="m">dy</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">De Heer Jesu leyt voor <rhyme label="c" type="m" corresp="#s1:l4">my</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">O ziele wilt nu <rhyme label="b" type="f" corresp="#s1:l3">treuren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hy die het al geschapen <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Onderhout, en â€™t leven <rhyme label="a" type="m" corresp="#s2:l1">geeft</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ontbloot men van zijn <rhyme label="b" type="f">kleeden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Zijn mont niet open <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">ik sie een beek van <rhyme label="c" type="m" corresp="#s2:l4">bloet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Beverwen al zijn <rhyme label="b" type="f" corresp="#s2:l3">Leden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy slaen met roeden alsoo <rhyme label="a" type="m">stijf</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En doorwonden Jesus <rhyme label="a" type="m" corresp="#s3:l1">lijf</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O Herder vol <rhyme label="b" type="f">genaden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gun dat ik voor u <rhyme label="c" type="m">kniel</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Om met u bloet mijn <rhyme label="c" type="m" corresp="#s3:l4">siel</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Te suyveren en te <rhyme label="b" type="f" corresp="#s3:l3">baden</rhyme>.</l>
      </lg>
    </lg>
  </text>
