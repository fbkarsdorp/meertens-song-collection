<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>36952</id>
    <date>
      <year-start>1677</year-start>
      <year-end>1677</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Bacchus heeft ons de wijn besteld</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EEn seker magher <rhyme label="a" type="m">Edel-man</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghelycker nu veel <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die hadd' ontrent den <rhyme label="c" type="m">Nieuwenjaer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gheslaghen een vet <rhyme label="b" type="m" corresp="#s1:l2">Swyn</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En van het edel <rhyme label="d" type="m">Verkens-bloedt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En van de vette <rhyme label="e" type="m">Lies</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Hadd' hy doen maken <rhyme label="f" type="f">Beulingen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Seer lecker en <rhyme label="e" type="m" corresp="#s1:l6">exquies</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die vette bloede <rhyme label="a" type="m">Beulinghen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hadd' hy aen alle <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Soo lieffelyck en <rhyme label="c" type="m">vrindelyck</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ghekruyt met <rhyme label="b" type="m" corresp="#s2:l2">Specery</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dat sy met hunnen <rhyme label="d" type="m">edelen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En aen-ghenamen <rhyme label="e" type="m">geur</rhyme></l>
        <l id="s2:l7" met="4" type="-+">De Keucken vulden met <rhyme label="f" type="f">parfum'</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En roken 't heel huys <rhyme label="e" type="m" corresp="#s2:l6">deur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een schotel van die <rhyme label="a" type="m">Beulingen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die sondt hy door syn <rhyme label="b" type="m">Maert</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Voor een Nieuw - jaer aen syn schoon <rhyme label="c" type="m">Lief</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Voor ryck, en schoon <rhyme label="b" type="m" corresp="#s3:l2">vermaert</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En om noch <rhyme label="d" type="m">over-vloedigher</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Te quyten synen <rhyme label="e" type="m">plicht</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Sondt hy haer met deês <rhyme label="f" type="f">Beulingen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Dit hups Nieuw-jaers <rhyme label="e" type="m" corresp="#s3:l6">Gedicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick moest u voor een <rhyme label="a" type="m">Nieuwen-jaer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Oft voor een <rhyme label="b" type="m">minne-pandt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ver-eeren met een Perle <rhyme label="c" type="m">snoer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Oft puyck van <rhyme label="b" type="m" corresp="#s4:l2">Diamant</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Maer nu en zyn 't maer <rhyme label="d" type="m">Beulingẽ</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Die uwen Vryer <rhyme label="e" type="m">gheeft</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Den Vryer die gheeft dat hy <rhyme label="f" type="f">heeft</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Is weerdigh dat hy <rhyme label="e" type="m" corresp="#s4:l6">leeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">g'En hebt noch ghelt, noch goet van <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Noch Perl' oft <rhyme label="b" type="m">Diamant</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ghy hebt voor hondert Ploeghen <rhyme label="c" type="m">landt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">G'hebt alles <rhyme label="b" type="m" corresp="#s5:l2">abondant</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">U en ghebreckt maer edel <rhyme label="d" type="m">bloedt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">By al uw machtigh <rhyme label="e" type="m">goet</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Daerom seynd' ick u <rhyme label="f" type="f">Beulinghen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Ghemaeckt van Lies en <rhyme label="e" type="m" corresp="#s5:l6">Bloet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O Vryster ghy zyt vet van <rhyme label="a" type="m">goedt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En ick ben eêl van <rhyme label="b" type="m">bloedt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Voeght uw vet Goet by myn eêl <rhyme label="c" type="m">bloedt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soo wort den Beulingh <rhyme label="b" type="m" corresp="#s6:l2">goedt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Is myn eêl bloedt u <rhyme label="d" type="m">willekom</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Neemt my voor <rhyme label="e" type="m">Bruydegom</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Paert uwe vette <rhyme label="f" type="f">Kore-blom</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Met mynen <rhyme label="e" type="m" corresp="#s6:l6">Edel-dom</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Als sy dit Dicht ghelesen <rhyme label="a" type="m">hadt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Schoot sy in hare <rhyme label="b" type="m">tes</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En gaf voor Drinck-gelt aen de <rhyme label="c" type="m">Maert</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Een Quaertjen vyf oft <rhyme label="b" type="m" corresp="#s7:l2">ses</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Sy seyd'; groet my u Meester <rhyme label="d" type="m">seer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Hy is een gheestigh <rhyme label="e" type="m">Man</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Hy magh my selver in <rhyme label="f" type="f">persoon</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Daer breeder spreken <rhyme label="e" type="m" corresp="#s7:l6">van</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De Maert neegh fray, en trock naer <rhyme label="a" type="m">huys</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met dit ghewenscht <rhyme label="b" type="m">bescheet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Den Eêl-man liep stracks naer de <rhyme label="c" type="m">smis</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Want 't y ser was toen <rhyme label="b" type="m" corresp="#s8:l2">heet</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Hy heeft de saeck soo ver <rhyme label="d" type="m">gebrogt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Door synen kloecken <rhyme label="e" type="m">gheest</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Dat hy heeft van die <rhyme label="f" type="f">Beulinghen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Ghemaeckt een <rhyme label="e" type="m" corresp="#s8:l6">Bruyloft-feest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
