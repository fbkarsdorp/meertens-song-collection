<song>
  <meta>
    <id>19914</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="sailor's song">zeemanslied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody>Iets moet ik u Laura vragen</melody>
    <categories>
      <category translation="work">werk</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">RYcke Zee met al u <rhyme label="a" type="f">schatten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wie kan uwe rijckdom <rhyme label="a" type="f" corresp="#s1:l1">vatten</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Wie besiet u rijck <rhyme label="b" type="m">tresoor</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Peerlen, Silver en <rhyme label="c" type="f">Coralen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Steentjens, Beentjens zijn te <rhyme label="c" type="f" corresp="#s1:l4">halen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Al u rijcke zanden <rhyme label="b" type="m" corresp="#s1:l3">door</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Niet is hier op deser <rhyme label="a" type="f">aerden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Oock hoe groot en schoon van <rhyme label="a" type="f" corresp="#s2:l1">waerden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat u konst niet aerdigh <rhyme label="b" type="m">maelt</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Paerden, Katten, Honden, <rhyme label="c" type="f">Swijnen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Boomen, Kruyden, <rhyme label="c" type="f" corresp="#s2:l4">Medecijnen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Alles uyt u boesem <rhyme label="b" type="m" corresp="#s2:l3">daelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Pluto sit daer oock te <rhyme label="a" type="f">grasen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Om sijn grage lust te <rhyme label="a" type="f" corresp="#s3:l1">asen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En hoe schenckt ons al te <rhyme label="b" type="m">met</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Wel een schelpjen voor ons <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ey hoe konstigh en <rhyme label="c" type="f" corresp="#s3:l4">gebogen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Hoe gebeeld, gevlamt en <rhyme label="b" type="m" corresp="#s3:l3">vet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Siet hem ramlen in de <rhyme label="a" type="f">schijven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Van ons scheepkens, die daer <rhyme label="a" type="f" corresp="#s4:l1">blijven</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Siet hem staeplen goud en <rhyme label="b" type="m">schat</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Daer heeft hy sijn Tolck en <rhyme label="c" type="f">boden</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Die ons by sijn schatten <rhyme label="c" type="f" corresp="#s4:l4">nooden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">'t Gierigh hert gebreeckt noch <rhyme label="b" type="m" corresp="#s4:l3">wat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Onse Scheepjens die <rhyme label="a" type="f">laveeren</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Die dan heen en weder <rhyme label="a" type="f" corresp="#s5:l1">keeren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Al op hope van de <rhyme label="b" type="m">winst</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Al ons geld en goet te <rhyme label="c" type="f">soecken</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Uyt de Noord en Zuyder <rhyme label="c" type="f" corresp="#s5:l4">hoecken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Maer om deughde denckt men <rhyme label="b" type="m" corresp="#s5:l3">minst</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Sietse hier eens op de <rhyme label="a" type="f">stranden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Met haer wimpels komen <rhyme label="a" type="f" corresp="#s6:l1">landen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Met haer vlercken en <rhyme label="b" type="m">getrom</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Sietse vlammen tot een <rhyme label="c" type="f">wonder</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Sietse spouwen vyer en <rhyme label="c" type="f" corresp="#s6:l4">donder</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Tot haer blijde <rhyme label="b" type="m" corresp="#s6:l3">wellekom</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Daer word alle vreughd <rhyme label="a" type="f">bedreven</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Als de Schipper schip en <rhyme label="a" type="f" corresp="#s7:l1">leven</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Brengt ter goeder haven <rhyme label="b" type="m">in</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Siet de Reeders eens <rhyme label="c" type="f">verlangen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Om een goede buyt te <rhyme label="c" type="f" corresp="#s7:l4">vangen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Siet haer gapen na 't <rhyme label="b" type="m" corresp="#s7:l3">gewin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Schipper hebdy 'tdan <rhyme label="a" type="f">verpeutert</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Hebdy hier en daer <rhyme label="a" type="f" corresp="#s8:l1">geleutert</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Hebdy van u Schip <rhyme label="b" type="m">verboomt</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Dan moogth ghy by tijts wel <rhyme label="c" type="f">singen</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Schipper moet de Ree <rhyme label="c" type="f" corresp="#s8:l4">afspringen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Als hy van den Nicker <rhyme label="b" type="m" corresp="#s8:l3">droomt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Maer hebt ghy soo slincx by <rhyme label="a" type="f">tijden</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Wat getaeckt bangeld of <rhyme label="a" type="f" corresp="#s9:l1">zijden</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Hebdy maer u schip <rhyme label="b" type="m">gevult</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Hebdy dan een potjen <rhyme label="c" type="f">broken</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Niemant heeft een woord <rhyme label="c" type="f" corresp="#s9:l4">gesproken</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Ghy zijt vry en hebt geen <rhyme label="b" type="m" corresp="#s9:l3">schuld</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Wie by Pluto gaet ter <rhyme label="a" type="f">schoolen</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Die sal niet in doncker <rhyme label="a" type="f" corresp="#s10:l1">doolen</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Want hy is een duyster <rhyme label="b" type="m">Godt</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Die sijn Kinders en sijn <rhyme label="c" type="f">Klercken</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Drijft tot roof en quade <rhyme label="c" type="f" corresp="#s10:l4">wercken</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">En hy maeckt haer stout en <rhyme label="b" type="m" corresp="#s10:l3">sot</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Doet u handel doch ter <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Wilt van Ionas Schippers <rhyme label="a" type="f" corresp="#s11:l1">leeren</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Als zy quamen uyt de <rhyme label="b" type="m">nood</rhyme>:</l>
        <l id="s11:l4" met="4" type="+-">Bracht en sy den Heere <rhyme label="c" type="f">gaven</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Die haer in een blijde <rhyme label="c" type="f" corresp="#s11:l4">haven</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Bracht in 't leven uyt de <rhyme label="b" type="m" corresp="#s11:l3">dood</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
