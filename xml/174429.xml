    <melody>En wil het u niet belgen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilt u vromelyck <rhyme label="a" type="f">Dragen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">al voor Gods heylige <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s1:l3" met="3" type="-+">zo zal In uwe <rhyme label="a" type="f" corresp="#s1:l1">dagen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">geen quaedt werden <rhyme label="b" type="m" corresp="#s1:l2">gehoort</rhyme></l>
        <l id="s1:l5" met="3" type="-+">dat ghy yet hebt <rhyme label="c" type="f">bedreven</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Tegens u <rhyme label="d" type="m">middelaer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">ghy comt Int eewich <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Int eerste nieuwe <rhyme label="d" type="m" corresp="#s1:l6">Jaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den Coninck al uijt <rhyme label="a" type="f">Spaengen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">is opt Nederlant <rhyme label="b" type="m">verwoet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">maer hout moet hem <rhyme label="a" type="f" corresp="#s2:l1">oraengen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Nassou tedel <rhyme label="b" type="m" corresp="#s2:l2">bloet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">hy zal u nyet <rhyme label="c" type="f">verlaten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">weest een getrou <rhyme label="d" type="m">Dienaer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">maer comen u te <rhyme label="c" type="f" corresp="#s2:l5">baten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">al In het nieuwe <rhyme label="d" type="m" corresp="#s2:l6">Jaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dus wilt de Spaensche <rhyme label="a" type="f">natij</rhyme></l>
        <l id="s3:l2" met="3" type="-+">doch geensints geloven <rhyme label="b" type="m">nyet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">want u van haer geen <rhyme label="a" type="f" corresp="#s3:l1">gratij</rhyme></l>
        <l id="s3:l4" met="3" type="-+">nimmermeer en <rhyme label="b" type="m" corresp="#s3:l2">geschiet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">alsoot wel is <rhyme label="c" type="f">gebleecken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">te naerden <rhyme label="d" type="m">openbaer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">dat daer al is <rhyme label="c" type="f" corresp="#s3:l5">doorsteecken</rhyme></l>
        <l id="s3:l8" met="3" type="-+">ziet toe Int nieuwe <rhyme label="d" type="m" corresp="#s3:l6">Jaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">want zij zullen nijet <rhyme label="a" type="f">houwen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">haer beloften cleyn noch <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s4:l3" met="3" type="-+">maer brengen u In <rhyme label="a" type="f" corresp="#s4:l1">rouwen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">als een schape dat men <rhyme label="b" type="m" corresp="#s4:l2">Doot</rhyme></l>
        <l id="s4:l5" met="3" type="-+">met des keysers <rhyme label="c" type="f">placcaten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">weeren zij haer te <rhyme label="d" type="m">gaer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dus wilt het geslacht <rhyme label="c" type="f" corresp="#s4:l5">haten</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Denckt het Int nieuwe <rhyme label="d" type="m" corresp="#s4:l6">Jaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt nu <rhyme label="a" type="f">obedieren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">uwen voorsichtigen <rhyme label="b" type="m">raet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">die daer gans doen <rhyme label="a" type="f" corresp="#s5:l1">verneeren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">den Spangaert <rhyme label="b" type="m" corresp="#s5:l2">opstinaet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">en houwen uwe <rhyme label="c" type="f">landen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In rust zonder <rhyme label="d" type="m">beswaer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">bevryen u voor <rhyme label="c" type="f" corresp="#s5:l5">schanden</rhyme></l>
        <l id="s5:l8" met="3" type="-+">looft haer Int nieuwe <rhyme label="d" type="m" corresp="#s5:l6">Jaer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
