<song>
  <meta>
    <id>185327</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mars van Dessau</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+"><rhyme label="a" type="m">Karsou</rhyme>,</l>
        <l id="s1:l2" met="1" type="-+">Ik heb <rhyme label="a" type="m" corresp="#s1:l1">Jou</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Zoo meenigmaal <rhyme label="b" type="f">bekeeken</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Maar al u looze <rhyme label="b" type="f" corresp="#s1:l3">streeken</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die lyke my <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Al u looze <rhyme label="d" type="f">streeken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die staan my niet <rhyme label="e" type="m">aan</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Daarom wil ik <rhyme label="f" type="f">treeden</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Waar ik koom van <rhyme label="e" type="m" corresp="#s1:l7">daan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+">'k Kwam <rhyme label="a" type="m">laast</rhyme>,</l>
        <l id="s2:l2" met="1" type="-+">Met <rhyme label="a" type="m" corresp="#s2:l1">haast</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Al door het <rhyme label="b" type="f">Heere-straatje</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Ik zy lieve <rhyme label="b" type="f" corresp="#s2:l3">Maatje</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Kom zet u hier wat <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Drinkt eens een <rhyme label="d" type="f">glaasje</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Rookt een pyp <rhyme label="e" type="m">tabak</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Met al die looze <rhyme label="f" type="f">streeken</rhyme>;</l>
        <l id="s2:l9" met="3" type="-+">Raakt het geld uit u <rhyme label="e" type="m" corresp="#s2:l7">zak</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+">Een <rhyme label="a" type="m">zoen</rhyme>,</l>
        <l id="s3:l2" met="1" type="-+">Kan <rhyme label="a" type="m" corresp="#s3:l1">doen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">De heele nagt te <rhyme label="b" type="f">blyven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dan hoord men niet als <rhyme label="b" type="f" corresp="#s3:l3">kyven</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Van onze <rhyme label="c" type="m">Officier</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zoo raken wy aan 't <rhyme label="d" type="f">dwaalen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Zoo dronken als een <rhyme label="e" type="m">Zwyn</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ons Schip legt voor de <rhyme label="f" type="f">palen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Wy moeten aan boord <rhyme label="e" type="m" corresp="#s3:l7">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="1" type="-+">Laat <rhyme label="a" type="m">wy</rhyme>;</l>
        <l id="s4:l2" met="1" type="-+">Eens <rhyme label="a" type="m" corresp="#s4:l1">bly</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Te zaam de glaasjes <rhyme label="b" type="f">klinken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En eens lustig <rhyme label="b" type="f" corresp="#s4:l3">drinken</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wy gaan na Holland <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Adieu Karsouze <rhyme label="d" type="f">Meisjes</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En Vaderlandse <rhyme label="e" type="m">Bruid</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Wy dringen nu voor 't <rhyme label="f" type="f">laatste</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Onze glaasjes <rhyme label="e" type="m" corresp="#s4:l7">uit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="1" type="-+">Toe <rhyme label="a" type="m">fors</rhyme>,</l>
        <l id="s5:l2" met="1" type="-+">Maak <rhyme label="a" type="m" corresp="#s5:l1">los</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Jou voor en agter <rhyme label="b" type="f">touwen</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Wild nu maar <rhyme label="b" type="f" corresp="#s5:l3">afhouwen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Men vaard na Holland <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daar is geen beter <rhyme label="d" type="f">leven</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Als by u Echten <rhyme label="e" type="m">Vrouw</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Verwenscht zyn alle <rhyme label="f" type="f">Hoeren</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Van 't Land <rhyme label="e" type="m" corresp="#s5:l7">Karsou</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
