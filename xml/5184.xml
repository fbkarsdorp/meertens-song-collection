<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5184</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag>beggar song</tag>
    </tags>
    <melody>Admirant</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Maer wie wil hooren een Nieu <rhyme label="a" type="m">liet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Maer wie wil hooren een Nieu <rhyme label="a" type="m" corresp="#s1:l1">liet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat t'Antwerpen is <rhyme label="a" type="m" corresp="#s1:l1">geschiet</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,,</l>
        <l id="s1:l5" met="4" type="+-">Dat t'Antwerpen is <rhyme label="a" type="m" corresp="#s1:l1">gheschiet</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al vande Spaensche <rhyme label="a" type="m">Rebelleusen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Al vande Spaensche <rhyme label="a" type="m" corresp="#s2:l1">Rebelleusen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Zy zijn veel argher dan de <rhyme label="a" type="m" corresp="#s2:l1">geusen</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Zy zijn veel argher dan de <rhyme label="a" type="m" corresp="#s2:l1">geusen</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zy quamen t'Antwerpen al over de <rhyme label="a" type="m">vest</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Zy quamen t'Antwerpen al over de <rhyme label="a" type="m" corresp="#s3:l1">vest</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zy seyden t'wasser ghegheven ten <rhyme label="a" type="m" corresp="#s3:l1">best</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Zy seyden t'wasser ghegheven ten <rhyme label="a" type="m" corresp="#s3:l1">best</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Seignor Daniel de <rhyme label="a" type="m">Casteleyn</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Seignor Daniel de <rhyme label="a" type="m" corresp="#s4:l1">Casteleyn</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Hy dedese vergaderen op de <rhyme label="a" type="m" corresp="#s4:l1">pleyn</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Hy dedese vergaderen op de <rhyme label="a" type="m" corresp="#s4:l1">pleyn</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Doe quam de groote <rhyme label="a" type="m">Commandeur</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Doe quam de groote <rhyme label="a" type="m" corresp="#s5:l1">Commandeur</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Schampen moest de <rhyme label="a" type="m" corresp="#s5:l1">Gouverneur</rhyme></l>
        <l id="s5:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Schampen moest de <rhyme label="a" type="m" corresp="#s5:l1">Gouverneur</rhyme></l>
        <l id="s5:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zy seyden de Gouverneur was een <rhyme label="a" type="m">guyt</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Zy seyden de Gouverneur was een <rhyme label="a" type="m" corresp="#s6:l1">guyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Zy smeten hem daer de glasen <rhyme label="a" type="m" corresp="#s6:l1">uut</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Zy smeten hem daer de glasen <rhyme label="a" type="m" corresp="#s6:l1">uut</rhyme></l>
        <l id="s6:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Voorts liepen sy al met groot <rhyme label="a" type="m">gedruys</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Voorts liepen sy al met groot <rhyme label="a" type="m" corresp="#s7:l1">gedruys</rhyme></l>
        <l id="s7:l3" met="4" type="+-">En namen de sleutelen vant <rhyme label="a" type="m" corresp="#s7:l1">Stadthuys</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">En namen de sleutelen vant <rhyme label="a" type="m" corresp="#s7:l1">Stadthuys</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Doe quamper meer al van die <rhyme label="a" type="m">soort</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Doe quamper meer al van die <rhyme label="a" type="m" corresp="#s8:l1">soort</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En namen de sleutelen vande <rhyme label="a" type="m" corresp="#s8:l1">poort</rhyme>,</l>
        <l id="s8:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">En namen de sleutelen vande <rhyme label="a" type="m" corresp="#s8:l1">Poort</rhyme></l>
        <l id="s8:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Zy lasen t'Placcaet vanden eersten <rhyme label="a" type="m">termijn</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Zy lasen t'Placcaet vanden eersten <rhyme label="a" type="m" corresp="#s9:l1">termijn</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Dat sy ten vollen betaelt souden <rhyme label="a" type="m" corresp="#s9:l1">zijn</rhyme></l>
        <l id="s9:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Dat sy te vollen betaelt wouden <rhyme label="a" type="m" corresp="#s9:l1">zijn</rhyme>,</l>
        <l id="s9:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Zy wouden niet eten dan <rhyme label="a" type="m">Wittebroot</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Zy wouden niet eten dan <rhyme label="a" type="m" corresp="#s10:l1">Wittebroot</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Van als ghebraden kleyn en <rhyme label="a" type="m" corresp="#s10:l1">groot</rhyme></l>
        <l id="s10:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Van als ghebraden kleyn en <rhyme label="a" type="m" corresp="#s10:l1">groot</rhyme></l>
        <l id="s10:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Zy liepen de rijcke lieden op de <rhyme label="a" type="m">poort</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Zy liepen de rijcke lieden op de <rhyme label="a" type="m" corresp="#s11:l1">poort</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">En droncken de Wijn met groote <rhyme label="a" type="m" corresp="#s11:l1">stoopen</rhyme></l>
        <l id="s11:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">En droncken de wijn met groote <rhyme label="a" type="m" corresp="#s11:l1">stoopen</rhyme></l>
        <l id="s11:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Zy riepen foire met groot <rhyme label="a" type="m">geschal</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Zy riepen foire met groot <rhyme label="a" type="m" corresp="#s12:l1">geschal</rhyme></l>
        <l id="s12:l3" met="4" type="+-">Daer creghen wel hondert vrouwen <rhyme label="a" type="m" corresp="#s12:l1">misval</rhyme>,</l>
        <l id="s12:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Daer cregen wel hondert vrouwen <rhyme label="a" type="m" corresp="#s12:l1">misval</rhyme></l>
        <l id="s12:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Zy deden vergaren den breeden <rhyme label="a" type="m">Raet</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Zy deden vergaren den breeden <rhyme label="a" type="m" corresp="#s13:l1">Raet</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Men schatte die burgers na haer <rhyme label="a" type="m" corresp="#s13:l1">staet</rhyme></l>
        <l id="s13:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Men schatte de borgers na haer <rhyme label="a" type="m" corresp="#s13:l1">staet</rhyme></l>
        <l id="s13:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Zy maecten alarm nacht ende <rhyme label="a" type="m">dach</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Zy maecten alarm nacht ende <rhyme label="a" type="m" corresp="#s14:l1">dach</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Tot datter ghelt was <rhyme label="a" type="m" corresp="#s14:l1">opghebracht</rhyme>,</l>
        <l id="s14:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">Tot datter gelt was <rhyme label="a" type="m" corresp="#s14:l1">opghebracht</rhyme></l>
        <l id="s14:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Het was de Borgers groot <rhyme label="a" type="m">bezwaer</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Het was de borghers groot <rhyme label="a" type="m" corresp="#s15:l1">bezwaer</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Datter gheen Regente en <rhyme label="a" type="m" corresp="#s15:l1">waer</rhyme></l>
        <l id="s15:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Datter gheen Regente en <rhyme label="a" type="m" corresp="#s15:l1">waer</rhyme></l>
        <l id="s15:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Die ons dit Liedeken eerstwerf <rhyme label="a" type="m">sanck</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Die ons dit liedeken eerstwerf <rhyme label="a" type="m" corresp="#s16:l1">sanck</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Hy wil niet wesen in Spaengiaerts <rhyme label="a" type="m" corresp="#s16:l1">bedwanck</rhyme>,</l>
        <l id="s16:l4" met="2" type="+-">Foire <rhyme label="b" type="f">viliaco</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Hy wil niet wesen in Spaengiaerts <rhyme label="a" type="m" corresp="#s16:l1">bedwanck</rhyme></l>
        <l id="s16:l6" met="2" type="+-">Foire <rhyme label="c" type="m">Ladron</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
