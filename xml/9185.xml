<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9185</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Courante la belle</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">COm Multi laet ons <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s1:l2" met="6" type="-+">Derwaerts daer sie ic Amaril en <rhyme label="a" type="m" corresp="#s1:l1">Luda</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Met Silvia, ay <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En wy en sien daer <rhyme label="b" type="m" corresp="#s1:l3">niet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Cephalus comt uyt <rhyme label="b" type="m" corresp="#s1:l3">'tRiet</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">En hy <rhyme label="c" type="m">vrijt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">U Amaril, 'tis <rhyme label="c" type="m" corresp="#s1:l6">tijdt</rhyme>.</l>
        <l id="s1:l8" met="2" type="+-">Dat Ghy <rhyme label="c" type="m" corresp="#s1:l6">zijt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">By haer, eer dat hy van haer <rhyme label="d" type="m">krijcht</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Troost en <rhyme label="e" type="f">weerminne</rhyme>.</l>
        <l id="s1:l11" met="2" type="+-">Hy sal niet <rhyme label="e" type="f" corresp="#s1:l10">winne</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Aen haer, dus Tyter daer van <rhyme label="d" type="m" corresp="#s1:l9">swijcht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn Harderin is <rhyme label="a" type="m">trou</rhyme>,</l>
        <l id="s2:l2" met="6" type="-+">Sy mint my, so dat eer den Hemel vallen <rhyme label="a" type="m" corresp="#s2:l1">sou</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Eer dat sy inde <rhyme label="b" type="m">Min</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Verand'ren sou van <rhyme label="b" type="m" corresp="#s2:l3">sin</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hoe wel dat ick haer <rhyme label="b" type="m" corresp="#s2:l3">bin</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Wt <rhyme label="c" type="m">'tghesicht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Sy is niet wulps en <rhyme label="c" type="m" corresp="#s2:l6">licht</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Dus en <rhyme label="c" type="m" corresp="#s2:l6">sticht</rhyme></l>
        <l id="s2:l9" met="3" type="-+">In my geen <rhyme label="d" type="m">jalousy</rhyme>.</l>
        <l id="s2:l10" met="2" type="-+">Ick ben te <rhyme label="e" type="f">vreeden</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Laet ons ras <rhyme label="e" type="f" corresp="#s2:l10">treden</rhyme></l>
        <l id="s2:l12" met="4" type="-+">Na haer Multi dat bid ick <rhyme label="d" type="m" corresp="#s2:l9">dy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Siet ginder op het <rhyme label="a" type="m">Landt</rhyme>,</l>
        <l id="s3:l2" met="6" type="-+">Daer maecken sy een Rey, en houden hant aen <rhyme label="a" type="m" corresp="#s3:l1">hant</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Amaril leyt den <rhyme label="b" type="m">dans</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En heeft een Roosen <rhyme label="b" type="m" corresp="#s3:l3">Crans</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Op 't Hooft, d'welck haer <rhyme label="b" type="m" corresp="#s3:l3">glans</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Meer op <rhyme label="c" type="m">pronckt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Cephalus Hart <rhyme label="c" type="m" corresp="#s3:l6">ontfonckt</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Siet hy <rhyme label="c" type="m" corresp="#s3:l6">lonckt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">En gluert na <rhyme label="d" type="m">Amaril</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Soet van ter <rhyme label="e" type="f">syen</rhyme>.</l>
        <l id="s3:l11" met="2" type="+-">Siet sy hem <rhyme label="e" type="f" corresp="#s3:l10">myen</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">'t Blijckt nu wie dat sy minnen <rhyme label="d" type="m" corresp="#s3:l9">wil</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Amaril laet de <rhyme label="a" type="m">Rey</rhyme>,</l>
        <l id="s4:l2" met="6" type="-+">Tyter siet sy loopt voor Cephalus in de <rhyme label="a" type="m" corresp="#s4:l1">Way</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nu sien ick eerst hoe <rhyme label="b" type="m">seer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Amaril voor haer <rhyme label="b" type="m" corresp="#s4:l3">Eer</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Strijt, want sy alree <rhyme label="b" type="m" corresp="#s4:l3">veer</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Is in't <rhyme label="c" type="m">Velt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Chephalus met <rhyme label="c" type="m" corresp="#s4:l6">ghewelt</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Daer neer <rhyme label="c" type="m" corresp="#s4:l6">stelt</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Dus Tyter ick gae het is <rhyme label="d" type="m">tijdt</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Dat ickt <rhyme label="e" type="f">beletten</rhyme>.</l>
        <l id="s4:l11" met="2" type="+-">Gaet haer <rhyme label="e" type="f" corresp="#s4:l10">ontsetten</rhyme></l>
        <l id="s4:l12" met="4" type="-+">Of ghy wert u Amaril <rhyme label="d" type="m" corresp="#s4:l9">quijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
