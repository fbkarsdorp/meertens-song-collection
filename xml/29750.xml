  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Voorwaer nu wel te <rhyme label="a" type="m">rechte</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Jammerlyck claghen <rhyme label="b" type="m">mach</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick arm onnosel <rhyme label="a" type="m" corresp="#s1:l1">kenechte</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Kermmende nacht en <rhyme label="b" type="m" corresp="#s1:l2">dacht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ick roep O wee O <rhyme label="b" type="m" corresp="#s1:l2">wach</rhyme></l>
        <l id="s1:l6" met="3" type="-+">A qui <rhyme label="c" type="m">tirannissant</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Heeft onder hare <rhyme label="b" type="m" corresp="#s1:l2">macht</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Mon cuer <rhyme label="c" type="m" corresp="#s1:l6">treslanguisant</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Voorwaer en minder <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wy te claghen <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die connen Int <rhyme label="a" type="m" corresp="#s2:l1">verdriet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Door u Gevensden <rhyme label="b" type="m" corresp="#s2:l2">schyn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ghy claecht t'elcken <rhyme label="b" type="m" corresp="#s2:l2">termyn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">De uos grandes <rhyme label="c" type="m">vigeurs</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En sulcx en sou niet <rhyme label="b" type="m" corresp="#s2:l2">syn</rhyme></l>
        <l id="s2:l8" met="3" type="-+">sy vous nesties <rhyme label="c" type="m" corresp="#s2:l6">trompeurs</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hept ghy den dienaer <rhyme label="a" type="m">dyn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Bevonden <rhyme label="b" type="m">desloyal</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Weest hem tot geen <rhyme label="a" type="m" corresp="#s3:l1">termyn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Troost waer noch <rhyme label="b" type="m" corresp="#s3:l2">cordiael</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Maer helpt gy <rhyme label="b" type="m" corresp="#s3:l2">teenenmael</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Trouua ou Joyael <rhyme label="c" type="m">amant</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Weest doch <rhyme label="b" type="m" corresp="#s3:l2">nediciael</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Son coeur <rhyme label="c" type="m" corresp="#s3:l6">treslanguissant</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Certeyn die groote <rhyme label="a" type="m">ontrouwe</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Der jongmans is soo <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dat diese Gelouen <rhyme label="a" type="m" corresp="#s4:l1">soue</rhyme>[.]</l>
        <l id="s4:l4" met="3" type="-+">Sou connen Inden <rhyme label="b" type="m" corresp="#s4:l2">noot</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ghy legt u hooft In <rhyme label="b" type="m" corresp="#s4:l2">schoot</rhyme></l>
        <l id="s4:l6" met="3" type="-+">A gemissements et <rhyme label="c" type="m">peeurs</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Ghy segt ghy sterft die <rhyme label="b" type="m" corresp="#s4:l2">doot</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Mais vous estes <rhyme label="c" type="m" corresp="#s4:l6">trompeurs</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt myn toch wyten <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">O alderliefste <rhyme label="b" type="m">myn</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Tgunt datter is <rhyme label="a" type="m" corresp="#s5:l1">geschiet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Door bedreegers <rhyme label="b" type="m" corresp="#s5:l2">fenyn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Want haer bewys en <rhyme label="b" type="m" corresp="#s5:l2">schyn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Nest point <rhyme label="c" type="m">resplendissant</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Tot eenigen <rhyme label="b" type="m" corresp="#s5:l2">termyn</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En mon coeur <rhyme label="c" type="m" corresp="#s5:l6">languissant</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hoe soudt ick weten <rhyme label="a" type="m">doch</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Of ghy Getrouwe <rhyme label="b" type="m">syt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Die soe weynich tyts <rhyme label="a" type="m" corresp="#s6:l1">noch</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Over myn hebt <rhyme label="b" type="m" corresp="#s6:l2">gevryt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ick most eerst hebben <rhyme label="b" type="m" corresp="#s6:l2">tyt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Pour me renere tout <rhyme label="c" type="m">seur</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Oft ghy oock niet en <rhyme label="b" type="m" corresp="#s6:l2">syt</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Cconaent aultres <rhyme label="c" type="m" corresp="#s6:l6">trompeurs</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Voorwaer eenich <rhyme label="a" type="m">Onrecht</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ick u wil geuen <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dat ghy van uwen <rhyme label="a" type="m" corresp="#s7:l1">knecht</rhyme></l>
        <l id="s7:l4" met="3" type="-+">De lieffde wilt prouven <rhyme label="b" type="m" corresp="#s7:l2">siet</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Maer daerom wilt toch <rhyme label="b" type="m" corresp="#s7:l2">niet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Estre trop <rhyme label="c" type="m">retardant</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Oft ghy brengt int <rhyme label="b" type="m" corresp="#s7:l2">verdriet</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Mon coeur <rhyme label="c" type="m" corresp="#s7:l6">tredanguissant</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Indiennen Gans <rhyme label="a" type="m">getrou</rhyme></l>
        <l id="s8:l2" met="3" type="-+">De lieffde in v <rhyme label="b" type="m">bevindt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Noyt was van eenige <rhyme label="a" type="m" corresp="#s8:l1">vrou</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Jongmans alsoo <rhyme label="b" type="m" corresp="#s8:l2">bemindt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Als ick tot v <rhyme label="b" type="m" corresp="#s8:l2">Gesindt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">En vous donnant mon <rhyme label="c" type="m">coeur</rhyme></l>
        <l id="s8:l7" met="3" type="-+">En v lieff sal syn <rhyme label="b" type="m" corresp="#s8:l2">verblint</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Sy vous nesties <rhyme label="c" type="m" corresp="#s8:l6">trompeur</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Noyt soo getrou en <rhyme label="a" type="m">was</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Piramus Thisbe <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Als ick v op dit <rhyme label="a" type="m" corresp="#s9:l1">pas</rhyme></l>
        <l id="s9:l4" met="3" type="-+">O lieffste lieff sal <rhyme label="b" type="m" corresp="#s9:l2">syn</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Helpt myn uyt alle <rhyme label="b" type="m" corresp="#s9:l2">pyn</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Vostre fidel <rhyme label="c" type="m">amant</rhyme></l>
        <l id="s9:l7" met="3" type="-+">En Geeft toch <rhyme label="b" type="m" corresp="#s9:l2">medecyn</rhyme></l>
