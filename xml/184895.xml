<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184895</id>
    <date>
      <year-start>1740</year-start>
      <year-end>1740</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Veel geluk o Isabella</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">TElt de Sterren van hier <rhyme label="a" type="f">boven</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Door geheel het <rhyme label="b" type="m">Firmament</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Telt al d'Engels, die Godt <rhyme label="a" type="f" corresp="#s1:l1">loven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Noch en sal u syn <rhyme label="b" type="m" corresp="#s1:l2">bekent</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Hoe lanck wesen sal naer <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Die lanckdurend' <rhyme label="d" type="m">Eeuwigheydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Voeght daer by al de <rhyme label="a" type="f">coleuren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Van de Bloemen in de <rhyme label="b" type="m">Lent</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Al de Kruyden met hun <rhyme label="a" type="f" corresp="#s2:l1">geuren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Noch en sal u syn <rhyme label="b" type="m" corresp="#s2:l2">bekent</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Hoe lanck wesen sal naer <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Die lanckdurend' <rhyme label="d" type="m">Eeuwigheydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Telt de Graenen, ende <rhyme label="a" type="f">hayren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Water-druppels, Hagels, <rhyme label="b" type="m">Sant</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Doet de vlocken sneeuw <rhyme label="a" type="f" corresp="#s3:l1">vergaeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Noch en vat gy met <rhyme label="b" type="m" corresp="#s3:l2">t'verstandt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Hoe lanck wesen sal naer <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Die lanckdurend' <rhyme label="d" type="m">Eeuwigheydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Voeght daer by al d'Hemels <rhyme label="a" type="f">Geesten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Stofkens, die men vliegen <rhyme label="b" type="m">siet</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Al de menschen, en de <rhyme label="a" type="f" corresp="#s4:l1">beesten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Uw verstandt en vat noch <rhyme label="b" type="m" corresp="#s4:l2">niet</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Hoe lanck wesen sal naer <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Die lanckdurend' <rhyme label="d" type="m">Eeuwigheydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
