  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">In mynen sin so heb Ick voorkoeren <rhyme label="a" type="m">saen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">een exselent seer Jent <rhyme label="b" type="m">bekent</rhyme></l>
        <l id="s1:l3" met="3" type="-+">blomke seer wel <rhyme label="a" type="m" corresp="#s1:l1">gedaen</rhyme></l>
        <l id="s1:l4" met="5" type="-+">om die eedele goddin soe grip Ick moet <rhyme label="a" type="m" corresp="#s1:l1">voertsen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">als Ick my went ontrent <rhyme label="b" type="m" corresp="#s1:l2">preesent</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat vrouwelicke <rhyme label="a" type="m" corresp="#s1:l1">graen</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Om haer soe moet Ick my <rhyme label="c" type="f">voerblyden</rhyme></l>
        <l id="s1:l8" met="4" type="-+">oeck doet sy my veel <rhyme label="c" type="f" corresp="#s1:l7">lyden</rhyme></l>
        <l id="s1:l9" met="3" type="-+">sy voerhuecht myn <rhyme label="d" type="m">hart</rhyme></l>
        <l id="s1:l10" met="3" type="-+">oeck doet sy myn groet <rhyme label="d" type="m" corresp="#s1:l9">smert</rhyme></l>
        <l id="s1:l11" met="4" type="-+">om haer soe maeck Ick noch <rhyme label="e" type="m">voerwaer</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Want voer en naer eenpaer seer <rhyme label="e" type="m" corresp="#s1:l11">klaer</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Is sy die liefste <rhyme label="e" type="m" corresp="#s1:l11">daer</rhyme></l>
        <l id="s1:l14" met="3" type="-+">seer swaer met druck <rhyme label="e" type="m" corresp="#s1:l11">geuaer</rhyme></l>
        <l id="s1:l15" met="4" type="-+">treckt met onwser voer haer <rhyme label="e" type="m" corresp="#s1:l11">aldaer</rhyme></l>
        <l id="s1:l16" met="3" type="-+">ende uwe getrouwe <rhyme label="e" type="m" corresp="#s1:l11">dienaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Aen dit schoen beelt heb Ick gewonne met <rhyme label="a" type="m">lust</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Dat druok voerJaecht gewsecht <rhyme label="b" type="m">behaecht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">oek als myn tserten <rhyme label="a" type="m" corresp="#s2:l1">lust</rhyme></l>
        <l id="s2:l4" met="5" type="-+">sy die my stelt het herte met <rhyme label="a" type="m" corresp="#s2:l1">onrust</rhyme></l>
        <l id="s2:l5" met="4" type="-+">twelck ter eeren waecht voersaecht <rhyme label="b" type="m" corresp="#s2:l2">beclaecht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">dat het niet wert <rhyme label="a" type="m" corresp="#s2:l1">gedoechde</rhyme></l>
        <l id="s2:l7" met="4" type="+-">gelyck veenes paerys <rhyme label="c" type="f">beloefde</rhyme></l>
        <l id="s2:l8" met="4" type="-+">heeleenae sy die uit grieken <rhyme label="c" type="f" corresp="#s2:l7">roofde</rhyme></l>
        <l id="s2:l9" met="3" type="-+">hoep dat dat <rhyme label="d" type="m">Jeweel</rhyme></l>
        <l id="s2:l10" met="3" type="-+">sal syn myn beste <rhyme label="d" type="m" corresp="#s2:l9">deel</rhyme></l>
        <l id="s2:l11" met="4" type="-+">seer reyn talder liefete <rhyme label="e" type="m">grein</rhyme></l>
        <l id="s2:l12" met="4" type="-+">gy syt sertein Int plein <rhyme label="e" type="m" corresp="#s2:l11">alleyn</rhyme></l>
        <l id="s2:l13" met="3" type="-+">die schoenste roese <rhyme label="e" type="m" corresp="#s2:l11">maerein</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Gedaen oeck met <rhyme label="e" type="m" corresp="#s2:l11">gemein</rhyme></l>
        <l id="s2:l15" met="4" type="-+">heb Ick serteyn onrein Int <rhyme label="e" type="m" corresp="#s2:l11">klein</rhyme></l>
        <l id="s2:l16" met="3" type="-+">geeleeden als <rhyme label="e" type="m" corresp="#s2:l11">vyoeleyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Haer weesen fier haer aemoerues <rhyme label="a" type="m">gesicht</rhyme></l>
        <l id="s3:l2" met="4" type="-+">haer meede persoen Idoen seer <rhyme label="b" type="m">schoen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">heeft myn Ionck hert <rhyme label="a" type="m" corresp="#s3:l1">verhuecht</rhyme></l>
        <l id="s3:l4" met="5" type="-+">hoert sulck bestier der liefden brant <rhyme label="a" type="m" corresp="#s3:l1">genuecht</rhyme></l>
        <l id="s3:l5" met="4" type="-+">spant sonder hoen die kroen ten <rhyme label="b" type="m" corresp="#s3:l2">toen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">gy schoen veenes <rhyme label="a" type="m" corresp="#s3:l1">nicht</rhyme></l>
        <l id="s3:l7" met="4" type="+-">schoender men noeit met oegen <rhyme label="c" type="f">aensach</rhyme></l>
        <l id="s3:l8" met="4" type="-+">dan die myn naer haer doet <rhyme label="c" type="f" corresp="#s3:l7">Jagen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">want haer schoene <rhyme label="d" type="m">aenschin</rhyme></l>
        <l id="s3:l10" met="3" type="-+">blinck als auroerae <rhyme label="d" type="m" corresp="#s3:l9">fyn</rhyme></l>
        <l id="s3:l11" met="4" type="-+">vaeliant goddin <rhyme label="e" type="m">tryonfant</rhyme></l>
        <l id="s3:l12" met="4" type="-+">Gy hebt geplant myn brant <rhyme label="e" type="m" corresp="#s3:l11">plaesant</rhyme></l>
        <l id="s3:l13" met="3" type="-+">en myn <rhyme label="e" type="m" corresp="#s3:l11">dieaemant</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Onuerstant myn bedienster <rhyme label="e" type="m" corresp="#s3:l11">bant</rhyme></l>
        <l id="s3:l15" met="4" type="-+">Ick heel ontmant tverstant Int <rhyme label="e" type="m" corresp="#s3:l11">sant</rhyme></l>
        <l id="s3:l16" met="3" type="-+">Daer omme reick my u <rhyme label="e" type="m" corresp="#s3:l11">hant</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Dit myn eedel getrou <rhyme label="a" type="m">prinsesse</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Want myn sinne naeme van <rhyme label="b" type="m">binne</rhyme></l>
        <l id="s4:l3" met="3" type="-+">eerbaere fresse <rhyme label="a" type="m" corresp="#s4:l1">moet</rhyme></l>
        <l id="s4:l4" met="5" type="-+">suieuer kersou schoene maetresse <rhyme label="a" type="m" corresp="#s4:l1">soet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">laet voersinne koemen <rhyme label="b" type="m" corresp="#s4:l2">minnen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">u Juechdelycke <rhyme label="a" type="m" corresp="#s4:l1">bloet</rhyme></l>
        <l id="s4:l7" met="4" type="+-">godt hadt gy dus genouch <rhyme label="c" type="f">bequaem</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Dat wy als bykens swermde <rhyme label="c" type="f" corresp="#s4:l7">schoene</rhyme></l>
        <l id="s4:l9" met="3" type="-+">soe waer myn pyne geent <rhyme label="d" type="m">daer</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Ick nu om lyde <rhyme label="d" type="m" corresp="#s4:l9">torment</rhyme></l>
        <l id="s4:l11" met="4" type="-+">verhoert schoen lief met <rhyme label="e" type="m">ackoert</rhyme></l>
        <l id="s4:l12" met="4" type="-+">die leit versmoert verkoert <rhyme label="e" type="m" corresp="#s4:l11">vermoert</rhyme></l>
        <l id="s4:l13" met="3" type="-+">In liefde rechte voert <rhyme label="e" type="m" corresp="#s4:l11">antwoerdt</rhyme></l>
        <l id="s4:l14" met="3" type="-+">groet hem van nu <rhyme label="e" type="m" corresp="#s4:l11">voert</rhyme></l>
        <l id="s4:l15" met="4" type="-+">want orboert antwoert nae <rhyme label="e" type="m" corresp="#s4:l11">spoert</rhyme></l>
        <l id="s4:l16" met="3" type="-+">Ick ontfanck met <rhyme label="e" type="m" corresp="#s4:l11">ackoert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
