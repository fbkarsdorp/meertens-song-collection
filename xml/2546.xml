<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2546</id>
    <date>
      <year-start>1622</year-start>
      <year-end>1622</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>La foule d'Angleterre</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VVie sou hem niet <rhyme label="a" type="f">verblyen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In u aenschijn <rhyme label="b" type="m">verheught</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ick wilt voor u <rhyme label="a" type="f" corresp="#s1:l1">belyen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghy syt mijn troost, mijn <rhyme label="b" type="m" corresp="#s1:l2">Vreucht</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Mijn stam, mijn <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Mijn Afgodt <rhyme label="c" type="m" corresp="#s1:l5">teer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Mijn lust, en mijn <rhyme label="b" type="m" corresp="#s1:l2">geneucht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick sal voor waarheyt <rhyme label="a" type="f">houwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat mijn ghesicht u <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Och Lief stelt u <rhyme label="a" type="f" corresp="#s2:l1">betrouwen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op t'wanckelbaare <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Eer u <rhyme label="c" type="m">t'gheluck</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Bereyt veel <rhyme label="c" type="m" corresp="#s2:l5">druck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En my een swaer <rhyme label="b" type="m" corresp="#s2:l2">verdriet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat hert sou niet <rhyme label="a" type="f">beweeghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door u <rhyme label="b" type="m">mistroosticheyt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ick was stracx soo <rhyme label="a" type="f" corresp="#s3:l1">gheneeghen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En behulp'lijck <rhyme label="b" type="m" corresp="#s3:l2">bereyt</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">O <rhyme label="c" type="m">Jonghelingh</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Of ick schoon <rhyme label="c" type="m" corresp="#s3:l5">singh</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Mijn hart inwendigh <rhyme label="b" type="m" corresp="#s3:l2">schreyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'k Sal my te vreden <rhyme label="a" type="f">stellen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Nu ghy't op mijn <rhyme label="b" type="m">begheert</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Lief wilt u niet meer <rhyme label="a" type="f" corresp="#s4:l1">quellen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dewijl't mijn hartje <rhyme label="b" type="m" corresp="#s4:l2">deert</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Dat ghy om <rhyme label="c" type="m">myn</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">U bly <rhyme label="c" type="m" corresp="#s4:l5">aenschyn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Soo droeffelijck <rhyme label="b" type="m" corresp="#s4:l2">verkeert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy kent soo veel <rhyme label="a" type="f">uytbeelden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ja Schilderen int <rhyme label="b" type="m">claer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">U ghebreck, en u <rhyme label="a" type="f" corresp="#s5:l1">weelden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">U vreuchden, en <rhyme label="b" type="m" corresp="#s5:l2">misbaer</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Dat sie ick <rhyme label="c" type="m">licht</rhyme></l>
        <l id="s5:l6" met="2" type="-+">In u <rhyme label="c" type="m" corresp="#s5:l5">ghesicht</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En Spieghel my <rhyme label="b" type="m" corresp="#s5:l2">aldaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Had my niet seer <rhyme label="a" type="f">verdrooten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">U ellend' en u <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ghy had nu lang <rhyme label="a" type="f" corresp="#s6:l1">ghenooten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">De jonst al van mijn <rhyme label="b" type="m" corresp="#s6:l2">hart</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">U eel <rhyme label="c" type="m">ghemoet</rhyme></l>
        <l id="s6:l6" met="2" type="-+">En heusheyt <rhyme label="c" type="m" corresp="#s6:l5">doet</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dat ick gants moe <rhyme label="b" type="m" corresp="#s6:l2">werd'</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hertogh van myn <rhyme label="a" type="f">ghedachten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mijn Prins van mijn <rhyme label="b" type="m">ghemoet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Met u wil ick <rhyme label="a" type="f" corresp="#s7:l1">verwachten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Gheluck en <rhyme label="b" type="m" corresp="#s7:l2">teghenspoet</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Wat ons Godt <rhyme label="c" type="m">gunt</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Mijn Lief en <rhyme label="c" type="m" corresp="#s7:l5">Vrunt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Het sy dan suer oft <rhyme label="b" type="m" corresp="#s7:l2">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
