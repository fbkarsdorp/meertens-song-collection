<song>
  <meta>
    <id>106512</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Petit Bordeaux</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ONlangs wou ick om wat <rhyme label="a" type="f">Baarsjes</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">By een Dorp niet hart van <rhyme label="b" type="m">Stee</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">'k Maackte Hengel, Sim // en <rhyme label="a" type="f" corresp="#s1:l1">Laarsjes</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Canis, <rhyme label="b" type="m" corresp="#s1:l2">Worm-Schoetjeree</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En 'k ginch stijfjes hene <rhyme label="c" type="f">stappen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Met een pijpje in men <rhyme label="d" type="m">mondt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">'k Wou een soopje laaten <rhyme label="c" type="f" corresp="#s1:l5">tappen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Maar ick niemant op een <rhyme label="d" type="m" corresp="#s1:l6">vondt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Huylde van een Boere <rhyme label="a" type="f">besje</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Een kleijn Schuytje, 'k kost wat <rhyme label="b" type="m">bier</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Met een kooltje in een <rhyme label="a" type="f" corresp="#s2:l1">tesje</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Om gestaag, te hebben <rhyme label="b" type="m" corresp="#s2:l2">vier</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Hier me voer ick deur een <rhyme label="c" type="f">Slootje</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Tot in bol gebaggert <rhyme label="d" type="m">nat</rhyme></l>
        <l id="s2:l7" met="4" type="+-">'k Peylde strackjes men <rhyme label="c" type="f" corresp="#s2:l5">Lootje</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Of de Dobbe diepte <rhyme label="d" type="m" corresp="#s2:l6">had</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Streeck men Pen, 'k besloech men <rhyme label="a" type="f">Haeckje</rhyme></l>
        <l id="s3:l2" met="4" type="+-">'k Kreeg een Noopje, maer 't was <rhyme label="b" type="m">mis</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">'k Sey get Diefje 'k wet ick <rhyme label="a" type="f" corresp="#s3:l1">raeckje</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Soo men Wurm maar taaytjes <rhyme label="b" type="m" corresp="#s3:l2">is</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ey daer isset dats een <rhyme label="c" type="f">Quantje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dats een Baarsje als een <rhyme label="d" type="m">ping</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">'t Moet al even wel in 't <rhyme label="c" type="f" corresp="#s3:l5">Mantje</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Of ick noch wat grooter <rhyme label="d" type="m" corresp="#s3:l6">ving</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'t Was gedaan geen Visie <rhyme label="a" type="f">hapte</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'k Roeyde, na een ander <rhyme label="b" type="m">kant</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Daar een Meysje Turrif <rhyme label="a" type="f" corresp="#s4:l1">trapte</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Op het uyt-ghetrocke <rhyme label="b" type="m" corresp="#s4:l2">Lant</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">'k Stont en keeck staag na haar <rhyme label="c" type="f">Treeties</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">(Dat ick 't Vissen schier <rhyme label="d" type="m">vergat</rhyme>)</l>
        <l id="s4:l7" met="4" type="+-">Hoese doude met 'er <rhyme label="c" type="f" corresp="#s4:l5">Leeties</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Hoese draeyde <rhyme label="d" type="m" corresp="#s4:l6">metter</rhyme> ....</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Sprack wel Troosie hoe dus <rhyme label="a" type="f">druckjes</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Heer wat doeie <rhyme label="b" type="m">Arrebeyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om de Kluyties, Flodder <rhyme label="a" type="f" corresp="#s5:l1">stuckies</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Rust een lutje ware <rhyme label="b" type="m" corresp="#s5:l2">Meyt</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Lusie niet een reijs te <rhyme label="c" type="f">Drincken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">'k Heb nogh Biertie in men <rhyme label="d" type="m">Kruyck</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Langie potie 'k sel wat <rhyme label="c" type="f" corresp="#s5:l5">schincken</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Nou ey laptet inje <rhyme label="d" type="m" corresp="#s5:l6">Buyck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'t Meysie liet haar wel <rhyme label="a" type="f">geseggen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ick ginch by 'er sitten <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">'k Liet mijn Hengel Tuychie <rhyme label="a" type="f" corresp="#s6:l1">leggen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">'k Docht niet eens naa't Vissen <rhyme label="b" type="m" corresp="#s6:l2">meer</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Doe geraackte wy aent <rhyme label="c" type="f">malle</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Na het Drincken heel en <rhyme label="d" type="m">half</rhyme></l>
        <l id="s6:l7" met="4" type="+-">'k Kittelde haer <rhyme label="c" type="f" corresp="#s6:l5">Mamme-Balle</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Datse quijlde als een <rhyme label="d" type="m" corresp="#s6:l6">Kalf</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Na dit koddich ginne <rhyme label="a" type="f">gappen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Sey zy och gy doetme <rhyme label="b" type="m">seer</rhyme></l>
        <l id="s7:l3" met="4" type="+-">'k Moet mijn Stickie of gaen <rhyme label="a" type="f" corresp="#s7:l1">trappen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Keert gy aanie Vissen <rhyme label="b" type="m" corresp="#s7:l2">weer</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Maar soo haest ick weer als <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Teech na groene <rhyme label="d" type="m">Kruycke-blaan</rhyme></l>
        <l id="s7:l7" met="4" type="+-">Was mijn Worm-Slof <rhyme label="c" type="f" corresp="#s7:l5">verlooren</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En ick bleef verlegen <rhyme label="d" type="m" corresp="#s7:l6">staan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">'k Draachdent Sloofte, maar het <rhyme label="a" type="f">Slutie</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Sey, ten wasser niet <rhyme label="b" type="m">estort</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">'k Sey nou kom soeckt mee een <rhyme label="a" type="f" corresp="#s8:l1">lutie</rhyme></l>
        <l id="s8:l4" met="4" type="+-">'k Loof ie hebtet in ie <rhyme label="b" type="m" corresp="#s8:l2">Schort</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Wel bevoeltme, sey zy <rhyme label="c" type="f">strackies</rhyme></l>
        <l id="s8:l6" met="4" type="+-">'k Hebbet zeker niet <rhyme label="d" type="m">gesien</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Ick deur-sochter Boere <rhyme label="c" type="f" corresp="#s8:l5">packies</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Daar 't ick vondt, by bey'er <rhyme label="d" type="m" corresp="#s8:l6">knien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
