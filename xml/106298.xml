<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>106298</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Krelis zei zij Krelis kind</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EY wat isser al te <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Om het lecker Eytje van het <rhyme label="a" type="m" corresp="#s1:l1">Hoen</rhyme>?</l>
        <l id="s1:l3" met="2" type="+-">Al de <rhyme label="b" type="m">Dis</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wanneer heht Paessen <rhyme label="b" type="m" corresp="#s1:l3">is</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die raeckt dan aen een <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">En 't Ey in <rhyme label="c" type="m" corresp="#s1:l5">d'hand</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'Teyrie is ze wonder <rhyme label="a" type="m">eel</rhyme></l>
        <l id="s2:l2" met="5" type="+-">'Tsmaeckt bekans een voet diep door de <rhyme label="a" type="m" corresp="#s2:l1">keel</rhyme></l>
        <l id="s2:l3" met="2" type="+-">Ey het <rhyme label="b" type="m">Ey</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Soo lestmael Pietje <rhyme label="b" type="m" corresp="#s2:l3">sey</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Verquickt een yder <rhyme label="c" type="m">een</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Meer als <rhyme label="c" type="m" corresp="#s2:l5">gemeen</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">T'eytie is van grote <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s3:l2" met="5" type="+-">'tHeefter menigh op de been <rhyme label="a" type="m" corresp="#s3:l1">gebracht</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">En doen <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gelijck een Jongen <rhyme label="b" type="m" corresp="#s3:l3">Haen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Al was hy schier <rhyme label="c" type="m">verdooft</rhyme>;</l>
        <l id="s3:l6" met="2" type="-+">En af <rhyme label="c" type="m" corresp="#s3:l5">geslooft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ey dan nieuw getrouwde <rhyme label="a" type="m">lien</rhyme>,</l>
        <l id="s4:l2" met="5" type="+-">Wilt de mond een pittigh Eytie <rhyme label="a" type="m" corresp="#s4:l1">bien</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Spoet uw <rhyme label="b" type="m">dra</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Eet Eyer-koeck en <rhyme label="b" type="m" corresp="#s4:l3">Vla</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Backt Struyffies bol en <rhyme label="c" type="m">vet</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Soo meughie <rhyme label="c" type="m" corresp="#s4:l5">bet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Eyer-kost is uw meer <rhyme label="a" type="m">waert</rhyme>,</l>
        <l id="s5:l2" met="5" type="+-">Als een ruyter 'tbeste <rhyme label="a" type="m" corresp="#s5:l1">Paert</rhyme>:</l>
        <l id="s5:l3" met="2" type="+-">Houdie aen't <rhyme label="b" type="m">nest</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Het isse verre <rhyme label="b" type="m" corresp="#s5:l3">best</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Soo komter nu en <rhyme label="c" type="m">dan</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">En Kuycke <rhyme label="c" type="m" corresp="#s5:l5">van</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
