    <melody>Moeder geef mij een man</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MY dunkt ik ben oud <rhyme label="a" type="m">genoeg</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">'k Ben al agtien <rhyme label="b" type="f">Jaaren</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">'t Was myn Moêr altyd te <rhyme label="a" type="m" corresp="#s1:l1">vroeg</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als ik sprak van <rhyme label="b" type="f" corresp="#s1:l2">Paaren</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Maar het sjert 'er zo na <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'k Ben myn eenzaam leeven <rhyme label="c" type="m" corresp="#s1:l5">moê</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">'k Wil een <rhyme label="d" type="m">Man</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Die wat <rhyme label="d" type="m" corresp="#s1:l7">kan</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Zulken vreugd daar hou ik <rhyme label="d" type="m" corresp="#s1:l7">van</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">'k Laat myn Maagdom <rhyme label="b" type="f" corresp="#s1:l2">vaaren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ik ben in myn besten <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Om wat vreugd te <rhyme label="b" type="f">rapen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ja dat ik maar wierd <rhyme label="a" type="m" corresp="#s2:l1">gevryd</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">'k Zou my straks <rhyme label="b" type="f" corresp="#s2:l2">vergapen</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Is het niet een groot <rhyme label="c" type="m">verdriet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat 'er niemand na myn <rhyme label="c" type="m" corresp="#s2:l5">ziet</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Want een <rhyme label="d" type="m">Maagd</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Word <rhyme label="d" type="m" corresp="#s2:l7">geplaagd</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Den eersten die het my maar <rhyme label="d" type="m" corresp="#s2:l7">vraagd</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Die zal by my <rhyme label="b" type="f" corresp="#s2:l2">slapen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Toen kwam daar een Kalis <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Wel-gemaakt van <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hy was netjes <rhyme label="a" type="m" corresp="#s3:l1">aangedaan</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Vol <rhyme label="b" type="f" corresp="#s3:l2">bevalligheden</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">'k Kon wel zien aan zyn <rhyme label="c" type="m">gelaat</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat hy was van laagen <rhyme label="c" type="m" corresp="#s3:l5">staat</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-"><rhyme label="d" type="m">Niettemin</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Had hy iets <rhyme label="d" type="m" corresp="#s3:l7">in</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Dat my wel was na de <rhyme label="d" type="m" corresp="#s3:l7">zin</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">'k Was met hem te <rhyme label="b" type="f" corresp="#s3:l2">vreden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maar nu pronkt die loozen <rhyme label="a" type="m">Guit</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Schier geheele <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Hy 's altyd ter deuren <rhyme label="a" type="m" corresp="#s4:l1">uit</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Moet ik dat <rhyme label="b" type="f" corresp="#s4:l2">verdragen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">En in plaats dat hy wat <rhyme label="c" type="m">zou</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Speelen met zyn Jonge <rhyme label="c" type="m" corresp="#s4:l5">Vrouw</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Bruid hy <rhyme label="d" type="m">uit</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Deze <rhyme label="d" type="m" corresp="#s4:l7">Guit</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Ben ik dan niet wel <rhyme label="d" type="m" corresp="#s4:l7">gebruid</rhyme>:</l>
        <l id="s4:l10" met="3" type="+-">'k Zal hem ook wel <rhyme label="b" type="f" corresp="#s4:l2">plagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ik zal zoeken beter <rhyme label="a" type="m">kans</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Om myn lust te <rhyme label="b" type="f">boeten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">'k Zal hem komen met een <rhyme label="a" type="m" corresp="#s5:l1">krans</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Van twee hoorens <rhyme label="b" type="f" corresp="#s5:l2">groeten</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'k Zal dan zeggen, hoor myn <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Sjerje myn ik sjerje <rhyme label="c" type="m" corresp="#s5:l5">weêr</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Weetje <rhyme label="d" type="m">wel</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Stout <rhyme label="d" type="m" corresp="#s5:l7">gezel</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Die zyn Vrouw brengt in 't <rhyme label="d" type="m" corresp="#s5:l7">geknel</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Zy zo varen <rhyme label="b" type="f" corresp="#s5:l2">moeten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
