    <melody>Laatst was hier een reizer</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WEl Melis hoe gaet <rhyme label="a" type="f">het</rhyme>?</l>
        <l id="s1:l2" met="2" type="-+">Wel Bruygom hoe staet <rhyme label="a" type="f" corresp="#s1:l1">het</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Hoe Knelis, wel mienje <rhyme label="b" type="m">mijn</rhyme>?</l>
        <l id="s1:l4" met="2" type="-+">Ick Bruygom te <rhyme label="c" type="f">heeten</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En dat niet te <rhyme label="c" type="f" corresp="#s1:l4">weten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn dunckt het kander niet <rhyme label="b" type="m" corresp="#s1:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Doch seght men een <rhyme label="a" type="f">reysje</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Met wat voor een <rhyme label="a" type="f" corresp="#s2:l1">Meysje</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Of hebje dat niet <rhyme label="b" type="m">ghehoort</rhyme>?</l>
        <l id="s2:l4" met="2" type="-+">‘t Is Kniertje tot <rhyme label="c" type="f">Trijntjes</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Iy klaert het soo <rhyme label="c" type="f" corresp="#s2:l4">fijntjes</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ick weet het van woordt tot <rhyme label="b" type="m" corresp="#s2:l3">woordt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Stoffeert soo de <rhyme label="a" type="f">leughen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Ick mach ‘er wel <rhyme label="a" type="f" corresp="#s3:l1">teughen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Neen seecker het waerder mijn <rhyme label="b" type="m">leet</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Ick weet wel, men <rhyme label="c" type="f">queldtme</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Maer Melis <rhyme label="c" type="f" corresp="#s3:l4">verteldtme</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hoe komt dat jyder dit <rhyme label="b" type="m" corresp="#s3:l3">weet</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Lest was ick in’t <rhyme label="a" type="f">Haentje</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">‘k Verteerden een <rhyme label="a" type="f" corresp="#s4:l1">Vaentje</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Daer raeckt’ ick by een <rhyme label="b" type="m">Ghesel</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Die by de <rhyme label="c" type="f">Blomisten</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Sen goedtje <rhyme label="c" type="f" corresp="#s4:l4">verquisten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die meende hy wistet <rhyme label="b" type="m" corresp="#s4:l3">wel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Hoe was hy <rhyme label="a" type="f">gheheeten</rhyme>?</l>
        <l id="s5:l2" met="2" type="-+">Dat is mijn <rhyme label="a" type="f" corresp="#s5:l1">vergheten</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Ick segh jou Knelis, siet <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Waer ‘t dat ick hem <rhyme label="c" type="f">kende</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">‘k Sou mijnder <rhyme label="c" type="f" corresp="#s5:l4">aenschenden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">‘t Was eveleens wie ‘t dan <rhyme label="b" type="m" corresp="#s5:l3">waer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Houdt Melis, <rhyme label="a" type="f">beraeje</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Wat seecker, hoe <rhyme label="a" type="f" corresp="#s6:l1">staeje</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ick gae, kom wilje <rhyme label="b" type="m">mee</rhyme>?</l>
        <l id="s6:l4" met="2" type="-+">Wy sellen tot <rhyme label="c" type="f">Slincken</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Een Biertje maer <rhyme label="c" type="f" corresp="#s6:l4">drincken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En daer mee weer nae <rhyme label="b" type="m" corresp="#s6:l3">Stee</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Kees had ick jou <rhyme label="a" type="f">Duyten</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Soo quam ick wel <rhyme label="a" type="f" corresp="#s7:l1">buyten</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">‘k Loof Melis niet datje nou <rhyme label="b" type="m">bluft</rhyme>?</l>
        <l id="s7:l4" met="2" type="-+">Iy plachter voor <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Gheen breeckspul te <rhyme label="c" type="f" corresp="#s7:l4">wesen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Wat scheeldje dat jyder nou <rhyme label="b" type="m" corresp="#s7:l3">suft</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Mijn Buydel die leegh <rhyme label="a" type="f">is</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Doet dat het gheen deegh <rhyme label="a" type="f" corresp="#s8:l1">is</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Jy weet niet watter mijn <rhyme label="b" type="m">schort</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Mijn is met de <rhyme label="c" type="f">Blommen</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Wat over <rhyme label="c" type="f" corresp="#s8:l4">ghekommen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">O Knelis! dat houdtme so <rhyme label="b" type="m" corresp="#s8:l3">kort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Nou Melis dat ‘s <rhyme label="a" type="f">over</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Daer wasser noch <rhyme label="a" type="f" corresp="#s9:l1">grover</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">De Koomenschap is tot <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Kom, kom Melis <rhyme label="c" type="f">Claessen</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">‘t Is altijdt gheen <rhyme label="c" type="f" corresp="#s9:l4">Paessen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Wie weet of ‘t weer <rhyme label="b" type="m" corresp="#s9:l3">geschiedt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Wel of wy dan <rhyme label="a" type="f">beyde</rhyme></l>
        <l id="s10:l2" met="2" type="-+">Een Biertjen aen <rhyme label="a" type="f" corresp="#s10:l1">leyde</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Wat houwje de fraeyste <rhyme label="b" type="m">plaets</rhyme>?</l>
        <l id="s10:l4" met="2" type="-+">In’t Vlissingher <rhyme label="c" type="f">wapen</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Daer vindje <rhyme label="c" type="f" corresp="#s10:l4">rechtschapen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Veel Borsten en kluchtighe <rhyme label="b" type="m" corresp="#s10:l3">Maets</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Dat’s in’t <rhyme label="a" type="f">Wijngaert-straetje</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Gut Knelis hoe <rhyme label="a" type="f" corresp="#s11:l1">praetje</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Kom, kom, dit moeten wy <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Ick hoor datser <rhyme label="c" type="f">sinne</rhyme></l>
        <l id="s11:l5" met="2" type="-+">Nou Melis treedt <rhyme label="c" type="f" corresp="#s11:l4">binnen</rhyme>.</l>
        <l id="s11:l6" met="3" type="-+">Neen Knelis, d’oudste gaet <rhyme label="b" type="m" corresp="#s11:l3">veur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
