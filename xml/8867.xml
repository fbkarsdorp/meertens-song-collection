<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8867</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>O Herr Gott begnade mich</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEER GOdt mijn Heylandt hoort doch <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Want Dach end' Nacht ick Roep tot <rhyme label="a" type="m" corresp="#s1:l1">dy</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wilt mijn Ghebedt <rhyme label="b" type="f">verhooren</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Neyghen tot my dijn <rhyme label="b" type="f" corresp="#s1:l3">Ooren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Want mijn Siel is vol Lijdens <rhyme label="c" type="m">Noot</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Mijn Leven is na by den <rhyme label="c" type="m" corresp="#s1:l5">Doot</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Ick ben gheleken <rhyme label="d" type="f">welle</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dien, die varen ter <rhyme label="d" type="f" corresp="#s1:l7">Helle</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Als die Berooft van Hulpe <rhyme label="e" type="m">gaer</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Ick ligghe by der Dooden <rhyme label="e" type="m" corresp="#s1:l9">schaer</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Als offt ick oock gheslaghen <rhyme label="f" type="m">waer</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">End' laegh' al in den Grave <rhyme label="f" type="m" corresp="#s1:l11">daer</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Gheheel end' al <rhyme label="g" type="f">Verlaten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daer aen ghy niet ghedencket <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Maer syn van dy Verstooten <rhyme label="a" type="m" corresp="#s2:l1">seer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ghy hebt my in die <rhyme label="b" type="f">Cuyle</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Verworpen diep end' <rhyme label="b" type="f" corresp="#s2:l3">Vuyle</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die Duysternis heeft my <rhyme label="c" type="m">bedeckt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">End' dijnen Grim my harde <rhyme label="c" type="m" corresp="#s2:l5">druckt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Met alle dijne <rhyme label="d" type="f">Vloede</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dringt ghy my also <rhyme label="d" type="f" corresp="#s2:l7">Woede</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Mijn Vrienden hebt ghy wech <rhyme label="e" type="m">ghedaen</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Dat sy nu verre van my <rhyme label="e" type="m" corresp="#s2:l9">staen</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Een grouwel ben ick worden <rhyme label="f" type="m">haer</rhyme>,</l>
        <l id="s2:l12" met="4" type="-+">End' liggh' also ghevangen <rhyme label="f" type="m" corresp="#s2:l11">swaer</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Dat gheen wtcomen <rhyme label="g" type="f">schijnet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn Aensicht Jamerlijck <rhyme label="a" type="m">vergaet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Midts mijnen seer Ellenden <rhyme label="a" type="m" corresp="#s3:l1">staet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">HEEr Daeg'lijcx ick <rhyme label="b" type="f">Aenroepe</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En voud' mijn Hand' te <rhyme label="b" type="f" corresp="#s3:l3">hoope</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Wilt ghy dan wonder Teecken <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Bewijsen in des Dooden <rhyme label="c" type="m" corresp="#s3:l5">Baen</rhyme>?</l>
        <l id="s3:l7" met="3" type="-+">Offt sal men wt den <rhyme label="d" type="f">graven</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Verrijsen? End' dy <rhyme label="d" type="f" corresp="#s3:l7">Loven</rhyme>?</l>
        <l id="s3:l9" met="4" type="-+">Moghen dan worden wel <rhyme label="e" type="m">bekant</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Dijn wonder Daed' int Duyster <rhyme label="e" type="m" corresp="#s3:l9">Lant</rhyme>?</l>
        <l id="s3:l11" met="4" type="+-">Dijn goetheyt en dijn Waerheyt <rhyme label="f" type="m">vroom</rhyme></l>
        <l id="s3:l12" met="4" type="-+">In graven end' verderff al <rhyme label="f" type="m" corresp="#s3:l11">om</rhyme>?</l>
        <l id="s3:l13" met="3" type="-+">Daermen doch niet <rhyme label="g" type="f">Vercondet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer Heer ick schrey, end' Aenroep <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Waerom verstoot mijn siele <rhyme label="a" type="m" corresp="#s4:l1">ghy</rhyme>?</l>
        <l id="s4:l3" met="3" type="-+">Verbercht my dijn <rhyme label="b" type="f">Aensichte</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">End' Troostet my oock <rhyme label="b" type="f" corresp="#s4:l3">Nichte</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Elendich end' in Onmacht <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Ben ick, want ghy my so <rhyme label="c" type="m" corresp="#s4:l5">verstoot</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ick lijd' dijns Schrickens <rhyme label="d" type="f">seere</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dat ick vertzaegh O <rhyme label="d" type="f" corresp="#s4:l7">Heere</rhyme>.</l>
        <l id="s4:l9" met="4" type="-+">Dijn Grim so swaer my over <rhyme label="e" type="m">gaet</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Dijn schricken druckt my boven <rhyme label="e" type="m" corresp="#s4:l9">Maet</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">De Vyanden my all' <rhyme label="f" type="m">ghelijck</rhyme>,</l>
        <l id="s4:l12" met="4" type="-+">Daegh'lijcx Bevechten <rhyme label="f" type="m" corresp="#s4:l11">Vinnichlijck</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Als Water vloet <rhyme label="g" type="f">Onstuere</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy maeckt met dese Plage <rhyme label="a" type="m">swaer</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Dat mijne Vrienden <rhyme label="a" type="m" corresp="#s5:l1">Allegaer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">End' oock all' mijne <rhyme label="b" type="f">Naesten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Haer Verre van my <rhyme label="b" type="f" corresp="#s5:l3">haesten</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">All' dien ick was so wel <rhyme label="c" type="m">Bekant</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">End' sy my waren nae <rhyme label="c" type="m" corresp="#s5:l5">verwant</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die staen nu al van <rhyme label="d" type="f">Verre</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">End' syn my Vreemt so <rhyme label="d" type="f" corresp="#s5:l7">seere</rhyme>.</l>
        <l id="s5:l9" met="4" type="-+">Wat is die saeck O HEERE <rhyme label="e" type="m">Godt</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Dat ick nu ben des Vyandts <rhyme label="e" type="m" corresp="#s5:l9">spot</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Verlaten oock van <rhyme label="f" type="m">Alleman</rhyme>?</l>
        <l id="s5:l12" met="4" type="-+">Hebb' ick doch haer geen quaet <rhyme label="f" type="m" corresp="#s5:l11">ghedaen</rhyme>,</l>
        <l id="s5:l13" met="3" type="-+">Tis al om dees <rhyme label="g" type="f">Ellende</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
