  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SChoon Lief ghy zijt mijn troost <rhyme label="a" type="f">alleyne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Vercoren boven <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Vercoren boven <rhyme label="b" type="m" corresp="#s1:l2">al</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ghy spijst Ziel en lijf in't <rhyme label="a" type="f" corresp="#s1:l1">gemeyne</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hier in dit Aerdtsche <rhyme label="b" type="m" corresp="#s1:l2">dal</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hier in dit Aerdtsche <rhyme label="b" type="m" corresp="#s1:l2">dal</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dese nijders <rhyme label="b" type="m" corresp="#s1:l2">geschal</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">met hare boose <rhyme label="c" type="f">treken</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Sy beletten my mijn Lief te <rhyme label="c" type="f" corresp="#s1:l8">spreken</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Sy beletten my mijn Lief te <rhyme label="c" type="f" corresp="#s1:l8">spreken</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">T'welck ick my wel beclaghen <rhyme label="b" type="m" corresp="#s1:l2">sal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Schoon lief wat soudt my moghen <rhyme label="a" type="f">baten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ter weerelt menich <rhyme label="b" type="m">pont</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ter weerelt menich <rhyme label="b" type="m" corresp="#s2:l2">pont</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En ick v dan sou moeten <rhyme label="a" type="f" corresp="#s2:l1">laten</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">So waer ick gants <rhyme label="b" type="m" corresp="#s2:l2">doorwont</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">So waer ick gants <rhyme label="b" type="m" corresp="#s2:l2">doorwont</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">In mijns Liefs rooder <rhyme label="b" type="m" corresp="#s2:l2">mondt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Is noyt bedroch <rhyme label="c" type="f">ghevonden</rhyme></l>
        <l id="s2:l9" met="4" type="-+">Dat mach ick liefken wel <rhyme label="c" type="f" corresp="#s2:l8">oorconden</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Dat mach ick liefken wel <rhyme label="c" type="f" corresp="#s2:l8">oorconden</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Alst v belieft maeckt my <rhyme label="b" type="m" corresp="#s2:l2">ghesont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Schriftuere lief, is uwen <rhyme label="a" type="f">name</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Al wort ghy seer <rhyme label="b" type="m">benijt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Al wort ghy seer <rhyme label="b" type="m" corresp="#s3:l2">benijt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">T'onrecht soud' ick't my moghen <rhyme label="a" type="f" corresp="#s3:l1">schame</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Want ghy die liefste <rhyme label="b" type="m" corresp="#s3:l2">zijt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Want ghy die liefste <rhyme label="b" type="m" corresp="#s3:l2">zijt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En sonder v <rhyme label="b" type="m" corresp="#s3:l2">jolijt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wat sal ick gaen <rhyme label="c" type="f">beginnen</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Helpt my schoon lief den strijt <rhyme label="c" type="f" corresp="#s3:l8">gewinnen</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Helpt my schoon lief den strijt <rhyme label="c" type="f" corresp="#s3:l8">gewinnen</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">In desen mijnen <rhyme label="b" type="m" corresp="#s3:l2">strijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Schoon lief ghy hadt my <rhyme label="a" type="f">wtvercoren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al eer ick v <rhyme label="b" type="m">vercoos</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Al eer ick v <rhyme label="b" type="m" corresp="#s4:l2">vercoos</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ghy socht my daer ick was <rhyme label="a" type="f" corresp="#s4:l1">verloren</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Daer ick lach <rhyme label="b" type="m" corresp="#s4:l2">troosteloos</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Daer ick lach <rhyme label="b" type="m" corresp="#s4:l2">troosteloos</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Alle mijn wonden <rhyme label="b" type="m" corresp="#s4:l2">boos</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Hebt ghy gheheel <rhyme label="c" type="f">ghenesen</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Met Wijn en Olie <rhyme label="c" type="f" corresp="#s4:l8">ghepresen</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Met Wijn en Olie <rhyme label="c" type="f" corresp="#s4:l8">ghepresen</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Dies ick v lief voor een ander <rhyme label="b" type="m" corresp="#s4:l2">vercoos</rhyme>.</l>
