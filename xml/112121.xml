    <melody>O doelwit van mijn min</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WEl Kloris hoe kan't <rhyme label="a" type="m">zyn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat gy zoo koel kunt <rhyme label="b" type="f">leven</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Daar ik die zoete <rhyme label="a" type="m" corresp="#s1:l1">pyn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die my uw oogjes <rhyme label="b" type="f" corresp="#s1:l2">geven</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Noch kan, nog wil <rhyme label="b" type="f" corresp="#s1:l2">weerstreven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Al schoon ik zagtjes <rhyme label="a" type="m" corresp="#s1:l1">quyn</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Kom myn Liefje, laat u <rhyme label="c" type="f">winnen</rhyme>.</l>
        <l id="s1:l8" met="4" type="+-">Gy zyt vrolyk, vry van <rhyme label="c" type="f" corresp="#s1:l7">Zinnen</rhyme>:</l>
        <l id="s1:l9" met="2" type="+-">En my <rhyme label="d" type="m">dunkt</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">En my dunkt die speelt met <rhyme label="e" type="m">glas</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Breekt ligt en quest zig <rhyme label="e" type="m" corresp="#s1:l10">ras</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Neen boefje, ik ken het <rhyme label="a" type="m">vuur</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Ik weet daar mee te <rhyme label="b" type="f">speelen</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">En vreeze geen <rhyme label="a" type="m" corresp="#s2:l1">quetsuur</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Gy zuld met al uw <rhyme label="b" type="f" corresp="#s2:l2">queelen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Behaagen nog <rhyme label="b" type="f" corresp="#s2:l2">verveelen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">'t Zy kort of lang van <rhyme label="a" type="m" corresp="#s2:l1">duur</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Ik vermaak my met de <rhyme label="c" type="f">klugtjes</rhyme>.</l>
        <l id="s2:l8" met="4" type="+-">Maar laat de <rhyme label="c" type="f" corresp="#s2:l7">Minnezugtjes</rhyme>.</l>
        <l id="s2:l9" met="2" type="+-">Zonder <rhyme label="d" type="m">dat</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Zonder dat daar door myn <rhyme label="e" type="m">hert</rhyme></l>
        <l id="s2:l11" met="4" type="+-">In 't minst bewogen <rhyme label="e" type="m" corresp="#s2:l10">werd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
