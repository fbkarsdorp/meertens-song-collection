      <tag translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Geen meerdere vreugde ter wereld en is</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ONtfermt v my God <rhyme label="a" type="f">heere</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick ben ghepijnt so <rhyme label="a" type="f" corresp="#s1:l1">seere</rhyme></l>
        <l id="s1:l3" met="3" type="-+">V minne eest diet mi <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gae ick tot uwer <rhyme label="c" type="f">schole</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Die werelt seyt dat ick <rhyme label="c" type="f" corresp="#s1:l4">dole</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Laet ick dat aertsche <rhyme label="b" type="m" corresp="#s1:l3">goet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Volghe ick v <rhyme label="a" type="f">leere</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die werelt versmaet mi so <rhyme label="a" type="f" corresp="#s2:l1">seere</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die ick noch laten <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">God de heere spreect daer <rhyme label="c" type="f">tegen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Die arme hebben dat <rhyme label="c" type="f" corresp="#s2:l4">leuen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daer om storte ick mijn <rhyme label="b" type="m" corresp="#s2:l3">bloet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoort / hoort alle die <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ick gheue v <rhyme label="a" type="f" corresp="#s3:l1">medecijn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wercter na dat is v <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ick was arm van <rhyme label="c" type="f">gheeste</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Suuer van herten reyn van <rhyme label="c" type="f" corresp="#s3:l4">vleesce</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Als een lam was ic <rhyme label="b" type="m" corresp="#s3:l3">gheuoet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wacht v die grote <rhyme label="a" type="f">blame</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die werelt van <rhyme label="a" type="f" corresp="#s4:l1">Adame</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Trede ick so snel onder de <rhyme label="b" type="m">voet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ick machs mi wel <rhyme label="c" type="f">scamen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Maer dat ick draghe die <rhyme label="c" type="f" corresp="#s4:l4">name</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat ick wil schinen <rhyme label="b" type="m" corresp="#s4:l3">goet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het is tijt dat ick <rhyme label="a" type="f">beghinne</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Te steruen minen <rhyme label="a" type="f" corresp="#s5:l1">sinne</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Die doot coemt <rhyme label="b" type="m">onuerhoet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Onse heere spreect van <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Hoort alle die mi <rhyme label="c" type="f" corresp="#s5:l4">minnen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Coemt in mijns vaders <rhyme label="b" type="m" corresp="#s5:l3">goet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
