    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Het voer een maagdje over Rijn</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ach met liefde gequelt soeck ick v <rhyme label="a" type="m">raet</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Raet / myn nu wel eer valt te <rhyme label="a" type="m" corresp="#s1:l1">laet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Laet myn niet heel <rhyme label="b" type="f">bederuen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Deruen / van trost doet <rhyme label="b" type="f" corresp="#s1:l3">steruen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sterff ick om v door lieffde <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Groot sal v pyn syn door v myn <rhyme label="a" type="m" corresp="#s2:l1">doot</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Doot gy v lyeff vol <rhyme label="b" type="f">trouwen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Trouwen sult gy veel <rhyme label="b" type="f" corresp="#s2:l3">rouwen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Rou sal van vlyen metter <rhyme label="a" type="m">spoet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Spoet gy te trosten myn <rhyme label="a" type="m" corresp="#s3:l1">gemoet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Moet v dienaer <rhyme label="b" type="f">ootmoedich</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Moedich maeck hem dooch <rhyme label="b" type="f" corresp="#s3:l3">spoedich</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Spoet v tot hem die ver <rhyme label="a" type="m">waecht</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Wacht v voor tonghen valsch vol <rhyme label="a" type="m" corresp="#s4:l1">pracht</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Pracht doet goet het hert <rhyme label="b" type="f">verkeeren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Keeren wilt nu met <rhyme label="b" type="f" corresp="#s4:l3">eeren</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Eert hem Princes die v <rhyme label="a" type="m">bemindt</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Mint hem die gy vol trouwen <rhyme label="a" type="m" corresp="#s5:l1">vint</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Vint gy hem vol trouw <rhyme label="b" type="f">gestadich</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Stadich syt myn <rhyme label="b" type="f" corresp="#s5:l3">genadich</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
