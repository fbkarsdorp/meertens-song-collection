<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>163022</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LIefhebbertjes en <rhyme label="a" type="f">aanschouwers</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wie gy ook <rhyme label="b" type="m">zyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hier hebje de <rhyme label="a" type="f" corresp="#s1:l1">bedriegery</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gaet vry by haer ter <rhyme label="b" type="m" corresp="#s1:l2">zy</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Want de eersten <rhyme label="c" type="m">brief</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">Die is voor uw myn <rhyme label="c" type="m" corresp="#s1:l5">Lief</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ik zal voor jou gaen <rhyme label="d" type="f">werken</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">So lang als ik <rhyme label="c" type="m" corresp="#s1:l5">Leef</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ionkman al uw <rhyme label="a" type="f">praeten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Og Liefje dat kan niet <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Myn Vader zal het nooit toe <rhyme label="a" type="f" corresp="#s2:l1">laten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Want dat was voor hem groote <rhyme label="b" type="m" corresp="#s2:l2">pyn</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Want den oude <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En zal het nooit <rhyme label="c" type="m" corresp="#s2:l5">toestaen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Daerom wilt <rhyme label="d" type="f">vertrekken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En na een ander <rhyme label="c" type="m" corresp="#s2:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik zal jou Vader wel <rhyme label="a" type="f">bepraeten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Liefje komt gy maer by <rhyme label="b" type="m">myn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ik en zal uw nooit <rhyme label="a" type="f" corresp="#s3:l1">verlaten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als gy maer getrouw zal <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Daerom gaet maer <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En weest wel te <rhyme label="c" type="m" corresp="#s3:l5">vreÃªn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ik zal uw wel <rhyme label="d" type="f">beminnen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Soo lang als ik <rhyme label="c" type="m" corresp="#s3:l5">leef</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ionkman al uw <rhyme label="a" type="f">praeten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat kan niet <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Myn Dogter zal nooit <rhyme label="a" type="f" corresp="#s4:l1">trouwen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Want ziet zy blyft by <rhyme label="b" type="m" corresp="#s4:l2">myn</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Want zo dat <rhyme label="c" type="m">geschied</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Soo leef ik in <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Daerom wilt <rhyme label="d" type="f">vetrekken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En gedenkt het <rhyme label="c" type="m" corresp="#s4:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wel Vader wat zyn dat voor <rhyme label="a" type="f">reden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat gy dat niet toe wilt <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Mankeert het aen de <rhyme label="a" type="f" corresp="#s5:l1">kleeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Of heb ik uw iets <rhyme label="b" type="m" corresp="#s5:l2">misdaen</rhyme>?</l>
        <l id="s5:l5" met="3" type="+-">Waer is dan uw <rhyme label="c" type="m">lief</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sy is tot uw <rhyme label="c" type="m" corresp="#s5:l5">gerief</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ik zal haer dan <rhyme label="d" type="f">trouwen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Voor myn Vrouw en <rhyme label="c" type="m" corresp="#s5:l5">Lief</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sy bloosde zaem van <rhyme label="a" type="f">Liefde</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Door een opregte <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Haer Siel te zamen <rhyme label="a" type="f" corresp="#s6:l1">griefde</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Naer weederzydsche <rhyme label="b" type="m" corresp="#s6:l2">zin</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Deed' hoog verheven <rhyme label="c" type="m">geesten</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Malkaer haer trouw <rhyme label="c" type="m" corresp="#s6:l5">bezweer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Weest vrolyk by dees <rhyme label="d" type="f">feeste</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Met u 't <rhyme label="c" type="m" corresp="#s6:l5">aldermeest</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nooit zag ik trouwer <rhyme label="a" type="f">Liefde</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Zo als ik leef op <rhyme label="b" type="m">aerd</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Gy zyt een schoone <rhyme label="a" type="f" corresp="#s7:l1">Flore</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En zyt wel Liefde <rhyme label="b" type="m" corresp="#s7:l2">waerd</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Speelt op Hobo en <rhyme label="c" type="m">Fluiten</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En op Instrumenten <rhyme label="c" type="m" corresp="#s7:l5">meer</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En wilt dit liet <rhyme label="d" type="f">besluiten</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Myn alderliefste <rhyme label="c" type="m" corresp="#s7:l5">Heer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
