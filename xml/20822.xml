<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20822</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>holy song</tag>
    </tags>
    <melody>Nadien uw goddelijkheid</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOmt tot den <rhyme label="a" type="m">Medicijn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die zijt vervult met <rhyme label="a" type="m" corresp="#s1:l1">pijn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Komt tot Sint Lucas <rhyme label="b" type="f">loopen</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Wilt wesen niet <rhyme label="c" type="m">bevreest</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Maer wilt vast in Hem <rhyme label="b" type="f" corresp="#s1:l3">hoopen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">'t Is heden sijne <rhyme label="c" type="m" corresp="#s1:l4">Feest</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Weest niet beswaert van <rhyme label="a" type="m">hart</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Sint Lucas sal uw' <rhyme label="a" type="m" corresp="#s2:l1">smart</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Genesen, en uw' <rhyme label="b" type="f">wonden</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Hy sal by God sijn <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Maecken dat uwe <rhyme label="b" type="f" corresp="#s2:l3">sonden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Zijn quijt gescholden <rhyme label="c" type="m" corresp="#s2:l4">we'er</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy wordt gehouden <rhyme label="a" type="m">voor</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Een hoogh-geleert <rhyme label="a" type="m" corresp="#s3:l1">Doctoor</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die 't lichaem kan <rhyme label="b" type="f">genesen</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Des moet dees waerden <rhyme label="c" type="m">man</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Lucas, zijn meer <rhyme label="b" type="f" corresp="#s3:l3">gepresen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die 't hart genesen <rhyme label="c" type="m" corresp="#s3:l4">kan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Behalven dese <rhyme label="a" type="m">kunst</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Heeft Lucas noch de <rhyme label="a" type="m" corresp="#s4:l1">gunst</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Van God sijn Heer <rhyme label="b" type="f">verkregen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat Hy <rhyme label="c" type="m">geconterfeyt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Heeft het Beeld wel te <rhyme label="b" type="f" corresp="#s4:l3">degen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van 's Magets <rhyme label="c" type="m" corresp="#s4:l4">Majesteyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'t Was Sinte Lucas <rhyme label="a" type="m">deel</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met sijn vermaert <rhyme label="a" type="m" corresp="#s5:l1">Pinceel</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Mariam af te <rhyme label="b" type="f">beelden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wiens suyver oogh, en <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Het gesicht noyt <rhyme label="b" type="f" corresp="#s5:l3">verveelden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Van 's Hemels <rhyme label="c" type="m" corresp="#s5:l4">Coningin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy sult voor <rhyme label="a" type="m">schilder-loon</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sint Luca! 's Hemels <rhyme label="a" type="m" corresp="#s6:l1">kroon</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Van 's Magets Soon <rhyme label="b" type="f">ontfangen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Na dat Ghy wesen <rhyme label="c" type="m">sult</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Aen d' Olijfboom <rhyme label="b" type="f" corresp="#s6:l3">gehangen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Voor Christus, sonder <rhyme label="c" type="m" corresp="#s6:l4">schuldt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">7.Wel-aen-dan als een <rhyme label="a" type="m">Held</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Maeckt dat voor 's Hemels <rhyme label="a" type="m" corresp="#s7:l1">veld</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Ghy vroomich komt te <rhyme label="b" type="f">strijden</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Maeckt dat Ghy 's Magets <rhyme label="c" type="m">Soon</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Lustich komt te <rhyme label="b" type="f" corresp="#s7:l3">belijden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Soo komt Gh' in 's Hemels <rhyme label="c" type="m" corresp="#s7:l4">throon</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick dwael; Ghy hebt <rhyme label="a" type="m">gestre'en</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Den last is al <rhyme label="a" type="m" corresp="#s8:l1">gele'en</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ghy hebt uw' loon <rhyme label="b" type="f">vernomen</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Ghy zijt in Godes <rhyme label="c" type="m">hof</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Deughde-rijck <rhyme label="b" type="f" corresp="#s8:l3">aengekomen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Daer singht men uwen <rhyme label="c" type="m" corresp="#s8:l4">lof</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En wy-luy hier <rhyme label="a" type="m">bene'en</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Versoecken uw' <rhyme label="a" type="m" corresp="#s9:l1">gebe'en</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">W' aenbidden U <rhyme label="b" type="f">eendrachtich</rhyme>;</l>
        <l id="s9:l4" met="3" type="-+">Wy roepen op dees <rhyme label="c" type="m">aerd'</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Bidt dat d'Heer God <rhyme label="b" type="f" corresp="#s9:l3">Almachtich</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Ons van de Hell' <rhyme label="c" type="m" corresp="#s9:l4">bewaert</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Helpt ons in onse <rhyme label="a" type="m">noodt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Als komen sal de <rhyme label="a" type="m" corresp="#s10:l1">dood</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Maeckt dat wy heylich <rhyme label="b" type="f">sterven</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Staet in die uyr ons <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Maeckt van God te <rhyme label="b" type="f" corresp="#s10:l3">verwerven</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Dat wy zijn eeuwich <rhyme label="c" type="m" corresp="#s10:l4">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
