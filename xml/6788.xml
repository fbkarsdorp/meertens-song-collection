    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Het is een kindeken ons <rhyme label="a" type="f">geboren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Voor anderen wt <rhyme label="a" type="f" corresp="#s1:l1">verkoren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat stillet Godes <rhyme label="a" type="f" corresp="#s1:l1">thoren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat stillet Godes <rhyme label="a" type="f" corresp="#s1:l1">thoren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sulck kindt is ons gegeven <rhyme label="a" type="f">huydt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als ons die schriftuer <rhyme label="a" type="f" corresp="#s2:l1">beduyt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Des frewen sich die <rhyme label="a" type="f" corresp="#s2:l1">luydt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Des frewen sich die <rhyme label="a" type="f" corresp="#s2:l1">luydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sijn edelen Naem seer heerlick <rhyme label="a" type="f">groot</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En oock sijn heylich <rhyme label="a" type="f" corresp="#s3:l1">bloet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maeckt nu de sonden <rhyme label="a" type="f" corresp="#s3:l1">doot</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Maeckt nu de sonden <rhyme label="a" type="f" corresp="#s3:l1">doot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sijn Rijck is vol <rhyme label="a" type="f">gerechticheyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Met vreed en <rhyme label="a" type="f" corresp="#s4:l1">sekerheyt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Weeret in <rhyme label="a" type="f" corresp="#s4:l1">eeuwicheyt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Weeret in <rhyme label="a" type="f" corresp="#s4:l1">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sijn rijckdom en sijn <rhyme label="a" type="f">vromigheyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dienet tot <rhyme label="a" type="f" corresp="#s5:l1">saligheyt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Der gantschen <rhyme label="a" type="f" corresp="#s5:l1">Christenheyt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Der gantschen <rhyme label="a" type="f" corresp="#s5:l1">Christenheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Der slangen kop vertreden <rhyme label="a" type="f">ist</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die ons door raedt en <rhyme label="a" type="f" corresp="#s6:l1">list</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Bracht heeft den eewigen <rhyme label="a" type="f" corresp="#s6:l1">twist</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Bracht heeft den eewigen <rhyme label="a" type="f" corresp="#s6:l1">twist</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daerom lovet tot aller <rhyme label="a" type="f">frist</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Den Heere Iesum <rhyme label="a" type="f" corresp="#s7:l1">Christ</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dat hy mensch worden <rhyme label="a" type="f" corresp="#s7:l1">ist</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat hy mensch worden <rhyme label="a" type="f" corresp="#s7:l1">ist</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dat hy also vruntlijcker <rhyme label="a" type="f">wijs</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Sich gaf al tot een <rhyme label="a" type="f" corresp="#s8:l1">prijs</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Den sy lof, eer en <rhyme label="a" type="f" corresp="#s8:l1">prijs</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Den sy lof, eer en <rhyme label="a" type="f" corresp="#s8:l1">prijs</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
