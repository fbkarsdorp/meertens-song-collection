<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>29696</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Zuid-Holland (Dordrecht?)</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">Alle myn betrouwen // stel ick op Godt den <rhyme label="a" type="m">heer</rhyme></l>
        <l id="s1:l2" met="6" type="-+">Alleen op v ick bouwen // verlaet myn <rhyme label="a" type="m" corresp="#s1:l1">nimmermeer</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Ghy syt myn troost myn <rhyme label="b" type="m">toeuerlaet</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Daer alle myn hoop op <rhyme label="b" type="m" corresp="#s1:l3">staet</rhyme></l>
        <l id="s1:l5" met="6" type="-+">Voor schande en oneer // behoet my doch o <rhyme label="c" type="m">heer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Want ghy o Godt gepresen // helpt my uut alle <rhyme label="a" type="m">noot</rhyme></l>
        <l id="s2:l2" met="6" type="-+">En ghy cont my genesen // door u genade <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dus bid ick vroech ende <rhyme label="b" type="m">laet</rhyme></l>
        <l id="s2:l4" met="4" type="-+">My toch met uwen hulp <rhyme label="b" type="m" corresp="#s2:l3">bystaet</rhyme></l>
        <l id="s2:l5" met="6" type="-+">Voor schande en oneer // behoet my doch o <rhyme label="c" type="m">Heer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Wilt my doch regeren // v arme schaepken <rhyme label="a" type="m">teer</rhyme></l>
        <l id="s3:l2" met="6" type="-+">Dat ick my mach verneren // ootmoedich na u <rhyme label="a" type="m" corresp="#s3:l1">leer</rhyme></l>
        <l id="s3:l3" met="4" type="-+">So sal ick verlaten <rhyme label="b" type="m">saen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Die werelt met haer boos <rhyme label="b" type="m" corresp="#s3:l3">vermaen</rhyme></l>
        <l id="s3:l5" met="6" type="-+">Voor schande en oneer // behoet my doch o <rhyme label="c" type="m">heer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">O Godt almachtich // verhoort my metter <rhyme label="a" type="m">daet</rhyme></l>
        <l id="s4:l2" met="6" type="-+">Want ick ben v seer clachtich // my doch niet en <rhyme label="a" type="m" corresp="#s4:l1">versmaet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Laet my doch beschamen <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Maer helpt my doch uut dit <rhyme label="b" type="m" corresp="#s4:l3">verdriet</rhyme></l>
        <l id="s4:l5" met="6" type="-+">Voor schande en oneer // behoet my doch o <rhyme label="c" type="m">Heer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="-+">O Godt der prinschen // laet my syn <rhyme label="a" type="m">onvertsaecht</rhyme></l>
        <l id="s5:l2" met="6" type="-+">Want al myn winschen // van v arme <rhyme label="a" type="m" corresp="#s5:l1">dienstmaecht</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Syn u bekent o heer <rhyme label="b" type="m">divyn</rhyme> //</l>
        <l id="s5:l4" met="4" type="-+">Verlost my doch van druck en <rhyme label="b" type="m" corresp="#s5:l3">pyn</rhyme></l>
        <l id="s5:l5" met="6" type="-+">Voor schande en oneer behoet my doch o <rhyme label="c" type="m">heer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
