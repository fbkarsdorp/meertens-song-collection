<song>
  <meta>
    <id>184702</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>den haag</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Si c'est pour mon pucelage</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wil dan niemant <rhyme label="a" type="f">rondom-Lelyk</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Eens uytstryken nae <rhyme label="b" type="m">waerdy</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Zoo moet ik 'er aen want <rhyme label="b" type="m" corresp="#s1:l2">zy</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Is onrelijk, lelyk, <rhyme label="a" type="f" corresp="#s1:l1">smelyk</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Trots, begeerich, smerigh <rhyme label="c" type="m">vuil</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Grof en ongeschikt van <rhyme label="c" type="m" corresp="#s1:l5">muil</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Plomp en Lelyk zyn haer <rhyme label="a" type="f">reeden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Leelyk is haer wreed <rhyme label="b" type="m">gelaet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Schriklyk leelyk is haer <rhyme label="b" type="m" corresp="#s2:l2">Praet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dik en leelyk zyn haer <rhyme label="a" type="f" corresp="#s2:l1">leeden</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En daerom ziet haer heele <rhyme label="c" type="m">lyf</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Rondom leelijk hier het <rhyme label="c" type="m" corresp="#s2:l5">wyf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want ik zeg u, 't is wat <rhyme label="a" type="f">zonders</rhyme>;</l>
        <l id="s3:l2" met="4" type="+-">'t Schynt een Monster of <rhyme label="b" type="m">gedroch</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Opgevoet met Beere <rhyme label="b" type="m" corresp="#s3:l2">zoch</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Men behoort haer voor wat <rhyme label="a" type="f" corresp="#s3:l1">wonders</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Op de markten aen de <rhyme label="c" type="m">lien</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">In een kraem te laeten <rhyme label="c" type="m" corresp="#s3:l5">zien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Want haer moer had <rhyme label="a" type="f">onvoorzichtigh</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Rondom-Leelyk op haer <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En doen traptze een padde <rhyme label="b" type="m" corresp="#s4:l2">doot</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Daer 't fenyn uyt schoot zeer <rhyme label="a" type="f" corresp="#s4:l1">schigtigh</rhyme></l>
        <l id="s4:l5" met="4" type="+-">En bespatte gantsch haer <rhyme label="c" type="m">borst</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Juist had Rondom-leelyk <rhyme label="c" type="m" corresp="#s4:l5">dorst</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">En zy heeft zoo sterk <rhyme label="a" type="f">gezoogen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Al 't fenyn al was ze <rhyme label="b" type="m">jongh</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Dat noch op dees uur haer <rhyme label="b" type="m" corresp="#s5:l2">tongh</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Met fenyn is <rhyme label="a" type="f" corresp="#s5:l1">overtoogen</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Daerom mag ook geen <rhyme label="c" type="m">fenyn</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">By haer tong geleeken <rhyme label="c" type="m" corresp="#s5:l5">zyn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zy is zoo gewoon te <rhyme label="a" type="f">liegen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat als zy de waerheit <rhyme label="b" type="m">ziet</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Zulks gants door <rhyme label="b" type="m" corresp="#s6:l2">onwetenheit</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Haer de monde uyt komt <rhyme label="a" type="f" corresp="#s6:l1">vliegen</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Want die eer noch goet en <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Acht niet hoe goddeloos zy <rhyme label="c" type="m" corresp="#s6:l5">leeft</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Alle gasten te <rhyme label="a" type="f">bekladden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Achter de ruch met loogen <rhyme label="b" type="m">sterk</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">'t Is dees Woudaeps daegelyk <rhyme label="b" type="m" corresp="#s7:l2">werk</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">O fenyn en schuim der <rhyme label="a" type="f" corresp="#s7:l1">padde</rhyme>!</l>
        <l id="s7:l5" met="4" type="+-">Stinkende kaelgat, <rhyme label="c" type="m">baviaen</rhyme>!</l>
        <l id="s7:l6" met="4" type="+-">Hoe zal u dit noch <rhyme label="c" type="m" corresp="#s7:l5">vergaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Men zal u noch zoo af <rhyme label="a" type="f">maelen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Dat de kinders <rhyme label="b" type="m">openbaer</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">U noch zullen roepen <rhyme label="b" type="m" corresp="#s8:l2">naer</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En u schant elk een <rhyme label="a" type="f" corresp="#s8:l1">verhaelen</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Als u meester word <rhyme label="c" type="m">beklaegt</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Dien gy al zyn profyt <rhyme label="c" type="m" corresp="#s8:l5">ontjaegt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
