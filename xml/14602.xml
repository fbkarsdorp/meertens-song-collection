    <melody>Te lucis ante terminum</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">TE lucis ante <rhyme label="a" type="m">terminum</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Rerum creator, <rhyme label="b" type="m">poscimus</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ut solita <rhyme label="c" type="m">clementia</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Sis praesul ad <rhyme label="d" type="m">custodiam</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Procul recedant <rhyme label="a" type="m">somnia</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Et noctium <rhyme label="b" type="m">phantasmata</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Hostemque nostrum <rhyme label="c" type="m">comprime</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ne polluantur <rhyme label="d" type="m">corpora</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Praesta Pater <rhyme label="a" type="m">omnipotens</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Per Iesum Christum <rhyme label="b" type="m">Dominum</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Qui tecum in <rhyme label="c" type="m">perpetuum</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Regnat, cum sancto Spiritu. <rhyme label="d" type="m">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
