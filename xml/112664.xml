    <melody>Belle brune que j'adore</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'k SPotte laastmaal met uw <rhyme label="a" type="f">oogjes</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Lief Taleta; maar ik <rhyme label="b" type="m">zie</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat die Goddelijke <rhyme label="a" type="f" corresp="#s1:l1">boogjes</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Wreeken als de <rhyme label="b" type="m" corresp="#s1:l2">Honingbie</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Die wanneer men zyne <rhyme label="c" type="f">raaten</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Al te roek'loos open <rhyme label="d" type="m">doet</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Straks zyn angel uit zal <rhyme label="c" type="f" corresp="#s1:l5">laten</rhyme></l>
        <l id="s1:l8" met="4" type="+-">En vergiftigen ons <rhyme label="d" type="m" corresp="#s1:l6">bloet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dat, gewent te gaan en <rhyme label="a" type="f">keeren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Door de sluisjes van ons <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daar de hette doet <rhyme label="a" type="f" corresp="#s2:l1">vermeeren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Door zyn aangeteegen <rhyme label="b" type="m" corresp="#s2:l2">smart</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Even ging het met de <rhyme label="c" type="f">vonkjes</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Zo veel pyltjes van den <rhyme label="d" type="m">boog</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Afgeschooten door de <rhyme label="c" type="f" corresp="#s2:l5">lonkjes</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Van uw alverwinnend' <rhyme label="d" type="m" corresp="#s2:l6">oog</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Dorst die tarten, 'k dorst die <rhyme label="a" type="f">vergen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Al haar kracht op my te <rhyme label="b" type="m">slaan</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Maar helaas! die Goden <rhyme label="a" type="f" corresp="#s3:l1">tergen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Treft haar wisse wraaklust <rhyme label="b" type="m" corresp="#s3:l2">aan</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Uit uw gitjes, uit uw <rhyme label="c" type="f">Zonnen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Oogjes zyn het immers <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Die myn hart verwinnen <rhyme label="c" type="f" corresp="#s3:l5">konnen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Vliegt de schigt die my <rhyme label="d" type="m" corresp="#s3:l6">doorschiet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Iets geheims kwam uit de <rhyme label="a" type="f">kringen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Van uw elpen <rhyme label="b" type="m">Hemelzaal</rhyme></l>
        <l id="s4:l3" met="4" type="+-">In myn leerzame oogjes <rhyme label="a" type="f" corresp="#s4:l1">dringen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Sneller als een <rhyme label="b" type="m" corresp="#s4:l2">blixemstraal</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Dat zig mengde met de <rhyme label="c" type="f">vliesjes</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Van myn teder <rhyme label="d" type="m">oogenligt</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">En myn hart voor duizent <rhyme label="c" type="f" corresp="#s4:l5">spiesjes</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Tot een boel heeft <rhyme label="d" type="m" corresp="#s4:l6">opgerigt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maar Ã´! schoone, die de <rhyme label="a" type="f">partjes</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Navolgt van de <rhyme label="b" type="m">Honingbie</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">In het quetsen onzer <rhyme label="a" type="f" corresp="#s5:l1">hartjes</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Wilt ghy nu ook wel, als <rhyme label="b" type="m" corresp="#s5:l2">die</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Met den Nectar uwer <rhyme label="c" type="f">lipjes</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Iets verzagten van myn <rhyme label="d" type="m">smart</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">'k Schenk u voor die lieve <rhyme label="c" type="f" corresp="#s5:l5">tipjes</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Een getrouw en rustig <rhyme label="d" type="m" corresp="#s5:l6">hart</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
