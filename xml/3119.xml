        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="mythological song">mythologisch lied</tag>
    </tags>
    <melody>Roosjes in de mei</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt windigh gebruys der stuyren <rhyme label="a" type="f">Koningh</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Moet haestigh ter <rhyme label="b" type="m">vlucht</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vertreckt na Eur'bus droeve <rhyme label="a" type="f" corresp="#s1:l1">wooningh</rhyme></l>
        <l id="s1:l4" met="1" type="-+">Daer 't <rhyme label="c" type="f">Noorden</rhyme></l>
        <l id="s1:l5" met="1" type="-+">Door <rhyme label="c" type="f" corresp="#s1:l4">boorden</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">En stoorden haer swoele <rhyme label="b" type="m" corresp="#s1:l2">Lucht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het weeligh Cruytje van d' hare <rhyme label="a" type="f">Rotsen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Groeyt jeughdigh <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">d' Herd'rinnen in 't Bos gaen d' Herders <rhyme label="a" type="f" corresp="#s2:l1">nabotsen</rhyme></l>
        <l id="s2:l4" met="1" type="-+">Met <rhyme label="c" type="f">singen</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s2:l4">springen</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Doen dringhen haer soet <rhyme label="b" type="m" corresp="#s2:l2">geluydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Silvaens gewest vermeerdert van <rhyme label="a" type="f">Boomen</rhyme>:</l>
        <l id="s3:l2" met="2" type="-+">Sijn Bladen-rijck <rhyme label="b" type="m">Wout</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Verquickt door 't bloeyen van Iris <rhyme label="a" type="f" corresp="#s3:l1">stroomen</rhyme>:</l>
        <l id="s3:l4" met="1" type="-+">De <rhyme label="c" type="f">Linde</rhyme></l>
        <l id="s3:l5" met="1" type="-+">Noyt <rhyme label="c" type="f" corresp="#s3:l4">minde</rhyme></l>
        <l id="s3:l6" met="3" type="-+">De Winden van Boreas <rhyme label="b" type="m" corresp="#s3:l2">hout</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dryady-Nimphjen vlecht <rhyme label="a" type="f">Meya-kranssen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Van bloempjes, en <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Om aen de Reye met vreughden te <rhyme label="a" type="f" corresp="#s4:l1">danssen</rhyme>,</l>
        <l id="s4:l4" met="1" type="-+">Met <rhyme label="c" type="f">spelen</rhyme></l>
        <l id="s4:l5" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s4:l4">quelen</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">Vreught telen, trotst die van 't <rhyme label="b" type="m" corresp="#s4:l2">Hof</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Laet nu o jonge <rhyme label="a" type="f">Batavieren</rhyme></l>
        <l id="s5:l2" met="2" type="-+">(Door Musaes <rhyme label="b" type="m">gheklanck</rhyme>)</l>
        <l id="s5:l3" met="4" type="-+">U blaeu lasuyren Wimpel <rhyme label="a" type="f" corresp="#s5:l1">zwieren</rhyme></l>
        <l id="s5:l4" met="1" type="-+">Met <rhyme label="c" type="f">vreughden</rhyme>,</l>
        <l id="s5:l5" met="1" type="-+">Vol <rhyme label="c" type="f" corresp="#s5:l4">Jeughden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met deughden: Geeft Meya nu <rhyme label="b" type="m" corresp="#s5:l2">danck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
