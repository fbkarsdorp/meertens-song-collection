  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wienig traw ist a ff <rhyme label="a" type="f">erdenn</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">darz  wienig <rhyme label="b" type="m">erbarkeitt</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">Die liebtt ist gar <rhyme label="a" type="f" corresp="#s1:l1">verkaltett</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Jn ithell bedroch haß vnnd <rhyme label="b" type="m" corresp="#s1:l2">neidtt</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Vill schwetzenn vnnd boser st <rhyme label="c" type="f">cker</rhyme></l>
        <l id="s1:l6" met="3" type="-+">ist jetz <rhyme label="d" type="m">ithell</rhyme></l>
        <l id="s1:l7" met="3" type="-+">darmitt sich mannicher th itt schm <rhyme label="c" type="f" corresp="#s1:l5">ckenn</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">traw ist Jetz ndt wilpratt <rhyme label="d" type="m" corresp="#s1:l6">mehr</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Th itt ir keinem menschenn <rhyme label="a" type="f">vertrawenn</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">alhier a ff diesert <rhyme label="b" type="m">weltt</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">A ff nemantt sollt jr nitt ba <rhyme label="a" type="f" corresp="#s2:l1">wenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Jr hatt jn irst recht <rhyme label="b" type="m" corresp="#s2:l2">erkandtt</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Er th itt sich gen e ch <rhyme label="c" type="f">erzeigen</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">gleich als meintt ehrs g <rhyme label="d" type="m">itt</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Mitt re erentz vnnd <rhyme label="c" type="f" corresp="#s2:l5">neigen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">geschichtt alleß a ß einem falschenn m <rhyme label="d" type="m" corresp="#s2:l6">tt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Jch hab jr vill vertra <rhyme label="a" type="f">wett</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">Vnnd sie f r grosse fr ndtt <rhyme label="b" type="m">geachtt</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">Daß hatt mich offt gera <rhyme label="a" type="f" corresp="#s3:l1">wett</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Vnnd jn schadenn <rhyme label="b" type="m" corresp="#s3:l2">gebracht</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Noch hab ichs horenn <rhyme label="c" type="f">sagenn</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">Vnnd ist gewißlich <rhyme label="d" type="m">whar</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Daß vntraw hatt <rhyme label="c" type="f" corresp="#s3:l5">geschlagenn</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">seinenn eigenn hernn z  <rhyme label="d" type="m" corresp="#s3:l6">thott</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Jch m s a ff mannichem ortte <rhyme label="a" type="f">sein</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">Dar ich nitt  gewesen en <rhyme label="b" type="m">bin</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Dar hefft die falsche wortt a ff <rhyme label="a" type="f" corresp="#s4:l1">mich</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ein jeder redtt waß ehr <rhyme label="b" type="m" corresp="#s4:l2">will</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Jch ließ ein jeder woll <rhyme label="c" type="f">pleibenn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ehr wer gleich wie ehr <rhyme label="d" type="m">whar</rhyme> /</l>
        <l id="s4:l7" met="3" type="-+">Jch kh ndt doch woll <rhyme label="c" type="f" corresp="#s4:l5">erleidenn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">ehr ließ mich a ch <rhyme label="d" type="m" corresp="#s4:l6">darfur</rhyme>.</l>
      </lg>
    </lg>
