<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100036</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Questa dolce sirena</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE blancke <rhyme label="a" type="m">Schuym-godin</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">Stak laatst haar Zoontjen <rhyme label="a" type="m" corresp="#s1:l1">in</rhyme>,</l>
        <l id="s1:l3" met="6" type="+-">Bits van spijt // en van nijt, dat den <rhyme label="b" type="f">bengel</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gemist had in ziin <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En toonden hem een <rhyme label="c" type="m">Maacht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Daar yder roem op <rhyme label="c" type="m" corresp="#s1:l5">draacht</rhyme>:</l>
        <l id="s1:l7" met="6" type="+-">Dat hy vloog // met zijn boog, tot dat <rhyme label="b" type="f" corresp="#s1:l3">Engel</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die hy ten Oorlog <rhyme label="c" type="m" corresp="#s1:l5">daacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy komt met flits en <rhyme label="a" type="m">vlerk</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Fraai toegerust in 't <rhyme label="a" type="m" corresp="#s2:l1">perk</rhyme>;</l>
        <l id="s2:l3" met="6" type="+-">Spant zijn pees // sonder vrees, en gaat <rhyme label="b" type="f">mikken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maar staroogt veel te <rhyme label="a" type="m" corresp="#s2:l1">sterk</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Want zy, dien 't quetzig <rhyme label="c" type="m">oog</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Verstrekte voor een <rhyme label="c" type="m" corresp="#s2:l5">boog</rhyme>,</l>
        <l id="s2:l7" met="6" type="+-">Laat dat licht // als een schicht, op hem <rhyme label="b" type="f" corresp="#s2:l3">blikken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">'t Geen dezen God <rhyme label="c" type="m" corresp="#s2:l5">bedroog</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Want door zo fellen <rhyme label="a" type="m">blik</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Geraakt met groote <rhyme label="a" type="m" corresp="#s3:l1">schrik</rhyme>,</l>
        <l id="s3:l3" met="6" type="+-">Lag zijn speer // voor baar neer, 't was geen <rhyme label="b" type="f">wonder</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En loopt zo in zijn <rhyme label="a" type="m" corresp="#s3:l1">strik</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Want kiezende de <rhyme label="c" type="m">vlucht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Gedreven door de <rhyme label="c" type="m" corresp="#s3:l5">lucht</rhyme>,</l>
        <l id="s3:l7" met="6" type="+-">Loop z' hem na // even dra, tot hy <rhyme label="b" type="f" corresp="#s3:l3">onder</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Haar knie gebogen, <rhyme label="c" type="m" corresp="#s3:l5">zucht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy ziet geknielt om <rhyme label="a" type="m">hoog</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Schreit om zijn pijl en <rhyme label="a" type="m" corresp="#s4:l1">boog</rhyme>,</l>
        <l id="s4:l3" met="6" type="+-">Breekt haar hart // door de smart, die hy <rhyme label="b" type="f">toonde</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ja schreit zijn oogen <rhyme label="a" type="m" corresp="#s4:l1">droog</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zy geeft hem, 't haarer <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Zijn boog en pijltje <rhyme label="c" type="m" corresp="#s4:l5">weer</rhyme>:</l>
        <l id="s4:l7" met="6" type="+-">Doch begekt // hoe genekt, zy hem <rhyme label="b" type="f" corresp="#s4:l3">loonde</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En bryzelde zijn <rhyme label="c" type="m" corresp="#s4:l5">speer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Cupido nu te <rhyme label="a" type="m">voet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Grijpt weder nieuwe <rhyme label="a" type="m" corresp="#s5:l1">moet</rhyme>,</l>
        <l id="s5:l3" met="6" type="+-">Drukt zijn staal // tot verhaal van zijn <rhyme label="b" type="f">schande</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Haar door de borst in't <rhyme label="a" type="m" corresp="#s5:l1">bloet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En stijght voort in de <rhyme label="c" type="m">lucht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Zy volcht hem met een <rhyme label="c" type="m" corresp="#s5:l5">zucht</rhyme>:</l>
        <l id="s5:l7" met="6" type="+-">Toen zy zag, zijn gelag, als hy <rhyme label="b" type="f" corresp="#s5:l3">strande</rhyme></l>
        <l id="s5:l8" met="3" type="-+">By Venus om die <rhyme label="c" type="m" corresp="#s5:l5">klucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
