<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185260</id>
    <date>
      <year-start>1829</year-start>
      <year-end>1829</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Daar was laatst een meisje loos</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Daar was laastmaal een Meisje <rhyme label="a" type="m">loos</rhyme>;</l>
        <l id="s1:l2" met="4" type="+-">Die wou gaan varen, die wou gaan <rhyme label="b" type="f">varen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daar was laastmaal een Meisje <rhyme label="a" type="m" corresp="#s1:l1">loos</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die wou gaan varen voor <rhyme label="a" type="m" corresp="#s1:l1">Matroos</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Toen kwam zy in <rhyme label="a" type="m">Amsterdam</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Alwaar zy by een, alwaar zy by <rhyme label="b" type="f">een</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Toen kwam zy in <rhyme label="a" type="m" corresp="#s2:l1">Amsterdam</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Alwaar zy by een Ronselaar <rhyme label="a" type="m" corresp="#s2:l1">kwam</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zy nam dienst voor zeven <rhyme label="a" type="m">jaar</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Omdat zy vreesde, omdat zy <rhyme label="b" type="f">vreesde</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zy nam dienst voor zeven <rhyme label="a" type="m" corresp="#s3:l1">jaar</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Omdat zy vreesde geen <rhyme label="a" type="m" corresp="#s3:l1">gevaar</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Toen bragt zy haar kist aan <rhyme label="a" type="m">boord</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Gelyk een jonge, gelyk een <rhyme label="b" type="f">jonge</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Toen bragt zy haar kist aan <rhyme label="a" type="m" corresp="#s4:l1">boord</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Gelyk een jonge Matroos <rhyme label="a" type="m" corresp="#s4:l1">behoord</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zy moest klimmen in den <rhyme label="a" type="m">mast</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Maken de zeilen, maken de <rhyme label="b" type="f">zeilen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Zy moest klimmen in de <rhyme label="a" type="m" corresp="#s5:l1">mast</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Maken de zeilen aan touwetjes <rhyme label="a" type="m" corresp="#s5:l1">vast</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maar door storm en <rhyme label="a" type="m">onweer</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Sloegen de zylen, sloegen de <rhyme label="b" type="f">zylen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Maar door storm en <rhyme label="a" type="m" corresp="#s6:l1">onweer</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Sloegen de zylen heen en <rhyme label="a" type="m" corresp="#s6:l1">weer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Zy wierd gebonden aan de <rhyme label="a" type="m">mast</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Maakten haar handen, maakten haar <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Zy wierd gebonden aan de <rhyme label="a" type="m" corresp="#s7:l1">mast</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Maakten haar handen en voeten <rhyme label="a" type="m" corresp="#s7:l1">vast</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Zy riep Kaptyntje slaat my <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Ik ben uw liefje, ik ben uw <rhyme label="b" type="f">liefje</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Zy riep Kaptyntje slaat my <rhyme label="a" type="m" corresp="#s8:l1">niet</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Ik ben uw liefje gelyk gy <rhyme label="a" type="m" corresp="#s8:l1">ziet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Eer het scheepje was aan <rhyme label="a" type="m">wal</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Was er het jonge, was er het <rhyme label="b" type="f">jonge</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Eer het scheepje was aan <rhyme label="a" type="m" corresp="#s9:l1">wal</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Was er het jonge Matroosje <rhyme label="a" type="m" corresp="#s9:l1">al</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Toen kwam zy weer in de <rhyme label="a" type="m">Stad</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Alwaar zy eene, alwaar zy <rhyme label="b" type="f">eene</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Toen kwam zy weer in de <rhyme label="a" type="m" corresp="#s10:l1">Stad</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Alwaar zy eene Moeder <rhyme label="a" type="m" corresp="#s10:l1">had</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Zy zei Moeder wordt niet <rhyme label="a" type="m">boos</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Ik heb gevaren, ik heb <rhyme label="b" type="f">gevaren</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Zy zei Moeder wordt niet <rhyme label="a" type="m" corresp="#s11:l1">boos</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Ik heb gevaren voor <rhyme label="a" type="m" corresp="#s11:l1">Matroos</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Van een die my opregt <rhyme label="a" type="m">bemind</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Heb ik dit kleine, heb ik dit <rhyme label="b" type="f">kleine</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Van een die my opregt <rhyme label="a" type="m" corresp="#s12:l1">bemind</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Heb ik dit kleine onnozel <rhyme label="a" type="m" corresp="#s12:l1">kind</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Oorlof jonge Meisje <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Wees voorzigtig, wees <rhyme label="b" type="f">voorzigtig</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Oorlof jonge Meisjes <rhyme label="a" type="m" corresp="#s13:l1">schoon</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Wees voorzigtig, voor een <rhyme label="a" type="m" corresp="#s13:l1">Matroos</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
