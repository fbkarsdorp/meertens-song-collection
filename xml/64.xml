  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">PRonk der maegden <rhyme label="a" type="f">pronkjes</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Wier <rhyme label="a" type="f" corresp="#s1:l1">Hemel-lonkjes</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Door 't <rhyme label="b" type="f">vermogen</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Van u <rhyme label="b" type="f" corresp="#s1:l3">oogen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Koestren <rhyme label="a" type="f" corresp="#s1:l1">herte-vonkjes</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Sullen weer-liefds <rhyme label="c" type="f">straalen</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">Noyt <rhyme label="c" type="f" corresp="#s1:l6">haalen</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">De <rhyme label="c" type="f" corresp="#s1:l6">paalen</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Ter burg van mijn verliefde <rhyme label="d" type="m">borst</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Die so veel rampen <rhyme label="d" type="m" corresp="#s1:l9">torst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Of schept gy <rhyme label="a" type="f">behaagen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">In minne <rhyme label="a" type="f" corresp="#s2:l1">plaagen</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Can mijn <rhyme label="b" type="f">lyen</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">U <rhyme label="b" type="f" corresp="#s2:l3">verblyen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">'k Sal 't gewilligh <rhyme label="a" type="f" corresp="#s2:l1">draagen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Doch wen stuursheyts <rhyme label="c" type="f">dampen</rhyme>,</l>
        <l id="s2:l7" met="1" type="-+">
          <rhyme label="c" type="f" corresp="#s2:l6">Sielsslampen</rhyme>
        </l>
        <l id="s2:l8" met="1" type="-+">Om <rhyme label="c" type="f" corresp="#s2:l6">clampen</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Door hulp van 't Stygiaensch <rhyme label="d" type="m">gedrocht</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Is dan u wens <rhyme label="d" type="m" corresp="#s2:l9">volbrocht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wel aan, kan 't u <rhyme label="a" type="f">noegen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Ick sal me <rhyme label="a" type="f" corresp="#s3:l1">voegen</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Om te <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Lijf en <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Doch wen 't hertens <rhyme label="a" type="f" corresp="#s3:l1">wroegen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Sal met naar <rhyme label="c" type="f">krioelen</rhyme>,</l>
        <l id="s3:l7" met="1" type="-+">Breyns <rhyme label="c" type="f" corresp="#s3:l6">doelen</rhyme>,</l>
        <l id="s3:l8" met="1" type="-+">Om <rhyme label="c" type="f" corresp="#s3:l6">woelen</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Sult gy sien, laas! te spaa, <rhyme label="d" type="m">bedocht</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Wat ramp, uw' stuursheyt <rhyme label="d" type="m" corresp="#s3:l9">wrocht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
