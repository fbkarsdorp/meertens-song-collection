<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100117</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Adieu mijn lief mijn waardste pand</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VVReede Moorster van mijn <rhyme label="a" type="m">jeught</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ick gae u nu <rhyme label="b" type="f">verlaten</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">GHy hebt benomen gansch mijn <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Mijn klagen mocht niet <rhyme label="b" type="f" corresp="#s1:l2">baten</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Hoe dat ick sugt en hoe ick <rhyme label="c" type="m">ween</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ghy blijft verhart gelijck een <rhyme label="c" type="m" corresp="#s1:l5">steen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daerom stel ick u uyt mijn <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Nu 'k u niet kan <rhyme label="b" type="f">belesen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En dencken na geen Venus <rhyme label="a" type="m" corresp="#s2:l1">min</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Soo hoef ick niet te <rhyme label="b" type="f" corresp="#s2:l2">vresen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Of u kop wel geschoren <rhyme label="c" type="m">staet</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Gelijck 't gemeen int vryen <rhyme label="c" type="m" corresp="#s2:l5">gaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maer hoort dit sal mijn Vryster <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Tabacq en Bier <rhyme label="b" type="f">gepresen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Daer op de frisse, <rhyme label="a" type="m" corresp="#s3:l1">Koele-wijn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die sal my wel <rhyme label="b" type="f" corresp="#s3:l2">genesen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Het glaesje Mom moet hier oock <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Ha nu ben van smerte <rhyme label="c" type="m" corresp="#s3:l5">vry</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wanneer ick savons zit by 't <rhyme label="a" type="m">vier</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En doe mijn doosjen <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En smoock een pijpje met <rhyme label="a" type="m" corresp="#s4:l1">playsier</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En dan weer eens <rhyme label="b" type="f" corresp="#s4:l2">gesoopen</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Dan ben ick wonder wel <rhyme label="c" type="m">gerust</rhyme></l>
        <l id="s4:l6" met="4" type="-+">'k En krijgh niet meer na Vrysters <rhyme label="c" type="m" corresp="#s4:l5">lust</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer als mijn altemets een <rhyme label="a" type="m">geur</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Komt mijn Ionck-hert door <rhyme label="b" type="f">vroeten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Soo komt mijn strack de Vocht te <rhyme label="a" type="m" corresp="#s5:l1">veur</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die kan dat weer <rhyme label="b" type="f" corresp="#s5:l2">versoeten</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ha denck ick dan met blyden <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s5:l6" met="4" type="-+">O Kan by u is vreugden <rhyme label="c" type="m" corresp="#s5:l5">in</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wat meught ghy by een spytigh <rhyme label="a" type="m">VVijf</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">U Ionge Ieught <rhyme label="b" type="f">verslyten</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Dick valtse norsigh om je <rhyme label="a" type="m" corresp="#s6:l1">lijf</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ensouje by kans <rhyme label="b" type="f" corresp="#s6:l2">byten</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Wegh, 'k min de Kan soo lang sy <rhyme label="c" type="m">leeft</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Om dat sy gansch geen gal en <rhyme label="c" type="m" corresp="#s6:l5">heeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
