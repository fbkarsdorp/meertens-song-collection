<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182568</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Bei Männer welche Liebe fühlen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wy Mannen die voor liefde <rhyme label="a" type="f">blaaken</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Vind men doorgaans een goedig <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">De Vrouwe pligt is steeds te <rhyme label="a" type="f" corresp="#s1:l1">waaken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Voor 't onheil welk de Mannen <rhyme label="b" type="m" corresp="#s1:l2">tart</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">De liefde maakt ons wel te <rhyme label="c" type="m">vreên</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wy leeve door de liefde <rhyme label="c" type="m" corresp="#s1:l5">alleen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De min verzagt de grootste <rhyme label="a" type="f">plaagen</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Haar offert ieder <rhyme label="b" type="m">Creatuur</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Zy streelt ons alle onze <rhyme label="a" type="f" corresp="#s2:l1">daagen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En werkt reeds vroeg in de <rhyme label="b" type="m" corresp="#s2:l2">Natuur</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dus dat men nooit iets vinden <rhyme label="c" type="m">kan</rhyme>;</l>
        <l id="s2:l6" met="4" type="-+">Zo aangenaam als Vrouw en <rhyme label="c" type="m" corresp="#s2:l5">Man</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
