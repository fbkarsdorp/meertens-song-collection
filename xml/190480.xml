<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190480</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Adieu schone Rosalinde</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Adieu! verhangt uw <rhyme label="a" type="f">Keezen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In uwe sterke <rhyme label="b" type="m">Stad</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Wie donder zou uw <rhyme label="a" type="f" corresp="#s1:l1">vreezen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gy Kaerls van myn <rhyme label="b" type="m" corresp="#s1:l2">gat</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Met al uw Roovers <rhyme label="c" type="f">benden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hebt gy nog niets <rhyme label="d" type="m">gedaan</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Het heiligst recht te <rhyme label="c" type="f" corresp="#s1:l5">schenden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dit dorst gy <rhyme label="d" type="m" corresp="#s1:l6">onderstaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Toen uwe <rhyme label="a" type="f">Vloek-Trawanten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">De Hollandsche <rhyme label="b" type="m">Cabaal</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Zig durfden <rhyme label="a" type="f" corresp="#s2:l1">teegenkanten</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">En zelfs met vuur en <rhyme label="b" type="m" corresp="#s2:l2">staal</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Oranjes Recht <rhyme label="c" type="f">vertreeden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dit deed gy, ô snood <rhyme label="d" type="m">gebroed</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">Verbasteraars der <rhyme label="c" type="f" corresp="#s2:l5">Zeeden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Gy haakte naar ons <rhyme label="d" type="m" corresp="#s2:l6">bloed</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Neen Utrecht zal niet <rhyme label="a" type="f">treuren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Um uw <rhyme label="b" type="m">lafhartigheid</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">In 't hart zult gy <rhyme label="a" type="f" corresp="#s3:l1">bespeuren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De wroeging, angst en <rhyme label="b" type="m" corresp="#s3:l2">nyd</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hoe gy ook pleeg te <rhyme label="c" type="f">blaffen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Gy hebt reeds <rhyme label="d" type="m">afgedaan</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">De Pruisch die zal uw <rhyme label="c" type="f" corresp="#s3:l5">straffen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Voor al uw snood <rhyme label="d" type="m" corresp="#s3:l6">bestaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Waar zyn nu al uw <rhyme label="a" type="f">Helden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gekogt voor Hollandsch <rhyme label="b" type="m">geld</rhyme>?</l>
        <l id="s4:l3" met="3" type="-+">Die het Cabaal <rhyme label="a" type="f" corresp="#s4:l1">vertelden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ter aard te zyn <rhyme label="b" type="m" corresp="#s4:l2">geveld</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Eer dat zy 't <rhyme label="c" type="f">overgaaven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Met Salms aan het <rhyme label="d" type="m">hoofd</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Gy Pensionaris, <rhyme label="c" type="f" corresp="#s4:l5">Slaaven</rhyme>!</l>
        <l id="s4:l8" met="3" type="-+">Wat is uw moed <rhyme label="d" type="m" corresp="#s4:l6">gedooft</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu juichen Utrechts <rhyme label="a" type="f">Wallen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daar Willem zich <rhyme label="b" type="m">bevind</rhyme>!</l>
        <l id="s5:l3" met="3" type="-+">Hy vreest geen <rhyme label="a" type="f" corresp="#s5:l1">donderballen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hy is uw te <rhyme label="b" type="m" corresp="#s5:l2">gezwind</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">Zyn braave Leeger <rhyme label="c" type="f">benden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die hebben uw <rhyme label="d" type="m">verrast</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Koomt uw naar Holland <rhyme label="c" type="f" corresp="#s5:l5">wenden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En werd daar <rhyme label="d" type="m" corresp="#s5:l6">aangetast</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">ô Schandelyke <rhyme label="a" type="f">Keezen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Bangmakers zonder <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Van Utrecht reeds <rhyme label="a" type="f" corresp="#s6:l1">verreezen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Gy werd nu <rhyme label="b" type="m" corresp="#s6:l2">uitgetart</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Koom toon eens uw <rhyme label="c" type="f">bravouren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dog met geen Kruid en <rhyme label="d" type="m">Lood</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Met Roode Wyn en <rhyme label="c" type="f" corresp="#s6:l5">Hoeren</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Viel gy liever <rhyme label="d" type="m" corresp="#s6:l6">dood</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dank zyn die Willems <rhyme label="a" type="f">Raden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die met <rhyme label="b" type="m">voorzigtigheid</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">By ogtend vroeg en <rhyme label="a" type="f" corresp="#s7:l1">spade</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Door eerlyk en fyn <rhyme label="b" type="m" corresp="#s7:l2">beleid</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Uw doel deeden <rhyme label="c" type="f">mislukken</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Hem zyn ons dank <rhyme label="d" type="m">gebragt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Voor hun zult gy nog <rhyme label="c" type="f" corresp="#s7:l5">bukken</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Tot 't laatste <rhyme label="d" type="m" corresp="#s7:l6">Nagestaht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Bezie de Kerk en <rhyme label="a" type="f">Toorens</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Van 't gantsch Utrechtse <rhyme label="b" type="m">Sricht</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Daar branden voor uw <rhyme label="a" type="f" corresp="#s8:l1">hoorens</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">De Oranje-Zon <rhyme label="b" type="m" corresp="#s8:l2">verlicht</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Zie daar de Wimpels <rhyme label="c" type="f">waaijen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Denk om uw stout <rhyme label="d" type="m">bestaan</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">De Fransche Haan zyn <rhyme label="c" type="f" corresp="#s8:l5">kraayen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Heeft ons veel nut <rhyme label="d" type="m" corresp="#s8:l6">gedaan</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Leef Willem met uw <rhyme label="a" type="f">Telgen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Met tweede <rhyme label="b" type="m">Catherien</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Tot lof van Hollands <rhyme label="a" type="f" corresp="#s9:l1">Belgen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Uw trouw die deed ons <rhyme label="b" type="m" corresp="#s9:l2">zien</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Hoe wy uw moeten <rhyme label="c" type="f">eeren</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Nimmer hebt gy <rhyme label="d" type="m">gefaalt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Dog zult Gy ons <rhyme label="c" type="f" corresp="#s9:l5">Regeeren</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Terwyl 't Cabaal <rhyme label="d" type="m" corresp="#s9:l6">verdwaalt</rhyme>, -</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Adieu dan Hollandsch <rhyme label="a" type="f">Zuylen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Van 't bitter <rhyme label="b" type="m">Keezendom</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Als honden zult gy <rhyme label="a" type="f" corresp="#s10:l1">huylen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Uw Moordkreet is reeds <rhyme label="b" type="m" corresp="#s10:l2">stom</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Ga berg uw uit deez' <rhyme label="c" type="f">streeken</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Met uw gestoolen <rhyme label="d" type="m">buit</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Eer dat wy 't aan uw <rhyme label="c" type="f" corresp="#s10:l5">wreeken</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Uw Vloekryk is reeds <rhyme label="d" type="m" corresp="#s10:l6">uit</rhyme> -</l>
      </lg>
    </lg>
  </text>
</song>
