  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GRooten discoureur en <rhyme label="a" type="f">eter</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">'k Danck u voor uw' goed <rhyme label="b" type="m">onthael</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Schoon ick loof, dat bed'laers <rhyme label="a" type="f" corresp="#s1:l1">beter</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Eten op haer <rhyme label="b" type="m" corresp="#s1:l2">avond-mael</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Doch om echter niet te <rhyme label="c" type="f">toonen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat ick heel ondanckbaer <rhyme label="d" type="m">schijn</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Kom 'k u met dit vaers <rhyme label="c" type="f" corresp="#s1:l5">belonen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Voor uw' vleysch, uw' brood en <rhyme label="d" type="m" corresp="#s1:l6">wijn</rhyme>.</l>
        <l id="s1:l9" met="4" type="+-">Voor uw' vleysch, uw' brood en <rhyme label="d" type="m" corresp="#s1:l6">wijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Wil u dan voor eerst <rhyme label="a" type="f">bedancken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Voor uw' stiere vleesch zoo <rhyme label="b" type="m">taay</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als door natte <rhyme label="a" type="f" corresp="#s2:l1">wijngaerdt-rancken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Taayer als een bonte <rhyme label="b" type="m" corresp="#s2:l2">kraay</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daer in zeen'wen als mijn <rhyme label="c" type="f">ving'ren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Liepen, zoo dat ick mijn <rhyme label="d" type="m">kop</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Vreesde aen de muyr te <rhyme label="c" type="f" corresp="#s2:l5">sling'ren</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Had ick niet gerezen <rhyme label="d" type="m" corresp="#s2:l6">op</rhyme>.</l>
        <l id="s2:l9" met="4" type="+-">Had ick niet gerezen <rhyme label="d" type="m" corresp="#s2:l6">op</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Danck u, voor u Friesche <rhyme label="a" type="f">zuyvel</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En voor u verschimmelt <rhyme label="b" type="m">broodt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat zoo hart was als de <rhyme label="a" type="f" corresp="#s3:l1">Duyvel</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Op zijn hoor'nen, en zoo <rhyme label="b" type="m" corresp="#s3:l2">root</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Of 't van roo-steen was <rhyme label="c" type="f">gebacken</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat ick niet verdouwen <rhyme label="d" type="m">kon</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Dat my heeft belet te <rhyme label="c" type="f" corresp="#s3:l5">kacken</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Daer 'k mijn ongemack in <rhyme label="d" type="m" corresp="#s3:l6">von</rhyme>.</l>
        <l id="s3:l9" met="4" type="+-">Daer 'k mijn ongemack in <rhyme label="d" type="m" corresp="#s3:l6">von</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Doe quam daer een zilv're <rhyme label="a" type="f">beecker</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Schuym ende vol beste <rhyme label="b" type="m">bier</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dicker als oyt dranck <rhyme label="a" type="f" corresp="#s4:l1">d'Ap'teecker</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Mengde, of als oyt by 't <rhyme label="b" type="m" corresp="#s4:l2">vier</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Kees de Veenboer in de <rhyme label="c" type="f">ketel</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Met zijn blote ruyge <rhyme label="d" type="m">borst</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Warmde in zijn groene <rhyme label="c" type="f" corresp="#s4:l5">zetel</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Daer hy stickte schier van <rhyme label="d" type="m" corresp="#s4:l6">dorst</rhyme>.</l>
        <l id="s4:l9" met="4" type="+-">Daer hy stickte schier van <rhyme label="d" type="m" corresp="#s4:l6">dorst</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Wil mijn danckbaerheydt oock <rhyme label="a" type="f">toonen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Aen u, fraayen noob'len <rhyme label="b" type="m">baas</rhyme>!</l>
        <l id="s5:l3" met="4" type="+-">Schoon ick u niet kan <rhyme label="a" type="f" corresp="#s5:l1">beloonen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Voor u schrale lebb'ge <rhyme label="b" type="m" corresp="#s5:l2">kaas</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Maer uw' starcke Fransche <rhyme label="c" type="f">wijnen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Schenckt die vry mijn Heer de <rhyme label="d" type="m">Veer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Om de dieven meÃª te <rhyme label="c" type="f" corresp="#s5:l5">pijnen</rhyme>:</l>
        <l id="s5:l8" met="4" type="+-">Want my lust geen droppel <rhyme label="d" type="m" corresp="#s5:l6">meer</rhyme>.</l>
        <l id="s5:l9" met="4" type="+-">Want my lust geen droppel <rhyme label="d" type="m" corresp="#s5:l6">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
