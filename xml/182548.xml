<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182548</id>
    <date>
      <year-start>1793</year-start>
      <year-end>1793</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Achter in de Ridderstraat</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Agter in de <rhyme label="a" type="f">Ridderstraet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Daer woonter een Meisje <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Een Matroosje kan haer <rhyme label="c" type="f">helpen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Een Matroosje kan haer <rhyme label="c" type="f" corresp="#s1:l3">helpe</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van al haer smert en <rhyme label="b" type="m" corresp="#s1:l2">pyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zy lief haer minnebrief <rhyme label="a" type="f">schryve</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Na de Stad <rhyme label="b" type="m">Rotterdam</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat zy by haer Lief moet <rhyme label="c" type="f">kome</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Als dat haer Lief moet <rhyme label="c" type="f" corresp="#s2:l3">kome</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die het Meisje helpe <rhyme label="b" type="m" corresp="#s2:l2">kan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer liefje in de kamer <rhyme label="a" type="f">kwam</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die zag het Meisje <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hy zag het wel aen haer bruin <rhyme label="c" type="f">oge</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hy zag het wel aen haer bruin <rhyme label="c" type="f" corresp="#s3:l3">oge</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Waer hy ze helpe <rhyme label="b" type="m" corresp="#s3:l2">kan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy namse in zyn <rhyme label="a" type="f">arme</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hy kuste haer <rhyme label="b" type="m">roodermont</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wel vier-en-veertig <rhyme label="c" type="f">werven</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wel vier-en-veertig <rhyme label="c" type="f" corresp="#s4:l3">werven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Het Meisje wierd <rhyme label="b" type="m" corresp="#s4:l2">gesond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wat zal men 't Matroosje <rhyme label="a" type="f">vereere</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Voor zyn kloekmoedige <rhyme label="b" type="m">daet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Een Kindje in de <rhyme label="c" type="f">wiege</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Een Kindje in de <rhyme label="c" type="f" corresp="#s5:l3">wiege</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met mooi gekrult zwart <rhyme label="b" type="m" corresp="#s5:l2">haer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maer als het kind een zoontje <rhyme label="a" type="f">is</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zal het een Matroosje <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En klimmen in de <rhyme label="c" type="f">maste</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En klimmen in de <rhyme label="c" type="f" corresp="#s6:l3">maste</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Geen weerga moet 'er <rhyme label="b" type="m" corresp="#s6:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maar als 't dan een dogter <rhyme label="a" type="f">is</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dan zal het een naaister <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dan zal ik haer late <rhyme label="c" type="f">naeije</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dan zal ik haer late <rhyme label="c" type="f" corresp="#s7:l3">naeije</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Door een Matroosje <rhyme label="b" type="m" corresp="#s7:l2">fyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Een Matroosje moet het <rhyme label="a" type="f">wese</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Spant boven al de <rhyme label="b" type="m">kroon</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Ik zal hem myn Trouwtje <rhyme label="c" type="f">geve</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ia, ia, van <rhyme label="d" type="m">valderala</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ik zal hem mijn Trouwtje <rhyme label="c" type="f" corresp="#s8:l3">geven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Het is een braef <rhyme label="b" type="m" corresp="#s8:l2">Persoon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
