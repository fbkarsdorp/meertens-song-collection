  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Och lief wilt niet <rhyme label="a" type="f">versmaden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die v toent <rhyme label="a" type="f" corresp="#s1:l1">onbeladen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Altyt troost deucht en <rhyme label="b" type="m">eer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wilt toch in liefde <rhyme label="a" type="f" corresp="#s1:l1">baden</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En nempt my in <rhyme label="a" type="f" corresp="#s1:l1">genaden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Eer ick door lieft <rhyme label="b" type="m" corresp="#s1:l3">verteer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Eerbare Ionckvrouwe <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ghy spant int hert de <rhyme label="a" type="f" corresp="#s2:l1">croone</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Als thisbe dede <rhyme label="b" type="m">fier</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In pieramus <rhyme label="a" type="f" corresp="#s2:l1">ydone</rhyme></l>
        <l id="s2:l5" met="3" type="-+">doch sy gaf hem te <rhyme label="a" type="f" corresp="#s2:l1">loone</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Wat hy begeerde <rhyme label="b" type="m" corresp="#s2:l3">schier</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Fyn lief laet oock soo <rhyme label="a" type="f">mercken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Den loon van myne <rhyme label="a" type="f" corresp="#s3:l1">wercken</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Soo sal ick syn <rhyme label="b" type="m">verblydt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Toont doch als paris <rhyme label="a" type="f" corresp="#s3:l1">clercken</rhyme></l>
        <l id="s3:l5" met="3" type="-+">die door helenas <rhyme label="a" type="f" corresp="#s3:l1">wercken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van rouwe was <rhyme label="b" type="m" corresp="#s3:l3">bevryt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In lieft wilt toch weer <rhyme label="a" type="f">keeren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wilt v o blom <rhyme label="a" type="f" corresp="#s4:l1">verneeren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Al mynen druck <rhyme label="b" type="m">versoet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Maer soo troost gaet <rhyme label="a" type="f" corresp="#s4:l1">faelgeren</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ick v lief moet <rhyme label="a" type="f" corresp="#s4:l1">ontberen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Steruen kies ick voor <rhyme label="b" type="m" corresp="#s4:l3">boet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Eerlycke liefde <rhyme label="a" type="f">reyne</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Draech ick lief <rhyme label="a" type="f" corresp="#s5:l1">certeyne</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Alsoo Leander <rhyme label="b" type="m">dee</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hier om kent v soo <rhyme label="a" type="f" corresp="#s5:l1">cleyne</rhyme></l>
        <l id="s5:l5" met="3" type="-+">En als hero Ick <rhyme label="a" type="f" corresp="#s5:l1">meyne</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Wilt liefde loonen <rhyme label="b" type="m" corresp="#s5:l3">mee</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Compt lief wilt helpen <rhyme label="a" type="f">stryden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Tegen die myn <rhyme label="a" type="f" corresp="#s6:l1">benyden</rhyme></l>
        <l id="s6:l3" met="3" type="-+">V lieft hier nv <rhyme label="b" type="m">betoont</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soo sal myn bitter <rhyme label="a" type="f" corresp="#s6:l1">lyden</rhyme></l>
        <l id="s6:l5" met="3" type="-+">En druck an alle <rhyme label="a" type="f" corresp="#s6:l1">syden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Met vreuchden syn <rhyme label="b" type="m" corresp="#s6:l3">geloont</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laet doch v liefde <rhyme label="a" type="f">Groeyen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Als aurora doet <rhyme label="a" type="f" corresp="#s7:l1">vloeyen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Den soeten dau <rhyme label="b" type="m">eenpaer</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Wilt doch nerstich <rhyme label="a" type="f" corresp="#s7:l1">besproyen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Onsluyt myn van dees <rhyme label="a" type="f" corresp="#s7:l1">boeyen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Wt reynder liefde <rhyme label="b" type="m" corresp="#s7:l3">claer</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Adieu schoone <rhyme label="a" type="f">Goddinne</rhyme> [-..]</l>
        <l id="s8:l2" met="3" type="-+">Ick met herten <rhyme label="a" type="f" corresp="#s8:l1">sinne</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Altyt bly-fue <rhyme label="b" type="m">Getrou-we</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Troost myn edel <rhyme label="a" type="f" corresp="#s8:l1">vriendinne</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Eer ick door uwe <rhyme label="a" type="f" corresp="#s8:l1">minne</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Moet lief steruen van <rhyme label="b" type="m" corresp="#s8:l3">rou</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">E[-...] eerlyck sal ick in <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Altyt dancklyck <rhyme label="a" type="f" corresp="#s9:l1">wesen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Al ben ick lief <rhyme label="b" type="m">absent</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Van myn wert ghy <rhyme label="a" type="f" corresp="#s9:l1">gepresen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">En tusschen hoop en <rhyme label="a" type="f" corresp="#s9:l1">vreesen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">W verwachten een goet <rhyme label="b" type="m" corresp="#s9:l3">endt</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Schoon princesse vol <rhyme label="a" type="f">trouwen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">In v schoonste der <rhyme label="a" type="f" corresp="#s10:l1">vrouwen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Stel ick myns leuens <rhyme label="b" type="m">Ieucht</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Prys sult ghy altyt <rhyme label="a" type="f" corresp="#s10:l1">houwen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Seer eerlyck Int <rhyme label="a" type="f" corresp="#s10:l1">Anschouwen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Roemen sal ick v <rhyme label="b" type="m" corresp="#s10:l3">deucht</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
