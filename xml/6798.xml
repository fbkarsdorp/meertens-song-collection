<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6798</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>emmerick</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Puer natus in Bethleem</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Een kint geboren te <rhyme label="a" type="m">Bethlehem</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">In dit soet <rhyme label="b" type="m">Iaer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Des frewet sich <rhyme label="a" type="m" corresp="#s1:l1">Ierusalem</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">In dit soet Iaer, segh ick <rhyme label="b" type="m" corresp="#s1:l2">voorwaer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Is ons geboren een kindeken <rhyme label="b" type="m" corresp="#s1:l2">klaer</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Een kindeken <rhyme label="b" type="m" corresp="#s1:l2">klaer</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Godes Soon van <rhyme label="c" type="m">Hemelrijck</rhyme>,</l>
        <l id="s1:l8" met="5" type="-+">In menschlicke natuer geboren <rhyme label="d" type="m">ist</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">In dit soet <rhyme label="b" type="m" corresp="#s1:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hier ligt het in dat <rhyme label="a" type="m">kribbeken</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">In dit soet <rhyme label="b" type="m">Iaer</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Sonder endt so is dat rijcke <rhyme label="a" type="m" corresp="#s2:l1">fijn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">In dit soet Iaer, segh ick <rhyme label="b" type="m" corresp="#s2:l2">voorwaer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Is ons geboren een kindeken <rhyme label="b" type="m" corresp="#s2:l2">klaer</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Een kindeken <rhyme label="b" type="m" corresp="#s2:l2">klaer</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Godes Soon van <rhyme label="c" type="m">Hemelrijck</rhyme>,</l>
        <l id="s2:l8" met="5" type="-+">In menschlicke natuer geboren <rhyme label="d" type="m">ist</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">In dit soet <rhyme label="b" type="m" corresp="#s2:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat Osken en dat <rhyme label="a" type="m">Eseltjen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">In dit soet <rhyme label="b" type="m">Iaer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Erkenden God den Heere <rhyme label="a" type="m" corresp="#s3:l1">fijn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In dit soet Iaer, segh ick <rhyme label="b" type="m" corresp="#s3:l2">voorwaer</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Is ons geboren een kindeken <rhyme label="b" type="m" corresp="#s3:l2">klaer</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Een kindeken <rhyme label="b" type="m" corresp="#s3:l2">klaer</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Godes Soon van <rhyme label="c" type="m">Hemelrijck</rhyme>,</l>
        <l id="s3:l8" met="5" type="-+">In menschlicke natuer geboren <rhyme label="d" type="m">ist</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">In dit soet <rhyme label="b" type="m" corresp="#s3:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dry Coningen van Saba quamen <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s4:l2" met="2" type="-+">In dit soet <rhyme label="b" type="m">Iaer</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Gout, Wyroock, Myrthen brachten sy <rhyme label="a" type="m" corresp="#s4:l1">daer</rhyme></l>
        <l id="s4:l4" met="4" type="-+">In dit soet Iaer, segh ick <rhyme label="b" type="m" corresp="#s4:l2">voorwaer</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Is ons geboren een kindeken <rhyme label="b" type="m" corresp="#s4:l2">klaer</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Een kindeken <rhyme label="b" type="m" corresp="#s4:l2">klaer</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Godes Soon van <rhyme label="c" type="m">Hemelrijck</rhyme>,</l>
        <l id="s4:l8" met="5" type="-+">In menschlicke natuer geboren <rhyme label="d" type="m">ist</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">In dit soet <rhyme label="b" type="m" corresp="#s4:l2">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
