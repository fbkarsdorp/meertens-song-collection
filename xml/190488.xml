<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190488</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wat hoort men nu al <rhyme label="a" type="f">nieuwigheden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In het Patriot <rhyme label="b" type="m">Geslacht</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Sy zoeken ook in alle <rhyme label="a" type="f" corresp="#s1:l1">Steeden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Hoe dat Oranje word <rhyme label="b" type="m" corresp="#s1:l2">veragt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ik laat my door Prins Willem <rhyme label="c" type="f">geleiden</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ik stry niet als voor Oranjes <rhyme label="d" type="m">Bloed</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Oranje zal my altoos <rhyme label="c" type="f" corresp="#s1:l5">bevryden</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Oranje is myn hert en <rhyme label="d" type="m" corresp="#s1:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zy zoeken door Geweer en <rhyme label="a" type="f">Waapen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Oranje te doen <rhyme label="b" type="m">ondergaan</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maar terwyl wy nog niet <rhyme label="a" type="f" corresp="#s2:l1">slaapen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Schoppen wy haar na de <rhyme label="b" type="m" corresp="#s2:l2">maan</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ik laat my door Prins Willem <rhyme label="c" type="f">geleiden</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ik stry niet als voor Oranjes <rhyme label="d" type="m">bloed</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Oranje zal my altoos <rhyme label="c" type="f" corresp="#s2:l5">bevryden</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Oranje is myn hert en <rhyme label="d" type="m" corresp="#s2:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wat hoort men van de Vryheid <rhyme label="a" type="f">schreeuwen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Maar die stem die past haar <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die Stem die zal haar nog eens doen <rhyme label="a" type="f" corresp="#s3:l1">beeven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">By de Duivel in de <rhyme label="b" type="m" corresp="#s3:l2">Hel</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ik laat my door Prins Willem <rhyme label="c" type="f">geleiden</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Ik stry niet als voor Oranjes <rhyme label="d" type="m">bloed</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Oranje zal myn altoos <rhyme label="c" type="f" corresp="#s3:l5">bevryden</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Oranje is myn hert en <rhyme label="d" type="m" corresp="#s3:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wel wat blinken nu die <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Maar die tyd is haast <rhyme label="b" type="m">voorby</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Zy loopen met gescheurde <rhyme label="a" type="f" corresp="#s4:l1">kleeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En slaapen reeds nu op het <rhyme label="b" type="m" corresp="#s4:l2">stroo</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ik laat my door Prins Willem <rhyme label="c" type="f">geleiden</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ik stry niet als voor Oranjes <rhyme label="d" type="m">bloed</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Oranje zal my altoos <rhyme label="c" type="f" corresp="#s4:l5">bevryden</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Oranje is myn hert en <rhyme label="d" type="m" corresp="#s4:l6">bloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
