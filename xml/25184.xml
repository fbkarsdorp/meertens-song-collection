  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">We vrauwin minne trudit in ir ere <rhyme label="a" type="m">niet</rhyme>.</l>
        <l id="s1:l2" met="5" type="-+">die wilt niet gar getruwenclichen <rhyme label="b" type="f">minnen</rhyme>.</l>
        <l id="s1:l3" met="6" type="-+">si selich wif. die in ir eren spegil <rhyme label="a" type="m" corresp="#s1:l1">sijt</rhyme>.</l>
        <l id="s1:l4" met="5" type="-+">wie si der velszer minnen muoge <rhyme label="b" type="f" corresp="#s1:l2">untrinnen</rhyme>.</l>
        <l id="s1:l5" met="6" type="-+">wif la dich sin eyn stede muot wail <rhyme label="c" type="m">behuot</rhyme>.</l>
        <l id="s1:l6" met="5" type="+-">wiltu dich den szonen szoner <rhyme label="d" type="f">machen</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">so strijch an dich so reynen <rhyme label="c" type="m" corresp="#s1:l5">muot</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">die dinen eren helpe <rhyme label="d" type="f" corresp="#s1:l6">wachen</rhyme>.</l>
        <l id="s1:l9" met="5" type="+-">de muot dich den szonen szoner <rhyme label="c" type="m" corresp="#s1:l5">duot</rhyme>.</l>
      </lg>
    </lg>
