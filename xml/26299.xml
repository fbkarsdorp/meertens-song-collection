<song>
  <meta>
    <id>26299</id>
    <date>
      <year-start>1591</year-start>
      <year-end>1591</year-end>
    </date>
    <location>
      <orginal-name>franeker]</orginal-name>
      <standard-name>Franeker</standard-name>
      <coordinates>
        <lon>53.1904513</lon>
        <lat>5.5417414</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="vermaanlied" translation="admonitory song">vermaanlied</tag>
    </tags>
    <melody>Hélàs pauvre monde</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe wel ick in 't <rhyme label="a" type="f">afwesen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met t' Lichaem verre <rhyme label="b" type="m">sy</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Liefd' is t' uwaert <rhyme label="a" type="f" corresp="#s1:l1">gheresen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wt rechter jonsten <rhyme label="b" type="m" corresp="#s1:l2">vry</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Vrienden wensch ick daer <rhyme label="b" type="m" corresp="#s1:l2">by</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat Godt Vader <rhyme label="c" type="f">Almachtich</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Eeuwich beware <rhyme label="b" type="m" corresp="#s1:l2">dy</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ende behoude <rhyme label="b" type="m" corresp="#s1:l2">my</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Al in zijn waerheyt <rhyme label="c" type="f" corresp="#s1:l6">crachtich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Seer noodelick ick <rhyme label="a" type="f">achte</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat elck neersticheyt <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Tot Godt roepen met <rhyme label="a" type="f" corresp="#s2:l1">crachte</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Door 't inwendich <rhyme label="b" type="m" corresp="#s2:l2">ghemoet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Om toenemen in 't <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wassen in liefden <rhyme label="c" type="f">vierich</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als Godts kinderen <rhyme label="b" type="m" corresp="#s2:l2">vroet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Al door zijn woorden <rhyme label="b" type="m" corresp="#s2:l2">soet</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">In vreden <rhyme label="c" type="f" corresp="#s2:l6">goedertierich</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vreedtsaem met herten <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Door Goddelijcker <rhyme label="b" type="m">aert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om dienen Godt <rhyme label="a" type="f" corresp="#s3:l1">alleyne</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als goe knechten <rhyme label="b" type="m" corresp="#s3:l2">vermaert</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die draghen goet <rhyme label="b" type="m" corresp="#s3:l2">regaert</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Verwachtende den <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Hem bidden <rhyme label="b" type="m" corresp="#s3:l2">onbeswaert</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dat hyse wel <rhyme label="b" type="m" corresp="#s3:l2">bewaert</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Als zijn ledekens <rhyme label="c" type="f" corresp="#s3:l6">teere</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Als een lidt swack <rhyme label="a" type="f">ellendich</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Mach ick wel bidden <rhyme label="b" type="m">eest</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat Godt my wil <rhyme label="a" type="f" corresp="#s4:l1">inwendich</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Stercken door zynen <rhyme label="b" type="m" corresp="#s4:l2">Gheest</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Om blyven <rhyme label="b" type="m" corresp="#s4:l2">onbevreest</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In zynen roep <rhyme label="c" type="f">ghestadich</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Die na zynen wille <rhyme label="b" type="m" corresp="#s4:l2">heeft</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Om troost en hulpe <rhyme label="b" type="m" corresp="#s4:l2">meest</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Is hy vrienden <rhyme label="c" type="f" corresp="#s4:l6">ghenadich</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die bidt na zynen <rhyme label="a" type="f">wille</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Vrienden werden <rhyme label="b" type="m">verhoort</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zonder twist oft <rhyme label="a" type="f" corresp="#s5:l1">gheschille</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met herte <rhyme label="b" type="m" corresp="#s5:l2">onghestoort</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die also nae zijn <rhyme label="b" type="m" corresp="#s5:l2">Woort</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hem aenroepen <rhyme label="c" type="f">ootmoedich</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Zal hy wel leyden <rhyme label="b" type="m" corresp="#s5:l2">voort</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">In vrede met <rhyme label="b" type="m" corresp="#s5:l2">accoort</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Al op zijn weghen <rhyme label="c" type="f" corresp="#s5:l6">goedich</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">By hem salt niet <rhyme label="a" type="f">ghebreken</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ist dat wy 't nemen <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maer men moet t' licht <rhyme label="a" type="f" corresp="#s6:l1">ontsteken</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En latent branden <rhyme label="b" type="m" corresp="#s6:l2">claer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Al op den <rhyme label="b" type="m" corresp="#s6:l2">Candelaer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">So Christus is <rhyme label="c" type="f">verhalich</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Of yemandt hier of <rhyme label="b" type="m" corresp="#s6:l2">daer</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Liete t' boos leven <rhyme label="b" type="m" corresp="#s6:l2">swaer</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Om hier naer werden <rhyme label="c" type="f" corresp="#s6:l6">salich</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Men moet hem daer toe <rhyme label="a" type="f">pynen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Vrienden tot inden <rhyme label="b" type="m">endt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Om t' licht te laten <rhyme label="a" type="f" corresp="#s7:l1">schynen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Hier in de Werelt <rhyme label="b" type="m" corresp="#s7:l2">blent</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Offer noch hier <rhyme label="b" type="m" corresp="#s7:l2">ontrent</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Waren vaten <rhyme label="c" type="f">verceurlijck</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Van goude <rhyme label="b" type="m" corresp="#s7:l2">excellent</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Of voor silver <rhyme label="b" type="m" corresp="#s7:l2">bekent</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Den Huys-Heer <rhyme label="c" type="f" corresp="#s7:l6">oorbeurlijck</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Laet ons al sonder <rhyme label="a" type="f">sneven</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Arbeyden groot en <rhyme label="b" type="m">cleyn</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Godt sal ghenade <rhyme label="a" type="f" corresp="#s8:l1">gheven</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hier in 't Egypsche <rhyme label="b" type="m" corresp="#s8:l2">pleyn</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Om omkeeren <rhyme label="b" type="m" corresp="#s8:l2">ghemeyn</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Gulden herten <rhyme label="c" type="f">verstandich</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Van 't Egyps volck <rhyme label="b" type="m" corresp="#s8:l2">vileyn</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Wt de Heydens <rhyme label="b" type="m" corresp="#s8:l2">onreyn</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Die leven voor Godt <rhyme label="c" type="f" corresp="#s8:l6">schandich</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Op sulcke menschen <rhyme label="a" type="f">puere</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Moeten wy gade <rhyme label="b" type="m">slaen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die gantsch op de <rhyme label="a" type="f" corresp="#s9:l1">Schriftuere</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En niet op Menschen <rhyme label="b" type="m" corresp="#s9:l2">staen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Vraghen die na de <rhyme label="b" type="m" corresp="#s9:l2">baen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Laet ons wesen <rhyme label="c" type="f">labuerich</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Om wel voren te <rhyme label="b" type="m" corresp="#s9:l2">gaen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Den wech haer wysen <rhyme label="b" type="m" corresp="#s9:l2">saen</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Tot Godt t' leven <rhyme label="c" type="f" corresp="#s9:l6">geduerich</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Met woort en t' leven <rhyme label="a" type="f">wysen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Als Wercklieden <rhyme label="b" type="m">vailliant</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">De goey wercken doen <rhyme label="a" type="f" corresp="#s10:l1">rysen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Vrienden aen elcken <rhyme label="b" type="m" corresp="#s10:l2">cant</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Handelen met <rhyme label="b" type="m" corresp="#s10:l2">verstant</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">In alle saken <rhyme label="c" type="f">dedich</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">O Godt biedt ons u <rhyme label="b" type="m" corresp="#s10:l2">handt</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Om blyven in den <rhyme label="b" type="m" corresp="#s10:l2">bandt</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">T' gordel der Liefden <rhyme label="c" type="f" corresp="#s10:l6">vredich</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
