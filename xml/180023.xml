  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'t Bagchusfeest dat onze leden steeds <rhyme label="a" type="m">kweld</rhyme>;</l>
        <l id="s1:l2" met="4" type="-+">Venus stoet, die het geheugen <rhyme label="a" type="m" corresp="#s1:l1">ontsteld</rhyme>;</l>
        <l id="s1:l3" met="2" type="-+">'t Dobblen en <rhyme label="b" type="f">spelen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Dat droefheid kan <rhyme label="b" type="f" corresp="#s1:l3">telen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Doen hier beërven vroeg ofte <rhyme label="c" type="m">spaê</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Puisten en <rhyme label="d" type="f">vlakken</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Ledige <rhyme label="d" type="f" corresp="#s1:l6">zakken</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Pokken, jicht, schurfd of <rhyme label="c" type="m" corresp="#s1:l5">podegra</rhyme>,</l>
      </lg>
    </lg>
