<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>114225</id>
    <date>
      <year-start>1745</year-start>
      <year-end>1745</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody>Tranquilles coeurs</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HElaes! hoe teder dat ik <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Kan ik u egter niet <rhyme label="b" type="f">behagen</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Gy hebt geheel myn hert en <rhyme label="a" type="m" corresp="#s1:l1">zin</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">'k Zal eeuwig voor u liefde <rhyme label="b" type="f" corresp="#s1:l2">dragen</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Gy hebt my diep <rhyme label="c" type="m">gewont</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In 't binnenst van myn <rhyme label="d" type="m">ziel</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ach! dat ik u <rhyme label="d" type="m" corresp="#s1:l6">geviel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ik Min met alle <rhyme label="a" type="m">teederheid</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ik heb myn Hert geheel <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Aen u, heb dan <rhyme label="a" type="m" corresp="#s2:l1">medogentheid</rhyme>;</l>
        <l id="s2:l4" met="4" type="-+">Gy zyt het Doelwit van myn <rhyme label="b" type="f" corresp="#s2:l2">leven</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Volhert in Wreedheid <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ã´ Zoete <rhyme label="d" type="m">Vyandin</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Of ik sterf door de <rhyme label="d" type="m" corresp="#s2:l6">Min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ach! dat Cupido kleine <rhyme label="a" type="m">Dief</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">U hart wou met een Pyltje <rhyme label="b" type="f">treffen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ik zoude dan myn Waerde <rhyme label="a" type="m" corresp="#s3:l1">Lief</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">My tot den Hemel hoog <rhyme label="b" type="f" corresp="#s3:l2">verheffen</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">Een Keizer op zyn <rhyme label="c" type="m">Throon</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ja al des werelds <rhyme label="d" type="m">Pracht</rhyme>!</l>
        <l id="s3:l7" met="3" type="-+">Wierd niet by my <rhyme label="d" type="m" corresp="#s3:l6">geacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
