    <melody>O Herr Gott begnade mich</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O HEERE ghi sijt mijn Godt <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ghi maect my van alle mijn sonden <rhyme label="a" type="m" corresp="#s1:l1">reen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Als ghi my ontfanckt in v <rhyme label="b" type="f">ghenaden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">So en can my niemant <rhyme label="b" type="f" corresp="#s1:l3">gheschaden</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Godt Vader ghy sijt mijn <rhyme label="c" type="m">toeuerlaet</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Door Jesum Christum mijn <rhyme label="c" type="m" corresp="#s1:l5">advocaet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die heeft my gantsch <rhyme label="d" type="f">ghenesen</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Sijnen naem moet sijn <rhyme label="d" type="f" corresp="#s1:l7">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O GOD in v is alle <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Helpt my doch wt dit boose <rhyme label="a" type="m" corresp="#s2:l1">gheslacht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die v woort willen <rhyme label="b" type="f">verdrucken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En latet haer niet <rhyme label="b" type="f" corresp="#s2:l3">ghelucken</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Sy hebbent so vast in haren <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Haer valsch voornemen daer si steken <rhyme label="c" type="m" corresp="#s2:l5">in</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Daer wyllen sy by <rhyme label="d" type="f">blijuen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Om eendrachticheyt te <rhyme label="d" type="f" corresp="#s2:l7">verdrijuen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Uwen Gheest en laet van my niet <rhyme label="a" type="m">scheen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Tot allen duechden wilt my <rhyme label="a" type="m" corresp="#s3:l1">bereen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat ick mach vast op v <rhyme label="b" type="f">betrouwen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ten sal my niet <rhyme label="b" type="f" corresp="#s3:l3">berouwen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Ghy sijt mijn troost die ick nv <rhyme label="c" type="m">vaet</rhyme></l>
        <l id="s3:l6" met="4" type="-+">En ghy gheeft my den besten <rhyme label="c" type="m" corresp="#s3:l5">raet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dat ick v woort sal <rhyme label="d" type="f">bewaren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Tspijt alle valsche <rhyme label="d" type="f" corresp="#s3:l7">scharen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Heer v leer die is so <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Die my leert kennen ws hemels <rhyme label="a" type="m" corresp="#s4:l1">troon</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Daer ick altijt nae moet <rhyme label="b" type="f">haken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Vwen Gheest doeghet my <rhyme label="b" type="f" corresp="#s4:l3">smaken</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ghy spreeckt soo vrij, coemt al tot <rhyme label="c" type="m">my</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Hooret mijn stemme, en blijft daer <rhyme label="c" type="m" corresp="#s4:l5">by</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dus wil ick by v <rhyme label="d" type="f">blijuen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Al soumen my <rhyme label="d" type="f" corresp="#s4:l7">ontlijuen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mijn  vader spreect wt den hemel <rhyme label="a" type="m">ydone</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Dit is mijnen beminden <rhyme label="a" type="m" corresp="#s5:l1">sone</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Hem suldy te recht <rhyme label="b" type="f">aenhooren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Soo suldy vruecht <rhyme label="b" type="f" corresp="#s5:l3">oorbooren</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Vanden deesem soo wilt u <rhyme label="c" type="m">scheen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Spreect Christus tot sijnen Apostelen <rhyme label="c" type="m" corresp="#s5:l5">gemeen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dit sijn der Papisten en seckten <rhyme label="d" type="f">ghesellen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die ons arme Christenen altijt <rhyme label="d" type="f" corresp="#s5:l7">quellen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O Heere ghy sijt mijn helper <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Neemt my nv in v <rhyme label="a" type="m" corresp="#s6:l1">behoet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">My en mach niet <rhyme label="b" type="f">miscomen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Uwen gheest die sal my <rhyme label="b" type="f" corresp="#s6:l3">vromen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">U woort is mijnen hoochsten <rhyme label="c" type="m">troost</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Van allen quaden, my snijdt en <rhyme label="c" type="m" corresp="#s6:l5">oost</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Leydt my in dijnen <rhyme label="d" type="f">pade</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Want ick stae in dijn <rhyme label="d" type="f" corresp="#s6:l7">ghenade</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Vwen gheest gheeft my die <rhyme label="a" type="m">cracht</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Al wordick van dyuersche seckten <rhyme label="a" type="m" corresp="#s7:l1">veracht</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Als ick op v <rhyme label="b" type="f">betrouwe</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Jck dan niet en <rhyme label="b" type="f" corresp="#s7:l3">verflouwe</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Op v betrout die siele <rhyme label="c" type="m">mijn</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Helpt my doch Heere volstandich <rhyme label="c" type="m" corresp="#s7:l5">sijn</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat ick van v woort niet en <rhyme label="d" type="f">wijcke</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Om al der weerelt <rhyme label="d" type="f" corresp="#s7:l7">rijcke</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die weerelt is nv met sienden oogen <rhyme label="a" type="m">blint</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Dat sy wilt wesen des Antechists <rhyme label="a" type="m" corresp="#s8:l1">kint</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Ghy woudtse salich <rhyme label="b" type="f">maken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Maer die doot dede sy v <rhyme label="b" type="f" corresp="#s8:l3">smaken</rhyme></l>
        <l id="s8:l5" met="4" type="-+">V steruen was ons geen schaeyende <rhyme label="c" type="m">doot</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Door uwe cracht, quaemt ghy wter <rhyme label="c" type="m" corresp="#s8:l5">noot</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Om al de weerelt te <rhyme label="d" type="f">ghenesen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Daer voor heeft sy v <rhyme label="d" type="f" corresp="#s8:l7">mispresen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die v woort hebben ghenomen <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Die hebt ghijt laten wel <rhyme label="a" type="m" corresp="#s9:l1">verstaen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Sy en laten haer niet <rhyme label="b" type="f">verleyden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Maer haer lustet te <rhyme label="b" type="f" corresp="#s9:l3">verbreyden</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Daer toe sijnse soo wel <rhyme label="c" type="m">ghesint</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Vwen gheest heuet in haer herte <rhyme label="c" type="m" corresp="#s9:l5">gheprint</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Sy latent aen haer <rhyme label="d" type="f">beclijuen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">En Christus den meester <rhyme label="d" type="f" corresp="#s9:l7">blijuen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Christus heeft ons sijn liefde <rhyme label="a" type="m">geleert</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Maer veel valsche geesten <rhyme label="a" type="m" corresp="#s10:l1">verweert</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Doen nv die liefde <rhyme label="b" type="f">vercouwen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Met valsche leer die si wt <rhyme label="b" type="f" corresp="#s10:l3">spouwen</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Van twisten en maken si geen <rhyme label="c" type="m">werck</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Sy willen wesen veel beter <rhyme label="c" type="m" corresp="#s10:l5">clerck</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Dan Paulus een vat <rhyme label="d" type="f">wtuercoren</rhyme></l>
        <l id="s10:l8" met="3" type="-+">O Wee die sulckx <rhyme label="d" type="f" corresp="#s10:l7">oorboren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Heer ter eeren v woort is dit <rhyme label="a" type="m">gedicht</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Jck bidde v mijn God mijn hert <rhyme label="a" type="m" corresp="#s11:l1">verlicht</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Dat ick niet en <rhyme label="b" type="f">versaghe</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Nv noch inden lesten <rhyme label="b" type="f" corresp="#s11:l3">daghe</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Wilt my dan heere bystant <rhyme label="c" type="m">doen</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Dat ick mach hooren v soet <rhyme label="c" type="m" corresp="#s11:l5">Sermoen</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Coemt ghy <rhyme label="d" type="f">ghebenedijden</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Dan sult ghy my recht <rhyme label="d" type="f" corresp="#s11:l7">verblijden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
