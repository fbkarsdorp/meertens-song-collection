<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182589</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Lass nur der Politiker sprechen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vivat Bataave laat ons <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Tot eer en roem van het <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Geen Brit of Keizer kan ons <rhyme label="a" type="f" corresp="#s1:l1">dwingen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">De Dwinglandy is by ons <rhyme label="b" type="m" corresp="#s1:l2">verband</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wy keeren terug en brengen <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die van ons lang gewenste <rhyme label="c" type="m" corresp="#s1:l5">Vree</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">wy trokken als braave <rhyme label="a" type="f">oorlogsknegten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Al voor de vryheid in het <rhyme label="b" type="m">Veld</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Het eerste woord was dood of <rhyme label="a" type="f" corresp="#s2:l1">vegton</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">De zinspreuk een bataatsche <rhyme label="b" type="m" corresp="#s2:l2">held</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">wy vogten met een srisse <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En waagden voor het Vaderland ons <rhyme label="c" type="m" corresp="#s2:l5">bloed</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Met veel gevaar en veel <rhyme label="a" type="f">Marscheeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Kwamen wy tog in Duitschland <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">weisburgs vesting wou zig <rhyme label="a" type="f" corresp="#s3:l1">weeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maar het waar haast met hun <rhyme label="b" type="m" corresp="#s3:l2">gedaan</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">want het bataafsche bom en <rhyme label="c" type="m">kanon</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Gaave wy haar de <rhyme label="c" type="m" corresp="#s3:l5">wellekom</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wy hadden de Vesting ras <rhyme label="a" type="f">beschooten</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Zy boode trouwe <rhyme label="b" type="m">wederstand</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Veel braave helde ter neer <rhyme label="a" type="f" corresp="#s4:l1">geschooten</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ten dienst van 't lieve <rhyme label="b" type="m" corresp="#s4:l2">Vaderland</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Maar den eenendertigste <rhyme label="c" type="m">dag</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Het Fort in onze handen <rhyme label="c" type="m" corresp="#s4:l5">zag</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Toen zag men ons wapen <rhyme label="a" type="f">triompheeren</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En net accoord wierd <rhyme label="b" type="m">aangegaan</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dat zy in volle krygsmans <rhyme label="a" type="f" corresp="#s5:l1">eeren</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Het Fort moesten veriaaten <rhyme label="b" type="m" corresp="#s5:l2">gaan</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Na haar Armoe frank en <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Bataaven! haar de <rhyme label="c" type="m" corresp="#s5:l5">slaverny</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Tot Zwynfort zag men nieuwe <rhyme label="a" type="f">blyken</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Var onze edele <rhyme label="b" type="m">Heldenschaar</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Kyzertyke armÃªe moest <rhyme label="a" type="f" corresp="#s6:l1">wyken</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Want onze jaagers en <rhyme label="b" type="m" corresp="#s6:l2">Huzaar</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Vervolgde haar van alle <rhyme label="c" type="m">kant</rhyme>.</l>
        <l id="s6:l6" met="4" type="-+">Met sabel en pistool in de <rhyme label="c" type="m" corresp="#s6:l5">hand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wy volgde haar al zonder <rhyme label="a" type="f">schroomen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Maar hadde nog een slimmer <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Eer wy in B[...]mberg zyn <rhyme label="a" type="f" corresp="#s7:l1">gekoomen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Het was voor ons een harde <rhyme label="b" type="m" corresp="#s7:l2">slag</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Maar 't Bataafsche volk <rhyme label="c" type="m">triumpheerd</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Met roem in haar Vaderland <rhyme label="c" type="m" corresp="#s7:l5">weerkeerd</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Nu zal Hollands tuyn weer <rhyme label="a" type="f">groeijen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Tot nut en best van 't <rhyme label="b" type="m">algemeen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Koopmanschap en Zeevaard <rhyme label="a" type="f" corresp="#s8:l1">bloeijen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En lieve meisjes houd u tot <rhyme label="b" type="m" corresp="#s8:l2">Vree</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Want wy Bataave zyn <rhyme label="c" type="m">bereid</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Trekke alryd voor u ten <rhyme label="c" type="m" corresp="#s8:l5">streid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
