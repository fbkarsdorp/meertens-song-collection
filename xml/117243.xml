    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn oversoete lief <rhyme label="a" type="f">gepresen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat is een Meisje <rhyme label="b" type="m">jent</rhyme></l>
        <l id="s1:l3" met="1" type="-+">‘k <rhyme label="b" type="m" corresp="#s1:l2">Bekend</rhyme>,</l>
        <l id="s1:l4" met="1" type="-+">By <rhyme label="a" type="f" corresp="#s1:l1">dezen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Sy is <rhyme label="b" type="m" corresp="#s1:l2">Excelent</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Haer neusje als een <rhyme label="c" type="m">klaeu</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Past by haer vrind’lik <rhyme label="a" type="f" corresp="#s1:l1">wezen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">En mond niet <rhyme label="c" type="m" corresp="#s1:l6">gaeu</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Ia lipjes vol en <rhyme label="c" type="m" corresp="#s1:l6">graeu</rhyme>.</l>
      </lg>
    </lg>
