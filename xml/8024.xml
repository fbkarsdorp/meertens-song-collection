  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NV laet ghy, Heer, <rhyme label="a" type="m">oprecht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gaen in vred' uwen <rhyme label="a" type="m" corresp="#s1:l1">knecht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Na u beloft' <rhyme label="b" type="f">gestadigh</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Na dat mijn oogen <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hebben gesien <rhyme label="c" type="m" corresp="#s1:l4">voorwaer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Vwen Heylandt, <rhyme label="b" type="f" corresp="#s1:l3">genadigh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Salighmaker <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Aller volcken <rhyme label="a" type="m" corresp="#s2:l1">gemeyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die aennemen zijn <rhyme label="b" type="f">leere</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Een licht schijnende <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Den Heydenen <rhyme label="c" type="m" corresp="#s2:l4">eenpaer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">End' oock Israels <rhyme label="b" type="f" corresp="#s2:l3">eere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
