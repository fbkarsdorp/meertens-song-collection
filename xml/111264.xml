        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="children's song">kinderlied</tag>
    </tags>
    <melody>Vrolijke uurtjes</melody>
    <categories>
      <category translation="children">kinderen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Silvere <rhyme label="a" type="f">stroomen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die de <rhyme label="a" type="f" corresp="#s1:l1">zoomen</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van dit weelig-land <rhyme label="b" type="m">besproeyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Door my traane <rhyme label="a" type="f" corresp="#s1:l1">voorgekoomen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">En myn schryen 't oog <rhyme label="b" type="m" corresp="#s1:l3">ontvloeyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Laat uw <rhyme label="a" type="f">huyzen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Zagjes <rhyme label="a" type="f" corresp="#s2:l1">bruyzen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daar myn lief te slaapen <rhyme label="b" type="m">lyt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maar als zy ontwaakt door 't <rhyme label="a" type="f" corresp="#s2:l1">bruyzen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Zegt dan hoe haar Minnaar <rhyme label="b" type="m" corresp="#s2:l3">schryt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Rooze <rhyme label="a" type="f">knopjes</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Aan de <rhyme label="a" type="f" corresp="#s3:l1">topjes</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Van uw struykjes vast <rhyme label="b" type="m">gehegt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Stort uw zoete hoonig <rhyme label="a" type="f" corresp="#s3:l1">dropjes</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Daar myn lief te slaape <rhyme label="b" type="m" corresp="#s3:l3">legt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Strooid u <rhyme label="a" type="f">blaatjes</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Langs de <rhyme label="a" type="f" corresp="#s4:l1">paatjes</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Als ontwaakt myn waarste <rhyme label="b" type="m">teer</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Daar zyn Floraas bloeme <rhyme label="a" type="f" corresp="#s4:l1">straatjes</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Hemel pruykt in lenten <rhyme label="b" type="m" corresp="#s4:l3">kleedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">'k Zie haar <rhyme label="a" type="f">naaderen</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Door de <rhyme label="a" type="f" corresp="#s5:l1">bladeren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ach wat hoop verspeld zy <rhyme label="b" type="m">my</rhyme>!</l>
        <l id="s5:l4" met="4" type="+-">'k Voel verheuging in myn <rhyme label="a" type="f" corresp="#s5:l1">aderen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Hemel staa myn Liefde <rhyme label="b" type="m" corresp="#s5:l3">by</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Vlieg Ã´ <rhyme label="a" type="f">winde</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Door de <rhyme label="a" type="f" corresp="#s6:l1">linde</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Door dees bosse van <rhyme label="b" type="m">lauwrier</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Haal myn lieve <rhyme label="a" type="f" corresp="#s6:l1">ziels-beminde</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Breng haar op uw vleuglen <rhyme label="b" type="m" corresp="#s6:l3">hier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-"><rhyme label="a" type="f">Nagtegaalen</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Die de <rhyme label="a" type="f" corresp="#s7:l1">daalen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Doet weergallemen in 't <rhyme label="b" type="m">ront</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Zing nu in uw groene <rhyme label="a" type="f" corresp="#s7:l1">zalen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Welkom hier u <rhyme label="b" type="m" corresp="#s7:l3">roozemond</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">Vlegt <rhyme label="a" type="f">lauwrieren</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Strooid <rhyme label="a" type="f" corresp="#s8:l1">saffiren</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Diasinten langs de <rhyme label="b" type="m">grond</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Wild dit zalig woud <rhyme label="a" type="f" corresp="#s8:l1">verzieren</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Op de komst van <rhyme label="b" type="m" corresp="#s8:l3">roozemond</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
