<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184513</id>
    <date>
      <year-start>1803</year-start>
      <year-end>1803</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Waar zal ik mijn klacht voleinden</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Coridon speeld een <rhyme label="a" type="f">liedje</rhyme></l>
        <l id="s1:l2" met="4" type="+-">In de koele <rhyme label="b" type="m">dageraad</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Op een dungebogen <rhyme label="a" type="f" corresp="#s1:l1">rietje</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Na de kunst en na de <rhyme label="b" type="m" corresp="#s1:l2">maat</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Hy zat dus heel lief te <rhyme label="c" type="f">kweelen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">In een aangenaam <rhyme label="d" type="m">saizoen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Wyl zyn wollig vee liep <rhyme label="c" type="f" corresp="#s1:l5">speelen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Door het lief en jeugdig <rhyme label="d" type="m" corresp="#s1:l6">groen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al zyn speelen en zyn <rhyme label="a" type="f">vreugde</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Was alleen om <rhyme label="b" type="m">Rosemond</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">En wyl hy zig dus <rhyme label="a" type="f" corresp="#s2:l1">verheugde</rhyme></l>
        <l id="s2:l4" met="4" type="+-">In dees blyde <rhyme label="b" type="m" corresp="#s2:l2">morgenstond</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Zag hy haar van verre <rhyme label="c" type="f">komen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">In een groene en lauge <rhyme label="d" type="m">laan</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Onder 't lommer van de <rhyme label="c" type="f" corresp="#s2:l5">boomen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Daar zy met haar vee kwam <rhyme label="d" type="m" corresp="#s2:l6">aan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy begon haar straks te <rhyme label="a" type="f">groeten</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Met een lief en zoet <rhyme label="b" type="m">gelaat</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En riep, wat komt my <rhyme label="a" type="f" corresp="#s3:l1">ontmoeten</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">O wat blyde <rhyme label="b" type="m" corresp="#s3:l2">dageraad</rhyme>!</l>
        <l id="s3:l5" met="4" type="+-">Gun my dat ik u mag <rhyme label="c" type="f">kussen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Aangenaame <rhyme label="d" type="m">Rozemond</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Wilt myn minnevlam dan <rhyme label="c" type="f" corresp="#s3:l5">blussen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">'t Minnewigt heest my <rhyme label="d" type="m" corresp="#s3:l6">doorwond</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Uw gezigt dat doet my <rhyme label="a" type="f">zwigten</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Voor het kleine kind der <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Want ik voel zyn looze <rhyme label="a" type="f" corresp="#s4:l1">schigten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Help my waarde <rhyme label="b" type="m" corresp="#s4:l2">herderin</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Ach myn aangenaame <rhyme label="c" type="f">meisje</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Help uw herder <rhyme label="d" type="m">Coridon</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Laat ons te zaamen een <rhyme label="c" type="f" corresp="#s4:l5">reisje</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Zitten aan dees klaare <rhyme label="d" type="m" corresp="#s4:l6">bron</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Coridon zeg my wat <rhyme label="a" type="f">reden</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Hebt gy dog om zoo te <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">'k Laat u immers wel te <rhyme label="a" type="f" corresp="#s5:l1">vreeden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En waerom komt gy by <rhyme label="b" type="m" corresp="#s5:l2">myn</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Gaat maar by u <rhyme label="c" type="f">Zilviaatje</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Daar is een poeslige <rhyme label="d" type="m">meid</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Ja zy houd graag van een <rhyme label="c" type="f" corresp="#s5:l5">praatje</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">En ook van de <rhyme label="d" type="m" corresp="#s5:l6">minnestryd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Neen, ik mag haar gantsch niet <rhyme label="a" type="f">lyden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Gy alleen legt in myn <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Liefje wilt my dog <rhyme label="a" type="f" corresp="#s6:l1">verblyden</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">En geneest myn <rhyme label="b" type="m" corresp="#s6:l2">minnesmert</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Zeg my, hoe kunt gy zo <rhyme label="c" type="f">wezen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">'k Min uw zoete Lief <rhyme label="d" type="m">alleen</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Gy hebt voor geen aar te <rhyme label="c" type="f" corresp="#s6:l5">vrezen</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Neen myn waarde Engel, <rhyme label="d" type="m" corresp="#s6:l6">neen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ach, ik zie wel braave <rhyme label="a" type="f">herder</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Dat uw zuivre kuische <rhyme label="b" type="m">min</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Gaat veel liever en veel <rhyme label="a" type="f" corresp="#s7:l1">verder</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Als ik my gebeeld had <rhyme label="b" type="m" corresp="#s7:l2">in</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Al uw aangenaame <rhyme label="c" type="f">vleijen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Die behaagen zo myn <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Dat ik u op 't hoogst mag <rhyme label="c" type="f" corresp="#s7:l5">leljen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En u in myn hert <rhyme label="d" type="m" corresp="#s7:l6">bemin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O wat lieffelyke <rhyme label="a" type="f">woorden</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Vloeijen uit uw lieve <rhyme label="b" type="m">mond</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Woorden die myn ziel <rhyme label="a" type="f" corresp="#s8:l1">bekoorden</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Zo haast als ik die <rhyme label="b" type="m" corresp="#s8:l2">verstond</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Daar ontfang dan nu wat <rhyme label="c" type="f">kusjes</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Aangenaame <rhyme label="d" type="m">herderin</rhyme>!</l>
        <l id="s8:l7" met="4" type="+-">Laat ons nu met dartele <rhyme label="c" type="f" corresp="#s8:l5">lusjes</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Aan een hegten onze <rhyme label="d" type="m" corresp="#s8:l6">min</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
