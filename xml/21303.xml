        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="vermaanlied" translation="admonitory song">vermaanlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">BEwaert my <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Tot elcken <rhyme label="a" type="f" corresp="#s1:l1">keere</rhyme></l>
        <l id="s1:l3" met="4" type="-+">In uwer liefden tot alder <rhyme label="b" type="m">stont</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Wilt toch vermeeren, v weghen <rhyme label="a" type="f" corresp="#s1:l1">leeren</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Ons die met sonden zijn <rhyme label="b" type="m" corresp="#s1:l3">doorwont</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Maect my Heere door v goetheyt <rhyme label="b" type="m" corresp="#s1:l3">ghesont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Want ick vol <rhyme label="a" type="f">sonden</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Nv ben <rhyme label="a" type="f" corresp="#s2:l1">ghebonden</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Mijn moeder heeft my daer in <rhyme label="b" type="m">ghebaert</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Tot allen stonden, hoort mijn <rhyme label="a" type="f" corresp="#s2:l1">vermonden</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Ben ick metter broosscher natueren <rhyme label="b" type="m" corresp="#s2:l3">beswaert</rhyme></l>
        <l id="s2:l6" met="4" type="-+">En hebbe deen quaet op dander <rhyme label="b" type="m" corresp="#s2:l3">vergaert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Want wy ons <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Hebben <rhyme label="a" type="f" corresp="#s3:l1">ghegheuen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Tot sonden en ouertredinghe <rhyme label="b" type="m">swaer</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Heere verheuen, wilt nv <rhyme label="a" type="f" corresp="#s3:l1">vergheuen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Ons misdaden bermhertighe <rhyme label="b" type="m" corresp="#s3:l3">vaer</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Maect nv v goetheyt aen ons <rhyme label="b" type="m" corresp="#s3:l3">openbaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wy zijn <rhyme label="a" type="f">verladen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Met ons <rhyme label="a" type="f" corresp="#s4:l1">misdaden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">En dinct doch Heere ons boosheyt <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Want wy vol quaden, dus om <rhyme label="a" type="f" corresp="#s4:l1">ghenaden</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Bidden wy Heere ons doch <rhyme label="b" type="m" corresp="#s4:l3">aensiet</rhyme></l>
        <l id="s4:l6" met="4" type="-+">En ghedenct doch niet, watter is <rhyme label="b" type="m" corresp="#s4:l3">gheschiet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Wie mach staen <rhyme label="a" type="f">blijuen</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Naer Dauids <rhyme label="a" type="f" corresp="#s5:l1">schrijuen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">En ghy met ons int oordeel wilt <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Wy snoo catijuen, met siel en <rhyme label="a" type="f" corresp="#s5:l1">lijuen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Souden int eewich vier moeten <rhyme label="b" type="m" corresp="#s5:l3">braen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">O Heere wilt ons in ghenaden <rhyme label="b" type="m" corresp="#s5:l3">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Wilt doch nv <rhyme label="a" type="f">spanen</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Naer Schrifts <rhyme label="a" type="f" corresp="#s6:l1">vermanen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Dat oude leuen seer boos en <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Beweent met tranen, en wilt niet <rhyme label="a" type="f" corresp="#s6:l1">wanen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">God sal vergheuen al v <rhyme label="b" type="m" corresp="#s6:l3">misdaet</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Want hy is doch selue ons <rhyme label="b" type="m" corresp="#s6:l3">aduocaet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wilt niet <rhyme label="a" type="f">verflouwen</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Maer vast <rhyme label="a" type="f" corresp="#s7:l1">betrouwen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Op Christum onse voorspraecke <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Die ons van rouwen, en swaer <rhyme label="a" type="f" corresp="#s7:l1">benouwen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Uerlost heeft door zijn liefde <rhyme label="b" type="m" corresp="#s7:l3">soet</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Met dat wtstorten van zijn roode <rhyme label="b" type="m" corresp="#s7:l3">bloet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Coemt al <rhyme label="a" type="f">ghemeene</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Die licht in <rhyme label="a" type="f" corresp="#s8:l1">weene</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Beswaert met groote packen <rhyme label="b" type="m">nv</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Want hy alleene, voor groot en <rhyme label="a" type="f" corresp="#s8:l1">cleene</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Sijn bloet ghestort heeft met grooten <rhyme label="b" type="m" corresp="#s8:l3">gru</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Uoor onse sonden dat segghe ick <rhyme label="b" type="m" corresp="#s8:l3">v</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Heere <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Ghy wilt <rhyme label="a" type="f" corresp="#s9:l1">ghenesen</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Al ons crancheden in corter <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Hier om by desen, also wy <rhyme label="a" type="f" corresp="#s9:l1">lesen</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Roept ghy alle die beladen <rhyme label="b" type="m" corresp="#s9:l3">zijt</rhyme></l>
        <l id="s9:l6" met="4" type="-+">En maect ons van al onse sonden <rhyme label="b" type="m" corresp="#s9:l3">quijt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Wie soude nv <rhyme label="a" type="f">trueren</rhyme></l>
        <l id="s10:l2" met="2" type="-+">Als wy van <rhyme label="a" type="f" corresp="#s10:l1">vueren</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Weten dat onse sonden zijn <rhyme label="b" type="m">betaelt</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Onse humueren, en swaer <rhyme label="a" type="f" corresp="#s10:l1">volueren</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Heeft God door zijn lijden <rhyme label="b" type="m" corresp="#s10:l3">afghehaelt</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Dus siet dat ghy hem te dancken niet en <rhyme label="b" type="m" corresp="#s10:l3">faelt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Laet doch niet <rhyme label="a" type="f">lijen</rhyme></l>
        <l id="s11:l2" met="2" type="-+">Eenighe <rhyme label="a" type="f" corresp="#s11:l1">tijen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Christum te dancken van zijn liefde <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Want hy quam lijen, om ons <rhyme label="a" type="f" corresp="#s11:l1">beurijen</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Uan duyuel, sonde, helle ende <rhyme label="b" type="m" corresp="#s11:l3">doot</rhyme></l>
        <l id="s11:l6" met="4" type="-+">En verlost al tsamen wter <rhyme label="b" type="m" corresp="#s11:l3">noot</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Lof al te <rhyme label="a" type="f">gader</rhyme></l>
        <l id="s12:l2" met="2" type="-+">God ende <rhyme label="a" type="f" corresp="#s12:l1">Uader</rhyme></l>
        <l id="s12:l3" met="4" type="-+">En seght hem lof en danck tot alder <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Hy is die ader, en ons <rhyme label="a" type="f" corresp="#s12:l1">berader</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Die zijnen sone <rhyme label="b" type="m" corresp="#s12:l3">ghebenedijt</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Uoor ons allen ghesonden heeft inden <rhyme label="b" type="m" corresp="#s12:l3">strijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
