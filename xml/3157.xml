    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Polyphemus aan de stranden</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HEy tsa lustighe <rhyme label="a" type="f">Gespeeltjes</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Laet u <rhyme label="a" type="f" corresp="#s1:l1">keeltjes</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Singen soete vaersjes <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Toont u vrolijcke <rhyme label="c" type="f">genuchjes</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Toont u <rhyme label="c" type="f" corresp="#s1:l4">kluchjes</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Roemt de Bruydegom en <rhyme label="b" type="m" corresp="#s1:l3">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Siet eens hoe veel <rhyme label="a" type="f">Hemel-gootjes</rhyme></l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s2:l1">Cupidootjes</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Datter dalen uyt den <rhyme label="b" type="m">Throon</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die van Mirten en <rhyme label="c" type="f">Lauw'rieren</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Kransjes <rhyme label="c" type="f" corresp="#s2:l4">cieren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Tot dees Bruylofs <rhyme label="b" type="m" corresp="#s2:l3">vreugde-kroon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Venus met haer <rhyme label="a" type="f">poes'leden</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Daelt <rhyme label="a" type="f" corresp="#s3:l1">beneden</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Selfs om dees' Feest te <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Is verselt met duysent <rhyme label="c" type="f">lusjes</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Lachjes, <rhyme label="c" type="f" corresp="#s3:l4">Kusjes</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Diese dees twee aen komt <rhyme label="b" type="m" corresp="#s3:l3">bien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dese twee die d' <rhyme label="a" type="f">Echtens-banden</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Met haer <rhyme label="a" type="f" corresp="#s4:l1">handen</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Met haer hert, haer ziel, ja <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Hebben 't saem soo vast <rhyme label="c" type="f">verbonden</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Dat geen <rhyme label="c" type="f" corresp="#s4:l4">wonden</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Dan de doot haer scheyen <rhyme label="b" type="m" corresp="#s4:l3">sal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Siet de Bruyd'gom als een <rhyme label="a" type="f">Koning</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Die die <rhyme label="a" type="f" corresp="#s5:l1">kroningh</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Van de heyl'ge Echt <rhyme label="b" type="m">ontfanght</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Waer in dat sijn ziels <rhyme label="c" type="f">beminde</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Vrucht gaet <rhyme label="c" type="f" corresp="#s5:l4">vinden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Die nae 't soetste soet <rhyme label="b" type="m" corresp="#s5:l3">verlanght</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Siet eens hoe haer oochjes <rhyme label="a" type="f">loncken</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Jae als <rhyme label="a" type="f" corresp="#s6:l1">voncken</rhyme></l>
        <l id="s6:l3" met="4" type="+-">'t Wt haer lieve lichjes <rhyme label="b" type="m">straelt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Die verselt met <rhyme label="c" type="f">Tover-kusjes</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">
          <rhyme label="c" type="f" corresp="#s6:l4">Minne-gunsjes</rhyme>
        </l>
        <l id="s6:l6" met="4" type="+-">Op de Bruydgoms boesem <rhyme label="b" type="m" corresp="#s6:l3">daelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hoogen Hemel wilt doch <rhyme label="a" type="f">geven</rhyme></l>
        <l id="s7:l2" met="2" type="+-">g' Luckigh <rhyme label="a" type="f" corresp="#s7:l1">leven</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Voor dees twee gehuwde <rhyme label="b" type="m">lien</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Datse als den Wijngaert <rhyme label="c" type="f">groeyen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">En door 't <rhyme label="c" type="f" corresp="#s7:l4">bloeyen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Hare jongen Telgen <rhyme label="b" type="m" corresp="#s7:l3">sien</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Nu de nacht sou soo wel <rhyme label="a" type="f">scheyen</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Laet ons <rhyme label="a" type="f" corresp="#s8:l1">leyen</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Dees twee Lieven nae haer <rhyme label="b" type="m">rust</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Maeckt dat bid' ick doch <rhyme label="c" type="f">Gespeeltjes</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Geen <rhyme label="c" type="f" corresp="#s8:l4">krackeeljes</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Maer vergunt haer soete <rhyme label="b" type="m" corresp="#s8:l3">lust</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Want ick segh u <rhyme label="a" type="f">Speelgenootje</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">
          <rhyme label="a" type="f" corresp="#s9:l1">Cupidootje</rhyme>
        </l>
        <l id="s9:l3" met="4" type="+-">Sit al sijn Boogh en <rhyme label="b" type="m">spant</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Laet ghy haest geen vreught <rhyme label="c" type="f">genieten</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Hy sal <rhyme label="c" type="f" corresp="#s9:l4">schieten</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Bey u hartjens in de <rhyme label="b" type="m" corresp="#s9:l3">brandt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
