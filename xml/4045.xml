        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="praise song">loflied</tag>
      <tag translation="">geestelijk</tag>
      <tag translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Och lig dij nu en slaapt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Heere van <rhyme label="a" type="m">hemelrijck</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Rekcht yst dat wij haer <rhyme label="b" type="f">louen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Si en heeft gheen <rhyme label="a" type="m" corresp="#s1:l1">ghelijck</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al inden throon hier <rhyme label="b" type="f" corresp="#s1:l2">bouen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Tot haerder <rhyme label="c" type="m">weerdicheyt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">So eest dat ick <rhyme label="d" type="f">beghinne</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Minen troost aen haer <rhyme label="c" type="m" corresp="#s1:l5">leyt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s1:l6">apostolinne</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haren naem is sonder <rhyme label="a" type="m">waen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Maria <rhyme label="b" type="f">magdaleene</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Penitencie heeft si <rhyme label="a" type="m" corresp="#s2:l1">ghedaen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ende wert van sonden <rhyme label="b" type="f" corresp="#s2:l2">reene</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Nae Magdalum dat <rhyme label="c" type="m">casteel</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Hiet si / Gods liefste <rhyme label="d" type="f">minne</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Si koos dat beste <rhyme label="c" type="m" corresp="#s2:l5">deel</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s2:l6">apostolinne</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daer Iesus sadt ende <rhyme label="a" type="m">adt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Quam si met saluen <rhyme label="b" type="f">binnen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Met tranen maecte si <rhyme label="a" type="m" corresp="#s3:l1">nat</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sijn voeten wt groter <rhyme label="b" type="f" corresp="#s3:l2">minnen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Die salue goot si op sijn <rhyme label="c" type="m">hoot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Alsoe ic wel <rhyme label="d" type="f">bekinne</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Si bleef bi hem ter <rhyme label="c" type="m" corresp="#s3:l5">doot</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s3:l6">apostolinne</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Si verlost vander <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als ick bescreuen <rhyme label="b" type="f">vinde</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Des princen vrouwe <rhyme label="a" type="m" corresp="#s4:l1">groot</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die sterf inder zee met <rhyme label="b" type="f" corresp="#s4:l2">kinde</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Te Romen als ick <rhyme label="c" type="m">vernam</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Track hy bedruct van <rhyme label="d" type="f">sinne</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Verblijt hi weder <rhyme label="c" type="m" corresp="#s4:l5">quam</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s4:l6">apostolinne</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghelijc die roose <rhyme label="a" type="m">root</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Is si vol minnen <rhyme label="b" type="f">schoone</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Si helpt al inder <rhyme label="a" type="m" corresp="#s5:l1">noot</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Si blinct nu inden <rhyme label="b" type="f" corresp="#s5:l2">throone</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Si verlicht den hemel <rhyme label="c" type="m">claer</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ghelijck een <rhyme label="d" type="f">seraphinne</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Mijn siele beueel ick <rhyme label="c" type="m" corresp="#s5:l5">haer</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s5:l6">apostolinne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Van vrouwen men niet en <rhyme label="a" type="m">leest</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die God soe hadde <rhyme label="b" type="f">vercoren</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ten hadde Marie <rhyme label="a" type="m" corresp="#s6:l1">gheweest</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Daer hi af wert <rhyme label="b" type="f" corresp="#s6:l2">gheboren</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Grote eere hi daer <rhyme label="c" type="m">bewees</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Si was sijn weerde <rhyme label="d" type="f">vriendinne</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Want hyse selue <rhyme label="c" type="m" corresp="#s6:l5">prees</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s6:l6">apostolinne</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Inder woestinen daer si <rhyme label="a" type="m">was</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wert si alle daghe <rhyme label="b" type="f">verheuen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Vanden engelen als ick <rhyme label="a" type="m" corresp="#s7:l1">las</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Die altijt bi haer <rhyme label="b" type="f" corresp="#s7:l2">bleuen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Aldus ick louen <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Hoe wel icx niet weerdich en <rhyme label="d" type="f">binne</rhyme></l>
        <l id="s7:l7" met="3" type="-+">V weerde vrouwe <rhyme label="c" type="m" corresp="#s7:l5">soet</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s7:l6">apostolinne</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O weerde vrouwe <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van God so hooch <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Mijn siele na v <rhyme label="a" type="m" corresp="#s8:l1">beheert</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Dat si bi v mach <rhyme label="b" type="f" corresp="#s8:l2">wesen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Bidt voer mi suyuer <rhyme label="c" type="m">iuecht</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Dat ick Gods rijck <rhyme label="d" type="f">ghewinne</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Want ghi dat wel <rhyme label="c" type="m" corresp="#s8:l5">vermuecht</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Lof Gods <rhyme label="d" type="f" corresp="#s8:l6">apostolinne</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
