      </coordinates>
    </location>
    <tags>
      <tag standard-name="afscheidslied" translation="farewell song">afscheidslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="occasions">gelegenheden</category>
    </categories>
  <author>Zacharias Heyns</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">OOrlof, danc prijs en <rhyme label="a" type="f">eere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O Speelcoorens <rhyme label="b" type="m">delicaet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die huyden meer en <rhyme label="a" type="f" corresp="#s1:l1">meere</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Bewesen hebt metter <rhyme label="b" type="m" corresp="#s1:l2">daet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">hoe dz hier Trou moet <rhyme label="c" type="f">blijcken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Want ghy vercierden <rhyme label="d" type="m">gheheel</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Het Hof der <rhyme label="c" type="f" corresp="#s1:l5">Rethorijcken</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Door constige blomkes <rhyme label="d" type="m" corresp="#s1:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon Orangni <rhyme label="a" type="f">lelyen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In liefd' groeyende <rhyme label="b" type="m">eerbaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Saghen wy met <rhyme label="a" type="f" corresp="#s2:l1">verblyen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ooc menich nut Cooren <rhyme label="b" type="m" corresp="#s2:l2">aer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Liefd' moet blijcken by <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Waren Rood' rooskens <rhyme label="d" type="m">bereyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wiens cleur ons herts <rhyme label="c" type="f" corresp="#s2:l5">genesen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Altoos Aensiet de <rhyme label="d" type="m" corresp="#s2:l6">jonckheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hier hadt ghy <rhyme label="a" type="f">d'Haselieren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Daermen Aensiet Gods cracht <rhyme label="b" type="m">jent</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Acoleykens vvit <rhyme label="a" type="f" corresp="#s3:l1">cieren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ons door Liefd' es <rhyme label="b" type="m" corresp="#s3:l2">'tfondament</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Noch vontmen <rhyme label="c" type="f">Akerboomkens</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Aensiet Liefd' sonder <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Sproten d'Lavender <rhyme label="c" type="f" corresp="#s3:l5">bloemkens</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">VVt levender jonste <rhyme label="d" type="m" corresp="#s3:l6">siet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">d'Lely onder den <rhyme label="a" type="f">dooren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Blijft noch VVt liefden <rhyme label="b" type="m">bestaen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">d'Galisbloemkens als <rhyme label="a" type="f" corresp="#s4:l1">voren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">VVt liefden versocht, siet <rhyme label="b" type="m" corresp="#s4:l2">aen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">B'neffens 'tGoud blomken t' <rhyme label="c" type="f">samen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">VVt jonsten begrepen <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">De Coren-blomkens <rhyme label="c" type="f" corresp="#s4:l5">quamen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Wt Met gheneuchten <rhyme label="d" type="m" corresp="#s4:l6">certeyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Sonne blomkens <rhyme label="a" type="f">groeyen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">noyt meerder vreucht hier dan <rhyme label="b" type="m">die</rhyme></l>
        <l id="s5:l3" met="3" type="-+">d'VVijngaert rancxken siet <rhyme label="a" type="f" corresp="#s5:l1">bloeyen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Liefd' boven al, Angiers <rhyme label="b" type="m" corresp="#s5:l2">vvit</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">In liefd' getrou, O <rhyme label="c" type="f">Schoone</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Prins tweeden <rhyme label="d" type="m">Terentivs</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Elc brengt zijn cracht ten <rhyme label="c" type="f" corresp="#s5:l5">toone</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Adieu nemende <rhyme label="d" type="m" corresp="#s5:l6">aldus</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
