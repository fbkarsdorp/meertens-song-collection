<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>111882</id>
    <date>
      <year-start>1745</year-start>
      <year-end>1745</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags>
      <tag>children's song</tag>
    </tags>
    <melody>Op een tijd op een tijd</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WAt voor Vreugd, Wat voor <rhyme label="a" type="m">Vreugd</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">ô Onbezonnen <rhyme label="a" type="m" corresp="#s1:l1">Jeugt</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Steld gy dog in het <rhyme label="b" type="f">Minnen</rhyme>?</l>
        <l id="s1:l4" met="4" type="-+">Uw soet verandert haest in <rhyme label="c" type="m">roet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ey wilt u over weeldrig <rhyme label="c" type="m" corresp="#s1:l4">bloet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Eer 't u berouwt, <rhyme label="b" type="f" corresp="#s1:l3">verwinnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Druk en smart, Druk en <rhyme label="a" type="m">smart</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gevoeld uw Jeugdig <rhyme label="a" type="m" corresp="#s2:l1">Hart</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Zoo drae gy komt te <rhyme label="b" type="f">Trouwen</rhyme>;</l>
        <l id="s2:l4" met="4" type="-+">De Speelman is nau van het <rhyme label="c" type="m">dak</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Uw Vryheit heeft terstond een <rhyme label="c" type="m" corresp="#s2:l4">krak</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wagt u dan voor de <rhyme label="b" type="f" corresp="#s2:l3">Vrouwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Binnens Huis, Binnens <rhyme label="a" type="m">Huis</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ontmoet gy 't meeste <rhyme label="a" type="m" corresp="#s3:l1">kruis</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Gy moet gedurig <rhyme label="b" type="f">slaeven</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Gelyk een Ezel Paert of <rhyme label="c" type="m">Bok</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Of gy voeld daetlyk met een <rhyme label="c" type="m" corresp="#s3:l4">stok</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Uw gantsche Lyf <rhyme label="b" type="f" corresp="#s3:l3">bedraeven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Alle dach, Alle <rhyme label="a" type="m">dach</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Moet gy met diep <rhyme label="a" type="m" corresp="#s4:l1">ontzach</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Opvolgen haer <rhyme label="b" type="f">bevelen</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Want zoo daer maer een hair aen <rhyme label="c" type="m">schort</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Het Varken u terstont <rhyme label="c" type="m" corresp="#s4:l4">beknort</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En tiert uyt luyder <rhyme label="b" type="f" corresp="#s4:l3">kelen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Als j' een Kind, Als j' een <rhyme label="a" type="m">Kind</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">By haer dan <rhyme label="a" type="m" corresp="#s5:l1">overwind</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gy moet gedurig <rhyme label="b" type="f">Wiegen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Gelyk een Sul of regte <rhyme label="c" type="m">Jan</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Past dat een vry geboren <rhyme label="c" type="m" corresp="#s5:l4">Man</rhyme>?</l>
        <l id="s5:l6" met="3" type="-+">Laet u zoo niet <rhyme label="b" type="f" corresp="#s5:l3">bedriegen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wat gy doet, Wat gy <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">'t Is nimmer wel of <rhyme label="a" type="m" corresp="#s6:l1">goet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Een Luybak moet gy <rhyme label="b" type="f">heeten</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En 't spreekwoord is maer al te <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">‘Een Man moet elke dag van 't <rhyme label="c" type="m" corresp="#s6:l4">Jaer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">‘Het geen zyn Wyf lust <rhyme label="b" type="f" corresp="#s6:l3">eeten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'t Meest Verdriet, 't Meest <rhyme label="a" type="m">Verdriet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Het geen men veeltyts <rhyme label="a" type="m" corresp="#s7:l1">ziet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">('k Derf 't naeulyks <rhyme label="b" type="f">openbaeren</rhyme>)</l>
        <l id="s7:l4" met="4" type="-+">Is dat een ander plukt met <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">De Bloemen van haer Eelste <rhyme label="c" type="m" corresp="#s7:l4">Jeugt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daer mee Kroond zy uw <rhyme label="b" type="f" corresp="#s7:l3">hairen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Daerom wagt, Daerom <rhyme label="a" type="m">wagt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">U voor het <rhyme label="a" type="m" corresp="#s8:l1">Vrouwgeslagt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Begeef u nooyt tot <rhyme label="b" type="f">Trouwen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Zoo leeft gy van die plaeg <rhyme label="c" type="m">bevryt</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">In vryheit en in <rhyme label="c" type="m" corresp="#s8:l4">Vrolykheit</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">'t Geen U nooyt zal <rhyme label="b" type="f" corresp="#s8:l3">berouwen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
