    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Och Jaatje lief, myn waardste <rhyme label="a" type="m">schat</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Ut, ré, mi, <rhyme label="b" type="m">fa</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Je hebt meer vlooijen als een <rhyme label="a" type="m" corresp="#s1:l1">kat</rhyme>;</l>
        <l id="s1:l4" met="2" type="-+">Mi, fa, sól, <rhyme label="b" type="m" corresp="#s1:l2">la</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Maar wil je my <rhyme label="c" type="f">behaagen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Ré, mi, fa, <rhyme label="d" type="m">sól</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Zal ik ze haast <rhyme label="c" type="f" corresp="#s1:l5">verjaagen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Ré, mi, fa, <rhyme label="d" type="m" corresp="#s1:l6">sól</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">En met B duur, en met B <rhyme label="d" type="m" corresp="#s1:l6">mól</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Ré, mi, fa, <rhyme label="d" type="m" corresp="#s1:l6">sól</rhyme>.</l>
        <l id="s1:l11" met="2" type="+-">Ré, mi, fa, <rhyme label="d" type="m" corresp="#s1:l6">sól</rhyme>.</l>
