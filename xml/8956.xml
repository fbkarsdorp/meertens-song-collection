<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8956</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Christ lag in Todesbanden</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">CHRIstus lach in Doodes <rhyme label="a" type="m">Banden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Voor onse Sonde <rhyme label="b" type="f">ghegheven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hy is weder <rhyme label="a" type="m" corresp="#s1:l1">opghestanden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Heeft ons ghebracht dat <rhyme label="b" type="f" corresp="#s1:l2">Leven</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dies sullen wy all' Vrolijck <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Godt loven en dancken <rhyme label="c" type="m" corresp="#s1:l5">fijn</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">End singhen <rhyme label="d" type="f">Alleluia</rhyme>.</l>
        <l id="s1:l8" met="2" type="-+"><rhyme label="e" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den doot niemant bedwingen <rhyme label="a" type="m">cond'</rhyme></l>
        <l id="s2:l2" met="3" type="-+">By allen Menschen <rhyme label="b" type="f">Kindren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat maecken alle onse <rhyme label="a" type="m" corresp="#s2:l1">Sond'</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gheen Onschult was te <rhyme label="b" type="f" corresp="#s2:l2">vinden</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Daer van soo quam de Doodt soo <rhyme label="c" type="m">bald'</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Ende nam over ons <rhyme label="c" type="m" corresp="#s2:l5">ghewalt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Hielt ons in syn Rijck <rhyme label="d" type="f">gh'vanghen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+"><rhyme label="e" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Jesus Christus waer Godes <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Aen onse Steed' is <rhyme label="b" type="f">comen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End heeft de Sonde wech <rhyme label="a" type="m" corresp="#s3:l1">ghedaen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daer mee den Doot <rhyme label="b" type="f" corresp="#s3:l2">ghenomen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Al zijn Recht end syn <rhyme label="c" type="m">Ghewalt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Daer bleef niet meer dan Doots <rhyme label="c" type="m" corresp="#s3:l5">gestalt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Syn Macht heeft hy <rhyme label="d" type="f">verloren</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+"><rhyme label="e" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het was een wonderlijcke <rhyme label="a" type="m">strijdt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Als Doot end Leven <rhyme label="b" type="f">ronghen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">T'leven behielt Victory <rhyme label="a" type="m" corresp="#s4:l1">wijt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Het heeft den Doot <rhyme label="b" type="f" corresp="#s4:l2">verslonghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Die Schriftuer heeft vercondicht <rhyme label="c" type="m">dat</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Hoe d'Eene Doot den Andren <rhyme label="c" type="m" corresp="#s4:l5">att</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Een spot wt den Doot is <rhyme label="d" type="f">worden</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+"><rhyme label="e" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hier is dat rechte <rhyme label="a" type="m">Paesschelam</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daer van Godt heeft <rhyme label="b" type="f">gheboden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dat isser aen des Cruyces <rhyme label="a" type="m" corresp="#s5:l1">stam</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">In heeter Lieffde <rhyme label="b" type="f" corresp="#s5:l2">ghebraden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Des Bloet dat teeckent onse <rhyme label="c" type="m">Deur</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Dat houdt de Gelooff' den Doode <rhyme label="c" type="m" corresp="#s5:l5">voor</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die Worgher can ons niet <rhyme label="d" type="f">roeren</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+"><rhyme label="e" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">So Vieren wy dat hooghe <rhyme label="a" type="m">Fest</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met Herten Vreuchd' end <rhyme label="b" type="f">Wonne</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De HEEr verschijnt tot onsen <rhyme label="a" type="m" corresp="#s6:l1">best</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">End hy is selvest die <rhyme label="b" type="f" corresp="#s6:l2">Sonne</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Die door syner ghenaden <rhyme label="c" type="m">glants</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Verleuchtet onse herten <rhyme label="c" type="m" corresp="#s6:l5">gants</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Der sonden Nacht is <rhyme label="d" type="f">vergangen</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+"><rhyme label="e" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wy Eten ende Leven <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">In rechte <rhyme label="b" type="f">Paesschevladen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die Oude Deesem niet syn <rhyme label="a" type="m" corresp="#s7:l1">sel</rhyme></l>
        <l id="s7:l4" met="3" type="-+">By den Woorde der <rhyme label="b" type="f" corresp="#s7:l2">Ghenaden</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Christus wil syn die Spijse <rhyme label="c" type="m">reyn</rhyme></l>
        <l id="s7:l6" met="4" type="-+">En spijsen onse Ziele <rhyme label="c" type="m" corresp="#s7:l5">alleyn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">De Gelooff' wil geens andren <rhyme label="d" type="f">Leven</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+"><rhyme label="e" type="m">Alleluia</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
