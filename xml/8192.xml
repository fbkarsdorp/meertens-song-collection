<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8192</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="lament">klaaglied</tag>
    </tags>
    <melody>Men lachte, danste en men zong er</melody>
    <categories>
      <category translation="emotions">emoties</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MEn lachte, danste, en men <rhyme label="a" type="f">songer</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Doen ick noch een jaar was <rhyme label="a" type="f" corresp="#s1:l1">jonger</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Maar nu laat ick de ooren <rhyme label="b" type="f">hangen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Nu Iacomijn gaat quade <rhyme label="b" type="f" corresp="#s1:l3">gangen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s1:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Terwyl ick sliep vaack vroegh en <rhyme label="a" type="f">wacker</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Zaayt een ander op mijn <rhyme label="a" type="f" corresp="#s2:l1">Acker</rhyme></l>
        <l id="s2:l3" met="4" type="-+">In dat duyvels weeligh <rhyme label="b" type="f">onkruyt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Wil al smorgens voor de zon <rhyme label="b" type="f" corresp="#s2:l3">uyt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s2:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Droegh ick noch vergulde <rhyme label="a" type="f">hoornen</rhyme>.</l>
        <l id="s3:l2" met="4" type="+-">'k Sou my soo hier niet <rhyme label="a" type="f" corresp="#s3:l1">vertoornen</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Maer dit alleen kan my nu <rhyme label="b" type="f">vertroosten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Dat van het Westen tot het <rhyme label="b" type="f" corresp="#s3:l3">Oosten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s3:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Men siet aen de <rhyme label="a" type="f">Sonne-wagen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Ram, Stier, Steenbock, <rhyme label="a" type="f" corresp="#s4:l1">Hoorn-dragen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Iupiter selver was'er moy <rhyme label="b" type="f">mee</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Toen hy 't om den suyveren kloy <rhyme label="b" type="f" corresp="#s4:l3">dee</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s4:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'t Geen ick eertyds plagh te <rhyme label="a" type="f">zayen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Moet ick nu wederom gaen <rhyme label="a" type="f" corresp="#s5:l1">mayen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">'k Heb achter 't Landt voor Bul <rhyme label="b" type="f">geloopen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s5:l5" met="4" type="-+">'k Moet nu die vreught wel dier <rhyme label="b" type="f" corresp="#s5:l3">bekopen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s5:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Brielingh-stoppers, Post, en <rhyme label="a" type="f">lagers</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Zijn verplicht aen <rhyme label="a" type="f" corresp="#s6:l1">Hooren-dragers</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Dat men de Hoorens niet in 't Lant <rhyme label="b" type="f">had</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Hoe quam het mes oyt aen 't <rhyme label="b" type="f" corresp="#s6:l3">hantvat</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s6:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Siet men de Hoorns niet <rhyme label="a" type="f">braveren</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Op Servies van groote <rhyme label="a" type="f" corresp="#s7:l1">Heeren</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Siet men de Hoorns niet <rhyme label="b" type="f">bewaren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Voor 't abebordt en <rhyme label="b" type="f" corresp="#s7:l3">Lantaeren</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s7:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die wel eens een verkeertje <rhyme label="a" type="f">wagen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Siet men steets na Hoorns <rhyme label="a" type="f" corresp="#s8:l1">vragen</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Daer wert de valsheydt mee <rhyme label="b" type="f">gebannen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Voor die hokesbokes <rhyme label="b" type="f" corresp="#s8:l3">mannen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s8:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">'k Wil nu geen meer honden <rhyme label="a" type="f">houwen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">'k Derf men daer niet op <rhyme label="a" type="f" corresp="#s9:l1">betrouwen</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Is Actaeon niet <rhyme label="b" type="f">op-gevreten</rhyme>?</l>
        <l id="s9:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">cornibus</rhyme></l>
        <l id="s9:l5" met="4" type="-+">En vlack het lest na mijn <rhyme label="b" type="f" corresp="#s9:l3">gebeten</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s9:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">'t Is waar Een-hoorens pryst men <rhyme label="a" type="f">krachtigh</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Maar twee is te veel, <rhyme label="a" type="f" corresp="#s10:l1">waarachtigh</rhyme>;</l>
        <l id="s10:l3" met="4" type="-+">Twee op een, naar't oude <rhyme label="b" type="f">seggen</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Doet de sterckste onder <rhyme label="b" type="f" corresp="#s10:l3">leggen</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s10:l4">omnibus</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ach helaas verbroken <rhyme label="a" type="f">Hoorn</rhyme>!</l>
        <l id="s11:l2" met="4" type="+-">Werd veel beter dan te <rhyme label="a" type="f" corresp="#s11:l1">vooren</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Doender de Nymphen om <rhyme label="b" type="f">vergaarden</rhyme></l>
        <l id="s11:l4" met="4" type="+-">Exaltatis <rhyme label="c" type="m">Cornibus</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Al de vruchten van der <rhyme label="b" type="f" corresp="#s11:l3">aarden</rhyme>.</l>
        <l id="s11:l6" met="4" type="+-">Cornua sunt <rhyme label="c" type="m" corresp="#s11:l4">omnibus</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
