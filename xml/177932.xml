<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>177932</id>
    <date>
      <year-start>1837</year-start>
      <year-end>1837</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Partant pour la Syrie</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik zie nog niets <rhyme label="a" type="f">aankomen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat voordeel aan mij <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">'k Lag van de nacht te <rhyme label="a" type="f" corresp="#s1:l1">droomen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Een voordeel nooit <rhyme label="b" type="m" corresp="#s1:l2">beleeft</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Een wagen stroo met <rhyme label="c" type="f">kalven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ging langs mijn zij <rhyme label="d" type="m">voorbij</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">'k Dagt dit en nog <rhyme label="c" type="f" corresp="#s1:l5">behalven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De boeten; 'k was zoo <rhyme label="d" type="m" corresp="#s1:l6">blij</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mogt dit eens zoo <rhyme label="a" type="f">gebeuren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wat was ik in mijn <rhyme label="b" type="m">schik</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">'k Zou z' van de wagen <rhyme label="a" type="f" corresp="#s2:l1">scheuren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Jij ook niet heintje <rhyme label="b" type="m" corresp="#s2:l2">pik</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Wel vriend dat was een <rhyme label="c" type="f">hapje</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Een lekker beetje <rhyme label="d" type="m">waar</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Hadden wij zoo een <rhyme label="c" type="f" corresp="#s2:l5">grapje</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dan was de zaak eerst <rhyme label="d" type="m" corresp="#s2:l6">klaar</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ha, ha! daar komt wat <rhyme label="a" type="f">draven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Fluks op uw post mijn <rhyme label="b" type="m">vriend</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Het komt van <rhyme label="a" type="f" corresp="#s3:l1">Delfshaven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat is wat dat ons <rhyme label="b" type="m" corresp="#s3:l2">dient</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Wel men wat brengt gij <rhyme label="c" type="f">binnen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Is 't vleesch of spek of <rhyme label="d" type="m">worst</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wat wou gij gaan <rhyme label="c" type="f" corresp="#s3:l5">beginnen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">'t Is water voor den <rhyme label="d" type="m" corresp="#s3:l6">dorst</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Pas op daar komt wat <rhyme label="a" type="f">anders</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat scheind een goede <rhyme label="b" type="m">buit</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ha ha! het is jan <rhyme label="a" type="f" corresp="#s4:l1">sanders</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met mandes vol <rhyme label="b" type="m" corresp="#s4:l2">beschuit</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Wel vriend waar wild gij <rhyme label="c" type="f">heenen</rhyme>?</l>
        <l id="s4:l6" met="3" type="-+">Zet neer; dat ik het <rhyme label="d" type="m">zie</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">'t Is geen beschuit maar <rhyme label="c" type="f" corresp="#s4:l5">beenen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die zijn voor scheele <rhyme label="d" type="m" corresp="#s4:l6">mie</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Zou er van daag niets <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat ons verkwikking <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Daar komt wat aan jan <rhyme label="a" type="f" corresp="#s5:l1">domen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Laat zien wat dat hij <rhyme label="b" type="m" corresp="#s5:l2">heeft</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Wat hebt gij vleesch, <rhyme label="c" type="f">genever</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Zet af, dat maak ik <rhyme label="d" type="m">prijs</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Het is een varkens <rhyme label="c" type="f" corresp="#s5:l5">lever</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Zijt gij dan nog niet <rhyme label="d" type="m" corresp="#s5:l6">wijs</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dat wijf dat ziet van <rhyme label="a" type="f">ond'ren</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zoo dik om haren <rhyme label="b" type="m">huik</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Laat zien, 'k moet mij <rhyme label="a" type="f" corresp="#s6:l1">verwond'ren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat g' als een hondje <rhyme label="b" type="m" corresp="#s6:l2">ruikt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Wou gij mijn <rhyme label="c" type="f">visiteeren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Neen guit dat past uw <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Gij moet het niet <rhyme label="c" type="f" corresp="#s6:l5">proberen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Want slaag krijgt gij van <rhyme label="d" type="m" corresp="#s6:l6">fiet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
