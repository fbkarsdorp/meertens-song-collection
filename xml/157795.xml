      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HIer woonen twee <rhyme label="a" type="f">Maeghden</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Of'er ymant na <rhyme label="a" type="f" corresp="#s1:l1">vraeghden</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">De laetsten van <rhyme label="b" type="f">elven</rhyme>;</l>
        <l id="s1:l4" met="2" type="-+">Sy woonen <rhyme label="c" type="m">altans</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">By gebreck van <rhyme label="c" type="m" corresp="#s1:l4">Mans</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Helaes op haer <rhyme label="b" type="f" corresp="#s1:l3">selven</rhyme>.</l>
