        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Fortuin helaas</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEluck moet u <rhyme label="a" type="f">toekomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Bruygom en Bruyt <rhyme label="b" type="m">eerbaer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Gods vreese <rhyme label="a" type="f" corresp="#s1:l1">uyt-genomen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Laet in u lichten <rhyme label="b" type="m" corresp="#s1:l2">klaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Op dat ghy soo hier <rhyme label="b" type="m" corresp="#s1:l2">naer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Als de Son blincken <rhyme label="c" type="m">meught</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Met Gods verkoren <rhyme label="b" type="m" corresp="#s1:l2">schaer</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In de eeuwighe <rhyme label="c" type="m" corresp="#s1:l6">vreught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als God Hemel en <rhyme label="a" type="f">Aerde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hadde geschapen <rhyme label="b" type="m">wijs</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Adam een vat van <rhyme label="a" type="f" corresp="#s2:l1">waerde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Na sijn beelde <rhyme label="b" type="m" corresp="#s2:l2">propijs</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Schiep God en sprack 't <rhyme label="b" type="m" corresp="#s2:l2">advijs</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ten is niet goet <rhyme label="c" type="m">certeyn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">(Na Schrifturen <rhyme label="b" type="m" corresp="#s2:l2">bewijs</rhyme>)</l>
        <l id="s2:l8" met="3" type="-+">Dat die mensch blijft <rhyme label="c" type="m" corresp="#s2:l6">alleyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick wil hem hulpe <rhyme label="a" type="f">maecken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die by hem wesen <rhyme label="b" type="m">sal</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">In slaep deed' hy <rhyme label="a" type="f" corresp="#s3:l1">geraecken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Den mensch, een in 't <rhyme label="b" type="m" corresp="#s3:l2">getal</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En nam tot sijn <rhyme label="b" type="m" corresp="#s3:l2">geval</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Een ribbe uyt sijn <rhyme label="c" type="m">lijf</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Waer af in 't aertsche <rhyme label="b" type="m" corresp="#s3:l2">dal</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Hy schiep Eva sijn <rhyme label="c" type="m" corresp="#s3:l6">wijf</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hier uyt soo kondy <rhyme label="a" type="f">weten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat die staet eerlijck <rhyme label="b" type="m">zy</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">God sal u <rhyme label="a" type="f" corresp="#s4:l1">ongemeten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Geven weldaden <rhyme label="b" type="m" corresp="#s4:l2">bly</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Hy heeft ons selve <rhyme label="b" type="m" corresp="#s4:l2">vry</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dees staet alsoo <rhyme label="c" type="m">vereert</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Binnen Cana, daer <rhyme label="b" type="m" corresp="#s4:l2">hy</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Water in Wijn <rhyme label="c" type="m" corresp="#s4:l6">verkeert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy die twee waerdt <rhyme label="a" type="f">verscheyden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Een vlees geworden <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Door des Heeren <rhyme label="a" type="f" corresp="#s5:l1">beleyden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die 't alles in dit <rhyme label="b" type="m" corresp="#s5:l2">krijt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Soo regeert ende <rhyme label="b" type="m" corresp="#s5:l2">vlijt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat hy als God <rhyme label="c" type="m">getrou</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Noch oock op deser <rhyme label="b" type="m" corresp="#s5:l2">tijdt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Geeft yeder sijn <rhyme label="c" type="m" corresp="#s5:l6">Huysvrou</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Malkand' ren niet wilt <rhyme label="a" type="f">haten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Noch twisten met <rhyme label="b" type="m">gekijf</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maer als heylige <rhyme label="a" type="f" corresp="#s6:l1">vaten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Leven, dat by u <rhyme label="b" type="m" corresp="#s6:l2">blijf</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Gods zegen, en u <rhyme label="b" type="m" corresp="#s6:l2">wijf</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Sal maecken seer <rhyme label="c" type="m">vruchtbaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat ghy sult sien te <rhyme label="b" type="m" corresp="#s6:l2">lijf</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Kindts kind'ren veel hier <rhyme label="c" type="m" corresp="#s6:l6">naer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Prins Bruydegom <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Als 't hooft 't lichaem <rhyme label="b" type="m">regeert</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Soo met <rhyme label="a" type="f" corresp="#s7:l1">sachtmoedicheden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">V Bruydt oock <rhyme label="b" type="m" corresp="#s7:l2">visiteert</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Vrou Bruydt, u Man <rhyme label="b" type="m" corresp="#s7:l2">ge-eert</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Van u laet wesen <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Soo sult ghy <rhyme label="b" type="m" corresp="#s7:l2">onverfeert</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Altijt leven seer <rhyme label="c" type="m" corresp="#s7:l6">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
