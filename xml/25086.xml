<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25086</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Laat ons met harten rein</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Die vader van hier <rhyme label="a" type="f">bouen</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Sprac sinen ingel <rhyme label="b" type="m">an</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Ic wille die helle <rhyme label="a" type="f" corresp="#s1:l1">scoren</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Verlossen wijf ende <rhyme label="c" type="f">man</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Gaet wech toter <rhyme label="d" type="m">fonteynen</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Marien claer <rhyme label="c" type="f" corresp="#s1:l4">aenschijn</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Ende segt haer wat ic <rhyme label="d" type="m" corresp="#s1:l5">meyne</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s1:l1">reyne</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s1:l2">kindekijn</rhyme>.</l>
        <l id="s1:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s1:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al binnen <rhyme label="a" type="f">nazarette</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Quam dingel <rhyme label="b" type="m">gabriel</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Hi gruete die <rhyme label="a" type="f" corresp="#s2:l1">violette</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Met suete woerden <rhyme label="c" type="f">snel</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">God gruete v suuer <rhyme label="d" type="m">reyne</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Vol gracien es v <rhyme label="c" type="f" corresp="#s2:l4">schijn</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">God es met v <rhyme label="d" type="m" corresp="#s2:l5">gemeyne</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s2:l1">reyne</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s2:l2">kindekijn</rhyme>.</l>
        <l id="s2:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s2:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Bi v soe sal <rhyme label="a" type="f">becliuen</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Dat adam heeft <rhyme label="b" type="m">ontvrijdt</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Want bouen alle <rhyme label="a" type="f" corresp="#s3:l1">wiuen</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Si di <rhyme label="c" type="f">gebenedijt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Ghi selt ontfaen een <rhyme label="d" type="m">greyne</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">In uwer herten <rhyme label="c" type="f" corresp="#s3:l4">scrine</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Ende bliuen een maget <rhyme label="d" type="m" corresp="#s3:l5">reyne</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s3:l1">reyne</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s3:l2">kindekijn</rhyme>.</l>
        <l id="s3:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s3:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Si sprac <rhyme label="a" type="f">oetmoedelike</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Hoe soude dat <rhyme label="b" type="m">gescien</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">En kinde <rhyme label="a" type="f" corresp="#s4:l1">sekerlike</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Noyt man des wondert <rhyme label="c" type="f">mie</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dat ic ende el ne <rhyme label="d" type="m">geyne</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">Gods moeder soude <rhyme label="c" type="f" corresp="#s4:l4">sijn</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Mijn herte es cout als <rhyme label="d" type="m" corresp="#s4:l5">steyne</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s4:l1">reyne</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s4:l2">kindekijn</rhyme>.</l>
        <l id="s4:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s4:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die heilege geest sal <rhyme label="a" type="f">comen</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">In v wel suuer <rhyme label="b" type="m">wijf</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Ontfaen tot onser <rhyme label="a" type="f" corresp="#s5:l1">vromen</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Aen v menschelijc <rhyme label="c" type="f">lijf</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Hi wilt af dwaen die <rhyme label="d" type="m">bleyne</rhyme>.</l>
        <l id="s5:l6" met="3" type="-+">Verlossen vter <rhyme label="c" type="f" corresp="#s5:l4">pine</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Alle sine vrienden <rhyme label="d" type="m" corresp="#s5:l5">gemeyne</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s5:l1">reyne</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s5:l2">kindekijn</rhyme>.</l>
        <l id="s5:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s5:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die ingel sprac tot <rhyme label="a" type="f">hare</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">O weerde suuer <rhyme label="b" type="m">maecht</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">En sijt in genen <rhyme label="a" type="f" corresp="#s6:l1">vare</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Het wonder dat ghi <rhyme label="c" type="f">claecht</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Want ghi selt sonder <rhyme label="d" type="m">pine</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Baren een <rhyme label="c" type="f" corresp="#s6:l4">kindekine</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Ende bliuen maget <rhyme label="d" type="m" corresp="#s6:l5">alleyne</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s6:l1">reyne</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s6:l2">kindekijn</rhyme>.</l>
        <l id="s6:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s6:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die maecht haer <rhyme label="a" type="f">conforteerde</rhyme>.</l>
        <l id="s7:l2" met="3" type="-+">In dat haer dingel <rhyme label="b" type="m">seide</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Ende si <rhyme label="a" type="f" corresp="#s7:l1">respondeerde</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Tot gode benic <rhyme label="c" type="f">bereit</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Na uwen woerden <rhyme label="d" type="m">reyne</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Stellic den wille <rhyme label="c" type="f" corresp="#s7:l4">mine</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Siet hier gods deerne <rhyme label="d" type="m" corresp="#s7:l5">cleyne</rhyme>.</l>
        <l id="s7:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s7:l1">reyne</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s7:l2">kindekijn</rhyme>.</l>
        <l id="s7:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s7:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Die yngel sciet van <rhyme label="a" type="f">daer</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">Maria bleef <rhyme label="b" type="m">beurucht</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Sonder wee oft <rhyme label="a" type="f" corresp="#s8:l1">vaer</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Oft sonder herte <rhyme label="c" type="f">versucht</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Baerde die edel suuer <rhyme label="d" type="m">greyne</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">Den hemelschen <rhyme label="c" type="f" corresp="#s8:l4">drochtijn</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Des vaders sone <rhyme label="d" type="m" corresp="#s8:l5">alleyne</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s8:l1">reyne</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s8:l2">kindekijn</rhyme>.</l>
        <l id="s8:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s8:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ons es een kint <rhyme label="a" type="f">geboren</rhyme>.</l>
        <l id="s9:l2" met="3" type="-+">Een sone <rhyme label="b" type="m">gepresenteert</rhyme>.</l>
        <l id="s9:l3" met="3" type="-+">Hi comt die helle <rhyme label="a" type="f" corresp="#s9:l1">scoren</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Als mensche <rhyme label="c" type="f">gefigureert</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Hi wilt ons alle <rhyme label="d" type="m">gemeyne</rhyme>.</l>
        <l id="s9:l6" met="3" type="-+">Verlossen vter <rhyme label="c" type="f" corresp="#s9:l4">pine</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Met sinen bloede <rhyme label="d" type="m" corresp="#s9:l5">alleyne</rhyme>.</l>
        <l id="s9:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s9:l1">reyne</rhyme></l>
        <l id="s9:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s9:l2">kindekijn</rhyme>.</l>
        <l id="s9:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s9:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dies mogen wi wel <rhyme label="a" type="f">dancken</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">Der maget diene <rhyme label="b" type="m">droech</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">In haer wel suuer <rhyme label="a" type="f" corresp="#s10:l1">lancken</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Die dracht haer niet en <rhyme label="c" type="f">verwoech</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Nv laet ons groot ende <rhyme label="d" type="m">cleene</rhyme>.</l>
        <l id="s10:l6" met="3" type="-+">Louen die maget <rhyme label="c" type="f" corresp="#s10:l4">fine</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Si brinct ons vter <rhyme label="d" type="m" corresp="#s10:l5">weyne</rhyme>.</l>
        <l id="s10:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s10:l1">reyne</rhyme></l>
        <l id="s10:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s10:l2">kindekijn</rhyme>.</l>
        <l id="s10:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s10:l1">weyne</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Geloeft soe moet si <rhyme label="a" type="f">wesen</rhyme>.</l>
        <l id="s11:l2" met="3" type="-+">Nv ende in alder <rhyme label="b" type="m">tijt</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Bi haer sijn wi <rhyme label="a" type="f" corresp="#s11:l1">genesen</rhyme>.</l>
        <l id="s11:l4" met="3" type="-+">Ende bracht in dit <rhyme label="c" type="f">iolijt</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">O weerde suuer <rhyme label="d" type="m">reyne</rhyme>.</l>
        <l id="s11:l6" met="3" type="-+">Des sondeers <rhyme label="c" type="f" corresp="#s11:l4">medecine</rhyme>.</l>
        <l id="s11:l7" met="3" type="-+">Bidt vor ons <rhyme label="d" type="m" corresp="#s11:l5">gemeyne</rhyme>.</l>
        <l id="s11:l8" met="3" type="-+">Laet ons met herten <rhyme label="a" type="f" corresp="#s11:l1">reyne</rhyme></l>
        <l id="s11:l9" met="3" type="-+">Louen dat suete <rhyme label="b" type="m" corresp="#s11:l2">kindekijn</rhyme>.</l>
        <l id="s11:l10" met="3" type="-+">Het brinct ons vter <rhyme label="a" type="f" corresp="#s11:l1">weyne</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
