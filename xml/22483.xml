  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">¶ Mit desen niewen <rhyme label="a" type="f">iare</rhyme></l>
        <l id="s1:l2" met="3" type="-+">so wert ons <rhyme label="a" type="f" corresp="#s1:l1">openbare</rhyme></l>
        <l id="s1:l3" met="3" type="-+">hoe dat een maecht <rhyme label="a" type="f" corresp="#s1:l1">vruchtbare</rhyme></l>
        <l id="s1:l4" met="3" type="-+">die werelt heeft <rhyme label="b" type="m">verblijt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s1:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s1:l5">maechdekijn</rhyme></l>
        <l id="s1:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s1:l4">tijt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">¶ Si baerde sonder <rhyme label="a" type="f">pijne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">ende bleef een maget <rhyme label="a" type="f" corresp="#s2:l1">fijne</rhyme></l>
        <l id="s2:l3" met="3" type="-+">des sondaers <rhyme label="a" type="f" corresp="#s2:l1">medicijne</rhyme></l>
        <l id="s2:l4" met="3" type="-+">dies hebben die ioden <rhyme label="b" type="m">spijt</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s2:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s2:l5">maechdekijn</rhyme></l>
        <l id="s2:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s2:l4">tijt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">¶ Hoe wel was haer te <rhyme label="a" type="f">moede</rhyme></l>
        <l id="s3:l2" met="3" type="-+">doen si in vleisch ende <rhyme label="a" type="f" corresp="#s3:l1">bloede</rhyme></l>
        <l id="s3:l3" met="3" type="-+">aensach haers hartsen <rhyme label="a" type="f" corresp="#s3:l1">hoede</rhyme></l>
        <l id="s3:l4" met="3" type="-+">den heer der werelt <rhyme label="b" type="m">wijt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s3:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s3:l5">maechdekijn</rhyme></l>
        <l id="s3:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s3:l4">tijt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">¶ Die enghelen songen <rhyme label="a" type="f">schone</rhyme></l>
        <l id="s4:l2" met="3" type="-+">lof inden hoghen <rhyme label="a" type="f" corresp="#s4:l1">trone</rhyme></l>
        <l id="s4:l3" met="3" type="-+">ter eren end ten <rhyme label="a" type="f" corresp="#s4:l1">loue</rhyme></l>
        <l id="s4:l4" met="3" type="-+">den kijnd des seker <rhyme label="b" type="m">sijt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s4:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s4:l5">maechdekijn</rhyme></l>
        <l id="s4:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s4:l4">tijt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">¶ Doe achtdaech waren <rhyme label="a" type="f">leden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">doe wert dat kijnt <rhyme label="a" type="f" corresp="#s5:l1">besneden</rhyme></l>
        <l id="s5:l3" met="3" type="-+">al nader ioedscher <rhyme label="a" type="f" corresp="#s5:l1">seden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">die ons van sonden <rhyme label="b" type="m">vrijt</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s5:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s5:l5">maechdekijn</rhyme></l>
        <l id="s5:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s5:l4">tijt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">¶ Drie coninghen dat <rhyme label="a" type="f">vernamen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">te bethleem dat si <rhyme label="a" type="f" corresp="#s6:l1">quamen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">si offerde drie <rhyme label="a" type="f" corresp="#s6:l1">gauen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">den kijnd <rhyme label="b" type="m">gebenedijt</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s6:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s6:l5">maechdekijn</rhyme></l>
        <l id="s6:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s6:l4">tijt</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">¶ Maria maget <rhyme label="a" type="f">simpel</rhyme></l>
        <l id="s7:l2" met="3" type="-+">droech haer lief kijnt ten <rhyme label="a" type="f" corresp="#s7:l1">tempel</rhyme></l>
        <l id="s7:l3" met="3" type="-+">den vrouwen een <rhyme label="a" type="f" corresp="#s7:l1">exempel</rhyme></l>
        <l id="s7:l4" met="3" type="-+">dies si haer niet <rhyme label="b" type="m">vermijt</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s7:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s7:l5">maechdekijn</rhyme></l>
        <l id="s7:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s7:l4">tijt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">¶ Doe symeon die <rhyme label="a" type="f">oude</rhyme></l>
        <l id="s8:l2" met="3" type="-+">tkijnt sach sijn hert <rhyme label="a" type="f" corresp="#s8:l1">vervroude</rhyme></l>
        <l id="s8:l3" met="3" type="-+">hi sprac dat hi ons <rhyme label="a" type="f" corresp="#s8:l1">soude</rhyme></l>
        <l id="s8:l4" met="3" type="-+">van sonden maken <rhyme label="b" type="m">quijt</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Geloeft moet sijn dat <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s8:l6" met="4" type="-+">gheeert moet sijn dat <rhyme label="c" type="m" corresp="#s8:l5">maechdekijn</rhyme></l>
        <l id="s8:l7" met="4" type="-+">nv ende ewelijc tot alre <rhyme label="b" type="m" corresp="#s8:l4">tijt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
