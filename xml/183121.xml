<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183121</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pots honderdduizend slapperment</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Alwaer de lieve Vriendschap <rhyme label="a" type="m">woond</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">En zoete <rhyme label="b" type="m">Eendrachtigheid</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">Daer word hy steeds met Palm <rhyme label="a" type="m" corresp="#s1:l1">bekroond</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">In spijt van haet en <rhyme label="b" type="m" corresp="#s1:l2">nijd</rhyme> /</l>
        <l id="s1:l5" met="4" type="-+">Kom rijk malkanderen de <rhyme label="c" type="m">hand</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">En slaet ze wel in <rhyme label="d" type="m">een</rhyme> /</l>
        <l id="s1:l7" met="4" type="-+">Als Broeders is de twist <rhyme label="c" type="m" corresp="#s1:l5">verband</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">De Vreugd in 't <rhyme label="d" type="m" corresp="#s1:l6">algemeen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat gund de goede God ons <rhyme label="a" type="m">steeds</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">Dat wy met Liefde en <rhyme label="b" type="m">lust</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">Malkanderen minnen als <rhyme label="a" type="m" corresp="#s2:l1">voorheen</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Blijft 't wraekvuur <rhyme label="b" type="m" corresp="#s2:l2">uitgeblust</rhyme> /</l>
        <l id="s2:l5" met="4" type="-+">Dan groeit en bloeit de <rhyme label="c" type="m">Koopvaerdy</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Te Land / en ook ter <rhyme label="d" type="m">Zee</rhyme> /</l>
        <l id="s2:l7" met="4" type="-+">Dan is men lustig op 't <rhyme label="c" type="m" corresp="#s2:l5">Y</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">Bevrijd van <rhyme label="d" type="m" corresp="#s2:l6">nageween</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hoe is ons schoone Kooprijke <rhyme label="a" type="m">Stad</rhyme>/</l>
        <l id="s3:l2" met="3" type="-+">In zulk een laeg <rhyme label="b" type="m">verval</rhyme>/</l>
        <l id="s3:l3" met="4" type="-+">Door Eigenbaet thans <rhyme label="a" type="m" corresp="#s3:l1">afgemat</rhyme>/</l>
        <l id="s3:l4" met="3" type="-+">Ons Vriendschap blijft steeds <rhyme label="b" type="m" corresp="#s3:l2">pal</rhyme>/</l>
        <l id="s3:l5" met="4" type="-+">Daerom komt allen weer tot <rhyme label="c" type="m">een</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">Bemind uw <rhyme label="d" type="m">Overigheid</rhyme> /</l>
        <l id="s3:l7" met="4" type="-+">Dat zy door hun <rhyme label="c" type="m" corresp="#s3:l5">liefdaedigheen</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">Zijn tot weldoen <rhyme label="d" type="m" corresp="#s3:l6">bereid</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">d'Oneenigheid bedroeft Stad en <rhyme label="a" type="m">Land</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">Zo wel onder groot als <rhyme label="b" type="m">kleen</rhyme> /</l>
        <l id="s4:l3" met="4" type="-+">Geen Stad blijft daer door in zijn <rhyme label="a" type="m" corresp="#s4:l1">stand</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">'t Vergaet in het <rhyme label="b" type="m" corresp="#s4:l2">algemeen</rhyme> /</l>
        <l id="s4:l5" met="4" type="-+">Den Koopman treurd in groot <rhyme label="c" type="m">verdriet</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">Den Winkelier zit in <rhyme label="d" type="m">nood</rhyme> /</l>
        <l id="s4:l7" met="4" type="-+">Den Ambagtsman die werkt <rhyme label="c" type="m" corresp="#s4:l5">niet</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">'t Raekt alles daer door <rhyme label="d" type="m" corresp="#s4:l6">dood</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer laet de lieve <rhyme label="a" type="m">eendrachtigheid</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ons brengen weer ten <rhyme label="b" type="m">top</rhyme> /</l>
        <l id="s5:l3" met="4" type="-+">En alles zal met groote <rhyme label="a" type="m" corresp="#s5:l1">vlyt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ten toppunt ryzen <rhyme label="b" type="m" corresp="#s5:l2">op</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Den Hemel schenkt ons lieve <rhyme label="c" type="m">vree</rhyme>/</l>
        <l id="s5:l6" met="3" type="-+">Zo dienstig voor het <rhyme label="d" type="m">Land</rhyme>/</l>
        <l id="s5:l7" met="4" type="-+">Op dat wy roepen dan <rhyme label="c" type="m" corresp="#s5:l5">hoezee</rhyme>/</l>
        <l id="s5:l8" met="3" type="-+">Den Prins is ons <rhyme label="d" type="m" corresp="#s5:l6">bystand</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ik raed u Vrienden tot <rhyme label="a" type="m">besluit</rhyme> /</l>
        <l id="s6:l2" met="3" type="-+">Dat gy 't <rhyme label="b" type="m">Oranje-Huis</rhyme> /</l>
        <l id="s6:l3" met="4" type="-+">Nog onze Vorst / die 't onheil <rhyme label="a" type="m" corresp="#s6:l1">stuit</rhyme> /</l>
        <l id="s6:l4" met="3" type="-+">Niet meer belaed met <rhyme label="b" type="m" corresp="#s6:l2">kruis</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">Hy zoekt de vrede en <rhyme label="c" type="m">d'eendracht</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Te planten in ons <rhyme label="d" type="m">Land</rhyme> /</l>
        <l id="s6:l7" met="4" type="-+">Wend alles aen met magt en <rhyme label="c" type="m" corresp="#s6:l5">kragt</rhyme> /</l>
        <l id="s6:l8" met="3" type="-+">God sterkt deeze <rhyme label="d" type="m" corresp="#s6:l6">Band</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nu Vrienden drink eens in 't <rhyme label="a" type="m">rond</rhyme> /</l>
        <l id="s7:l2" met="3" type="-+">De Oranje <rhyme label="b" type="m">Brandewyn</rhyme> /</l>
        <l id="s7:l3" met="4" type="-+">En roept dan uit met volle <rhyme label="a" type="m" corresp="#s7:l1">mond</rhyme> /</l>
        <l id="s7:l4" met="3" type="-+">'t Zal de gezondheid <rhyme label="b" type="m" corresp="#s7:l2">zyn</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Van onze Prins en ons <rhyme label="c" type="m">Princes</rhyme> /</l>
        <l id="s7:l6" met="3" type="-+">De jonge Spruiten <rhyme label="d" type="m">mee</rhyme> /</l>
        <l id="s7:l7" met="4" type="-+">En roept dan uit een opregt <rhyme label="c" type="m" corresp="#s7:l5">hert</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Drievoudig nu <rhyme label="d" type="m" corresp="#s7:l6">hoezee</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dan is de liefde van de <rhyme label="a" type="m">Vorst</rhyme>/</l>
        <l id="s8:l2" met="3" type="-+">Eens eindelyk <rhyme label="b" type="m">bekroond</rhyme>/</l>
        <l id="s8:l3" met="4" type="-+">Als goede vriendschap / 't kwaede <rhyme label="a" type="m" corresp="#s8:l1">torst</rhyme>/</l>
        <l id="s8:l4" met="3" type="-+">Van ieder die hier <rhyme label="b" type="m" corresp="#s8:l2">woond</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Schenk dan nog eens in voor het <rhyme label="c" type="m">lest</rhyme> /</l>
        <l id="s8:l6" met="3" type="-+">'t Heelal dat roept <rhyme label="d" type="m">hoezee</rhyme> /</l>
        <l id="s8:l7" met="4" type="-+">Dat groot en klein steeds doet zyn <rhyme label="c" type="m" corresp="#s8:l5">best</rhyme>/</l>
        <l id="s8:l8" met="3" type="-+">Tot eene dierbre <rhyme label="d" type="m" corresp="#s8:l6">vree</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
