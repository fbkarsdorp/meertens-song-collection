<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20899</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Veni creator spiritus</melody>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O God, die zijt het lot en <rhyme label="a" type="m">kroon</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Van uw' Soldaten, en hun <rhyme label="a" type="m" corresp="#s1:l1">loon</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ons van der sonden band <rhyme label="b" type="m">ontslaet</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die loflijck vieren uw' <rhyme label="b" type="m" corresp="#s1:l3">Soldaet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Te weten desen vromen <rhyme label="a" type="m">Held</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Gekomen is tot 's hemels <rhyme label="a" type="m" corresp="#s2:l1">veld</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Om dat Hy 't lockend' 's werelts <rhyme label="b" type="m">aes</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Niet meer en achten als een <rhyme label="b" type="m" corresp="#s2:l3">blaes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Kloeckmoedigh heeft dien Held <rhyme label="a" type="m">gestree'en</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Tormenten heeft Hy veel <rhyme label="a" type="m" corresp="#s3:l1">gele'en</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Vergietende voor U sijn <rhyme label="b" type="m">bloed</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Besit Hy nu het eeuwigh' <rhyme label="b" type="m" corresp="#s3:l3">goet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dies bidden wy met <rhyme label="a" type="m">nedricheyd</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Uw' goedertiere <rhyme label="a" type="m" corresp="#s4:l1">Majesteyt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Op deses Martyrs groote <rhyme label="b" type="m">Feest</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">De wonden onser ziel <rhyme label="b" type="m" corresp="#s4:l3">geneest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat glory God den Vader <rhyme label="a" type="m">zy</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En Godes Soon ten allen <rhyme label="a" type="m" corresp="#s5:l1">ty</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">En d' Heyl'gen Geest zy eer en <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Soo hier als in den hemels <rhyme label="b" type="m" corresp="#s5:l3">hof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
