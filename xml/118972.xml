    <melody>Das Lieben ist mein Leben</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="7" type="+-">DAs lieben ist myn leben/ Fa, la, la, <rhyme label="a" type="m">la</rhyme>,</l>
        <l id="s1:l2" met="6" type="+-">Dem hab ich mich ergeben/ Fa, la, <rhyme label="a" type="m" corresp="#s1:l1">la</rhyme>,</l>
        <l id="s1:l3" met="7" type="+-">Lieben wol ich alle zeit/ M, m, <rhyme label="b" type="m">m</rhyme>,</l>
        <l id="s1:l4" met="5" type="+-">Alle zeit/ M, m, <rhyme label="b" type="m" corresp="#s1:l3">m</rhyme>,</l>
        <l id="s1:l5" met="9" type="+-">Lieben wil ich alle zeit/ M, m, Fa, la, <rhyme label="a" type="m" corresp="#s1:l1">la</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="7" type="+-">Sie is von hohen stamme/ Fa, la, la, <rhyme label="a" type="m">la</rhyme>,</l>
        <l id="s2:l2" met="6" type="+-">Eine Adeliche Dame/ Fa, la, <rhyme label="a" type="m" corresp="#s2:l1">la</rhyme>,</l>
        <l id="s2:l3" met="7" type="+-">Sie ist schun unt Adelich/ M, m, <rhyme label="b" type="m">m</rhyme>,</l>
        <l id="s2:l4" met="5" type="+-">Adelich/ M, m, <rhyme label="b" type="m" corresp="#s2:l3">m</rhyme>,</l>
        <l id="s2:l5" met="9" type="+-">Wer das nich ein braut vor mich/ M, m, Fa, la, <rhyme label="a" type="m" corresp="#s2:l1">la</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="7" type="+-">Ihr ougen gebeu strhalen/ Fa, la, la, <rhyme label="a" type="m">la</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">Das hertz im leib thut knallen/ Fa, la, <rhyme label="a" type="m" corresp="#s3:l1">la</rhyme>,</l>
        <l id="s3:l3" met="7" type="+-">Sie ist auch ein Engel gleich/ M, m, <rhyme label="b" type="m">m</rhyme>,</l>
        <l id="s3:l4" met="5" type="+-">Engel gleich/ M, m, <rhyme label="b" type="m" corresp="#s3:l3">m</rhyme>,</l>
        <l id="s3:l5" met="9" type="+-">Lieber as das Rumsche Reich/ M, m, Fa, la, <rhyme label="a" type="m" corresp="#s3:l1">la</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="7" type="+-">Sie thut mir wolgefallen/ Fa, la, la, <rhyme label="a" type="m">la</rhyme>,</l>
        <l id="s4:l2" met="6" type="+-">So thun de Iungfrenallen/ Fa, la, <rhyme label="a" type="m" corresp="#s4:l1">la</rhyme>,</l>
        <l id="s4:l3" met="7" type="+-">Zuker sus ist irhe mund/ M, m, <rhyme label="b" type="m">m</rhyme>,</l>
        <l id="s4:l4" met="5" type="+-">Irhe mund/ M, m, <rhyme label="b" type="m" corresp="#s4:l3">m</rhyme>,</l>
        <l id="s4:l5" met="9" type="+-">Ein kuss hat sie mir fargunt/ M, m, Fa, la, <rhyme label="a" type="m" corresp="#s4:l1">la</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="7" type="+-">War ich zo weit gekommen/ Fa, la, la, <rhyme label="a" type="m">la</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Dasz ich sie kont bekommen/ Fa, la, <rhyme label="a" type="m" corresp="#s5:l1">la</rhyme>,</l>
        <l id="s5:l3" met="7" type="+-">Ach! die schunster augen fier/ M, m, <rhyme label="b" type="m">m</rhyme>,</l>
        <l id="s5:l4" met="5" type="+-">Augen fier/ M, m, <rhyme label="b" type="m" corresp="#s5:l3">m</rhyme>,</l>
        <l id="s5:l5" met="9" type="+-">Ewig bleib ich tog by dier/ M, m, Fa, la, <rhyme label="a" type="m" corresp="#s5:l1">la</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
