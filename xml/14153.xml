  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EEn Liet wil ick <rhyme label="a" type="f">beginnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ende verclaren <rhyme label="b" type="m">dat</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoe wy met herte en <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Godt lief hebben <rhyme label="b" type="m" corresp="#s1:l2">ghehat</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat wy verstaet dit <rhyme label="b" type="m" corresp="#s1:l2">plat</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ons overgaven <rhyme label="c" type="m">hem</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Om t'commen in die <rhyme label="b" type="m" corresp="#s1:l2">Stat</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">t' Nieuwe <rhyme label="c" type="m" corresp="#s1:l6">Jerusalem</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ten eersten hebt voor <rhyme label="a" type="f">ooghen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoe dat ghy eens voor <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Vwe knien hebt <rhyme label="a" type="f" corresp="#s2:l1">ghebooghen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al voor die waerheyt <rhyme label="b" type="m" corresp="#s2:l2">claer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En ghy beloofdet <rhyme label="b" type="m" corresp="#s2:l2">daer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Soo met gheboghen <rhyme label="c" type="m">knien</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Vwen Hemelschen <rhyme label="b" type="m" corresp="#s2:l2">Vaer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Te dienen t allen <rhyme label="c" type="m" corresp="#s2:l6">tien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wilt daerom wel <rhyme label="a" type="f">ghedincken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mijn Broeder <rhyme label="b" type="m">delicaet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Of ghy noch sonder <rhyme label="a" type="f" corresp="#s3:l1">hincken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Staet in dien selven <rhyme label="b" type="m" corresp="#s3:l2">graet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Meucht ghy noch seggen <rhyme label="b" type="m" corresp="#s3:l2">jae</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Het is alsoo met <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maeckt dat met u soo <rhyme label="b" type="m" corresp="#s3:l2">gae</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">V Leven waer dat <rhyme label="c" type="m" corresp="#s3:l6">sy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maer u verlichte <rhyme label="a" type="f">daghen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghedenckt die <rhyme label="b" type="m">onghequelt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hoe t'herte was <rhyme label="a" type="f" corresp="#s4:l1">verslaghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Des Vleesch lust en <rhyme label="b" type="m" corresp="#s4:l2">ghewelt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hoe waert ghy doen <rhyme label="b" type="m" corresp="#s4:l2">ghestelt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hoe brande de Liefde <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Maeckt datse niet en <rhyme label="b" type="m" corresp="#s4:l2">smelt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Noch en laetse niet <rhyme label="c" type="m" corresp="#s4:l6">vercoen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Want ghy waert heel <rhyme label="a" type="f">ontsteken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Al metter liefden <rhyme label="b" type="m">brant</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als ghy eens zijt <rhyme label="a" type="f" corresp="#s5:l1">gheweken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Al uyt Egypten <rhyme label="b" type="m" corresp="#s5:l2">lant</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Godt nam u by der <rhyme label="b" type="m" corresp="#s5:l2">hant</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En leyde u door die Zee <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat is dat wy <rhyme label="b" type="m" corresp="#s5:l2">vaylliant</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Verlost zijn deur zijn <rhyme label="c" type="m" corresp="#s5:l6">doot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat een Liefde <rhyme label="a" type="f">overvloedich</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hat ghy als doen ter <rhyme label="b" type="m">tijt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Vernedert en <rhyme label="a" type="f" corresp="#s6:l1">ootmoedich</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En sonder haet en <rhyme label="b" type="m" corresp="#s6:l2">nijt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Vwer Sielen <rhyme label="b" type="m" corresp="#s6:l2">profijt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Socht ghy van herten <rhyme label="c" type="m">gront</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En socht u te maken <rhyme label="b" type="m" corresp="#s6:l2">quijt</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Dat Gods woort teghen <rhyme label="c" type="m" corresp="#s6:l6">stont</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Al wat Godts woort stont <rhyme label="a" type="f">tegen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Socht ghy te laten <rhyme label="b" type="m">jent</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Want ghy waert gantsch <rhyme label="a" type="f" corresp="#s7:l1">genegen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Tot de waerheyt <rhyme label="b" type="m" corresp="#s7:l2">excellent</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ja t'herte was <rhyme label="b" type="m" corresp="#s7:l2">ghewent</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Tot den Heere uwen <rhyme label="c" type="m">Godt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Om hier tot inden <rhyme label="b" type="m" corresp="#s7:l2">ent</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Te doen nae zijn <rhyme label="c" type="m" corresp="#s7:l6">ghebot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dus wilt den Heere <rhyme label="a" type="f">dancken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En loven vrouch en <rhyme label="b" type="m">spae</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat ghy zijt van zijn <rhyme label="a" type="f" corresp="#s8:l1">Rancken</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Gheworden deur <rhyme label="b" type="m" corresp="#s8:l2">ghenae</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Hy heeft u ghesleghen <rhyme label="b" type="m" corresp="#s8:l2">gae</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Doen ghy doot laecht <rhyme label="c" type="m">ghewont</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Maer deur zijn liefde <rhyme label="b" type="m" corresp="#s8:l2">jae</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Soo maeckte hy u <rhyme label="c" type="m" corresp="#s8:l6">ghesont</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maer doen hy u <rhyme label="a" type="f">verloste</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Vander doot zijt dit <rhyme label="b" type="m">vroet</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Silver noch Gaut hem <rhyme label="a" type="f" corresp="#s9:l1">coste</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Maer zijn reyn dierbaer <rhyme label="b" type="m" corresp="#s9:l2">bloet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Dus danckt hem met <rhyme label="b" type="m" corresp="#s9:l2">ootmoet</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Geeft hem lof, prijs en <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Van alle gaven <rhyme label="b" type="m" corresp="#s9:l2">goet</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Die ghy hebt vanden <rhyme label="c" type="m" corresp="#s9:l6">Heer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wilt hem loven en <rhyme label="a" type="f">prijsen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Van zijn weldaden <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En hem eer <rhyme label="a" type="f" corresp="#s10:l1">bewijsen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Van zijn goedtheyt <rhyme label="b" type="m" corresp="#s10:l2">devijn</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Die u hielp uyt <rhyme label="b" type="m" corresp="#s10:l2">ghepijn</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Doen ghy laecht int <rhyme label="c" type="m">verdriet</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Want gheen ander <rhyme label="b" type="m" corresp="#s10:l2">medecijn</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">En conde u helpen <rhyme label="c" type="m" corresp="#s10:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Looft hem want hy ist <rhyme label="a" type="f">weerdich</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Hy heeft voor u <rhyme label="b" type="m">voldaen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">En blijft hem by <rhyme label="a" type="f" corresp="#s11:l1">volheerdich</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Al sonder af te <rhyme label="b" type="m" corresp="#s11:l2">gaen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Maer blijft op zijn woort <rhyme label="b" type="m" corresp="#s11:l2">staen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Siet altijts neerstich <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">En wilt doch dit <rhyme label="b" type="m" corresp="#s11:l2">vermaen</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Van my nemen int <rhyme label="c" type="m" corresp="#s11:l6">goe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
