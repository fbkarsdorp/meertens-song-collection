    <melody>Een kindje is ons geboren</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Drie Koningen <rhyme label="a" type="f">uytvercoren</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Quamen te <rhyme label="b" type="m">Ierusalem</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sy vraechden waer is <rhyme label="a" type="f" corresp="#s1:l1">geboren</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Den Koning der <rhyme label="b" type="m" corresp="#s1:l2">Ioden</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wy sagen in <rhyme label="c" type="m">Orienten</rhyme></l>
        <l id="s1:l6" met="3" type="-+">die sterne <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En komen aen te <rhyme label="c" type="m" corresp="#s1:l5">beden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">dat soete <rhyme label="d" type="m" corresp="#s1:l6">kindekijn</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s1:l9">thoren</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s1:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Doe Herodes dat <rhyme label="a" type="f">vernam</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Dat het kindt geboren <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hy wert toornich ende <rhyme label="a" type="f" corresp="#s2:l1">gram</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Met sijner gantschen <rhyme label="b" type="m" corresp="#s2:l2">schaer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Vermemden hy sou <rhyme label="c" type="m">verliesen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dat rijcker <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Daerom bracht hy te <rhyme label="c" type="m" corresp="#s2:l5">vernielen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat kleine <rhyme label="d" type="m" corresp="#s2:l6">kindeken</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s2:l9">thoren</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s2:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy vraechde met <rhyme label="a" type="f">listigheden</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Waer dat kindt geboren <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Te Bethlehem in die <rhyme label="a" type="f" corresp="#s3:l1">steden</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Soo was datmen daer <rhyme label="b" type="m" corresp="#s3:l2">las</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Daer is der Heer <rhyme label="c" type="m">geboren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+"><rhyme label="d" type="m">Wtvercoren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die ons al door fijn <rhyme label="c" type="m" corresp="#s3:l5">lijden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Sal <rhyme label="d" type="m" corresp="#s3:l6">verblijden</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s3:l9">thoren</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s3:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Herodes sprack tot die <rhyme label="a" type="f">wijsen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Gaet heen en soeckt dat <rhyme label="b" type="m">kint</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met also groote <rhyme label="a" type="f" corresp="#s4:l1">vlijten</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Al tot dat gy dat <rhyme label="b" type="m" corresp="#s4:l2">vint</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Want boven der Koningen <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Soo is hy <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Men segt hy sal <rhyme label="c" type="m" corresp="#s4:l5">besitten</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat rijcke <rhyme label="d" type="m" corresp="#s4:l6">mijn</rhyme>.</l>
        <l id="s4:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s4:l9">thoren</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s4:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Als gy dat kint hebt <rhyme label="a" type="f">vonden</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Soo keert weder tot <rhyme label="b" type="m">my</rhyme></l>
        <l id="s5:l3" met="3" type="-+">In also korten <rhyme label="a" type="f" corresp="#s5:l1">stonden</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">En seght my waer dat <rhyme label="b" type="m" corresp="#s5:l2">sy</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ick soud soo gern <rhyme label="c" type="m">aenbeden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat kleine <rhyme label="d" type="m">kindeken</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Het heeft soo seer <rhyme label="c" type="m" corresp="#s5:l5">doorsneden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat herte <rhyme label="d" type="m" corresp="#s5:l6">mijn</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s5:l9">thoren</rhyme>,</l>
        <l id="s5:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s5:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer als die Koningen <rhyme label="a" type="f">quamen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Buyten <rhyme label="b" type="m">Ierusalem</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met vreugden sy <rhyme label="a" type="f" corresp="#s6:l1">vernamen</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">De sterne staen voor <rhyme label="b" type="m" corresp="#s6:l2">hen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Soo lang tot dat sy <rhyme label="c" type="m">vonden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dat soete <rhyme label="d" type="m">kindekijn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">In kleyne doecxkens <rhyme label="c" type="m" corresp="#s6:l5">gewonden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">By der Moeder <rhyme label="d" type="m" corresp="#s6:l6">fijn</rhyme>.</l>
        <l id="s6:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s6:l9">thoren</rhyme>,</l>
        <l id="s6:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s6:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die Koningen <rhyme label="a" type="f">aenbaden</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Dat kindeken niet seer <rhyme label="b" type="m">out</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sy offerden wel <rhyme label="a" type="f" corresp="#s7:l1">beraden</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Myrthe, Wieroock ende <rhyme label="b" type="m" corresp="#s7:l2">Gout</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Die gaven heeft <rhyme label="c" type="m">ontfangen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die Ionckfrou <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Sy aenbaden met <rhyme label="c" type="m" corresp="#s7:l5">verlangen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Dat wonder <rhyme label="d" type="m" corresp="#s7:l6">kindekijn</rhyme>.</l>
        <l id="s7:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s7:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s7:l9">thoren</rhyme>,</l>
        <l id="s7:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s7:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Des snachts als sy slapen <rhyme label="a" type="f">wouden</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Quam d’Engel Gods tot <rhyme label="b" type="m">hen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat sy niet keeren en <rhyme label="a" type="f" corresp="#s8:l1">souden</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Al door <rhyme label="b" type="m" corresp="#s8:l2">Ierusalem</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Door eenen anderen <rhyme label="c" type="m">weghe</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Sijn sy <rhyme label="d" type="m">gekeert</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Tot in haer plaetse ter <rhyme label="c" type="m" corresp="#s8:l5">deghe</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Alsmen ons <rhyme label="d" type="m" corresp="#s8:l6">leert</rhyme>.</l>
        <l id="s8:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s8:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s8:l9">thoren</rhyme>,</l>
        <l id="s8:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s8:l10">hem</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wy bidden nu en <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Dit kint Iesus <rhyme label="b" type="m">genandt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Dat hy ons al wil <rhyme label="a" type="f" corresp="#s9:l1">brengen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">In sijn schoon <rhyme label="b" type="m" corresp="#s9:l2">vaderlandt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Daer hem die Engelen <rhyme label="c" type="m">eeren</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Tot aller <rhyme label="d" type="m">tijdt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Dat gunne ons God der <rhyme label="c" type="m" corresp="#s9:l5">Heere</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+"><rhyme label="d" type="m" corresp="#s9:l6">Gebenedijt</rhyme>.</l>
        <l id="s9:l9" met="3" type="-+">Een kindeken is ons <rhyme label="e" type="f">geboren</rhyme>,</l>
        <l id="s9:l10" met="2" type="-+">In <rhyme label="f" type="m">Bethlehem</rhyme>,</l>
        <l id="s9:l11" met="3" type="+-">Dat hadde Herodes <rhyme label="e" type="f" corresp="#s9:l9">thoren</rhyme>,</l>
        <l id="s9:l12" met="2" type="-+">Dat scheen aen <rhyme label="f" type="m" corresp="#s9:l10">hem</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
