  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Nu dancken wy u <rhyme label="a" type="f">seere</rhyme></l>
        <l id="s1:l2" met="3" type="-+">O barmhertighe <rhyme label="b" type="m">Vaer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die ghy ons lieve <rhyme label="a" type="f" corresp="#s1:l1">Heere</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Nu hebt behoedt te <rhyme label="b" type="m" corresp="#s1:l2">gaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dees nacht ons rusten <rhyme label="c" type="f">laten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ghewaeckt voor ons <rhyme label="d" type="m">ghewis</rhyme></l>
        <l id="s1:l7" met="3" type="-+">In aller goeder <rhyme label="c" type="f" corresp="#s1:l5">maten</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Tot den dach comen <rhyme label="d" type="m" corresp="#s1:l6">is</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Eenen Leydtsman <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Heer uwen goeden <rhyme label="b" type="m">Gheest</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Laert stadich by ons <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met sijnen crachten <rhyme label="b" type="m" corresp="#s2:l2">meest</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Daer deur wilt ons <rhyme label="c" type="f">verstercken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Op dat wy nu <rhyme label="d" type="m">voortaen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Moghen in al ons <rhyme label="c" type="f" corresp="#s2:l5">wercken</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Oprechtelijck voort <rhyme label="d" type="m" corresp="#s2:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo desen dach <rhyme label="a" type="f">deurbringhen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Naer uwen heyl’ghen <rhyme label="b" type="m">wil</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En soo voor alle <rhyme label="a" type="f" corresp="#s3:l1">dinghen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Gherustich ende <rhyme label="b" type="m" corresp="#s3:l2">stil</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Sober, rechtveerdich <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Van u verwachten <rhyme label="d" type="m">fijn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Wat ons sal in dit <rhyme label="c" type="f" corresp="#s3:l5">leven</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Alhier van nooden <rhyme label="d" type="m" corresp="#s3:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gheneghen als die <rhyme label="a" type="f">cloecken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Vwe eere <rhyme label="b" type="m">alleyn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Nu alletijdt te <rhyme label="a" type="f" corresp="#s4:l1">soecken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In al ons doen <rhyme label="b" type="m" corresp="#s4:l2">ghemeyn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Vergheeft onse <rhyme label="c" type="f">misdaden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Scheldt ons schulden <rhyme label="d" type="m">quijt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wt louterer <rhyme label="c" type="f" corresp="#s4:l5">ghenaden</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Want ghy barmhertich <rhyme label="d" type="m" corresp="#s4:l6">zijt</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Leert ons (lief Heer) <rhyme label="a" type="f">betrachten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Waer toe ghy ons schiept <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Heerlijck deur uwe <rhyme label="a" type="f" corresp="#s5:l1">crachten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Den Hemel oock daer <rhyme label="b" type="m" corresp="#s5:l2">by</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En die Aerd’ om <rhyme label="c" type="f">verhoghen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met aller <rhyme label="d" type="m">vruchtbaerheyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat wy’t ghebruycken <rhyme label="c" type="f" corresp="#s5:l5">moghen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">T’uwer eeren <rhyme label="d" type="m" corresp="#s5:l6">bereyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Naer u Godlijck <rhyme label="a" type="f">behaghen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En dat misbruycken <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Nu voortaen onse <rhyme label="a" type="f" corresp="#s6:l1">daghen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">In blijschap en <rhyme label="b" type="m" corresp="#s6:l2">verdriet</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Gheeft ons mate te <rhyme label="c" type="f">houwen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En alle dinghen <rhyme label="d" type="m">voort</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Te doen in u <rhyme label="c" type="f" corresp="#s6:l5">aenschouwen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Heer naer u heylich <rhyme label="d" type="m" corresp="#s6:l6">woort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Tsamen all’ onse <rhyme label="a" type="f">saken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Bevelen in u <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wilt over ons nu <rhyme label="a" type="f" corresp="#s7:l1">waken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Heer met uwen <rhyme label="b" type="m" corresp="#s7:l2">bystandt</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Wy dancken u met <rhyme label="c" type="f">namen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Altijdt en <rhyme label="d" type="m">ymmermeer</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Laet dit doch worden <rhyme label="c" type="f" corresp="#s7:l5">Amen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Amen O lieve <rhyme label="d" type="m" corresp="#s7:l6">Heer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
