  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ach Gott was soll ich <rhyme label="a" type="f">singenn</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">K rtzweill ist mir worden the <rhyme label="b" type="m">r</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">Vorzeittenn ginck ich <rhyme label="a" type="f" corresp="#s1:l1">springenn</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">das bos ich alles h <rhyme label="b" type="m" corresp="#s1:l2">ett</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">mitt grossenn s chtenn <rhyme label="c" type="f">schwer</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">verzehr ich mangenn <rhyme label="d" type="m">tag</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">vngefell ist widder <rhyme label="c" type="f" corresp="#s1:l5">gefer</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">wiewoll ich des nhemantz en <rhyme label="d" type="m" corresp="#s1:l6">clagh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Liebhaben vnnd z  <rhyme label="a" type="f">meiden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">ist mir ein schwere <rhyme label="b" type="m">boiß</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Das machtt des kleffers z <rhyme label="a" type="f" corresp="#s2:l1">ngen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">daß ich dich meiden m <rhyme label="b" type="m" corresp="#s2:l2">s</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Das ich dich hab <rhyme label="c" type="f">verlassenn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">so gantz vnnd <rhyme label="d" type="m">vberall</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">So bin ich lieb dein <rhyme label="c" type="f" corresp="#s2:l5">eigen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">gleub mir z  diesem <rhyme label="d" type="m" corresp="#s2:l6">malh</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">D  hast mir <rhyme label="a" type="f">vmbfangen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Das j nge hertze <rhyme label="b" type="m">mein</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">nach dir hab ich <rhyme label="a" type="f" corresp="#s3:l1">verlangen</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Du zartte J nckfrewlein <rhyme label="b" type="m" corresp="#s3:l2">fein</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Dein m ndtlein rott mir zu <rhyme label="c" type="f">meidenn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">ist mir ein schwere b <rhyme label="d" type="m">iß</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Deß tra renn ich winter vnnd <rhyme label="c" type="f" corresp="#s3:l5">Sommer</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">das ich dich meiden <rhyme label="d" type="m" corresp="#s3:l6">moß</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Jch sall vnnd moß <rhyme label="a" type="f">scheidenn</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">eß kan nitt anders <rhyme label="b" type="m">sein</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Das brengtt mir grosses <rhyme label="a" type="f" corresp="#s4:l1">leidenn</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">ist mir ein schwere <rhyme label="b" type="m" corresp="#s4:l2">pein</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Ach scheiden jmmer <rhyme label="c" type="f">scheidenn</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">wehr hatt dich <rhyme label="d" type="m">erdacht</rhyme> /</l>
        <l id="s4:l7" met="3" type="-+">D  hast mein j nges <rhyme label="c" type="f" corresp="#s4:l5">hertz</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">a ß fre denn jn tra renn <rhyme label="d" type="m" corresp="#s4:l6">bracht</rhyme></l>
      </lg>
    </lg>
