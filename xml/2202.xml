<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2202</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Celestine</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">
          <rhyme label="a" type="m">Celestyn</rhyme>
        </l>
        <l id="s1:l2" met="3" type="-+">Nu schey ik uyt de <rhyme label="a" type="m" corresp="#s1:l1">pijn</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">O salig <rhyme label="b" type="m">siel-gesucht</rhyme>!</l>
        <l id="s1:l4" met="2" type="-+">Schept nu <rhyme label="b" type="m" corresp="#s1:l3">genucht</rhyme>,</l>
        <l id="s1:l5" met="1" type="-+">
          <rhyme label="c" type="m">Gewis</rhyme>
        </l>
        <l id="s1:l6" met="1" type="-+">Nu <rhyme label="c" type="m" corresp="#s1:l5">is</rhyme></l>
        <l id="s1:l7" met="1" type="-+">Uw <rhyme label="d" type="m">klacht</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Noch wel ten end <rhyme label="d" type="m" corresp="#s1:l7">gebracht</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">'t Is wel <rhyme label="e" type="m">gelukt</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Dat meenig <rhyme label="e" type="m" corresp="#s1:l9">onderdrukt</rhyme>.</l>
        <l id="s1:l11" met="3" type="+-">O Fidamant die <rhyme label="f" type="m">eerst</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Haar vleyden op het <rhyme label="f" type="m" corresp="#s1:l11">seerst</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">En schreyden Als sy <rhyme label="g" type="m">niet</rhyme></l>
        <l id="s1:l14" met="2" type="-+">Hilp uw <rhyme label="g" type="m" corresp="#s1:l13">verdriet</rhyme>.</l>
        <l id="s1:l15" met="1" type="-+">Uw <rhyme label="h" type="m">Min</rhyme></l>
        <l id="s1:l16" met="1" type="-+">leeft <rhyme label="h" type="m" corresp="#s1:l15">in</rhyme></l>
        <l id="s1:l17" met="1" type="-+">die <rhyme label="i" type="m">Maagd</rhyme></l>
        <l id="s1:l18" met="3" type="-+">Die gy uw hart <rhyme label="i" type="m" corresp="#s1:l17">opdraagt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Al mijn <rhyme label="a" type="m">geween</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gejammer en <rhyme label="a" type="m" corresp="#s2:l1">gesteen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Getreuren <rhyme label="b" type="m">nacht-gewaak</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Keertin <rhyme label="b" type="m" corresp="#s2:l3">vermaak</rhyme>:</l>
        <l id="s2:l5" met="1" type="-+">Mijn <rhyme label="c" type="m">jeugd</rhyme></l>
        <l id="s2:l6" met="1" type="-+">
          <rhyme label="c" type="m" corresp="#s2:l5">Verheugd</rhyme>
        </l>
        <l id="s2:l7" met="1" type="-+">Sich <rhyme label="d" type="m">om</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat ik die <rhyme label="d" type="m" corresp="#s2:l7">Maagde-blom</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Verwonnen <rhyme label="e" type="m">heb</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">En in de <rhyme label="e" type="m" corresp="#s2:l9">linne-web</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Die plukken sal na <rhyme label="f" type="m">wil</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Daar ik mijn bloed om <rhyme label="f" type="m" corresp="#s2:l11">spil</rhyme>.</l>
        <l id="s2:l13" met="3" type="-+">Nu ik haar Hart als <rhyme label="g" type="m">steen</rhyme></l>
        <l id="s2:l14" met="2" type="-+">Heb kunnen <rhyme label="g" type="m" corresp="#s2:l13">kneen</rhyme>,</l>
        <l id="s2:l15" met="1" type="-+"><rhyme label="h" type="m">Geschrey</rhyme>,</l>
        <l id="s2:l16" met="1" type="-+">En <rhyme label="h" type="m" corresp="#s2:l15">ey</rhyme>!</l>
        <l id="s2:l17" met="1" type="-+">En <rhyme label="i" type="m">ach</rhyme>!</l>
        <l id="s2:l18" met="3" type="-+">Verandert in <rhyme label="i" type="m" corresp="#s2:l17">gelach</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Welaan dan <rhyme label="a" type="m">Lief</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Behoud uw <rhyme label="a" type="m" corresp="#s3:l1">Minne-grief</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En set uw Echte <rhyme label="b" type="m">min</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Noyt uyt uw <rhyme label="b" type="m" corresp="#s3:l3">sin</rhyme>.</l>
        <l id="s3:l5" met="1" type="-+">Noemt <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s3:l6" met="1" type="-+">Dien <rhyme label="c" type="m" corresp="#s3:l5">gy</rhyme></l>
        <l id="s3:l7" met="1" type="-+"><rhyme label="d" type="m">Naamtan</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Uw al-getrouwe <rhyme label="d" type="m" corresp="#s3:l7">Man</rhyme>.</l>
        <l id="s3:l9" met="2" type="-+">Ik sal dan <rhyme label="e" type="m">ook</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Uw weesen en <rhyme label="e" type="m" corresp="#s3:l9">gestrook</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Graveeren in mijn <rhyme label="f" type="m">hart</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Daar 't noyt versleeten <rhyme label="f" type="m" corresp="#s3:l11">ward</rhyme>:</l>
        <l id="s3:l13" met="3" type="-+">Noyt sal ik <rhyme label="g" type="m">Celestyn</rhyme></l>
        <l id="s3:l14" met="2" type="-+">U trowloos <rhyme label="g" type="m" corresp="#s3:l13">sijn</rhyme>;</l>
        <l id="s3:l15" met="1" type="-+">Maar <rhyme label="h" type="m">blijf</rhyme></l>
        <l id="s3:l16" met="1" type="-+">Met <rhyme label="h" type="m" corresp="#s3:l15">lijf</rhyme></l>
        <l id="s3:l17" met="1" type="-+">En <rhyme label="i" type="m">hand</rhyme></l>
        <l id="s3:l18" met="3" type="-+">Uw eygen <rhyme label="i" type="m" corresp="#s3:l17">Fidamant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
