    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">Geestelijk lied</tag>
    </tags>
    <melody>Engelse klokkedans</melody>
    <categories/>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">WIlt <rhyme label="a" type="f">verjubileren</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Christelijcke <rhyme label="b" type="m">schaer</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">'Tis de Feest des <rhyme label="a" type="f" corresp="#s1:l1">Heeren</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">'Taengename <rhyme label="b" type="m" corresp="#s1:l2">Iaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">'Tzijn bequame <rhyme label="c" type="f">tijden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'Tis den dagh der <rhyme label="d" type="m">saligheyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Wilt u nu <rhyme label="c" type="f" corresp="#s1:l5">verblijden</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Siet het is nu al <rhyme label="d" type="m" corresp="#s1:l6">bereydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">'Tis nu al <rhyme label="a" type="f">gheslaghen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">'Tvee kleyn ende <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Hier werdt <rhyme label="a" type="f" corresp="#s2:l1">voorghedraghen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">'Tgheen ons is van <rhyme label="b" type="m" corresp="#s2:l2">noodt</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Wilt nu niet <rhyme label="c" type="f">vertoeven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Laet ons tot der Feeste <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Wilt hongerigh <rhyme label="c" type="f" corresp="#s2:l5">proeven</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Hy roept al die zijn <rhyme label="d" type="m" corresp="#s2:l6">belaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Al die zijn <rhyme label="a" type="f">beladen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Met haer sonden <rhyme label="b" type="m">meest</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Gaet 'tis u <rhyme label="a" type="f" corresp="#s3:l1">gheraden</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Tot dees Bruylofts <rhyme label="b" type="m" corresp="#s3:l2">Feest</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Hy sal u <rhyme label="c" type="f">vermaecken</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Desen Bruydegom <rhyme label="d" type="m">bemint</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Wilt ghy u <rhyme label="c" type="f" corresp="#s3:l5">versaecken</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En omkeeren als een <rhyme label="d" type="m" corresp="#s3:l6">kindt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wilt ghy u <rhyme label="a" type="f">omkeeren</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Doet oprechte <rhyme label="b" type="m">boet</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Wilt van Christo <rhyme label="a" type="f" corresp="#s4:l1">leeren</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Liefde en <rhyme label="b" type="m" corresp="#s4:l2">ootmoet</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Ruste sult ghy <rhyme label="c" type="f">vinden</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Aen u ziele seer <rhyme label="d" type="m">bequaem</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Hy die u <rhyme label="c" type="f" corresp="#s4:l5">beminden</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Sal u leyden in sijn <rhyme label="d" type="m" corresp="#s4:l6">Naem</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Dus wilt u <rhyme label="a" type="f">vercieren</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Om ter Feest te <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Leert Christus <rhyme label="a" type="f" corresp="#s5:l1">manieren</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Hy is hooft seer <rhyme label="b" type="m" corresp="#s5:l2">saen</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Sonden quade <rhyme label="c" type="f">wercken</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Moeten gantsch zijn <rhyme label="d" type="m">afgeleyt</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Deughden moetmen <rhyme label="c" type="f" corresp="#s5:l5">mercken</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Anders blijftmen <rhyme label="d" type="m" corresp="#s5:l6">onbereyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">'Tvrye Iaer des <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Is <rhyme label="b" type="m">gheopenbaert</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Elck mach nu <rhyme label="a" type="f" corresp="#s6:l1">keeren</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Vry en <rhyme label="b" type="m" corresp="#s6:l2">onbeswaert</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Tot Syons <rhyme label="c" type="f">beminden</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Hy sal ons wel maecken <rhyme label="d" type="m">vry</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Ghy sult oock <rhyme label="c" type="f" corresp="#s6:l5">bevinden</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Wie dat u Regeerder <rhyme label="d" type="m" corresp="#s6:l6">zy</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Hy regeert wel <rhyme label="a" type="f">geeren</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Daer hy plaetse <rhyme label="b" type="m">vindt</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Hy wil wel <rhyme label="a" type="f" corresp="#s7:l1">logeeren</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Waer hy werdt <rhyme label="b" type="m" corresp="#s7:l2">bemindt</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Dus opent hem <rhyme label="c" type="f">trouwen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Op dat hy sijn <rhyme label="d" type="m">Avontmael</rhyme></l>
        <l id="s7:l7" met="3" type="+-">By u hier mach <rhyme label="c" type="f" corresp="#s7:l5">houden</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En ghy met hem <rhyme label="d" type="m" corresp="#s7:l6">generael</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Houdt nu <rhyme label="a" type="f">Lover-feeste</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Met vrolijck <rhyme label="b" type="m">jolijt</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Ieg'lijck zy de <rhyme label="a" type="f" corresp="#s8:l1">meeste</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Inder deught <rhyme label="b" type="m" corresp="#s8:l2">altijdt</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Soeckt nu te <rhyme label="c" type="f">behaghen</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Hem die uwe ziel <rhyme label="d" type="m">bemint</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Wilt u Cruyce <rhyme label="c" type="f" corresp="#s8:l5">draghen</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">En als Christus zijt <rhyme label="d" type="m" corresp="#s8:l6">ghesint</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Christelijcke <rhyme label="a" type="f">Tempel</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Gheestelijck <rhyme label="b" type="m">verciert</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Volght zijn <rhyme label="a" type="f" corresp="#s9:l1">exempel</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Hem die u <rhyme label="b" type="m" corresp="#s9:l2">regiert</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Hy wil in u <rhyme label="c" type="f">woonen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Daerom scheydt u uyt van <rhyme label="d" type="m">haer</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">Sijn dochters en <rhyme label="c" type="f" corresp="#s9:l5">soonen</rhyme></l>
        <l id="s9:l8" met="4" type="+-">Sult ghy werden <rhyme label="d" type="m" corresp="#s9:l6">openbaer</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Wilt den Sabboth <rhyme label="a" type="f">rusten</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Wandelt <rhyme label="b" type="m">onbesmet</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Hebt al uwe <rhyme label="a" type="f" corresp="#s10:l1">lusten</rhyme></l>
        <l id="s10:l4" met="3" type="+-">In des Heeren <rhyme label="b" type="m" corresp="#s10:l2">wet</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Sticht altijdt <rhyme label="c" type="f">malkander</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Want in desen Sabbath <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s10:l7" met="3" type="+-">Komt d'een feest aen <rhyme label="c" type="f" corresp="#s10:l5">d'ander</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Ghy moet altijdt verciert <rhyme label="d" type="m" corresp="#s10:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Wilt doch niet <rhyme label="a" type="f">besmetten</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Vwen <rhyme label="b" type="m">Bruylofts-kleedt</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Al wat u mach <rhyme label="a" type="f" corresp="#s11:l1">letten</rhyme></l>
        <l id="s11:l4" met="3" type="+-">Om te werden <rhyme label="b" type="m" corresp="#s11:l2">reedt</rhyme>.</l>
        <l id="s11:l5" met="3" type="+-">Wilt dat van u <rhyme label="c" type="f">weeren</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Op dat ghy hem niet en <rhyme label="d" type="m">slacht</rhyme>,</l>
        <l id="s11:l7" met="3" type="+-">Die gheen <rhyme label="c" type="f" corresp="#s11:l5">Bruylofts-kleeren</rhyme></l>
        <l id="s11:l8" met="4" type="+-">Aen en hadde, hier op <rhyme label="d" type="m" corresp="#s11:l6">acht</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Wilt gheen onschult <rhyme label="a" type="f">bieden</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Weygert niet te <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">Wat baet het de <rhyme label="a" type="f" corresp="#s12:l1">lieden</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">Die weygherden <rhyme label="b" type="m" corresp="#s12:l2">saen</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Wat baet ontschult <rhyme label="c" type="f">maecken</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Gheen van dien zijn <rhyme label="d" type="m">Avondtmael</rhyme></l>
        <l id="s12:l7" met="3" type="+-">Namaels mochte <rhyme label="c" type="f" corresp="#s12:l5">smaecken</rhyme>,</l>
        <l id="s12:l8" met="4" type="+-">Vrienden 'tis Christus <rhyme label="d" type="m" corresp="#s12:l6">verhael</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Christus wil u <rhyme label="a" type="f">sparen</rhyme></l>
        <l id="s13:l2" met="3" type="+-">Mijn vrienden <rhyme label="b" type="m">bekent</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">In 't gheloof <rhyme label="a" type="f" corresp="#s13:l1">bewaren</rhyme></l>
        <l id="s13:l4" met="3" type="+-">Tot des levens <rhyme label="b" type="m" corresp="#s13:l2">endt</rhyme>,</l>
        <l id="s13:l5" met="3" type="+-">Dat wy by den <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s13:l6" met="4" type="+-">En sijn uytverkoren <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s13:l7" met="3" type="+-">Eeuwigh <rhyme label="c" type="f" corresp="#s13:l5">triumpheren</rhyme>,</l>
        <l id="s13:l8" met="4" type="+-">In des Hemels Feeste <rhyme label="d" type="m" corresp="#s13:l6">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
