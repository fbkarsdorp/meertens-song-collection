    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Ik ben verwonnen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">Ick ben verwonnen / door een lieffelyck <rhyme label="a" type="m">ghesicht</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Al myn sinnekens  / en myn <rhyme label="b" type="m">verstant</rhyme></l>
        <l id="s1:l3" met="6" type="-+">Sy en cannen / niet gewinnen cupydos <rhyme label="a" type="m" corresp="#s1:l1">sinnen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Noch ghedoghen venus <rhyme label="b" type="m" corresp="#s1:l2">brant</rhyme></l>
        <l id="s1:l5" met="3" type="-+">V cnecht / v lieff <rhyme label="b" type="m" corresp="#s1:l2">playsant</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Heeft oprechte / lieffde <rhyme label="b" type="m" corresp="#s1:l2">gheplant</rhyme></l>
        <l id="s1:l7" met="6" type="-+">Dus wilt hem tonen / lieffde schonen / het hart <rhyme label="b" type="m" corresp="#s1:l2">verlicht</rhyme></l>
        <l id="s1:l8" met="4" type="-+">O schoon eedele <rhyme label="b" type="m" corresp="#s1:l2">dyamant</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Wilt v verneren / lieff vol eeren / door <rhyme label="a" type="m">goedtheyt</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En op uwen dienaer <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s2:l3" met="6" type="-+">Met lamanteren / laet v deeren / syn <rhyme label="a" type="m" corresp="#s2:l1">bereyt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">En helpen wt dit <rhyme label="b" type="m" corresp="#s2:l2">verdriet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In noot / v gracy <rhyme label="b" type="m" corresp="#s2:l2">biedt</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Eer die doot / myn hart <rhyme label="b" type="m" corresp="#s2:l2">doorschiet</rhyme></l>
        <l id="s2:l7" met="6" type="-+">Sou myn begeren / niet fayleren / sulcx <rhyme label="b" type="m" corresp="#s2:l2">verdriet</rhyme></l>
        <l id="s2:l8" met="4" type="-+">En verwacht ick van v lieffken <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Hopen verwachten / myn gedachten / troost <rhyme label="a" type="m">vandyn</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Lieff al ben ick heel <rhyme label="b" type="m">onbequaem</rhyme></l>
        <l id="s3:l3" met="6" type="-+">Wilt dit niet achten / maer versachten / t'lyden <rhyme label="a" type="m" corresp="#s3:l1">myn</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Laet myn / v dienst syn / <rhyme label="b" type="m" corresp="#s3:l2">aengenaem</rhyme></l>
        <l id="s3:l5" met="3" type="-+">O lieff reyn / o lieff <rhyme label="b" type="m" corresp="#s3:l2">eersaem</rhyme></l>
        <l id="s3:l6" met="4" type="-+">En certeyn / ick niet en <rhyme label="b" type="m" corresp="#s3:l2">schaem</rhyme></l>
        <l id="s3:l7" met="6" type="-+">Ick sal niet slachten / die verpachten / <rhyme label="b" type="m" corresp="#s3:l2">vielany</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Noch behinderen uwen <rhyme label="b" type="m" corresp="#s3:l2">faem</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">Na v vermoghen / wilt betoghen / myn v <rhyme label="a" type="m">deucht</rhyme></l>
        <l id="s4:l2" met="4" type="+-">O prinssesse <rhyme label="b" type="m">delycat</rhyme></l>
        <l id="s4:l3" met="6" type="-+">Hebt doch medoghen / slaet v oghen / op myn <rhyme label="a" type="m" corresp="#s4:l1">jeucht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Die sonder u heel <rhyme label="b" type="m" corresp="#s4:l2">vergaet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">En weest niet <rhyme label="b" type="m" corresp="#s4:l2">opstynaet</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Maer schoon lieffken compt te <rhyme label="b" type="m" corresp="#s4:l2">baet</rhyme></l>
        <l id="s4:l7" met="6" type="-+">Myn hart ghebogen / ongheloghen / waer <rhyme label="b" type="m" corresp="#s4:l2">verheucht</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Dat ghy deet na schoon lieff al na myn <rhyme label="b" type="m" corresp="#s4:l2">raet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
