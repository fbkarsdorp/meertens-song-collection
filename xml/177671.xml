  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Hoe lang, o HEER, mijn <rhyme label="a" type="m">toeverlaat</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">Vergeet Gij mijnen <rhyme label="a" type="m" corresp="#s1:l1">jammerstaat</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Hoe lang zult Gij, in mijn <rhyme label="b" type="f">ellenden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Van mij uw vriendlijk aanschijn <rhyme label="b" type="f" corresp="#s1:l3">wenden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Daar al mijn moed en kracht <rhyme label="a" type="m" corresp="#s1:l1">vergaat</rhyme>?</l>
      </lg>
    </lg>
  </text>
