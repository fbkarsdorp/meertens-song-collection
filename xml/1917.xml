    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIe vvinter kout is nu eerst <rhyme label="a" type="f">ontslapen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Mayas <rhyme label="b" type="m">jeucht</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Heeft verblijdich vveer gaen <rhyme label="a" type="f" corresp="#s1:l1">rapen</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Ons tot <rhyme label="b" type="m" corresp="#s1:l2">vreucht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Haer reyn ghecleurde mantel <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Het groen-rijck soet ons <rhyme label="b" type="m" corresp="#s1:l2">verheucht</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Mayas <rhyme label="c" type="m" corresp="#s1:l5">kroon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Flora toont haer gheschilderde <rhyme label="a" type="f">vvanghen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Bloosich <rhyme label="b" type="m">root</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Phebus blont die verhoocht oock sijn <rhyme label="a" type="f" corresp="#s2:l1">ganghen</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">D'aerd <rhyme label="b" type="m" corresp="#s2:l2">aenstoot</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ontsluyt de moeder van 't <rhyme label="c" type="m">ghevvas</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Gheeft vveer leven dat scheen <rhyme label="b" type="m" corresp="#s2:l2">doodt</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Voedt 't loof-rijcke <rhyme label="c" type="m" corresp="#s2:l5">gras</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Verblijdich singen de bos <rhyme label="a" type="f">Najaden</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Mayas <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Gaen haer in gulden vlieten <rhyme label="a" type="f" corresp="#s3:l1">baden</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Driaeds <rhyme label="b" type="m" corresp="#s3:l2">hof</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hooren den Philomela <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Die het hert ziels vreucht maeckt <rhyme label="b" type="m" corresp="#s3:l2">grof</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">En verquicken <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het vlugh-gediert gaet nu vreuchdich <rhyme label="a" type="f">queelen</rhyme></l>
        <l id="s4:l2" met="2" type="+-">'s Hemels <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">'T vier-ghevoedt begint oock te <rhyme label="a" type="f" corresp="#s4:l1">speelen</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Door het <rhyme label="b" type="m" corresp="#s4:l2">Meer</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dat des Meys vergroenen <rhyme label="c" type="m">gheeft</rhyme>:</l>
        <l id="s4:l6" met="4" type="+-">'T is een jeuchdelijck <rhyme label="b" type="m" corresp="#s4:l2">begheer</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Dat vernieuvvich <rhyme label="c" type="m" corresp="#s4:l5">leeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Prins en Heer een vvaren <rhyme label="a" type="f">alvoeder</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Door u <rhyme label="b" type="m">gaet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">De Mey ontsluyten de dorre <rhyme label="a" type="f" corresp="#s5:l1">Moeder</rhyme></l>
        <l id="s5:l4" met="2" type="+-">Tot ons <rhyme label="b" type="m" corresp="#s5:l2">baet</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Wt 's levens jonst: orbooren <rhyme label="c" type="m">trou</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Batavia bloeyich <rhyme label="b" type="m" corresp="#s5:l2">staet</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">En verlicht veel <rhyme label="c" type="m" corresp="#s5:l5">rou</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
