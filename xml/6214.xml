<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6214</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Naar oostland wil ik varen</melody>
  <author>C.P. van Wesbusch</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NAe Santvoort wil ick <rhyme label="a" type="f">ryden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">De waghen die stater al <rhyme label="b" type="m">ree</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Mijn soete Lief aen mijn <rhyme label="a" type="f" corresp="#s1:l1">zyde</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Om kussen// en <rhyme label="c" type="f">blussen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">De Brant in de volle <rhyme label="b" type="m" corresp="#s1:l2">Zee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe lustigh wil ick <rhyme label="a" type="f">singhen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wanneer ick de Wagen voel <rhyme label="b" type="m">gaen</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">En sie de Paerden <rhyme label="a" type="f" corresp="#s2:l1">springen</rhyme>;</l>
        <l id="s2:l4" met="2" type="-+">Tsa! lustigh// en <rhyme label="c" type="f">rustigh</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Wat hotsen en kan niet <rhyme label="b" type="m" corresp="#s2:l2">schaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als wy de Duyn <rhyme label="a" type="f">ghenaeken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En komen in 't gulle <rhyme label="b" type="m">sant</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Stae vast! dan roode <rhyme label="a" type="f" corresp="#s3:l1">kaecken</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Die bloosen// als <rhyme label="c" type="f">Roosen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Van liefde, en <rhyme label="b" type="m" corresp="#s3:l2">Minnen-brant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Soo dickmael als wy <rhyme label="a" type="f">stygen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Op eenige Berghen haer <rhyme label="b" type="m">Kruyn</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Sal elck een Kusjen <rhyme label="a" type="f" corresp="#s4:l1">kryghen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Van haeren// <rhyme label="c" type="f">Minnaere</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Soo lang als wy ryden in <rhyme label="b" type="m" corresp="#s4:l2">Duyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Als wy te Santvoort <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En rennen so voorts nae 't <rhyme label="b" type="m">strant</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Elck Meysjen mach wel <rhyme label="a" type="f" corresp="#s5:l1">schromen</rhyme>;</l>
        <l id="s5:l4" met="2" type="-+">Want knechjes// niet <rhyme label="c" type="f">slechjes</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Verdryven dan haest den <rhyme label="b" type="m" corresp="#s5:l2">Brant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Soo sy 't oock niet en <rhyme label="a" type="f">deden</rhyme>:</l>
        <l id="s6:l2" met="3" type="-+">Wy lieten het heel <rhyme label="b" type="m">Iaer</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Haer nimmermeer met <rhyme label="a" type="f" corresp="#s6:l1">vreden</rhyme>;</l>
        <l id="s6:l4" met="2" type="-+">Maer plaghen// en <rhyme label="c" type="f">klaghen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Schier aller weegs over <rhyme label="b" type="m" corresp="#s6:l2">haer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dus om de schimp te <rhyme label="a" type="f">myden</rhyme>:</l>
        <l id="s7:l2" met="3" type="-+">De Iongmans seer wel <rhyme label="b" type="m">bedacht</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Houden ons aen haer <rhyme label="a" type="f" corresp="#s7:l1">syden</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Om spoelen// en <rhyme label="c" type="f">koelen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Ons billen; dat niemandt <rhyme label="b" type="m" corresp="#s7:l2">versmacht</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dan rijdtmen weer om <rhyme label="a" type="f">hooge</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Te Santvoort al binnen de <rhyme label="b" type="m">straet</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">Om Hembt, en Rock te <rhyme label="a" type="f" corresp="#s8:l1">drooghen</rhyme>:</l>
        <l id="s8:l4" met="2" type="-+">Men 't samen// met <rhyme label="c" type="f">schamen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Een Herberghe binnen <rhyme label="b" type="m" corresp="#s8:l2">gaet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Daer gh'eten, en <rhyme label="a" type="f">gedroncken</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Gedrebbelt al voor het <rhyme label="b" type="m">vyer</rhyme>;</l>
        <l id="s9:l3" met="3" type="-+">Den Wijn wordt ons <rhyme label="a" type="f" corresp="#s9:l1">gheschoncken</rhyme>;</l>
        <l id="s9:l4" met="2" type="-+">Wy singhen// en <rhyme label="c" type="f">springhen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">En maecken daer goede <rhyme label="b" type="m" corresp="#s9:l2">cier</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De Paerden in <rhyme label="a" type="f">ghespannen</rhyme>;</l>
        <l id="s10:l2" met="3" type="-+">Wy setten ons twee, en <rhyme label="b" type="m">twee</rhyme>;</l>
        <l id="s10:l3" met="3" type="-+">Elck Iongh-man is geen <rhyme label="a" type="f" corresp="#s10:l1">Hannen</rhyme>:</l>
        <l id="s10:l4" met="2" type="-+">Sy sollen// haer <rhyme label="c" type="f">snollen</rhyme>;</l>
        <l id="s10:l5" met="3" type="-+">Verlaten alsoo de <rhyme label="b" type="m" corresp="#s10:l2">Zee</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De Son die is aen 't <rhyme label="a" type="f">dalen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Den Avont ons <rhyme label="b" type="m">overvalt</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">De Voerman voert ons <rhyme label="a" type="f" corresp="#s11:l1">dwalen</rhyme>;</l>
        <l id="s11:l4" met="2" type="-+">Sy sluyten// ons <rhyme label="c" type="f">buyten</rhyme>;</l>
        <l id="s11:l5" met="3" type="-+">Ist dan niet ter deghen <rhyme label="b" type="m" corresp="#s11:l2">ghemalt</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Als wy dan buyten <rhyme label="a" type="f">blyven</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Verwachten den morghen <rhyme label="b" type="m">root</rhyme>:</l>
        <l id="s12:l3" met="3" type="-+">Denckt eens wat wy <rhyme label="a" type="f" corresp="#s12:l1">bedryven</rhyme>:</l>
        <l id="s12:l4" met="2" type="-+">Wy rusten// met <rhyme label="c" type="f">lusten</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">De Knechjes in onsen <rhyme label="b" type="m" corresp="#s12:l2">Schoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
