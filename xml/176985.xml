  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">JOris, kom reis by <rhyme label="a" type="f">me</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m">doen</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">'k Wil wel gaarne by jouw <rhyme label="c" type="f">komen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wyl ik 's nachts van jouw moet <rhyme label="c" type="f" corresp="#s1:l3">dromen</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Joris, kom reis by <rhyme label="a" type="f" corresp="#s1:l1">me</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m" corresp="#s1:l2">doen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Joris, geef me je <rhyme label="a" type="f">handje</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m">doen</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">'k Voel, nouw jy myn hand wilt <rhyme label="c" type="f">voelen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">In men lyf zoo'n raar <rhyme label="c" type="f" corresp="#s2:l3">krieoelen</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">Joris, geef me je <rhyme label="a" type="f" corresp="#s2:l1">handje</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m" corresp="#s2:l2">doen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Joris, geef men een <rhyme label="a" type="f">zoentje</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m">doen</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">'k Wil jouw wel een zoentje <rhyme label="c" type="f">geven</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Maar dan raak ik heel aan 't <rhyme label="c" type="f" corresp="#s3:l3">beven</rhyme>;</l>
        <l id="s3:l5" met="3" type="+-">Joris, geef men een <rhyme label="a" type="f" corresp="#s3:l1">zoentje</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m" corresp="#s3:l2">doen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Joris, praat wat met <rhyme label="a" type="f">me</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m">doen</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">'k Wouw je graag wel wat <rhyme label="c" type="f">vertellen</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Maar 'k voel dan men lyf puur <rhyme label="c" type="f" corresp="#s4:l3">zwellen</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Joris, praat wat met <rhyme label="a" type="f" corresp="#s4:l1">me</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m" corresp="#s4:l2">doen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Joris, willen we <rhyme label="a" type="f">trouwen</rhyme>?</l>
        <l id="s5:l2" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m">doen</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">'k Wil zoo gaarne met je <rhyme label="c" type="f">trouwen</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Want ik kan 't niet langer <rhyme label="c" type="f" corresp="#s5:l3">houwen</rhyme>;</l>
        <l id="s5:l5" met="3" type="+-">Joris, willen we <rhyme label="a" type="f" corresp="#s5:l1">trouwen</rhyme>?</l>
        <l id="s5:l6" met="3" type="-+">Och Huipje! kom laten we 't <rhyme label="b" type="m" corresp="#s5:l2">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Joris, gaa je dan met <rhyme label="a" type="f">me</rhyme>?</l>
        <l id="s6:l2" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m">doen</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Laten wy voor alle <rhyme label="c" type="f">zaken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Bruid en Bru√Ægom ons doen <rhyme label="c" type="f" corresp="#s6:l3">maken</rhyme>;</l>
        <l id="s6:l5" met="3" type="+-">Joris, gaa je dan met <rhyme label="a" type="f" corresp="#s6:l1">me</rhyme>?</l>
        <l id="s6:l6" met="3" type="-+">Och Huipje! dat wil ik wel <rhyme label="b" type="m" corresp="#s6:l2">doen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
