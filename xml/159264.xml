  <author>Marijn de Brauwer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KNechten des Heeren looft den <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die hemel en Aerde heeft <rhyme label="b" type="m">gemaect</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Wilt zynen naem verbreyden <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die met hem uyt de doot sijt <rhyme label="b" type="m" corresp="#s1:l2">geraect</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En oock smaeckt vriendlijck <rhyme label="c" type="m">dat</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Die Heere is elck // <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s1:l7" met="3" type="+-">ghelijck het water <rhyme label="c" type="m" corresp="#s1:l5">nat</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Vloeyt van hem Wijn en melck // <rhyme label="d" type="m" corresp="#s1:l6">soet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Comt doch al die daer dorst en <rhyme label="a" type="m">drinct</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Alle beyde Wijn en melck om <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Die Heere seer mildelick <rhyme label="a" type="m" corresp="#s2:l1">schinckt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Want zijn Fonteyne altijt over <rhyme label="b" type="m" corresp="#s2:l2">vliet</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Want hy anbiet zijn <rhyme label="c" type="m">gena</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Dus komt met vlijt // <rhyme label="d" type="m">smaeckt</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Doet naer zynen <rhyme label="c" type="m" corresp="#s2:l5">ra</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Eer dat ghyt al quijt // <rhyme label="d" type="m" corresp="#s2:l6">raeckt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Elck moet hem so besteden // <rhyme label="a" type="m">fris</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Te lange toeven ist lopen <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s3:l3" met="4" type="+-">So lange alst met ons heden // <rhyme label="a" type="m" corresp="#s3:l1">is</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Zyne genade altijt open <rhyme label="b" type="m" corresp="#s3:l2">staet</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Want haest vergaet hier den <rhyme label="c" type="m">mensch</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Als roock verdwijnt // <rhyme label="d" type="m">snel</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Niet naer haren <rhyme label="c" type="m" corresp="#s3:l5">wensch</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Alzoo men dat bevint // <rhyme label="d" type="m" corresp="#s3:l6">wel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Stellen niet uyt tot morgen // <rhyme label="a" type="m">maer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Soect den Heer hier int misprezen // <rhyme label="b" type="m">dal</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Soo komt ghy niet in zorgen // <rhyme label="a" type="m" corresp="#s4:l1">zwaer</rhyme></l>
        <l id="s4:l4" met="4" type="-+">gy en wit niet wat morgen wezen // <rhyme label="b" type="m" corresp="#s4:l2">zal</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Haest tot afval // komt die <rhyme label="c" type="m">blom</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Met haer coleure // <rhyme label="d" type="m">jent</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">So die menschen <rhyme label="c" type="m" corresp="#s4:l5">som</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Qualijck zynen speure // <rhyme label="d" type="m" corresp="#s4:l6">hent</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Princelijck God van machte // <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Ghy waert, en sijt, en ooc blyven // <rhyme label="b" type="m">sult</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Veel sijn van ons geslachte // <rhyme label="a" type="m" corresp="#s5:l1">doot</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Want ghy haer met vel katijven // <rhyme label="b" type="m" corresp="#s5:l2">vult</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Hebt doch gedult met ons <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Die doch van slijcke // <rhyme label="d" type="m">sijn</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Wilt ons tuwer <rhyme label="c" type="m" corresp="#s5:l5">eer</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Bringen in u Rijcke // <rhyme label="d" type="m" corresp="#s5:l6">fijn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
