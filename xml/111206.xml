    <melody>Wat is ons al vreugde gegeven</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="amusement">vermaak</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laat ons vry van blydschap <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Rechtgeaarde <rhyme label="a" type="f" corresp="#s1:l1">Feestelingen</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Daar men onze blyden <rhyme label="b" type="m">disch</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Ciert met vleesch en wyn en <rhyme label="b" type="m" corresp="#s1:l3">visch</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Laat het vuur des Oorlogs <rhyme label="a" type="f">woeden</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Amstels Burgers blyft ons <rhyme label="a" type="f" corresp="#s2:l1">hoeden</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Syne gunst en milden <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hield gelyk in <rhyme label="b" type="m" corresp="#s2:l3">vreede-stand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Sou dit gryze <rhyme label="a" type="f">dankbezeffen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">'t Mededeelzaam hart niet <rhyme label="a" type="f" corresp="#s3:l1">treffen</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">Vreugde ciert dit <rhyme label="b" type="m">Eeuwgety</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En kroont Neerlands zorge aan 't <rhyme label="b" type="m" corresp="#s3:l3">Y</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Braaven die ons trouw <rhyme label="a" type="f">Regeeren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Willen zich met vreugd <rhyme label="a" type="f" corresp="#s4:l1">vernêeren</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Hunne zorg rykt spyze en <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die het hart verheugd doet <rhyme label="b" type="m" corresp="#s4:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Met heet Brood en <rhyme label="a" type="f">Chocolaade</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Mogten we onzen lust <rhyme label="a" type="f" corresp="#s5:l1">verzaaden</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Thans werd onzen <rhyme label="b" type="m">middags-disch</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">weêr voorsien met lekkre <rhyme label="b" type="m" corresp="#s5:l3">visch</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Honger, kommer, zorg en <rhyme label="a" type="f">vreezen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Armoê zou ons noodlot <rhyme label="a" type="f" corresp="#s6:l1">weezen</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Maar de <rhyme label="b" type="m">mededeelzaamheid</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Heeft ons kleên en speiz' <rhyme label="b" type="m" corresp="#s6:l3">bereid</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Amstel Burgers, milde <rhyme label="a" type="f">daden</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Schenken 't Kalfsvleesch rein <rhyme label="a" type="f" corresp="#s7:l1">gebraden</rhyme></l>
        <l id="s7:l3" met="4" type="+-">En om recht verheugt te <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Schenkt hun goedheid helder <rhyme label="b" type="m" corresp="#s7:l3">wyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Deze wyn doet ons by 't <rhyme label="a" type="f">Schenken</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Aan het lot dier ouden <rhyme label="a" type="f" corresp="#s8:l1">denken</rhyme>;</l>
        <l id="s8:l3" met="4" type="+-">Die en hier en gins <rhyme label="b" type="m">verspreid</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Voor eene Eeuw dit werd <rhyme label="b" type="m" corresp="#s8:l3">ontzeid</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Groentes, Suiker, zachte <rhyme label="a" type="f">spyzen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Doen thans onzen Lofgalm <rhyme label="a" type="f" corresp="#s9:l1">ryzen</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Ons besteurdren <rhyme label="b" type="m">aangezicht</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Acht een gryze vreugd thans <rhyme label="b" type="m" corresp="#s9:l3">plicht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Onze vreugd en <rhyme label="a" type="f">vergnoegen</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Kroont der Amstelaaren <rhyme label="a" type="f" corresp="#s10:l1">zwoegen</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">'t Heil dat in dit huis <rhyme label="b" type="m">geschied</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Eischt een zeegen wenschend <rhyme label="b" type="m" corresp="#s10:l3">Lied</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Leeft bestuurders, left <rhyme label="a" type="f">regeerders</rhyme>!</l>
        <l id="s11:l2" met="4" type="+-">Leeft roemwaarde <rhyme label="a" type="f" corresp="#s11:l1">Land-verweerders</rhyme>!</l>
        <l id="s11:l3" met="4" type="+-">'t Gryze hair valle u ten <rhyme label="b" type="m">deel</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">'t Strekke uw deugd ten <rhyme label="b" type="m" corresp="#s11:l3">pronkjuweel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Traanen van genoegen <rhyme label="a" type="f">vloeijen</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Als de Burgren heil mog <rhyme label="a" type="f" corresp="#s12:l1">groeijen</rhyme>:</l>
        <l id="s12:l3" met="4" type="+-">Hunne zorg en noeste <rhyme label="b" type="m">vlyt</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Werd aan een Eeuwigheid <rhyme label="b" type="m" corresp="#s12:l3">gewyd</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Welstand wekkend <rhyme label="a" type="f">alvermoogen</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Doe elk op zyn gunsten <rhyme label="a" type="f" corresp="#s13:l1">boogen</rhyme>:</l>
        <l id="s13:l3" met="4" type="+-">Vreede en voorspoed juicht aan 't <rhyme label="b" type="m">Y</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">In een volgend <rhyme label="b" type="m" corresp="#s13:l3">Eeuwgety</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
