  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Sa wel <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Laat ons te samen <rhyme label="b" type="f">drinken</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Sa wel <rhyme label="a" type="m" corresp="#s1:l1">aan</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dit diend eens rond te <rhyme label="a" type="m" corresp="#s1:l1">gaan</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Want dese <rhyme label="c" type="m">wijn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die heerlijk staat te <rhyme label="b" type="f" corresp="#s1:l2">blinken</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Want dese <rhyme label="c" type="m" corresp="#s1:l5">wijn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Bevryd het hert van <rhyme label="c" type="m" corresp="#s1:l5">pijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Welk een <rhyme label="a" type="m">vreugd</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Geeft dit aan onse <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Welk een <rhyme label="a" type="m" corresp="#s2:l1">vreugd</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Geefd dit aan onse <rhyme label="a" type="m" corresp="#s2:l1">jeugd</rhyme>!</l>
        <l id="s2:l5" met="2" type="-+">Laat ons <rhyme label="c" type="m">altyd</rhyme></l>
        <l id="s2:l6" met="3" type="-+">God Baggus gaven <rhyme label="b" type="f" corresp="#s2:l2">minnen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Die   ons <rhyme label="c" type="m" corresp="#s2:l5">altyd</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Van alle smert <rhyme label="c" type="m" corresp="#s2:l5">bevryd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Laten <rhyme label="a" type="m">wy</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Terwijl dees droppels <rhyme label="b" type="f">blinken</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Laten <rhyme label="a" type="m" corresp="#s3:l1">wy</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Eens kussen zy aan <rhyme label="a" type="m" corresp="#s3:l1">zy</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Dan sal de <rhyme label="c" type="m">wijn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Veel soeter zijn in 't <rhyme label="b" type="f" corresp="#s3:l2">drinken</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Dan zal de <rhyme label="c" type="m" corresp="#s3:l5">wijn</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Veel aangenamer <rhyme label="c" type="m" corresp="#s3:l5">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
