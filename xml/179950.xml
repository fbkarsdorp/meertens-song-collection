<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179950</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Die de wereld wel beziet</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ze weet zoo waar niet wat ze al <rhyme label="a" type="m">oor</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Veur rare <rhyme label="b" type="f">dinken</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Ze praat zoo klukt van ons <rhyme label="c" type="m">Katryn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ze weet niet of ze waar zal <rhyme label="c" type="m" corresp="#s1:l3">zyn</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Um zel ze <rhyme label="b" type="f" corresp="#s1:l2">zinken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ze zek, die koede lieve <rhyme label="a" type="m">meid</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Ze laat van <rhyme label="b" type="f">'eren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Zoo wel 'ereis, voor de <rhyme label="c" type="m">plaizier</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">(Maar ik keloof ze niet een <rhyme label="c" type="m" corresp="#s2:l3">zier</rhyme>)</l>
        <l id="s2:l5" met="2" type="-+">Um <rhyme label="b" type="f" corresp="#s2:l2">Carresseren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ze praat de folk zoo malle <rhyme label="a" type="m">praat</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Um mot ze <rhyme label="b" type="f">skamen</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Waarakt de lui, ze lyk wel <rhyme label="c" type="m">kek</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ze zek ze zelf, zoo waar ze <rhyme label="c" type="m" corresp="#s3:l3">zek</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Dat zy mot <rhyme label="b" type="f" corresp="#s3:l2">kramen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maar ok! ze loof ze niet een <rhyme label="a" type="m">beet</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Ze is kwade <rhyme label="b" type="f">spreken</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Ze 'eb ze Kaa nok van de <rhyme label="c" type="m">week</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Van onderen tot poov' <rhyme label="c" type="m" corresp="#s4:l3">bekeek'</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Ze zie keen <rhyme label="b" type="f" corresp="#s4:l2">teken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maar wat ze doen zel, weet ze <rhyme label="a" type="m">wel</rhyme>;</l>
        <l id="s5:l2" met="2" type="-+">Ze wil van <rhyme label="b" type="f">daken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Zelf by de mooije Kaatje <rhyme label="c" type="m">kaan</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En spreker um dan frindlyk <rhyme label="c" type="m" corresp="#s5:l3">aan</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Ze zel ze <rhyme label="b" type="f" corresp="#s5:l2">fraken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">En as ze dan niet waareit <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Um zel niet <rhyme label="b" type="f">zwyken</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Ze zel ze skreeuw: um pen <rhyme label="c" type="m">bedriek</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Die van de soete meisje <rhyme label="c" type="m" corresp="#s6:l3">liek</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Ze zel ze <rhyme label="b" type="f" corresp="#s6:l2">kryken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
