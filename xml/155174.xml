<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155174</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Te mei als alle vogels zingen</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WY dancken u, O Vader <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Voor al het geen dat ghy ons <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Door uwen milden <rhyme label="b" type="f">zegen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die op ons daelt in <rhyme label="a" type="m" corresp="#s1:l1">overvloet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Als eenen milden <rhyme label="b" type="f" corresp="#s1:l3">Regen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy dancken u hier <rhyme label="a" type="m">al-te-saem</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En loven dijnen grooten <rhyme label="a" type="m" corresp="#s2:l1">Naem</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die eeuwigh sy <rhyme label="b" type="f">gepresen</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Maeck ons al meer en meer <rhyme label="a" type="m" corresp="#s2:l1">bequaem</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Om danckbaer u te <rhyme label="b" type="f" corresp="#s2:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy dancken u, dat ghy dit <rhyme label="a" type="m">Paer</rhyme></l>
        <l id="s3:l2" met="4" type="-+">In d'Echt gebracht hebt by <rhyme label="a" type="m" corresp="#s3:l1">malkaer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En t'samen vast <rhyme label="b" type="f">gebonden</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Laet al haer doen sijn voor als <rhyme label="a" type="m" corresp="#s3:l1">naer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Tot saligheyt <rhyme label="b" type="f" corresp="#s3:l3">bevonden</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Geef aen haer beyde vreede, <rhyme label="a" type="m">rust</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Geef datse met vermaeck en <rhyme label="a" type="m" corresp="#s4:l1">lust</rhyme></l>
        <l id="s4:l3" met="3" type="-+">U gaven hier <rhyme label="b" type="f">be-erven</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">En brenght haer aen de rijcke <rhyme label="a" type="m" corresp="#s4:l1">Kust</rhyme></l>
        <l id="s4:l5" met="3" type="-+">By haer die saligh <rhyme label="b" type="f" corresp="#s4:l3">sterven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wy dancken u, ons' Vader <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Met uwen Soon en Geest <rhyme label="a" type="m" corresp="#s5:l1">gelijck</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat ghy ons spijst te <rhyme label="b" type="f">gader</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Zijt onse Ziel een vaste <rhyme label="a" type="m" corresp="#s5:l1">wijck</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En een versoenden <rhyme label="b" type="f" corresp="#s5:l3">Vader</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Treck door uw' gaven hier <rhyme label="a" type="m">beneen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Ons op tot u nae boven <rhyme label="a" type="m" corresp="#s6:l1">heen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Geef dat wy ons <rhyme label="b" type="f">verkloecken</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Om uwe schatten daer <rhyme label="a" type="m" corresp="#s6:l1">alleen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Met volle lust te <rhyme label="b" type="f" corresp="#s6:l3">soecken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">En laet dit blijde <rhyme label="a" type="m">Bruylofts-Feest</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Ons dienen, die hier minst en <rhyme label="a" type="m" corresp="#s7:l1">meest</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Ter Tafel zijn <rhyme label="b" type="f">geseten</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Dat wy u, Heer, in onsen <rhyme label="a" type="m" corresp="#s7:l1">geest</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Te dancken niet <rhyme label="b" type="f" corresp="#s7:l3">vergeten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy zijt voorwaer de hooghste <rhyme label="a" type="m">schat</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">En thien mael dubbel waerdigh, <rhyme label="a" type="m" corresp="#s8:l1">dat</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Wy hier u Name <rhyme label="b" type="f">loven</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En eeuwigh in de rijcke <rhyme label="a" type="m" corresp="#s8:l1">Stadt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ons' Vader-landt <rhyme label="b" type="f" corresp="#s8:l3">daerboven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Daer is een eeuwigh Feest <rhyme label="a" type="m">bereydt</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Daer men altijdt in vreughde <rhyme label="a" type="m" corresp="#s9:l1">weydt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En vrolijck is te <rhyme label="b" type="f">samen</rhyme>:</l>
        <l id="s9:l4" met="4" type="-+">Die hier sijn sonden heeft <rhyme label="a" type="m" corresp="#s9:l1">beschreyt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Sal daer verschijnen, <rhyme label="b" type="f" corresp="#s9:l3">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
