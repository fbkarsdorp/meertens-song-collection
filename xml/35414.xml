  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">UIs nu <rhyme label="a" type="m">Bruydegom</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Volkomen recht <rhyme label="b" type="f">gheboren</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Tot uw' <rhyme label="b" type="f" corresp="#s1:l2">Uyt-verkoren</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Door d'echte <rhyme label="c" type="m">Trouw</rhyme></l>
        <l id="s1:l5" met="5" type="-+">Ben ick gheworden uw beminde <rhyme label="c" type="m" corresp="#s1:l4">Vrouw</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ghenaeckt tot uwen weerden <rhyme label="d" type="m">pandt</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">En neemt u weder-helft nu metter <rhyme label="d" type="m" corresp="#s1:l6">handt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">U in dit <rhyme label="e" type="f">leven</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Van Godt <rhyme label="e" type="f" corresp="#s1:l8">ghegheven</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Door den echten <rhyme label="d" type="m" corresp="#s1:l6">bandt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghewenschsten <rhyme label="a" type="m">Med'-ghesel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Verkoren boven <rhyme label="b" type="f">allen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Aen my <rhyme label="b" type="f" corresp="#s2:l2">toe-ghevallen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Myn ander <rhyme label="c" type="m">ick</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Myns hertjens vreught door wie myn jeught <rhyme label="c" type="m" corresp="#s2:l4">verquickt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ghy staet gheplant in myn <rhyme label="d" type="m">ghemoet</rhyme></l>
        <l id="s2:l7" met="5" type="-+">Meer als myn Vader ende Moeder <rhyme label="d" type="m" corresp="#s2:l6">doet</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Om u te <rhyme label="e" type="f">vinden</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Liet ick myn <rhyme label="e" type="f" corresp="#s2:l8">Vrinden</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">En myn naeste <rhyme label="d" type="m" corresp="#s2:l6">bloedt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ontfanght myn teere <rhyme label="a" type="m">Bloem</rhyme></l>
        <l id="s3:l2" met="3" type="-+">By niemandt oyt <rhyme label="b" type="f">ghelesen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Oft in 't sandt <rhyme label="b" type="f" corresp="#s3:l2">gheresen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Uw echte <rhyme label="c" type="m">Wyf</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Een suyver Maeght een on-gheschonden <rhyme label="c" type="m" corresp="#s3:l4">lyf</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Een Roosken met den dauw <rhyme label="d" type="m">besproeyt</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">Een witte Leli' die noch groeyt, en <rhyme label="d" type="m" corresp="#s3:l6">bloeyt</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Een eerbaer <rhyme label="e" type="f">troni'</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Een <rhyme label="e" type="f" corresp="#s3:l8">Animoni'</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Die van schaemte <rhyme label="d" type="m" corresp="#s3:l6">gloeyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ontfanght uw lieve <rhyme label="a" type="m">Bruydt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En dese teere <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Die ick wil <rhyme label="b" type="f" corresp="#s4:l2">besteden</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Naer uwen <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Tot dinst en winst van ouderlinghe <rhyme label="c" type="m" corresp="#s4:l4">min</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Wy twee zyn nu geworden <rhyme label="d" type="m">een</rhyme></l>
        <l id="s4:l7" met="5" type="-+">Maeckt ons twee Zielen in een lyf <rhyme label="d" type="m" corresp="#s4:l6">ghemeen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Op dat ons <rhyme label="e" type="f">beyden</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Niet en kan <rhyme label="e" type="f" corresp="#s4:l8">scheyden</rhyme></l>
        <l id="s4:l10" met="3" type="+-">Als de doodt <rhyme label="d" type="m" corresp="#s4:l6">alleen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laet ons in peys en <rhyme label="a" type="m">vred'</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ghelyck twee Lammers <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Soo sal Godt ons <rhyme label="b" type="f" corresp="#s5:l2">gheven</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Naer onsen <rhyme label="c" type="m">eysch</rhyme></l>
        <l id="s5:l5" met="5" type="-+">Het eeuwigh leven in syn <rhyme label="c" type="m" corresp="#s5:l4">Paradys</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Laet ons hier in dit aerdtsche <rhyme label="d" type="m">dal</rhyme></l>
        <l id="s5:l7" met="5" type="-+">Ghelyck twee Tortel-Duyfkens sonder <rhyme label="d" type="m" corresp="#s5:l6">gal</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Uyt hert en <rhyme label="e" type="f">sinnen</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Malkander' <rhyme label="e" type="f" corresp="#s5:l8">minnen</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">En Godt boven <rhyme label="d" type="m" corresp="#s5:l6">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
