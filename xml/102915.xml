<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>102915</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La moutarde réformée</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">Aangename schoone <rhyme label="a" type="m">Rosemont</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Komt mijn Ziel, mijn Zon, mijn Jeugde <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s1:l3" met="5" type="+-">Laat u suycker-stroopte soete <rhyme label="a" type="m" corresp="#s1:l1">Mont</rhyme>,</l>
        <l id="s1:l4" met="5" type="+-">Mijn maar slegh een eenig Cusje <rhyme label="b" type="f" corresp="#s1:l2">geven</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Eer mijn grage lust <rhyme label="c" type="m">verdort</rhyme>.</l>
        <l id="s1:l6" met="3" type="+-">Wel wat meugje <rhyme label="d" type="f">kallen</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Souje lust <rhyme label="d" type="f" corresp="#s1:l6">vervallen</rhyme>?</l>
        <l id="s1:l8" met="4" type="+-">Nou hoe staje dus en <rhyme label="c" type="m" corresp="#s1:l5">hort</rhyme>:</l>
        <l id="s1:l9" met="5" type="-+">Men dunckt ghy Rosemont, weer spytig <rhyme label="c" type="m" corresp="#s1:l5">wort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">K'seg Leander dat je mijn laat <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">K'moet mijn Beesjes melck af gaan <rhyme label="b" type="f">trecken</rhyme></l>
        <l id="s2:l3" met="5" type="+-">D'elders zijn niet eensjes <rhyme label="a" type="m" corresp="#s2:l1">overlaan</rhyme>:</l>
        <l id="s2:l4" met="5" type="+-">K'wetse inde graasjes neder <rhyme label="b" type="f" corresp="#s2:l2">lecken</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Word ick spytig, soete <rhyme label="c" type="m">Knegt</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Tyt vry by een <rhyme label="d" type="f">ander</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Gaat vry heen <rhyme label="d" type="f" corresp="#s2:l6">Leander</rhyme>:</l>
        <l id="s2:l8" met="4" type="+-">Word ick spitig alse <rhyme label="c" type="m" corresp="#s2:l5">segt</rhyme>?</l>
        <l id="s2:l9" met="5" type="-+">Men dunckt weer dat jou vreugt in sotheyt <rhyme label="c" type="m" corresp="#s2:l5">legt</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Aardig Nimphje t'was mijn losse <rhyme label="a" type="m">tong</rhyme>,</l>
        <l id="s3:l2" met="5" type="+-">Wilt u toorn weer in jonst <rhyme label="b" type="f">verkeren</rhyme>,</l>
        <l id="s3:l3" met="5" type="+-">K'weet mijn Jaartjes zijn heel zot en <rhyme label="a" type="m" corresp="#s3:l1">jong</rhyme></l>
        <l id="s3:l4" met="5" type="+-">'Ksal mijn graag van u wel laten <rhyme label="b" type="f" corresp="#s3:l2">leren</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">K'heb berou: sie daar's mijn <rhyme label="c" type="m">handt</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">K'salje niet <rhyme label="d" type="f">bereiken</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Tis een eeuwig <rhyme label="d" type="f" corresp="#s3:l6">teiken</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">'Kvoel geen Min, ick voel geen <rhyme label="c" type="m" corresp="#s3:l5">Brant</rhyme>:</l>
        <l id="s3:l9" met="5" type="-+">Voor Cupidoo zijn toorsje in me <rhyme label="c" type="m" corresp="#s3:l5">plant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
