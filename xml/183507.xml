<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183507</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags/>
    <melody>Quand on sait aimer et plaire</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laat ons Neêrlands Helden <rhyme label="a" type="f">roemen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die de laatste <rhyme label="b" type="m">Batavier</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Nooit, dan met ontzag, zal <rhyme label="a" type="f" corresp="#s1:l1">noemen</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Dat ons loof hun' schedel <rhyme label="b" type="m" corresp="#s1:l2">fier</rhyme>!</l>
        <l id="s1:l5" met="4" type="+-">Laat ons hun gedacht'nis <rhyme label="c" type="f">eeren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Meer dan gloeiend goud <rhyme label="c" type="f" corresp="#s1:l5">waarderen</rhyme>!</l>
        <l id="s1:l7" met="4" type="+-">Daar, - door hunne trouw en <rhyme label="d" type="m">moed</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Neêrlands Vryheid is <rhyme label="d" type="m" corresp="#s1:l7">behoed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Zie op Zee de Ruyter <rhyme label="a" type="f">pralen</rhyme>;</l>
        <l id="s2:l2" met="4" type="+-">Langs den Theems den trotschen <rhyme label="b" type="m">Brit</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Knarssen, om het <rhyme label="a" type="f" corresp="#s2:l1">zegepralen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van dien halsvriend van de <rhyme label="b" type="m" corresp="#s2:l2">Wit</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">'k Zie hem Vloot by Vloot <rhyme label="c" type="f">verjagen</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Volk by Volk den vreê doen <rhyme label="c" type="f" corresp="#s2:l5">vragen</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Neêrland redden, keer op <rhyme label="d" type="m">keer</rhyme>;</l>
        <l id="s2:l8" met="4" type="+-">Saeuv'len op het bed van <rhyme label="d" type="m" corresp="#s2:l7">eer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Zie de Witten, als <rhyme label="a" type="f">Pilaaren</rhyme>.</l>
        <l id="s3:l2" met="4" type="+-">Voor de zaak der Vryheid <rhyme label="b" type="m">staan</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Woede, en haat, en <rhyme label="a" type="f" corresp="#s3:l1">lyfsgevaren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Onverschrokken, tegen <rhyme label="b" type="m" corresp="#s3:l2">gaan</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">'k Zie hen Neêrland grootheid <rhyme label="c" type="f">staven</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Weer dan 't immer Vorsten <rhyme label="c" type="f" corresp="#s3:l5">gaven</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Doch ter Slagtbank heen <rhyme label="d" type="m">gesleurd</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En, voor trouwe dienst, <rhyme label="d" type="m" corresp="#s3:l7">verscheurd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Zie noch groter <rhyme label="a" type="f">gruwelstukken</rhyme>!</l>
        <l id="s4:l2" met="4" type="+-">'k Zie 's Lands Vader, <rhyme label="b" type="m">barneveld</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Aan het heilig recht <rhyme label="a" type="f" corresp="#s4:l1">ontrukken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Opgeofferd aan 't <rhyme label="b" type="m" corresp="#s4:l2">geweld</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Hem, den schrik van 't machtig <rhyme label="c" type="f">Spanje</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Grootheids stichter van <rhyme label="c" type="f" corresp="#s4:l5">Oranje</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">Neêrlands Vriend, den Vriend van <rhyme label="d" type="m">God</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Sterven op het <rhyme label="d" type="m" corresp="#s4:l7">Moordschavot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Zie noch andre kloeke <rhyme label="a" type="f">Helden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Die, mee pen of <rhyme label="b" type="m">Heldenstaal</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Zich in de open bresse <rhyme label="a" type="f" corresp="#s5:l1">stelden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Voor het veege <rhyme label="b" type="m" corresp="#s5:l2">Letuwendaal</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">'k Zie de Groot met lauren <rhyme label="c" type="f">pralen</rhyme>;</l>
        <l id="s5:l6" met="4" type="+-">Trompen, Zweertsen en van <rhyme label="c" type="f" corresp="#s5:l5">Galen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Duizend and'ren, grootsch <rhyme label="d" type="m">bekroond</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Om de trouw, door hun <rhyme label="d" type="m" corresp="#s5:l7">betoond</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">ô Dat hunne stille <rhyme label="a" type="f">graven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Zyn met lauweren <rhyme label="b" type="m">gedekt</rhyme>!</l>
        <l id="s6:l3" met="4" type="+-">Zo lang de eer van Neêrlands <rhyme label="a" type="f" corresp="#s6:l1">Braven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Neerland tot geen walg <rhyme label="b" type="m" corresp="#s6:l2">verstrekt</rhyme>!</l>
        <l id="s6:l5" met="4" type="+-">Laat ons, bovenal, hen <rhyme label="c" type="f">achten</rhyme>.</l>
        <l id="s6:l6" met="4" type="+-">Die de Heerschzucht wreed deedt <rhyme label="c" type="f" corresp="#s6:l5">slagten</rhyme>!</l>
        <l id="s6:l7" met="4" type="+-">Zulk een naam zy 't hoogst <rhyme label="d" type="m">geroemd</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Die door slaven wordt <rhyme label="d" type="m" corresp="#s6:l7">gedoemt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
