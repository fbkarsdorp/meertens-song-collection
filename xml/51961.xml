<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>51961</id>
    <date>
      <year-start>1701</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HEt <rhyme label="a" type="f">Roose-knopje</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Ontfangt graag een <rhyme label="a" type="f" corresp="#s1:l1">dropje</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Des Dauws om beide te <rhyme label="b" type="m">voen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Maar 't is nau <rhyme label="c" type="f">ontlooken</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Of 't bey die <rhyme label="c" type="f" corresp="#s1:l4">gedoken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dan onder het Jeugdige <rhyme label="b" type="m" corresp="#s1:l3">Groen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Komt 't Bloempje <rhyme label="d" type="f">belagen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">In 't rieken der <rhyme label="d" type="f" corresp="#s1:l7">dagen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">En geeft het zoo meenige <rhyme label="b" type="m" corresp="#s1:l3">zoen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Maar Minne <rhyme label="a" type="f">Lusjes</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">En Dertele <rhyme label="a" type="f" corresp="#s2:l1">kusjes</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van 't beydie die waaren <rhyme label="b" type="m">gemeen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Want als het heeft <rhyme label="c" type="f">verhoolen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Den Koning <rhyme label="c" type="f" corresp="#s2:l4">gestolen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dan vliegt 'er dat diefje weer <rhyme label="b" type="m" corresp="#s2:l3">heen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Zegt tegen het <rhyme label="d" type="f">Roosje</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Ik gun u het <rhyme label="d" type="f" corresp="#s2:l7">Bloosje</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">U Nectar die dient myn <rhyme label="b" type="m" corresp="#s2:l3">alleen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
