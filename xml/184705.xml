<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184705</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>den haag</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zoet herderin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Wie hoor ik <rhyme label="a" type="m">daar</rhyme>?</l>
        <l id="s1:l2" met="4" type="-+">Wie komt hier myn rust <rhyme label="b" type="f">verstooren</rhyme>?</l>
        <l id="s1:l3" met="2" type="+-">Wat vreemde <rhyme label="a" type="m" corresp="#s1:l1">maar</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Brengt gy hier, Seg Ouden <rhyme label="a" type="m" corresp="#s1:l1">vaar</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Wat komt gy hier al <rhyme label="c" type="f">praten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wilt gy uyt vryen <rhyme label="d" type="m">gaan</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Gy kont nauwelyks meer <rhyme label="d" type="m" corresp="#s1:l6">staan</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wilt Soetelijk <rhyme label="c" type="f" corresp="#s1:l5">laten</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Ach ik word <rhyme label="e" type="m">vervaard</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Als ik sie naar uwen <rhyme label="e" type="m" corresp="#s1:l9">baard</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ach liefste <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ik zal u in een koets doen <rhyme label="b" type="f">ryden</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Want geld en <rhyme label="a" type="m" corresp="#s2:l1">goed</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat heb ik in <rhyme label="a" type="m" corresp="#s2:l1">overvloed</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Schoon lief wilt my <rhyme label="c" type="f">ontfermen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gy sult dagelyks <rhyme label="d" type="m">syn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gekleet in ’t wit <rhyme label="d" type="m" corresp="#s2:l6">satyn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ach hoort toch na mijn <rhyme label="c" type="f" corresp="#s2:l5">kermen</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Schenkt my u <rhyme label="e" type="m">trouw</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Gy sult wesen een <rhyme label="e" type="m" corresp="#s2:l9">Mevrouw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Door al u <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En sult gy mijnen jonst niet <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Gy Ouden <rhyme label="a" type="m" corresp="#s3:l1">bloed</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">’t Is om niet al wat gy <rhyme label="a" type="m" corresp="#s3:l1">doet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En komt my niet meer <rhyme label="c" type="f">kwellen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">‘k Ben uwen klap al <rhyme label="d" type="m">moe</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">‘k En Ben al niet voor <rhyme label="d" type="m" corresp="#s3:l6">u</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Gy Oude <rhyme label="c" type="f" corresp="#s3:l5">geselle</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Peijst op u <rhyme label="e" type="m">graf</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">En legt al u vrijen <rhyme label="e" type="m" corresp="#s3:l9">af</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Schoon lief <rhyme label="a" type="m">waarom</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En wilt gy my u trouw niet <rhyme label="b" type="f">geven</rhyme>!</l>
        <l id="s4:l3" met="2" type="+-">Seg schoone <rhyme label="a" type="m" corresp="#s4:l1">blom</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Is ’t om mijnen <rhyme label="a" type="m" corresp="#s4:l1">Ouderdom</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ben ik wat oud van <rhyme label="c" type="f">jaren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Soo waar ik haast een <rhyme label="d" type="m">lijk</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dan zijt gy magtig <rhyme label="d" type="m" corresp="#s4:l6">rijk</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ach laat ons t’ samen <rhyme label="c" type="f" corresp="#s4:l5">paren</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Als ik dan <rhyme label="e" type="m">sterf</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Maak ik u al myn goed in <rhyme label="e" type="m" corresp="#s4:l9">erf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Grampeer meynd <rhyme label="a" type="m">gy</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Het ja woord door u geld te <rhyme label="b" type="f">krijgen</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Neen gaat van <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Maakt daar mee een ander <rhyme label="a" type="m" corresp="#s5:l1">bly</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Waar ik niet een <rhyme label="c" type="f">sottinne</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat ik soon Ouden <rhyme label="d" type="m">grol</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Met sijnen grijsen <rhyme label="d" type="m" corresp="#s5:l6">bol</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Om geld of goed sou <rhyme label="c" type="f" corresp="#s5:l5">minnen</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Neen gy Ouden <rhyme label="e" type="m">sot</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Gaat vry van hier, Gy zijt half <rhyme label="e" type="m" corresp="#s5:l9">rot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ach liefste <rhyme label="a" type="m">meyd</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ik bid u en weest niet so <rhyme label="b" type="f">spytig</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Ik heb u <rhyme label="a" type="m" corresp="#s6:l1">gevryd</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Wel eenen soo langen <rhyme label="a" type="m" corresp="#s6:l1">tyd</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Schoon maget, Lief <rhyme label="c" type="f">gepresen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Geeft my al voor het <rhyme label="d" type="m">lest</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Den fiat op het <rhyme label="d" type="m" corresp="#s6:l6">request</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dan sal ik sijn <rhyme label="c" type="f" corresp="#s6:l5">genesen</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Al van mijn <rhyme label="e" type="m">smert</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Dien ik gevoel aan myn <rhyme label="e" type="m" corresp="#s6:l9">hert</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Grampeer hoe sou <rhyme label="a" type="m">‘t</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Gy nog minnenbrand <rhyme label="b" type="f">gevoelen</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Gy zijt seer <rhyme label="a" type="m" corresp="#s7:l1">oud</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En als eenen steen soo <rhyme label="a" type="m" corresp="#s7:l1">koud</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Gy en kond my niet <rhyme label="c" type="f">verblijden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Gy stijven <rhyme label="d" type="m">serviteur</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Gaat straks al van mijn <rhyme label="d" type="m" corresp="#s7:l6">deur</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Gy ouden <rhyme label="c" type="f" corresp="#s7:l5">geselle</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Gaat weg <rhyme label="e" type="m">Grampeer</rhyme>,</l>
        <l id="s7:l10" met="4" type="+-">En komt gy hier nimmer <rhyme label="e" type="m" corresp="#s7:l9">weer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">U <rhyme label="a" type="m">spijtigheijd</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Sou my ’t vrijen doen <rhyme label="b" type="f">verdrieten</rhyme>,</l>
        <l id="s8:l3" met="2" type="+-">Adieu ’t is <rhyme label="a" type="m" corresp="#s8:l1">tyd</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ik hebbe genoeg van dit <rhyme label="a" type="m" corresp="#s8:l1">verwijt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Sout gy my doen <rhyme label="c" type="f">verlangen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat ik waar stijf en <rhyme label="d" type="m">oud</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Wilt gy niet worden <rhyme label="d" type="m" corresp="#s8:l6">oud</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Laat u in tijds <rhyme label="c" type="f" corresp="#s8:l5">ophangen</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Ik en kom niet <rhyme label="e" type="m">weer</rhyme>,</l>
        <l id="s8:l10" met="4" type="+-">’t Is nu voor de leste <rhyme label="e" type="m" corresp="#s8:l9">keer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
