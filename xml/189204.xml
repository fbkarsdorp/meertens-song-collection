  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">De mag're <rhyme label="a" type="m">Gierigaart</rhyme></l>
        <l id="s1:l2" met="3" type="-+">syn gelt en kost <rhyme label="a" type="m" corresp="#s1:l1">bespaart</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En zelf gebrek moet <rhyme label="b" type="f">lyden</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Hy is een ryke <rhyme label="c" type="m">bedelaar</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En heeft met zorg en angst te <rhyme label="b" type="f" corresp="#s1:l3">stryden</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Hy vreest voor onheil en <rhyme label="c" type="m" corresp="#s1:l4">gevaar</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Als hy syn schaduw word <rhyme label="c" type="m" corresp="#s1:l4">gewaar</rhyme>.</l>
      </lg>
    </lg>
