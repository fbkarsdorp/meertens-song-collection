<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>36030</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen [=amsterdam]</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>christmas song</tag>
    </tags>
    <melody>O débonnaire Jésus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Och! daer en is in dese <rhyme label="a" type="m">Stal</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Niet met <rhyme label="a" type="m" corresp="#s1:l1">al</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Onder dit kleyn <rhyme label="b" type="m">dack</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Is geen <rhyme label="b" type="m" corresp="#s1:l3">gemack</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Iesus light <rhyme label="c" type="m">hier</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Sonder <rhyme label="c" type="m" corresp="#s1:l5">vyer</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">In de <rhyme label="d" type="m">windt</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Gelijck een <rhyme label="d" type="m" corresp="#s1:l7">kindt</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Vol van <rhyme label="e" type="m">druck</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En vol van <rhyme label="e" type="m" corresp="#s1:l9">ongeluck</rhyme>.</l>
        <l id="s1:l11" met="4" type="+-">Siet, hy light daer op het <rhyme label="f" type="m">stroy</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">En hy is gedeckt met <rhyme label="f" type="m" corresp="#s1:l11">hoy</rhyme>;</l>
        <l id="s1:l13" met="5" type="+-">Is dit voor een Konincks kint niet <rhyme label="f" type="m" corresp="#s1:l11">moy</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een yegelijck die gaet ter <rhyme label="a" type="m">zy</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Of <rhyme label="a" type="m" corresp="#s2:l1">voorby</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Daer is geen <rhyme label="b" type="m">besoeck</rhyme></l>
        <l id="s2:l4" met="2" type="-+">In desen <rhyme label="b" type="m" corresp="#s2:l3">hoeck</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Noch aen de <rhyme label="c" type="m">deur</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Staet <rhyme label="c" type="m" corresp="#s2:l5">Monsieur</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Moch <rhyme label="d" type="m">Me-vrouw</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Zy vreest de <rhyme label="d" type="m" corresp="#s2:l7">kouw</rhyme>’;</l>
        <l id="s2:l9" met="2" type="+-">Daer is <rhyme label="e" type="m">hier</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Noch meyt, noch <rhyme label="e" type="m" corresp="#s2:l9">kamenier</rhyme>.</l>
        <l id="s2:l11" met="4" type="+-">Ioseph die gaet op en <rhyme label="f" type="m">neer</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Ioseph die gaet wegh, en <rhyme label="f" type="m" corresp="#s2:l11">weer</rhyme>,</l>
        <l id="s2:l13" met="5" type="+-">Ioseph dient den nieuw-geboren <rhyme label="f" type="m" corresp="#s2:l11">Heer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick sach daer maer wat Herders <rhyme label="a" type="m">by</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Twee of <rhyme label="a" type="m" corresp="#s3:l1">dry</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En besijden <rhyme label="b" type="m">stont</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Den Herders <rhyme label="b" type="m" corresp="#s3:l3">hondt</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">d’Een die bracht <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Melck en <rhyme label="c" type="m" corresp="#s3:l5">saen</rhyme>;</l>
        <l id="s3:l7" met="2" type="+-">d’Ander <rhyme label="d" type="m">quam</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Met een vet <rhyme label="d" type="m" corresp="#s3:l7">Lam</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">‘t Derde <rhyme label="e" type="m">schonck</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Het kindt een duyve <rhyme label="e" type="m" corresp="#s3:l9">jonck</rhyme>.</l>
        <l id="s3:l11" met="4" type="+-">En doen namen sy hun <rhyme label="f" type="m">fluyt</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Maeckten t’samen soet <rhyme label="f" type="m" corresp="#s3:l11">geluyt</rhyme>,</l>
        <l id="s3:l13" met="5" type="+-">Daer mee gingen sy den stal weer <rhyme label="f" type="m" corresp="#s3:l11">uyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
