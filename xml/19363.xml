<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19363</id>
    <date>
      <year-start>1552</year-start>
      <year-end>1554</year-end>
    </date>
    <location>
      <orginal-name>antwerpen]</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Te Brunswijk staat een stenen huis</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Heere aensiet toch al mijn lijden <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Gheeft my nv stercheyt in deser <rhyme label="a" type="m" corresp="#s1:l1">noot</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Op dat ick niet en <rhyme label="b" type="f">beswijcke</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Maect my volstantich tot inder <rhyme label="a" type="m" corresp="#s1:l1">doot</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Dat ick van v waerheyt niet en <rhyme label="b" type="f" corresp="#s1:l3">wijcke</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Op God staet alle mijn hope <rhyme label="a" type="m">alleyn</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Maect my van alle mijn sonden <rhyme label="a" type="m" corresp="#s2:l1">reyn</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Wilt mijn ghelooue <rhyme label="b" type="f">vermeeren</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Uan v compt alle mijn hulpe <rhyme label="a" type="m" corresp="#s2:l1">ghemeyn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">O almachtich Heere der <rhyme label="b" type="f" corresp="#s2:l3">Heeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O Uader vergheeft my alle mijn <rhyme label="a" type="m">schult</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ick weet dat ghy my niet verlaten en <rhyme label="a" type="m" corresp="#s3:l1">sult</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Maect my doch los van mijnen <rhyme label="b" type="f">sonden</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Ghy hebt my met uwen gheest <rhyme label="a" type="m" corresp="#s3:l1">veruult</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Wie can v goetheyt <rhyme label="b" type="f" corresp="#s3:l3">doorgronden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Heere van v coempt alle mijn <rhyme label="a" type="m">salicheyt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Wilt my verlossen niet langhe en <rhyme label="a" type="m" corresp="#s4:l1">beyt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Mijn stercte mijnen steen <rhyme label="b" type="f">verheuen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Mijn toeulucht heb ick op v <rhyme label="a" type="m" corresp="#s4:l1">gheleyt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ghy en sult my nemmermeer <rhyme label="b" type="f" corresp="#s4:l3">begheuen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O God mijn siele is in my <rhyme label="a" type="m">bedroeft</rhyme></l>
        <l id="s5:l2" met="4" type="-+">U goddelijcke helpe si <rhyme label="a" type="m" corresp="#s5:l1">behoeft</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Mijnen steen waerom hebdy my <rhyme label="b" type="f">verlaten</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Helpt my o Heere niet en <rhyme label="a" type="m" corresp="#s5:l1">vertoeft</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Want vele zijnder die my <rhyme label="b" type="f" corresp="#s5:l3">haten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wat bedroeft ghy v siele <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Waerom wilt ghy in my so truerich <rhyme label="a" type="m" corresp="#s6:l1">zijn</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Stelt v betrouwen op den <rhyme label="b" type="f">Heere</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Ghy sult noch aenschouwen zijn claer <rhyme label="a" type="m" corresp="#s6:l1">aenschijn</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Uwen druck sal in blyschap <rhyme label="b" type="f" corresp="#s6:l3">verkeeren</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ghy ghelouighen weest nv <rhyme label="a" type="m">verblijt</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Al eest dat ghy cleyn van ghetale <rhyme label="a" type="m" corresp="#s7:l1">zijt</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Wie can v deeren ofte <rhyme label="b" type="f">letten</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Eest dat God den Heere voor ons <rhyme label="a" type="m" corresp="#s7:l1">strijt</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Wie wil hem teghen ons dan <rhyme label="b" type="f" corresp="#s7:l3">setten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die Heere is stercker dan Casteel oft <rhyme label="a" type="m">slot</rhyme></l>
        <l id="s8:l2" met="4" type="-+">O Heere weest my een beschermende <rhyme label="a" type="m" corresp="#s8:l1">Godt</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Teghen alle mijn boose <rhyme label="b" type="f">vianden</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Ghy zijt o Heere mijn deel en <rhyme label="a" type="m" corresp="#s8:l1">lot</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Uerlost my vander leeuwen <rhyme label="b" type="f" corresp="#s8:l3">tanden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Heere machtich inden <rhyme label="a" type="m">strijt</rhyme></l>
        <l id="s9:l2" met="4" type="-+">O hooftman Israels die ons <rhyme label="a" type="m" corresp="#s9:l1">beurijt</rhyme></l>
        <l id="s9:l3" met="4" type="-+">O Godt na v heb ick <rhyme label="b" type="f">verlanghen</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Uwen dienaer die hier veruolghinge <rhyme label="a" type="m" corresp="#s9:l1">lijdt</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Wilt hem toch in v rijck <rhyme label="b" type="f" corresp="#s9:l3">ontfanghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
