  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HAd ic vlogel als <rhyme label="a" type="m">seraphin</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ic soude so hooghe <rhyme label="b" type="f">vlieghen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Hier bouen inder <rhyme label="c" type="m">eewicheyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Tot heer Iesum Marien <rhyme label="b" type="f" corresp="#s1:l2">lieue</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">So soudick seggen heer vader <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wanneer suldijt mi <rhyme label="b" type="f">loonen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dat ic v lange heb <rhyme label="c" type="m">ghedient</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Nu spant mi uwe <rhyme label="b" type="f" corresp="#s2:l2">croone</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wat soudic v mijn crone <rhyme label="a" type="m">spannen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghi sijt so edel so <rhyme label="b" type="f">schoone</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Want ghi die selue spieghel <rhyme label="c" type="m">sijt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer ic mi in <rhyme label="b" type="f" corresp="#s3:l2">verthoone</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat ic selue die spieghel <rhyme label="a" type="m">ben</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dies mach ick mi <rhyme label="b" type="f">verbliden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Met ciere ons dese verwaende <rhyme label="c" type="m">bruyt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ende settese neuen mijn <rhyme label="b" type="f" corresp="#s4:l2">side</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Nu wel op goede <rhyme label="a" type="m">seraphin</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ende oock Gods enghelen <rhyme label="b" type="f">alle</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Men ciere ons deze verwaende <rhyme label="c" type="m">bruyt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Si is mi wel <rhyme label="b" type="f" corresp="#s5:l2">beuallen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sij ghinghen daer inde <rhyme label="a" type="m">camer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Sij souden die bruyt gaan <rhyme label="b" type="f">chieren</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Daer quam die heylige <rhyme label="c" type="m">driuuldicheyt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ende ghincse <rhyme label="b" type="f" corresp="#s6:l2">confirmeren</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maria bracht een hemdeken <rhyme label="a" type="m">wit</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat was van siden <rhyme label="b" type="f">cleyne</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Het was haer suyver <rhyme label="c" type="m">reynicheyt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat bracht si haer <rhyme label="b" type="f" corresp="#s7:l2">alleyne</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die heylighe gheest bracht een <rhyme label="a" type="m">sercoet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat was met goude <rhyme label="b" type="f">besleghen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Die enghelen van <rhyme label="c" type="m">hemelrijck</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Die booghen haer ende <rhyme label="b" type="f" corresp="#s8:l2">neghen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die engel bracht haer eenen <rhyme label="a" type="m">rock</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Die was al sonder <rhyme label="b" type="f">nade</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Vrou bruyt draecht nu desen <rhyme label="c" type="m">roc</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Vanden seluen draecht v <rhyme label="b" type="f" corresp="#s9:l2">vader</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Si ginc voer sijn voeten <rhyme label="a" type="m">staen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Si sach hem aen van <rhyme label="b" type="f">minnen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Hi sprack haer also lieflijck <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">God groet v <rhyme label="b" type="f" corresp="#s10:l2">coninghinne</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Doen ons vrouwe ghenesen <rhyme label="a" type="m">was</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Ontrent der seste haluer <rhyme label="b" type="f">weken</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Doen leydemense ter kercken <rhyme label="c" type="m">waert</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Als ander vrouwen <rhyme label="b" type="f" corresp="#s11:l2">pleghen</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Twee duyfkens bracht si in haer <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Ende een keersse <rhyme label="b" type="f">clare</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Dat was haer suuer <rhyme label="c" type="m">reynicheyt</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Die drachtse <rhyme label="b" type="f" corresp="#s12:l2">openbare</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Doe ons vrou inden tempel <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Symeon quam haer <rhyme label="b" type="f">teghen</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Nu sie ic al dat ic <rhyme label="c" type="m">begheere</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Ic en ghere niet langer te <rhyme label="b" type="f" corresp="#s13:l2">leuen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Al dat is ende wesen <rhyme label="a" type="m">mach</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Heeft God in sijnder <rhyme label="b" type="f">machte</rhyme></l>
        <l id="s14:l3" met="4" type="-+">Al sonder wee was hi <rhyme label="c" type="m">gheboren</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Ter couder <rhyme label="b" type="f" corresp="#s14:l2">middernachte</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">O o o o <rhyme label="a" type="m">gloria</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Dat soetste vander <rhyme label="b" type="f">minnen</rhyme></l>
        <l id="s15:l3" met="4" type="-+">Dat is heer Iesus Marien <rhyme label="c" type="m">sone</rhyme></l>
        <l id="s15:l4" met="3" type="-+">God late ons hem <rhyme label="b" type="f" corresp="#s15:l2">ghewinnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
