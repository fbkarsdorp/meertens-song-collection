  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ade ich mos mich <rhyme label="a" type="f">scheidenn</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">ade ich mos <rhyme label="b" type="m">daruann</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">ich bitt nh  dragtt kein <rhyme label="a" type="f" corresp="#s1:l1">leidenn</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">das ich mos vonn dir <rhyme label="a" type="f" corresp="#s1:l1">scheidenn</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">gedenck hertzlieb <rhyme label="b" type="m" corresp="#s1:l2">darann</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gib nemantz baldtt gla <rhyme label="a" type="f">benn</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">vertraw nitt <rhyme label="b" type="m">jedermann</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">schlag alles a s dein <rhyme label="a" type="f" corresp="#s2:l1">synnenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">das dir kein schmertzen <rhyme label="a" type="f" corresp="#s2:l1">brengenn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">gedenck hertzlieb <rhyme label="b" type="m" corresp="#s2:l2">darann</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Negst Gott bist  mir die <rhyme label="a" type="f">liebste</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">schwer ich a ff meinenn <rhyme label="b" type="m">aidt</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">Das hertz jn meinem <rhyme label="a" type="f" corresp="#s3:l1">leibe</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">ist dein vnnd sall dir <rhyme label="a" type="f" corresp="#s3:l1">pleibenn</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">wehr ich schonn vber thausentt meilen <rhyme label="b" type="m" corresp="#s3:l2">weit</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ade d  <rhyme label="a" type="f">auserwelte</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">ade ich moes dar <rhyme label="b" type="m">ann</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Gott moes dir jn gesontheitt <rhyme label="a" type="f" corresp="#s4:l1">sparenn</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">vnnd dich vor leidt <rhyme label="a" type="f" corresp="#s4:l1">bewarenn</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">bis das ich wieder <rhyme label="b" type="m" corresp="#s4:l2">kom</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
