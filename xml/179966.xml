    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wie kwam mij te wekken zo ras</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zouw ik myn Celinde <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Vrolyk groeten met een <rhyme label="a" type="m" corresp="#s1:l1">lied</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daar zy heden Jarig <rhyme label="b" type="m">is</rhyme>?</l>
        <l id="s1:l4" met="2" type="-+">ô Ja, <rhyme label="b" type="m" corresp="#s1:l3">gewis</rhyme></l>
        <l id="s1:l5" met="4" type="+-">'k Zal u, mondig Meisje, <rhyme label="c" type="m">thans</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Met geen Bloem of <rhyme label="c" type="m" corresp="#s1:l5">Mirthekrans</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Die de Herfst ons <rhyme label="d" type="m">roofd</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Versieren 't jeugdig <rhyme label="d" type="m" corresp="#s1:l7">hoofd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Neen, ik heb iets aêrs <rhyme label="a" type="m">bedagt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Geen u word van my <rhyme label="a" type="m" corresp="#s2:l1">gebragt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En u in dit bar <rhyme label="b" type="m">zaizoen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Meer nut zal <rhyme label="b" type="m" corresp="#s2:l3">doen</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Dek uw teêre leên daar <rhyme label="c" type="m">meê</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Zoo als Laura eertyds <rhyme label="c" type="m" corresp="#s2:l5">deê</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Die my, door de <rhyme label="d" type="m">dood</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Deed erven dit <rhyme label="d" type="m" corresp="#s2:l7">kleinood</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vyf en twintig jarig <rhyme label="a" type="m">mensch</rhyme>!</l>
        <l id="s3:l2" met="4" type="+-">Zo gy krygt het geen ik <rhyme label="a" type="m" corresp="#s3:l1">wensch</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zult gy krygen alle <rhyme label="b" type="m">goed</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Van de <rhyme label="b" type="m" corresp="#s3:l3">overvloed</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Ja, 'k zal u met ditto <rhyme label="c" type="m">waar</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Noch verwarmen jaar op <rhyme label="c" type="m" corresp="#s3:l5">jaar</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">En steeds zyn <rhyme label="d" type="m">verblyd</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Wanneer gy jarig <rhyme label="d" type="m" corresp="#s3:l7">zyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Schenk daarop dit glaasjen <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Leef lang en vernoegt, <rhyme label="a" type="m" corresp="#s4:l1">vrindin</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">'k Drink het daarop schoontjes <rhyme label="b" type="m">uit</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Daar is de <rhyme label="b" type="m" corresp="#s4:l3">fluit</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Tirsis, volg uw' Kloris <rhyme label="c" type="m">na</rhyme>;</l>
        <l id="s4:l6" met="4" type="+-">Hebt ge 'er zin toe? 'k zie van <rhyme label="c" type="m" corresp="#s4:l5">ja</rhyme>;</l>
        <l id="s4:l7" met="3" type="+-">Roep met my: <rhyme label="d" type="m">Hoezee</rhyme>!</l>
        <l id="s4:l8" met="3" type="-+">Celinde, leef in <rhyme label="d" type="m" corresp="#s4:l7">vreê</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
