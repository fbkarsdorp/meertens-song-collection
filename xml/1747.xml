    <melody>Te Brunswijk staat een kasteel</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">TE Bruynswijck staet een <rhyme label="a" type="m">casteel</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Daer woonen ghebroeders <rhyme label="b" type="f">sesse</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Die een die hadde een meysken <rhyme label="c" type="m">lief</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hi en condese niet <rhyme label="b" type="f" corresp="#s1:l2">vergessen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Dat clage ic <rhyme label="d" type="m">god</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Hi en condese niet <rhyme label="b" type="f" corresp="#s1:l2">verghesse</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die ridder sprac sinen schiltknecht <rhyme label="a" type="m">toe</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Och hansken lieue <rhyme label="b" type="f">gheselle</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Nv sadelt mi mijn alder beste <rhyme label="c" type="m">ros</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Laet ruysschen ouer den <rhyme label="b" type="f" corresp="#s2:l2">velde</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Dat clage ic <rhyme label="d" type="m">god</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Laet ruysschen ouer den <rhyme label="b" type="f" corresp="#s2:l2">velde</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Doen hy dat groene woudt ouer <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s3:l2" met="3" type="-+">dat ros dat viel ter <rhyme label="b" type="f">aerden</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Helpt mi Maria schoone moeder <rhyme label="c" type="m">gods</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wat sal mijns nv <rhyme label="b" type="f" corresp="#s3:l2">ghewerden</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Dat clage ic <rhyme label="d" type="m">god</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wat sal mijns nv <rhyme label="b" type="f" corresp="#s3:l2">ghewerden</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ende hi hief op een liedeken hi <rhyme label="a" type="m">sanck</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Alle druc woude hi <rhyme label="b" type="f">vergeten</rhyme></l>
        <l id="s4:l3" met="4" type="-+">So luyde dat opter salen <rhyme label="c" type="m">clanc</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer zijn liefken was <rhyme label="b" type="f" corresp="#s4:l2">gheseten</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Dat claghe ic <rhyme label="d" type="m">god</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Daer zijn liefken was <rhyme label="b" type="f" corresp="#s4:l2">gheseten</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Si stack haer hooft ter tinnen <rhyme label="a" type="m">wt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met twee beweende <rhyme label="b" type="f">ooghen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Helpt mi Maria schoone moeder <rhyme label="c" type="m">gods</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Men gaet mijn lief <rhyme label="b" type="f" corresp="#s5:l2">onthoofden</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Dat claghe ick <rhyme label="d" type="m">god</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Men gaet mijn lief <rhyme label="b" type="f" corresp="#s5:l2">onthoofden</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Si nam dat hooft al in haer <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Si wiesschet vanden <rhyme label="b" type="f">bloede</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Helpt mi Maria schoone moeder <rhyme label="c" type="m">gods</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hoe wee is mi te <rhyme label="b" type="f" corresp="#s6:l2">moede</rhyme></l>
        <l id="s6:l5" met="2" type="-+">dat clage ick <rhyme label="d" type="m">god</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Hoe wee is mi te <rhyme label="b" type="f" corresp="#s6:l2">moede</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Si nam dat swaert al metten <rhyme label="a" type="m">knoop</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Si setet op haer <rhyme label="b" type="f">herte</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Si litet so lijselijc innewaert <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Des hadde die ridder <rhyme label="b" type="f" corresp="#s7:l2">smerte</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Dat claghe ic <rhyme label="d" type="m">god</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Des hadde die ridder <rhyme label="b" type="f" corresp="#s7:l2">smerte</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Nv zijn daer twee ghelieuekens <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die een om des anders <rhyme label="b" type="f">wille</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Waer vintmense nv in <rhyme label="c" type="m">kerstenrijck</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Die draghen sulcke <rhyme label="b" type="f" corresp="#s8:l2">minne</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Dat claghe ick <rhyme label="d" type="m">god</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Die draghen sulcke <rhyme label="b" type="f" corresp="#s8:l2">minne</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Nv zijn daer twee gheliefkens <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Och waer salmense <rhyme label="b" type="f">grauen</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Al onder eenen <rhyme label="c" type="m">eglentier</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat graf sal rooskens <rhyme label="b" type="f" corresp="#s9:l2">draghen</rhyme></l>
        <l id="s9:l5" met="2" type="-+">dat claghe ic <rhyme label="d" type="m">god</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Dat graf sal rooskens <rhyme label="b" type="f" corresp="#s9:l2">draghen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
