  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ay treurt niet in het Bruylofts <rhyme label="a" type="m">kleedt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Nu ‘t bruytje is <rhyme label="b" type="m">gegaen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">In ‘t kloostertjen van lief en <rhyme label="a" type="m" corresp="#s1:l1">leet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Daer twee paer schoentjens <rhyme label="b" type="m" corresp="#s1:l2">staan</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Twee paar in lieffde <rhyme label="a" type="m">neergestort</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Voor ‘t ongeschonden <rhyme label="b" type="m">bedt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Daer ‘t donsje overgooten <rhyme label="a" type="m" corresp="#s2:l1">wort</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met thym en <rhyme label="b" type="m" corresp="#s2:l2">Violet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daer Cupido het soete <rhyme label="a" type="m">wight</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met ‘t stooke brantje <rhyme label="b" type="m">swiert</rhyme></l>
        <l id="s3:l3" met="4" type="-+">En met een geurigh hemels <rhyme label="a" type="m" corresp="#s3:l1">ligt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Haer blijde Nachjens <rhyme label="b" type="m" corresp="#s3:l2">viert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat ben je een soet sape <rhyme label="a" type="m">Nesch</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ik gloey so ras hy sydt <rhyme label="b" type="m">Matres</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Ik glinster by twe peerlen <rhyme label="a" type="m" corresp="#s4:l1">in</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Raadt wie betoovert siel en <rhyme label="b" type="m" corresp="#s4:l2">sin</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Met roose krqansjes op haer <rhyme label="a" type="m">hooft</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Gepluckt met volle <rhyme label="b" type="m">tier</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Daer ‘t vertreckje is <rhyme label="a" type="m" corresp="#s5:l1">doorstooft</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Met puyck van <rhyme label="b" type="m" corresp="#s5:l2">eglantier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wiens snatertje dan niet en <rhyme label="a" type="m">gaet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Wiens hertje suchtens <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Past beter inde sneeu op <rhyme label="a" type="m" corresp="#s6:l1">straet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dan byde blye <rhyme label="b" type="m" corresp="#s6:l2">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ia soetertjes die dorheyt <rhyme label="a" type="m">haat</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En gloeyt in reyne <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">V twede is u rijckste <rhyme label="a" type="m" corresp="#s7:l1">staat</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Prent dit vry in u <rhyme label="b" type="m" corresp="#s7:l2">sin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die mint en wint is vreught <rhyme label="a" type="m">beryt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Maer inde hoogste <rhyme label="b" type="m">noot</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Teelt eenigheyt, <rhyme label="a" type="m" corresp="#s8:l1">oneenigheyt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Oneenigheyt: de <rhyme label="b" type="m" corresp="#s8:l2">Doot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Een deuntie van een klopje <rhyme label="a" type="m">Iong</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dast maalt my inde <rhyme label="b" type="m">kruyn</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Die ‘t kapjen op het tuyntjen <rhyme label="a" type="m" corresp="#s9:l1">hong</rhyme></l>
        <l id="s9:l4" met="3" type="-+">En doolend’ song in <rhyme label="b" type="m" corresp="#s9:l2">duyn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Wy dennen dikwils op het <rhyme label="a" type="m">bedt</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Maar ‘t mysie maakt noyt mysie <rhyme label="b" type="m">vet</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Ik beleg ‘t my als sy syd <rhyme label="a" type="m" corresp="#s10:l1">bonsjour</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En niet en knie-knikt tot de <rhyme label="b" type="m" corresp="#s10:l2">vloer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
