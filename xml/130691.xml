  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Te Hellevoetsluis daer staet een <rhyme label="a" type="m">huis</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Het is geen huis met <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Huis met <rhyme label="b" type="f" corresp="#s1:l2">eeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Daer leggen de Meisjes de vensters <rhyme label="a" type="m" corresp="#s1:l1">uit</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer de Soldaten <rhyme label="b" type="f" corresp="#s1:l2">verkeeren</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">La, la lette ziante la <rhyme label="c" type="f">coure</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">La, la lette zian belle <rhyme label="d" type="m">mie</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s1:l7">couragie</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s1:l7">couragie</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Moeder al tegen de dogter <rhyme label="a" type="m">sprak</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">‘Gy moet wat fraeitjes <rhyme label="b" type="f">praten</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Fraeitjes <rhyme label="b" type="f" corresp="#s2:l2">praten</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En gaan dan in uw deurtjen <rhyme label="a" type="m" corresp="#s2:l1">staen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Verwagten de <rhyme label="b" type="f" corresp="#s2:l2">Soldaten</rhyme>.’</l>
        <l id="s2:l6" met="4" type="+-">La, la lette ziante la <rhyme label="c" type="f">coure</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">La, la lette zian belle <rhyme label="d" type="m">mie</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s2:l7">couragie</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s2:l7">couragie</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Vader al tegen de Moeder <rhyme label="a" type="m">sprack</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">‘Gy bent geen vrouw met <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Vrouw met <rhyme label="b" type="f" corresp="#s3:l2">eeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat gy uw jongste <rhyme label="a" type="m" corresp="#s3:l1">dogtertje</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Gaet sulke dingen <rhyme label="b" type="f" corresp="#s3:l2">leeren</rhyme>.’</l>
        <l id="s3:l6" met="4" type="+-">La, la lette ziante la <rhyme label="c" type="f">coure</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">La, la lette zian belle <rhyme label="d" type="m">mie</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s3:l7">couragie</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s3:l7">couragie</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Moeder al tegen de Vader <rhyme label="a" type="m">sprak</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">‘Zy is ’er niet aen <rhyme label="b" type="f">bedrogen</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Aen <rhyme label="b" type="f" corresp="#s4:l2">bedrogen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Krijgt zy een steek in haer <rhyme label="a" type="m" corresp="#s4:l1">kombuis</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">’t Is ver van haer bruin <rhyme label="b" type="f" corresp="#s4:l2">oogen</rhyme>.’</l>
        <l id="s4:l6" met="4" type="+-">La, la lette ziante la <rhyme label="c" type="f">coure</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">La, la lette zian belle <rhyme label="d" type="m">mie</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s4:l7">couragie</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s4:l7">couragie</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dan heeft zy nog een rokje <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat is van Rood <rhyme label="b" type="f">Scharlaken</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Rood <rhyme label="b" type="f" corresp="#s5:l2">Scharlaken</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Van vooren wat kort, van agter wat <rhyme label="a" type="m" corresp="#s5:l1">lank</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">’t Is makkelyk op te <rhyme label="b" type="f" corresp="#s5:l2">rapen</rhyme>.</l>
        <l id="s5:l6" met="4" type="+-">La, la lette ziante la <rhyme label="c" type="f">coure</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">La, la lette zian belle <rhyme label="d" type="m">mie</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s5:l7">couragie</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s5:l7">couragie</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dan heeft zy nog een Hembje <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat is van <rhyme label="b" type="f">Kamerdoeke</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-"><rhyme label="b" type="f" corresp="#s6:l2">Kamerdoeke</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Daer onder schuild een roosje <rhyme label="a" type="m" corresp="#s6:l1">rood</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Daer de Soldaten na <rhyme label="b" type="f" corresp="#s6:l2">soeken</rhyme>.</l>
        <l id="s6:l6" met="4" type="+-">La, la lette ziante la <rhyme label="c" type="f">coure</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">La, la lette zian belle <rhyme label="d" type="m">mie</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s6:l7">couragie</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s6:l7">couragie</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die ons dit ligt Lied hebben <rhyme label="a" type="m">gedigt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat waren drie <rhyme label="b" type="f">Soldaten</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Drie <rhyme label="b" type="f" corresp="#s7:l2">Soldaten</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Zy hadden geslapen by haer <rhyme label="a" type="m" corresp="#s7:l1">nigt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Zy konden haer niet <rhyme label="b" type="f" corresp="#s7:l2">verlaten</rhyme>.</l>
        <l id="s7:l6" met="4" type="+-">La, la lette ziante la <rhyme label="c" type="f">coure</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">La, la lette zian belle <rhyme label="d" type="m">mie</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s7:l7">couragie</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">Siante la coure <rhyme label="d" type="m" corresp="#s7:l7">couragie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
