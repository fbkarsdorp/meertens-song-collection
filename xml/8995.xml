    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Psalm 130 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh Godt van <rhyme label="a" type="f">HEmelrijcke</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Door Christum dijnen <rhyme label="b" type="m">Soon</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verleent <rhyme label="a" type="f" corresp="#s1:l1">ghenadichlijcke</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dijn heyl'ghen Gheest so <rhyme label="b" type="m" corresp="#s1:l2">schoon</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat ick mach vrolijck <rhyme label="c" type="f">singhen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van dijn Godlijcke <rhyme label="d" type="m">Woort</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat nu Troostlijck doet <rhyme label="c" type="f" corresp="#s1:l5">klinghen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">So claer aen men'ghen <rhyme label="d" type="m" corresp="#s1:l6">Oort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wilt ons niet weder <rhyme label="a" type="f">nemen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van Weghen onser <rhyme label="b" type="m">Sond'</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Help dat wy wel <rhyme label="a" type="f" corresp="#s2:l1">bekennen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wy armen Adams <rhyme label="b" type="m" corresp="#s2:l2">Kindt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat wy door ons <rhyme label="c" type="f">vermoghen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Niet comen tot dijn <rhyme label="d" type="m">Eer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als wy ghelooven <rhyme label="c" type="f" corresp="#s2:l5">Loghen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">End valsche Menschen <rhyme label="d" type="m" corresp="#s2:l6">Leer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dijn Woort sal eewich <rhyme label="a" type="f">blijven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Als ons Schriftuer <rhyme label="b" type="m">vermelt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Niemant mach dat <rhyme label="a" type="f" corresp="#s3:l1">verdrijven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gheen Macht van dees <rhyme label="b" type="m" corresp="#s3:l2">Werelt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hoe wel hem dick daer <rhyme label="c" type="f">weder</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Mann'gher heeft setten <rhyme label="d" type="m">doen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Sy ligghen all' daer <rhyme label="c" type="f" corresp="#s3:l5">neder</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Door Christum dijnen <rhyme label="d" type="m" corresp="#s3:l6">Soon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Oordeelt niet in dijn <rhyme label="a" type="f">Thooren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghy Vaderlijcke <rhyme label="b" type="m">Hert</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Door dijn Soon <rhyme label="a" type="f" corresp="#s4:l1">wtvercoren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ghedenck hoe hy met <rhyme label="b" type="m" corresp="#s4:l2">smert</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Willich aent Cruys is <rhyme label="c" type="f">storven</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van weghen onser <rhyme label="d" type="m">Sond'</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Daer door dijn Huld' <rhyme label="c" type="f" corresp="#s4:l5">verworven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dien, die gheloovich <rhyme label="d" type="m" corresp="#s4:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Eenighe Godt end <rhyme label="a" type="f">HEEre</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dryvuldich in <rhyme label="b" type="m">Persoon</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wilt ons Gheloove <rhyme label="a" type="f" corresp="#s5:l1">meeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat wy voor dy <rhyme label="b" type="m" corresp="#s5:l2">bestaen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Want so ghy wilt <rhyme label="c" type="f">Aenschouwen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Onse <rhyme label="d" type="m">Gherechticheyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">So mach dy daer voor <rhyme label="c" type="f" corresp="#s5:l5">grouwen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Nae dijnes Woorts <rhyme label="d" type="m" corresp="#s5:l6">bescheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ach Vader denck der <rhyme label="a" type="f">Woorde</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die wy beschreven <rhyme label="b" type="m">haen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Johans aen derden <rhyme label="a" type="f" corresp="#s6:l1">Oorde</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Hoe ghy ons dijnen <rhyme label="b" type="m" corresp="#s6:l2">Soon</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Wt louter Lieffd' hebt <rhyme label="c" type="f">gheven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Wie dat ghelooven <rhyme label="d" type="m">doet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die heeft dat Eewich <rhyme label="c" type="f" corresp="#s6:l5">Leven</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Ontgaet der Hellen <rhyme label="d" type="m" corresp="#s6:l6">gloet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Sulcx wil ick oock <rhyme label="a" type="f">ghelooven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">End gheenen Twijffel <rhyme label="b" type="m">haen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Niemant sal my <rhyme label="a" type="f" corresp="#s7:l1">bedooven</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Met eenich Menschen <rhyme label="b" type="m" corresp="#s7:l2">Waen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Op eenich Werck te <rhyme label="c" type="f">bouwen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Hoe schoon het mach <rhyme label="d" type="m">ghezijn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Voor Menschen aen te <rhyme label="c" type="f" corresp="#s7:l5">schouwen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Als dijn Woort leert so <rhyme label="d" type="m" corresp="#s7:l6">fijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Recht Christen haen gheen <rhyme label="a" type="f">Twijffel</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Sy sullen wel <rhyme label="b" type="m">bestaen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Teghen Sond', Doot end <rhyme label="a" type="f" corresp="#s8:l1">Duyvel</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die haer niet schaden <rhyme label="b" type="m" corresp="#s8:l2">kan</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Dat moet men gaer wel <rhyme label="c" type="f">mercken</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In deser booser <rhyme label="d" type="m">Tijdt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Die Herten daer op <rhyme label="c" type="f" corresp="#s8:l5">stercken</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">So syn wy wel <rhyme label="d" type="m" corresp="#s8:l6">bereydt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maer wie niet so <rhyme label="a" type="f">gheloovet</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Die selffd' is al <rhyme label="b" type="m">Ghericht</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Der Salicheyt <rhyme label="a" type="f" corresp="#s9:l1">beroovet</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Syn Wercke helpen hem <rhyme label="b" type="m" corresp="#s9:l2">nicht</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">T'gelooff brengt wt van <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Goed' Wercken metter <rhyme label="d" type="m">Daet</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Om ander meer te <rhyme label="c" type="f" corresp="#s9:l5">winnen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Den Naesten all' ter <rhyme label="d" type="m" corresp="#s9:l6">baet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Bewaert <rhyme label="a" type="f">ghenadichlijcken</rhyme></l>
        <l id="s10:l2" met="3" type="-+">HEER dijn Volck hier op <rhyme label="b" type="m">Aerd'</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Int Cruys niet af te <rhyme label="a" type="f" corresp="#s10:l1">wijcken</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Daer door ghy ons <rhyme label="b" type="m" corresp="#s10:l2">Probeert</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Lichtlijck ist Woort te <rhyme label="c" type="f">voeren</rhyme></l>
        <l id="s10:l6" met="3" type="-+">In veeler Menschen <rhyme label="d" type="m">Mondt</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Maer gaet die Vrucht <rhyme label="c" type="f" corresp="#s10:l5">verloren</rhyme></l>
        <l id="s10:l8" met="3" type="-+">So ist een valschen <rhyme label="d" type="m" corresp="#s10:l6">grondt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
