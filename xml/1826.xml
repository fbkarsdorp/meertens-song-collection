        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LVstigh Hymen als een <rhyme label="a" type="m">Man</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Dapper <rhyme label="a" type="m" corresp="#s1:l1">an</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Slae tapijten aen de <rhyme label="b" type="f">vvanden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Ruym de schoone <rhyme label="c" type="m">Bruylofts-sael</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s1:l4">T’eenemael</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Laet dijn fackels luchtigh <rhyme label="b" type="f" corresp="#s1:l3">branden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">VVant het puyckjen van ons <rhyme label="a" type="m">Ieughd</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Dat in <rhyme label="a" type="m" corresp="#s2:l1">deughd</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Als de Sonne plagh te <rhyme label="b" type="f">blincken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">VVil haer maeghdelijcke <rhyme label="c" type="m">bloem</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Aen de <rhyme label="c" type="m" corresp="#s2:l4">roem</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van de Iongh-mans, nu <rhyme label="b" type="f" corresp="#s2:l3">verschincken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Nodight op dees <rhyme label="a" type="m">Bruylofts-feest</rhyme></l>
        <l id="s3:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s3:l1">Aldermeest</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die na Venus rechten <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">VVant sy heeft aen haren <rhyme label="c" type="m">soon</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Voor sijn <rhyme label="c" type="m" corresp="#s3:l4">loon</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Nieuvve vvetten <rhyme label="b" type="f" corresp="#s3:l3">voorgheschreven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kroont ons Bruydegom en <rhyme label="a" type="m">Bruydt</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Vlecht van <rhyme label="a" type="m" corresp="#s4:l1">kruydt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Mirten kranskens en <rhyme label="b" type="f">Lauvvrieren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Lovers stroyt en <rhyme label="c" type="m">Maeghde-palm</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Laet de <rhyme label="c" type="m" corresp="#s4:l4">galm</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Vreughdigh uyt u keelkens <rhyme label="b" type="f" corresp="#s4:l3">svvieren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Roept Lvcina by haer <rhyme label="a" type="m">naem</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Tot de <rhyme label="a" type="m" corresp="#s5:l1">kraem</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En om’t pluymich bed te <rhyme label="b" type="f">spreyen</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Snellijck vliegh na <rhyme label="c" type="m">Cynthia</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Dat sy <rhyme label="c" type="m" corresp="#s5:l4">drae</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Thien mael haren krinck vvil <rhyme label="b" type="f" corresp="#s5:l3">drayen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Houdt des Eendrachts trouvven <rhyme label="a" type="m">bandt</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Als een <rhyme label="a" type="m" corresp="#s6:l1">pandt</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Vast gheschakelt in u <rhyme label="b" type="f">herten</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Bruyckt des Tortel-duyfkens <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s6:l5" met="2" type="+-">In de <rhyme label="c" type="m" corresp="#s6:l4">Min</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Soo verdvvijnen uvve <rhyme label="b" type="f" corresp="#s6:l3">smerten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Drijft het vuyle snoo <rhyme label="a" type="m">ghespuys</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Uyt u <rhyme label="a" type="m" corresp="#s7:l1">huys</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Herberght vvijsheyt in u <rhyme label="b" type="f">deuren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dan sal d’uytgheteerde <rhyme label="c" type="m">Nijdt</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Tot haer <rhyme label="c" type="m" corresp="#s7:l4">spijt</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Haer verschroockte kaken <rhyme label="b" type="f" corresp="#s7:l3">scheuren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Heyligh zy dees <rhyme label="a" type="m">Bruylofts-dagh</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Daer men <rhyme label="a" type="m" corresp="#s8:l1">magh</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Vreughd’ in reyne liefde <rhyme label="b" type="f">pleghen</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">’K vvensch dat ’sHemels <rhyme label="c" type="m">opper-sael</rhyme></l>
        <l id="s8:l5" met="2" type="+-">V <rhyme label="c" type="m" corresp="#s8:l4">bestrael</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Met der Goden milden <rhyme label="b" type="f" corresp="#s8:l3">seghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">’K vvensch voorts dat dees jonghe <rhyme label="a" type="m">schaer</rhyme></l>
        <l id="s9:l2" met="2" type="+-">Over <rhyme label="a" type="m" corresp="#s9:l1">jaer</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Mee mach sitten in dees <rhyme label="b" type="f">vreughde</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Ey omhelst de soete <rhyme label="c" type="m">Min</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">’T is u <rhyme label="c" type="m" corresp="#s9:l4">sin</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Alst gheschidt in eer en <rhyme label="b" type="f" corresp="#s9:l3">deughde</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
