  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn Heer en wil't niet <rhyme label="a" type="f">weeten</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Maar kendt mya anders <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">'tIs u of my <rhyme label="a" type="f" corresp="#s1:l1">vergeeten</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Mits ik my staagh <rhyme label="b" type="m" corresp="#s1:l2">versel</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Met edelen en <rhyme label="c" type="f">Heeren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat synse die u <rhyme label="c" type="f" corresp="#s1:l5">scheeren</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Tot op het naakte <rhyme label="b" type="m" corresp="#s1:l2">vel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wel, hoe sal't met u <rhyme label="a" type="f">weesen</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Met wien ist dat ghy <rhyme label="b" type="m">spreekt</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Ik seg dat ghy voor <rhyme label="a" type="f" corresp="#s2:l1">deesen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Een ambachts-knecht <rhyme label="b" type="m" corresp="#s2:l2">geleekt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ick ben een Edel <rhyme label="c" type="f">Jonker</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ghy zyt een kaalen <rhyme label="c" type="f" corresp="#s2:l5">pronker</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die stam en goedt <rhyme label="b" type="m" corresp="#s2:l2">ontbreekt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wilt ghy myn Adel <rhyme label="a" type="f">buygen</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Door spottelijk <rhyme label="b" type="m">verwyt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O Lieve Joris <rhyme label="a" type="f" corresp="#s3:l1">huygen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Men weet wel wie ghy <rhyme label="b" type="m" corresp="#s3:l2">zyt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Waar toe doch deese <rhyme label="c" type="f">woorden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Waar't dat het jemandt <rhyme label="c" type="f" corresp="#s3:l5">hoorden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Myn eere raakte' ik <rhyme label="b" type="m" corresp="#s3:l2">quyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik noem u by u <rhyme label="a" type="f">naame</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En winder geen doeken <rhyme label="b" type="m">om</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Ik sta ter goeder <rhyme label="a" type="f" corresp="#s4:l1">faame</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">By Al den <rhyme label="b" type="m" corresp="#s4:l2">Aldeldom</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die sulkes niet en <rhyme label="c" type="f">weeten</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">En my heel anders <rhyme label="c" type="f" corresp="#s4:l5">heeten</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ey seg my toch <rhyme label="b" type="m" corresp="#s4:l2">waarom</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Om tot wat staats te <rhyme label="a" type="f">raaken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat isser d'oorsaak <rhyme label="b" type="m">af</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Of jet tot niet te <rhyme label="a" type="f" corresp="#s5:l1">maaken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En weelde tot een <rhyme label="b" type="m" corresp="#s5:l2">straf</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ik hoop 'tsal berer <rhyme label="c" type="f">lucken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">De hoop gaat vaak op <rhyme label="c" type="f" corresp="#s5:l5">ktucken</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ja dikwyls tot int <rhyme label="b" type="m" corresp="#s5:l2">Graf</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hoe komt dat ghy soo <rhyme label="a" type="f">spott'lljk</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En schampelijke <rhyme label="b" type="m">kalt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Hoe komt dat ghy soo <rhyme label="a" type="f" corresp="#s6:l1">sott'lljk</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">U kleyne goedt <rhyme label="b" type="m" corresp="#s6:l2">vermaldt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Om winst, is dat <rhyme label="c" type="f">vermallen</rhyme>?</l>
        <l id="s6:l6" met="3" type="-+">Misiechien sult ghy <rhyme label="c" type="f" corresp="#s6:l5">vervallen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Eer jets voor u <rhyme label="b" type="m" corresp="#s6:l2">vervalt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ik leeve' op vaster <rhyme label="a" type="f">hoopen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Daar heb ik't <rhyme label="b" type="m">opgestelt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Maar Ampten moetmen <rhyme label="a" type="f" corresp="#s7:l1">koopen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Wat raadt om't lieve <rhyme label="b" type="m" corresp="#s7:l2">geldt</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">De groote willen't <rhyme label="c" type="f">dryven</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Niet als door Goude <rhyme label="c" type="f" corresp="#s7:l5">schyven</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">'tSal kosten niet een <rhyme label="b" type="m" corresp="#s7:l2">speldt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dat sult ghy wel <rhyme label="a" type="f">bevinden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Indien het comt te <rhyme label="b" type="m">slaagh</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ik hebb' heel goede <rhyme label="a" type="f" corresp="#s8:l1">vrinden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">An't Hof in 'sGraaven <rhyme label="b" type="m" corresp="#s8:l2">Haagh</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En of dan dese <rhyme label="c" type="f">Stylen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Eens storven <rhyme label="c" type="f" corresp="#s8:l5">onderwylen</rhyme>?</l>
        <l id="s8:l7" met="3" type="-+">Godt hoede sulken <rhyme label="b" type="m" corresp="#s8:l2">plaagh</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Legh of u <rhyme label="a" type="f">Paauwe-veeren</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Schud' uyt den <rhyme label="b" type="m">Edelman</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ken wil my niet <rhyme label="a" type="f" corresp="#s9:l1">verneeren</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Soo langh ik ummer <rhyme label="b" type="m" corresp="#s9:l2">kan</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Den hoogh-moedt sal u <rhyme label="c" type="f">krenken</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Het aensien doet <rhyme label="c" type="f" corresp="#s9:l5">gedenken</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">En't kleedt dat maakt de <rhyme label="b" type="m" corresp="#s9:l2">Man</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Denk wat u ouders <rhyme label="a" type="f">waaren</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">En eens wie dat ghy <rhyme label="b" type="m">zyt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Die tyt is lang <rhyme label="a" type="f" corresp="#s10:l1">vervaaren</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">'tIs nu een ander <rhyme label="b" type="m" corresp="#s10:l2">tyr</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Ik soek een ander <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Ik soek wat: seyde <rhyme label="c" type="f" corresp="#s10:l5">Steven</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Doe lagh hy inde <rhyme label="b" type="m" corresp="#s10:l2">byt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De soekers sullen <rhyme label="a" type="f">vinden</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Gelijk de schrift <rhyme label="b" type="m">bediedt</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Ghy praar gelijk den <rhyme label="a" type="f" corresp="#s11:l1">blinden</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Die Peirlen socht in't <rhyme label="b" type="m" corresp="#s11:l2">riet</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Myn hoop is sonder <rhyme label="c" type="f">vreesen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">U Lot sal, ducht ik, <rhyme label="c" type="f" corresp="#s11:l5">weesen</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Een op-gepronkte <rhyme label="b" type="m" corresp="#s11:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ik soek een gulde <rhyme label="a" type="f">waagen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ten minsten vinde ik <rhyme label="b" type="m">buyt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Het valt somwyl op <rhyme label="a" type="f" corresp="#s12:l1">slaagen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Ten lange lesten <rhyme label="b" type="m" corresp="#s12:l2">uyt</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Wilt ghy te hooghe <rhyme label="c" type="f">stuiren</rhyme>?</l>
        <l id="s12:l6" met="3" type="-+">U staat en sal niet <rhyme label="c" type="f" corresp="#s12:l5">duiren</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Gelijk een Elants <rhyme label="b" type="m" corresp="#s12:l2">huyt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ik ga, want al het <rhyme label="a" type="f">praaten</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Besluyt ghy met een <rhyme label="b" type="m">gek</rhyme>.</l>
        <l id="s13:l3" met="3" type="-+">Gaat liever by de <rhyme label="a" type="f" corresp="#s13:l1">Staeten</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Daar isser een <rhyme label="b" type="m" corresp="#s13:l2">ghebrek</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Tot dat u goedt te <rhyme label="c" type="f">loor-is</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Dan salmen u, o <rhyme label="c" type="f" corresp="#s13:l5">Joris</rhyme>.</l>
        <l id="s13:l7" met="3" type="-+">Anschouwen met de <rhyme label="b" type="m" corresp="#s13:l2">Nek</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">En sult noch tot u <rhyme label="a" type="f">schade</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Van jeder syn <rhyme label="b" type="m">veracht</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Dies gaat, als ik u <rhyme label="a" type="f" corresp="#s14:l1">rade</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">In Burgerlijke <rhyme label="b" type="m" corresp="#s14:l2">draght</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Verlaat dit hooghe <rhyme label="c" type="f">vlieghen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Of sult u selfs <rhyme label="c" type="f" corresp="#s14:l5">bedriegen</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">En werden uy <rhyme label="b" type="m" corresp="#s14:l2">t-gelacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
