    </tags>
    <melody>Psalm 006 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Nae ‘t Avontmael <rhyme label="a" type="f">devijne</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Christus midden de <rhyme label="a" type="f" corresp="#s1:l1">sijne</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Staend’, als ootmoedigh <rhyme label="b" type="m">Knecht</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Sprack met vriendelijck <rhyme label="c" type="f">aenschouwen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ick ben wilt dit <rhyme label="c" type="f" corresp="#s1:l4">betrouwen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Eenen Wijn-Stock <rhyme label="b" type="m" corresp="#s1:l3">oprecht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Tot des Ranckskens <rhyme label="a" type="f">ghenesen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Is mijn Vader <rhyme label="a" type="f" corresp="#s2:l1">ghepresen</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Den bouwer wijs, en <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Alle Ranck in my <rhyme label="c" type="f">gheene</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Vrucht draghend’ in ‘t <rhyme label="c" type="f" corresp="#s2:l4">ghemeene</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Sal hy af-snijden <rhyme label="b" type="m" corresp="#s2:l3">vroet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maer alle rancke <rhyme label="a" type="f">weynich</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vrucht draghende, hy <rhyme label="a" type="f" corresp="#s3:l1">reynich</rhyme></l>
        <l id="s3:l3" met="3" type="-+">En suyver maken <rhyme label="b" type="m">sal</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Op dat sy hem <rhyme label="c" type="f">behaghen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">End’ voorts noch vruchten <rhyme label="c" type="f" corresp="#s3:l4">draghen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al veel meer in ‘t <rhyme label="b" type="m" corresp="#s3:l3">ghetal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy zijt nu <rhyme label="a" type="f">altesamen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gheworden reyn met <rhyme label="a" type="f" corresp="#s4:l1">namen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Om de wille van ‘t <rhyme label="b" type="m">woort</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat ick u heb <rhyme label="c" type="f">ghesproken</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">End’ dat ghy <rhyme label="c" type="f" corresp="#s4:l4">onghebroken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Gheloovich hebt <rhyme label="b" type="m" corresp="#s4:l3">ghehoort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Blijft in my en ick <rhyme label="a" type="f">mede</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">In u want so ter <rhyme label="a" type="f" corresp="#s5:l1">stede</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gheen Wijn-Rancke, ten <rhyme label="b" type="m">zy</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sy aen den Wijn-Stock <rhyme label="c" type="f">blijve</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Gheen vrucht, brenght ten <rhyme label="c" type="f" corresp="#s5:l4">beklijve</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van selfs, ghelijcks oock <rhyme label="b" type="m" corresp="#s5:l3">ghy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ten zy dat ghy blijft <rhyme label="a" type="f">stadich</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In my, ghy kondt <rhyme label="a" type="f" corresp="#s6:l1">weldasdich</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Gheen vrucht ghebrenghen, <rhyme label="b" type="m">want</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ick den Wijn-Stock <rhyme label="c" type="f">verkoren</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ben, ghy uyt my <rhyme label="c" type="f" corresp="#s6:l4">gheboren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">De Trancxkens wel <rhyme label="b" type="m" corresp="#s6:l3">gheplant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die in my blijft <rhyme label="a" type="f">eendrachtich</rhyme></l>
        <l id="s7:l2" met="3" type="-+">En ick in hem <rhyme label="a" type="f" corresp="#s7:l1">waerachtich</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Die brenght veel vruchten, <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Want als u is <rhyme label="c" type="f">ontoghen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Mijn hulp, al u <rhyme label="c" type="f" corresp="#s7:l4">vermoghen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Om yet te doen is <rhyme label="b" type="m" corresp="#s7:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wie niet in my <rhyme label="a" type="f">volheerdigh</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En blijft, al mijns <rhyme label="a" type="f" corresp="#s8:l1">onweerdigh</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Hy wegh gheworpen <rhyme label="b" type="m">wert</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ghelijck de <rhyme label="c" type="f">Wijngaert-Rancke</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Die vergaend’ als de <rhyme label="c" type="f" corresp="#s8:l4">krancke</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Af-ghescheyden <rhyme label="b" type="m" corresp="#s8:l3">verdort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De sulcke men <rhyme label="a" type="f">vergaren</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dan gaet, en sonder <rhyme label="a" type="f" corresp="#s9:l1">sparen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Te samen werpt in ‘t <rhyme label="b" type="m">vyer</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Daer sy moeten <rhyme label="c" type="f">verbranden</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Met pijne swaer vol <rhyme label="c" type="f" corresp="#s9:l4">schanden</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Gheschrey, en droef <rhyme label="b" type="m" corresp="#s9:l3">ghetier</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Is ‘t dat ghy vast <rhyme label="a" type="f">ghebonden</rhyme></l>
        <l id="s10:l2" met="3" type="-+">In my blijft t’allen <rhyme label="a" type="f" corresp="#s10:l1">stonden</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Mijn woorden neffens <rhyme label="b" type="m">dien</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Blijven u die in ‘t <rhyme label="c" type="f">midden</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Der herten, wat ghy <rhyme label="c" type="f" corresp="#s10:l4">bidden</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Wilt, sal u dan <rhyme label="b" type="m" corresp="#s10:l3">gheschien</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Daer in wort mijnen <rhyme label="a" type="f">Vader</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Ghe-eert, dat ghy te <rhyme label="a" type="f" corresp="#s11:l1">gader</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Vrucht brenght in <rhyme label="b" type="m">overvloet</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En dat ghy soo by <rhyme label="c" type="f">desen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Mijn jongheren gaet <rhyme label="c" type="f" corresp="#s11:l4">wesen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">In mijn gheboden <rhyme label="b" type="m" corresp="#s11:l3">soet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ghelijck mijn Vader <rhyme label="a" type="f">blijde</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">My lief heeft t’allen <rhyme label="a" type="f" corresp="#s12:l1">tijde</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Soo groote liefde <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Draegh ick u <rhyme label="c" type="f">desghelijcken</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Och blijft doch sonder <rhyme label="c" type="f" corresp="#s12:l4">wijcken</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">In mijnder liefd’ <rhyme label="b" type="m" corresp="#s12:l3">eenpaer</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Is ‘t dat ghy mijn <rhyme label="a" type="f">gheboden</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Houdt, ghy blijft <rhyme label="a" type="f" corresp="#s13:l1">onghevloden</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Dan in mijn liefde <rhyme label="b" type="m">vast</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Ghelijck ick in ‘t <rhyme label="c" type="f">volbringhen</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Oock houde alle <rhyme label="c" type="f" corresp="#s13:l4">dinghen</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Wat mijn Vader <rhyme label="b" type="m" corresp="#s13:l3">belast</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">En ghelijck ick <rhyme label="a" type="f">ootmoedigh</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Blijf in sijn liefde <rhyme label="a" type="f" corresp="#s14:l1">gloedigh</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Dit’s u van my <rhyme label="b" type="m">gheseyt</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Op dat mijn blijtschap <rhyme label="c" type="f">blijve</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">In u, en dat <rhyme label="c" type="f" corresp="#s14:l4">beklijve</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">U blijtschap seer <rhyme label="b" type="m" corresp="#s14:l3">planteyt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Dat u blijtschap <rhyme label="a" type="f">volkomen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Zy, dat is t’eender <rhyme label="a" type="f" corresp="#s15:l1">sommen</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Siet mijn ghebodt als <rhyme label="b" type="m">nu</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Ghy sult met reyne <rhyme label="c" type="f">sinnen</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Malkanderen <rhyme label="c" type="f" corresp="#s15:l4">beminnen</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Ghelijck ick dede <rhyme label="b" type="m" corresp="#s15:l3">u</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Siet Broeders, dit’s de <rhyme label="a" type="f">mate</rhyme></l>
        <l id="s16:l2" met="3" type="-+">En hebben vroegh en <rhyme label="a" type="f" corresp="#s16:l1">late</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">Hier ghenoegh aen te <rhyme label="b" type="m">doen</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Te rechte wy ons <rhyme label="c" type="f">sullen</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Voeghen, om te <rhyme label="c" type="f" corresp="#s16:l4">vervullen</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Als Christen Rancken <rhyme label="b" type="m" corresp="#s16:l3">groen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
