<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25110</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="Marian song">Marialied</tag>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Die dageraet geet op ende <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Daer wt soe compt des hemels <rhyme label="a" type="m" corresp="#s1:l1">scijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat is der maget <rhyme label="b" type="f">sone</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Hy woude hier neder comen <rhyme label="c" type="m">tons</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En laten sinen <rhyme label="b" type="f" corresp="#s1:l3">troone</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die ouer scone bruut ende <rhyme label="a" type="m">maegt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Sy heeft den coninc best <rhyme label="a" type="m" corresp="#s2:l1">behaecht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Daer om heeft sy sijn <rhyme label="b" type="f">minne</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Sy es gemaect sijn <rhyme label="c" type="m">aduocaet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Der ingelen <rhyme label="b" type="f" corresp="#s2:l3">coninginne</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die coninc sach die scone <rhyme label="a" type="m">sale</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Sy beuiel hem alsoe <rhyme label="a" type="m" corresp="#s3:l1">wale</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat hi er in wonen <rhyme label="b" type="f">quam</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Bouen alle <rhyme label="c" type="m">creaturen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Genuechte hy in haer <rhyme label="b" type="f" corresp="#s3:l3">nam</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy brachte dat suete cleine <rhyme label="a" type="m">kindekijn</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Des vaders sone een eeuwich <rhyme label="a" type="m" corresp="#s4:l1">scijn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Sy es soe wel <rhyme label="b" type="f">geraect</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Al woude hy suegen in haren <rhyme label="c" type="m">scoot</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hy heeftse selue <rhyme label="b" type="f" corresp="#s4:l3">gemaect</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy sijt dat hemelsce <rhyme label="a" type="m">paradijs</rhyme></l>
        <l id="s5:l2" met="4" type="-+">In v soe wies dat godlijc <rhyme label="a" type="m" corresp="#s5:l1">rijs</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En quam in deser <rhyme label="b" type="f">tijt</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Die in dat paradijs mocht <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Hy waer te mael <rhyme label="b" type="f" corresp="#s5:l3">verblijt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O sterre der zee ghy sijt soe <rhyme label="a" type="m">scoone</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Ghy verblijt daer al den <rhyme label="a" type="m" corresp="#s6:l1">troone</rhyme></l>
        <l id="s6:l3" met="3" type="-+">En die te water <rhyme label="b" type="f">sijn</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Sy hebbe hope en troost <rhyme label="c" type="m">alleene</rhyme></l>
        <l id="s6:l5" met="3" type="-+">aen uwen scoenen <rhyme label="b" type="f" corresp="#s6:l3">scijn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O scone wingaert rancke ghy sijt soe <rhyme label="a" type="m">fier</rhyme></l>
        <l id="s7:l2" met="4" type="-+">V rueke verdrijft dat helsce <rhyme label="a" type="m" corresp="#s7:l1">dier</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Ende houtten in uwen <rhyme label="b" type="f">bant</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Edel vrouwe ghy mochten <rhyme label="c" type="m">dwingen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Want hy ontsiet v <rhyme label="b" type="f" corresp="#s7:l3">hant</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy sijt die roose in <rhyme label="a" type="m">iherico</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Ghy maket hemel ende eerde <rhyme label="a" type="m" corresp="#s8:l1">vro</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Met uwer <rhyme label="b" type="f">sueticheit</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Hulpt ons vrouwe na deser <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">In de eeuwige <rhyme label="b" type="f" corresp="#s8:l3">salicheit</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
