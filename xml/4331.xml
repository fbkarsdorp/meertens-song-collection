<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4331</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Als Phoebus vertoog</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">GAngs Dibberig <rhyme label="a" type="m">buur</rhyme>!</l>
        <l id="s1:l2" met="2" type="-+">Hoe zieje zo <rhyme label="a" type="m" corresp="#s1:l1">zuur</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Of hebje de pagt van <rhyme label="b" type="m">d'eek</rhyme>?</l>
        <l id="s1:l4" met="2" type="-+">VVat roert het jouw <rhyme label="c" type="m">schongk</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Voort pakje van <rhyme label="c" type="m" corresp="#s1:l4">hongk</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ier ikje de lendene <rhyme label="b" type="m" corresp="#s1:l3">breek</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Onnodig <rhyme label="d" type="m">beschik</rhyme>!</l>
        <l id="s1:l8" met="2" type="-+">Sou jy me zo <rhyme label="d" type="m" corresp="#s1:l7">strik</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Men lendene an stikke <rhyme label="e" type="m">slaen</rhyme>;</l>
        <l id="s1:l10" met="7" type="-+">Dat was, dat was, dat was van jouw, hiel bijster quaelik <rhyme label="e" type="m" corresp="#s1:l9">edaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">En alsje m' al <rhyme label="a" type="m">had'</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Men lieve <rhyme label="a" type="m" corresp="#s2:l1">Ian-gat</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">VVat wouwje me liere? <rhyme label="b" type="m">zeg</rhyme>!</l>
        <l id="s2:l4" met="2" type="-+">Hoe ik het mit <rhyme label="c" type="m">jouw</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Kalvateren <rhyme label="c" type="m" corresp="#s2:l4">zouw</rhyme>?</l>
        <l id="s2:l6" met="3" type="-+">Ie zoenen, en leggen je <rhyme label="b" type="m" corresp="#s2:l3">weg</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Men lieve <rhyme label="d" type="m">kappoen</rhyme>!</l>
        <l id="s2:l8" met="2" type="-+">Hoe word'je zo <rhyme label="d" type="m" corresp="#s2:l7">groen</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Zoen Beertje-buur onger 'er <rhyme label="e" type="m">staert</rhyme>:</l>
        <l id="s2:l10" met="7" type="-+">Zo vangje ligtelik, (dient het luk) een warreme <rhyme label="e" type="m" corresp="#s2:l9">vijge-taert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">VVel dat waer te <rhyme label="a" type="m">bongt</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Een &amp;c. in men <rhyme label="a" type="m" corresp="#s3:l1">mongt</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Hoe! binje zoo <rhyme label="b" type="m">lekker</rhyme>?</l>
        <l id="s3:l4" met="2" type="-+">Ouwij! 'k Houw mier van <rhyme label="c" type="m">kandiel</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Mit suiker in k' <rhyme label="c" type="m" corresp="#s3:l4">niel</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Ia karrene-zoep in je <rhyme label="b" type="m" corresp="#s3:l3">pij</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">So word'je niet <rhyme label="d" type="m">dol</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Mijn drooge <rhyme label="d" type="m" corresp="#s3:l7">Iaap-hol</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Een kus. VVeg murrewe <rhyme label="e" type="m">gek</rhyme>!</l>
        <l id="s3:l10" met="7" type="-+">Krui heen ier ik je 't spinne-wiel rij tussen hals en <rhyme label="e" type="m" corresp="#s3:l9">nek</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
