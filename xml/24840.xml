    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Dach na dach in lanc so <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dinct mi haer minne <rhyme label="b" type="f">wassen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Met trauwen vast in minen <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Nacht na nacht vernieut mijn <rhyme label="a" type="m" corresp="#s1:l1">zeer</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Want niders up mi <rhyme label="b" type="f" corresp="#s1:l2">bassen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Mijn alre liefste vrauwe <rhyme label="a" type="m">leer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dijn herte in trauwen <rhyme label="b" type="f">passen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Daer voren coric heil no <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Dach na dach in lanc so <rhyme label="a" type="m" corresp="#s2:l1">meer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dinct mi haer minne <rhyme label="b" type="f" corresp="#s2:l2">wassen</rhyme></l>
      </lg>
    </lg>
