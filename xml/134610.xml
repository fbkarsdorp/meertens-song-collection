<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>134610</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>alkmaar</orginal-name>
      <standard-name>Alkmaar, The Netherlands</standard-name>
      <coordinates>
        <lon>52.6312592</lon>
        <lat>4.7503798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>wedding song</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Christus had uyt reynder <rhyme label="a" type="m">min</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Sijn <rhyme label="a" type="m" corresp="#s1:l1">vriendin</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Duysent duysent Jaer te <rhyme label="b" type="f">voren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Eer in deser orden <rhyme label="c" type="m">stond</rhyme>’</l>
        <l id="s1:l5" met="2" type="+-">‘s Wereldts <rhyme label="c" type="m" corresp="#s1:l4">rond</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Tot sijn eygen lief <rhyme label="b" type="f" corresp="#s1:l3">verkoren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Daer na kreegh haer d’helsche <rhyme label="a" type="m">slang</rhyme></l>
        <l id="s2:l2" met="2" type="+-">In sijn <rhyme label="a" type="m" corresp="#s2:l1">dwangh</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">‘t Welck haer Minnaer Christus <rhyme label="b" type="f">siende</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Quam gedaelt uyt ‘s Hemels <rhyme label="c" type="m">eer</rhyme></l>
        <l id="s2:l5" met="2" type="+-">T’Onswaerts <rhyme label="c" type="m" corresp="#s2:l4">neer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Deed ‘t gheen tot verlossingh <rhyme label="b" type="f" corresp="#s2:l3">diende</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Heeft ghekocht haer met sijn <rhyme label="a" type="m">Bloedt</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">‘t Beste <rhyme label="a" type="m" corresp="#s3:l1">goedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Datmen heeft, heeft hy <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Heeft ghebracht haer uyt haer <rhyme label="c" type="m">noodt</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Uit haer <rhyme label="c" type="m" corresp="#s3:l4">doodt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Door sijn noot, sijn doodt ten <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Heeft met schoone <rhyme label="a" type="m">cierlijckheyt</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Haer <rhyme label="a" type="m" corresp="#s4:l1">bereydt</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Opgepronckt tot sijner <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Haer gewaed en haer <rhyme label="c" type="m">gesmijd</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Is van <rhyme label="c" type="m" corresp="#s4:l4">zijd</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Maer ‘t zijn geestelijcke <rhyme label="b" type="f" corresp="#s4:l3">kleeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Heeft verthoont sich selfs met <rhyme label="a" type="m">haer</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Om <rhyme label="a" type="m" corresp="#s5:l1">eenpaer</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Beyde t’saem in d’Echt te <rhyme label="b" type="f">blijven</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Desen Trouringh is <rhyme label="c" type="m">geweest</rhyme></l>
        <l id="s5:l5" met="2" type="+-">d'Heyl’gen <rhyme label="c" type="m" corresp="#s5:l4">Geest</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Die den Trouw in ‘t hert soud’ <rhyme label="b" type="f" corresp="#s5:l3">schrijven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Voorts so sal in ‘s Hemels <rhyme label="a" type="m">zael</rhyme></l>
        <l id="s6:l2" met="2" type="+-">‘t Bruylofts <rhyme label="a" type="m" corresp="#s6:l1">mael</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Werden feestelijck <rhyme label="b" type="f">gehouden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">O wel saligh Echte <rhyme label="c" type="m">paer</rhyme></l>
        <l id="s6:l5" met="2" type="+-">Noyt te <rhyme label="c" type="m" corresp="#s6:l4">gaer</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Meer soodanigh paren <rhyme label="b" type="f" corresp="#s6:l3">trouden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
