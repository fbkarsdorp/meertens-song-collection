  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ade ich m s mich <rhyme label="a" type="f">scheidenn</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">aus tra rentlichem m <rhyme label="b" type="m">tt</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">So en geschach mir nehe so <rhyme label="a" type="f" corresp="#s1:l1">leide</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Jch hoff es soll noch werdenn g <rhyme label="b" type="m" corresp="#s1:l2">itt</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Zu diesem newen <rhyme label="c" type="m">Jaire</rhyme></l>
        <l id="s1:l6" met="3" type="-+">erfrew dich megdlein der st <rhyme label="d" type="m">ndtt</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">Sei hatt ein gelb kra s <rhyme label="c" type="m" corresp="#s1:l5">hare</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">Zwei falckenn a gen <rhyme label="c" type="m" corresp="#s1:l5">clare</rhyme> /</l>
        <l id="s1:l9" met="3" type="-+">Deß lachett jr rother m <rhyme label="d" type="m" corresp="#s1:l6">ndt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sie tritt darher gar <rhyme label="a" type="f">schone</rhyme></l>
        <l id="s2:l2" met="3" type="-+">rechtt wie der Pfa wenn <rhyme label="b" type="m">artt</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Von goltt dragtt sei ein <rhyme label="a" type="f" corresp="#s2:l1">krone</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Sei schrifft sich vonn edler <rhyme label="b" type="m" corresp="#s2:l2">artt</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Woltt gott mocht ich ir <rhyme label="c" type="m">dhienenn</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">biß a ff das ende <rhyme label="d" type="m">mein</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Wie kan vnnd mag mir bas <rhyme label="c" type="m" corresp="#s2:l5">gesein</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">wantt sei erfrewett mir das hertze <rhyme label="c" type="m" corresp="#s2:l5">mein</rhyme> /</l>
        <l id="s2:l9" met="3" type="-+">Z  diesem newenn <rhyme label="d" type="m" corresp="#s2:l6">Jair</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Jr leib ist woll <rhyme label="a" type="f">gezierett</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">Sei ist vonn edlem <rhyme label="b" type="m">geschlechtt</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">Ehr ist woll drei oder <rhyme label="a" type="f" corresp="#s3:l1">vierhe</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Jch hoff ich sein der <rhyme label="b" type="m" corresp="#s3:l2">rechtt</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Die liebe die ich <rhyme label="c" type="m">trage</rhyme></l>
        <l id="s3:l6" met="3" type="-+">al d rch denn willen <rhyme label="d" type="m">ir</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Jch hatt einen Bolenn aus g etter <rhyme label="c" type="m" corresp="#s3:l5">wan</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">Sei sagtt ich soltt kein ander <rhyme label="c" type="m" corresp="#s3:l5">han</rhyme> /</l>
        <l id="s3:l9" met="3" type="-+">So lieb  als ich jr <rhyme label="d" type="m" corresp="#s3:l6">bin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">A ff erdenn enwartt nehe kein <rhyme label="a" type="f">geborn</rhyme> /</l>
        <l id="s4:l2" met="3" type="-+">vnd der mir baß <rhyme label="b" type="m">gefeltt</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Ach gott hatt sei mich a <rhyme label="a" type="f" corresp="#s4:l1">ßerkornn</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">Ich hab sei a <rhyme label="b" type="m" corresp="#s4:l2">sserweltt</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Bo en allen schonenn <rhyme label="c" type="m">Frawenn</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">Das solst  gla benn <rhyme label="d" type="m">mir</rhyme> /</l>
        <l id="s4:l7" met="3" type="-+">mein lieblich lieb iß wol <rhyme label="c" type="m" corresp="#s4:l5">gestaltt</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">Jr weiß die ist so <rhyme label="c" type="m" corresp="#s4:l5">mannichfaltt</rhyme> /</l>
        <l id="s4:l9" met="3" type="-+">Wollt gott wehr ich bei <rhyme label="d" type="m" corresp="#s4:l6">jr</rhyme>.</l>
      </lg>
