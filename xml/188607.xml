  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Welkom, braave <rhyme label="a" type="m">Heldenstoet</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Gij die spaarden goed noch <rhyme label="a" type="m" corresp="#s1:l1">bloed</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Om uw Broeders vrij te <rhyme label="b" type="f">maaken</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Gij kwaamt onze kluisters <rhyme label="b" type="f" corresp="#s1:l3">slaaken</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Steeds wordt gij met roem <rhyme label="c" type="m">vermeld</rhyme>!</l>
        <l id="s1:l6" met="2" type="-+">'t Geweld, 't <rhyme label="c" type="m" corresp="#s1:l5">geweld</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Wierd door uwen arm <rhyme label="c" type="m" corresp="#s1:l5">geveld</rhyme>;</l>
        <l id="s1:l8" met="4" type="+-">Het geen de despoten <rhyme label="d" type="f">duchten</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Blijkt uit hun <rhyme label="d" type="f" corresp="#s1:l8">vlugten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Nederlanders! weest nu <rhyme label="a" type="m">blij</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Daar gij zijt verlost en <rhyme label="a" type="m" corresp="#s2:l1">vrij</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Gij zijt thans geheel <rhyme label="b" type="f">ontslagen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van een juk, te lang <rhyme label="b" type="f" corresp="#s2:l3">gedragen</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Pruissens magt benauwd ons <rhyme label="c" type="m">niet</rhyme>;</l>
        <l id="s2:l6" met="2" type="-+">Het liet, het <rhyme label="c" type="m" corresp="#s2:l5">liet</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Nu zijn maagschap in 't <rhyme label="c" type="m" corresp="#s2:l5">verdriet</rhyme>;</l>
        <l id="s2:l8" met="4" type="+-">En het geen die snoden <rhyme label="d" type="f">duchten</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Bleek uit hun <rhyme label="d" type="f" corresp="#s2:l8">vlugten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Engeland weet ook geen <rhyme label="a" type="m">raad</rhyme>;</l>
        <l id="s3:l2" met="4" type="+-">Zijne armée is buiten <rhyme label="a" type="m" corresp="#s3:l1">staat</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">De despoten zijn <rhyme label="b" type="f">geweken</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Franschen! gij hebt, <rhyme label="b" type="f" corresp="#s3:l3">onbezweken</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ons belang steeds <rhyme label="c" type="m">voorgestaan</rhyme>;</l>
        <l id="s3:l6" met="2" type="-+">Uw daân, uw <rhyme label="c" type="m" corresp="#s3:l5">daân</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Bragten ons de Vrijheid <rhyme label="c" type="m" corresp="#s3:l5">aan</rhyme>;</l>
        <l id="s3:l8" met="4" type="+-">Gij verloste deze <rhyme label="d" type="f">Landen</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Uit hunne <rhyme label="d" type="f" corresp="#s3:l8">banden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Vrijheid en gelijkheid <rhyme label="a" type="m">zij</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Steeds de band der <rhyme label="a" type="m" corresp="#s4:l1">maatschappij</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Heil en broederschap <rhyme label="b" type="f">verbinden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Ons aan u, o <rhyme label="b" type="f" corresp="#s4:l3">menschenvrinden</rhyme>!</l>
        <l id="s4:l5" met="4" type="+-">Want door u wierdt, <rhyme label="c" type="m">onverwagt</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">De magt, de <rhyme label="c" type="m" corresp="#s4:l5">magt</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Van de dwing'landij <rhyme label="c" type="m" corresp="#s4:l5">verkragt</rhyme>;</l>
        <l id="s4:l8" met="4" type="+-">Zo zag men de kluisters <rhyme label="d" type="f">breeken</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Monsters <rhyme label="d" type="f" corresp="#s4:l8">verbleeken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dappre Helden, Broedren <rhyme label="a" type="m">rij</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dat uw deugd ons richtsnoer <rhyme label="a" type="m" corresp="#s5:l1">zij</rhyme>!</l>
        <l id="s5:l3" met="4" type="+-">Dwinglandij en Heerschzugt <rhyme label="b" type="f">wijken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Daar gij doet uw vaandels <rhyme label="b" type="f" corresp="#s5:l3">prijken</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Neem ons dan als broedren <rhyme label="c" type="m">aan</rhyme>;</l>
        <l id="s5:l6" met="2" type="-+">Voortaan, <rhyme label="c" type="m" corresp="#s5:l5">voortaan</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Blijft ons hart voor Vrijheid <rhyme label="c" type="m" corresp="#s5:l5">slaan</rhyme>;</l>
        <l id="s5:l8" met="4" type="+-">Eeuwig zullen wij u <rhyme label="d" type="f">danken</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Met blijde <rhyme label="d" type="f" corresp="#s5:l8">klanken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
