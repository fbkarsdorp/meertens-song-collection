  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">Beminnelyke, Lieve <rhyme label="a" type="m">meid</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">ja lieve <rhyme label="b" type="m">meid</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Myn' schoone <rhyme label="a" type="m" corresp="#s1:l1">zielsvriendin'</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ik heb u jaaren lang <rhyme label="b" type="m" corresp="#s1:l2">gevreid</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Schenk my toch <rhyme label="b" type="m" corresp="#s1:l2">wedermin</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Neen, jongman neen, dat doe ik <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">dat, doe ik <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Ik leev' thans vry en <rhyme label="a" type="m" corresp="#s2:l1">bly</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Ik steek my nog in geen <rhyme label="b" type="m" corresp="#s2:l2">verdriet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Van huwelyks <rhyme label="b" type="m" corresp="#s2:l2">slaverny</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Ik heb door hagel sneeuw en <rhyme label="a" type="m">wind</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">ja sneeuw en <rhyme label="b" type="m">wind</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">En 't guur en barste <rhyme label="a" type="m" corresp="#s3:l1">weêr'</rhyme></l>
        <l id="s3:l4" met="3" type="-+">U naargevolgt; Gy zyt <rhyme label="b" type="m" corresp="#s3:l2">bemind</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van my; Ja 'k min u <rhyme label="b" type="m" corresp="#s3:l2">teêr</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">Ja teêr, of niet? ik min u <rhyme label="a" type="m">mee</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">ik min u <rhyme label="b" type="m">mee</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Maar meest, om myn <rhyme label="a" type="m" corresp="#s4:l1">plaisier</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Wil gy uit gaan; ik ben te <rhyme label="b" type="m" corresp="#s4:l2">vree</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Dan gaane wy ginds, dan <rhyme label="b" type="m" corresp="#s4:l2">hier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="-+">Heb ik, toen gy my laastmaal <rhyme label="a" type="m">zei</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">ja laastmaal <rhyme label="b" type="m">zei</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">Laat ons by Toontje <rhyme label="a" type="m" corresp="#s5:l1">gaan</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Gezegd myn lieve Ko! 'k ben <rhyme label="b" type="m" corresp="#s5:l2">bly</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zeg my; wat staat u <rhyme label="b" type="m" corresp="#s5:l2">aan</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="-+">Ja jonge lief, dat deedt gy <rhyme label="a" type="m">wel</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">dat deedt gy <rhyme label="b" type="m">wel</rhyme>!</l>
        <l id="s6:l3" met="4" type="-+">Maar ben ik nu <rhyme label="a" type="m" corresp="#s6:l1">verplicht</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Myn vryheid voor u <rhyme label="b" type="m" corresp="#s6:l2">aftestaan</rhyme>?</l>
        <l id="s6:l5" met="3" type="-+">Dit gaat (zoo waar) niet <rhyme label="b" type="m" corresp="#s6:l2">ligt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="-+">Daar gaf ik u, uw buikje <rhyme label="a" type="m">dik</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">uw buikje <rhyme label="b" type="m">dik</rhyme>!</l>
        <l id="s7:l3" met="4" type="-+">Met Paling mals en <rhyme label="a" type="m" corresp="#s7:l1">goed</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Toen waard gv wonder in uw <rhyme label="b" type="m" corresp="#s7:l2">schik</rhyme></l>
        <l id="s7:l5" met="3" type="-+">En ik was vol van <rhyme label="b" type="m" corresp="#s7:l2">moed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="6" type="-+">Die paling, ach; die smaakt my <rhyme label="a" type="m">noch</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">die smaakt my <rhyme label="b" type="m">noch</rhyme>!</l>
        <l id="s8:l3" met="4" type="-+">Wat was ze lekker, <rhyme label="a" type="m" corresp="#s8:l1">mals</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En dik! - O neen! 't is geen <rhyme label="b" type="m" corresp="#s8:l2">bedrog</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ik handelde nooit <rhyme label="b" type="m" corresp="#s8:l2">valsch</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="6" type="-+">Woud gy eens in een speelhuis <rhyme label="a" type="m">gaan</rhyme></l>
        <l id="s9:l2" met="3" type="-+">een speelhuis <rhyme label="b" type="m">gaan</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Of in den O <rhyme label="a" type="m" corresp="#s9:l1">jevaar</rhyme>;</l>
        <l id="s9:l4" met="3" type="-+">Of schouwburg, sprak gy my maar <rhyme label="b" type="m" corresp="#s9:l2">aan</rhyme>!</l>
        <l id="s9:l5" met="3" type="-+">Aanstonds zoo was ik <rhyme label="b" type="m" corresp="#s9:l2">klaar</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="6" type="-+">'t Is waar, neen! dat ontken ik <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">ontken ik <rhyme label="b" type="m">niet</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">Gy moest myn lydsman <rhyme label="a" type="m" corresp="#s10:l1">zyn</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Was ik by u, 'k had geen <rhyme label="b" type="m" corresp="#s10:l2">verdriet</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En gy ook niet by <rhyme label="b" type="m" corresp="#s10:l2">my</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="6" type="-+">Wel waarom dan uw hart en <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">uw hart en <rhyme label="b" type="m">hand</rhyme>!</l>
        <l id="s11:l3" met="4" type="-+">Waar om ik bid en <rhyme label="a" type="m" corresp="#s11:l1">smeek</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">My niet geschonken? Lieve <rhyme label="b" type="m" corresp="#s11:l2">Ant</rhyme>!</l>
        <l id="s11:l5" met="3" type="-+">Schenkt 't my dog deze <rhyme label="b" type="m" corresp="#s11:l2">week</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="6" type="-+">Wat henker 't is al <rhyme label="a" type="m">Saturdag</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">al <rhyme label="b" type="m">Saturdag</rhyme>?</l>
        <l id="s12:l3" met="4" type="-+">Gy spot wat met my <rhyme label="a" type="m" corresp="#s12:l1">koos</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Hoe zugt gy zoo? al <rhyme label="b" type="m" corresp="#s12:l2">Saturdag</rhyme>;</l>
        <l id="s12:l5" met="3" type="-+">Blyft hier, ik ben niet <rhyme label="b" type="m" corresp="#s12:l2">boos</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="6" type="-+">Ik dacht het alreeds maandag <rhyme label="a" type="m">waar</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">reeds maandag <rhyme label="b" type="m">waar</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Ik ben er van <rhyme label="a" type="m" corresp="#s13:l1">ontsteld</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Bedenk, u d'and're week dan <rhyme label="b" type="m" corresp="#s13:l2">maar</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Ik dwing niet met <rhyme label="b" type="m" corresp="#s13:l2">geweld</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="6" type="-+">Neen koos, blyf hier? ik bid u; <rhyme label="a" type="m">gaa</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">ik bid u, <rhyme label="b" type="m">gaa</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Zoo spoedig toch niet <rhyme label="a" type="m" corresp="#s14:l1">heen'</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Myn hart dat zeide altoos <rhyme label="b" type="m" corresp="#s14:l2">ja</rhyme>?</l>
        <l id="s14:l5" met="3" type="-+">Of schoon myn mond zei' <rhyme label="b" type="m" corresp="#s14:l2">neen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="6" type="-+">Neen Antje neen ik heb geen <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">ik heb, geen <rhyme label="b" type="m">tyd</rhyme>;</l>
        <l id="s15:l3" met="4" type="-+">Zyt tot weerziens <rhyme label="a" type="m" corresp="#s15:l1">gegroet</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Gy hebt my lang genoeg <rhyme label="b" type="m" corresp="#s15:l2">verleid</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Wis om myn geld en <rhyme label="b" type="m" corresp="#s15:l2">goed</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="6" type="-+">Daar gaat hy heen, och, meisjes <rhyme label="a" type="m">hoor</rhyme></l>
        <l id="s16:l2" met="3" type="-+">och meisjes <rhyme label="b" type="m">hoord</rhyme></l>
        <l id="s16:l3" met="4" type="-+">En leer uit myne <rhyme label="a" type="m" corresp="#s16:l1">daad</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Komt u een hupsche minnaar <rhyme label="b" type="m" corresp="#s16:l2">voor</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Grypt toe, eer 't is te <rhyme label="b" type="m" corresp="#s16:l2">laat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
