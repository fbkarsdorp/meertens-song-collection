    <tags>
      <tag translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Wee mij ik mag wel klagen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wee mijn Ick mach wel <rhyme label="a" type="f">klaegen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">van pijn en swaer <rhyme label="b" type="m">voerdriet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat mij naeckt alle <rhyme label="a" type="f" corresp="#s1:l1">Dage</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Duer een die mijn gebiet/ <rhyme label="b" type="m" corresp="#s1:l2">Siet</rhyme></l>
        <l id="s1:l5" met="4" type="+-">sij staet Int hart geplant/ <rhyme label="c" type="m">want</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Schoender en leefter <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die lieft neit mij <rhyme label="a" type="f">voerwonne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">van dees goddin <rhyme label="b" type="m">Idoen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Och hadt Ick noeit <rhyme label="a" type="f" corresp="#s2:l1">begonnen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Want voer mijn loen/ <rhyme label="b" type="m" corresp="#s2:l2">schchoen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">sal al mijn leeven pijn/ <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">hier In dees weerels <rhyme label="b" type="m" corresp="#s2:l2">troen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat baetet al mijn <rhyme label="a" type="f">klachte</rhyme></l>
        <l id="s3:l2" met="3" type="-+">want sij mij doch <rhyme label="b" type="m">voersmat</rhyme></l>
        <l id="s3:l3" met="3" type="-+">geen troest mach ick <rhyme label="a" type="f" corresp="#s3:l1">voorwachten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">vant maechdelicke saet/ <rhyme label="b" type="m" corresp="#s3:l2">raet</rhyme></l>
        <l id="s3:l5" met="4" type="+-">sou mijn weesen goedt/ <rhyme label="c" type="m">spoet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">u eert valt te <rhyme label="b" type="m" corresp="#s3:l2">laet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den tidt heb ick <rhyme label="a" type="f">geweeten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">maer tis nu al voor <rhyme label="b" type="m">keert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dat Ick wist haer <rhyme label="a" type="f" corresp="#s4:l1">seekreeten</rhyme></l>
        <l id="s4:l4" met="4" type="+-">van haer ben Ick gekeert/ <rhyme label="b" type="m" corresp="#s4:l2">eert</rhyme></l>
        <l id="s4:l5" met="4" type="+-">een die blift konstant/ <rhyme label="c" type="m">want</rhyme></l>
        <l id="s4:l6" met="3" type="-+">sij den ruch miJn <rhyme label="b" type="m" corresp="#s4:l2">keert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gij Jonckmans wilt wel <rhyme label="a" type="f">letten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die nu bij dochters <rhyme label="b" type="m">gaet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">U beck daer na te <rhyme label="a" type="f" corresp="#s5:l1">setten</rhyme></l>
        <l id="s5:l4" met="4" type="+-">op dat gij niet door list / <rhyme label="b" type="m" corresp="#s5:l2">mist</rhyme></l>
        <l id="s5:l5" met="4" type="+-">noch doer goet bedroch/ <rhyme label="c" type="m">och</rhyme></l>
        <l id="s5:l6" met="3" type="-+">achter tnet en <rhyme label="b" type="m" corresp="#s5:l2">vist</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Orlof gij Jonge <rhyme label="a" type="f">geesten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">volcht niet mijn liefde <rhyme label="b" type="m">blint</rhyme></l>
        <l id="s6:l3" met="3" type="-+">op bancketten noch op <rhyme label="a" type="f" corresp="#s6:l1">feesten</rhyme></l>
        <l id="s6:l4" met="4" type="+-">voersint eer gij begint/ <rhyme label="b" type="m" corresp="#s6:l2">mint</rhyme></l>
        <l id="s6:l5" met="4" type="+-">een die blift getrou/ <rhyme label="c" type="m">bout</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Daer gij liefde <rhyme label="b" type="m" corresp="#s6:l2">vint</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Prinsesse schoen <rhyme label="a" type="f">gepreesen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Draecht eenen huesen <rhyme label="b" type="m">mont</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Seekreet sal ick <rhyme label="a" type="f" corresp="#s7:l1">weesen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Want tot geender stont/ <rhyme label="b" type="m" corresp="#s7:l2">vont</rhyme></l>
        <l id="s7:l5" met="4" type="+-">gij In mij bedroch/ <rhyme label="c" type="m">noch</rhyme></l>
        <l id="s7:l6" met="3" type="-+">eenen valechen <rhyme label="b" type="m" corresp="#s7:l2">gront</rhyme>.</l>
      </lg>
    </lg>
  </text>
