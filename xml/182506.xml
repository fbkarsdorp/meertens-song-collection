  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Te Amsterdam daer staet een <rhyme label="a" type="m">huis</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Daer is een oude grompot <rhyme label="a" type="m" corresp="#s1:l1">tuis</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Toesoer, <rhyme label="b" type="m" corresp="#s1:l2">Toesoer</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s1:l2">lamoer</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Lamoer, <rhyme label="b" type="m" corresp="#s1:l2">toesoer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s1:l2">lamoer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dan komen zy 's avonds aen haer <rhyme label="a" type="m">deur</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dan komt die oude grompt <rhyme label="a" type="m" corresp="#s2:l1">veur</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Toesoet, <rhyme label="b" type="m" corresp="#s2:l2">Toesoer</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s2:l2">lamoer</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Lamoer, <rhyme label="b" type="m" corresp="#s2:l2">toesoer</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s2:l2">lamoer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy komt zomtyds wel over <rhyme label="a" type="m">haer</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Want hy is bang dat zy raekt <rhyme label="a" type="m" corresp="#s3:l1">zwaer</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Toesoer <rhyme label="b" type="m" corresp="#s3:l2">toesoer</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s3:l2">lamoer</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Lamoer <rhyme label="b" type="m" corresp="#s3:l2">toesoer</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s3:l2">lamoer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Al hebje nog zo een dikken <rhyme label="a" type="m">hals</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Al evenwel moet gy aen den <rhyme label="a" type="m" corresp="#s4:l1">dans</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Toesoer <rhyme label="b" type="m" corresp="#s4:l2">toesoer</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s4:l2">lamoer</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Lamoer <rhyme label="b" type="m" corresp="#s4:l2">toesoer</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s4:l2">lamoer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Schilder les haer op haer <rhyme label="a" type="m">rug</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">De Schilder die gaf zugt op <rhyme label="a" type="m" corresp="#s5:l1">zugt</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Toesoer <rhyme label="b" type="m" corresp="#s5:l2">toesoer</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s5:l2">lamoer</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Lamoer <rhyme label="b" type="m" corresp="#s5:l2">toesoer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Toelmalette <rhyme label="b" type="m" corresp="#s5:l2">lamoer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zy moet by de Officier in de <rhyme label="a" type="m">kraem</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Soldaten moeten voor Vader <rhyme label="a" type="m" corresp="#s6:l1">staen</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Toesoer <rhyme label="b" type="m" corresp="#s6:l2">toesoer</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s6:l2">lamoer</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Lamoer <rhyme label="b" type="m" corresp="#s6:l2">toesoer</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s6:l2">lamoer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dan gaet zy 's avonds na de <rhyme label="a" type="m">Hoofdwagt</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Zy haelt de Soldaten van <rhyme label="a" type="m" corresp="#s7:l1">Schildwagt</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Toesoer <rhyme label="b" type="m" corresp="#s7:l2">toesoer</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Toelmalette <rhyme label="b" type="m" corresp="#s7:l2">lamoer</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Lamoer, <rhyme label="b" type="m" corresp="#s7:l2">toesoer</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Toelemalette, <rhyme label="b" type="m" corresp="#s7:l2">lamoer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die dit Liedje heeft <rhyme label="a" type="m">gedigt</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Toelemalette <rhyme label="b" type="m">lamoer</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">'t Was een Soldaet zyn hert was <rhyme label="a" type="m" corresp="#s8:l1">ligt</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Toesoer, <rhyme label="b" type="m" corresp="#s8:l2">toesoer</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s8:l2">lamoer</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Lamoer <rhyme label="b" type="m" corresp="#s8:l2">toesoer</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Toelemalette <rhyme label="b" type="m" corresp="#s8:l2">lamoer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
