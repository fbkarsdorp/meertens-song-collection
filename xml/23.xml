  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HAer poesel Lichaem net <rhyme label="a" type="f">besneden</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Uyt munt in alle <rhyme label="a" type="f" corresp="#s1:l1">geestigheden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">De Deucht en wijsheyts glans sijn <rhyme label="b" type="m">beyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ten coor van haer gedacht <rhyme label="c" type="f">gesteegen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Die alles na de maet eerst <rhyme label="c" type="f" corresp="#s1:l4">weegen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">'t Geen uyt haer Lieve lipjes <rhyme label="b" type="m" corresp="#s1:l3">scheyt</rhyme>.</l>
      </lg>
    </lg>
