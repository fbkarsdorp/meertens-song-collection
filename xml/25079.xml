<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25079</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Ik zie de morgenster</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Edel herte houdt v <rhyme label="a" type="m">vry</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Van eertschen <rhyme label="b" type="f">creatueren</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Ende laetse beelden niet in <rhyme label="a" type="m" corresp="#s1:l1">di</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Ende dwinct dijn edele <rhyme label="b" type="f" corresp="#s1:l2">natuere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Si moechten di <rhyme label="a" type="m">verweeren</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Al in die eewege <rhyme label="b" type="f">doot</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Heere god doer dine <rhyme label="a" type="m" corresp="#s2:l1">edelhede</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Huedt ons van selker <rhyme label="b" type="f" corresp="#s2:l2">noet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als der minnen <rhyme label="a" type="m">trane</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Dat aenschijn <rhyme label="b" type="f">ouergaen</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Soe comt die minnere <rhyme label="a" type="m" corresp="#s3:l1">selue</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Ende geet hem scincken <rhyme label="b" type="f" corresp="#s3:l2">saen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat geeft hi hem <rhyme label="a" type="m">drincken</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Hem seluen geeft hi <rhyme label="b" type="f">al</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Die wel te rechte wil <rhyme label="a" type="m" corresp="#s4:l1">dincken</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Sijn loen en es niet <rhyme label="b" type="f" corresp="#s4:l2">smal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Kinder laet ons gode <rhyme label="a" type="m">minnen</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Het dunct my wesen meer dan <rhyme label="b" type="f">tijt</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Mochten wi den hogen loen <rhyme label="a" type="m" corresp="#s5:l1">gewinnen</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Soe souden wi leuen sonder <rhyme label="b" type="f" corresp="#s5:l2">nijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Vroude ende groot <rhyme label="a" type="m">iolijt</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">Es hen altoes <rhyme label="b" type="f">bereit</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Die daer dient sonder <rhyme label="a" type="m" corresp="#s6:l1">vermijt</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Den heere daert al an <rhyme label="b" type="f" corresp="#s6:l2">steit</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ic wille den heere <rhyme label="a" type="m">alleene</rhyme>.</l>
        <l id="s7:l2" met="3" type="-+">Te male getrouwe <rhyme label="b" type="f">sijn</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Ic minnen alte <rhyme label="a" type="m" corresp="#s7:l1">cleene</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Dat es die scade <rhyme label="b" type="f" corresp="#s7:l2">mijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nv willic my gaen <rhyme label="a" type="m">voegen</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">Te male in <rhyme label="b" type="f">eenicheit</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">In wil voert meer niet <rhyme label="a" type="m" corresp="#s8:l1">proeuen</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Der menschen <rhyme label="b" type="f" corresp="#s8:l2">ongestadicheit</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Hi es te male <rhyme label="a" type="m">alleene</rhyme>.</l>
        <l id="s9:l2" met="3" type="-+">Die my verurouden <rhyme label="b" type="f">mach</rhyme>.</l>
        <l id="s9:l3" met="3" type="-+">Der menschen troest es <rhyme label="a" type="m" corresp="#s9:l1">cleene</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Dies proeuic alden <rhyme label="b" type="f" corresp="#s9:l2">dach</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Nv laet ons gerne <rhyme label="a" type="m">dogen</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">Liden ende <rhyme label="b" type="f">ongelijc</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">Ende doet die ziele <rhyme label="a" type="m" corresp="#s10:l1">hoeden</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Ende maecse in duechden <rhyme label="b" type="f" corresp="#s10:l2">rijc</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">En wils niement <rhyme label="a" type="m">clagen</rhyme>.</l>
        <l id="s11:l2" met="3" type="-+">Dan siere <rhyme label="b" type="f">houescheit</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Hi saelt my hulpen <rhyme label="a" type="m" corresp="#s11:l1">dragen</rhyme>.</l>
        <l id="s11:l4" met="3" type="-+">Alst my ouer <rhyme label="b" type="f" corresp="#s11:l2">ghijt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Nv en es hier geen <rhyme label="a" type="m">dueren</rhyme>.</l>
        <l id="s12:l2" met="3" type="-+">In deser <rhyme label="b" type="f">nederheit</rhyme>.</l>
        <l id="s12:l3" met="3" type="-+">En es maer een <rhyme label="a" type="m" corresp="#s12:l1">vre</rhyme>.</l>
        <l id="s12:l4" met="3" type="-+">Tsegen die <rhyme label="b" type="f" corresp="#s12:l2">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ic bidde v hoge <rhyme label="a" type="m">natuere</rhyme>.</l>
        <l id="s13:l2" met="3" type="-+">Maect my van duechden <rhyme label="b" type="f">rijc</rhyme>.</l>
        <l id="s13:l3" met="3" type="-+">Dat ic suete ende <rhyme label="a" type="m" corresp="#s13:l1">suere</rhyme>.</l>
        <l id="s13:l4" met="3" type="-+">Mach liden al <rhyme label="b" type="f" corresp="#s13:l2">gelijc</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Sceiden dat ic van heer ihesus <rhyme label="a" type="m">sciet</rhyme>.</l>
        <l id="s14:l2" met="3" type="-+">Daer omme mach ic wel <rhyme label="b" type="f">weenen</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Ende ic hem in goeder trouwen <rhyme label="a" type="m" corresp="#s14:l1">liet</rhyme>.</l>
        <l id="s14:l4" met="3" type="-+">Daer in soe bleuic <rhyme label="b" type="f" corresp="#s14:l2">cleene</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Ic sciet van hem ende hi niet van <rhyme label="a" type="m">my</rhyme>.</l>
        <l id="s15:l2" met="3" type="-+">Ic doelde in dit <rhyme label="b" type="f">allinde</rhyme>.</l>
        <l id="s15:l3" met="3" type="-+">Soe ic my meer <rhyme label="a" type="m" corresp="#s15:l1">keerde</rhyme>.</l>
        <l id="s15:l4" met="3" type="-+">Van hem soe ic my min <rhyme label="b" type="f" corresp="#s15:l2">bekinde</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
