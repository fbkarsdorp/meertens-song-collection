<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182520</id>
    <date>
      <year-start>1793</year-start>
      <year-end>1793</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Sinjeur de Schelde</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Sinjeur, Sinjeur de <rhyme label="a" type="f">Schelde</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die gy zo dikwils <rhyme label="b" type="m">aenziet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Eer dat gy trok te <rhyme label="a" type="f" corresp="#s1:l1">velde</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Het diend tot u <rhyme label="b" type="m" corresp="#s1:l2">verdriet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Want die vergulde <rhyme label="c" type="f">Stroomen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die brengen u in de <rhyme label="d" type="m">schand</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">U hoop dat zyn maer <rhyme label="c" type="f" corresp="#s1:l5">droomen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Het is een gesloten <rhyme label="d" type="m" corresp="#s1:l6">gat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Holland, Holland, vol <rhyme label="a" type="f">streeken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Vol van <rhyme label="b" type="m">eenvoudigheid</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sinjeur vol van <rhyme label="a" type="f" corresp="#s2:l1">gebreeken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En vol van <rhyme label="b" type="m" corresp="#s2:l2">eenzamheid</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Holland die heeft <rhyme label="c" type="f">behouden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Door zyn <rhyme label="d" type="m">siptilligheid</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Sinjeur gy moet gaen <rhyme label="c" type="f" corresp="#s2:l5">dwalen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">De Schelden zyt gy <rhyme label="d" type="m" corresp="#s2:l6">kwyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laet ons, laet ons <rhyme label="a" type="f">bemerken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wat hier al is <rhyme label="b" type="m">belagt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wat ons mag <rhyme label="a" type="f" corresp="#s3:l1">versterken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Het is waerlyk <rhyme label="b" type="m" corresp="#s3:l2">belagt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Antwerpen zal <rhyme label="c" type="f">floreeren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Met hun staet en <rhyme label="d" type="m">pragt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Sinjeur gy gaet <rhyme label="c" type="f" corresp="#s3:l5">mankeeren</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Het is nu al te <rhyme label="d" type="m" corresp="#s3:l6">laet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Kredit, Kredit, <rhyme label="a" type="f">verlooren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daer by u doorvaert <rhyme label="b" type="m">kwyt</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Prosyn en alles <rhyme label="a" type="f" corresp="#s4:l1">verlooren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat heeft u ook <rhyme label="b" type="m" corresp="#s4:l2">misleid</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat gy door al het <rhyme label="c" type="f">zweeven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van de <rhyme label="d" type="m">Negotiant</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">De groote <rhyme label="c" type="f" corresp="#s4:l5">Bankzoetieren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zyn altyd voor de <rhyme label="d" type="m" corresp="#s4:l6">hant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Helaas, helaas <rhyme label="a" type="f">verlaten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wat nu ons <rhyme label="b" type="m">gedaen</rhyme>?</l>
        <l id="s5:l3" met="3" type="-+">Niemand komt ons te <rhyme label="a" type="f" corresp="#s5:l1">baten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">By wie om troost <rhyme label="b" type="m" corresp="#s5:l2">gegaen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">De schelden zyn <rhyme label="c" type="f">gesloten</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En dat al voor <rhyme label="d" type="m">altyd</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Sinjeur ik wil u <rhyme label="c" type="f" corresp="#s5:l5">groeten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Gy zyt de Schelden <rhyme label="d" type="m" corresp="#s5:l6">kwyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O Zottin, o Zottin Ã´ <rhyme label="a" type="f">dwasen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met al u <rhyme label="b" type="m">geraes</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wild van de schelde niet <rhyme label="a" type="f" corresp="#s6:l1">praeten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Draegt over haer de <rhyme label="b" type="m" corresp="#s6:l2">rou</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Holland met al zyn <rhyme label="c" type="f">schatten</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Heeft het u al <rhyme label="d" type="m">verdooft</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Blyft in de staet wat <rhyme label="c" type="f" corresp="#s6:l5">praeten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Ie zyt daer van <rhyme label="d" type="m" corresp="#s6:l6">berooft</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hoe kan, hoe kan het <rhyme label="a" type="f">weesen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat gy nog niet word <rhyme label="b" type="m">wys</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hebt gy dan niet <rhyme label="a" type="f" corresp="#s7:l1">gelesen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Artikels van <rhyme label="b" type="m" corresp="#s7:l2">Parys</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">De Vreede is <rhyme label="c" type="f">gesloten</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Dat doet u grote <rhyme label="d" type="m">pyn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En wist uit wedde of <rhyme label="c" type="f" corresp="#s7:l5">loten</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Of gy gekult zou <rhyme label="d" type="m" corresp="#s7:l6">zyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ga heen, ga heen ga <rhyme label="a" type="f">zotte</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gy zyt te <rhyme label="b" type="m">vriendelyk</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wild met de Schelde niet <rhyme label="a" type="f" corresp="#s8:l1">spotten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Holland dat is de <rhyme label="b" type="m" corresp="#s8:l2">wyk</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">De Brusselsche Scheepen die <rhyme label="c" type="f">varen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En dan voor hunne <rhyme label="d" type="m">vragt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">De vragt die zyne vol <rhyme label="c" type="f" corresp="#s8:l5">zotten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Adieu Sinjeur <rhyme label="d" type="m" corresp="#s8:l6">genagt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
