<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100039</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het was een boer uit Vlaanderland</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EY siet wat vliegt daer gins om <rhyme label="a" type="m">hoog</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wel Trijngtje watte <rhyme label="b" type="f">dinge</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">'t Is een kleyn mantje met een <rhyme label="a" type="m" corresp="#s1:l1">boog</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Sie daer nou komt hy <rhyme label="b" type="f" corresp="#s1:l2">springe</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hoe brilt zijn naeckte <rhyme label="c" type="f">gatje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Het lijckent wel een <rhyme label="c" type="f" corresp="#s1:l5">platje</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Ey siet hy <rhyme label="d" type="m">daelt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En hy mostert <rhyme label="d" type="m" corresp="#s1:l7">maelt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Waer of hy vleugels <rhyme label="d" type="m" corresp="#s1:l7">haelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wel Pietje wel hoe praetje <rhyme label="a" type="m">soo</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoe benje noch soo <rhyme label="b" type="f">slegjes</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dit Mannetje hiet <rhyme label="a" type="m" corresp="#s2:l1">Cupido</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy schiet de Jonge <rhyme label="b" type="f" corresp="#s2:l2">Knegjes</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wel waerom doet hy <rhyme label="c" type="f">datte</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Om datse moete <rhyme label="c" type="f" corresp="#s2:l5">vatte</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">De Meysjes <rhyme label="d" type="m">jent</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Nou weetjet <rhyme label="d" type="m" corresp="#s2:l7">vent</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Waerom hy pijltjes <rhyme label="d" type="m" corresp="#s2:l7">sent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wel Tringtje hoe is hy soo <rhyme label="a" type="m">arm</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Dat hy soo bloot moet <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">'t Is kout, en kleertjes decken <rhyme label="a" type="m" corresp="#s3:l1">warm</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Laet ons hem kleeren <rhyme label="b" type="f" corresp="#s3:l2">geven</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Want 't Quant je mogt <rhyme label="c" type="f">verstijven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wy kunnen hem <rhyme label="c" type="f" corresp="#s3:l5">geryven</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Met een ouwen <rhyme label="d" type="m">Broock</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Of een linden <rhyme label="d" type="m" corresp="#s3:l7">doock</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Ick bidje doetet <rhyme label="d" type="m" corresp="#s3:l7">oock</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wel Pietje hy is Venus <rhyme label="a" type="m">seun</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hy is alsoo <rhyme label="b" type="f">geboren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En 't naeckt gaen is hy wel <rhyme label="a" type="m" corresp="#s4:l1">geweun</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hy sou in kleeren <rhyme label="b" type="f" corresp="#s4:l2">smoren</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Siet toe hy selje <rhyme label="c" type="f">schieten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Maer 't moetje niet <rhyme label="c" type="f" corresp="#s4:l5">verdrieten</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Dat zijn pijltjen <rhyme label="d" type="m">velt</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">'t Is een braven <rhyme label="d" type="m" corresp="#s4:l7">Helt</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Zijn Boogjen hy nu <rhyme label="d" type="m" corresp="#s4:l7">stelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ay, ay, ay, lieve <rhyme label="a" type="m">Cupido</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Je moet me niet <rhyme label="b" type="f">vernielen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Je felle pijltje smertme <rhyme label="a" type="m" corresp="#s5:l1">soo</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hout op 'k sel voorje <rhyme label="b" type="f" corresp="#s5:l2">knielen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En ick sel Tringtjes <rhyme label="c" type="f">grijpen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En in mijn arren <rhyme label="c" type="f" corresp="#s5:l5">nijpen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Wel kom doetet <rhyme label="d" type="m">dan</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Weerje als een <rhyme label="d" type="m" corresp="#s5:l7">Man</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">En toont eens watje <rhyme label="d" type="m" corresp="#s5:l7">kan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
