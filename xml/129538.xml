    <melody>Ik weet er geen schonere maagd</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik weeter geen schoonder <rhyme label="a" type="m">Maagd</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Als die   mijn jong hert <rhyme label="a" type="m" corresp="#s1:l1">behaagt</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Mogt ik eens by haer <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Genesen soo was ‘r myn <rhyme label="c" type="m">wond</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Soete Lief, maak’er myn hert <rhyme label="c" type="m" corresp="#s1:l4">gesont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haer wit albasterde <rhyme label="a" type="m">borst</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Daar myn jonghert na <rhyme label="a" type="m" corresp="#s2:l1">dorst</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Haar oogen vol lonken en <rhyme label="b" type="f">stralen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Van tale so <rhyme label="c" type="m">vriendelyk</rhyme></l>
        <l id="s2:l5" met="4" type="+-">En van deugde maar also <rhyme label="c" type="m" corresp="#s2:l4">ryk</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Haer wangen schoon <rhyme label="a" type="m">gebloost</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Veel schoonder als een <rhyme label="a" type="m" corresp="#s3:l1">roos</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">Geen schilder en kan <rhyme label="b" type="f">afmalen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Te schilderen   sulcken <rhyme label="c" type="m">Beelt</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Die mijn jonck hert en sinnen <rhyme label="c" type="m" corresp="#s3:l4">steelt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">‘s Avonts maar alsoo <rhyme label="a" type="m">laet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dan wandel ik over <rhyme label="a" type="m" corresp="#s4:l1">straat</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Sy wilder my niet in <rhyme label="b" type="f">laeten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En praten met myn een <rhyme label="c" type="m">reys</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Soete Lief, denckt eens hoe dat ik <rhyme label="c" type="m" corresp="#s4:l4">peys</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Lief, seght my de <rhyme label="a" type="m">oorsaack</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Waarom dat gy my <rhyme label="a" type="m" corresp="#s5:l1">verlaet</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Ben ik te los van <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van leden te plomp of <rhyme label="c" type="m">grof</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Soete Lief ik bender nog   Edel van <rhyme label="c" type="m" corresp="#s5:l4">stof</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Of is het om ‘t Werelts <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat ik u derven <rhyme label="a" type="m" corresp="#s6:l1">moet</rhyme>?</l>
        <l id="s6:l3" met="3" type="-+">En wilter my brengen <rhyme label="b" type="f">niet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">In sulken zwaar <rhyme label="c" type="m">verdriet</rhyme>!</l>
        <l id="s6:l5" met="4" type="+-">Rijck of Arm en ben ik <rhyme label="c" type="m" corresp="#s6:l4">niet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Liefde gekocht om <rhyme label="a" type="m">Gout</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Die is so haest <rhyme label="a" type="m" corresp="#s7:l1">verflout</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Men siet de Minnaars <rhyme label="b" type="f">gedachten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Gelijken een Riedt dat <rhyme label="c" type="m">draeyt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Dat met alle de Winden <rhyme label="c" type="m" corresp="#s7:l4">waeyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Adieu, mijn Princes <rhyme label="a" type="m">eerbaer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Wy scheyden nu van <rhyme label="a" type="m" corresp="#s8:l1">malkaer</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Denckt eens wie dat het sal <rhyme label="b" type="f">rouwen</rhyme>?</l>
        <l id="s8:l4" met="3" type="-+">Jonckvrouw, nu voor het <rhyme label="c" type="m">slot</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Ik wensch u de Vrede by <rhyme label="c" type="m" corresp="#s8:l4">Godt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
