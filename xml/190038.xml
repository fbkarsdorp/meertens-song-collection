  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">En dat edele <rhyme label="a" type="f">Matresje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Al op haer Beddetje <rhyme label="b" type="m">lag</rhyme>:</l>
        <l id="s1:l3" met="5" type="-+">En om vlooytjes te gaen vangen, Ja <rhyme label="c" type="f">vangen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daer na ws myn <rhyme label="c" type="f" corresp="#s1:l3">verlangen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Of zy nog Maget <rhyme label="b" type="m" corresp="#s1:l2">was</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">En de Kaers ging uit en ’t was <rhyme label="a" type="f">donker</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En ’t krygen dat was <rhyme label="b" type="m">gedaen</rhyme>:</l>
        <l id="s2:l3" met="5" type="-+">En doen ging ik leggen lonken, Ja <rhyme label="c" type="f">lonken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Vrouw Venus <rhyme label="c" type="f" corresp="#s2:l3">Minne-vonken</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En ik ben weer <rhyme label="b" type="m" corresp="#s2:l2">opgestaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Kaersjes en mooije <rhyme label="a" type="f">Meisjes</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die hebben myn <rhyme label="b" type="m">verleid</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">En wyt ik het op myn oogen, Ja <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die hebben my <rhyme label="c" type="f" corresp="#s3:l3">bedrogen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Door myn <rhyme label="b" type="m" corresp="#s3:l2">Nieuwschierigheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
