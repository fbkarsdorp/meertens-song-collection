    <melody>Ach Vater unser der du bist</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Godt die onse Vader <rhyme label="a" type="m">bist</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Door Iesum <rhyme label="a" type="m" corresp="#s1:l1">Christ</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Gheeft uwen Geest ons <rhyme label="b" type="m">alghemeyn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die ons ter waerheyt <rhyme label="c" type="f">leyde</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Verhoort ons doch tot deser <rhyme label="d" type="m">stondt</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Opent den <rhyme label="d" type="m" corresp="#s1:l5">Mont</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Uwes Dienaers, dat hy u woort <rhyme label="b" type="m" corresp="#s1:l3">reyn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En vrymoedigh <rhyme label="c" type="f" corresp="#s1:l4">verbreyde</rhyme>:</l>
        <l id="s1:l9" met="4" type="-+">Daer toe, O Heer <rhyme label="e" type="m">Ghenadelijck</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Opent ons hert en <rhyme label="f" type="f">Ooren</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Dat wy dat hooren <rhyme label="e" type="m" corresp="#s1:l9">vlijtelijck</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">En trouwelijck <rhyme label="g" type="f">bewaren</rhyme>,</l>
        <l id="s1:l13" met="4" type="-+">Op dat wy moghen <rhyme label="e" type="m" corresp="#s1:l9">vruchtbaerlijck</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">U Loff altijdt <rhyme label="g" type="f" corresp="#s1:l12">verklaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
