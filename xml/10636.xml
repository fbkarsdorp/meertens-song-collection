  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Nachtegael in 't <rhyme label="a" type="f">wilde</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Princesse <rhyme label="b" type="m">amoureus</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Gaet segghen tot mijn <rhyme label="c" type="f">beminde</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ick coem terstont by <rhyme label="b" type="m" corresp="#s1:l2">heur</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s1:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gaet segghen tot mijn <rhyme label="a" type="f">beminde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ick com' terstont by <rhyme label="b" type="m">heur</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Savonts in de Mane <rhyme label="c" type="f">schijn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Vandt ickse in de <rhyme label="b" type="m" corresp="#s2:l2">deur</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s2:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Savonts in de <rhyme label="a" type="f">Maneschijn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vandt ickse in de <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Bon soir ma <rhyme label="c" type="f">Matresse</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Bon soir mon <rhyme label="b" type="m" corresp="#s3:l2">serviteur</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s3:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Bon soir ma <rhyme label="a" type="f">Matresse</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Bon soir mon <rhyme label="b" type="m">serviteur</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hoe gheerne soud' ick <rhyme label="c" type="f">draghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Schoon lief van v <rhyme label="b" type="m" corresp="#s4:l2">faveur</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s4:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoe gheerne soud' ick <rhyme label="a" type="f">draghen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Schoon Lief van v <rhyme label="b" type="m">faveur</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Schoon Lief ghy comt te <rhyme label="c" type="f">spade</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Daer is een ander <rhyme label="b" type="m" corresp="#s5:l2">veur</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s5:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Schoon Lief com' ick te <rhyme label="a" type="f">spade</rhyme>?</l>
        <l id="s6:l2" met="3" type="-+">Is daer een ander <rhyme label="b" type="m">veur</rhyme>?</l>
        <l id="s6:l3" met="3" type="-+">Hy nam zijn poingaert in zijn <rhyme label="c" type="f">handt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dit is mijn leste <rhyme label="b" type="m" corresp="#s6:l2">uer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s6:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy nam zijn poingaert in zijn <rhyme label="a" type="f">handt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dit is mijn leste <rhyme label="b" type="m">uer</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sy sprack met soete <rhyme label="c" type="f">woorden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Hoe bleeck wordt v <rhyme label="b" type="m" corresp="#s7:l2">coleur</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s7:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sy sprack met soete <rhyme label="a" type="f">woorden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Hoe bleeck wort v <rhyme label="b" type="m">coleur</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ick sal v noch liefde <rhyme label="c" type="f">draghen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Al isser een ander <rhyme label="b" type="m" corresp="#s8:l2">veur</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s8:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick sal v noch liefde <rhyme label="a" type="f">draghen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Al isser een ander <rhyme label="b" type="m">veur</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Al waer de Paus van <rhyme label="c" type="f">Roomen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Mijn Vader in't <rhyme label="b" type="m" corresp="#s9:l2">ghetreur</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s9:l2">langeur</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Al waer die Paus van <rhyme label="a" type="f">Roomen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Mijn Vader in't <rhyme label="b" type="m">ghetreur</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Soo sal ick mijn liefde niet <rhyme label="c" type="f">laten</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Van v mijn <rhyme label="b" type="m" corresp="#s10:l2">serviteur</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Pourtant que je suis <rhyme label="d" type="f">bruynette</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Viveray-je en <rhyme label="b" type="m" corresp="#s10:l2">langeur</rhyme>.</l>
      </lg>
    </lg>
