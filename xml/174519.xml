<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>174519</id>
    <date>
      <year-start>1620</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag>new year song</tag>
    </tags>
    <melody>Laat ons de Heer loven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gheen meerder vreucht om <rhyme label="a" type="f">hooren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">als gods heerlycke <rhyme label="b" type="m">stem</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Is In Broot stadt <rhyme label="a" type="f" corresp="#s1:l1">geboren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">genaemt cleijn <rhyme label="b" type="m" corresp="#s1:l2">Bethlehem</rhyme></l>
        <l id="s1:l5" met="3" type="-+">De harders opten <rhyme label="c" type="f">velden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Schrickten voor tlicht zeer <rhyme label="d" type="m">claer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">gods engel hun <rhyme label="c" type="f" corresp="#s1:l5">vertelde</rhyme></l>
        <l id="s1:l8" met="3" type="-+">t god salich nieuwe <rhyme label="d" type="m" corresp="#s1:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die rijck was, hooch <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">comt In arme <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">maeckt ons ryck daer <rhyme label="a" type="f" corresp="#s2:l1">beneven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Deur lieffde metter <rhyme label="b" type="m" corresp="#s2:l2">daet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Een vrede vorst <rhyme label="c" type="f">gepresen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">voor spraeck en <rhyme label="d" type="m">middelaer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">t woort des vaders by <rhyme label="c" type="f" corresp="#s2:l5">deze</rhyme></l>
        <l id="s2:l8" met="3" type="-+">wordt vleesch Int nieuwe <rhyme label="d" type="m" corresp="#s2:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Off werelt meent te <rhyme label="a" type="f">weeten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">wat comeet ster <rhyme label="b" type="m">beduijt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">godt Int hoogste <rhyme label="a" type="f" corresp="#s3:l1">geseten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Behout aen hem <rhyme label="b" type="m" corresp="#s3:l2">tbesluijt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">hierom laet off te <rhyme label="c" type="f">twisten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">van saecken veel te <rhyme label="d" type="m">zwaer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">godt kent d'oprechte <rhyme label="c" type="f" corresp="#s3:l5">christen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">lydt saem Int nieuwe <rhyme label="d" type="m" corresp="#s3:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die gods geboort <rhyme label="a" type="f">beminnen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dragen geen haet noch <rhyme label="b" type="m">nijdt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Maer gaen vrij moedich <rhyme label="a" type="f" corresp="#s4:l1">binnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">daer armen zijn <rhyme label="b" type="m" corresp="#s4:l2">altyt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Deelen van godes <rhyme label="c" type="f">segen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In winter noot <rhyme label="d" type="m">voorwaer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">godt doet harten <rhyme label="c" type="f" corresp="#s4:l5">bewegen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">tot hulp Int nieuwe <rhyme label="d" type="m" corresp="#s4:l6">Iaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Iaren vast deur <rhyme label="a" type="f">loopen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">t'genaeckt den laatsten <rhyme label="b" type="m">dach</rhyme></l>
        <l id="s5:l3" met="3" type="-+">weijnich op vrede <rhyme label="a" type="f" corresp="#s5:l1">hopen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">die lange duyren <rhyme label="b" type="m" corresp="#s5:l2">mach</rhyme></l>
        <l id="s5:l5" met="3" type="-+">van oorlooch hoort men <rhyme label="c" type="f">spreecken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">ter werelt hier en <rhyme label="d" type="m">daer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">nijet blijff In sonden <rhyme label="c" type="f" corresp="#s5:l5">steecken</rhyme></l>
        <l id="s5:l8" met="3" type="-+">t'geheele nieuwe <rhyme label="d" type="m" corresp="#s5:l6">jaer</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghij princen vorsten <rhyme label="a" type="f">heeren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">gods Soon doch eere <rhyme label="b" type="m">biet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">syn ryck streckt om <rhyme label="a" type="f" corresp="#s6:l1">vermeeren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">van deser werelt <rhyme label="b" type="m" corresp="#s6:l2">nijet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Sijn macht wilt <rhyme label="c" type="f">onderscheyen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">eer dat de straff comt <rhyme label="d" type="m">naer</rhyme></l>
        <l id="s6:l7" met="3" type="-+">leeft als wit <rhyme label="c" type="f" corresp="#s6:l5">accoleyen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">vreed'saem Int nieuwe <rhyme label="d" type="m" corresp="#s6:l6">Iaer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
