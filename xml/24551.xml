<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24551</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="dialooglied" translation="dialogue song">dialooglied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Rijke God wie zal ik klagen</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="emotions">emoties</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Heer god wie sal ic <rhyme label="a" type="f">claeghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">al myne verlaeren <rhyme label="b" type="m">tyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">myne conciencie wil altoes <rhyme label="a" type="f" corresp="#s1:l1">claeghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">sie en gheuet my gheen <rhyme label="b" type="m" corresp="#s1:l2">respyt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ic moet my leren <rhyme label="c" type="f">liden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">die schulden die synt <rhyme label="d" type="m">myn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">ic en can my nyet <rhyme label="c" type="f" corresp="#s1:l5">verbliden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">druck moet myn eyghen <rhyme label="d" type="m" corresp="#s1:l6">syn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Och heer ic sal v <rhyme label="a" type="f">vraghen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">wolde ghi my wal <rhyme label="b" type="m">verstaen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">sal ic noch langher <rhyme label="a" type="f" corresp="#s2:l1">iaeghen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">eer ic v sal connen <rhyme label="b" type="m" corresp="#s2:l2">gheuaen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">van rouwen duchte ic toe <rhyme label="c" type="f">steruen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">bedroeuet isset har  te <rhyme label="d" type="m">myn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">can ic v nyet <rhyme label="c" type="f" corresp="#s2:l5">verwaruen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">druck moet myn eyghen <rhyme label="d" type="m" corresp="#s2:l6">syn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Och mensche myn weselike <rhyme label="a" type="f">belde</rhyme></l>
        <l id="s3:l2" met="3" type="-+">och lidet v nu ter <rhyme label="b" type="m">tyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">wat wildi my <rhyme label="a" type="f" corresp="#s3:l1">vertellen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">dat ghi yn drucke <rhyme label="b" type="m" corresp="#s3:l2">synt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">ic hebbe v <rhyme label="c" type="f">wtuercoeren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">ic kenne wal uwen <rhyme label="d" type="m">gront</rhyme></l>
        <l id="s3:l7" met="3" type="-+">mach v wat troestes <rhyme label="c" type="f" corresp="#s3:l5">gheboeren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">weest hoeues yn uwen <rhyme label="d" type="m" corresp="#s3:l6">mont</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die warlt is schoene van <rhyme label="a" type="f">woerden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">bedroech heeft si <rhyme label="b" type="m">gheleert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">recht als die wynt van <rhyme label="a" type="f" corresp="#s4:l1">noerden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">soe is sie omme <rhyme label="b" type="m" corresp="#s4:l2">ghekiert</rhyme></l>
        <l id="s4:l5" met="3" type="-+">men en mach daer nyet op <rhyme label="c" type="f">betrouwen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">dat segghe ic v myt <rhyme label="d" type="m">vlyt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">aen ihesus wil ic my <rhyme label="c" type="f" corresp="#s4:l5">holden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">nu ende tot alre <rhyme label="d" type="m" corresp="#s4:l6">tyt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Och mensche myn lieflike <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">och lyt v nu ter <rhyme label="b" type="m">tyt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ick hebbe v wal <rhyme label="a" type="f" corresp="#s5:l1">bewesen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">dat ghi die liefste <rhyme label="b" type="m" corresp="#s5:l2">synt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">ic hebbe omme v <rhyme label="c" type="f">gheleden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">soe menych zwaer <rhyme label="d" type="m">torment</rhyme></l>
        <l id="s5:l7" met="3" type="-+">wolde ghi my nu <rhyme label="c" type="f" corresp="#s5:l5">begheuen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">soe worde ghi myns <rhyme label="d" type="m" corresp="#s5:l6">conteent</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Och heer wilt doch <rhyme label="a" type="f">ghedencken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">hoe menych zwaer <rhyme label="b" type="m">versucht</rhyme></l>
        <l id="s6:l3" met="3" type="-+">als ic v plach toe <rhyme label="a" type="f" corresp="#s6:l1">schencken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">noch doe ic des <rhyme label="b" type="m" corresp="#s6:l2">ghelick</rhyme></l>
        <l id="s6:l5" met="3" type="-+">ic haepe die tyt sal <rhyme label="c" type="f">coemen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">al wt den harten <rhyme label="d" type="m">myn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">laet my nu troest <rhyme label="c" type="f" corresp="#s6:l5">verwaruen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">vroude sal myn eyghen <rhyme label="d" type="m" corresp="#s6:l6">syn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O ziele v soete <rhyme label="a" type="f">woerde</rhyme></l>
        <l id="s7:l2" met="3" type="-+">gaen yn dat harte <rhyme label="b" type="m">myn</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ghi wordet wal <rhyme label="a" type="f" corresp="#s7:l1">ghenesen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">aen dencket myn groete <rhyme label="b" type="m" corresp="#s7:l2">pyn</rhyme></l>
        <l id="s7:l5" met="3" type="-+">myne gonst soeldi <rhyme label="c" type="f">vercryghen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">wildi ghetrouwe <rhyme label="d" type="m">syn</rhyme></l>
        <l id="s7:l7" met="3" type="-+">soe soele ghi bi my <rhyme label="c" type="f" corresp="#s7:l5">blyuen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">vroude sal v eyghen <rhyme label="d" type="m" corresp="#s7:l6">syn</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O ihesus ghemynde <rhyme label="a" type="f">here</rhyme></l>
        <l id="s8:l2" met="3" type="-+">ghi hebt my wal <rhyme label="b" type="m">gheleert</rhyme></l>
        <l id="s8:l3" met="3" type="-+">myn harte dat wil ic <rhyme label="a" type="f" corresp="#s8:l1">kieren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">tot al dat ghi <rhyme label="b" type="m" corresp="#s8:l2">beghert</rhyme></l>
        <l id="s8:l5" met="3" type="-+">seer vrentlick wil ic v <rhyme label="c" type="f">draeghen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">al yn dat harte <rhyme label="d" type="m">myn</rhyme></l>
        <l id="s8:l7" met="3" type="-+">nae v is al myn <rhyme label="c" type="f" corresp="#s8:l5">iaeghen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">laet vroude myn eyghen <rhyme label="d" type="m" corresp="#s8:l6">syn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O mensche myn lieflike <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">nu leuet yn goeden <rhyme label="b" type="m">haepe</rhyme></l>
        <l id="s9:l3" met="3" type="-+">daer lichter soe menych <rhyme label="a" type="f" corresp="#s9:l1">gheuanghen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">die naemels woert <rhyme label="b" type="m" corresp="#s9:l2">verloest</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Weest hoeues yn uwen <rhyme label="c" type="f">woerden</rhyme></l>
        <l id="s9:l6" met="3" type="-+">v liden en claeghet nyemant dan <rhyme label="d" type="m">my</rhyme></l>
        <l id="s9:l7" met="3" type="-+">troest soldi aen my <rhyme label="c" type="f" corresp="#s9:l5">vynden</rhyme></l>
        <l id="s9:l8" met="3" type="-+">nu ende tot alre <rhyme label="d" type="m" corresp="#s9:l6">tyt</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ic dancke v ihesus <rhyme label="a" type="f">here</rhyme></l>
        <l id="s10:l2" met="3" type="-+">van uwer mynnen <rhyme label="b" type="m">groet</rhyme></l>
        <l id="s10:l3" met="3" type="-+">laet my uwen troest <rhyme label="a" type="f" corresp="#s10:l1">verwaruen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">helpet my wt alre <rhyme label="b" type="m" corresp="#s10:l2">noet</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Dat ic doch mach <rhyme label="c" type="f">aenschouwen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">v vrentlike <rhyme label="d" type="m">aenschyn</rhyme></l>
        <l id="s10:l7" met="3" type="-+">boeuen alle reyne <rhyme label="c" type="f" corresp="#s10:l5">vrouwen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">maria die moeder <rhyme label="d" type="m" corresp="#s10:l6">dyn</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wat mach my nu <rhyme label="a" type="f">bezwiken</rhyme></l>
        <l id="s11:l2" met="3" type="-+">myn lief heeft my <rhyme label="b" type="m">verblit</rhyme></l>
        <l id="s11:l3" met="3" type="-+">doch wil ic van hem <rhyme label="a" type="f" corresp="#s11:l1">schryuen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">nu ende tot alre <rhyme label="b" type="m" corresp="#s11:l2">tyt</rhyme></l>
        <l id="s11:l5" met="3" type="-+">myt synre soeter <rhyme label="c" type="f">stralen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">heeft hi myn herte <rhyme label="d" type="m">ghewont</rhyme></l>
        <l id="s11:l7" met="3" type="-+">ic beuele my hem alte <rhyme label="c" type="f" corresp="#s11:l5">maele</rhyme></l>
        <l id="s11:l8" met="3" type="-+">al yn syns  harten <rhyme label="d" type="m" corresp="#s11:l6">gront</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
