  <author>Albertus Ignatius d'Hanins</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WAt weet Cupîdo Godt der <rhyme label="a" type="m">minn</rhyme>’</l>
        <l id="s1:l2" met="3" type="-+">Al droefheyt en <rhyme label="b" type="f">ellenden</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Dagh en nacht te <rhyme label="b" type="f" corresp="#s1:l2">wenden</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Op des minnaers <rhyme label="a" type="m" corresp="#s1:l1">sin</rhyme>!</l>
        <l id="s1:l5" met="3" type="+-">Hoe doet hy ons <rhyme label="c" type="f">duchten</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Hoe doet hy ons <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Met tranen en met <rhyme label="c" type="f" corresp="#s1:l5">suchten</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Meer en meer <rhyme label="d" type="m" corresp="#s1:l6">belaen</rhyme>!</l>
        <l id="s1:l9" met="3" type="-+">Wie kan hem <rhyme label="d" type="m" corresp="#s1:l6">weder-staen</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Men seght het Wichtjen is heel <rhyme label="a" type="m">blindt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">’t En maeckt gheen droeve <rhyme label="b" type="f">wonden</rhyme></l>
        <l id="s2:l3" met="3" type="+-">(Wesende <rhyme label="b" type="f" corresp="#s2:l2">verbonden</rhyme>)</l>
        <l id="s2:l4" met="3" type="+-">’t Kleyne <rhyme label="a" type="m" corresp="#s2:l1">Venus-kindt</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Ick seggh’ dat sijn’ <rhyme label="c" type="f">schichten</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Treffen wonder <rhyme label="d" type="m">fel</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En dat hy all’ sijn’ <rhyme label="c" type="f" corresp="#s2:l5">lichten</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Heeft tot sijn <rhyme label="d" type="m" corresp="#s2:l6">bevel</rhyme>:</l>
        <l id="s2:l9" met="3" type="-+">Mijn hertjen voelt’et <rhyme label="d" type="m" corresp="#s2:l6">wel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy heeft mijn anghstigh hert’ <rhyme label="a" type="m">ghewondt</rhyme>;</l>
        <l id="s3:l2" met="3" type="-+">Maer ghy, o <rhyme label="b" type="f">Amatantjen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Kont ’t ghetreft <rhyme label="b" type="f" corresp="#s3:l2">Galantjen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Maken we’er <rhyme label="a" type="m" corresp="#s3:l1">ghesondt</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Ghy kont my <rhyme label="c" type="f">ghenesen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Door een soet <rhyme label="d" type="m">ghesicht</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Ghy kont mijn’ helpster <rhyme label="c" type="f" corresp="#s3:l5">wesen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Door een <rhyme label="d" type="m" corresp="#s3:l6">minne-plicht</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Wordt heel mijn hert’ <rhyme label="d" type="m" corresp="#s3:l6">verlicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Segt schoonste Nymph’ waer-aen het <rhyme label="a" type="m">schort</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat ghy uw’ <rhyme label="b" type="f">lodder-ooghen</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Door een soet <rhyme label="b" type="f" corresp="#s4:l2">med’-dooghen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Op my niet en <rhyme label="a" type="m" corresp="#s4:l1">stort</rhyme>?</l>
        <l id="s4:l5" met="3" type="+-">Heeft mijn trouwigh <rhyme label="c" type="f">minnen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Dijnen haet <rhyme label="d" type="m">verdint</rhyme>?</l>
        <l id="s4:l7" met="3" type="-+">Och neen, verkeert uw’ <rhyme label="c" type="f" corresp="#s4:l5">sinnen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Weest tot my <rhyme label="d" type="m" corresp="#s4:l6">ghesint</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En mindt die u <rhyme label="d" type="m" corresp="#s4:l6">bemint</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Cupîdo oorsaeck van mijn <rhyme label="a" type="m">smert</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">‘k Sal dijnen lof <rhyme label="b" type="f">verheffen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Soo ghy oock wilt <rhyme label="b" type="f" corresp="#s5:l2">treffen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Amarantjens <rhyme label="a" type="m" corresp="#s5:l1">hert</rhyme>’:</l>
        <l id="s5:l5" met="3" type="+-">‘k Sal u all’ mijn <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Door mijn’ soete <rhyme label="d" type="m">Lier</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Lof, prijs, en eere <rhyme label="c" type="f" corresp="#s5:l5">gheven</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Soo ghy door uw’ <rhyme label="d" type="m" corresp="#s5:l6">vier</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Ontsteeckt dit killigh <rhyme label="d" type="m" corresp="#s5:l6">dier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
