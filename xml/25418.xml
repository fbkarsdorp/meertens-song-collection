  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Van nyen zender <rhyme label="a" type="m">arbeyt</rhyme></l>
        <l id="s1:l2" met="4" type="+-">moyz ich leyder trurich <rhyme label="b" type="m">ziin</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Mir west iamer unde <rhyme label="a" type="m" corresp="#s1:l1">leyt</rhyme></l>
        <l id="s1:l4" met="4" type="+-">durch die liefste vrouwe <rhyme label="b" type="m" corresp="#s1:l2">mijn</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Ydoch zo yst zi mijn hoyste <rhyme label="c" type="m">oyrt</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Zi ist zo goet, zi helft <rhyme label="d" type="m">noch</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">in den wane leve ich <rhyme label="c" type="m" corresp="#s1:l5">voert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Of ich ie ghewan dem <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">das ir ere werde <rhyme label="b" type="m">cranch</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Tswaer, zo ne ghesciet mir nemmer <rhyme label="a" type="m" corresp="#s2:l1">goet</rhyme></l>
        <l id="s2:l4" met="4" type="+-">onde ich diene yr ane <rhyme label="b" type="m" corresp="#s2:l2">danch</rhyme>!</l>
        <l id="s2:l5" met="4" type="-+">Mir gheyt dii tsiit mit leide <rhyme label="c" type="m">hyn</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">das zi mit cleynen dinghen <rhyme label="d" type="m">wol</rhyme></l>
        <l id="s2:l7" met="4" type="+-">wende in vreuden riicher <rhyme label="c" type="m" corresp="#s2:l5">sciin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">God g√únne mir, des ich yr <rhyme label="a" type="m">gan</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">nicht mer wensch mine <rhyme label="b" type="m">daghe</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Mucht ych eyn gruezen van yr <rhyme label="a" type="m" corresp="#s3:l1">haen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">so verwonne ich mine <rhyme label="b" type="m" corresp="#s3:l2">claghe</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Weer ich die lieve noch zo <rhyme label="c" type="m">has</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">ich wolde yr dienen miin <rhyme label="d" type="m">iaer</rhyme></l>
        <l id="s3:l7" met="4" type="+-">und wenschen yr ye <rhyme label="c" type="m" corresp="#s3:l5">bas</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
