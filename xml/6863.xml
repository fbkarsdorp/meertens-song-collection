    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Ave Maria ghy Hemels <rhyme label="a" type="m">Koningin</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Aller werelt een <rhyme label="a" type="m" corresp="#s1:l1">troosterin</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Heyligh en saligh sijt ghy <rhyme label="b" type="m">geboren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dan Godt die heeft u <rhyme label="b" type="m" corresp="#s1:l3">wtverkoren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wt allen Ionckvrouwen <rhyme label="c" type="f">reyne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Ave Maria ghy edle <rhyme label="a" type="m">Keyserin</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ghy sijt onse <rhyme label="a" type="m" corresp="#s2:l1">voorsprekerin</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Heyligh en saligh spreeckt u alle <rhyme label="b" type="m">werelt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van Godt sijt ghy wel <rhyme label="b" type="m" corresp="#s2:l3">wtgetelt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Een Moeder des aller <rhyme label="c" type="f">hoochsten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Ave Maria ghy schoone <rhyme label="a" type="m">Roosengaerdt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ghy wel gezierde Ionckvrou <rhyme label="a" type="m" corresp="#s3:l1">zart</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Heyligh en saligh zijt ghy <rhyme label="b" type="m">genamt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">V hebben die Propheten <rhyme label="b" type="m" corresp="#s3:l3">eerkandt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van u hebben sy <rhyme label="c" type="f">geschreven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ave Maria ghy lof der <rhyme label="a" type="m">Christenheyt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Een Moeder der <rhyme label="a" type="m" corresp="#s4:l1">bermhertigheyt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ghy Godes stadt, ghy klare <rhyme label="b" type="m">Son</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ghy stercke thoorn, ghy Iacobs <rhyme label="b" type="m" corresp="#s4:l3">bron</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Een Vrouw der gantser <rhyme label="c" type="f">eerden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Ave Maria ghy Tempel <rhyme label="a" type="m">Salomons</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ghy gulden vel des <rhyme label="a" type="m" corresp="#s5:l1">Gedeons</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ghy sijt een spiegel der <rhyme label="b" type="m">gerechtigheyt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ghy sijt de vorm der <rhyme label="b" type="m" corresp="#s5:l3">reynigheyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Een Roos sonder alle <rhyme label="c" type="f">doornen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Ave Maria ghy hooge <rhyme label="a" type="m">Cederboom</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ghy schoone Lely, ghy Veldes <rhyme label="a" type="m" corresp="#s6:l1">bloem</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ghy sijt die beslotene <rhyme label="b" type="m">poort</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Bidt Godt voor ons int Hemels <rhyme label="b" type="m" corresp="#s6:l3">fort</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat wy alle salich <rhyme label="c" type="f">werden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Ave Maria singen wy van herten <rhyme label="a" type="m">gront</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">V loven wy met hert en <rhyme label="a" type="m" corresp="#s7:l1">mont</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Heylich en saligh, u spreken wy yet <rhyme label="b" type="m">zondt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Staet ons by in den lesten <rhyme label="b" type="m" corresp="#s7:l3">stont</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Want wy van hier sullen <rhyme label="c" type="f">scheyden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
