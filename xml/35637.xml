    <melody>Wees nimf gegroet</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ghedenckt toch <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Te gaen al na den <rhyme label="a" type="m" corresp="#s1:l1">stal</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Den hemelschen Coridon is <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">De lieve Engheltjens singhen <rhyme label="b" type="m" corresp="#s1:l3">claer</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Hun stemmen <rhyme label="c" type="m">me</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Bootschappen oock den <rhyme label="c" type="m" corresp="#s1:l5">vre</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Al aende Herders van dat <rhyme label="d" type="m">landt</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Dat sy samen <rhyme label="d" type="m" corresp="#s1:l7">ghelijcker-handt</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Souden gaen groeten met al <rhyme label="e" type="m">ootmoet</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">En vallen dat soet Kindt te <rhyme label="e" type="m" corresp="#s1:l9">voet</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Dat daer leydt inde cribbe soo <rhyme label="f" type="m">bloot</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">In arme doecxkens en in groot <rhyme label="f" type="m" corresp="#s1:l11">noot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">O grootsten <rhyme label="a" type="m">al</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Wie isser in dit <rhyme label="a" type="m" corresp="#s2:l1">dal</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die can begrijpen uwen <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ofte verstaen dees wonder <rhyme label="b" type="m" corresp="#s2:l3">min</rhyme>?</l>
        <l id="s2:l5" met="2" type="-+">D'hemelsche <rhyme label="c" type="m">cracht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Leydt hier als sonder <rhyme label="c" type="m" corresp="#s2:l5">macht</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Die de winden, en vloeden <rhyme label="d" type="m">drijft</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Leydt hier van coude soo <rhyme label="d" type="m" corresp="#s2:l7">verstijft</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Comt alle versteende hert van <rhyme label="e" type="m">aert</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">En weest toch van gheen kintjen <rhyme label="e" type="m" corresp="#s2:l9">vervaert</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Sijn traentjens locken u tot den <rhyme label="f" type="m">soen</rhyme>,</l>
        <l id="s2:l12" met="4" type="-+">Hy is den ghene diet voor u sal <rhyme label="f" type="m" corresp="#s2:l11">doen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Werckt ghy maer <rhyme label="a" type="m">me</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Men hoort het is al <rhyme label="a" type="m" corresp="#s3:l1">vre</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Voor die van goeden wille <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Is te gheven Godts grati <rhyme label="b" type="m" corresp="#s3:l3">fijn</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Hebdy gheen <rhyme label="c" type="m">macht</rhyme>?</l>
        <l id="s3:l6" met="3" type="-+">Dit kindt dat is vol <rhyme label="c" type="m" corresp="#s3:l5">cracht</rhyme>,?</l>
        <l id="s3:l7" met="4" type="-+">Wildy maer wesen sijns <rhyme label="d" type="m">ghelijck</rhyme>?</l>
        <l id="s3:l8" met="4" type="-+">Hy sal u maken eeuwigh <rhyme label="d" type="m" corresp="#s3:l7">rijck</rhyme>.</l>
        <l id="s3:l9" met="4" type="-+">Comt om den sondaer die afstant <rhyme label="e" type="m">doet</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Begeert hy te gheven d'eeuwigh <rhyme label="e" type="m" corresp="#s3:l9">goet</rhyme></l>
        <l id="s3:l11" met="4" type="+-">En wilt dat wy door liefde sijn <rhyme label="f" type="m">een</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">Malcand'ren beminnen om hem <rhyme label="f" type="m" corresp="#s3:l11">alleen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">D'opperste <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Leert ons rechte <rhyme label="a" type="m" corresp="#s4:l1">ootmoet</rhyme>,'</l>
        <l id="s4:l3" met="4" type="-+">d'Woort wort vleesch door't 's vaders <rhyme label="b" type="m">bevel</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">De liefde werckt dit wonder <rhyme label="b" type="m" corresp="#s4:l3">spel</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Hebdy <rhyme label="c" type="m">gheloof</rhyme>?</l>
        <l id="s4:l6" met="3" type="-+">Hoe sijdy dan soo <rhyme label="c" type="m" corresp="#s4:l5">doof</rhyme>?</l>
        <l id="s4:l7" met="4" type="-+">Oft dunckt u dat is een apen <rhyme label="d" type="m">spel</rhyme>?</l>
        <l id="s4:l8" met="4" type="-+">Neen, neen, hoort toe en luystert <rhyme label="d" type="m" corresp="#s4:l7">wel</rhyme>.</l>
        <l id="s4:l9" met="4" type="-+">Dat woort is Godt, en Godt is dat <rhyme label="e" type="m">woort</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">En dat en brenght niet als waerheyt <rhyme label="e" type="m" corresp="#s4:l9">voort</rhyme>:</l>
        <l id="s4:l11" met="4" type="+-">Hemel ende aerde sal voorby <rhyme label="f" type="m">gaen</rhyme>,</l>
        <l id="s4:l12" met="4" type="-+">Maer Godts woorden blijven altijt <rhyme label="f" type="m" corresp="#s4:l11">staen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Mijn siel t'is <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat ghy sonder <rhyme label="a" type="m" corresp="#s5:l1">respijt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Eens de bijl aende wortel <rhyme label="b" type="m">stelt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En doet u quaey natuer <rhyme label="b" type="m" corresp="#s5:l3">ghewelt</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Keert maer u <rhyme label="c" type="m">oogh</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Tot den Heere om <rhyme label="c" type="m" corresp="#s5:l5">hoogh</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Sijn cracht sal in u dalen <rhyme label="d" type="m">ras</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">t'Comt hem tot alder uer te <rhyme label="d" type="m" corresp="#s5:l7">pas</rhyme></l>
        <l id="s5:l9" met="4" type="-+">Te wercken met u: hout u maer <rhyme label="e" type="m">stil</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Verloochent alleen den eyghen <rhyme label="e" type="m" corresp="#s5:l9">wil</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Van lusten, begeerte wilt u <rhyme label="f" type="m">ontslaen</rhyme>,</l>
        <l id="s5:l12" met="4" type="-+">U om de liefde te buyten <rhyme label="f" type="m" corresp="#s5:l11">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
