  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IK geen Apollus in myn <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ook geen rede of zeden <rhyme label="b" type="f">staven</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dog prysen de soete <rhyme label="c" type="m">Jeugt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die sig wat vrolyk konnen <rhyme label="d" type="f">maken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Insonderheid als <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Op 't eene of 't andere <rhyme label="b" type="f">Feest</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat dan den Geest der <rhyme label="c" type="m">Jeugt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Eens vrolyk en Helder <rhyme label="d" type="f">leest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En dat se dan als <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Eens luchtig omme <rhyme label="b" type="f">springt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En rykelyk Hand aan <rhyme label="c" type="m">Hand</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In 's Werelds ronde <rhyme label="d" type="f">Kring</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En dan eens syt <rhyme label="a" type="m">verblyt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Met haar behoorlyk <rhyme label="b" type="f">lot</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Het syn Maagt of Knegten <rhyme label="c" type="m">Rey</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">In deses eer grasige <rhyme label="d" type="f">Wey</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dit is 't geen my <rhyme label="a" type="m">behaagt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dog ook niet alle <rhyme label="b" type="f">daag</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ja hier in 's Werelds <rhyme label="c" type="m">plek</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Heb ik ook myn <rhyme label="d" type="f">bestek</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ik houde het met die <rhyme label="a" type="m">Helden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die sig als Helden <rhyme label="b" type="f">telden</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dan als de soete <rhyme label="c" type="m">Jeugt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Eens weet waar om se sig <rhyme label="d" type="f">verheugt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dan, weest dan lustig <rhyme label="a" type="m">vrolyk</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En dat dan de soete <rhyme label="b" type="f">tyt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">U Helden maakt <rhyme label="c" type="m">verblyd</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En so ter eynde glyt en <rhyme label="d" type="f">slyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
