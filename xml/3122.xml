<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3122</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Cette cruelle</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">BEroemt Breda, ghy hooft-stadt van de <rhyme label="a" type="f">Steden</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Gelegen in het schoonste van het <rhyme label="b" type="m">Landt</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Brabantsche pronck men heeft om u <rhyme label="a" type="f" corresp="#s1:l1">ghestreden</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Van 't eerste dat de vryheyt wiert <rhyme label="b" type="m" corresp="#s1:l2">geplandt</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Nu komt <rhyme label="c" type="f">Oraengjen</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Wil u maken <rhyme label="d" type="m">vry</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Van 't Jock van <rhyme label="c" type="f" corresp="#s1:l5">Spaengjen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">En van <rhyme label="d" type="m" corresp="#s1:l6">slaverny</rhyme></l>
        <l id="s1:l9" met="3" type="-+">En eyst zijn <rhyme label="d" type="m" corresp="#s1:l6">Baronny</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Ghy zijt sijn erf en hy u Erf-Heer <rhyme label="a" type="f">moedigh</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Verslaet u niet als ghy hem hooren <rhyme label="b" type="m">komt</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Hy draeght met Mars den blancken degen <rhyme label="a" type="f" corresp="#s2:l1">bloedigh</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Waer op den Leeu van Hollandt stadig <rhyme label="b" type="m" corresp="#s2:l2">bromt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Hy sal <rhyme label="c" type="f">wallen</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Van de Stadt <rhyme label="d" type="m">Breda</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Haest neer doen <rhyme label="c" type="f" corresp="#s2:l5">vallen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">En u komen <rhyme label="d" type="m" corresp="#s2:l6">na</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Maer niet tot Burgers <rhyme label="d" type="m" corresp="#s2:l6">scha</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Ons Prince sal den Spaensche Nest <rhyme label="a" type="f">omroeren</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Dat derthien Jaren u gedwongen <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">En in u plaets de vryheyts-Standert <rhyme label="a" type="f" corresp="#s3:l1">voerren</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Waer door sijn fame tot de Wolcken <rhyme label="b" type="m" corresp="#s3:l2">sweeft</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Want u <rhyme label="c" type="f">Maranen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Zijn hem wel <rhyme label="d" type="m">gewent</rhyme></l>
        <l id="s3:l7" met="2" type="-+">By d' <rhyme label="c" type="f" corresp="#s3:l5">Indianen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Tot aen 's Werelt <rhyme label="d" type="m" corresp="#s3:l6">ent</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Daer is sijn Naem <rhyme label="d" type="m" corresp="#s3:l6">bekent</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ons Bat'rijen, Reduyten en <rhyme label="a" type="f">Loopgraven</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Die zijn nu vaerdigh, en haest <rhyme label="b" type="m">op-gemaeckt</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Het grof geschut de paerden vast me <rhyme label="a" type="f" corresp="#s4:l1">draven</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Of schoon het spaens Canon sijn vlamme <rhyme label="b" type="m" corresp="#s4:l2">braeckt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Haest hoort ghy <rhyme label="c" type="f">brallen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">'t Braef Hollansche <rhyme label="d" type="m">Kruyt</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Op uwe <rhyme label="c" type="f" corresp="#s4:l5">wallen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Dat het groot <rhyme label="d" type="m" corresp="#s4:l6">geluydt</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Tot binnen Spaengjen <rhyme label="d" type="m" corresp="#s4:l6">stuyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Maer spaenschen hoop wilt niet <rhyme label="a" type="f">verwond'ren</rhyme></l>
        <l id="s5:l2" met="5" type="-+">Of ghy met 't Oor eens inder aerden <rhyme label="b" type="m">lagh</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Of ghy 't Canon in Hollandt hoorde <rhyme label="a" type="f" corresp="#s5:l1">dond'ren</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Soo slaet een kruysje noch als 't wesen <rhyme label="b" type="m" corresp="#s5:l2">magh</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Loopt uyt u <rhyme label="c" type="f">Cellen</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Schelt en belt vry <rhyme label="d" type="m">aen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Wijd u <rhyme label="c" type="f" corresp="#s5:l5">Cappellen</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Schuert den <rhyme label="d" type="m" corresp="#s5:l6">Moorejaen</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Want 't is met u <rhyme label="d" type="m" corresp="#s5:l6">gedaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Den Paus sijn stoel die wil hem schier <rhyme label="a" type="f">begeven</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Sijn Jesuwijten, och! sy klouwent <rhyme label="b" type="m">Hooft</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">De Spaensche Kroon die schijnt van angst te <rhyme label="a" type="f" corresp="#s6:l1">beven</rhyme></l>
        <l id="s6:l4" met="5" type="-+">Mits hem een schoone Parel wert <rhyme label="b" type="m" corresp="#s6:l2">berooft</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Roemt dan <rhyme label="c" type="f">Oraengjen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Geeft sijn Hoogheyt <rhyme label="d" type="m">Lof</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Die 't trotse <rhyme label="c" type="f" corresp="#s6:l5">Spaengjen</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Snoeyt sijn wiecken <rhyme label="d" type="m" corresp="#s6:l6">of</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En smijtse in het <rhyme label="d" type="m" corresp="#s6:l6">stof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
