<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157916</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Jonkmans wil niet ver lopen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Jonkmans wilt niet ver <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Om Meisjes van <rhyme label="b" type="m">plaisier</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Men vind 'er heele <rhyme label="a" type="f" corresp="#s1:l1">hoopen</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Al na de nieuwste <rhyme label="b" type="m" corresp="#s1:l2">zwier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zy draegen een Hoed met <rhyme label="a" type="f">kanten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Een Muts met Lint zeer <rhyme label="b" type="m">net</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Oorbellen met <rhyme label="a" type="f" corresp="#s2:l1">Briljanten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Heel sierlyk <rhyme label="b" type="m" corresp="#s2:l2">bezet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Haer Iakken lang van <rhyme label="a" type="f">schooten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van agteren met een <rhyme label="b" type="m">tip</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hofmouwen, als de <rhyme label="a" type="f" corresp="#s3:l1">Grooten</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Den Halsdoek lang van <rhyme label="b" type="m" corresp="#s3:l2">slip</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Keurslyven zy ook <rhyme label="a" type="f">draegen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wat breet en laeg van <rhyme label="b" type="m">Borst</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Zy doenen niet als <rhyme label="a" type="f" corresp="#s4:l1">klaegen</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">Al van de Minne <rhyme label="b" type="m" corresp="#s4:l2">dorst</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Boeslaers met zyde <rhyme label="a" type="f">Banden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Een Ketting om haer <rhyme label="b" type="m">Hals</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En schuiven op haer <rhyme label="a" type="f" corresp="#s5:l1">Handen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van Goud; maer veeltyds <rhyme label="b" type="m" corresp="#s5:l2">vals</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Beulingen en <rhyme label="a" type="f">Punthaken</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Draegen zy na de <rhyme label="b" type="m">zwier</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Men ziet op haere <rhyme label="a" type="f" corresp="#s6:l1">kaeken</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Het lonkent <rhyme label="b" type="m" corresp="#s6:l2">Minnevier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Rokken, van agter en <rhyme label="a" type="f">vooren</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wat korter als op <rhyme label="b" type="m">Zy</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Om Ionkmans te <rhyme label="a" type="f" corresp="#s7:l1">bekooren</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En brengen in de <rhyme label="b" type="m" corresp="#s7:l2">Ley</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Kouzen met zyde <rhyme label="a" type="f">Klinken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En schoentjes na de <rhyme label="b" type="m">trant</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Gespes die 'er op <rhyme label="a" type="f" corresp="#s8:l1">blinken</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die zyn ront of <rhyme label="b" type="m" corresp="#s8:l2">Vierkant</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Zoo gaen de Meisjes <rhyme label="a" type="f">zwieren</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En stellen haer ten <rhyme label="b" type="m">toon</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Lonken in hun <rhyme label="a" type="f" corresp="#s9:l1">maniere</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Om meenig <rhyme label="b" type="m" corresp="#s9:l2">Manspersoon</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wilt gy haer streeken <rhyme label="a" type="f">leeren</rhyme>;</l>
        <l id="s10:l2" met="3" type="-+">Ionkmans doet dan als <rhyme label="b" type="m">ik</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Gaet 's maendags Lappies <rhyme label="a" type="f" corresp="#s10:l1">keeren</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En let eens op 't <rhyme label="b" type="m" corresp="#s10:l2">stik</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Hoe dat zy daer gaen <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Zeer dikwils heên en <rhyme label="b" type="m">weêr</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Het is niet om te <rhyme label="a" type="f" corresp="#s11:l1">koopen</rhyme>;</l>
        <l id="s11:l4" met="3" type="-+">Maer om een <rhyme label="b" type="m" corresp="#s11:l2">Mentineur</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Doch alle Ionge <rhyme label="a" type="f">Meisjes</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Die trekken 't haer niet <rhyme label="b" type="m">aen</rhyme>;</l>
        <l id="s12:l3" met="3" type="-+">Hoort nu nog maer een <rhyme label="a" type="f" corresp="#s12:l1">reisje</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Wat ik u zal gaen <rhyme label="b" type="m" corresp="#s12:l2">raân</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Kleet u zeer fraay in <rhyme label="a" type="f">kleeren</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Zoo net als gy <rhyme label="b" type="m">begeert</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Wagt u voor Ionkmans <rhyme label="a" type="f" corresp="#s13:l1">Eeden</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Dan word gy niet <rhyme label="b" type="m" corresp="#s13:l2">onteerd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
