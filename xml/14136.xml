  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ALmachtich God <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Eeuwich <rhyme label="b" type="m">ghebenedijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wilt my doch sterckheyt <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In desen benauden <rhyme label="b" type="m" corresp="#s1:l2">tijdt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En in so groot <rhyme label="c" type="f">verdriet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Want ick zijn int <rhyme label="d" type="m">benauwen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wilt my met uwen geest <rhyme label="c" type="f" corresp="#s1:l5">bedauwen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En my genadich <rhyme label="d" type="m" corresp="#s1:l6">aensien</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In Godts vreese te <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hebbe ick altijt <rhyme label="b" type="m">bedacht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wilt my u gracie <rhyme label="a" type="f" corresp="#s2:l1">geven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat bidde ick uyt al mijn <rhyme label="b" type="m" corresp="#s2:l2">cracht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">O Godt wilt my doch <rhyme label="c" type="f">stercken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Al met u woort <rhyme label="d" type="m">ydoon</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En crachtich in my <rhyme label="c" type="f" corresp="#s2:l5">wercken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat ick ontfanghe de <rhyme label="d" type="m" corresp="#s2:l6">croon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O Heere verhoort mijn <rhyme label="a" type="f">kermen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En al mijn swaer <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O Heere wilt my <rhyme label="a" type="f" corresp="#s3:l1">ontfermen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Bidde ick u in dit <rhyme label="b" type="m" corresp="#s3:l2">Liet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die vroom begheert te <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Bidt Godt dach ende <rhyme label="d" type="m">nacht</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Om die waerheyt <rhyme label="c" type="f" corresp="#s3:l5">verheven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Al deur des Geests <rhyme label="d" type="m" corresp="#s3:l6">cracht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick roepe tot den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al met den <rhyme label="b" type="m">Publycaen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat ware wel mijn <rhyme label="a" type="f" corresp="#s4:l1">begeere</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Mocht hier haest zijn <rhyme label="b" type="m" corresp="#s4:l2">ghedaen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Als ick verlost zijn uyt dit <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van mijn ellende <rhyme label="d" type="m">swaer</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wilt my dan ruste <rhyme label="c" type="f" corresp="#s4:l5">geven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Al onder den <rhyme label="d" type="m" corresp="#s4:l6">Autaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O Heere wilt my <rhyme label="a" type="f">verhooren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Al met de <rhyme label="b" type="m">Moordenaer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En laet my niet <rhyme label="a" type="f" corresp="#s5:l1">verlooren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">My arm ellendich <rhyme label="b" type="m" corresp="#s5:l2">sondaer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wilt my genade <rhyme label="c" type="f">bewijsen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Al door die goetheyt <rhyme label="d" type="m">dijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Als ick sal moeten <rhyme label="c" type="f" corresp="#s5:l5">verrijsen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Verlost my vande <rhyme label="d" type="m" corresp="#s5:l6">pijn</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O Heere zijt my <rhyme label="a" type="f">geduldich</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met u onnutte knecht <rhyme label="b" type="m">coen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die u thien Pont is <rhyme label="a" type="f" corresp="#s6:l1">schuldigh</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En hebbe niet om te <rhyme label="b" type="m" corresp="#s6:l2">voldoen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Maer wiltse my <rhyme label="c" type="f">vergeven</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En mijn ghenadich <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat ick mach worden <rhyme label="c" type="f" corresp="#s6:l5">verheven</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">In u glorie <rhyme label="d" type="m" corresp="#s6:l6">fijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ick comme met <rhyme label="a" type="f">beswaren</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Tot u Heere mijn <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wilt my wel <rhyme label="a" type="f" corresp="#s7:l1">bewaren</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Voor mijn vyanden <rhyme label="b" type="m" corresp="#s7:l2">bodt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dat sy my niet en <rhyme label="c" type="f">verrasschen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">In haren grimmen <rhyme label="d" type="m">moet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Haer handen niet en <rhyme label="c" type="f" corresp="#s7:l5">wasschen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">In mijn ontschuldich <rhyme label="d" type="m" corresp="#s7:l6">bloet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Vreest niet mijn <rhyme label="a" type="f">Wtvercooren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Al wordt ghy hier <rhyme label="b" type="m">benijdt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ick sal u wel <rhyme label="a" type="f" corresp="#s8:l1">verhooren</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Als ghy tot my roept en <rhyme label="b" type="m" corresp="#s8:l2">crijdt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ick en sal u niet <rhyme label="c" type="f">begheven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Al verliet een Moeder haer <rhyme label="d" type="m">Kindt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Waer voor soo wilt ghy <rhyme label="c" type="f" corresp="#s8:l5">beven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Als ghy genade <rhyme label="d" type="m" corresp="#s8:l6">vindt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ghy moet noch leeren <rhyme label="a" type="f">strijden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Met my teghen dat <rhyme label="b" type="m">Beest</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Maer ick sal u <rhyme label="a" type="f" corresp="#s9:l1">bevrijden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En wilt niet zijn <rhyme label="b" type="m" corresp="#s9:l2">bevreest</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Al gaetse haer <rhyme label="c" type="f">verblyden</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Met haren grooten <rhyme label="d" type="m">boel</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Ick salse noch doen <rhyme label="c" type="f" corresp="#s9:l5">lyden</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Inden vierighen <rhyme label="d" type="m" corresp="#s9:l6">poel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Als sy nu is <rhyme label="a" type="f">verdreven</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">In de eeuwighe pijne <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Dan sult ghy zijn <rhyme label="a" type="f" corresp="#s10:l1">verheven</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Met Lasarus in Abrahams <rhyme label="b" type="m" corresp="#s10:l2">schoot</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Als dan sullense <rhyme label="c" type="f">rasen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Deur haren valschen <rhyme label="d" type="m">list</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Wy uytsinnighe <rhyme label="c" type="f" corresp="#s10:l5">dwasen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Hebben den rechten wegh <rhyme label="d" type="m" corresp="#s10:l6">gemist</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Sy sullen tot u comen <rhyme label="a" type="f">gheboghen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Die u hier hebben <rhyme label="b" type="m">verdruckt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Ghelastert ende <rhyme label="a" type="f" corresp="#s11:l1">beloghen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Die werden wech <rhyme label="b" type="m" corresp="#s11:l2">gheruckt</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Die sullen u vallen te <rhyme label="c" type="f">voete</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En nomen des Heeren <rhyme label="d" type="m">stadt</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">De heylighe in Israel <rhyme label="c" type="f" corresp="#s11:l5">soete</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">En een uytvercoren <rhyme label="d" type="m" corresp="#s11:l6">vat</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">De Heere sal u <rhyme label="a" type="f">gheven</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Eenen nieuwen Name <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Als Kinderen <rhyme label="a" type="f" corresp="#s12:l1">verheven</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Sult ghy ghecroont <rhyme label="b" type="m" corresp="#s12:l2">zijn</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Al inde hant des <rhyme label="c" type="f">Heeren</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Eenen Conelicken <rhyme label="d" type="m">hoet</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">V en sal geen leet meer <rhyme label="c" type="f" corresp="#s12:l5">deeren</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Hier om hebt goeden <rhyme label="d" type="m" corresp="#s12:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Op Syons Berch <rhyme label="a" type="f">ghepreesen</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Daer sult ghy zijn <rhyme label="b" type="m">gheloont</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Soo wy in Esdras <rhyme label="a" type="f" corresp="#s13:l1">leesen</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Vanden Jonghelinck <rhyme label="b" type="m" corresp="#s13:l2">ghecroont</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Met Palmen Tacken inde <rhyme label="c" type="f">hant</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Een croone op u <rhyme label="d" type="m">hooft</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Ghy en sult niet comen tot <rhyme label="c" type="f" corresp="#s13:l5">schanden</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Ist dat ghy vast en <rhyme label="d" type="m" corresp="#s13:l6">ghelooft</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">O eeuwich Godt <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">V sy lof aen elcken <rhyme label="b" type="m">cant</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Dat ghy my hebt <rhyme label="a" type="f" corresp="#s14:l1">ghegheven</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Wijsheyt ende <rhyme label="b" type="m" corresp="#s14:l2">verstant</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Om de Draecke te <rhyme label="c" type="f">ontvliegen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Met haren valschen <rhyme label="d" type="m">schijn</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Sy meende my te <rhyme label="c" type="f" corresp="#s14:l5">bedrieghen</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">En te bringhen int <rhyme label="d" type="m" corresp="#s14:l6">ghepijn</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Die ons dit Liet eerst <rhyme label="a" type="f">dichten</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Die was van crachten <rhyme label="b" type="m">teer</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">O Heere wilt hem <rhyme label="a" type="f" corresp="#s15:l1">verlichten</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Deur uwe rechte <rhyme label="b" type="m" corresp="#s15:l2">Leer</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Dat hy u eere mach <rhyme label="c" type="f">bewijsen</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Heere mijn toe <rhyme label="d" type="m">verlaet</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">En uwen Name <rhyme label="c" type="f" corresp="#s15:l5">prijsen</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Inder waerheyt en inder <rhyme label="d" type="m" corresp="#s15:l6">daet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
