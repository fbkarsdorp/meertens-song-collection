<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>34421</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>christmas song</tag>
    </tags>
    <melody>O Jesu mi dulcissime spes et solamen animae</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O JESU alderhooghste <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Waer toe doch een soo diep <rhyme label="a" type="m" corresp="#s1:l1">oodtmoet</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Dat ghy verlaet het Vaeders <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En komt betaelen Adams <rhyme label="a" type="m" corresp="#s1:l1">boet</rhyme>!</l>
        <l id="s1:l5" met="4" type="+-">Komt ghy dan in eenen <rhyme label="b" type="m">stal</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s1:l5">val</rhyme>!</l>
        <l id="s1:l7" met="2" type="-+">Jn eenen <rhyme label="b" type="m" corresp="#s1:l5">stal</rhyme>!</l>
        <l id="s1:l8" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s1:l5">val</rhyme>!</l>
        <l id="s1:l9" met="2" type="-+">O Godt van <rhyme label="b" type="m" corresp="#s1:l5">al</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een Maeghet reyn heeft u <rhyme label="a" type="m">ontsaen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">(Soo is de Prophety <rhyme label="a" type="m" corresp="#s2:l1">voldaen</rhyme>)</l>
        <l id="s2:l3" met="4" type="-+">Die u ghebaert heeft sonder <rhyme label="a" type="m" corresp="#s2:l1">smert</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En in een kribb' gheleyt van <rhyme label="a" type="m" corresp="#s2:l1">bert</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Jn soo eenen armen <rhyme label="b" type="m">stal</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s2:l5">val</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Jn eenen <rhyme label="b" type="m" corresp="#s2:l5">stal</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s2:l5">val</rhyme>;</l>
        <l id="s2:l9" met="2" type="-+">O Godt van <rhyme label="b" type="m" corresp="#s2:l5">al</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hier light ghy als een arm <rhyme label="a" type="m">mensch</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die het al had naer uwen <rhyme label="a" type="m" corresp="#s3:l1">wensch</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Die d'aerd' en hemel heeft <rhyme label="a" type="m" corresp="#s3:l1">ghebouwt</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Leyt moeder-naeckt in winden <rhyme label="a" type="m" corresp="#s3:l1">kouwt</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Kiest hierom den armen <rhyme label="b" type="m">stal</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s3:l5">val</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">Den armen <rhyme label="b" type="m" corresp="#s3:l5">stal</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s3:l5">val</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">O Godt van <rhyme label="b" type="m" corresp="#s3:l5">al</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Onder de beesten light ghy <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Jn dese kouw' en sonder <rhyme label="a" type="m" corresp="#s4:l1">vier</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Heeft Bedleem de naeste <rhyme label="a" type="m" corresp="#s4:l1">stadt</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Voor u niet een kleyn plaets <rhyme label="a" type="m" corresp="#s4:l1">ghehadt</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Maer neen ghy wilt desen <rhyme label="b" type="m">stal</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s4:l5">val</rhyme>!</l>
        <l id="s4:l7" met="2" type="-+">Wilt desen <rhyme label="b" type="m" corresp="#s4:l5">stal</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s4:l5">val</rhyme></l>
        <l id="s4:l9" met="2" type="-+">O Godt van <rhyme label="b" type="m" corresp="#s4:l5">al</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">U doecxkens sijn van traentîens <rhyme label="a" type="m">nat</rhyme>;</l>
        <l id="s5:l2" met="4" type="-+">Waer toe weent ghy, ô edel <rhyme label="a" type="m" corresp="#s5:l1">vat</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">Jst niet om mijne sonden <rhyme label="a" type="m" corresp="#s5:l1">groodt</rhyme>?</l>
        <l id="s5:l4" met="4" type="-+">En my te helpen uyt den <rhyme label="a" type="m" corresp="#s5:l1">noodt</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Daerom komt ghy inden <rhyme label="b" type="m">stal</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s5:l5">val</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">K'omt g'in den <rhyme label="b" type="m" corresp="#s5:l5">stal</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s5:l5">val</rhyme>:</l>
        <l id="s5:l9" met="2" type="-+">O Godt van <rhyme label="b" type="m" corresp="#s5:l5">al</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Nu dan ô Jesu weent niet <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Jck sal u heeten mijnen <rhyme label="a" type="m" corresp="#s6:l1">Heer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Jck sal u loven met den <rhyme label="a" type="m" corresp="#s6:l1">sanck</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En dienen u mijn leven <rhyme label="a" type="m" corresp="#s6:l1">lanck</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Al light ghy in desen <rhyme label="b" type="m">stal</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s6:l5">val</rhyme>.</l>
        <l id="s6:l7" met="2" type="-+">Jn desen <rhyme label="b" type="m" corresp="#s6:l5">stal</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s6:l5">val</rhyme></l>
        <l id="s6:l9" met="2" type="-+">O Godt van <rhyme label="b" type="m" corresp="#s6:l5">al</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Siet Kindeken k'om met <rhyme label="a" type="m">oodtmoet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">My worpen hier voor u te <rhyme label="a" type="m" corresp="#s7:l1">voet</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">Wilt mijne sonden wasschen <rhyme label="a" type="m" corresp="#s7:l1">uyt</rhyme></l>
        <l id="s7:l4" met="4" type="-+">En nemt my aen voor uwe <rhyme label="a" type="m" corresp="#s7:l1">bruydt</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Daer-om komt ghy inden <rhyme label="b" type="m">stal</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s7:l5">val</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Jn eenen <rhyme label="b" type="m" corresp="#s7:l5">stal</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Om Adams <rhyme label="b" type="m" corresp="#s7:l5">val</rhyme></l>
        <l id="s7:l9" met="2" type="-+">O Godt van <rhyme label="b" type="m" corresp="#s7:l5">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
