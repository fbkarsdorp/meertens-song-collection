  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">U Zy de <rhyme label="a" type="f">eere</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Alleen ô! <rhyme label="a" type="f" corresp="#s1:l1">Heere</rhyme>:</l>
        <l id="s1:l3" met="2" type="-+">ô <rhyme label="b" type="f">Opper-Koning</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">In uwe <rhyme label="b" type="f" corresp="#s1:l3">woning</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">d' Alder <rhyme label="c" type="f">hoogste-stede</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Die u genade <rhyme label="d" type="m">toond</rhyme></l>
        <l id="s1:l7" met="2" type="-+">(Na uwen <rhyme label="c" type="f" corresp="#s1:l5">Ede</rhyme>)</l>
        <l id="s1:l8" met="3" type="+-">An den mensch <rhyme label="c" type="f" corresp="#s1:l5">benede</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">Die gy met heyl <rhyme label="d" type="m" corresp="#s1:l6">bekroond</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">En schenkt u <rhyme label="c" type="f" corresp="#s1:l5">vrede</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">U vreed' op <rhyme label="a" type="f">Aarden</rhyme>:</l>
        <l id="s2:l2" met="2" type="-+">Die sy <rhyme label="a" type="f" corresp="#s2:l1">an-vaarden</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">In haar <rhyme label="b" type="f">verstanden</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">En <rhyme label="b" type="f" corresp="#s2:l3">in-gewanden</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Neffens uwen <rhyme label="c" type="f">zegen</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Door desen <rhyme label="d" type="m">Vrede-Vorst</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">En Held <rhyme label="c" type="f" corresp="#s2:l5">verkregen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Als een dau, een <rhyme label="c" type="f" corresp="#s2:l5">Regen</rhyme>:</l>
        <l id="s2:l9" met="3" type="-+">Om die der <rhyme label="d" type="m" corresp="#s2:l6">zielen-dorst</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Te stellen <rhyme label="c" type="f" corresp="#s2:l5">tegen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">So zy de <rhyme label="a" type="f">menschen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Een billijk <rhyme label="a" type="f" corresp="#s3:l1">wenschen</rhyme>:</l>
        <l id="s3:l3" met="2" type="-+">Een <rhyme label="b" type="f">wel-behagen</rhyme></l>
        <l id="s3:l4" met="2" type="-+">In dese <rhyme label="b" type="f" corresp="#s3:l3">dagen</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Op dat sy u <rhyme label="c" type="f">loven</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Die haar (als in de <rhyme label="d" type="m">dood</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Van u <rhyme label="c" type="f" corresp="#s3:l5">verschoven</rhyme>)</l>
        <l id="s3:l8" met="3" type="+-">Nu verlost van <rhyme label="c" type="f" corresp="#s3:l5">boven</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">En so (als in u <rhyme label="d" type="m" corresp="#s3:l6">schoot</rhyme>)</l>
        <l id="s3:l10" met="2" type="-+">Bewaard voor 't <rhyme label="c" type="f" corresp="#s3:l5">roven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
