    <melody>Psalm 091 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIE in Godes bewaeringh' <rhyme label="a" type="m">sterck</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Begeeft hem <rhyme label="b" type="f">onbesweken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die woent in een vast <rhyme label="a" type="m" corresp="#s1:l1">bollewerck</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dies hy seer wel magh <rhyme label="b" type="f" corresp="#s1:l2">spreken</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">End segghen by hem t'aller <rhyme label="c" type="m">stondt</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Godt is myn hulpe <rhyme label="d" type="f">krachtigh</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Myn borght end oock myn vaste <rhyme label="c" type="m" corresp="#s1:l5">grondt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Daer ick op hop' <rhyme label="d" type="f" corresp="#s1:l6">eendrachtigh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Van des jaeghers valstricken <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">End van t'ghewelt seer <rhyme label="b" type="f">machtigh</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Van schaedelicke pesten <rhyme label="a" type="m" corresp="#s2:l1">sal</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy v oock helpen <rhyme label="b" type="f" corresp="#s2:l2">krachtigh</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Hy sal met den vlueghelen <rhyme label="c" type="m">fijn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">V decken end <rhyme label="d" type="f">bevryden</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Sijn hulp' sal v in noot end <rhyme label="c" type="m" corresp="#s2:l5">pyn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Een schildt sijn t'allen <rhyme label="d" type="f" corresp="#s2:l6">tyden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">S'nachts sult ghy dat dingh vreesen <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat die menschen doet <rhyme label="b" type="f">beuen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Noch die pylen die s'daeghs met <rhyme label="a" type="m" corresp="#s3:l1">vliet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gheschoten sijn <rhyme label="b" type="f" corresp="#s3:l2">daerneuen</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Oock voor gheen steruen scherp noch <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat t'volck des nachts <rhyme label="d" type="f">bespringhet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Noch voor t'haestighe, t'welck den <rhyme label="c" type="m" corresp="#s3:l5">doodt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Den menschen s'daeghs <rhyme label="d" type="f" corresp="#s3:l6">toebringhet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Als storuen aen v' lincke <rhyme label="a" type="m">zijd'</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Duysent, end tien duyst <rhyme label="b" type="f">t'saeme</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ter rechter: noch sijt ghy <rhyme label="a" type="m" corresp="#s4:l1">bevrydt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Voor schaeden <rhyme label="b" type="f" corresp="#s4:l2">onbequaeme</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Ghy sult onbevreest schauwen <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die boos in haer <rhyme label="d" type="f">verderuen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">End hoe se haeren loon seer <rhyme label="c" type="m" corresp="#s4:l5">saen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ontfanghen end <rhyme label="d" type="f" corresp="#s4:l6">beeruen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Om dat ghy tot Godt spreeckt <rhyme label="a" type="m">eenpaer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ghy behoedt my O <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">End op Godt in alle <rhyme label="a" type="m" corresp="#s5:l1">gheuaer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hoept, end hem doet dees <rhyme label="b" type="f" corresp="#s5:l2">eere</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Met gheen schaeden wert ghy <rhyme label="c" type="m">beswaert</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Noch gheen plaeghen met <rhyme label="d" type="f">allen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">En sullen v' huys wel <rhyme label="c" type="m" corresp="#s5:l5">vermaert</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Gheensins dan <rhyme label="d" type="f" corresp="#s5:l6">ouervallen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy gheeft den Enghelen <rhyme label="a" type="m">beuel</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sijnde tot v <rhyme label="b" type="f">gheneghen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dats'op v, steedts acht hebben <rhyme label="a" type="m" corresp="#s6:l1">wel</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">In allen uwen <rhyme label="b" type="f" corresp="#s6:l2">weghen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Se sullen in handen, dit's <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">V draeghen fyn <rhyme label="d" type="f">besloten</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Dat ghy niet vallen sult daer <rhyme label="c" type="m" corresp="#s6:l5">naer</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Noch v aen den steen <rhyme label="d" type="f" corresp="#s6:l6">stooten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ionghe leeuwen end slanghen <rhyme label="a" type="m">wreedt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Leeuwen end draeken <rhyme label="b" type="f">mede</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Sult ghy al sonder schaed' of <rhyme label="a" type="m" corresp="#s7:l1">leedt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Onder den voeten <rhyme label="b" type="f" corresp="#s7:l2">treden</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Want dit spreeckt Godt van v <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Om dat hy my <rhyme label="d" type="f">vereeret</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Ick sal hem helpen wt noot <rhyme label="c" type="m" corresp="#s7:l5">swaer</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Want mijn naem hy <rhyme label="d" type="f" corresp="#s7:l6">vermeeret</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Roept hy my aen, hy wert <rhyme label="a" type="m">verhoort</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">In den noot sal ick <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">By hem, t'aller tydt van nu <rhyme label="a" type="m" corresp="#s8:l1">voort</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Hy wert van my <rhyme label="b" type="f" corresp="#s8:l2">ghepresen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Ick sal hem vervullen seer <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Met veel vroylicke <rhyme label="d" type="f">daeghen</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">End hem oock laeten smaeken <rhyme label="c" type="m" corresp="#s8:l5">soet</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Myn hulp' nae myn <rhyme label="d" type="f" corresp="#s8:l6">behaeghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
