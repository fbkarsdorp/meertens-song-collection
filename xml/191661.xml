    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik zucht zucht op zucht</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VOlmaakte <rhyme label="a" type="m">Celestyn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hoe lang zal ik nog <rhyme label="b" type="f">klagen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In Vuur'ge <rhyme label="a" type="m" corresp="#s1:l1">Minnepyn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Kan ik u niet <rhyme label="b" type="f" corresp="#s1:l2">behagen</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Schoon-oogde slaat uw <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Op hem die <rhyme label="d" type="m">Nederknielt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Op hope van <rhyme label="c" type="f" corresp="#s1:l5">genezen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gevoel-loos, half <rhyme label="d" type="m" corresp="#s1:l6">ontzielt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Geef uw Sneeuw-witte <rhyme label="a" type="m">Handt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met Parlemoere <rhyme label="b" type="f">platen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En rukt my uyt den <rhyme label="a" type="m" corresp="#s2:l1">brand</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ik zal u nooyt <rhyme label="b" type="f" corresp="#s2:l2">verlaaten</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Nog bannen uyt <rhyme label="c" type="f">gedagten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">U Schoone <rhyme label="d" type="m">Minne-beeld</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als gy geneest myn <rhyme label="c" type="f" corresp="#s2:l5">klagten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En myne wonden <rhyme label="d" type="m" corresp="#s2:l6">heelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zo werv' ik volle <rhyme label="a" type="m">Vreugt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Zo zal de Liefde <rhyme label="b" type="f">Pralen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">In 't Bloeyen van ons <rhyme label="a" type="m" corresp="#s3:l1">Jeugt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In d'Echte Liefde <rhyme label="b" type="f" corresp="#s3:l2">Zalen</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Ey toond u <rhyme label="c" type="f">Mede-dogen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Of ik geraakt in 't <rhyme label="d" type="m">graf</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Wischt waardste van myn <rhyme label="c" type="f" corresp="#s3:l5">Oogen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">De brakke traantjes <rhyme label="d" type="m" corresp="#s3:l6">af</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
