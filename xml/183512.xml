<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183512</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wie moet nu niet verwondert <rhyme label="a" type="m">staan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van al die vreemde <rhyme label="b" type="f">zaken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Waar door ons Land te grond zal <rhyme label="a" type="m" corresp="#s1:l1">gaan</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En Vryheid weg moet <rhyme label="b" type="f" corresp="#s1:l2">raken</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Den Staatsman die wel eer ons <rhyme label="c" type="m">Land</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Verdeedigde met hart en <rhyme label="c" type="m" corresp="#s1:l5">hand</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Roept nu - wie zou 't <rhyme label="d" type="f">geloven</rhyme>!</l>
        <l id="s1:l8" met="3" type="-+">Vivat, Oranje <rhyme label="d" type="f" corresp="#s1:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den Eerste Willem van dien <rhyme label="a" type="m">Naam</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Deed Spanjen's Hoogmoed <rhyme label="b" type="f">wyken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Was zeker door en door <rhyme label="a" type="m" corresp="#s2:l1">bekwaam</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Maar om hem te <rhyme label="b" type="f" corresp="#s2:l2">verryken</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Sloeg hy de eerste hand aan 't <rhyme label="c" type="m">werk</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Tot nadeel van ons Land en <rhyme label="c" type="m" corresp="#s2:l5">Kerk</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maar 't wierd op 't Volk <rhyme label="d" type="f">geschoven</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Vivat, Oranje <rhyme label="d" type="f" corresp="#s2:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zyn zoon Prins Maurits was een <rhyme label="a" type="m">Held</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En dapper in het <rhyme label="b" type="f">stryden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hy liet den Gryzen <rhyme label="a" type="m" corresp="#s3:l1">Barneveld</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De grootste schande <rhyme label="b" type="f" corresp="#s3:l2">lyden</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En deed hem, als een <rhyme label="c" type="m">boevenrot</rhyme>.</l>
        <l id="s3:l6" met="4" type="-+">Onthoofden op een Moord - <rhyme label="c" type="m" corresp="#s3:l5">Schavot</rhyme>!</l>
        <l id="s3:l7" met="3" type="-+">Zo leeft men aan de <rhyme label="d" type="f">Hoven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En roept Oranje <rhyme label="d" type="f" corresp="#s3:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zyn Broeder Hendrik Fredrik <rhyme label="a" type="m">was</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Een Prins van beter <rhyme label="b" type="f">zeden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hy redde Nederland wel <rhyme label="a" type="m" corresp="#s4:l1">ras</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Van al die <rhyme label="b" type="f" corresp="#s4:l2">treurigheden</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Daar Maurits 't in gelaten <rhyme label="c" type="m">had</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">En trok van hen een groote <rhyme label="c" type="m" corresp="#s4:l5">schat</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dog ook niet zonder <rhyme label="d" type="f">roven</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Vivat, Oranje <rhyme label="d" type="f" corresp="#s4:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Tweede Willem van dien <rhyme label="a" type="m">Stam</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Deed' alles voor hem <rhyme label="b" type="f">vryken</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Behalven voor het <rhyme label="a" type="m" corresp="#s5:l1">Amsterdam</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daar moest zyn Hoogmoed <rhyme label="b" type="f" corresp="#s5:l2">stryken</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">'t Kanon wierd op de Wal <rhyme label="c" type="m">geplant</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Zyn Volk was nauwlyks <rhyme label="c" type="m" corresp="#s5:l5">aangeland</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Of 't was weer <rhyme label="d" type="f">weggestoven</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Vivat, Oranje <rhyme label="d" type="f" corresp="#s5:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Derde Willem had <rhyme label="a" type="m">verstand</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Maar 't meest om <rhyme label="b" type="f">t'overheeren</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Hy wierd gekroond in <rhyme label="a" type="m" corresp="#s6:l1">Engeland</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wou ook ons NeÃªrland <rhyme label="b" type="f" corresp="#s6:l2">leeren</rhyme>, -</l>
        <l id="s6:l5" met="4" type="-+">Dog echter hy stierf <rhyme label="c" type="m">Kinderloos</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Ach had men toen maar voor <rhyme label="c" type="m" corresp="#s6:l5">altoos</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat heerschend Hof <rhyme label="d" type="f">verschoven</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Vivat, Oranje <rhyme label="d" type="f" corresp="#s6:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De Vierde Willem met de <rhyme label="a" type="m">bult</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wierd hier weer <rhyme label="b" type="f">ingeroepen</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Dog hier aan had het Volk geen <rhyme label="a" type="m" corresp="#s7:l1">schuld</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Maar 't Vee dat toen by <rhyme label="b" type="f" corresp="#s7:l2">troepen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Bedrogen wierd door loose <rhyme label="c" type="m">schyn</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Oranje moest weer boven <rhyme label="c" type="m" corresp="#s7:l5">zyn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Niets kon dien drift <rhyme label="d" type="f">verdoven</rhyme>.</l>
        <l id="s7:l8" met="3" type="-+">Vivat, Oranje <rhyme label="d" type="f" corresp="#s7:l7">boven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De Vyfde Willem is een <rhyme label="a" type="m">Zwyn</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Verbonden aan 't <rhyme label="b" type="f">rapailje</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Verzoopen in Bourgonje <rhyme label="a" type="m" corresp="#s8:l1">Wyn</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Het Hoofd van al 't <rhyme label="b" type="f" corresp="#s8:l2">canailje</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Die, met dat rot te zaam <rhyme label="c" type="m">gespant</rhyme>.</l>
        <l id="s8:l6" met="4" type="-+">Het braafste Volk uit 't Land <rhyme label="c" type="m" corresp="#s8:l5">verband</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Of boud zig stil <rhyme label="d" type="f">verschoven</rhyme>. -</l>
        <l id="s8:l8" met="3" type="-+">Dat 's uit. - Oranje <rhyme label="d" type="f" corresp="#s8:l7">boven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
