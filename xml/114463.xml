  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">CUpido kan weer niet <rhyme label="a" type="f">Rusten</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Al zyn <rhyme label="a" type="f" corresp="#s1:l1">lusten</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Strekken weer tot <rhyme label="b" type="m">tydverdryf</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En hy schiet weer ondert <rhyme label="c" type="f">Hondert</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Dat het <rhyme label="c" type="f" corresp="#s1:l4">Donderd</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Yder krygt de Min op 't <rhyme label="b" type="m" corresp="#s1:l3">Lyf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Domme Klaes is wel <rhyme label="a" type="f">getroffen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Al zyn <rhyme label="a" type="f" corresp="#s2:l1">stoffen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En zyn blaesen, is maer <rhyme label="b" type="m">Windt</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Hy moet zig tot Liefde <rhyme label="c" type="f">wennen</rhyme></l>
        <l id="s2:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s2:l4">bekennen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Dat hy Jansje teer <rhyme label="b" type="m" corresp="#s2:l3">bemind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Franck die vryd zyn <rhyme label="a" type="f">Jacomyntje</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Ook geen <rhyme label="a" type="f" corresp="#s3:l1">klyntje</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Maer de Vader die Zeyd <rhyme label="b" type="m">Neen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Nu wil Franck haer stil <rhyme label="c" type="f">bekruypen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Of gaen <rhyme label="c" type="f" corresp="#s3:l4">sluypen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Met haer naer Vianen <rhyme label="b" type="m" corresp="#s3:l3">heen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Keesje noemd zyn <rhyme label="a" type="f">Alidaetje</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Beste <rhyme label="a" type="f" corresp="#s4:l1">Maetje</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En hy meend het zal wel <rhyme label="b" type="m">gaen</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Kyk wat maekt hy al <rhyme label="c" type="f">Bravaden</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s4:l4">Strykaden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Als een France <rhyme label="b" type="m" corresp="#s4:l3">Courtizaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Karel met zyn kromme <rhyme label="a" type="f">Scheentjes</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Zet zyn <rhyme label="a" type="f" corresp="#s5:l1">Beentjes</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Tans veel netter in <rhyme label="b" type="m">postuur</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Ja hy is geheel <rhyme label="c" type="f">ontlooken</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Zints <rhyme label="c" type="f" corresp="#s5:l4">Catooke</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Hem ontstak door 't <rhyme label="b" type="m" corresp="#s5:l3">Minnevuur</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Bram die schynd een <rhyme label="a" type="f">Minnekruyer</rhyme>:</l>
        <l id="s6:l2" met="2" type="+-">Want dien <rhyme label="a" type="f" corresp="#s6:l1">bruyer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Blyft niet langer op een <rhyme label="b" type="m">Stee</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Als de luy die met de <rhyme label="c" type="f">Wagen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Loopen <rhyme label="c" type="f" corresp="#s6:l4">Vragen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Moetje Knollen, Kool of <rhyme label="b" type="m" corresp="#s6:l3">Peen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Gys wou ook een Kansje <rhyme label="a" type="f">wagen</rhyme>;</l>
        <l id="s7:l2" met="2" type="+-">Op zyn <rhyme label="a" type="f" corresp="#s7:l1">Vragen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Antwoord Mietje, spytig <rhyme label="b" type="m">Neen</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Nu gaet hy geheele <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Eenzaem <rhyme label="c" type="f" corresp="#s7:l4">klagen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En betreurd zyn blauwe <rhyme label="b" type="m" corresp="#s7:l3">scheen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ik heb 't ook gevraegt, aen <rhyme label="a" type="f">Leysje</rhyme>;</l>
        <l id="s8:l2" met="2" type="+-">Maer dat <rhyme label="a" type="f" corresp="#s8:l1">Meysje</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Keek zoo bars gelyk de <rhyme label="b" type="m">Droes</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Nu gaen ik myn brand <rhyme label="c" type="f">verkoelen</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s8:l4">afspoelen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">In een frisse volle <rhyme label="b" type="m" corresp="#s8:l3">Kroes</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
