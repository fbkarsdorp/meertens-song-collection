<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25094</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Laet ons eeren ende <rhyme label="a" type="f">louen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Eenen hogen <rhyme label="b" type="m">sant</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Hi siert met edelen <rhyme label="c" type="f">doechden</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Al dat hemels <rhyme label="b" type="m" corresp="#s1:l2">lant</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s1:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s1:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s1:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In erterike ende in <rhyme label="a" type="f">hemelrike</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">En vintmen <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Dat dese groote <rhyme label="c" type="f">eere</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Yemende es <rhyme label="b" type="m" corresp="#s2:l2">gesciet</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s2:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s2:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s2:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hi heeft verlicht al <rhyme label="a" type="f">kerstenheit</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Dese hoghe <rhyme label="b" type="m">man</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Met drien heilegen <rhyme label="c" type="f">ordenen</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Die hi stichte <rhyme label="b" type="m" corresp="#s3:l2">dan</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s3:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s3:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s3:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hi es een vader der <rhyme label="a" type="f">minderbrueders</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ende der claren dat <rhyme label="b" type="m">wet</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Ende ooc der derder <rhyme label="c" type="f">orden</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Es hi een prinse <rhyme label="b" type="m" corresp="#s4:l2">geset</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s4:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s4:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s4:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s4:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Franciscus es een <rhyme label="a" type="f">bloeme</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Der genadicheit <rhyme label="b" type="m">gepresen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Die willen <rhyme label="c" type="f">mogent</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">In sijn leuen <rhyme label="b" type="m" corresp="#s5:l2">lesen</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s5:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s5:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s5:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s5:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Int hoechste willich <rhyme label="a" type="f">armoede</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">Soe was hi <rhyme label="b" type="m">geraect</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Van miltheden ende van <rhyme label="c" type="f">minnen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Was hi al <rhyme label="b" type="m" corresp="#s6:l2">volmaect</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s6:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s6:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s6:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s6:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Franciscns es een die <rhyme label="a" type="f">meeste</rhyme>.</l>
        <l id="s7:l2" met="3" type="-+">Geminde in <rhyme label="b" type="m">hemelrijc</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Want die almechtich <rhyme label="c" type="f">coninc</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Tsierden hem seluen <rhyme label="b" type="m" corresp="#s7:l2">gelijc</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s7:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s7:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s7:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s7:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">In handen in voeten int herte <rhyme label="a" type="f">fijn</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">Hi mach wel een claer spiegel <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Die al verblijt dat hemelsce <rhyme label="c" type="f">hof</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Des heefti eewich eere ende <rhyme label="b" type="m" corresp="#s8:l2">lof</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s8:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s8:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s8:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s8:l9">openbaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Nv hulpt ons heilege <rhyme label="a" type="f">vader</rhyme>.</l>
        <l id="s9:l2" met="3" type="-+">Noch na deser <rhyme label="b" type="m">tijt</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Onder v <rhyme label="c" type="f">banieren</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Want si es herde <rhyme label="b" type="m" corresp="#s9:l2">wijt</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">En hi sal comen ten ordeel <rhyme label="d" type="m">gods</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Geteekent metten <rhyme label="e" type="f">teeken</rhyme></l>
        <l id="s9:l7" met="2" type="-+">Des eewichs <rhyme label="f" type="m">woorts</rhyme>.</l>
        <l id="s9:l8" met="4" type="-+">Metten heilegen v <rhyme label="g" type="f">wonden</rhyme>.</l>
        <l id="s9:l9" met="2" type="-+">Dat wet <rhyme label="h" type="m">voirwaer</rhyme>.</l>
        <l id="s9:l10" met="4" type="-+">Die selen wi daer scouwen <rhyme label="h" type="m" corresp="#s9:l9">openbaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
