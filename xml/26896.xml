    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ich ruf zu dir Herr Jesu Christ</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOd willɇ onser ghenadigh <rhyme label="a" type="m">syn</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">End ons synen seghen <rhyme label="b" type="f">gheuen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hy late lichten syn <rhyme label="a" type="m" corresp="#s1:l1">aenschyn</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Op ons, ten eewighen <rhyme label="b" type="f" corresp="#s1:l2">leuen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dat dyn wegh op ęrd werdɇ <rhyme label="c" type="m">erkent</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">End dyn saligheyt den <rhyme label="d" type="f">Heyden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Aen allen <rhyme label="d" type="f" corresp="#s1:l6">syden</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">T’volck dancke dy wel <rhyme label="c" type="m" corresp="#s1:l5">bekent</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Ia alle volcken <rhyme label="d" type="f" corresp="#s1:l6">blyde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De volcken laet verblyden <rhyme label="a" type="m">sick</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Om dat du hen goed recht <rhyme label="b" type="f">wysest</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">End dat du se beleydest <rhyme label="a" type="m" corresp="#s2:l1">frisch</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">T’volck sal dy louen end <rhyme label="b" type="f" corresp="#s2:l2">prysen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">D’ęrd bringhe voort de vruchten <rhyme label="c" type="m">syn</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">God gheuɇ ons synen <rhyme label="d" type="f">seghen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Altyd te <rhyme label="d" type="f" corresp="#s2:l6">deghen</rhyme>:</l>
        <l id="s2:l8" met="4" type="+-">God seghenɇ ons, end hem <rhyme label="c" type="m" corresp="#s2:l5">fyn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Vreese t’volck, alle <rhyme label="d" type="f" corresp="#s2:l6">weghen</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
