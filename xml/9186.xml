<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9186</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat speelt er in mijn zin zeg dartele herderin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IN Phoebus held're <rhyme label="a" type="m">glans</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Vlecht Silvia een <rhyme label="a" type="m" corresp="#s1:l1">Crans</rhyme></l>
        <l id="s1:l3" met="6" type="-+">Van Roosen root, en wit, om t'avont aen den <rhyme label="a" type="m" corresp="#s1:l1">dans</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Mee te gaen // die sy <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Malcand're t'samen <rhyme label="c" type="m">hecht</rhyme>,</l>
        <l id="s1:l6" met="6" type="-+">En met Maechde-Palm groen, haer Cransje cierlijck <rhyme label="c" type="m" corresp="#s1:l5">vlecht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dewijl dat <rhyme label="a" type="m">Coridon</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hem schuylde voor de <rhyme label="a" type="m" corresp="#s2:l1">Son</rhyme>.</l>
        <l id="s2:l3" met="6" type="-+">En vrolijck op sijn Harders Fluytje doe <rhyme label="a" type="m" corresp="#s2:l1">begon</rhyme></l>
        <l id="s2:l4" met="4" type="+-">In het Riet // een nieu <rhyme label="b" type="m">Liedt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Te schrillen soetjes <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s2:l6" met="6" type="-+">So dat het aen den Oever weerschatrend' over <rhyme label="c" type="m" corresp="#s2:l5">stuyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Waer door dat <rhyme label="a" type="m">Silvia</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Haer held're stemme <rhyme label="a" type="m" corresp="#s3:l1">dra</rhyme></l>
        <l id="s3:l3" met="6" type="-+">Op hefte, en haer Coridon vryde <rhyme label="a" type="m" corresp="#s3:l1">na</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Soo insoet // dat hy <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Een toontje hoogher <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s3:l6" met="6" type="-+">Om de wijs van Silvia beter te <rhyme label="c" type="m" corresp="#s3:l5">verstaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Terstont Silvia <rhyme label="a" type="m">sweech</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En van haer plaets op <rhyme label="a" type="m" corresp="#s4:l1">steech</rhyme>,</l>
        <l id="s4:l3" met="6" type="-+">Soo dat sy Coridon soetjes van acht'ren <rhyme label="a" type="m" corresp="#s4:l1">kreech</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Wiens ghesicht // sy seer <rhyme label="b" type="m">licht</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Bedeckte met haer <rhyme label="c" type="m">hant</rhyme>,</l>
        <l id="s4:l6" met="6" type="-+">Dies Coridon hem van Silvia verrast <rhyme label="c" type="m" corresp="#s4:l5">vant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Silvia ghy zijt <rhyme label="a" type="m">die</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Doet dat ick niet en <rhyme label="a" type="m" corresp="#s5:l1">sie</rhyme></l>
        <l id="s5:l3" met="6" type="-+">Sprack Coridon laet los, ick heb gheraden <rhyme label="a" type="m" corresp="#s5:l1">wie</rhyme></l>
        <l id="s5:l4" met="4" type="+-">My bedeckt // dus <rhyme label="b" type="m">aftreckt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">U handen van myn <rhyme label="c" type="m">Ooch</rhyme>,</l>
        <l id="s5:l6" met="6" type="-+">Ghy zijt Ã´ Silvia die my aldus <rhyme label="c" type="m" corresp="#s5:l5">bedrooch</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Waer op dat sy los <rhyme label="a" type="m">liet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En Coridon die <rhyme label="a" type="m" corresp="#s6:l1">biet</rhyme></l>
        <l id="s6:l3" met="6" type="-+">Haer een kus, omdat hy haer had sien comen <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Maer sy heeft // hem <rhyme label="b" type="m">beleeft</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Tien kusjes voor <rhyme label="c" type="m">gejont</rhyme>,</l>
        <l id="s6:l6" met="6" type="-+">Die Coridon haer weer toe telde voor sijn <rhyme label="c" type="m" corresp="#s6:l5">Mont</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
