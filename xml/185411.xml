  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WY Boeren en <rhyme label="a" type="f">Boerinnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wy werken dag en <rhyme label="b" type="m">nagt</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Wy ploegen en wy <rhyme label="a" type="f" corresp="#s1:l1">spinnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En wy singen uyt ons <rhyme label="b" type="m" corresp="#s1:l2">magt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Lieve Heer, kost en <rhyme label="c" type="m">kleer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Hemelryk, en dan niet <rhyme label="c" type="m" corresp="#s1:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy spitten en wy <rhyme label="a" type="f">spaeyen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Geheele dagen <rhyme label="b" type="m">lang</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wy zaeyen, en wy <rhyme label="a" type="f" corresp="#s2:l1">maeyen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">In groot geneugt met desen <rhyme label="b" type="m" corresp="#s2:l2">zang</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Lieve Heer, kost en <rhyme label="c" type="m">kleer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Hemelryk, en dan niet <rhyme label="c" type="m" corresp="#s2:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy eeten alle <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">â€™s Morgens Boek-weyte <rhyme label="b" type="m">pap</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Soo vullen wy ons <rhyme label="a" type="f" corresp="#s3:l1">magen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En wy zingen even <rhyme label="b" type="m" corresp="#s3:l2">rap</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Lieve Heer, kost en <rhyme label="c" type="m">kleer</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Hemelryk, en dan niet <rhyme label="c" type="m" corresp="#s3:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy eeten soete <rhyme label="a" type="f">Boter</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Melk is voor onsen <rhyme label="b" type="m">dorst</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dan zijn wy veel <rhyme label="a" type="f" corresp="#s4:l1">devoter</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En wy zingen uytter <rhyme label="b" type="m" corresp="#s4:l2">borst</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Lieve Heer, kost en <rhyme label="c" type="m">kleer</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Hemelryk, en dan niet <rhyme label="c" type="m" corresp="#s4:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wy decken zelden <rhyme label="a" type="f">Tafel</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Een stuxken uyt ons <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat smaekt ons als een <rhyme label="a" type="f" corresp="#s5:l1">Waeffel</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En wy zingen langs den <rhyme label="b" type="m" corresp="#s5:l2">kant</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Lieve Heer, kost en <rhyme label="c" type="m">kleer</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Hemelryk, en dan niet <rhyme label="c" type="m" corresp="#s5:l5">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
