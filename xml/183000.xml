<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183000</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Juich thans, gy Amstels Burgerschaar! <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s1:l2" met="5" type="-+">En prys den Opperzegenaar, <rhyme label="a" type="m" corresp="#s1:l1">hoezée</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Die door zyn Vaderlyke <rhyme label="b" type="m">Hand</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Prins Willem weder bragt in <rhyme label="b" type="m" corresp="#s1:l3">stand</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s1:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Hy heeft hem heden noch bewaard, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s2:l2" met="5" type="-+">Zyn Koninglyke Vrouw gespaard, <rhyme label="a" type="m" corresp="#s2:l1">hoezée</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Ook schonk Hy, tot ons heil en <rhyme label="b" type="m">troost</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Zyn zegen aan 't Oranje <rhyme label="b" type="m" corresp="#s2:l3">kroost</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s2:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Draag nu Kokardes op uw Hoed, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s3:l2" met="5" type="-+">Ter eere van 't Nassouse bloed, <rhyme label="a" type="m" corresp="#s3:l1">hoezée</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Terwyl Hy ons heeft laaten <rhyme label="b" type="m">zien</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Hoe Hy het recht komt hulde <rhyme label="b" type="m" corresp="#s3:l3">biên</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s3:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Hy is de zuil van 't Staatsgebouw, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s4:l2" met="5" type="-+">Die altoos bleef aan 't Recht getrouw, <rhyme label="a" type="m" corresp="#s4:l1">hoezée</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Waardoor hem nu dit lot <rhyme label="b" type="m">geviel</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Het welk verheugd ons hart en <rhyme label="b" type="m" corresp="#s4:l3">ziel</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s4:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Hoe heilzaam is thans niet het lot, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s5:l2" met="5" type="-+">Dat hem is toegeschikt van God, <rhyme label="a" type="m" corresp="#s5:l1">hoezée</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Terwyl hy, in den Haag <rhyme label="b" type="m">onthaald</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Gelyk voorheen weer <rhyme label="b" type="m" corresp="#s5:l3">zegenpraald</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s5:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Gy maagden komt nu weer ten dans, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s6:l2" met="5" type="-+">Zyn Hoogheid vlegte een roozekrans, <rhyme label="a" type="m" corresp="#s6:l1">hoezée</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Wasch af uw traanen, weend geen <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Maar juigd dien Vorst een lied ter <rhyme label="b" type="m" corresp="#s6:l3">Eer</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s6:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Zo lang de waereld zal bestaan, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s7:l2" met="5" type="-+">Moet nooit de Oranjestam vergaan, <rhyme label="a" type="m" corresp="#s7:l1">hoezée</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Want God heeft zelfs dien boom <rhyme label="b" type="m">geplant</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Tot heil van 't lieve <rhyme label="b" type="m" corresp="#s7:l3">Vaderland</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s7:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Hy, die u dit bewezen heeft, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s8:l2" met="5" type="-+">Maakt dat uw druk in vreugd herleefd, <rhyme label="a" type="m" corresp="#s8:l1">hoezée</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Daarom staak uw <rhyme label="b" type="m">bekommernis</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">'t Is God die met Oranje <rhyme label="b" type="m" corresp="#s8:l3">is</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s8:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">'t Is God die met Oranje stryd, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s9:l2" met="5" type="-+">Die Vyfde Willem heeft verblyd, <rhyme label="a" type="m" corresp="#s9:l1">hoezée</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Dies weest verheugd in dit <rhyme label="b" type="m">geval</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">'t Is God die hem behoeden <rhyme label="b" type="m" corresp="#s9:l3">zal</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s9:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">Hy sterkt zyn lieve Gemaalin, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s10:l2" met="5" type="-+">Hy zegend elker Spruit niet min, <rhyme label="a" type="m" corresp="#s10:l1">hoezée</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Beveiligd hen van druk en <rhyme label="b" type="m">kruis</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Zyn oog bewaakt 't Vorstelyk <rhyme label="b" type="m" corresp="#s10:l3">Huis</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s10:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="-+">Juig, juig nogmaals, ô Burgerstoet! <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s11:l2" met="5" type="-+">En draaid en zwaaid uw Vryheidshoed, <rhyme label="a" type="m" corresp="#s11:l1">hoezée</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Ter eere van uw <rhyme label="b" type="m">Overigheid</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Wier liefdezorg u heeft <rhyme label="b" type="m" corresp="#s11:l3">bevryd</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s11:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="5" type="-+">ô Braaven die uw vry bestaan, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s12:l2" met="5" type="-+">Niet ligt zult laaten overgaan, <rhyme label="a" type="m" corresp="#s12:l1">hoezée</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Maar staat reeds pal voor 't oude <rhyme label="b" type="m">Recht</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Dat van 't begin is <rhyme label="b" type="m" corresp="#s12:l3">aangelegt</rhyme>.</l>
        <l id="s12:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s12:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="5" type="-+">God zy uw steun in raad en daad, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s13:l2" met="5" type="-+">Tot heil van onze Burgerstaat, <rhyme label="a" type="m" corresp="#s13:l1">hoezée</rhyme>.</l>
        <l id="s13:l3" met="4" type="-+">Hy geeft dat elk van u <rhyme label="b" type="m">verheugd</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">ô Burgervad'ren! leven <rhyme label="b" type="m" corresp="#s13:l3">meugt</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s13:l1">hoezée</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="5" type="-+">Ja leeven moogt tot onderstand, <rhyme label="a" type="m">hoezée</rhyme>.</l>
        <l id="s14:l2" met="5" type="-+">Van vrye Kerk en Stad en Land, <rhyme label="a" type="m" corresp="#s14:l1">hoezée</rhyme>.</l>
        <l id="s14:l3" met="4" type="-+">Dat dan uw doel gezegend <rhyme label="b" type="m">zy</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">ô Ed'le Raad! dat wenschen <rhyme label="b" type="m" corresp="#s14:l3">wy</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">Hoezée, Viva, <rhyme label="a" type="m" corresp="#s14:l1">hoezée</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
