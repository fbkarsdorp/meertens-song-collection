<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6177</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Al hebben de prinsen haar wens</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">'T	Is wel te <rhyme label="a" type="f">beklaghen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Dat heden ten <rhyme label="a" type="f" corresp="#s1:l1">daghe</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Veel Iongh-mans <rhyme label="a" type="f" corresp="#s1:l1">vertraghen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">In vriend'lijcke <rhyme label="b" type="m">Min</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Daer wy hier sien <rhyme label="c" type="f">sweven</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Soo aerdigh nae 't <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">(Door Minne <rhyme label="c" type="f" corresp="#s1:l5">ghedreven</rhyme>)</l>
        <l id="s1:l8" met="2" type="-+">Soo menich <rhyme label="b" type="m" corresp="#s1:l4">Goddin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Men siet het niet <rhyme label="a" type="f">missen</rhyme>:</l>
        <l id="s2:l2" met="2" type="-+">Vee, Vogh'len, en <rhyme label="a" type="f" corresp="#s2:l1">Vissen</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Elck gaet hem <rhyme label="a" type="f" corresp="#s2:l1">verfrissen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Met 's Somers <rhyme label="b" type="m">ghety</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Maer sommighe <rhyme label="c" type="f">knapen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Gaen droomen, en <rhyme label="c" type="f" corresp="#s2:l5">slapen</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Gheen vreucht sy en <rhyme label="c" type="f" corresp="#s2:l5">rapen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">In <rhyme label="b" type="m" corresp="#s2:l4">Minnery</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Fy u! koele <rhyme label="a" type="f">Troevers</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Ghy Tierebus <rhyme label="a" type="f" corresp="#s3:l1">Schroevers</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">O, onlust <rhyme label="a" type="f" corresp="#s3:l1">proevers</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Waer is u <rhyme label="b" type="m">Natuer</rhyme>?</l>
        <l id="s3:l5" met="2" type="-+">Dees gheestighe <rhyme label="c" type="f">Sufjes</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Verwachten u <rhyme label="c" type="f" corresp="#s3:l5">kusjes</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Met snackende <rhyme label="c" type="f" corresp="#s3:l5">lusjes</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Dus beydt niet een <rhyme label="b" type="m" corresp="#s3:l4">uyr</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ick ben nu <rhyme label="a" type="f">ghebonden</rhyme>:</l>
        <l id="s4:l2" met="2" type="-+">En mach 't gheenen <rhyme label="a" type="f" corresp="#s4:l1">stonden</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">(Van weghen de <rhyme label="a" type="f" corresp="#s4:l1">Sonden</rhyme>)</l>
        <l id="s4:l4" met="2" type="-+">Hanteeren dit <rhyme label="b" type="m">Spel</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">Maer mocht ick het <rhyme label="c" type="f">waghen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Het sou my <rhyme label="c" type="f" corresp="#s4:l5">behaghen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Iaer, maenden, en <rhyme label="c" type="f" corresp="#s4:l5">daghen</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Deed' ick het noch <rhyme label="b" type="m" corresp="#s4:l4">wel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Maer wilt ghy dus <rhyme label="a" type="f">blyven</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Gheen Maeghden <rhyme label="a" type="f" corresp="#s5:l1">gheryven</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Noch vreughden <rhyme label="a" type="f" corresp="#s5:l1">bedryven</rhyme>:</l>
        <l id="s5:l4" met="2" type="-+">Soo moeten wy <rhyme label="b" type="m">Mans</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Tot uwer <rhyme label="c" type="f">schandalen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Ons tijden <rhyme label="c" type="f" corresp="#s5:l5">verhalen</rhyme>:</l>
        <l id="s5:l7" met="2" type="-+">De Maeghden <rhyme label="c" type="f" corresp="#s5:l5">bestralen</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Met onsen <rhyme label="b" type="m" corresp="#s5:l4">glans</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Sy sullen 't <rhyme label="a" type="f">ghedooghen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Al waert voor u <rhyme label="a" type="f" corresp="#s6:l1">ooghen</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Tot spijt van u <rhyme label="a" type="f" corresp="#s6:l1">drooghe</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Ghesellen, dus <rhyme label="b" type="m">koel</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">My wondert u <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Dus eensaem in <rhyme label="c" type="f" corresp="#s6:l5">desen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Of gaet ghy u <rhyme label="c" type="f" corresp="#s6:l5">vreesen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Voor 't vriend'lijck <rhyme label="b" type="m" corresp="#s6:l4">ghewoel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Ghy loopt met u <rhyme label="a" type="f">Iapen</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Steedts gieten, en <rhyme label="a" type="f" corresp="#s7:l1">gapen</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">O drollighe <rhyme label="a" type="f" corresp="#s7:l1">Apen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Ghedraeyt van een <rhyme label="b" type="m">Hout</rhyme>;</l>
        <l id="s7:l5" met="2" type="-+">Verlaet uws <rhyme label="c" type="f">ghelijcken</rhyme>:</l>
        <l id="s7:l6" met="2" type="-+">Nae Meysjes wilt <rhyme label="c" type="f" corresp="#s7:l5">kijcken</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Die voor u gaen <rhyme label="c" type="f" corresp="#s7:l5">strijcken</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">En blincken als <rhyme label="b" type="m" corresp="#s7:l4">Gout</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Hierom jongh' <rhyme label="a" type="f">Ghesellen</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">V wacker wilt <rhyme label="a" type="f" corresp="#s8:l1">stellen</rhyme>:</l>
        <l id="s8:l3" met="2" type="-+">Bestormt Venus <rhyme label="a" type="f" corresp="#s8:l1">Hellen</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">En schrickt niet het <rhyme label="b" type="m">vyer</rhyme>:</l>
        <l id="s8:l5" met="2" type="-+">Laet u dus niet <rhyme label="c" type="f">schelden</rhyme>:</l>
        <l id="s8:l6" met="2" type="-+">Begheeft u te <rhyme label="c" type="f" corresp="#s8:l5">velden</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Als wackere <rhyme label="c" type="f" corresp="#s8:l5">Helden</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Met menigh soet <rhyme label="b" type="m" corresp="#s8:l4">Dier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
