        <l id="s5:l4" met="3" type="+-">Puer qui nos <rhyme label="b" type="f" corresp="#s5:l2">scribat</rhyme></l>
        <l id="s5:l5" met="4" type="+-">In illorum <rhyme label="c" type="m">curia</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Qui canebant <rhyme label="c" type="m" corresp="#s5:l5">gloria</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Noue <rhyme label="d" type="f">dignitatis</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Deus in <rhyme label="c" type="m" corresp="#s5:l5">sublimibus</rhyme></l>
        <l id="s5:l9" met="4" type="+-">Det pacem <rhyme label="c" type="m" corresp="#s5:l5">hominibus</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Boue <rhyme label="d" type="f" corresp="#s5:l7">voluntatis</rhyme>.</l>
