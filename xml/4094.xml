        <l id="s3:l7" met="3" type="+-">Creatorem <rhyme label="d" type="f">rerum</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Quod vber <rhyme label="c" type="m" corresp="#s3:l5">mundicie</rhyme></l>
