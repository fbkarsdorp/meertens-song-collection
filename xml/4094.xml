        <l id="s4:l4" met="3" type="+-">Virgo post et <rhyme label="b" type="f" corresp="#s4:l2">ante</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Felix est <rhyme label="c" type="m">puerpera</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Cuius sacra <rhyme label="c" type="m" corresp="#s4:l5">viscera</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Deum <rhyme label="d" type="f">postauerunt</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Et beata <rhyme label="c" type="m" corresp="#s4:l5">vbera</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Que etate <rhyme label="c" type="m" corresp="#s4:l5">tenera</rhyme></l>
