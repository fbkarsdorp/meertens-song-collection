        <l id="s2:l2" met="3" type="+-">Pater hic est <rhyme label="b" type="f">natus</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Quis audiuit <rhyme label="a" type="m" corresp="#s2:l1">talia</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Deus homo <rhyme label="b" type="f" corresp="#s2:l2">natus</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Seruus est et <rhyme label="c" type="m">dominus</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Qui ubique <rhyme label="c" type="m" corresp="#s2:l5">cominus</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Nescit <rhyme label="d" type="f">comprehendi</rhyme>.</l>
        <l id="s2:l8" met="4" type="+-">Stupor iste <rhyme label="c" type="m" corresp="#s2:l5">geminus</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Presens est et <rhyme label="c" type="m" corresp="#s2:l5">eminus</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Nequit <rhyme label="d" type="f" corresp="#s2:l7">comprehendi</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Orto dei <rhyme label="a" type="m">filio</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Virgine de <rhyme label="b" type="f">pura</rhyme></l>
