  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SIet dat vyer der liefde <rhyme label="a" type="f">blaken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Godt, die schiep dat <rhyme label="b" type="m">hemelrijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Om u sondaer te <rhyme label="a" type="f" corresp="#s1:l1">hermaken</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Wordt ghelijck aen 'taersche <rhyme label="b" type="m" corresp="#s1:l2">slijck</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">'sVaders Woordt komt neder <rhyme label="c" type="f">daelen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En voor Adams schuldt <rhyme label="c" type="f" corresp="#s1:l5">betaelen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Iesus, boven al <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Alle prijs, en eere <rhyme label="b" type="m">weird</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Voor wie al die Enghels <rhyme label="a" type="f" corresp="#s2:l1">beven</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Sich vernedert totter <rhyme label="b" type="m" corresp="#s2:l2">eird'</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">In der beesten stal <rhyme label="c" type="f">ghebooren</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Ons bevrijdt van Godes <rhyme label="c" type="f" corresp="#s2:l5">thooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Iesus komt ghewilligh <rhyme label="a" type="f">lijden</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Hongher, koude, smerte, <rhyme label="b" type="m">dorst</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Om de menschen te <rhyme label="a" type="f" corresp="#s3:l1">verblijden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Als een kindje zuyght de <rhyme label="b" type="m" corresp="#s3:l2">borst</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Hy laet scepter, ende <rhyme label="c" type="f">kroone</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Om te zijn een Maghets <rhyme label="c" type="f" corresp="#s3:l5">Soone</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wordt in doecxkens teer <rhyme label="a" type="f">ghewonden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Om te maecken los, en <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Adams kinders van der <rhyme label="a" type="f" corresp="#s4:l1">sonden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Harte jock, en <rhyme label="b" type="m" corresp="#s4:l2">slaverny</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Hy door minne-brandt <rhyme label="c" type="f">ghedreven</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Komt ons schijncken 't eeuwigh <rhyme label="c" type="f" corresp="#s4:l5">leven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Laet ons allegader <rhyme label="a" type="f">loven</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Die verheven <rhyme label="b" type="m">Majesteyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Met de Princen van hier <rhyme label="a" type="f" corresp="#s5:l1">boven</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Sijnghen prijs, en <rhyme label="b" type="m" corresp="#s5:l2">danckbaerheyt</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Lof zy aen de Maghet <rhyme label="c" type="f">Moeder</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Die ghebaert heeft onsen <rhyme label="c" type="f" corresp="#s5:l5">Broeder</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
