<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190510</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Paul Jonas</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Veels geluk myn Vorst <rhyme label="a" type="m">geluk</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Gy hebt de Keeze <rhyme label="b" type="f">overwonnen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ach! wat was het regt aan <rhyme label="a" type="m" corresp="#s1:l1">stuk</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wie zou dat gedoogen <rhyme label="b" type="f" corresp="#s1:l2">konnen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Zoo te stryden voor Willems <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Hem doemen in den Afgrond <rhyme label="c" type="m" corresp="#s1:l5">neer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Alles op een valsche <rhyme label="d" type="m">grond</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">ô Vervloekte Keeze <rhyme label="d" type="m" corresp="#s1:l7">Hond</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Blyke zyn hoe zy <rhyme label="a" type="m">altyd</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Op een valsche Vryheid <rhyme label="b" type="f">roemde</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat heeft meenig een <rhyme label="a" type="m" corresp="#s2:l1">misleid</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dat men Muitzucht Vryheid <rhyme label="b" type="f" corresp="#s2:l2">noemde</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daarom zeg ik Keeze <rhyme label="c" type="m">Hond</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Steek je Zabel in de <rhyme label="c" type="m" corresp="#s2:l5">Grond</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Want een Zabel hoeft niets <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Voor een laffe Vryheids <rhyme label="d" type="m" corresp="#s2:l7">Zoon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Keesje legt je Wapens <rhyme label="a" type="m">af</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Scheid 'er uit uit met <rhyme label="b" type="f">Exerceeren</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Want gy zyt dog veels te <rhyme label="a" type="m" corresp="#s3:l1">laf</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Pruissen kon jy dog niet <rhyme label="b" type="f" corresp="#s3:l2">keeren</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Daarom roept nu voor en <rhyme label="c" type="m">na</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Vorst ik bid U om <rhyme label="c" type="m" corresp="#s3:l5">genâ</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En dat zonder <rhyme label="d" type="m">veinzery</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">ô Vervloekte <rhyme label="d" type="m" corresp="#s3:l7">Keezery</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kees voort scheerje uit de <rhyme label="a" type="m">Hal</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dat is voor jou vervloekte <rhyme label="b" type="f">plage</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Met moetwil kwam jy in 't <rhyme label="a" type="m" corresp="#s4:l1">geval</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat jy jou lieve Vorst wou <rhyme label="b" type="f" corresp="#s4:l2">plage</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Daarom roep ik <rhyme label="c" type="m">overluid</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Kees jy hebt het mooy <rhyme label="c" type="m" corresp="#s4:l5">verbruid</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Gooid je Snaphaan in de <rhyme label="d" type="m">Vaart</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En ook mee jou <rhyme label="d" type="m" corresp="#s4:l7">Knevelbaard</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Jy wist immers van 't <rhyme label="a" type="m">geval</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">'t Waaren niet dan <rhyme label="b" type="f">Schelmstukken</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Weerligs tuig jy was te <rhyme label="a" type="m" corresp="#s5:l1">mal</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dat kon immers niet <rhyme label="b" type="f" corresp="#s5:l2">gelukken</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Daarom Dieven keer nu <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En betoond aan Willem <rhyme label="c" type="m" corresp="#s5:l5">eer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Want die Vorst die is het <rhyme label="d" type="m">waard</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Geroemd te zyn op al de <rhyme label="d" type="m" corresp="#s5:l7">Aard</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
