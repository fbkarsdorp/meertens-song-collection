      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn siel maect groot den <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Mijn geest verhuecht hem <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In mijnen Godt vol <rhyme label="b" type="f">trouwen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hy is mijn <rhyme label="c" type="m">salicheyt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En wil oock de <rhyme label="c" type="m" corresp="#s1:l4">cleynheyt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sijner dienstmaecht <rhyme label="b" type="f" corresp="#s1:l3">aenschouwen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Siet hierom sullen <rhyme label="a" type="m">my</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Alle gheslachten <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Wel ghelucsalich <rhyme label="b" type="f">achten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Want onse Godt seer <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Groote dinghen nv <rhyme label="c" type="m" corresp="#s2:l4">doet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Door sijn hant sterck in <rhyme label="b" type="f" corresp="#s2:l3">crachten</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Heylich is sinen <rhyme label="a" type="m">naem</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En sijn goetheyt <rhyme label="a" type="m" corresp="#s3:l1">bequaem</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sal eewichlick <rhyme label="b" type="f">beclijuen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van Kints Kinderen <rhyme label="c" type="m">voortaen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Voor hem die recht wel <rhyme label="c" type="m" corresp="#s3:l4">gaen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En in Gods vreese <rhyme label="b" type="f" corresp="#s3:l3">blijuen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een schoon en Heerlick <rhyme label="a" type="m">werck</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Door sijnen arm seer <rhyme label="a" type="m" corresp="#s4:l1">sterck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Heeft gedaen God <rhyme label="b" type="f">almachtich</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hy heeft die stoute <rhyme label="c" type="m">quaet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">En hares herten <rhyme label="c" type="m" corresp="#s4:l4">raet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Tot niet ghemaeckt zeer <rhyme label="b" type="f" corresp="#s4:l3">crachtich</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dit stout zijn, in <rhyme label="a" type="m">hoochmoet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Vol van eer en van <rhyme label="a" type="m" corresp="#s5:l1">ghoet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Heeft Godt neder <rhyme label="b" type="f">ghedreuen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En die arm zijn en <rhyme label="c" type="m">cleyn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Heeft zijn ghoetheyt <rhyme label="c" type="m" corresp="#s5:l4">alleyn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Seer Heerlick nv <rhyme label="b" type="f" corresp="#s5:l3">verheuen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die arm zijn nae den <rhyme label="a" type="m">gheest</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Den welcken honghert <rhyme label="a" type="m" corresp="#s6:l1">meest</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Versaedt die Heer <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die Rijck sijn vol en <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Heeft hi ledich en <rhyme label="c" type="m" corresp="#s6:l4">bloot</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Van hem vry <rhyme label="b" type="f" corresp="#s6:l3">afghewesen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy verheeft <rhyme label="a" type="m">Israhel</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Sijn Soon ende <rhyme label="a" type="m" corresp="#s7:l1">gedenct</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Aen zijn groote <rhyme label="b" type="f">ghenade</rhyme></l>
        <l id="s7:l4" met="3" type="-+">So hi heeft <rhyme label="c" type="m">Abraham</rhyme></l>
        <l id="s7:l5" met="3" type="-+">En tVolck dat na hem <rhyme label="c" type="m" corresp="#s7:l4">quam</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Toegheseydt vroech en <rhyme label="b" type="f" corresp="#s7:l3">spade</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
