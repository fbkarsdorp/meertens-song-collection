<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191607</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik breng u op een dronkje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ZA wakker noble <rhyme label="a" type="f">bazen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Beminnaars van de <rhyme label="a" type="f" corresp="#s1:l1">glazen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En van de Franse <rhyme label="b" type="m">Wyn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vat lustig de <rhyme label="c" type="f">Boutelje</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En speelt daar mee <rhyme label="c" type="f" corresp="#s1:l4">revelje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Daar moet gedronken <rhyme label="b" type="m" corresp="#s1:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Laat ons te zamen <rhyme label="a" type="f">klinken</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">'k Zal uw gezondheid <rhyme label="a" type="f" corresp="#s2:l1">drinken</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Charmante <rhyme label="b" type="m">Meesteres</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Nu spoey ik het naar <rhyme label="c" type="f">binnen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ha dat verheugt myn <rhyme label="c" type="f" corresp="#s2:l4">zinnen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Geliefde <rhyme label="b" type="m" corresp="#s2:l3">Zielvoogdes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wel zoete lieve <rhyme label="a" type="f">Zusje</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mag ik nu wel een <rhyme label="a" type="f" corresp="#s3:l1">kusje</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Van uw vergoode <rhyme label="b" type="m">Mond</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">Dan ryd myn hert op <rhyme label="c" type="f">stelten</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat zweer ik by zint <rhyme label="c" type="f" corresp="#s3:l4">felten</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En heelt myn diepe <rhyme label="b" type="m" corresp="#s3:l3">wond</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gants bloed dat kusje <rhyme label="a" type="f">smaaktme</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Inwendig, en het <rhyme label="a" type="f" corresp="#s4:l1">raaktme</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Tot 't centrum van myn <rhyme label="b" type="m">hert</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Wat zyn de schoone <rhyme label="c" type="f">lippen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Al steyle <rhyme label="c" type="f" corresp="#s4:l4">Minneklippen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Daar m'opgesmeten <rhyme label="b" type="m" corresp="#s4:l3">werd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Liefhebbers, die t wel <rhyme label="a" type="f">meugen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Kom proef nog eens de <rhyme label="a" type="f" corresp="#s5:l1">teugen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Van Bacchus onzen <rhyme label="b" type="m">Vrind</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Wy zullen nu eens <rhyme label="c" type="f">trooyen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En tonen door ons <rhyme label="c" type="f" corresp="#s5:l4">pooyen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hoe dat hy werd <rhyme label="b" type="m" corresp="#s5:l3">bemind</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ey zie, zy geeft m'een <rhyme label="a" type="f">lonkje</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat 's beter als een <rhyme label="a" type="f" corresp="#s6:l1">dronkje</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Kom hou dan bekje <rhyme label="b" type="m">by</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Laat vry die wil nu <rhyme label="c" type="f">drinken</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ik lach met al het <rhyme label="c" type="f" corresp="#s6:l4">klinken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En voeg me tot <rhyme label="b" type="m" corresp="#s6:l3">gevrey</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
