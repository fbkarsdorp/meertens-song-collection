    <melody>Psalm 146 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEL op myn siel wilt nu <rhyme label="a" type="f">prysen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Den Heer', want myn leuen <rhyme label="b" type="m">langh</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Sal ick hem eere <rhyme label="a" type="f" corresp="#s1:l1">bewysen</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Ick wil met Psalmen <rhyme label="b" type="m" corresp="#s1:l2">ghesangh</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Steedts louen Godt <rhyme label="c" type="f">verheuen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">So langh' als ick sal <rhyme label="c" type="f" corresp="#s1:l5">leuen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt niet stellen v' <rhyme label="a" type="f">vertrauwen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Op Princen groot ofte <rhyme label="b" type="m">kleen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wilt op den mensche niet <rhyme label="a" type="f" corresp="#s2:l1">bauwen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Want by hem is hulpe <rhyme label="b" type="m" corresp="#s2:l2">gheen</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Als sijn gheest wtuaert <rhyme label="c" type="f">ghelyck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wert hy weder ęerd' end <rhyme label="c" type="f" corresp="#s2:l5">slyck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Met hem vergaen sijn raeden <rhyme label="a" type="f">al</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">End werden haestlick tot <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wel hem dien Godt t'aller tydt <rhyme label="a" type="f" corresp="#s3:l1">sal</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Syn hulp' aenbieden met <rhyme label="b" type="m" corresp="#s3:l2">vliet</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Die tot Godt heeft sijn <rhyme label="c" type="f">toevlucht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In noot end in quaet <rhyme label="c" type="f" corresp="#s3:l5">gherucht'</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy is't die krachtigh ghemaeckt <rhyme label="a" type="f">heeft</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Den hemel end t'ęerdtryck <rhyme label="b" type="m">breedt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die Zee end all wat daer in <rhyme label="a" type="f" corresp="#s4:l1">leeft</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">End hem beweeght met <rhyme label="b" type="m" corresp="#s4:l2">bescheedt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Hy onderhoudt sijn <rhyme label="c" type="f">waerheit</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Seer vast' in der <rhyme label="c" type="f" corresp="#s4:l5">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hy doet hen recht die daer <rhyme label="a" type="f">lyden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ouerlast end groot <rhyme label="b" type="m">ghewelt</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Hy gheeft broot tot allen <rhyme label="a" type="f" corresp="#s5:l1">tyden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Hen die de hongher scherp <rhyme label="b" type="m" corresp="#s5:l2">quelt</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Hy maeckt los end vry van <rhyme label="c" type="f">pyn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hen die vast'ghebonden <rhyme label="c" type="f" corresp="#s5:l5">sijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">T'ghesichte gheeft hy hen <rhyme label="a" type="f">allen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Die daer gantschelick sijn <rhyme label="b" type="m">blindt</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Sy, die hart sijn <rhyme label="a" type="f" corresp="#s6:l1">nedergheuallen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Heft hy op, end die <rhyme label="b" type="m" corresp="#s6:l2">versint</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Hy heeft lief ende <rhyme label="c" type="f">behoedt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die oprecht vrom sijn end <rhyme label="c" type="f" corresp="#s6:l5">goet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Met vliet bewaert oock de <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Den vremden in lyden <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Desghelycks die weesen <rhyme label="a" type="f" corresp="#s7:l1">teere</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Behoedt Godt in angst end <rhyme label="b" type="m" corresp="#s7:l2">noot</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Weduwen hy oock <rhyme label="c" type="f">bewaert</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Dat hen gheen leedt <rhyme label="c" type="f" corresp="#s7:l5">wedervaert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hy sal die weghen <rhyme label="a" type="f">verderuen</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Des godtloosen met der <rhyme label="b" type="m">daet</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">End sal hem een ryck <rhyme label="a" type="f" corresp="#s8:l1">verweruen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Dat eewiglick vast <rhyme label="b" type="m" corresp="#s8:l2">bestaet</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Sien ws Godts <rhyme label="c" type="f">heerlickheit</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Wert gheloeft in <rhyme label="c" type="f" corresp="#s8:l5">eewicheit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
