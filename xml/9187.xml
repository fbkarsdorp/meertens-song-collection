    <tags/>
    <melody>La Dauphine</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">OP ien Sondach tot Kees <rhyme label="a" type="m">Koeck</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Raeckte ick iens inden <rhyme label="a" type="m" corresp="#s1:l1">hoeck</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Van't deurtje // van't <rhyme label="b" type="f">deurtje</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Met ien spelder nieuwe <rhyme label="a" type="m" corresp="#s1:l1">Broeck</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Quam daer aen treen Jans <rhyme label="b" type="f" corresp="#s1:l3">Geurtje</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Heer het selschip was soo <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat hy quam met Trijn en <rhyme label="a" type="m" corresp="#s2:l1">Fy</rhyme></l>
        <l id="s2:l3" met="2" type="-+">In Kniertje // in <rhyme label="b" type="f">Kniertje</rhyme></l>
        <l id="s2:l4" met="4" type="+-">t'Segens 'tlach soo seyde <rhyme label="a" type="m" corresp="#s2:l1">sy</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Piet brocht Geurt stracx ien <rhyme label="b" type="f" corresp="#s2:l3">biertje</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hay laet comen seyde <rhyme label="a" type="m">Geurt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">'tIs sen leven niet <rhyme label="a" type="m" corresp="#s3:l1">ebeurt</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Say Claertje // Say <rhyme label="b" type="f">Claertje</rhyme></l>
        <l id="s3:l4" met="4" type="+-">'t Is uyt, soo hem niemes <rhyme label="a" type="m" corresp="#s3:l1">steurt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ick wet hy drinckt ien <rhyme label="b" type="f" corresp="#s3:l3">paertje</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Syme gingh ierst aen den <rhyme label="a" type="m">dans</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Met Jaep, in Griet, Teun, Ael, in <rhyme label="a" type="m" corresp="#s4:l1">Hans</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">In Koentje // in <rhyme label="b" type="f">Koentje</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Maer Jan en Kees hadde gien <rhyme label="a" type="m" corresp="#s4:l1">kans</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Geurt gat Trijntje ien <rhyme label="b" type="f" corresp="#s4:l3">soentje</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Heer hoe lustich can Jaeps <rhyme label="a" type="m">Trijn</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Dansen als daer Vryers <rhyme label="a" type="m" corresp="#s5:l1">zijn</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Tot Keesjes // tot <rhyme label="b" type="f">Keesjes</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">S'overtreft Sely en <rhyme label="a" type="m" corresp="#s5:l1">Stijn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Se danst mit al de <rhyme label="b" type="f" corresp="#s5:l3">geesjes</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Kees knol riep staech om de <rhyme label="a" type="m">Kan</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Langh men daer iens 'tseje <rhyme label="a" type="m" corresp="#s6:l1">an</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Ien twietje, ien <rhyme label="b" type="f">twietje</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Riep inde aere hoeck ien <rhyme label="a" type="m" corresp="#s6:l1">Man</rhyme></l>
        <l id="s6:l5" met="3" type="-+">En Crelis om ien <rhyme label="b" type="f" corresp="#s6:l3">bietje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wie moet hier hebben de <rhyme label="a" type="m">A</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Riep de Jonghen after <rhyme label="a" type="m" corresp="#s7:l1">na</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Int Cietje // int <rhyme label="b" type="f">Cietje</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Was voor Piet, en Jaep de <rhyme label="a" type="m" corresp="#s7:l1">Ka</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En Jongh Kees had het <rhyme label="b" type="f" corresp="#s7:l3">Dietje</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">'t Was gien giest die niet en <rhyme label="a" type="m">sprack</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Hayn die riep vast om <rhyme label="a" type="m" corresp="#s8:l1">Toeback</rhyme></l>
        <l id="s8:l3" met="2" type="-+">In Pleuntje // in <rhyme label="b" type="f">Pleuntje</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Quam proncken mit het Sundachs <rhyme label="a" type="m" corresp="#s8:l1">Jack</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Daer van Haes Ariaens <rhyme label="b" type="f" corresp="#s8:l3">Teuntje</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Van het vier quam Tymen <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">'tLijf was vol, hy con niet <rhyme label="a" type="m" corresp="#s9:l1">staen</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Hiel droncken // hiel <rhyme label="b" type="f">droncken</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Terwijl Jan Gysen steelwijs <rhyme label="a" type="m" corresp="#s9:l1">aen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Sijn Vrijster scheen te <rhyme label="b" type="f" corresp="#s9:l3">loncken</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Somma, elck kreech daer sen <rhyme label="a" type="m">deel</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Quint die speulde op de <rhyme label="a" type="m" corresp="#s10:l1">Veel</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Se spronge // se <rhyme label="b" type="f">spronge</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Tot het ghelt was door de <rhyme label="a" type="m" corresp="#s10:l1">keel</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En Teun en Lijfje <rhyme label="b" type="f" corresp="#s10:l3">songhe</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Doe de vocht clam inde <rhyme label="a" type="m">Cop</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">En 't Lijf was elaen vol <rhyme label="a" type="m" corresp="#s11:l1">sop</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Dees maetjes // dees <rhyme label="b" type="f">maetjes</rhyme></l>
        <l id="s11:l4" met="4" type="+-">Sloeghen in het hoopje <rhyme label="a" type="m" corresp="#s11:l1">op</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Haer eyghen <rhyme label="b" type="f" corresp="#s11:l3">Cameraetjes</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Daer tarte Kees Jongh' Kees <rhyme label="a" type="m">uyt</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Maer hy kreech wat op sen <rhyme label="a" type="m" corresp="#s12:l1">huyt</rhyme></l>
        <l id="s12:l3" met="2" type="-+">En d'aere // en <rhyme label="b" type="f">d'aere</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Knechs en Maysjes riepen <rhyme label="a" type="m" corresp="#s12:l1">luyt</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Ick wist niet waer we <rhyme label="b" type="f" corresp="#s12:l3">ware</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">'k Seyde Willem wilje na <rhyme label="a" type="m">Stee</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">'t Volckje is hier hiel <rhyme label="a" type="m" corresp="#s13:l1">t'onvree</rhyme>,</l>
        <l id="s13:l3" met="2" type="-+">We droncke // we <rhyme label="b" type="f">droncke</rhyme></l>
        <l id="s13:l4" met="4" type="+-">'t Pijntje uyt en doe daer <rhyme label="a" type="m" corresp="#s13:l1">mee</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Wy 'toverschot Kees <rhyme label="b" type="f" corresp="#s13:l3">schonken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
