  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Cecilia: liefste <rhyme label="a" type="f">Engelinne</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">die aen my zoo zeer <rhyme label="b" type="m">behaegd</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">ziet hoe ik uw <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme></l>
        <l id="s1:l4" met="4" type="+-">en myn hert naer u <rhyme label="b" type="m" corresp="#s1:l2">jaegd</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">want gy zyt myn <rhyme label="c" type="f">uytverkoren</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Cecilia, liefste <rhyme label="d" type="m">zoet</rhyme>!</l>
        <l id="s1:l7" met="4" type="+-">'t schynt dat gy ook zyt <rhyme label="c" type="f" corresp="#s1:l5">geboren</rhyme></l>
        <l id="s1:l8" met="2" type="+-">voor Ferdinands <rhyme label="d" type="m" corresp="#s1:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ferdinand, wat voor <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">of wat voor een minne <rhyme label="b" type="m">klagt</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">komt, gy hier nu op <rhyme label="a" type="f" corresp="#s2:l1">heden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">doen, in 't midden van den <rhyme label="b" type="m" corresp="#s2:l2">nagt</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">wilt my hier in ruste <rhyme label="c" type="f">laeten</rhyme>!</l>
        <l id="s2:l6" met="3" type="+-">want de zoete <rhyme label="d" type="m">liberteyt</rhyme></l>
        <l id="s2:l7" met="4" type="+-">bemin ik zeer boven <rhyme label="c" type="f" corresp="#s2:l5">maeten</rhyme>.</l>
        <l id="s2:l8" met="2" type="+-">neemt van hier <rhyme label="d" type="m" corresp="#s2:l6">afscheyd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Cecilia hoort myn <rhyme label="a" type="f">klagten</rhyme></l>
        <l id="s3:l2" met="4" type="+-">en ontfangt uw' <rhyme label="b" type="m">serviteur</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">laet uw jeugdig hert <rhyme label="a" type="f" corresp="#s3:l1">verzagten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">en opent doch uw <rhyme label="b" type="m" corresp="#s3:l2">deur</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">voor my die zoo is <rhyme label="c" type="f">genegen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">om te zyn met uw in <rhyme label="d" type="m">trouw</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">daerom, laet ons t'saemen <rhyme label="c" type="f" corresp="#s3:l5">plegen</rhyme>;</l>
        <l id="s3:l8" met="2" type="+-">en word doch myn' <rhyme label="d" type="m" corresp="#s3:l6">Vrouw</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik ben al te jong van <rhyme label="a" type="f">jaeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">om de trouw te nemen <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">daerom wilt uw moeyte <rhyme label="a" type="f" corresp="#s4:l1">spaeren</rhyme></l>
        <l id="s4:l4" met="4" type="+-">en vry op een ander <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">en ook al te zwak van <rhyme label="c" type="f">leden</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">daer by ook nog veel te <rhyme label="d" type="m">teer</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">om met uw in trouw te <rhyme label="c" type="f" corresp="#s4:l5">treden</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">stoort myne rust niet <rhyme label="d" type="m" corresp="#s4:l6">meer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Lief! 't is in onz' jonge <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">dat men komen moet in <rhyme label="b" type="m">trouw</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">zegt wie heeft'er oyt <rhyme label="a" type="f" corresp="#s5:l1">behaegen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">in een ouden man of <rhyme label="b" type="m" corresp="#s5:l2">vrouw</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">daerom laet ons t'saemen <rhyme label="c" type="f">paeren</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">in het bloeyen van ons <rhyme label="d" type="m">jeugd</rhyme></l>
        <l id="s5:l7" met="4" type="+-">en uyt liefde in min <rhyme label="c" type="f" corresp="#s5:l5">vergaeren</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">met veel eer en <rhyme label="d" type="m" corresp="#s5:l6">deugd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ferdinand g'hebt door uw <rhyme label="a" type="f">zoet</rhyme></l>
        <l id="s6:l2" met="4" type="+-">bekort myn zinnen en gemoed, <rhyme label="b" type="m">(spreke</rhyme></l>
        <l id="s6:l3" met="3" type="+-">'k voel my door de liefde <rhyme label="a" type="f" corresp="#s6:l1">ontsteken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Ã´ getrouwen minnaer <rhyme label="b" type="m" corresp="#s6:l2">zoet</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">ik gaen my met uw <rhyme label="c" type="f">begeven</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">en in zoete <rhyme label="d" type="m">minzaemheyd</rhyme></l>
        <l id="s6:l7" met="4" type="+-">zullen wy te saemen <rhyme label="c" type="f" corresp="#s6:l5">leven</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">tot de dood ons <rhyme label="d" type="m" corresp="#s6:l6">scheyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
