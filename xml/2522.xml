  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">De blijdscap komt van <rhyme label="a" type="m">mijn</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Een yeder juyghd van <rhyme label="b" type="m">vreughd</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een woningh vande <rhyme label="a" type="m" corresp="#s1:l1">pijn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Is, laes! mijn jonghe <rhyme label="b" type="m" corresp="#s1:l2">jeughd</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Ay! hoe bochten mijn gedachten // <rhyme label="c" type="m">wild</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Om d'ontrouw die ick <rhyme label="d" type="m">doe</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Ay Hemel! al mijn klachten // <rhyme label="c" type="m" corresp="#s1:l5">stild</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">'t En quam by my niet <rhyme label="d" type="m" corresp="#s1:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Speelt, springht, en <rhyme label="a" type="m">quinckeleert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Vervrolijckt, u <rhyme label="b" type="m">verblyd</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Myn droevigh hart <rhyme label="a" type="m" corresp="#s2:l1">begeert</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wat vryheyd, en wat <rhyme label="b" type="m" corresp="#s2:l2">tijd</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Ay! hoe bochten mijn gedachten // <rhyme label="c" type="m">wilt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Om d'ontrouw die ick <rhyme label="d" type="m">doe</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ay Hemel! al mijn klachten // <rhyme label="c" type="m" corresp="#s2:l5">stild</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">'t En quam by my niet <rhyme label="d" type="m" corresp="#s2:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Besadight u, mijn <rhyme label="a" type="m">hart</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hoe holdy dus <rhyme label="b" type="m">ontoomt</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Helaes! van enckel <rhyme label="a" type="m" corresp="#s3:l1">smart</rhyme></l>
        <l id="s3:l4" met="3" type="-+">'t Gesicht van tranen <rhyme label="b" type="m" corresp="#s3:l2">stroomt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Ay! hoe bochten mijn gedachten // <rhyme label="c" type="m">wild</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Om d'ontrouw die ick <rhyme label="d" type="m">doe</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Ay Hemel! al mijn klachten // <rhyme label="c" type="m" corresp="#s3:l5">stild</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">'t En quam by my niet <rhyme label="d" type="m" corresp="#s3:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De leckernye <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die, proevende, <rhyme label="b" type="m">vermaeckt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">In mynen mond als <rhyme label="a" type="m" corresp="#s4:l1">roet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Of bitt're galle <rhyme label="b" type="m" corresp="#s4:l2">smaackt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Ay! hoe bochten mijn gedachten // <rhyme label="c" type="m">wild</rhyme>?</l>
        <l id="s4:l6" met="3" type="-+">Om d'ontrouw die ick <rhyme label="d" type="m">doe</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Ay Hemel! al mijn klachten // <rhyme label="c" type="m" corresp="#s4:l5">stilt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ten quam by my niet <rhyme label="d" type="m" corresp="#s4:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De blije sang en <rhyme label="a" type="m">spel</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Luyd-klagend' in mijn <rhyme label="b" type="m">oor</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">My dunckt dat ick seer <rhyme label="a" type="m" corresp="#s5:l1">hel</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Mijn Lief daer suchten <rhyme label="b" type="m" corresp="#s5:l2">hoor</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Ay! hoe bochten mijn gedachten // <rhyme label="c" type="m">wilt</rhyme>!</l>
        <l id="s5:l6" met="3" type="-+">Om d' ontrouw die ick <rhyme label="d" type="m">doe</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Ay Hemel! al mijn klachten // <rhyme label="c" type="m" corresp="#s5:l5">stilt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Ten quam by my niet <rhyme label="d" type="m" corresp="#s5:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De lieffelijcke <rhyme label="a" type="m">Lucht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat is my niet dan <rhyme label="b" type="m">stanck</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De Reuck die and'ren <rhyme label="a" type="m" corresp="#s6:l1">helpt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Die maackt my flauw en <rhyme label="b" type="m" corresp="#s6:l2">kranck</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Ay! hoe bochten mijn gedachten // <rhyme label="c" type="m">wilt</rhyme>!</l>
        <l id="s6:l6" met="3" type="-+">Om d' ontrouw die ick <rhyme label="d" type="m">doe</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Ay Hemel! al mijn klachten // <rhyme label="c" type="m" corresp="#s6:l5">stilt</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ten quam by my niet <rhyme label="d" type="m" corresp="#s6:l6">toe</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ick doe niet dan ick <rhyme label="a" type="m">suf</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Afwysich, schuw, en <rhyme label="b" type="m">koel</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">'k Heb in mijn Man geen <rhyme label="a" type="m" corresp="#s7:l1">puf</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Noch lieffelijck <rhyme label="b" type="m" corresp="#s7:l2">gevoel</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Hoe bochten mijn gedachten // <rhyme label="c" type="m">wilt</rhyme>?</l>
        <l id="s7:l6" met="3" type="-+">Om d' ontrouw die ick <rhyme label="d" type="m">doe</rhyme>.</l>
        <l id="s7:l7" met="4" type="+-">Ach Hemel! al mijn klachten // <rhyme label="c" type="m" corresp="#s7:l5">stilt</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Ten quam by my niet <rhyme label="d" type="m" corresp="#s7:l6">toe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
