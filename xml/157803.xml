    <tags/>
    <melody>Daar was laatst een meisje loos</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Hier van daen naer het <rhyme label="a" type="m">Groenewout</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Mietje doet <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Mietje doet <rhyme label="b" type="f" corresp="#s1:l2">open</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hier van daen naar ‘t <rhyme label="a" type="m" corresp="#s1:l1">Groenewout</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Mietje doet <rhyme label="b" type="f" corresp="#s1:l2">open</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Myn voetjes zyn <rhyme label="a" type="m" corresp="#s1:l1">kout</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hier van daen na de witte <rhyme label="a" type="m">Zwaen</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Kaetje doet <rhyme label="b" type="f">open</rhyme></l>
        <l id="s2:l3" met="2" type="+-">Kaetje doet <rhyme label="b" type="f" corresp="#s2:l2">open</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hier van daen naar de witte <rhyme label="a" type="m" corresp="#s2:l1">Swaen</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Kaetje doet <rhyme label="b" type="f" corresp="#s2:l2">open</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Ik moet ‘er eens <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hier van daen na de geele <rhyme label="a" type="m">Trompet</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Pleuntje doet <rhyme label="b" type="f">open</rhyme>;</l>
        <l id="s3:l3" met="2" type="+-">Pleuntje doet <rhyme label="b" type="f" corresp="#s3:l2">open</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Hier van daen na de geele <rhyme label="a" type="m" corresp="#s3:l1">Trompet</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Pleuntje doet <rhyme label="b" type="f" corresp="#s3:l2">open</rhyme>:</l>
        <l id="s3:l6" met="2" type="-+">Ik moet ‘er na <rhyme label="a" type="m" corresp="#s3:l1">bed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hier van daen na de gouwe <rhyme label="a" type="m">Klok</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Iurrie doet <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Iurrie doet <rhyme label="b" type="f" corresp="#s4:l2">open</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Hier van daen na de Gouwe <rhyme label="a" type="m" corresp="#s4:l1">Klok</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Iurrie doet <rhyme label="b" type="f" corresp="#s4:l2">open</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Ik moet ‘er eens <rhyme label="a" type="m" corresp="#s4:l1">op</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hier van daen naer Moeder <rhyme label="a" type="m">malet</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Moet doet <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Moeder doet <rhyme label="b" type="f" corresp="#s5:l2">open</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Hier van daen naer Moeder <rhyme label="a" type="m" corresp="#s5:l1">malet</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Moeder doet <rhyme label="b" type="f" corresp="#s5:l2">open</rhyme>:</l>
        <l id="s5:l6" met="2" type="-+">Ik moet ‘er na <rhyme label="a" type="m" corresp="#s5:l1">bed</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hier van daen naer de <rhyme label="a" type="m">Wildeman</rhyme></l>
        <l id="s6:l2" met="2" type="+-">Seytje doet <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Seytje doet <rhyme label="b" type="f" corresp="#s6:l2">open</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Hier van daen naer de <rhyme label="a" type="m" corresp="#s6:l1">Wildeman</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Seytje doet <rhyme label="b" type="f" corresp="#s6:l2">open</rhyme>:</l>
        <l id="s6:l6" met="2" type="-+">Ic moet ‘er eens <rhyme label="a" type="m" corresp="#s6:l1">aen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hy za viva la <rhyme label="a" type="m">Krakeling</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Als ik je <rhyme label="b" type="f">sie</rhyme>:</l>
        <l id="s7:l3" met="2" type="+-">Dan moet ik je <rhyme label="b" type="f" corresp="#s7:l2">vatten</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Hy za viva la <rhyme label="a" type="m" corresp="#s7:l1">krakeling</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Als ik je <rhyme label="b" type="f" corresp="#s7:l2">sie</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Dan staet myn <rhyme label="a" type="m" corresp="#s7:l1">zin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Waer om zou myn sin niet <rhyme label="a" type="m">staen</rhyme>?</l>
        <l id="s8:l2" met="2" type="+-">Als ik je <rhyme label="b" type="f">Roosje</rhyme>;</l>
        <l id="s8:l3" met="2" type="+-">Als ik je <rhyme label="b" type="f" corresp="#s8:l2">Roosje</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Waerom zou myn sin niet <rhyme label="a" type="m" corresp="#s8:l1">staen</rhyme>:</l>
        <l id="s8:l5" met="2" type="+-">Als ik je <rhyme label="b" type="f" corresp="#s8:l2">Roosje</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Sie open <rhyme label="a" type="m" corresp="#s8:l1">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
