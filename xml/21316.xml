    <melody>Toutes les nuits</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">DEn slaep <rhyme label="a" type="m">ontwaect</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die nv in sonden <rhyme label="b" type="f">leuen</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Rijst op en <rhyme label="a" type="m" corresp="#s1:l1">smaect</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gods goetheyt nv <rhyme label="b" type="f" corresp="#s1:l2">ghegheuen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Den winter <rhyme label="c" type="m">cout</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Den reghen wreet en <rhyme label="c" type="m" corresp="#s1:l5">stout</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Is <rhyme label="d" type="m">ghepasseert</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Dlicht <rhyme label="d" type="m" corresp="#s1:l7">domineert</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Ierusalem <rhyme label="e" type="f">ghepresen</rhyme></l>
        <l id="s1:l10" met="5" type="+-">Is nv gebout, daer in ons vruecht moet <rhyme label="e" type="f" corresp="#s1:l9">wezen</rhyme></l>
        <l id="s1:l11" met="2" type="-+">De bruyt <rhyme label="f" type="m">verchiert</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Reyn <rhyme label="f" type="m" corresp="#s1:l11">ghemaniert</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Is onder ons <rhyme label="g" type="f">ghesonden</rhyme></l>
        <l id="s1:l14" met="2" type="-+">Blijschap <rhyme label="f" type="m" corresp="#s1:l11">anthiert</rhyme></l>
        <l id="s1:l15" met="2" type="-+">Haer vruchten <rhyme label="f" type="m" corresp="#s1:l11">viert</rhyme></l>
        <l id="s1:l16" met="3" type="-+">Als dienaers tallen <rhyme label="g" type="f" corresp="#s1:l13">stonden</rhyme></l>
        <l id="s1:l17" met="2" type="-+">In liefden <rhyme label="a" type="m" corresp="#s1:l1">blaect</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Haer claerheyt <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Uerschijnt in alle <rhyme label="b" type="f">hoecken</rhyme></l>
        <l id="s2:l3" met="2" type="-+">De Heydens <rhyme label="a" type="m" corresp="#s2:l1">bloot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Daer salicheyt in <rhyme label="b" type="f" corresp="#s2:l2">soecken</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Haer mueren <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Uan Iaspis steenen <rhyme label="c" type="m" corresp="#s2:l5">fijn</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Seer <rhyme label="d" type="m">triumphael</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Als claer <rhyme label="d" type="m" corresp="#s2:l7">Cristael</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Is sy reyn van <rhyme label="e" type="f">beschouwe</rhyme></l>
        <l id="s2:l10" met="5" type="+-">Haer straten die zijn van louteren <rhyme label="e" type="f" corresp="#s2:l9">gouwe</rhyme></l>
        <l id="s2:l11" met="2" type="-+">Haer <rhyme label="f" type="m">fondament</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Seer <rhyme label="f" type="m" corresp="#s2:l11">excellent</rhyme></l>
        <l id="s2:l13" met="3" type="-+">Is dierbaer van <rhyme label="g" type="f">ghesmijden</rhyme></l>
        <l id="s2:l14" met="2" type="-+">Wie is so <rhyme label="f" type="m" corresp="#s2:l11">blent</rhyme></l>
        <l id="s2:l15" met="2" type="-+">Oft <rhyme label="f" type="m" corresp="#s2:l11">onbekent</rhyme></l>
        <l id="s2:l16" met="3" type="-+">Die haer nv stelt <rhyme label="g" type="f" corresp="#s2:l13">bezijden</rhyme></l>
        <l id="s2:l17" met="2" type="-+">Om Babel <rhyme label="a" type="m" corresp="#s2:l1">snoot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Salich zijn <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die binnen haer <rhyme label="b" type="f">bemueren</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Sijn staende <rhyme label="a" type="m" corresp="#s3:l1">vry</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Gheen leet sal haer <rhyme label="b" type="f" corresp="#s3:l2">aenrueren</rhyme></l>
        <l id="s3:l5" met="2" type="-+">So sy met <rhyme label="c" type="m">vruecht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Uerschijnt in alder <rhyme label="c" type="m" corresp="#s3:l5">duecht</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Moeten oock <rhyme label="d" type="m">staen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Die daer in <rhyme label="d" type="m" corresp="#s3:l7">gaen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Nieu inden gheest <rhyme label="e" type="f">herboren</rhyme></l>
        <l id="s3:l10" met="5" type="+-">En strijtbaer zijnde, totten strijt <rhyme label="e" type="f" corresp="#s3:l9">ghecoren</rhyme></l>
        <l id="s3:l11" met="2" type="-+">Die Dlam <rhyme label="f" type="m">bekant</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Uolghen <rhyme label="f" type="m" corresp="#s3:l11">playsant</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Sijn maechden sonder <rhyme label="g" type="f">vlecken</rhyme></l>
        <l id="s3:l14" met="2" type="-+">Die sullen <rhyme label="f" type="m" corresp="#s3:l11">stant</rhyme></l>
        <l id="s3:l15" met="2" type="-+">Int lustich <rhyme label="f" type="m" corresp="#s3:l11">lant</rhyme></l>
        <l id="s3:l16" met="3" type="-+">Crijghen sonder <rhyme label="g" type="f" corresp="#s3:l13">vertrecken</rhyme></l>
        <l id="s3:l17" met="2" type="-+">Met blijschap <rhyme label="a" type="m" corresp="#s3:l1">bly</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Christus <rhyme label="a" type="m">eerbaer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Is haren tempel <rhyme label="b" type="f">reene</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Gheen licht <rhyme label="a" type="m" corresp="#s4:l1">voorwaer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Behoeft haer groot noch <rhyme label="b" type="f" corresp="#s4:l2">cleene</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Want Dlam <rhyme label="c" type="m">certeyn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Can haer verlichten <rhyme label="c" type="m" corresp="#s4:l5">reyn</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Dach ende <rhyme label="d" type="m">nacht</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Staen vry <rhyme label="d" type="m" corresp="#s4:l7">bedacht</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Altijt haer poorten <rhyme label="e" type="f">open</rhyme></l>
        <l id="s4:l10" met="5" type="+-">O wel salich zijn sy, die daer in <rhyme label="e" type="f" corresp="#s4:l9">lopen</rhyme></l>
        <l id="s4:l11" met="2" type="-+">Daer vloeyt de <rhyme label="f" type="m">vloet</rhyme></l>
        <l id="s4:l12" met="2" type="-+">Des leuens <rhyme label="f" type="m" corresp="#s4:l11">soet</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Midden op haerder <rhyme label="g" type="f">straten</rhyme></l>
        <l id="s4:l14" met="2" type="-+">De boomkens <rhyme label="f" type="m" corresp="#s4:l11">goet</rhyme></l>
        <l id="s4:l15" met="2" type="-+">Die draghen <rhyme label="f" type="m" corresp="#s4:l11">vroet</rhyme></l>
        <l id="s4:l16" met="3" type="-+">Haer vruchtkens sonder <rhyme label="g" type="f" corresp="#s4:l13">laten</rhyme></l>
        <l id="s4:l17" met="2" type="-+">Rustich en <rhyme label="a" type="m" corresp="#s4:l1">claer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Princelick <rhyme label="a" type="m">saet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Wilt dit <rhyme label="b" type="f">considereren</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Schaft wijsen <rhyme label="a" type="m" corresp="#s5:l1">raet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die dit van my <rhyme label="b" type="f" corresp="#s5:l2">opteren</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Want uwen <rhyme label="c" type="m">fluer</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Staet nv in schoon <rhyme label="c" type="m" corresp="#s5:l5">valuer</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Dies doet <rhyme label="d" type="m">deuoor</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Desen <rhyme label="d" type="m" corresp="#s5:l7">thresoor</rhyme></l>
        <l id="s5:l9" met="3" type="-+">En mach niet elck <rhyme label="e" type="f">verweruen</rhyme></l>
        <l id="s5:l10" met="5" type="+-">Aengrijpt hem nv oft ghy moet eewich <rhyme label="e" type="f" corresp="#s5:l9">steruen</rhyme></l>
        <l id="s5:l11" met="2" type="-+">Gheen twist noch <rhyme label="f" type="m">nijt</rhyme></l>
        <l id="s5:l12" met="2" type="-+">Noch boos <rhyme label="f" type="m" corresp="#s5:l11">verwijt</rhyme></l>
        <l id="s5:l13" met="3" type="-+">En mach daer binnen <rhyme label="g" type="f">comen</rhyme></l>
        <l id="s5:l14" met="2" type="-+">In desen <rhyme label="f" type="m" corresp="#s5:l11">strijt</rhyme></l>
        <l id="s5:l15" met="2" type="-+">Staet ons ter <rhyme label="f" type="m" corresp="#s5:l11">tijt</rhyme></l>
        <l id="s5:l16" met="3" type="-+">Tbehouden of <rhyme label="g" type="f" corresp="#s5:l13">tverdomen</rhyme></l>
        <l id="s5:l17" met="2" type="-+">Dies schuwet <rhyme label="a" type="m" corresp="#s5:l1">tquaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
