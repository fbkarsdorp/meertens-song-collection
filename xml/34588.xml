  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AH vrient, de Spaensche <rhyme label="a" type="f">druyven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Verwermen soo myn <rhyme label="b" type="m">Borst</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat voor my he'en heen moet <rhyme label="a" type="f" corresp="#s1:l1">stuyven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Den kloecksten heldt en <rhyme label="b" type="m" corresp="#s1:l2">Vorst</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wat 's Spaigniaert, u <rhyme label="c" type="f">bedryven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ontvlucht ghy 't edel <rhyme label="d" type="m">vat</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat ons doet Meester <rhyme label="c" type="f" corresp="#s1:l5">blyven</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Voor ieders heyr, en <rhyme label="d" type="m" corresp="#s1:l6">stadt</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">V welvaert, wilt ghy 't <rhyme label="a" type="f">weten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hanght aan dat edel <rhyme label="b" type="m">vocht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En wordt ghy <rhyme label="a" type="f" corresp="#s2:l1">neer-ghesmeten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Soo wyt het dese <rhyme label="b" type="m" corresp="#s2:l2">locht</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Want brocht ghy Bacchus <rhyme label="c" type="f">Wapen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Tot u behoudingh <rhyme label="d" type="m">me</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Al watter is <rhyme label="c" type="f" corresp="#s2:l5">gheschapen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ontsagh u zweerdt, en <rhyme label="d" type="m" corresp="#s2:l6">sne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">V deghens konnen <rhyme label="a" type="f">hacken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door Fransche deghens <rhyme label="b" type="m">he'en</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">'t En zy sy u <rhyme label="a" type="f" corresp="#s3:l1">ontsacken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Door al te dorre <rhyme label="b" type="m" corresp="#s3:l2">le'en</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Ey spoelt, spoelt dan de <rhyme label="c" type="f">glasen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Versekert u in <rhyme label="d" type="m">tydt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Oft al u kloeckste <rhyme label="c" type="f" corresp="#s3:l5">basen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Zyn kracht, en voordeel <rhyme label="d" type="m" corresp="#s3:l6">quyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy Duytsche konnen <rhyme label="a" type="f">tuygen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wat Spaenschen wyn <rhyme label="b" type="m">vermagh</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Noyt sloegh-men ons in <rhyme label="a" type="f" corresp="#s4:l1">duygen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Als onder 't <rhyme label="b" type="m" corresp="#s4:l2">bier-ghelagh</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Verflouwen wy in 't <rhyme label="c" type="f">vechten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">'t Is by ghebreck van <rhyme label="d" type="m">wyn</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Want Neder-duytsche <rhyme label="c" type="f" corresp="#s4:l5">knechten</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Noyt anders kloeck en <rhyme label="d" type="m" corresp="#s4:l6">zyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
