  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Haar aangenaam keeltjen, (als 't gorgeltje <rhyme label="a" type="m">luit</rhyme>,)</l>
        <l id="s1:l2" met="1" type="-+">Luit <rhyme label="b" type="f">heller</rhyme>,</l>
        <l id="s1:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s1:l2">scheller</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Als cimbel of <rhyme label="a" type="m" corresp="#s1:l1">fluit</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">'t Schijnt zij, het <rhyme label="c" type="m">gebergt</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Van Helikon, <rhyme label="c" type="m" corresp="#s1:l5">tergt</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">VVanneerze, uit den borst, een zangrijmpjen <rhyme label="a" type="m" corresp="#s1:l1">uit</rhyme>.</l>
      </lg>
    </lg>
