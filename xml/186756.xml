<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186756</id>
    <date>
      <year-start>1674</year-start>
      <year-end>1674</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 081 Datheen</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Singt een vroolik <rhyme label="a" type="m">lied</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Onsen God, en <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Die ons sterkte <rhyme label="a" type="m" corresp="#s1:l1">biedt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Juicht, en roept van <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">In uw geest <rhyme label="c" type="m" corresp="#s1:l4">verheugt</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Jakobs God ter <rhyme label="b" type="f" corresp="#s1:l2">eere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Singt een nieuwen <rhyme label="a" type="m">psalm</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Doet den trommel <rhyme label="b" type="f">geeven</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Slaat een schoonen <rhyme label="a" type="m" corresp="#s2:l1">galm</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Lieflik van <rhyme label="c" type="m">geluit</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Op de harp, en <rhyme label="c" type="m" corresp="#s2:l4">luidt</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Saamen <rhyme label="b" type="f" corresp="#s2:l2">opgeheeven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Laat basuinen <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Schalt met heldre <rhyme label="b" type="f">toonen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">In de nieuwe <rhyme label="a" type="m" corresp="#s3:l1">maan</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Wilt aldus <rhyme label="c" type="m">verblydt</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Ter bestemder <rhyme label="c" type="m" corresp="#s3:l4">tyd</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Onsen feestdag <rhyme label="b" type="f" corresp="#s3:l2">kroonen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Sulx liet Jakobs <rhyme label="a" type="m">God</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Israël <rhyme label="b" type="f">gebieden</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">'t Is syn rechts <rhyme label="a" type="m" corresp="#s4:l1">gebod</rhyme>.</l>
        <l id="s4:l4" met="3" type="+-">Die insetting <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Ten <rhyme label="c" type="m" corresp="#s4:l4">getuigenis</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Onder Josefs <rhyme label="b" type="f" corresp="#s4:l2">lieden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Want sy kreeg haar <rhyme label="a" type="m">stand</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Doe hy ging uit <rhyme label="b" type="f">moorden</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Door Egiptens <rhyme label="a" type="m" corresp="#s5:l1">land</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Daarwe, met <rhyme label="c" type="m">verdriet</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Lippen, diewe <rhyme label="c" type="m" corresp="#s5:l4">niet</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Recht verstonden, <rhyme label="b" type="f" corresp="#s5:l2">hoorden</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Gy, wien ik het <rhyme label="a" type="m">jok</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Van steeds last te <rhyme label="b" type="f">draagen</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Van den schouder <rhyme label="a" type="m" corresp="#s6:l1">trok</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Wien ik ook de <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Van des potwerks <rhyme label="c" type="m" corresp="#s6:l4">mand</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Eeuwig heb <rhyme label="b" type="f" corresp="#s6:l2">ontslaagen</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Gy, uit angst, en <rhyme label="a" type="m">stryd</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Alsge riept, en <rhyme label="b" type="f">huilde</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Door myn kracht <rhyme label="a" type="m" corresp="#s7:l1">gevrydt</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Wien ik antwoord <rhyme label="c" type="m">sond</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Uit der wolken <rhyme label="c" type="m" corresp="#s7:l4">mond</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Daar de donder <rhyme label="b" type="f" corresp="#s7:l2">schuilde</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Gy, die doe daar <rhyme label="a" type="m">naa</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Aan de <rhyme label="b" type="f">waaterbeeken</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Van uw <rhyme label="a" type="m" corresp="#s8:l1">Meriba</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Eens door my <rhyme label="c" type="m">beproeft</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Voorts my hebt <rhyme label="c" type="m" corresp="#s8:l4">bedroeft</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Door uw twistig <rhyme label="b" type="f" corresp="#s8:l2">spreeken</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Gy, myn volk, hoort <rhyme label="a" type="m">toe</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Ik zal u <rhyme label="b" type="f">betuigen</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Wat ik wil men <rhyme label="a" type="m" corresp="#s9:l1">doe</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Och, myn <rhyme label="c" type="m">Israël</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Woudge myn <rhyme label="c" type="m" corresp="#s9:l4">bevel</rhyme></l>
        <l id="s9:l6" met="3" type="+-">Hooren, en u <rhyme label="b" type="f" corresp="#s9:l2">buigen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Dat geen ander <rhyme label="a" type="m">God</rhyme></l>
        <l id="s10:l2" met="3" type="+-">By u zy <rhyme label="b" type="f">geleeden</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Buigt u niet, als <rhyme label="a" type="m" corresp="#s10:l1">sot</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Voor die, iets in <rhyme label="c" type="m">schyn</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Vreemde gooden <rhyme label="c" type="m" corresp="#s10:l4">zyn</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Ik zy <rhyme label="b" type="f" corresp="#s10:l2">aangebeeden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Ik, de hoogste <rhyme label="a" type="m">magt</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">Ben uw God, de <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Die u herwaart <rhyme label="a" type="m" corresp="#s11:l1">bragt</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Door myn sterke <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">Uit Egiptens <rhyme label="c" type="m" corresp="#s11:l4">land</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">Houd dies my in <rhyme label="b" type="f" corresp="#s11:l2">eere</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Doet dan uwen <rhyme label="a" type="m">mond</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Naa uw nooddruft, <rhyme label="b" type="f">oopen</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">Gaapt zo wydt gy <rhyme label="a" type="m" corresp="#s12:l1">kont</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">En, myn volk, gy <rhyme label="c" type="m">zult</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Door myn gunst <rhyme label="c" type="m" corresp="#s12:l4">vervult</rhyme>,</l>
        <l id="s12:l6" met="3" type="+-">Nimmer hongrig <rhyme label="b" type="f" corresp="#s12:l2">loopen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Maar myn volk, te <rhyme label="a" type="m">wars</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Wou myn stem niet <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">Israël, te <rhyme label="a" type="m" corresp="#s13:l1">dwars</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">Wou niet zoo ik <rhyme label="c" type="m">wou</rhyme>,</l>
        <l id="s13:l5" met="3" type="+-">Maar week <rhyme label="c" type="m" corresp="#s13:l4">ongetrou</rhyme></l>
        <l id="s13:l6" met="3" type="+-">Voorts uit myne <rhyme label="b" type="f" corresp="#s13:l2">spooren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">Dies liet ikse <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s14:l2" met="3" type="+-">Naa hun hart <rhyme label="b" type="f">begeerde</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">In hun blinden <rhyme label="a" type="m" corresp="#s14:l1">waan</rhyme>,</l>
        <l id="s14:l4" met="3" type="+-">En een ieder <rhyme label="c" type="m">ging</rhyme>,</l>
        <l id="s14:l5" met="3" type="+-">In syn <rhyme label="c" type="m" corresp="#s14:l4">wandeling</rhyme>,</l>
        <l id="s14:l6" met="3" type="+-">Naa syn raad hem <rhyme label="b" type="f" corresp="#s14:l2">leerde</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="+-">Och was naa myn <rhyme label="a" type="m">woord</rhyme></l>
        <l id="s15:l2" met="3" type="+-">By myn volk <rhyme label="b" type="f">gehandelt</rhyme>!</l>
        <l id="s15:l3" met="3" type="+-">Had het doch <rhyme label="a" type="m" corresp="#s15:l1">gehoort</rhyme>!</l>
        <l id="s15:l4" met="3" type="+-">Och had <rhyme label="c" type="m">Israël</rhyme>,</l>
        <l id="s15:l5" met="3" type="+-">Naa myn goed <rhyme label="c" type="m" corresp="#s15:l4">bevel</rhyme>,</l>
        <l id="s15:l6" met="3" type="+-">In myn weg <rhyme label="b" type="f" corresp="#s15:l2">gewandelt</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="+-">'k Had hun vyands <rhyme label="a" type="m">magt</rhyme>,</l>
        <l id="s16:l2" met="3" type="+-">Al in 't kort, in 't <rhyme label="b" type="f">lyen</rhyme></l>
        <l id="s16:l3" met="3" type="+-">Onder hen <rhyme label="a" type="m" corresp="#s16:l1">gebragt</rhyme>,</l>
        <l id="s16:l4" met="3" type="+-">En myn hand, met <rhyme label="c" type="m">slaan</rhyme>,</l>
        <l id="s16:l5" met="3" type="+-">Omgewendt doen <rhyme label="c" type="m" corresp="#s16:l4">gaan</rhyme></l>
        <l id="s16:l6" met="3" type="+-">Teegen hun <rhyme label="b" type="f" corresp="#s16:l2">partyen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="+-">Wie den Heere <rhyme label="a" type="m">haat</rhyme>,</l>
        <l id="s17:l2" met="3" type="+-">Had sich valsch <rhyme label="b" type="f">begeeven</rhyme></l>
        <l id="s17:l3" met="3" type="+-">Onder synen <rhyme label="a" type="m" corresp="#s17:l1">staat</rhyme>,</l>
        <l id="s17:l4" met="3" type="+-">Maar hun goede <rhyme label="c" type="m">tyd</rhyme></l>
        <l id="s17:l5" met="3" type="+-">Waar hen, sonder <rhyme label="c" type="m" corresp="#s17:l4">stryd</rhyme>,</l>
        <l id="s17:l6" met="3" type="+-">Eeuwig <rhyme label="b" type="f" corresp="#s17:l2">bygebleeven</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="+-">Hy had hen, tot <rhyme label="a" type="m">spys</rhyme>,</l>
        <l id="s18:l2" met="3" type="+-">'t Vetst des tarws doen <rhyme label="b" type="f">laaden</rhyme>.</l>
        <l id="s18:l3" met="3" type="+-">Doch blyft gy nu <rhyme label="a" type="m" corresp="#s18:l1">wys</rhyme>.</l>
        <l id="s18:l4" met="3" type="+-">Met des rotsteens <rhyme label="c" type="m">vloed</rhyme>,</l>
        <l id="s18:l5" met="3" type="+-">Hoonigryk, en <rhyme label="c" type="m" corresp="#s18:l4">soet</rhyme>,</l>
        <l id="s18:l6" met="3" type="+-">Zal ik u <rhyme label="b" type="f" corresp="#s18:l2">versaaden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
