<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178685</id>
    <date>
      <year-start>1707</year-start>
      <year-end>1707</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 081 Datheen</melody>
  <author>François Halma</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">ZIngt bly onzen <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Hy is onze <rhyme label="b" type="f">sterkte</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Als die Jakobs <rhyme label="a" type="m" corresp="#s1:l1">lot</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Neemt in zyn <rhyme label="c" type="m">hoede</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Juigt hem, die 't <rhyme label="c" type="m" corresp="#s1:l4">goede</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Steeds voor hem <rhyme label="b" type="f" corresp="#s1:l2">bewerkte</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Zingt hem eenen <rhyme label="a" type="m">Psalm</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">En slaat op den <rhyme label="b" type="f">trommel</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Heldren toon en <rhyme label="a" type="m" corresp="#s2:l1">galm</rhyme>.</l>
        <l id="s2:l4" met="3" type="+-">Op harpe en <rhyme label="c" type="m">luiten</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">En zoete <rhyme label="c" type="m" corresp="#s2:l4">fluiten</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Klink' <rhyme label="b" type="f" corresp="#s2:l2">muzykgedommel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">In de niewe <rhyme label="a" type="m">Maan</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Laat bazuinen <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Datmen 't feest <rhyme label="a" type="m" corresp="#s3:l1">voortaan</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Vrolyk ten <rhyme label="c" type="m">tyde</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Als 't Godt <rhyme label="c" type="m" corresp="#s3:l4">inwyde</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Viere zonder <rhyme label="b" type="f" corresp="#s3:l2">stooren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Want dat is een <rhyme label="a" type="m">wet</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Van Godt zelf <rhyme label="b" type="f">gegeeven</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Door hem <rhyme label="a" type="m" corresp="#s4:l1">ingezet</rhyme>;</l>
        <l id="s4:l4" met="3" type="+-">'t Recht en de <rhyme label="c" type="m">leere</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Van Jakobs <rhyme label="c" type="m" corresp="#s4:l4">Heere</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Josef <rhyme label="b" type="f" corresp="#s4:l2">voorgeschreven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Hy heeft dit <rhyme label="a" type="m">gestelt</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Tot <rhyme label="b" type="f">genade-blyken</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Als hy 't boos <rhyme label="a" type="m" corresp="#s5:l1">gewelt</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Brak van <rhyme label="c" type="m">Egipten</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Dat zy <rhyme label="c" type="m" corresp="#s5:l4">ontslipten</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Toen hy 't deedt <rhyme label="b" type="f" corresp="#s5:l2">bezwyken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Daar heeft het <rhyme label="a" type="m">gehoort</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Onverstaanbre <rhyme label="b" type="f">taalen</rhyme>.</l>
        <l id="s6:l3" met="3" type="+-">'k Heb zyn' schouder <rhyme label="a" type="m" corresp="#s6:l1">voort</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Den last <rhyme label="c" type="m">ontheven</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">En hem <rhyme label="c" type="m" corresp="#s6:l4">gegeeven</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Dat hy aêm kon <rhyme label="b" type="f" corresp="#s6:l2">haalen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Ik ontrok zyn <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Van den <rhyme label="b" type="f">ticheloven</rhyme>.</l>
        <l id="s7:l3" met="3" type="+-">Als gy, <rhyme label="a" type="m" corresp="#s7:l1">overmant</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Riept in het <rhyme label="c" type="m">lyden</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Quam ik u <rhyme label="c" type="m" corresp="#s7:l4">vryden</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Door myn hulp van <rhyme label="b" type="f" corresp="#s7:l2">boven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Ik antwoordde <rhyme label="a" type="m">uit</rhyme></l>
        <l id="s8:l2" met="3" type="+-">'s Donders <rhyme label="b" type="f">donkerheden</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Met een zwaar <rhyme label="a" type="m" corresp="#s8:l1">geluidt</rhyme>.</l>
        <l id="s8:l4" met="3" type="+-">'k Proefde u aan 't <rhyme label="c" type="m">water</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Dat met <rhyme label="c" type="m" corresp="#s8:l4">geschater</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Stroomde naar <rhyme label="b" type="f" corresp="#s8:l2">beneden</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Dat te <rhyme label="a" type="m">Meriba</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Uit de rotz quam <rhyme label="b" type="f">vloeien</rhyme>.</l>
        <l id="s9:l3" met="3" type="+-">Toen zeide ik daar <rhyme label="a" type="m" corresp="#s9:l1">na</rhyme>:</l>
        <l id="s9:l4" met="3" type="+-">Myn volk wil <rhyme label="c" type="m">hooren</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">En leen uw <rhyme label="c" type="m" corresp="#s9:l4">ooren</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Dat uw heil mag <rhyme label="b" type="f" corresp="#s9:l2">groeien</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Ik zal, <rhyme label="a" type="m">Israël</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Onder u <rhyme label="b" type="f">betuigen</rhyme>;</l>
        <l id="s10:l3" met="3" type="+-">Dient, naar myn <rhyme label="a" type="m" corresp="#s10:l1">bevel</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Nooit vreemde <rhyme label="c" type="m">Goden</rhyme>;</l>
        <l id="s10:l5" met="3" type="+-">Wil in uw <rhyme label="c" type="m" corresp="#s10:l4">nooden</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">U voor hen niet <rhyme label="b" type="f" corresp="#s10:l2">buigen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Want ik ben de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">Uwe Godt, ik <rhyme label="b" type="f">leidde</rhyme></l>
        <l id="s11:l3" met="3" type="+-">U, tot myn naams <rhyme label="a" type="m" corresp="#s11:l1">eer</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Uit 's vyandts <rhyme label="c" type="m">banden</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">En Memfis <rhyme label="c" type="m" corresp="#s11:l4">landen</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">In een ruimer <rhyme label="b" type="f" corresp="#s11:l2">weide</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Open uwen <rhyme label="a" type="m">mondt</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Laat uw hart <rhyme label="b" type="f">ontvonken</rhyme></l>
        <l id="s12:l3" met="3" type="+-">Naar myn <rhyme label="a" type="m" corresp="#s12:l1">heilverbondt</rhyme>.</l>
        <l id="s12:l4" met="3" type="+-">Dat myn <rhyme label="c" type="m">genade</rhyme></l>
        <l id="s12:l5" met="3" type="+-">Uw ziel <rhyme label="c" type="m" corresp="#s12:l4">verzaade</rhyme>,</l>
        <l id="s12:l6" met="3" type="+-">U van my <rhyme label="b" type="f" corresp="#s12:l2">geschonken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Maar myn volk, <rhyme label="a" type="m">verhardt</rhyme></l>
        <l id="s13:l2" met="3" type="+-">In zyn booze <rhyme label="b" type="f">wegen</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">Heeft my steeds <rhyme label="a" type="m" corresp="#s13:l1">gesart</rhyme>;</l>
        <l id="s13:l4" met="3" type="+-">'t Hart op myn <rhyme label="c" type="m">wetten</rhyme></l>
        <l id="s13:l5" met="3" type="+-">Niet willen <rhyme label="c" type="m" corresp="#s13:l4">zetten</rhyme>,</l>
        <l id="s13:l6" met="3" type="+-">Als my <rhyme label="b" type="f" corresp="#s13:l2">ongenegen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">Dies gaf ik 't, in <rhyme label="a" type="m">wraak</rhyme>,</l>
        <l id="s14:l2" met="3" type="+-">Over aan zyn <rhyme label="b" type="f">harte</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">Dat het naar zyn' <rhyme label="a" type="m" corresp="#s14:l1">smaak</rhyme></l>
        <l id="s14:l4" met="3" type="+-">Leefde en zyn <rhyme label="c" type="m">zinnen</rhyme>;</l>
        <l id="s14:l5" met="3" type="+-">Om 't quaadt te <rhyme label="c" type="m" corresp="#s14:l4">minnen</rhyme>,</l>
        <l id="s14:l6" met="3" type="+-">Dat myn Godtheit <rhyme label="b" type="f" corresp="#s14:l2">tarte</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="+-">Och, hadt <rhyme label="a" type="m">Israël</rhyme></l>
        <l id="s15:l2" met="3" type="+-">My toch willen <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s15:l3" met="3" type="+-">En, naar myn <rhyme label="a" type="m" corresp="#s15:l1">bevel</rhyme>,</l>
        <l id="s15:l4" met="3" type="+-">Den weg <rhyme label="c" type="m">bewandelt</rhyme>,</l>
        <l id="s15:l5" met="3" type="+-">En trou <rhyme label="c" type="m" corresp="#s15:l4">gehandelt</rhyme>,</l>
        <l id="s15:l6" met="3" type="+-">Als myn <rhyme label="b" type="f" corresp="#s15:l2">uitverkoren</rhyme> !</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="+-">'k Hadde in korten <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s16:l2" met="3" type="+-">'s Vyandts magt <rhyme label="b" type="f">verslagen</rhyme>,</l>
        <l id="s16:l3" met="3" type="+-">Schoon vol woede en <rhyme label="a" type="m" corresp="#s16:l1">spyt</rhyme>;</l>
        <l id="s16:l4" met="3" type="+-">En zyne <rhyme label="c" type="m">benden</rhyme></l>
        <l id="s16:l5" met="3" type="+-">Gedempt, in 't <rhyme label="c" type="m" corresp="#s16:l4">wenden</rhyme></l>
        <l id="s16:l6" met="3" type="+-">Van myn handt, door <rhyme label="b" type="f" corresp="#s16:l2">plaagen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="+-">Zy, die in 't <rhyme label="a" type="m">gemoedt</rhyme></l>
        <l id="s17:l2" met="3" type="+-">Zelfs den Heere <rhyme label="b" type="f">haaten</rhyme>,</l>
        <l id="s17:l3" met="3" type="+-">Hadden hem te <rhyme label="a" type="m" corresp="#s17:l1">voet</rhyme>,</l>
        <l id="s17:l4" met="3" type="+-">Geveinst, <rhyme label="c" type="m">gevallen</rhyme>;</l>
        <l id="s17:l5" met="3" type="+-">En geen van <rhyme label="c" type="m" corresp="#s17:l4">allen</rhyme></l>
        <l id="s17:l6" met="3" type="+-">Immer hem <rhyme label="b" type="f" corresp="#s17:l2">verlaaten</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="+-">Maar hun <rhyme label="a" type="m">levensstaat</rhyme></l>
        <l id="s18:l2" met="3" type="+-">Hadmen lang zien <rhyme label="b" type="f">bloeien</rhyme>.</l>
        <l id="s18:l3" met="3" type="+-">Hy had hun <rhyme label="a" type="m" corresp="#s18:l1">verzaadt</rhyme></l>
        <l id="s18:l4" met="3" type="+-">Met tarwe in <rhyme label="c" type="m">goedheit</rhyme>,</l>
        <l id="s18:l5" met="3" type="+-">En honigs <rhyme label="c" type="m" corresp="#s18:l4">zoetheit</rhyme>,</l>
        <l id="s18:l6" met="3" type="+-">Die de rotz liet <rhyme label="b" type="f" corresp="#s18:l2">vloeien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
