  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EEnigh in dees duyster <rhyme label="a" type="f">Wouden</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Wil ick <rhyme label="b" type="m">straf</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Mijn jeughd' en tijd gaen <rhyme label="a" type="f" corresp="#s1:l1">verouden</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Tot in 't <rhyme label="b" type="m" corresp="#s1:l2">graf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Achter dese hooghe <rhyme label="a" type="f">Berghen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="b" type="m">Onbekent</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wil ick, als een ballinck, <rhyme label="a" type="f" corresp="#s2:l1">terghen</rhyme></l>
        <l id="s2:l4" met="2" type="+-">'t Levens <rhyme label="b" type="m" corresp="#s2:l2">end</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">En ghy Boom, en <rhyme label="a" type="f">woud-Goddinnen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Oock <rhyme label="b" type="m">Napaeaen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wilt dit treuren klaegh'lijck <rhyme label="a" type="f" corresp="#s3:l1">singhen</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s3:l2">verbreen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Weent met my ghy Boss', en <rhyme label="a" type="f">boomen</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Droevigh <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Beeckjens, vlietjens, en ghy <rhyme label="a" type="f" corresp="#s4:l1">stroomen</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">'t <rhyme label="b" type="m" corresp="#s4:l2">Ongeval</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dat ick nu met heete <rhyme label="a" type="f">traenen</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Droef <rhyme label="b" type="m">beschrey</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">En gheboren is uyt <rhyme label="a" type="f" corresp="#s5:l1">waenen</rhyme></l>
        <l id="s5:l4" met="2" type="+-">Van ons <rhyme label="b" type="m" corresp="#s5:l2">bey</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Nu wel aen! ghy wreede <rhyme label="a" type="f">Schoone</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Naer u <rhyme label="b" type="m">wil</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Sal ick in een kluys gaen <rhyme label="a" type="f" corresp="#s6:l1">woonen</rhyme></l>
        <l id="s6:l4" met="2" type="+-">Eeuwigh <rhyme label="b" type="m" corresp="#s6:l2">stil</rhyme>;</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Daer gheen sterff'lijck mensch sal <rhyme label="a" type="f">weten</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Van myn <rhyme label="b" type="m">staet</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Daer so wil ick zijn <rhyme label="a" type="f" corresp="#s7:l1">vergeten</rhyme></l>
        <l id="s7:l4" met="2" type="+-">Voor myn <rhyme label="b" type="m" corresp="#s7:l2">quaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
