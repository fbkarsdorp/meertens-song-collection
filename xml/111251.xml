<song>
  <meta>
    <id>111251</id>
    <date>
      <year-start>1785</year-start>
      <year-end>1785</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">VIvat Capellen is 't woord, ha <rhyme label="a" type="m">ha</rhyme></l>
        <l id="s1:l2" met="6" type="+-">Hy is hersteld gelyk het behoord, ha <rhyme label="a" type="m" corresp="#s1:l1">ha</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hy treed de Staeten Kamer <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s1:l4" met="10" type="+-">En maekt met stemmen een begin, ha ha ha ha ha <rhyme label="a" type="m" corresp="#s1:l1">ha</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">De Gulde Vryheid op den Troon, ha <rhyme label="a" type="m">ha</rhyme></l>
        <l id="s2:l2" met="6" type="+-">Blinkt als de Dageraed zo schoon, ha <rhyme label="a" type="m" corresp="#s2:l1">ha</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Zo gaet het als het <rhyme label="b" type="m">Vrygemeen</rhyme>,</l>
        <l id="s2:l4" met="10" type="+-">Gedrukt raekt eenmael op de been, ha ha ha ha ha <rhyme label="a" type="m" corresp="#s2:l1">ha</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Vergeeft dan Bridsche Dwinglandy, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">Het volk van banden en Kluisters vry, ha, <rhyme label="a" type="m" corresp="#s3:l1">ha</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Belagt haer drygen en <rhyme label="b" type="m">gevroet</rhyme>,</l>
        <l id="s3:l4" met="10" type="+-">Met al haer aenhang en haer stoet, ha ha ha ha ha <rhyme label="a" type="m" corresp="#s3:l1">ha</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Triumph Cappellen is hersteld, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s4:l2" met="6" type="+-">Nu vreest geen Boer meer voor geweld, ha, <rhyme label="a" type="m" corresp="#s4:l1">ha</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Van Slaefsche Drosten diensten <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s4:l4" met="10" type="+-">Dankt hy de brave Burgary, ha ha ha ha ha <rhyme label="a" type="m" corresp="#s4:l1">ha</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Dank zy de steeden Magistraet, ha <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Zy waren steeds ons toeverlaet, ha <rhyme label="a" type="m" corresp="#s5:l1">ha</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hun roem klim op aen 't <rhyme label="b" type="m">Sterrenhof</rhyme>,</l>
        <l id="s5:l4" met="10" type="+-">Verdonkrend der Vadren Lof, ha ha ha ha ha <rhyme label="a" type="m" corresp="#s5:l1">ha</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="+-">Kom zingen en klinken en drinken wy, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s6:l2" met="6" type="+-">Van Zorg, en van bekommering vry, ha, <rhyme label="a" type="m" corresp="#s6:l1">ha</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Schenk jongen, schenk een bottel <rhyme label="b" type="m">vol</rhyme>,</l>
        <l id="s6:l4" met="10" type="+-">'t Moet met dees avond op de hol, ha ha ha ha ha <rhyme label="a" type="m" corresp="#s6:l1">ha</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="+-">Elk vatte steeds den Beker an, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s7:l2" met="6" type="+-">En roept al wat hy roepen kan, ha, <rhyme label="a" type="m" corresp="#s7:l1">ha</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Vivat Capellen vryheid <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s7:l4" met="10" type="+-">Veredelt steeds ons hard en zin, ha ha ha ha ha <rhyme label="a" type="m" corresp="#s7:l1">ha</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
