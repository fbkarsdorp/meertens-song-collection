    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Het sloegh daer eeven <rhyme label="a" type="m">drie</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Denckt dat de nacht al <rhyme label="a" type="m" corresp="#s1:l1">vlie</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En nu soos haest ten end' <rhyme label="b" type="f">is</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Men siet de dagh te <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die vol van alle <rhyme label="c" type="m" corresp="#s1:l4">soet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En vry van all' elend' <rhyme label="b" type="f" corresp="#s1:l3">is</rhyme>.</l>
      </lg>
    </lg>
