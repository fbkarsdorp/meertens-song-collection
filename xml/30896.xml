    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ic weet een vrouken <rhyme label="a" type="m">amorues</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die ic van herten <rhyme label="b" type="f">minne</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Haer wesen es so <rhyme label="a" type="m" corresp="#s1:l1">gratiues</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Si staet in mijnen <rhyme label="b" type="f" corresp="#s1:l2">sinne</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Gratiues es si te aller <rhyme label="c" type="m">tit</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Men vindt er niet <rhyme label="d" type="f">vele</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Want si hat einen lachenden <rhyme label="e" type="m">mont</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Twee borsten <rhyme label="e" type="m" corresp="#s1:l7">rondt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">End een snee wite <rhyme label="d" type="f" corresp="#s1:l6">kele</rhyme>.</l>
      </lg>
    </lg>
