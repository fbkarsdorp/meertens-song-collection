      <year-start>1828</year-start>
      <year-end>1828</year-end>
    </date>
    <location>
      <orginal-name>monickendam</orginal-name>
      <standard-name>Monnickendam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4570766</lon>
        <lat>5.0373214</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat ik eens was, dat was <rhyme label="a" type="f">ik</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En ben ik nu niet <rhyme label="b" type="m">meer</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">'k Was jongen bij de <rhyme label="c" type="f">schaafbank</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En nu? ik ben een <rhyme label="b" type="m" corresp="#s1:l2">Heer</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">De grootste der <rhyme label="d" type="f">sinjeuren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Belief ik met mijn <rhyme label="e" type="m">raad</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">'t Gemeen is me <rhyme label="f" type="f">onverschillig</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Schoon 't mij benijdt en <rhyme label="e" type="m" corresp="#s1:l6">haat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ja, 'k zal misschien eens <rhyme label="a" type="f">worden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het geen ik nog niet <rhyme label="b" type="m">ben</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Men acht mij zoo <rhyme label="c" type="f">geslepen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als of ik toovren <rhyme label="b" type="m" corresp="#s2:l2">ken</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">De grooten, wat een <rhyme label="d" type="f">wonder</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Zijn altijd op mijn <rhyme label="e" type="m">hand</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En ook de <rhyme label="f" type="f">burgerklasse</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Roept luid van mijn <rhyme label="e" type="m" corresp="#s2:l6">verstand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maar wat ik ben, dat ben <rhyme label="a" type="f">ik</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En morgen ligt niet <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dan loop ik om een <rhyme label="c" type="f">daghuur</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Of aan de schaafbank <rhyme label="b" type="m" corresp="#s3:l2">wÃªer</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Een keizer is, <rhyme label="d" type="f">voorzeker</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Een hoog gezeteld <rhyme label="e" type="m">man</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En toch, wij weten 't <rhyme label="f" type="f">allen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hoe ras hij rollen <rhyme label="e" type="m" corresp="#s3:l6">kan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dit wil ik niet <rhyme label="a" type="f">vergeten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">In mijn verheven <rhyme label="b" type="m">rang</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ook deze kan <rhyme label="c" type="f">verkeeren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Het dure kort of <rhyme label="b" type="m" corresp="#s4:l2">lang</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">En zoo mij trots <rhyme label="d" type="f">bekruipe</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Het moog' er mij naar <rhyme label="e" type="m">gaan</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">'k Verdien dan weer het <rhyme label="f" type="f">schootsvel</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En trek het werkpak <rhyme label="e" type="m" corresp="#s4:l6">aan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
