    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Fortuin helaas</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WEest hertelijck <rhyme label="a" type="m">ghegroet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hemelsche <rhyme label="b" type="m">Coningin</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want uwer deughden <rhyme label="a" type="m" corresp="#s1:l1">vloet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Bewillichde de <rhyme label="b" type="m" corresp="#s1:l2">sin</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van Godes Soon tot <rhyme label="b" type="m" corresp="#s1:l2">min</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Om ons te lossen <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Als vrienden, doen wy <rhyme label="b" type="m" corresp="#s1:l2">in</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dootdts banden lagen <rhyme label="c" type="m" corresp="#s1:l6">neer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghy zijt <rhyme label="a" type="m">Abigael</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Vrouw der Eng'len <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dus meughen wy u <rhyme label="a" type="m" corresp="#s2:l1">wel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Bedancken <rhyme label="b" type="m" corresp="#s2:l2">allegaer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Als Hester, die de <rhyme label="b" type="m" corresp="#s2:l2">(maer</rhyme>)</l>
        <l id="s2:l6" met="3" type="-+">Van Haman hebt <rhyme label="c" type="m">verrast</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Want hy quam selfs daer <rhyme label="b" type="m" corresp="#s2:l2">naer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Door u aen eenen <rhyme label="c" type="m" corresp="#s2:l6">bast</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De wortel Iesse <rhyme label="a" type="m">heeft</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Noyt beter Boom <rhyme label="b" type="m">voortbracht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als ghy zijt, want ghy <rhyme label="a" type="m" corresp="#s3:l1">geeft</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ons vrucht die al de <rhyme label="b" type="m" corresp="#s3:l2">cracht</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van Godts gramschap <rhyme label="b" type="m" corresp="#s3:l2">versacht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dus sy u lof, o <rhyme label="c" type="m">poort</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die onser sonden <rhyme label="b" type="m" corresp="#s3:l2">nacht</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Het licht eerst brachtet <rhyme label="c" type="m" corresp="#s3:l6">voor</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Verblijt u dan o <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Glorieus is u <rhyme label="b" type="m">gheval</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">want ghy hebt hier <rhyme label="a" type="m" corresp="#s4:l1">behaeght</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Godts Soone boven <rhyme label="b" type="m" corresp="#s4:l2">al</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hierin dit aerdtsche <rhyme label="b" type="m" corresp="#s4:l2">dal</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Was niemant u 's <rhyme label="c" type="m">ghelijck</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Noch is, noch wesen <rhyme label="b" type="m" corresp="#s4:l2">sal</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">*Oock in het Hemels <rhyme label="c" type="m" corresp="#s4:l6">rijck</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Vaert wel dan waerde <rhyme label="a" type="m">Vrou</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Heel uytter maten <rhyme label="b" type="m">schoon</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Bidt voor ons in ons <rhyme label="a" type="m" corresp="#s5:l1">rou</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Aen Christum uwen <rhyme label="b" type="m" corresp="#s5:l2">Soon</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Laet toe dat ick u <rhyme label="b" type="m" corresp="#s5:l2">kroon</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ootmoedich <rhyme label="c" type="m">altemet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Begroet in 's Hemels <rhyme label="b" type="m" corresp="#s5:l2">troon</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Door een verneert <rhyme label="c" type="m" corresp="#s5:l6">ghebedt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
