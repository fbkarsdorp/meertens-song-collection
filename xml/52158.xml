<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>52158</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1819</year-end>
    </date>
    <tags>
      <tag translation="soldier's song">soldatenlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="work">werk</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wy zyn <rhyme label="a" type="f">Vaderlanders</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wy dienen voor het <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wy leggen in de <rhyme label="c" type="f">Brakken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Aan een <rhyme label="b" type="m" corresp="#s1:l2">wtterkant</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Wy hebben daar minn <rhyme label="d" type="f">sie</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Voor VyfÃ«nvyftig <rhyme label="e" type="m">man</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wy dienen tot <rhyme label="d" type="f" corresp="#s1:l5">avontasie</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Al voor ons <rhyme label="e" type="m" corresp="#s1:l6">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy moeten steeds <rhyme label="a" type="f">leeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoe en wat men moeten <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Alle dagen <rhyme label="c" type="f">exerseeren</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Op de Straat of in het <rhyme label="b" type="m" corresp="#s2:l2">Groen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wy moe en <rhyme label="d" type="f">exerseeren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Alle dagen met het <rhyme label="e" type="m">Kanon</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Zoodra als wy <rhyme label="d" type="f" corresp="#s2:l5">mankeeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dan moeten wy in de <rhyme label="e" type="m" corresp="#s2:l6">Preson</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy hebben nog <rhyme label="a" type="f">couragie</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Daar by ook goede <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maar wy dienen tot <rhyme label="c" type="f">avontagie</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Voor ons lieve Vaderland <rhyme label="b" type="m" corresp="#s3:l2">zoet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Men krygt uit de <rhyme label="d" type="f">menagie</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al het geen 'er toe <rhyme label="e" type="m">behoord</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Leggen in de <rhyme label="d" type="f" corresp="#s3:l5">Brakken</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En leiden thans geen <rhyme label="e" type="m" corresp="#s3:l6">nood</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy zynen <rhyme label="a" type="f">Kannonnieren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En krygen ook ons <rhyme label="b" type="m">Geld</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wy willen ook wel <rhyme label="c" type="f">zweeren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En stryden als een <rhyme label="b" type="m" corresp="#s4:l2">held</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wy krygen ook <rhyme label="d" type="f">monteering</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Daar zyn wy op <rhyme label="e" type="m">gesteld</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En ziet wy moeten <rhyme label="d" type="f" corresp="#s4:l5">leeren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Te trekken in het <rhyme label="e" type="m" corresp="#s4:l6">Veld</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En daarom lieve <rhyme label="a" type="f">Ouders</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laat daarom geenen <rhyme label="b" type="m">traan</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En ook de lieve <rhyme label="c" type="f">Meisjes</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Want ziet wy moeten <rhyme label="b" type="m" corresp="#s5:l2">gaan</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wy vreezen geen <rhyme label="d" type="f">Tirannen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wij leeven frank en <rhyme label="e" type="m">vrij</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En door geen vrees <rhyme label="d" type="f" corresp="#s5:l5">verbannen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Zijn wij gerust en <rhyme label="e" type="m" corresp="#s5:l6">blij</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Kom laat ons nu <rhyme label="a" type="f">besluiten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Het is de wil van <rhyme label="b" type="m">God</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Men kan 'er niets tegen <rhyme label="c" type="f">uiten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Want ziet het is ons <rhyme label="b" type="m" corresp="#s6:l2">lot</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Daarom mijn lieve <rhyme label="d" type="f">Vrinden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Wij zullen strijden <rhyme label="e" type="m">gaan</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En zullen ons gerust <rhyme label="d" type="f" corresp="#s6:l5">verbinden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Om onder 's Keizer's magt te <rhyme label="e" type="m" corresp="#s6:l6">staan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Adieu nu al te <rhyme label="a" type="f">zaamen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wij wenschen uw veel <rhyme label="b" type="m">plaisier</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En als wij <rhyme label="c" type="f">wederkomen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Met een Tak van <rhyme label="b" type="m" corresp="#s7:l2">Lauwerier</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dan zullen wij kunnen <rhyme label="d" type="f">zingen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Met een verheugd <rhyme label="e" type="m">gemoed</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Van Vaders, Moeders, <rhyme label="d" type="f" corresp="#s7:l5">Vrinden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En scheppen goede <rhyme label="e" type="m" corresp="#s7:l6">moed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu Vrienden wy gaan <rhyme label="a" type="f">verder</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En stryden voor ons <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Onder Keizerlyke <rhyme label="c" type="f">Banieren</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Dan blyft ons Land in <rhyme label="b" type="m" corresp="#s8:l2">stand</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">God wil ons Zegen <rhyme label="d" type="f">geeven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dan hebben wy geen <rhyme label="e" type="m">nood</rhyme></l>
        <l id="s8:l7" met="3" type="-+">En eindigen vrolyk ons <rhyme label="d" type="f" corresp="#s8:l5">levven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">En vegten Vrij of <rhyme label="e" type="m" corresp="#s8:l6">Dood</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
