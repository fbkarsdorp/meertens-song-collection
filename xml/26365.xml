    <melody>Lof zij u God hoogste regent</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O Christus lief minnare // <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die my schinckt u dierbare // <rhyme label="a" type="m" corresp="#s1:l1">bloet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Nae u verlanght my t' allen <rhyme label="b" type="f">uren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Soo den Hert sijnd' in vare // <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Nae de Fonteyne clare // <rhyme label="a" type="m" corresp="#s1:l1">vloet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">In dorstich pijnlick <rhyme label="b" type="f" corresp="#s1:l3">besueren</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Ick leve in sware <rhyme label="b" type="f" corresp="#s1:l3">dolueren</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Hoe langhe, Lief, sal dit <rhyme label="b" type="f" corresp="#s1:l3">ghedueren</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Wanneer sal ick uyt rouwen, <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Wanneer sal ick aenschouwen, <rhyme label="c" type="m" corresp="#s1:l9">dijn</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Aenschijn, schoon boven <rhyme label="b" type="f" corresp="#s1:l3">naturen</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">O lief, in s werelts wijcke // <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Van my in gheen versijcke // <rhyme label="a" type="m" corresp="#s2:l1">vliet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer helpt my, dat mijn teere <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Niet en buuyghen ghelijcke // <rhyme label="a" type="m" corresp="#s2:l1">riet</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Maer van u soete Rijcke, <rhyme label="a" type="m" corresp="#s2:l1">siet</rhyme></l>
        <l id="s2:l6" met="4" type="-+">My hier in dit dal <rhyme label="b" type="f" corresp="#s2:l3">beneden</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Want ick vinde my <rhyme label="b" type="f" corresp="#s2:l3">bestreden</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Van den Prince der <rhyme label="b" type="f" corresp="#s2:l3">duysterheden</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">De werelt in t' verstranghen // <rhyme label="c" type="m">quaet</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Alle dat nydich Slanghen // <rhyme label="c" type="m" corresp="#s2:l9">zaet</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">En laet my niet met <rhyme label="b" type="f" corresp="#s2:l3">vrede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Jonstich voor my gheneghen // <rhyme label="a" type="m">hecht</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Leydet my in uwe weghen // <rhyme label="a" type="m" corresp="#s3:l1">recht</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">door uwen Gheest, o Lief <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Want siet ick ben bedeghen // <rhyme label="a" type="m" corresp="#s3:l1">slecht</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Een teecken daer elck teghen // <rhyme label="a" type="m" corresp="#s3:l1">seght</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Voor raserye acht men mijn <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En ick nochtans <rhyme label="b" type="f" corresp="#s3:l3">becleven</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Wt uwe zyde sonder <rhyme label="b" type="f" corresp="#s3:l3">sneven</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">U lief, u Bruydt, u Kercke // <rhyme label="c" type="m">reen</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Ick doe door u, O stercke // <rhyme label="c" type="m" corresp="#s3:l9">steen</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Die Helsche poorten <rhyme label="b" type="f" corresp="#s3:l3">beven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">O Prince der Princieren <rhyme label="a" type="m">//jent</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ghy die herten en nieren // <rhyme label="a" type="m" corresp="#s4:l1">kent</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Aenhoort mijn bitter <rhyme label="b" type="f">claghen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Uwen Gheest goedertieren // <rhyme label="a" type="m" corresp="#s4:l1">sendt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dat hy door zijn regieren // <rhyme label="a" type="m" corresp="#s4:l1">went</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Mijn ledekens nae u <rhyme label="b" type="f" corresp="#s4:l3">behaghen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Want t' sijn nu quade <rhyme label="b" type="f" corresp="#s4:l3">daghen</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Hierom, o Lief moet ick u <rhyme label="b" type="f" corresp="#s4:l3">vraghen</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Sal ick van al dit lyden // <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Met u niet haest verblyden // <rhyme label="c" type="m" corresp="#s4:l9">my</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">En s Levens Croone <rhyme label="b" type="f" corresp="#s4:l3">draghen</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
