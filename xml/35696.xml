    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Het vuur brandt zeer</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">ADams <rhyme label="a" type="m">zondt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Heeft 't menschelijck <rhyme label="b" type="m">geslacht</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Dat <rhyme label="a" type="m" corresp="#s1:l1">toe-stondt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">'t Quaet, tot den doodt <rhyme label="b" type="m" corresp="#s1:l2">gebracht</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">In dese noodt, in dese <rhyme label="c" type="m">rouw</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Belooft wordt een Verlosser <rhyme label="c" type="m" corresp="#s1:l5">trouw</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s1:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">
          <rhyme label="a" type="m">Gabriël</rhyme>
        </l>
        <l id="s2:l2" met="3" type="-+">Daelt neer uyt s' Hemels <rhyme label="b" type="m">hof</rhyme></l>
        <l id="s2:l3" met="2" type="+-">Goods <rhyme label="a" type="m" corresp="#s2:l1">bevel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Verhaelt: ende den <rhyme label="b" type="m" corresp="#s2:l2">lof</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Van Maria den zuyv're <rhyme label="c" type="m">maeght</rhyme>;</l>
        <l id="s2:l6" met="4" type="-+">Den lof die haer en ons <rhyme label="c" type="m" corresp="#s2:l5">behaeght</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Spreeckt met <rhyme label="d" type="m">ootmoedt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s2:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Vol zijt <rhyme label="a" type="m">ghy</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gracy; den waertste <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">En weet <rhyme label="a" type="m" corresp="#s3:l1">vry</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat geen Vrouw heeft <rhyme label="b" type="m" corresp="#s3:l2">gehadt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Gracy, als ghy, o Maget! <rhyme label="c" type="m">Meer</rhyme>.</l>
        <l id="s3:l6" met="4" type="-+">En daer by, Met u is den <rhyme label="c" type="m" corresp="#s3:l5">Heer</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s3:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Ghy <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Onder den Vrouwen <rhyme label="b" type="m">al</rhyme>;</l>
        <l id="s4:l3" met="2" type="+-">Anders <rhyme label="a" type="m" corresp="#s4:l1">geen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den Moeder wezen <rhyme label="b" type="m" corresp="#s4:l2">zal</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Van den verlosser van den <rhyme label="c" type="m">mensch</rhyme>.</l>
        <l id="s4:l6" met="4" type="-+">Zijt gebenedijt na u <rhyme label="c" type="m" corresp="#s4:l5">wensch</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s4:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Dese <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met groot <rhyme label="b" type="m">ned'righeydt</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">d' Engel <rhyme label="a" type="m" corresp="#s5:l1">vraeght</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Geeft my hier van <rhyme label="b" type="m" corresp="#s5:l2">bescheyt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Seght my hoe dit geschiede <rhyme label="c" type="m">kan</rhyme>?</l>
        <l id="s5:l6" met="4" type="-+">Ick ben een Maeght, beken geen <rhyme label="c" type="m" corresp="#s5:l5">Man</rhyme>.</l>
        <l id="s5:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s5:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Met <rhyme label="a" type="m">vermaeck</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Uyt Gabriël <rhyme label="b" type="m">verstaet</rhyme>;</l>
        <l id="s6:l3" met="2" type="+-">Dat geen <rhyme label="a" type="m" corresp="#s6:l1">saeck</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Godts macht te boven <rhyme label="b" type="m" corresp="#s6:l2">gaet</rhyme></l>
        <l id="s6:l5" met="4" type="-+">En hoort dat haer d'Heyligen <rhyme label="c" type="m">Geest</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Om lom'ren zal, weest niet <rhyme label="c" type="m" corresp="#s6:l5">bevreest</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoedt</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s6:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">Maget <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Heyligh zal zijn u <rhyme label="b" type="m">kint</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Uwen <rhyme label="a" type="m" corresp="#s7:l1">Soon</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Den mensch verdoemt soo <rhyme label="b" type="m" corresp="#s7:l2">mint</rhyme>;</l>
        <l id="s7:l5" met="4" type="-+">Die sal verlossen door sijn <rhyme label="c" type="m">bloedt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Voor die zal koopen 's Hemels <rhyme label="c" type="m" corresp="#s7:l5">goet</rhyme>.</l>
        <l id="s7:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Maria, weest <rhyme label="d" type="m" corresp="#s7:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">Sy spreeckt; <rhyme label="a" type="m">ziet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Den Dienstmaeght van den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s8:l3" met="2" type="+-">'k Behouw <rhyme label="a" type="m" corresp="#s8:l1">niet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Of 't is tot Godes <rhyme label="b" type="m" corresp="#s8:l2">eer</rhyme>;</l>
        <l id="s8:l5" met="4" type="-+">Mijn tijdt, mijn goet, mijn ziel, mijn <rhyme label="c" type="m">lijf</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Is tot Goods dienst ende <rhyme label="c" type="m" corresp="#s8:l5">gerijf</rhyme>.</l>
        <l id="s8:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s8:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="+-">Met <rhyme label="a" type="m">geneught</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Ontfanght in desen <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s9:l3" met="2" type="+-">En met <rhyme label="a" type="m" corresp="#s9:l1">vreught</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Haer Soon, tot ons <rhyme label="b" type="m" corresp="#s9:l2">profijt</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Voor ons verlaet zijn <rhyme label="c" type="m">Hemelrijck</rhyme>;</l>
        <l id="s9:l6" met="4" type="-+">(Behalven 't quaedt is ons <rhyme label="c" type="m" corresp="#s9:l5">gelijck</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Singht: Iesu weest <rhyme label="d" type="m" corresp="#s9:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="+-">Vrienden <rhyme label="a" type="m">Leert</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Van Godes Moeder <rhyme label="b" type="m">net</rhyme>:</l>
        <l id="s10:l3" met="2" type="+-">Strijdt en <rhyme label="a" type="m" corresp="#s10:l1">eert</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Voor reyn'heyt, en Goods <rhyme label="b" type="m" corresp="#s10:l2">Wet</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Tegen Goods wil weest noyt <rhyme label="c" type="m">rebel</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Wat Godt eyscht, doet, seght 't is mijn <rhyme label="c" type="m" corresp="#s10:l5">wel</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Met yver <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Leest dickmaels 't weest <rhyme label="d" type="m" corresp="#s10:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="+-">Josephs <rhyme label="a" type="m">Bruyt</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Moeder Goods, Maeght en <rhyme label="b" type="m">Vrouw</rhyme></l>
        <l id="s11:l3" met="2" type="+-">Van ons <rhyme label="a" type="m" corresp="#s11:l1">sluyt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Al Sathans macht, en <rhyme label="b" type="m" corresp="#s11:l2">rouw</rhyme>.</l>
        <l id="s11:l5" met="4" type="-+">Weest onse troost, en <rhyme label="c" type="m">soetigheydt</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Nu noch in de doodt van ons <rhyme label="c" type="m" corresp="#s11:l5">scheyt</rhyme>.</l>
        <l id="s11:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Maria, weest <rhyme label="d" type="m" corresp="#s11:l7">gegroet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="+-">Stelt ten <rhyme label="a" type="m">toon</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">U Moederlijcke <rhyme label="b" type="m">borst</rhyme></l>
        <l id="s12:l3" met="2" type="+-">Aen u <rhyme label="a" type="m" corresp="#s12:l1">Soon</rhyme></l>
        <l id="s12:l4" met="3" type="-+">In onse zielen <rhyme label="b" type="m" corresp="#s12:l2">dorst</rhyme>.</l>
        <l id="s12:l5" met="4" type="-+">Staet, en blijft by ons <rhyme label="c" type="m">allegaer</rhyme>;</l>
        <l id="s12:l6" met="4" type="-+">Krijght voor ons u Soons aenschijn <rhyme label="c" type="m" corresp="#s12:l5">klaer</rhyme>,</l>
        <l id="s12:l7" met="2" type="-+">Met groot <rhyme label="d" type="m">ootmoet</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Maria weest <rhyme label="d" type="m" corresp="#s12:l7">gegroet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
