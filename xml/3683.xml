    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 080 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHY Hęerder Israels wilt <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die de kudde Iosephs <rhyme label="a" type="f" corresp="#s1:l1">verkoren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wtgheuoert hebt als schaepen <rhyme label="b" type="m">goet</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Toont ons v' lieflick aenschyn <rhyme label="b" type="m" corresp="#s1:l3">soet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ghy die hoogh' sijt in uwen <rhyme label="c" type="m">throon</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Bouen den Cherubinen <rhyme label="c" type="m" corresp="#s1:l5">schoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laett uwe kracht Heere <rhyme label="a" type="f">verschynen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ephraim tot nut end den <rhyme label="a" type="f" corresp="#s2:l1">sijnen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Oock Manasse end <rhyme label="b" type="m">Benjamin</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Keert doch tot ons Heer uwen <rhyme label="b" type="m" corresp="#s2:l3">sin</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Troost ons met v' lieflick <rhyme label="c" type="m">aenschyn</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Op dat wy van schaeden vry <rhyme label="c" type="m" corresp="#s2:l5">sijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hoe men ons handelt kondt ghy <rhyme label="a" type="f">mercken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dies wilt ons doer v' goetheit <rhyme label="a" type="f" corresp="#s3:l1">stercken</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Slaet ouer ons t'ghesicht' <rhyme label="b" type="m">eenpaer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Vwer vriendtlicker ooghen <rhyme label="b" type="m" corresp="#s3:l3">klaer</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">So sullen wy van t'kruys niet <rhyme label="c" type="m">kleyn</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Vry sijn doer v' ghesicht <rhyme label="c" type="m" corresp="#s3:l5">alleyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoe langh' sal O Heer der <rhyme label="a" type="f">hęerkrachten</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Vwen toorn noch branden met <rhyme label="a" type="f" corresp="#s4:l1">machten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Teghen uwes volcks ghebedt <rhyme label="b" type="m">reyn</rhyme>?</l>
        <l id="s4:l4" met="4" type="-+">Ghy hebt ons met traenen <rhyme label="b" type="m" corresp="#s4:l3">ghemeyn</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ghespyst, oock was bitter <rhyme label="c" type="m">gheklagh</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Onse dranck Heer den gantschen <rhyme label="c" type="m" corresp="#s4:l5">dagh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy hebt ons ghemaeckt den <rhyme label="a" type="f">naebueren</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Tot twist end ghekyf t'allen <rhyme label="a" type="f" corresp="#s5:l1">vren</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Ons vianden spotten ons <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Verlost ons wt schaeden O <rhyme label="b" type="m" corresp="#s5:l3">Heer</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Laet ons schauwen v' aenschyn <rhyme label="c" type="m">bly</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">So werden wy seker end <rhyme label="c" type="m" corresp="#s5:l5">vry</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vwen wijngaert wt AEgypten <rhyme label="a" type="f">landt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Braght ghy Heer, end ghy hebt hem <rhyme label="a" type="f" corresp="#s6:l1">gheplant</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Daer verdilcht sijn die volcken <rhyme label="b" type="m">wreet</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ghy maecktet hem een plaetse <rhyme label="b" type="m" corresp="#s6:l3">breet</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Daer hy ghewortelt vastlick <rhyme label="c" type="m">kleeft</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">So dat hy t'landt vervullet <rhyme label="c" type="m" corresp="#s6:l5">heeft</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De berghen sijn seer schoon <rhyme label="a" type="f">bedecket</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Met sijn schaduwe die wyt <rhyme label="a" type="f" corresp="#s7:l1">strecket</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Sijn rancken hoogh' klimmen end <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Ghelyck de Cederboomen <rhyme label="b" type="m" corresp="#s7:l3">staen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Sijn tacken sijn ghewasen <rhyme label="c" type="m">bloot</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Van t'water tot aen de Zee <rhyme label="c" type="m" corresp="#s7:l5">groot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Waerom hebt ghy den tuyn doen <rhyme label="a" type="f">breken</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">End sijt den rooueren <rhyme label="a" type="f" corresp="#s8:l1">gheweken</rhyme>?</l>
        <l id="s8:l3" met="4" type="-+">Hoe komt dat wilde swynen <rhyme label="b" type="m">fel</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Daerin gheuallen sijn so <rhyme label="b" type="m" corresp="#s8:l3">snel</rhyme>?</l>
        <l id="s8:l5" met="4" type="-+">Waerom heeft t'wildt tot deser <rhyme label="c" type="m">stondt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Hem so verdoruen in den <rhyme label="c" type="m" corresp="#s8:l5">grondt</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Godt die een Heer sijt der <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Siet ons aen, wilt v tot ons <rhyme label="a" type="f" corresp="#s9:l1">keeren</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Besoeckt end siet den wynstock <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Dien v' handt gheplant heeft <rhyme label="b" type="m" corresp="#s9:l3">alleyn</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Wilt dien wederbauwen voort <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Dat ghy daer van prys moeght <rhyme label="c" type="m" corresp="#s9:l5">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hy sal schier tot asschen <rhyme label="a" type="f">verbranden</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">End is nu gantsch ghemaeckt te <rhyme label="a" type="f" corresp="#s10:l1">schanden</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Hy vergaet doer v' gramschap <rhyme label="b" type="m">haest</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Streckt v' handt wt, maeckt <rhyme label="b" type="m" corresp="#s10:l3">onuerbaest</rhyme></l>
        <l id="s10:l5" met="4" type="-+">T'volck Heer dat op v alleyn <rhyme label="c" type="m">bauwt</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">End van herten vastlick <rhyme label="c" type="m" corresp="#s10:l5">vertrauwt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wy sullen ons niet meer <rhyme label="a" type="f">begheuen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Tot afwyckinghe, laet ons <rhyme label="a" type="f" corresp="#s11:l1">leuen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Wy sullen steedts singhen v' <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Troost ende helpt ons langhs so <rhyme label="b" type="m" corresp="#s11:l3">meer</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">V' aensicht gheue sijnen <rhyme label="c" type="m">schyn</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">So sullen wy gheholpen <rhyme label="c" type="m" corresp="#s11:l5">sijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
