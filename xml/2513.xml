      <year-start>1622</year-start>
      <year-end>1622</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="moralistisch lied" translation="moralistic song">moralistisch lied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Die sich veynst, waent of <rhyme label="a" type="m">vermeet</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Dat hy <rhyme label="a" type="m" corresp="#s1:l1">weet</rhyme></l>
        <l id="s1:l3" met="4" type="+-">'t Geen hy niet en weet te <rhyme label="b" type="f">weten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Werd met recht van elck <rhyme label="c" type="m">bespot</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Ja een <rhyme label="c" type="m" corresp="#s1:l4">sot</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Of goed-dunckende <rhyme label="b" type="f" corresp="#s1:l3">geheeten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die alleen zijn oogen <rhyme label="a" type="m">slaet</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Op 't <rhyme label="a" type="m" corresp="#s2:l1">gewaet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Sonder eens na deughd te <rhyme label="b" type="f">kijcken</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Machmen die door dit <rhyme label="c" type="m">gebreck</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">By een <rhyme label="c" type="m" corresp="#s2:l4">Geck</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Om zijn geckheyd niet <rhyme label="b" type="f" corresp="#s2:l3">gelijcken</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die boven alle dingen <rhyme label="a" type="m">acht</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Het <rhyme label="a" type="m" corresp="#s3:l1">geslacht</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Sonder de persoon <rhyme label="b" type="f">t'aenschouwen</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Machmen die oock niet wel <rhyme label="c" type="m">voor</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Dwaes of <rhyme label="c" type="m" corresp="#s3:l4">Door</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Om zijn grove sotheyd <rhyme label="b" type="f" corresp="#s3:l3">houwen</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die by een leelijck mensch <rhyme label="a" type="m">verselt</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Om het <rhyme label="a" type="m" corresp="#s4:l1">geld</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Met een weer-sin gaet <rhyme label="b" type="f">vernachten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Machmen die met alle <rhyme label="c" type="m">recht</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Niet voor <rhyme label="c" type="m" corresp="#s4:l4">slecht</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Of voor eenen gouwt-nar <rhyme label="b" type="f" corresp="#s4:l3">achten</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Die al waent, en houwt voor <rhyme label="a" type="m">goedt</rhyme></l>
        <l id="s5:l2" met="2" type="+-">'t Geen hy <rhyme label="a" type="m" corresp="#s5:l1">doet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En eens anders acht voor <rhyme label="b" type="f">leuren</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Datmen die in sulck <rhyme label="c" type="m">geval</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Noemde <rhyme label="c" type="m" corresp="#s5:l4">mal</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Souwmen daer oock an <rhyme label="b" type="f" corresp="#s5:l3">verbeuren</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die staegh op een ander <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Scheld en <rhyme label="a" type="m" corresp="#s6:l1">schiet</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Met veel spitse woorden <rhyme label="b" type="f">vinnigh</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En die sich selfs niet en <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s6:l5" met="2" type="+-">Recht sien <rhyme label="c" type="m" corresp="#s6:l4">an</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Noemt men die oock niet <rhyme label="b" type="f" corresp="#s6:l3">uytsinnigh</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die men klaer voor oogen <rhyme label="a" type="m">leyd</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Zijn <rhyme label="a" type="m" corresp="#s7:l1">dwaesheyd</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Sonder omslagh of <rhyme label="b" type="f">bewimpel</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Die zijn trouwen vrunden <rhyme label="c" type="m">raed</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Grootsch <rhyme label="c" type="m" corresp="#s7:l4">versmaed</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Die acht ick noch nesck en <rhyme label="b" type="f" corresp="#s7:l3">simpel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ghy waert so quaed doen ick <rhyme label="a" type="m">sey</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Dat de <rhyme label="a" type="m" corresp="#s8:l1">key</rhyme></l>
        <l id="s8:l3" met="4" type="+-">U achter 't oor leyd en <rhyme label="b" type="f">lottert</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">'t Is mijn schult, 't is waer, ick <rhyme label="c" type="m">kent</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Want ghy <rhyme label="c" type="m" corresp="#s8:l4">bent</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Maer een onbeschaemde <rhyme label="b" type="f" corresp="#s8:l3">spottert</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Spiegeld in dees spiegel <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Niet u <rhyme label="a" type="m" corresp="#s9:l1">vel</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Om dat cierlijck op te <rhyme label="b" type="f">toyen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Vind ghy eenigh vuyl of <rhyme label="c" type="m">swart</rhyme></l>
        <l id="s9:l5" met="2" type="+-">In u <rhyme label="c" type="m" corresp="#s9:l4">hart</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Soeckt dat wijslijck uyt te <rhyme label="b" type="f" corresp="#s9:l3">royen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Als ick (Lief) in mijn selfs <rhyme label="a" type="m">ga</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">En sie <rhyme label="a" type="m" corresp="#s10:l1">na</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Al mijn eygen liefdsche <rhyme label="b" type="f">trecken</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">So dunckt my dat ick in <rhyme label="c" type="m">mijn</rhyme></l>
        <l id="s10:l5" met="2" type="+-">Alles <rhyme label="c" type="m" corresp="#s10:l4">vijn</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Dat ick in and're wil <rhyme label="b" type="f" corresp="#s10:l3">begecken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
