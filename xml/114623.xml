  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O Norberte, <rhyme label="a" type="f">uytgheschenen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Hebt ghy, als een <rhyme label="b" type="m">Morghe-sterr'</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Door u licht, verbreyt soo <rhyme label="b" type="m" corresp="#s1:l2">verr'</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Is de duysternis <rhyme label="a" type="f" corresp="#s1:l1">verdwenen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Door u leven menich <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Tot de deught onsteken <rhyme label="c" type="m" corresp="#s1:l5">wert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Doen de werelt te <rhyme label="a" type="f">verleyen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">U begost, heeft Godt by <rhyme label="b" type="m">tijdt</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van haer banden u <rhyme label="b" type="m" corresp="#s2:l2">bevrijt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En uyt haer terstont doen <rhyme label="a" type="f" corresp="#s2:l1">scheyen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Door 'nen donderslachs <rhyme label="c" type="m">ghewelt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Met u peert ter aerd' <rhyme label="c" type="m" corresp="#s2:l5">ghevelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doen hebd' u tot 't strenghste <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s3:l2" met="4" type="+-">(Weldich in't hof <rhyme label="b" type="m">opghevoedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Eel en rijck van 'swerelts <rhyme label="b" type="m" corresp="#s3:l2">goedt</rhyme>)</l>
        <l id="s3:l4" met="4" type="+-">Door Godts liefde u <rhyme label="a" type="f" corresp="#s3:l1">begheven</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Barvoets met een <rhyme label="c" type="m">schape-kleet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Van de zielen iever <rhyme label="c" type="m" corresp="#s3:l5">heet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dus ginghdy allom doen <rhyme label="a" type="f">preken</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Met 'nen Apostoolsen <rhyme label="b" type="m">gheest</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die met 't leven preckte <rhyme label="b" type="m" corresp="#s4:l2">meest</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat gheen tongh kan uyt <rhyme label="a" type="f" corresp="#s4:l1">ghespeken</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En deet Godt den meesten <rhyme label="c" type="m">dienst</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Met een groote ziele <rhyme label="c" type="m" corresp="#s4:l5">winst</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Waer toe oock u Witte <rhyme label="a" type="f">Orden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Van den Heer daer toe <rhyme label="b" type="m">bereyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Inghestelt in <rhyme label="b" type="m" corresp="#s5:l2">Premonstreyt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ievrich is van u <rhyme label="a" type="f" corresp="#s5:l1">gheworden</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Witter die door <rhyme label="c" type="m">suyverheyt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Heylich was door u <rhyme label="c" type="m" corresp="#s5:l5">beleyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Augustinus self <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">U den reghel gheven <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Waer uyt ghy <rhyme label="b" type="m" corresp="#s6:l2">ghetuyghenis</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Van Godts wil hebt oock <rhyme label="a" type="f" corresp="#s6:l1">ghenomen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Oock gehesterckt heeft u den <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Aen het Cruys die blonck daer <rhyme label="c" type="m" corresp="#s6:l5">seer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die met seven <rhyme label="a" type="f">Sonne-stralen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Was verciert, en tot hem <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Quam een groot ghetal te <rhyme label="b" type="m" corresp="#s7:l2">gaer</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Uyt alle de werelts <rhyme label="a" type="f" corresp="#s7:l1">palen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Waer door u wert als <rhyme label="c" type="m">voorseyt</rhyme></l>
        <l id="s7:l6" met="4" type="+-">d' Ordens <rhyme label="c" type="m" corresp="#s7:l5">menichvuldicheyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Korten tijdt daer naer <rhyme label="a" type="f">verdreven</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Uyt heel Antwerpen <rhyme label="b" type="m">hebdy</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Tanchelinus <rhyme label="b" type="m" corresp="#s8:l2">kettery</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">'t Heylich Sacrament <rhyme label="a" type="f" corresp="#s8:l1">verheven</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">En 't ghebruyck werom daer <rhyme label="c" type="m">van</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Oock ghewent aen alle <rhyme label="c" type="m" corresp="#s8:l5">man</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Eyndelijck gheweest <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Zijt ghy tot d'Artsbischops <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Waer in uytghemunt noch <rhyme label="b" type="m" corresp="#s9:l2">meer</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Heeft in deught u heylich <rhyme label="a" type="f" corresp="#s9:l1">leven</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Waer den Heer in had <rhyme label="c" type="m">ghenucht</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">En ghenoot daer van de <rhyme label="c" type="m" corresp="#s9:l5">vrucht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Waer ghy oock den loon daer <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Nu gheniet in alle <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Maer doet volghen ons u <rhyme label="b" type="m" corresp="#s10:l2">deught</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Op dat wy met u eens <rhyme label="a" type="f" corresp="#s10:l1">loven</rhyme></l>
        <l id="s10:l5" met="4" type="+-">t' Allen eeuwen Godt den <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Die u deught daer loont soo <rhyme label="c" type="m" corresp="#s10:l5">seer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
