    <melody>Immense coeli conditor</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Onende scepper van s'hemels <rhyme label="a" type="m">huijs</rhyme></l>
        <l id="s1:l2" met="4" type="-+">op dat tgemenck niet sij <rhyme label="a" type="m" corresp="#s1:l1">confuijs</rhyme></l>
        <l id="s1:l3" met="4" type="-+">hebt ghij gewilt t'waters <rhyme label="b" type="m">termijn</rhyme></l>
        <l id="s1:l4" met="4" type="-+">den sconen hemel soude <rhyme label="b" type="m" corresp="#s1:l3">sijn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maeckt plaets tot shemels <rhyme label="a" type="m">creatuer</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ende in aert des vloets <rhyme label="a" type="m" corresp="#s2:l1">natuer</rhyme></l>
        <l id="s2:l3" met="4" type="-+">om als het water bluscht het <rhyme label="b" type="m">vier</rhyme></l>
        <l id="s2:l4" met="4" type="-+">het aertrijck niet comt in <rhyme label="b" type="m" corresp="#s2:l3">dangier</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als nu soo stort heer <rhyme label="a" type="m">goedertier</rhyme></l>
        <l id="s3:l2" met="4" type="-+">v ewich grati in ons <rhyme label="a" type="m" corresp="#s3:l1">hier</rhyme></l>
        <l id="s3:l3" met="4" type="-+">op dat doer val van nieu <rhyme label="b" type="m">belet</rhyme></l>
        <l id="s3:l4" met="4" type="-+">die oude scult ons niet <rhyme label="b" type="m" corresp="#s3:l3">verplet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laet tgeloeff stout, vinden het <rhyme label="a" type="m">licht</rhyme></l>
        <l id="s4:l2" met="4" type="-+">en doer lichts stralen <rhyme label="a" type="m" corresp="#s4:l1">geuesticht</rhyme></l>
        <l id="s4:l3" met="4" type="-+">om te veriagen <rhyme label="b" type="m">ijdelheijt</rhyme></l>
        <l id="s4:l4" met="4" type="-+">oock niet verplet wort deur <rhyme label="b" type="m" corresp="#s4:l3">valscheijt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
