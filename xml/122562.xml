    <melody>Zwart wil ik wel wezen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Zwart wil ik wel <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Maar als de duivel <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Och Betje wil niet <rhyme label="a" type="f" corresp="#s1:l1">vreezen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wanneer gij mijn <rhyme label="b" type="m" corresp="#s1:l2">beziet</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Mijn kleur belet geen <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">ô Schoone <rhyme label="c" type="m" corresp="#s1:l5">Koningin</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wil mijn u gunst <rhyme label="d" type="f">verleenen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ivoor met <rhyme label="d" type="f" corresp="#s1:l7">Elpebeenen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Maak diamanten <rhyme label="d" type="f" corresp="#s1:l7">steenen</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Zoo schoon men denken <rhyme label="e" type="m">kan</rhyme>:</l>
        <l id="s1:l11" met="4" type="+-">ô Ja! ô ja! zonder <rhyme label="f" type="m">dat</rhyme>:</l>
        <l id="s1:l12" met="6" type="-+">Zonder dat wil ik geen <rhyme label="e" type="m" corresp="#s1:l10">man</rhyme>.</l>
        <l id="s1:l13" met="4" type="-+">Zonder dat wil ik geen <rhyme label="e" type="m" corresp="#s1:l10">man</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Is waar de kleur <rhyme label="a" type="f">mismaaken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Maar liefde, zijt me is <rhyme label="b" type="m">blind</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Men kân tot heil <rhyme label="a" type="f" corresp="#s2:l1">geraaken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als men opregt <rhyme label="b" type="m" corresp="#s2:l2">bemind</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Stelt het uit u <rhyme label="c" type="m">zin</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">ô Schoone <rhyme label="c" type="m" corresp="#s2:l5">Koningin</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gij weet het van te <rhyme label="d" type="f">vooren</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Dat trouw uit min <rhyme label="d" type="f" corresp="#s2:l7">gebooren</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Bestendig uw <rhyme label="d" type="f" corresp="#s2:l7">bekooren</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Zoo schoon men denken <rhyme label="e" type="m">kan</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">ô Ja! ô ja! zonder <rhyme label="f" type="m">dat</rhyme>:</l>
        <l id="s2:l12" met="6" type="-+">Zonder dat wil ik geen <rhyme label="e" type="m" corresp="#s2:l10">man</rhyme>,</l>
        <l id="s2:l13" met="4" type="-+">Zonder dat wil ik geen <rhyme label="e" type="m" corresp="#s2:l10">man</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maar meester als wij <rhyme label="a" type="f">trouwen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Geef zeker uw aan <rhyme label="b" type="m">mij</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Houratie maar <rhyme label="a" type="f" corresp="#s3:l1">mevrouwe</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Geef uw nog veel <rhyme label="b" type="m" corresp="#s3:l2">daarbij</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zoo dat door dit <rhyme label="c" type="m">gewin</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Ons gansch <rhyme label="c" type="m" corresp="#s3:l5">huisgezin</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En huisseljjke <rhyme label="d" type="f">zaken</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Daar door in orde <rhyme label="d" type="f" corresp="#s3:l7">raaken</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Zoo dat ons <rhyme label="d" type="f" corresp="#s3:l7">vermaaken</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Bestendig duren <rhyme label="e" type="m">kan</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">ô Ja! ô ja! zonder <rhyme label="f" type="m">dat</rhyme>:</l>
        <l id="s3:l12" met="6" type="-+">Zonder dat wil ik geen <rhyme label="e" type="m" corresp="#s3:l10">man</rhyme>,</l>
        <l id="s3:l13" met="4" type="-+">Zonder dat wil ik geen <rhyme label="e" type="m" corresp="#s3:l10">man</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
