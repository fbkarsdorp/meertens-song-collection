<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3925</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="farewell song">afscheidslied</tag>
      <tag translation="">geestelijk</tag>
      <tag translation="mystical song">mystiek lied</tag>
    </tags>
    <melody>Ik zeg adieu</melody>
    <categories>
      <category translation="occasions">gelegenheden</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ICk seg <rhyme label="a" type="m">adieu</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Werelt wi twee wi moeten <rhyme label="b" type="f">sceyden</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Van een lief <rhyme label="a" type="m" corresp="#s1:l1">nyeu</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wil ick troost gaen <rhyme label="b" type="f" corresp="#s1:l2">verbeyden</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Ick laet v Iesu dat herte <rhyme label="c" type="m">mijn</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Want waer ghi sijt daer moet ick <rhyme label="c" type="m" corresp="#s1:l5">sijn</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Tsy vruecht oft pijn / tsy vruecht oft <rhyme label="c" type="m" corresp="#s1:l5">pijn</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Altijt moet ick v vry eygen <rhyme label="c" type="m" corresp="#s1:l5">sijn</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Altijd moet ick v vry eygen <rhyme label="c" type="m" corresp="#s1:l5">sijn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Mijns sins <rhyme label="a" type="m">ghequel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat doet mi dicwils <rhyme label="b" type="f">trueren</rhyme></l>
        <l id="s2:l3" met="2" type="-+">En tvleesch <rhyme label="a" type="m" corresp="#s2:l1">rebel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wil des gheests vrede <rhyme label="b" type="f" corresp="#s2:l2">schueren</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Maer Iesu ghi brengt mi wter <rhyme label="c" type="m">noot</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Met liue ghewont aent cruyce <rhyme label="c" type="m" corresp="#s2:l5">bloot</rhyme></l>
        <l id="s2:l7" met="4" type="-+">O liefde groot / o liefde <rhyme label="c" type="m" corresp="#s2:l5">groot</rhyme></l>
        <l id="s2:l8" met="4" type="-+">V eygen blijf ic tot in die <rhyme label="c" type="m" corresp="#s2:l5">doot</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">V eygen blijf ic tot in die <rhyme label="c" type="m" corresp="#s2:l5">doot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ick dancke v <rhyme label="a" type="m">God</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Reyn minnelic God <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Van allen <rhyme label="a" type="m" corresp="#s3:l1">spot</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat ghi mi hebt <rhyme label="b" type="f" corresp="#s3:l2">ghenesen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Nyders fel met haer <rhyme label="c" type="m">fenijn</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Willen ons beletten v blide <rhyme label="c" type="m" corresp="#s3:l5">aenschijn</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Op dit termijn / op dit <rhyme label="c" type="m" corresp="#s3:l5">termijn</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Altijd moet ick v vry eygen <rhyme label="c" type="m" corresp="#s3:l5">sijn</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Altijd moet ick v vry eygen <rhyme label="c" type="m" corresp="#s3:l5">sijn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Myn hope is <rhyme label="a" type="m">troost</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die ghi tot mi doet <rhyme label="b" type="f">keeren</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Heere op my <rhyme label="a" type="m" corresp="#s4:l1">gloost</rhyme></l>
        <l id="s4:l4" met="3" type="-+">So sal mijn vruecht <rhyme label="b" type="f" corresp="#s4:l2">vermeeren</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Want moste ic v deruen ter <rhyme label="c" type="m">noot</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Ic bleue verdruct in liden <rhyme label="c" type="m" corresp="#s4:l5">groot</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Wanhopich bloot / wanhopich <rhyme label="c" type="m" corresp="#s4:l5">bloot</rhyme></l>
        <l id="s4:l8" met="4" type="-+">V eygen bliue ick tot inder <rhyme label="c" type="m" corresp="#s4:l5">doot</rhyme></l>
        <l id="s4:l9" met="4" type="-+">V eygen bliue ick tot inder <rhyme label="c" type="m" corresp="#s4:l5">doot</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Gheenssins van <rhyme label="a" type="m">mi</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En moechdy sijn <rhyme label="b" type="f">ghesceyden</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Met gheen dan <rhyme label="a" type="m" corresp="#s5:l1">di</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Mach ick mi gaen <rhyme label="b" type="f" corresp="#s5:l2">vermeyden</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">In v so vinde ic voetsel <rhyme label="c" type="m">fijn</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Dwelck maect vroem die siele <rhyme label="c" type="m" corresp="#s5:l5">mijn</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Ende lost wt pijn / ende lost wt <rhyme label="c" type="m" corresp="#s5:l5">pijn</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Altijd moet ick v vrij eygen <rhyme label="c" type="m" corresp="#s5:l5">sijn</rhyme></l>
        <l id="s5:l9" met="4" type="-+">Altijd moetick v vry eygen <rhyme label="c" type="m" corresp="#s5:l5">sijn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ghi sijt een <rhyme label="a" type="m">schat</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Verciert met allen <rhyme label="b" type="f">vruechden</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Een costelijck <rhyme label="a" type="m" corresp="#s6:l1">vat</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Alleen veruult met <rhyme label="b" type="f" corresp="#s6:l2">duechden</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Ghi helpt alleen wt <rhyme label="c" type="m">wederstoot</rhyme>.</l>
        <l id="s6:l6" met="4" type="-+">Die hen op v betrouwen <rhyme label="c" type="m" corresp="#s6:l5">bloot</rhyme></l>
        <l id="s6:l7" met="4" type="-+">In alder noot / in alder <rhyme label="c" type="m" corresp="#s6:l5">noot</rhyme>.</l>
        <l id="s6:l8" met="4" type="-+">V eygen blijf ic tot in die <rhyme label="c" type="m" corresp="#s6:l5">doot</rhyme>.</l>
        <l id="s6:l9" met="4" type="-+">V eygen blijf ic tot in die <rhyme label="c" type="m" corresp="#s6:l5">doot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
