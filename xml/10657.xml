  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ISt wel ghedaen, ist wel <rhyme label="a" type="m">ghedaen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Seght my wel schoon <rhyme label="b" type="f">Goddinne</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Seght my wel schoon <rhyme label="b" type="f" corresp="#s1:l2">Goddinne</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat ghy dus saen, dat ghy dus <rhyme label="a" type="m" corresp="#s1:l1">saen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Heel stelt wt uwen <rhyme label="b" type="f" corresp="#s1:l2">sinne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die v bemint die v <rhyme label="a" type="m">bemint</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Boven alle die daer <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Boven alle die daer <rhyme label="b" type="f" corresp="#s2:l2">leven</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">O Venus kindt, O Venus <rhyme label="a" type="m" corresp="#s2:l1">kindt</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Wilt v dienaer troost <rhyme label="b" type="f" corresp="#s2:l2">gheven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want ghy hebt de macht, Want ghy hebt de <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mijn droefheyt te <rhyme label="b" type="f">ghenesen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Mijn droefheyt te <rhyme label="b" type="f" corresp="#s3:l2">ghenesen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Comt toont v cracht, Comt toont v <rhyme label="a" type="m" corresp="#s3:l1">cracht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Soo suldy zijn <rhyme label="b" type="f" corresp="#s3:l2">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Mijn sins confoort, mijn sins <rhyme label="a" type="m">confoort</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En can ick niet <rhyme label="b" type="f">ghehelpen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En can ick niet <rhyme label="b" type="f" corresp="#s4:l2">ghehelpen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Noch der Voghelen gheluyt, Noch der Voghelen <rhyme label="a" type="m" corresp="#s4:l1">gheluyt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En can gheen droefheyt <rhyme label="b" type="f" corresp="#s4:l2">stelpen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Nu comt doch troost, Nu comt doch <rhyme label="a" type="m">troost</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ghy beyt my veel te <rhyme label="b" type="f">langhe</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ghy beyt my veel te <rhyme label="b" type="f" corresp="#s5:l2">langhe</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ick ben verdwijnt, ick ben <rhyme label="a" type="m" corresp="#s5:l1">verdwijnt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Waerom valt ghy my soo <rhyme label="b" type="f" corresp="#s5:l2">stranghe</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">T'is niet mijn schult, T'is niet mijn <rhyme label="a" type="m">schult</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat ick v hebbe <rhyme label="b" type="f">verloren</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Dat ick v hebbe <rhyme label="b" type="f" corresp="#s6:l2">verloren</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Lijdet lief verdult, lijdet lief <rhyme label="a" type="m" corresp="#s6:l1">verdult</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ick bender toe <rhyme label="b" type="f" corresp="#s6:l2">gheboren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Princelijcke Lief, Princelijcke <rhyme label="a" type="m">Lief</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Peynst eens om den tijdt <rhyme label="b" type="f">voorleden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Peynst eens om den tijdt <rhyme label="b" type="f" corresp="#s7:l2">voorleden</rhyme></l>
        <l id="s7:l4" met="4" type="-+">En om v gherief, en om v <rhyme label="a" type="m" corresp="#s7:l1">gherief</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Daer ick v om heb <rhyme label="b" type="f" corresp="#s7:l2">ghebeden</rhyme>.</l>
      </lg>
    </lg>
  </text>
