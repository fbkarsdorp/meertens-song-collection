    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Aus tiefer Not schrei ich zu dir</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk Roep tot Godt met mijner <rhyme label="a" type="m">stem</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ick smeecke tot den <rhyme label="b" type="f">HEERE</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ick schud' wt mijn Gebedt voor <rhyme label="a" type="m" corresp="#s1:l1">hem</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Hy salt verhooren <rhyme label="b" type="f" corresp="#s1:l2">geerne</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Geeff hem te kennen mijnen <rhyme label="c" type="m">Noot</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Als mijn Geest is in Ancxten <rhyme label="c" type="m" corresp="#s1:l5">groot</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">So neemt ghy HEEr my <rhyme label="d" type="f">aene</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sy legghen Strick' op mijne <rhyme label="a" type="m">Baen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Daer op ick soude <rhyme label="b" type="f">Wand'len</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ter Rechter handt wilt mercken <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe sy doch met my <rhyme label="b" type="f" corresp="#s2:l2">hand'len</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Jae, niemandt wil my kennen <rhyme label="c" type="m">meer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Het is vergeefs mijn smeecken <rhyme label="c" type="m" corresp="#s2:l5">HEER</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Niemandt vraeght nae mijn <rhyme label="d" type="f">Ziele</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Tot dy Roep ick HEEr Godt mijn <rhyme label="a" type="m">Heyl</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ick seg: Ghy zijt mijn <rhyme label="b" type="f">Hoope</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Int Landt der Levenden mijn <rhyme label="a" type="m" corresp="#s3:l1">deyl</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met Clacht ick tot dy <rhyme label="b" type="f" corresp="#s3:l2">loope</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Siet HEEr, hoe swaer een Cruys ick <rhyme label="c" type="m">draeg'</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Vrijdt my van mijns Vervolgers <rhyme label="c" type="m" corresp="#s3:l5">plaeg'</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Want sy zijn my te <rhyme label="d" type="f">Machtich</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Mijn Ziel Heer wt den Kercker <rhyme label="a" type="m">leyd'</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat ick Danck dijnen <rhyme label="b" type="f">Name</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die Vrome sullen wel <rhyme label="a" type="m" corresp="#s4:l1">bereydt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">By my al comen <rhyme label="b" type="f" corresp="#s4:l2">tsame</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Wanneer dat ghy my Heer wel <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dan sal ick dijne Weldaet <rhyme label="c" type="m" corresp="#s4:l5">goet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">In dijn Gemeynt' <rhyme label="d" type="f">verconden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
