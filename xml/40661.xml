<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>40661</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Grâce au bon petit Jésus</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">LOff Iesu die tot ons <rhyme label="a" type="m">profijt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Op desen <rhyme label="a" type="m" corresp="#s1:l1">tijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghedaelt sijt wt des Vaders <rhyme label="b" type="m">schoot</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Om te <rhyme label="c" type="f">verdraghen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Naer luttel <rhyme label="c" type="f" corresp="#s1:l4">daghen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die bitter <rhyme label="b" type="m" corresp="#s1:l3">doodt</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s1:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s1:l8">beminnen</rhyme>:</l>
        <l id="s1:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s1:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy comt ter werelt naeckt en <rhyme label="a" type="m">bloot</rhyme></l>
        <l id="s2:l2" met="2" type="-+">In grooten <rhyme label="a" type="m" corresp="#s2:l1">noot</rhyme></l>
        <l id="s2:l3" met="4" type="-+">En achter laet 's Hemels <rhyme label="b" type="m">plaisier</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Om deur <rhyme label="c" type="f">lijden</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Ons te <rhyme label="c" type="f" corresp="#s2:l4">verblijden</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Van 's Helsche <rhyme label="b" type="m" corresp="#s2:l3">vier</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s2:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s2:l8">beminnen</rhyme>:</l>
        <l id="s2:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s2:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Aensiet o mensch hoe dat dit <rhyme label="a" type="m">kijnt</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Sy selven <rhyme label="a" type="m" corresp="#s3:l1">pijnt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En maeckt hem cleyn in 'smenschen <rhyme label="b" type="m">schijn</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Om ons te <rhyme label="c" type="f">trecken</rhyme></l>
        <l id="s3:l5" met="2" type="-+">En te <rhyme label="c" type="f" corresp="#s3:l4">verwecken</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Om kleyn te <rhyme label="b" type="m" corresp="#s3:l3">sijn</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s3:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s3:l8">beminnen</rhyme>:</l>
        <l id="s3:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s3:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Siet oft ghy sulck een liefde <rhyme label="a" type="m">vint</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Godt wordt een <rhyme label="a" type="m" corresp="#s4:l1">kint</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Die eeuwigh was wort inden <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Een <rhyme label="c" type="f">creature</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Wt liefde <rhyme label="c" type="f" corresp="#s4:l4">pure</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Tot ons <rhyme label="b" type="m" corresp="#s4:l3">profijt</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s4:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s4:l8">beminnen</rhyme>:</l>
        <l id="s4:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s4:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wie heeft sulcx wonder oyt <rhyme label="a" type="m">ghehoort</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Vleesch wordt het <rhyme label="a" type="m" corresp="#s5:l1">Woordt</rhyme></l>
        <l id="s5:l3" met="4" type="-+">En onsen Vader op dit <rhyme label="b" type="m">pas</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Wort onsen <rhyme label="c" type="f">broede</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Een Maghet sijns <rhyme label="c" type="f" corresp="#s5:l4">Moeder</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Wiens kint hy <rhyme label="b" type="m" corresp="#s5:l3">was</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s5:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s5:l8">beminnen</rhyme>:</l>
        <l id="s5:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s5:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">In't kribbeken wiert hy <rhyme label="a" type="m">gheleyt</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Wiens <rhyme label="a" type="m" corresp="#s6:l1">Majesteyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Coninghen te boven <rhyme label="b" type="m">gaet</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Die wert <rhyme label="c" type="f">ghewonden</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Om onse <rhyme label="c" type="f" corresp="#s6:l4">sonden</rhyme></l>
        <l id="s6:l6" met="2" type="-+">En doecxkens <rhyme label="b" type="m" corresp="#s6:l3">quaet</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s6:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s6:l8">beminnen</rhyme>:</l>
        <l id="s6:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s6:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Met woorden toonen sijn <rhyme label="a" type="m">verdriet</rhyme></l>
        <l id="s7:l2" met="2" type="-+">En kan hy <rhyme label="a" type="m" corresp="#s7:l1">niet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Maer sijn kleyn ooghskens sijn <rhyme label="b" type="m">vervuldt</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Met veel <rhyme label="c" type="f">tranen</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Om te <rhyme label="c" type="f" corresp="#s7:l4">vermanen</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Der menschen <rhyme label="b" type="m" corresp="#s7:l3">schult</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s7:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s7:l8">beminnen</rhyme>:</l>
        <l id="s7:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s7:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O Moeder Godts en laet ons <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s8:l2" met="2" type="-+">In ons <rhyme label="a" type="m" corresp="#s8:l1">verdriet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Maer op dat wy sonder <rhyme label="b" type="m">misdaet</rhyme></l>
        <l id="s8:l4" met="2" type="-+">V Soon <rhyme label="c" type="f">behaghen</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Van ons <rhyme label="c" type="f" corresp="#s8:l4">verjaghen</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Wilt alle <rhyme label="b" type="m" corresp="#s8:l3">quaet</rhyme>.</l>
        <l id="s8:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s8:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s8:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s8:l8">beminnen</rhyme>:</l>
        <l id="s8:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s8:l7">soet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Iesus die tot ons nu ter <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Ghecomen <rhyme label="a" type="m" corresp="#s9:l1">zijt</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Wy bidden u gheeft ons den <rhyme label="b" type="m">loon</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Daer naer dit <rhyme label="c" type="f">lijden</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Wy ons <rhyme label="c" type="f" corresp="#s9:l4">verblijden</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">In uwen <rhyme label="b" type="m" corresp="#s9:l3">throon</rhyme>.</l>
        <l id="s9:l7" met="4" type="-+">Laet ons met eenen blijden <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s9:l8" met="2" type="-+">Nu eens <rhyme label="e" type="f">beghinnen</rhyme></l>
        <l id="s9:l9" met="2" type="-+">V te <rhyme label="e" type="f" corresp="#s9:l8">beminnen</rhyme>:</l>
        <l id="s9:l10" met="2" type="-+">O Iesu <rhyme label="d" type="m" corresp="#s9:l7">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
