<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8166</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Liefste gij zijt ongestadig</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LIefste ghy zijt mijn <rhyme label="a" type="f">genadigh</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Kus ick eens u rooder <rhyme label="b" type="m">mond</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ghy weeroom kust myn <rhyme label="a" type="f" corresp="#s1:l1">gestadigh</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En dat maackt mijn ziel <rhyme label="b" type="m" corresp="#s1:l2">gesont</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Voel ick dan myn lusjes <rhyme label="c" type="f">branden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Om in uwe schoot te <rhyme label="c" type="f" corresp="#s1:l5">landen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Soo blust ghy mijn <rhyme label="d" type="m">minne-vlam</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Die van u sijn oorsprongh <rhyme label="d" type="m" corresp="#s1:l7">nam</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maar gy kent die soo niet <rhyme label="a" type="f">blussen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Of 't ontfonckt al weder <rhyme label="b" type="m">aan</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Door u soet en vriendelick <rhyme label="a" type="f" corresp="#s2:l1">kussen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Houd ghy myn jonck hart <rhyme label="b" type="m" corresp="#s2:l2">gevaan</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Maar soo ick dan u lief moet <rhyme label="c" type="f">derven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Sal ick smeecken om mijn <rhyme label="c" type="f" corresp="#s2:l5">sterven</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Want myn hart, en ziel, en <rhyme label="d" type="m">min</rhyme>;</l>
        <l id="s2:l8" met="4" type="+-">Heerst ghy schoone <rhyme label="d" type="m" corresp="#s2:l7">veld-Godin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Heugt mijn dat ick door u <rhyme label="a" type="f">loncken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">d' Aldergrootste vreucht <rhyme label="b" type="m">verkreegh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En van 't kussen wierd soo <rhyme label="a" type="f" corresp="#s3:l1">droncken</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Dat ick uyt u armen <rhyme label="b" type="m" corresp="#s3:l2">zeegh</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Doe zagh ick u rooder <rhyme label="c" type="f">bloosen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Als de alderschoonste <rhyme label="c" type="f" corresp="#s3:l5">Roosen</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Die in Venus lusthof <rhyme label="d" type="m">staan</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En van Rijckdom zwanger <rhyme label="d" type="m" corresp="#s3:l7">gaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ach hoe vaack laaght ghy <rhyme label="a" type="f">gestrengelt</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Met u beentjes om de <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Daar mijn lust meer steets naar <rhyme label="a" type="f" corresp="#s4:l1">hengelt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Hemel moet dat altyd <rhyme label="b" type="m" corresp="#s4:l2">zyn</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Liefste ghy zyt mijn <rhyme label="c" type="f">getuygen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat ick quam de Nectar <rhyme label="c" type="f" corresp="#s4:l5">zuygen</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Uyt u roo koorale <rhyme label="d" type="m">mond</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">'T geen myn zieltje heeft <rhyme label="d" type="m" corresp="#s4:l7">gewond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Door het soet en lieflick <rhyme label="a" type="f">woelen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">In u blanck albaste <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Deed ick u een vreucht <rhyme label="a" type="f" corresp="#s5:l1">gevoelen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Ach! een vreuchde alsoo <rhyme label="b" type="m" corresp="#s5:l2">groot</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Dat u lodderlicke <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">In u voorhooft so <rhyme label="c" type="f" corresp="#s5:l5">vervlogen</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Dat ick niet en zagh als 't <rhyme label="d" type="m">scheel</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Maar doen kreegh ick mee mijn <rhyme label="d" type="m" corresp="#s5:l7">deel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'K hoop de tijd weer haast sal <rhyme label="a" type="f">komen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Dat de Nectar in u <rhyme label="b" type="m">schoot</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Sal van wellust <rhyme label="a" type="f" corresp="#s6:l1">overstroomen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Door de vreuchde <rhyme label="b" type="m" corresp="#s6:l2">overgroot</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Dan sal ick vermoeyt van <rhyme label="c" type="f">lusten</rhyme></l>
        <l id="s6:l6" met="4" type="+-">In u blancke armen <rhyme label="c" type="f" corresp="#s6:l5">rusten</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Tot dat ons een nieuwe <rhyme label="d" type="m">lust</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Wederom beneemt de <rhyme label="d" type="m" corresp="#s6:l7">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
