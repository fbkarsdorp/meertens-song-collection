<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184238</id>
    <date>
      <year-start>1798</year-start>
      <year-end>1798</year-end>
    </date>
    <tags/>
    <melody>Schone beemden zalig veld</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zing ô Vaderlandsche <rhyme label="a" type="m">Jeugd</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die gebooren voor de <rhyme label="a" type="m" corresp="#s1:l1">vreugd</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Sombre droefheid blyft <rhyme label="b" type="f">ontvlieden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Blydschap moet uw hart <rhyme label="b" type="f" corresp="#s1:l3">gebieden</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Zing nu vroolyk zing nu <rhyme label="c" type="m">bly</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Lustig, <rhyme label="d" type="f">lustig</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Zing nu vroolyk zing nu <rhyme label="c" type="m" corresp="#s1:l5">bly</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Jongheid haat de <rhyme label="c" type="m" corresp="#s1:l5">knynzery</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Waarom zoud Gy zoet <rhyme label="a" type="m">vermaak</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Niet aan Schoonheid Kunst en <rhyme label="a" type="m" corresp="#s2:l1">smaak</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">In uw lieve Lente <rhyme label="b" type="f">Jaaren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Blyde Jongheid vroolyk <rhyme label="b" type="f" corresp="#s2:l3">paaren</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Zing dan zing dan op de <rhyme label="c" type="m">maat</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Lustig, <rhyme label="d" type="f">lustig</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Zing dan zing dan op de <rhyme label="c" type="m" corresp="#s2:l5">maat</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Die myn zwakke Zangster <rhyme label="c" type="m" corresp="#s2:l5">staat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zing tot eer den poezie <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En Vronw Juno haar <rhyme label="a" type="m" corresp="#s3:l1">vriendin</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">De een eist leevens <rhyme label="b" type="f">Offerhanden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">De andre hecht de Huuwlyks <rhyme label="b" type="f" corresp="#s3:l3">banden</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Zing den Lof der zalig <rhyme label="c" type="m">Echt</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Lustig, <rhyme label="d" type="f">lustig</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Zing den Lof der zalig <rhyme label="c" type="m" corresp="#s3:l5">Echt</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Die de Harten samen <rhyme label="c" type="m" corresp="#s3:l5">hecht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Of wild gy een andren <rhyme label="a" type="m">toon</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Zing dan niet van Venus <rhyme label="a" type="m" corresp="#s4:l1">Zoon</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Maar van Mars dien <rhyme label="b" type="f">Koppenkloover</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Landverwoester Steden <rhyme label="b" type="f" corresp="#s4:l3">roover</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Zing dan van het Oorlogs <rhyme label="c" type="m">veld</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Lustig, <rhyme label="d" type="f">lustig</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Zing dan van het Oorlogs <rhyme label="c" type="m" corresp="#s4:l5">veld</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En schenk Lauwren aan de <rhyme label="c" type="m" corresp="#s4:l5">Held</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dat de Lente uw hart <rhyme label="a" type="m">bekoor</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Zing ô Jeugd, ik zing u <rhyme label="a" type="m" corresp="#s5:l1">voor</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Laat den toon van <rhyme label="b" type="f">Philomeelen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Uwe jonge zinnen <rhyme label="b" type="f" corresp="#s5:l3">streelen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Zing dan zing dan in het <rhyme label="c" type="m">groen</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Lustig, <rhyme label="d" type="f">lustig</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Zing dan zing dan in het <rhyme label="c" type="m" corresp="#s5:l5">groen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Lente kan de Zanglust <rhyme label="c" type="m" corresp="#s5:l5">voen</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zing van Maaij en buurman <rhyme label="a" type="m">piet</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Goede Jorde en kwaade <rhyme label="a" type="m" corresp="#s6:l1">Griet</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Zing met my het buiten <rhyme label="b" type="f">leven</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Dat kan elk genoegen <rhyme label="b" type="f" corresp="#s6:l3">geven</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Maar vergeet ook niet, den <rhyme label="c" type="m">Wyn</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Lustig, <rhyme label="d" type="f">lustig</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Maar vergeet ook niet de <rhyme label="c" type="m" corresp="#s6:l5">Wyn</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Naar moet van gezongen <rhyme label="c" type="m" corresp="#s6:l5">zyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
