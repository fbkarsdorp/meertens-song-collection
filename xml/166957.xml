  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">De Oostinje-vaars zijn <rhyme label="a" type="f">binnen</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Te zamen van ‘t <rhyme label="b" type="m">Iaar</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Komt laat ons <rhyme label="a" type="f" corresp="#s1:l1">beginnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Te wenschen <rhyme label="b" type="m" corresp="#s1:l2">malkaar</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wellekom mijn <rhyme label="c" type="f">welbeminden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wellekom liefjes <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Het gelukt my u te <rhyme label="c" type="f" corresp="#s1:l5">vinden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Hier aan de <rhyme label="d" type="m" corresp="#s1:l6">Amstel-wal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Amsterdamse <rhyme label="a" type="f">Waardinne</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">Die zyn weer vol <rhyme label="b" type="m">zwier</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Het Na-schip is <rhyme label="a" type="f" corresp="#s2:l1">binnen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">‘t Is haar een <rhyme label="b" type="m" corresp="#s2:l2">plysier</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Nog meerder de <rhyme label="c" type="f">Hoeren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met haar geveinst <rhyme label="d" type="m">gestreel</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die leggen te <rhyme label="c" type="f" corresp="#s2:l5">loeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En wenschen haar <rhyme label="d" type="m" corresp="#s2:l6">deel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Nu zitten de <rhyme label="a" type="f">Hoeren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Als met open <rhyme label="b" type="m">keel</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om u te <rhyme label="a" type="f" corresp="#s3:l1">vervoeren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Door haar zoet <rhyme label="b" type="m" corresp="#s3:l2">gestreel</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Zy weeten <rhyme label="c" type="f">Oost-Inje</rhyme></l>
        <l id="s3:l6" met="3" type="-+">De naam brengt het <rhyme label="d" type="m">mee</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Om je gelt, ik <rhyme label="c" type="f" corresp="#s3:l5">beminje</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Wyn Vrind, weest te <rhyme label="d" type="m" corresp="#s3:l6">vree</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Oost-Indiesche <rhyme label="a" type="f">Matroozen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Als zy zyn aan <rhyme label="b" type="m">wal</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zy gaanen <rhyme label="a" type="f" corresp="#s4:l1">Liefkozen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En dat <rhyme label="b" type="m" corresp="#s4:l2">overal</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">By Nagten en <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En ‘s Morgens <rhyme label="d" type="m">vroeg</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Komt men na haar <rhyme label="c" type="f" corresp="#s4:l5">vragen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zy zyn in de <rhyme label="d" type="m" corresp="#s4:l6">kroeg</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ia ook de <rhyme label="a" type="f">Officieren</rhyme>:</l>
        <l id="s5:l2" met="3" type="-+">Die leven <rhyme label="b" type="m">briljant</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Zy gaan eens <rhyme label="a" type="f" corresp="#s5:l1">zwieren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met Iuffrouws van de <rhyme label="b" type="m" corresp="#s5:l2">trant</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Bardeel en <rhyme label="c" type="f">Biliarte</rhyme>;’</l>
        <l id="s5:l6" met="3" type="-+">Die houden weer <rhyme label="d" type="m">aan</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Zy laaten zig niet <rhyme label="c" type="f" corresp="#s5:l5">tarte</rhyme>;</l>
        <l id="s5:l8" met="3" type="-+">Het kan ‘er op <rhyme label="d" type="m" corresp="#s5:l6">staan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Des ‘s Middags niet te <rhyme label="a" type="f">Eeten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Des ‘s Avonds niet ‘t <rhyme label="b" type="m">huis</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Men moet dan ook <rhyme label="a" type="f" corresp="#s6:l1">weeten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat daar is munt nog <rhyme label="b" type="m" corresp="#s6:l2">kruys</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Zo lang dat is by <rhyme label="c" type="f">haar</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Soo is ‘t schenk maar <rhyme label="d" type="m">in</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Sa Hospes rap ons <rhyme label="c" type="f" corresp="#s6:l5">maat</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat gaat na ons <rhyme label="d" type="m" corresp="#s6:l6">zin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dan kan men ze <rhyme label="a" type="f">vinden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Des ‘s Avonds zeer <rhyme label="b" type="m">laat</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Met Snol of <rhyme label="a" type="f" corresp="#s7:l1">Beminden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Men wandelt op <rhyme label="b" type="m" corresp="#s7:l2">straat</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Men hoort zoete <rhyme label="c" type="f">Liefje</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Wilt gy met myn <rhyme label="d" type="m">gaan</rhyme></l>
        <l id="s7:l7" met="3" type="-+">(Seg zoete lieve <rhyme label="c" type="f" corresp="#s7:l5">diefje</rhyme>,)</l>
        <l id="s7:l8" met="3" type="-+">Niet veer hier <rhyme label="d" type="m" corresp="#s7:l6">daan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maar dog voor het <rhyme label="a" type="f">leste</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Oost Indies-vaarders <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ik raad uw ten <rhyme label="a" type="f" corresp="#s8:l1">besten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Als gy zyt aan <rhyme label="b" type="m" corresp="#s8:l2">Wal</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Wilt uw wat <rhyme label="c" type="f">toonen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En leven zoo niet <rhyme label="d" type="m">voort</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Ofse zult weer haast <rhyme label="c" type="f" corresp="#s8:l5">koomen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Aen een ander <rhyme label="d" type="m" corresp="#s8:l6">Scheepsboort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
