    <date>
      <year-start>1745</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody>Le printemps rappelle aux armes</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">CUpidootje al u <rhyme label="a" type="f">plagen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Kan ik niet <rhyme label="a" type="f" corresp="#s1:l1">dragen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Schelmpje gy doed my <rhyme label="a" type="f" corresp="#s1:l1">klagen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">En lyden <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Zo gy hooger trekt u <rhyme label="a" type="f" corresp="#s1:l1">lagen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Mag ik niet in wezen <rhyme label="b" type="m" corresp="#s1:l4">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zal altyd die zoete <rhyme label="a" type="f">Schoone</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Haar troost <rhyme label="a" type="f" corresp="#s2:l1">vertoonen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wreden zult gy niet eens <rhyme label="a" type="f" corresp="#s2:l1">loonen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Die om u <rhyme label="b" type="m">zugt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Over lang hebt gy doen <rhyme label="a" type="f" corresp="#s2:l1">toonen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Al 't geen my baard <rhyme label="b" type="m" corresp="#s2:l4">ongenugt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Steeds zyn alle myn <rhyme label="a" type="f">gedagten</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">By dag en <rhyme label="a" type="f" corresp="#s3:l1">nagten</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Vol van troostelooze <rhyme label="a" type="f" corresp="#s3:l1">klagten</rhyme></l>
        <l id="s3:l4" met="2" type="-+">En bittere <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Zo gy die niet wilt <rhyme label="a" type="f" corresp="#s3:l1">verzagten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Treft my dan maar diep in 't <rhyme label="b" type="m" corresp="#s3:l4">hert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'t Is een straf, myn jeugdig <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Zo te doen <rhyme label="a" type="f" corresp="#s4:l1">beven</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Ik ben gantsch niet <rhyme label="a" type="f" corresp="#s4:l1">onbedreven</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">In stuk van <rhyme label="b" type="m">Min</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Wilt gy dat ik blyf in 't <rhyme label="a" type="f" corresp="#s4:l1">leven</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Zo toond my dog <rhyme label="b" type="m" corresp="#s4:l4">weder-min</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Help my dog myn jonge <rhyme label="a" type="f">jaaren</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Fris te <rhyme label="a" type="f" corresp="#s5:l1">bewaaren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Help my dog myn jonge <rhyme label="a" type="f" corresp="#s5:l1">jaaren</rhyme></l>
        <l id="s5:l4" met="2" type="-+">En schoone <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Want 't zaizoen der gryze <rhyme label="a" type="f" corresp="#s5:l1">hairen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Gantsch geen liefde meer en <rhyme label="b" type="m" corresp="#s5:l4">lyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
