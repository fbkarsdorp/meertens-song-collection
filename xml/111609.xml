  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">BY Somersche <rhyme label="a" type="f">Dagen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">In Bosschen en <rhyme label="a" type="f" corresp="#s1:l1">Hagen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Zo tragt ik de <rhyme label="a" type="f" corresp="#s1:l1">plagen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Der Min te <rhyme label="b" type="m">ontvli'en</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">BY Somersche <rhyme label="a" type="f" corresp="#s1:l1">Dagen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">In Bosschen en <rhyme label="a" type="f" corresp="#s1:l1">Hagen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Zo tragt ik de <rhyme label="a" type="f" corresp="#s1:l1">plagen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Der Min te <rhyme label="b" type="m" corresp="#s1:l4">ontvli'en</rhyme>;</l>
        <l id="s1:l9" met="4" type="+-">En heb ik dan geen <rhyme label="c" type="f">Maitresje</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Zo kus ik het <rhyme label="c" type="f" corresp="#s1:l9">flesje</rhyme>,</l>
        <l id="s1:l11" met="5" type="+-">En helpt geen glasje zo neem ik 'er <rhyme label="b" type="m" corresp="#s1:l4">tien</rhyme></l>
        <l id="s1:l12" met="2" type="-+">En zoen ik geen <rhyme label="d" type="f">Meysje</rhyme>,</l>
        <l id="s1:l13" met="2" type="-+">Zo rook in een <rhyme label="d" type="f" corresp="#s1:l12">reysje</rhyme>:</l>
        <l id="s1:l14" met="5" type="+-">Noyt sal iemand my swaarmoedig <rhyme label="b" type="m" corresp="#s1:l4">zien</rhyme>.</l>
      </lg>
    </lg>
