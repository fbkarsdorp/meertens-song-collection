    <tags/>
    <melody>Ik lach om het meisje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Myn Lief <rhyme label="a" type="f">gedoken</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">In schaeuw van <rhyme label="b" type="f">Rozen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Die lieflyk <rhyme label="b" type="f" corresp="#s1:l2">blozen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Van Daeuw <rhyme label="c" type="m">doornat</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Werd laetst <rhyme label="a" type="f" corresp="#s1:l1">gestoken</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Door's Bytjes <rhyme label="d" type="f">degen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Dat neÃªr <rhyme label="d" type="f" corresp="#s1:l6">gezegen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Op 't Roosje <rhyme label="c" type="m" corresp="#s1:l4">zat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Zy voelt het <rhyme label="a" type="f">steken</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">En valt aen 't <rhyme label="b" type="f">kermen</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">Ik vol <rhyme label="b" type="f" corresp="#s2:l2">ontfermen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Hoor hare <rhyme label="c" type="m">klagt</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">'k Zei; hy 's <rhyme label="a" type="f" corresp="#s2:l1">geweken</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Doch heeft de <rhyme label="d" type="f">tipjes</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Van uwe <rhyme label="d" type="f" corresp="#s2:l6">lipjes</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Een Roos <rhyme label="c" type="m" corresp="#s2:l4">geacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Voelt ge u zoo <rhyme label="a" type="f">kwellen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Door een klein <rhyme label="b" type="f">wondje</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Aen uw Liefs <rhyme label="b" type="f" corresp="#s3:l2">mondje</rhyme>?</l>
        <l id="s3:l4" met="2" type="-+">Baert u dit <rhyme label="c" type="m">smart</rhyme>?</l>
        <l id="s3:l5" met="2" type="-+">Hoe zal ik 't <rhyme label="a" type="f" corresp="#s3:l1">stellen</rhyme>?</l>
        <l id="s3:l6" met="2" type="-+">Wyl uit uwe <rhyme label="d" type="f">oogen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Veel schichten <rhyme label="d" type="f" corresp="#s3:l6">vlogen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Tot in myn <rhyme label="c" type="m" corresp="#s3:l4">hart</rhyme>.</l>
      </lg>
    </lg>
  </text>
