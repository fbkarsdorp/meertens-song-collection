<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24557</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>O Heer der hemelen stichter</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Here der <rhyme label="a" type="f">hemelen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">stichter ende alle der werlde <rhyme label="a" type="f" corresp="#s1:l1">verlichter</rhyme></l>
        <l id="s1:l3" met="3" type="-+">als ic my van bynnen <rhyme label="b" type="f">beschouwe</rhyme></l>
        <l id="s1:l4" met="3" type="-+">soe heb ik <rhyme label="b" type="f" corresp="#s1:l3">grote</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Myn schult is also <rhyme label="a" type="f">groete</rhyme></l>
        <l id="s2:l2" met="3" type="-+">die ic betaelen <rhyme label="a" type="f" corresp="#s2:l1">moete</rhyme></l>
        <l id="s2:l3" met="3" type="-+">myn mysdaet is alsoe <rhyme label="b" type="f">menchuolt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">tys wonder dat my god <rhyme label="b" type="f" corresp="#s2:l3">ontholt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick mochte wal rechte <rhyme label="a" type="f">myshapen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">van groten swaeren <rhyme label="a" type="f" corresp="#s3:l1">saken</rhyme></l>
        <l id="s3:l3" met="3" type="-+">mer my is troest <rhyme label="b" type="f">gecomen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">nye vroude heb ic <rhyme label="b" type="f" corresp="#s3:l3">vernamen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Crystus is ons <rhyme label="a" type="f">gebaren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">ten blyuet nyemant <rhyme label="a" type="f" corresp="#s4:l1">verlaren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">die Jhesum wil <rhyme label="b" type="f">versoeken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">gewonden in armen <rhyme label="b" type="f" corresp="#s4:l3">doeken</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick wil myn willeken <rhyme label="a" type="f">richten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">soe en derf ic niet meer <rhyme label="a" type="f" corresp="#s5:l1">swichten</rhyme></l>
        <l id="s5:l3" met="3" type="-+">want nv die enghelen <rhyme label="b" type="f">syngen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">vrede den menschen van gueden <rhyme label="b" type="f" corresp="#s5:l3">willen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick wil mitten hierdekens <rhyme label="a" type="f">lyden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">toe betlahem ter <rhyme label="a" type="f" corresp="#s6:l1">syden</rhyme></l>
        <l id="s6:l3" met="3" type="-+">daer vynde ic dat wal suete <rhyme label="b" type="f">kynt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">mit syre moeder seer <rhyme label="b" type="f" corresp="#s6:l3">ghemynt</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Al Ist in doeken <rhyme label="a" type="f">gewonden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">het vryet nochtant van <rhyme label="a" type="f" corresp="#s7:l1">sunden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">al Ist in eenre crybben <rhyme label="b" type="f">geleyt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">hy Ist die hemel ende eerde <rhyme label="b" type="f" corresp="#s7:l3">draghet</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick wil mitten Conyngen <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">want ic hebbe wal <rhyme label="a" type="f" corresp="#s8:l1">vernamen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">dat sy ter stede syn <rhyme label="b" type="f">geleyt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">mit eenre steernen <rhyme label="b" type="f" corresp="#s8:l3">onderscheyt</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Sy offerden vanden <rhyme label="a" type="f">besten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">al dat sy conden <rhyme label="a" type="f" corresp="#s9:l1">geleysten</rhyme></l>
        <l id="s9:l3" met="3" type="-+">ic wil oec des gelickes <rhyme label="b" type="f">doen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">met mynen eyget die gades <rhyme label="b" type="f" corresp="#s9:l3">soen</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Och mocht ic vorder <rhyme label="a" type="f">naken</rhyme></l>
        <l id="s10:l2" met="3" type="-+">dat my maria oer lieue <rhyme label="a" type="f" corresp="#s10:l1">kynt</rhyme></l>
        <l id="s10:l3" met="3" type="-+">wolde  handelen laten seer <rhyme label="b" type="f">ghemynt</rhyme></l>
        <l id="s10:l4" met="3" type="-+">dat solde myn hert <rhyme label="b" type="f" corresp="#s10:l3">verblyden</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Hoe geerne wolde icken <rhyme label="a" type="f">ontfangen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">mit mynen armen <rhyme label="a" type="f" corresp="#s11:l1">omvangen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">ic wolden aen my <rhyme label="b" type="f">drucken</rhyme></l>
        <l id="s11:l4" met="3" type="-+">mochtet my nae willen <rhyme label="b" type="f" corresp="#s11:l3">gelucken</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Syn voetkens wolde ic <rhyme label="a" type="f">cussen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">mit mynnen aen my <rhyme label="a" type="f" corresp="#s12:l1">drucken</rhyme></l>
        <l id="s12:l3" met="3" type="-+">syn hertekyn vol der <rhyme label="b" type="f">godtelicheit</rhyme></l>
        <l id="s12:l4" met="3" type="-+">dat wolde ic cussen al <rhyme label="b" type="f" corresp="#s12:l3">bereyt</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Mochte hi mynen adem <rhyme label="a" type="f">liden</rhyme></l>
        <l id="s13:l2" met="3" type="-+">ic wolde noch nare <rhyme label="a" type="f" corresp="#s13:l1">tyden</rhyme></l>
        <l id="s13:l3" met="3" type="-+">ick wolde syn mondekyn <rhyme label="b" type="f">rueren</rhyme></l>
        <l id="s13:l4" met="3" type="-+">och mochtet lange <rhyme label="b" type="f" corresp="#s13:l3">dueren</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wolde hi my syn handekens <rhyme label="a" type="f">reyken</rhyme></l>
        <l id="s14:l2" met="3" type="-+">een mynlic vredelic <rhyme label="a" type="f" corresp="#s14:l1">teyken</rhyme></l>
        <l id="s14:l3" met="3" type="-+">soe waer mynen noet <rhyme label="b" type="f">verwonnen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">wat solde my deren <rhyme label="b" type="f" corresp="#s14:l3">connen</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Al is dit wal <rhyme label="a" type="f">gesongen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">ten is niet dan <rhyme label="a" type="f" corresp="#s15:l1">begonnen</rhyme></l>
        <l id="s15:l3" met="3" type="-+">die Jhesus wil <rhyme label="b" type="f">gewynnen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">die sueken al van <rhyme label="b" type="f" corresp="#s15:l3">bynnen</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Die Jhesus heuet te <rhyme label="a" type="f">vrende</rhyme></l>
        <l id="s16:l2" met="3" type="-+">hy voert oen nae deser <rhyme label="a" type="f" corresp="#s16:l1">ellenden</rhyme></l>
        <l id="s16:l3" met="3" type="-+">al in syns vaders <rhyme label="b" type="f">hemelryke</rhyme></l>
        <l id="s16:l4" met="3" type="-+">och vrent wie doet ons des <rhyme label="b" type="f" corresp="#s16:l3">ghelick</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Och Jhesus heer Conynck <rhyme label="a" type="f">auergroet</rhyme></l>
        <l id="s17:l2" met="3" type="-+">vryet ons van sunden voerder <rhyme label="a" type="f" corresp="#s17:l1">doet</rhyme></l>
        <l id="s17:l3" met="3" type="-+">geeft dat wy ons <rhyme label="b" type="f">besynnen</rhyme></l>
        <l id="s17:l4" met="3" type="-+">ende v alleene <rhyme label="b" type="f" corresp="#s17:l3">mynnen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
