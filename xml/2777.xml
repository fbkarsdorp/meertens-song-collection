  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Veenes met uwen minne <rhyme label="a" type="f">straele</rhyme></l>
        <l id="s1:l2" met="5" type="+-">Waerom hebt gy myn Jonger hert <rhyme label="b" type="m">geraeckt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">gy syt er die prinse <rhyme label="a" type="f" corresp="#s1:l1">paele</rhyme></l>
        <l id="s1:l4" met="5" type="+-">Daer myn jonger hert na <rhyme label="b" type="m" corresp="#s1:l2">haeokt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">En in vieryger minne <rhyme label="b" type="m" corresp="#s1:l2">blaeckt</rhyme></l>
        <l id="s1:l6" met="4" type="+-">En om u reyn maechdelick <rhyme label="b" type="m" corresp="#s1:l2">saet</rhyme></l>
        <l id="s1:l7" met="4" type="+-">en een sweer torment en een swaer <rhyme label="c" type="m">torment</rhyme></l>
        <l id="s1:l8" met="5" type="+-">altyt blyf Ick van myn lief <rhyme label="c" type="m" corresp="#s1:l7">apsent</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Een aesom soet van <rhyme label="a" type="f">guere</rhyme></l>
        <l id="s2:l2" met="5" type="+-">Heeft die alderliefste <rhyme label="b" type="m">delykaet</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Twee wangetges net en <rhyme label="a" type="f" corresp="#s2:l1">puere</rhyme></l>
        <l id="s2:l4" met="5" type="+-">bloeyende als een appel van <rhyme label="b" type="m" corresp="#s2:l2">gernaet</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Die Int herte wel <rhyme label="b" type="m" corresp="#s2:l2">voerstset</rhyme></l>
        <l id="s2:l6" met="4" type="+-">en reyn maechdelick <rhyme label="b" type="m" corresp="#s2:l2">delykaet</rhyme></l>
        <l id="s2:l7" met="4" type="+-">en u oechkens schoen <rhyme label="c" type="m">en</rhyme></l>
        <l id="s2:l8" met="5" type="+-">slaet ouer myn schaemele <rhyme label="c" type="m" corresp="#s2:l7">persoen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Schoen lief schoen blomken bouen <rhyme label="a" type="f">maeten</rhyme></l>
        <l id="s3:l2" met="5" type="+-">Waerom sydy dus <rhyme label="b" type="m">voerstoert</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Tsyn niet dan klapper die ons <rhyme label="a" type="f" corresp="#s3:l1">haeten</rhyme></l>
        <l id="s3:l4" met="5" type="+-">Die en souke niet dan <rhyme label="b" type="m" corresp="#s3:l2">diskoert</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Die soe gaeren soude selfs aen <rhyme label="b" type="m" corresp="#s3:l2">boert</rhyme></l>
        <l id="s3:l6" met="4" type="+-">soude liece smaeken <rhyme label="b" type="m" corresp="#s3:l2">bloet</rhyme></l>
        <l id="s3:l7" met="4" type="+-">seer tryonfant seer <rhyme label="c" type="m">tryonfant</rhyme></l>
        <l id="s3:l8" met="5" type="+-">oe schoen blinckende wel <rhyme label="c" type="m" corresp="#s3:l7">dieaemant</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Prinsesse lief <rhyme label="a" type="f">gepresen</rhyme></l>
        <l id="s4:l2" met="5" type="+-">en voerhoert myn bitter klaegen <rhyme label="b" type="m">groet</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Laet myn u schaemeledienaer <rhyme label="a" type="f" corresp="#s4:l1">weese</rhyme></l>
        <l id="s4:l4" met="5" type="+-">en verlost myn lief uit deesen <rhyme label="b" type="m" corresp="#s4:l2">noet</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Anders dan die bittere <rhyme label="b" type="m" corresp="#s4:l2">doet</rhyme></l>
        <l id="s4:l6" met="4" type="+-">moet Ick liefie smaeken <rhyme label="b" type="m" corresp="#s4:l2">bloet</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Dus siet voer din dus siet voer <rhyme label="c" type="m">din</rhyme></l>
        <l id="s4:l8" met="5" type="+-">Oerelof liefie peinst toch eens om <rhyme label="c" type="m" corresp="#s4:l7">myn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
