<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>174030</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags>
      <tag>political song</tag>
      <tag>patriotic song</tag>
    </tags>
    <melody>Dans quel trouble te plonge</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Moest uw moedwil zo ver <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gebaate <rhyme label="b" type="m">Dwinglandy</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Dat ge 't edel bloed deedt <rhyme label="a" type="f" corresp="#s1:l1">stroomen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van Neêrlands <rhyme label="b" type="m" corresp="#s1:l2">Heldenry</rhyme>! ---</l>
        <l id="s1:l5" met="4" type="+-">Schandlyk Monster, hooft der <rhyme label="c" type="f">plaagen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Van het Menschelyk <rhyme label="d" type="m">geslacht</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dat geen Keetenen wil <rhyme label="c" type="f" corresp="#s1:l5">dragen</rhyme>!</l>
        <l id="s1:l8" met="4" type="+-">Thans wordt gy nog meer <rhyme label="d" type="m" corresp="#s1:l6">veracht</rhyme>.</l>
        <l id="s1:l9" met="4" type="+-">Door 't pleegen, door 't pleegen, van zulke <rhyme label="e" type="f">daaden</rhyme>;</l>
        <l id="s1:l10" met="4" type="-+">Door 't pleegen, door 't pleegen, van zulke <rhyme label="e" type="f" corresp="#s1:l9">daaden</rhyme>;</l>
        <l id="s1:l11" met="2" type="-+">Helaas! wat <rhyme label="f" type="m">smart</rhyme>!</l>
        <l id="s1:l12" met="3" type="-+">Voor 't teergevoelig <rhyme label="f" type="m" corresp="#s1:l11">hart</rhyme>!</l>
        <l id="s1:l13" met="3" type="-+">Voor 't teergevoelig <rhyme label="f" type="m" corresp="#s1:l11">hart</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">En schoon m' uwe vuige <rhyme label="a" type="f">Knegten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Doot 't allerlafst <rhyme label="b" type="m">verraad</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Bato's Nakroost zag <rhyme label="a" type="f" corresp="#s2:l1">bevechten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat voor 's Lands vryheid <rhyme label="b" type="m" corresp="#s2:l2">staat</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Gy, Gy vielt, met al uw <rhyme label="c" type="f">slaaven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Voor het bliksemend <rhyme label="d" type="m">geweer</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Der zo moedige <rhyme label="c" type="f" corresp="#s2:l5">Bataaven</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">In 't schandlyk stof ter <rhyme label="d" type="m" corresp="#s2:l6">neêr</rhyme>. ---</l>
        <l id="s2:l9" met="4" type="+-">Uw snoodheid, uw snoodheid wreedaartig <rhyme label="e" type="f">Monster</rhyme>!</l>
        <l id="s2:l10" met="4" type="-+">Uw snoodheid, uw snoodheid wreedaartig <rhyme label="e" type="f" corresp="#s2:l9">Monster</rhyme>!</l>
        <l id="s2:l11" met="2" type="-+">Ziet nooit de <rhyme label="f" type="m">magt</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Van Bato's Kroost <rhyme label="f" type="m" corresp="#s2:l11">verkracht</rhyme>!</l>
        <l id="s2:l13" met="3" type="-+">Van Bato's Kroost <rhyme label="f" type="m" corresp="#s2:l11">verkracht</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Nimmer zult ge uw wil <rhyme label="a" type="f">bekomen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Afschuwelyk <rhyme label="b" type="m">Gedrocht</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Hoe veel bloeds gy ook deedt <rhyme label="a" type="f" corresp="#s3:l1">stroomen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wat rampen gy ook <rhyme label="b" type="m" corresp="#s3:l2">wrocht</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Aan 't puik der <rhyme label="c" type="f">Nederlanders</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Zy, ô helsche <rhyme label="d" type="m">Dwinglandy</rhyme>!</l>
        <l id="s3:l7" met="4" type="+-">Blyven eeuwig <rhyme label="c" type="f" corresp="#s3:l5">tegenstanders</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Van uw dolle <rhyme label="d" type="m" corresp="#s3:l6">Heerschappy</rhyme>. ---</l>
        <l id="s3:l9" met="4" type="+-">'s Lands vryheid, 's Lands vryheid is by hen <rhyme label="e" type="f">veilig</rhyme>!</l>
        <l id="s3:l10" met="4" type="-+">'s Lands vryheid, 's Lands vryheid is by hen <rhyme label="e" type="f" corresp="#s3:l9">veilig</rhyme>!</l>
        <l id="s3:l11" met="2" type="-+">Hoe fel, hoe <rhyme label="f" type="m">wreed</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Gy haar altyd <rhyme label="f" type="m" corresp="#s3:l11">bestreedt</rhyme>!</l>
        <l id="s3:l13" met="3" type="-+">Gy haar altyd <rhyme label="f" type="m" corresp="#s3:l11">bestreedt</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laat des al uw schandlyk <rhyme label="a" type="f">poogen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Uw bloeddorst na, --- o <rhyme label="b" type="m">Pest</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Gy, die zonder <rhyme label="a" type="f" corresp="#s4:l1">mededoogen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Op 't Nederlands <rhyme label="b" type="m" corresp="#s4:l2">Gewest</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Steeds blyft waaden, zult niet <rhyme label="c" type="f">slagen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">In deez' vry gevochten <rhyme label="d" type="m">staat</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Schoon ge u vaak ziet <rhyme label="c" type="f" corresp="#s4:l5">onderschraagen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Door het schandelykst <rhyme label="d" type="m" corresp="#s4:l6">verraad</rhyme>! ---</l>
        <l id="s4:l9" met="4" type="+-">Want fierheid en kloekheid der <rhyme label="e" type="f">Batavieren</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Want fierheid en kloekheid der <rhyme label="e" type="f" corresp="#s4:l9">Batavieren</rhyme>,</l>
        <l id="s4:l11" met="2" type="-+">Staan trouw en <rhyme label="f" type="m">pal</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">In 't allerwreedst <rhyme label="f" type="m" corresp="#s4:l11">geval</rhyme>!</l>
        <l id="s4:l13" met="3" type="-+">In 't allerwreedst <rhyme label="f" type="m" corresp="#s4:l11">geval</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
