<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185290</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Edele kersouw</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Zyon <rhyme label="a" type="m">Bruydt</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Ghy mijn Vriendinn <rhyme label="b" type="f">schoone</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Siet deur u oogen <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Mijn hert <rhyme label="a" type="m" corresp="#s1:l1">ontsluyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Tot u Liefd’ ick <rhyme label="b" type="f" corresp="#s1:l2">betoone</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Als een moeder tot <rhyme label="c" type="m" corresp="#s1:l3">haer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Kint, soo is <rhyme label="c" type="m" corresp="#s1:l3">voorwaer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Mijn hert ‘t uwaerts <rhyme label="d" type="f">gheresen</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Dus vriendin // en laet niet <rhyme label="e" type="m">min</rhyme></l>
        <l id="s1:l10" met="3" type="+-">V Liefde tot my <rhyme label="d" type="f" corresp="#s1:l8">wesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wt Liefde <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Liet ick my <rhyme label="b" type="f">overgeven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En betaelde u <rhyme label="c" type="m">schult</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Met mijn bloet <rhyme label="a" type="m" corresp="#s2:l1">root</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Op dat ghy soudet <rhyme label="b" type="f" corresp="#s2:l2">leven</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Had ick met u <rhyme label="c" type="m" corresp="#s2:l3">ghedult</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als een minnaer <rhyme label="c" type="m" corresp="#s2:l3">vervult</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Met Liefde <rhyme label="d" type="f">uytgelesen</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Dus vriendin // en laet niet <rhyme label="e" type="m">min</rhyme></l>
        <l id="s2:l10" met="3" type="+-">V Liefde tot my <rhyme label="d" type="f" corresp="#s2:l8">wesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ick liet mijn <rhyme label="a" type="m">rijck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Om u van uwe <rhyme label="b" type="f">sonden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Te wasschen claer en <rhyme label="c" type="m">bloot</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Werd’ ick <rhyme label="a" type="m" corresp="#s3:l1">ghelijck</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Als een mensche <rhyme label="b" type="f" corresp="#s3:l2">bevonden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Hier op des werelts <rhyme label="c" type="m" corresp="#s3:l3">Cloot</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Om u als ghy laeght <rhyme label="c" type="m" corresp="#s3:l3">doot</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wederom te <rhyme label="d" type="f">ghenesen</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Dus vriendin // en laet niet <rhyme label="e" type="m">min</rhyme></l>
        <l id="s3:l10" met="3" type="+-">V Liefde tot my <rhyme label="d" type="f" corresp="#s3:l8">wesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Als ick hier <rhyme label="a" type="m">quam</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Niet om te <rhyme label="b" type="f">Gouverneeren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Naer mijn <rhyme label="c" type="m">groot-mogentheyt</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Maer als een <rhyme label="a" type="m" corresp="#s4:l1">Lam</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Aldus wilt van my <rhyme label="b" type="f" corresp="#s4:l2">leeren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Soo ick hebbe <rhyme label="c" type="m" corresp="#s4:l3">geseyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat ghy een leven <rhyme label="c" type="m" corresp="#s4:l3">leyt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Seer ootmoedigh <rhyme label="d" type="f">ghepresen</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Dus vriendin // en laet niet <rhyme label="e" type="m">min</rhyme></l>
        <l id="s4:l10" met="3" type="+-">V Liefde tot my <rhyme label="d" type="f" corresp="#s4:l8">wesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Veel spot en <rhyme label="a" type="m">smaet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Heb ick om u <rhyme label="b" type="f">gheleden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dit in u herte <rhyme label="c" type="m">draeght</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Hoe ick <rhyme label="a" type="m" corresp="#s5:l1">ghehaet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Deur anghst besweet, <rhyme label="b" type="f" corresp="#s5:l2">ghetreden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">De perss’, o suyver <rhyme label="c" type="m" corresp="#s5:l3">Maeght</rhyme>!</l>
        <l id="s5:l7" met="3" type="-+">My selven niet <rhyme label="c" type="m" corresp="#s5:l3">behaeght</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Wanneer ick was <rhyme label="d" type="f">verknesen</rhyme>,</l>
        <l id="s5:l9" met="4" type="-+">Dus vriendin // en laet niet <rhyme label="e" type="m">min</rhyme></l>
        <l id="s5:l10" met="3" type="+-">V Liefde tot my <rhyme label="d" type="f" corresp="#s5:l8">wesen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Princesse <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ick heb u <rhyme label="b" type="f">uytvercoren</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Doet altijt na mijn <rhyme label="c" type="m">woort</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Mijn rijck is <rhyme label="a" type="m" corresp="#s6:l1">dijn</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Als noch staende te <rhyme label="b" type="f" corresp="#s6:l2">voren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Al waermen met <rhyme label="c" type="m" corresp="#s6:l3">accoort</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Eeuwighe vreucht <rhyme label="c" type="m" corresp="#s6:l3">orboort</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Mijn Liefde doet u <rhyme label="d" type="f">desen</rhyme>,</l>
        <l id="s6:l9" met="4" type="-+">Dus vriendin // en laet niet <rhyme label="e" type="m">min</rhyme></l>
        <l id="s6:l10" met="3" type="+-">V Liefde tot my <rhyme label="d" type="f" corresp="#s6:l8">wesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
