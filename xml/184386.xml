      <year-start>1785</year-start>
      <year-end>1785</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zijn kost te kunnen winnen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat stond ik daar te <rhyme label="a" type="f">praaten</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik mogt het toch wel <rhyme label="a" type="f" corresp="#s1:l1">laaten</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">'t Is, of ook niets kan <rhyme label="a" type="f" corresp="#s1:l1">baaten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maar is dat wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Ik snapte met die <rhyme label="c" type="f">wyven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wat andre luî <rhyme label="c" type="f" corresp="#s1:l5">bedryven</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En van myn Juffrouws <rhyme label="c" type="f" corresp="#s1:l5">kyven</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">ô Neen! 't moet anders <rhyme label="b" type="m" corresp="#s1:l4">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zie wel myn <rhyme label="a" type="f">gebreken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En hou toch de oude <rhyme label="a" type="f" corresp="#s2:l1">streeken</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En blyf ook <rhyme label="a" type="f" corresp="#s2:l1">tegenspreeken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maar is dat wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">'k Heb lest een boek <rhyme label="c" type="f">geleezen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hoe of een meid moet <rhyme label="c" type="f" corresp="#s2:l5">weezen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">'k Begon toen zo te <rhyme label="c" type="f" corresp="#s2:l5">vreezen</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">ô Neen! 't moet anders <rhyme label="b" type="m" corresp="#s2:l4">gaan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Foei 'k voel myn wezen <rhyme label="a" type="f">gloeyen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'k Zal met myn werk my <rhyme label="a" type="f" corresp="#s3:l1">moeyen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">My na myn Juffrouw <rhyme label="a" type="f" corresp="#s3:l1">spoeyen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat 's dunkt my wel <rhyme label="b" type="m">gedaan</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">En voor dit myn <rhyme label="c" type="f">gedraagen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Aan haar excuus gaan <rhyme label="c" type="f" corresp="#s3:l5">vraagen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dan zal 'k haar wel <rhyme label="c" type="f" corresp="#s3:l5">behaagen</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">o Jaa! zo moet het <rhyme label="b" type="m" corresp="#s3:l4">gaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
