    <date>
      <year-start>1611</year-start>
      <year-end>1611</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Het vinnige stralen van de zon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt vinnich stralen van de <rhyme label="a" type="m">Son</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ontschuyl ick in't <rhyme label="b" type="f">bosschagie</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Indien dit bosjen klappen <rhyme label="a" type="m" corresp="#s1:l1">kon</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wat melde't al <rhyme label="b" type="f" corresp="#s1:l2">vryagie</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vryagie? neen. Vryagie? <rhyme label="a" type="m">jae</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Vryagie sonder <rhyme label="b" type="f">menen</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Van hondert Harders (ist niet <rhyme label="a" type="m" corresp="#s2:l1">scha</rhyme>?)</l>
        <l id="s2:l4" met="3" type="-+">Vindtm'er ghetrou niet <rhyme label="b" type="f" corresp="#s2:l2">eenen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een wullepsch knaepjen altijt <rhyme label="a" type="m">stuyrt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Nae nieuwe lust zijn <rhyme label="b" type="f">sinnen</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Niet langer als het weygeren <rhyme label="a" type="m" corresp="#s3:l1">duyrt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Niet langher duyrt het <rhyme label="b" type="f" corresp="#s3:l2">minnen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Mijn hartje treckt my wel soo <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Soo seer dorst ick het <rhyme label="b" type="f">waghen</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Maer neen ick waegh het <rhyme label="a" type="m" corresp="#s4:l1">nemmermeer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Haer minnen zijn maer <rhyme label="b" type="f" corresp="#s4:l2">vlaghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer vlaghen die t'hans <rhyme label="a" type="m">overgaen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En op een ander <rhyme label="b" type="f">vallen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Nochtans ick sie mijn Vryer <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Voor trouste van haer <rhyme label="b" type="f" corresp="#s5:l2">allen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maer oft u miste domme <rhyme label="a" type="m">maecht</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ghy siet hem niet van <rhyme label="b" type="f">binnen</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Dan't schijnt wel, die gheen rust en <rhyme label="a" type="m" corresp="#s6:l1">waecht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Kan qualijck lust <rhyme label="b" type="f" corresp="#s6:l2">ghewinnen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Of ick hem oock lichtveerdich <rhyme label="a" type="m">von</rhyme></l>
        <l id="s7:l2" met="3" type="-+">En't bleef in dit <rhyme label="b" type="f">bosschagie</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Indien dit bosje klappen <rhyme label="a" type="m" corresp="#s7:l1">kon</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Wat melden't al <rhyme label="b" type="f" corresp="#s7:l2">boelagie</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
