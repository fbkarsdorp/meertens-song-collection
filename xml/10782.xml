<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>10782</id>
    <date>
      <year-start>1618</year-start>
      <year-end>1618</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody>Ik klaag u Venus' dieren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WIlt u met my <rhyme label="a" type="f">bekeeren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Weereltsche dienaers <rhyme label="b" type="m">coen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Tot Godt den Heer der <rhyme label="a" type="f" corresp="#s1:l1">Heeren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Soo ick begheer te <rhyme label="b" type="m" corresp="#s1:l2">doen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Al ben ick jonck en <rhyme label="b" type="m" corresp="#s1:l2">coen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met veel zonden <rhyme label="c" type="f">beladen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Nochtans wil ick my <rhyme label="b" type="m" corresp="#s1:l2">spoen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">De weerelt te <rhyme label="c" type="f" corresp="#s1:l6">versmaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Met den verlooren <rhyme label="a" type="f">soone</rhyme></l>
        <l id="s2:l2" met="3" type="-+">So sal ick nu op <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met vast betrouwen <rhyme label="a" type="f" corresp="#s2:l1">schoone</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Tot Godt mijn Vader <rhyme label="b" type="m" corresp="#s2:l2">gaen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Hy sal my wel <rhyme label="b" type="m" corresp="#s2:l2">ontfaen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Door sijn groote <rhyme label="c" type="f">ghenaden</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Want ick wil nu voort <rhyme label="b" type="m" corresp="#s2:l2">aen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Die weerelt gantsch <rhyme label="c" type="f" corresp="#s2:l6">versmaden</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick socht des weerelts <rhyme label="a" type="f">weelden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En haer gheneuchte <rhyme label="b" type="m">pleyn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ick dansten en ick <rhyme label="a" type="f" corresp="#s3:l1">speelden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In ydelheyt <rhyme label="b" type="m" corresp="#s3:l2">onreyn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Ick wandelden <rhyme label="b" type="m" corresp="#s3:l2">ghemeyn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En hare brede <rhyme label="c" type="f">paden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maer nu wil ick <rhyme label="b" type="m" corresp="#s3:l2">certeyn</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Die weerelt gantsch <rhyme label="c" type="f" corresp="#s3:l6">versmaden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hooveerdigh ick <rhyme label="a" type="f">vercieren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Mijn lichaem <rhyme label="b" type="m">delicaet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Daer men ydelheyt <rhyme label="a" type="f" corresp="#s4:l1">hantierden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Daer was ick vroegh en <rhyme label="b" type="m" corresp="#s4:l2">laet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Tot alle zonden <rhyme label="b" type="m" corresp="#s4:l2">quaet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wert ick als doen <rhyme label="c" type="f">gheraden</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Nu wil ick metter <rhyme label="b" type="m" corresp="#s4:l2">daet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Die weerelt gantsch <rhyme label="c" type="f" corresp="#s4:l6">versmaden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Doen ick so <rhyme label="a" type="f">triumpheerden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In alle <rhyme label="b" type="m">ydelheyt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Die weerelt my <rhyme label="a" type="f" corresp="#s5:l1">laudeerden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Mijn eere wert <rhyme label="b" type="m" corresp="#s5:l2">verbreyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Also wert ick <rhyme label="b" type="m" corresp="#s5:l2">verleyt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Door haer soete <rhyme label="c" type="f">sucaden</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Maer nu ben ick <rhyme label="b" type="m" corresp="#s5:l2">bereyt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Die weerelt te <rhyme label="c" type="f" corresp="#s5:l6">versmaden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Haer lof sal ick <rhyme label="a" type="f">afsnijden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En versaken haer <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Haer vreucht aen allen <rhyme label="a" type="f" corresp="#s6:l1">zijden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En acht ick nu niet <rhyme label="b" type="m" corresp="#s6:l2">meer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Want ick soeck Godt den <rhyme label="b" type="m" corresp="#s6:l2">Heer</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die my verlost van <rhyme label="c" type="f">quaden</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">t'Is mijn begheerte <rhyme label="b" type="m" corresp="#s6:l2">seer</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Die weerelt te <rhyme label="c" type="f" corresp="#s6:l6">versmaden</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die werelt heeft mijn <rhyme label="a" type="f">bedrogen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Door haer wellusten <rhyme label="b" type="m">sacht</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Mijn hert van Godt <rhyme label="a" type="f" corresp="#s7:l1">ghetoghen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">By daghe en by <rhyme label="b" type="m" corresp="#s7:l2">nacht</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Een Gheest wert ick <rhyme label="b" type="m" corresp="#s7:l2">gheacht</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Seer groot tot mijnder <rhyme label="c" type="f">schaden</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Maer nu heb ick <rhyme label="b" type="m" corresp="#s7:l2">ghedacht</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Die weerelt te <rhyme label="c" type="f" corresp="#s7:l6">versmaden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick heb meest al mijn <rhyme label="a" type="f">daghen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ghedient de weerelt <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat sal ick nu <rhyme label="a" type="f" corresp="#s8:l1">beclaghen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Beschreyen met <rhyme label="b" type="m" corresp="#s8:l2">ootmoet</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Die penitentie <rhyme label="b" type="m" corresp="#s8:l2">doet</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Godt geeft hem sijn <rhyme label="c" type="f">weldaden</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Dus wil ick met der <rhyme label="b" type="m" corresp="#s8:l2">spoet</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Die weerelt gantsch <rhyme label="c" type="f" corresp="#s8:l6">versmaden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick wil my gaen <rhyme label="a" type="f">begheven</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Tot alle deughde <rhyme label="b" type="m">claer</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Veranderen mijn <rhyme label="a" type="f" corresp="#s9:l1">leven</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Gheestigh ende <rhyme label="b" type="m" corresp="#s9:l2">eerbaer</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Heer wilt mijn helpen <rhyme label="b" type="m" corresp="#s9:l2">daer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Al op der deughden <rhyme label="c" type="f">graden</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Want ick wil nu <rhyme label="b" type="m" corresp="#s9:l2">voorwaer</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Die weerelt gantsch <rhyme label="c" type="f" corresp="#s9:l6">versmaden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Den Prins sal ick <rhyme label="a" type="f">versaken</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Van dese weerelt <rhyme label="b" type="m">ront</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Want hy moet eeuwigh <rhyme label="a" type="f" corresp="#s10:l1">blaken</rhyme></l>
        <l id="s10:l4" met="3" type="-+">In der hellen <rhyme label="b" type="m" corresp="#s10:l2">afgront</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Met Godt maeck ick <rhyme label="b" type="m" corresp="#s10:l2">verbont</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Die mijn ziel can <rhyme label="c" type="f">versaken</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Als dat ick nu <rhyme label="b" type="m" corresp="#s10:l2">terstont</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Die Weerelt sal <rhyme label="c" type="f" corresp="#s10:l6">versmaden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
