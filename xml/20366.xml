<song>
  <meta>
    <id>20366</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Quam Mariam dulcibus</melody>
  <author>B. Targier, Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">ACh wat <rhyme label="a" type="f">plagen</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Ontrouw wort nu hoogh <rhyme label="b" type="m">ghekroont</rhyme>.</l>
        <l id="s1:l3" met="2" type="+-">'k Moet <rhyme label="a" type="f" corresp="#s1:l1">beklagen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat men goedt met quaedt <rhyme label="b" type="m" corresp="#s1:l2">beloont</rhyme>:</l>
        <l id="s1:l5" met="2" type="+-">Want de <rhyme label="c" type="f">loosheydt</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Van het wispeltuerigh <rhyme label="d" type="m">breyn</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">En haer <rhyme label="c" type="f" corresp="#s1:l5">boosheydt</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Is nu leyder seer <rhyme label="d" type="m" corresp="#s1:l6">ghemeyn</rhyme>.</l>
        <l id="s1:l9" met="7" type="+-">Meest der Vrouwen sinnen Voor ons ongebrijp'lijck <rhyme label="e" type="m">zijn</rhyme>:</l>
        <l id="s1:l10" met="2" type="+-">Dees sal <rhyme label="f" type="f">smeken</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Die haer <rhyme label="f" type="f" corresp="#s1:l10">spreken</rhyme>;</l>
        <l id="s1:l12" met="4" type="+-">Elck koockt ons een boos <rhyme label="e" type="m" corresp="#s1:l9">fenijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Dees seydt <rhyme label="a" type="f">Hartje</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Daer mijn lust en sin nae <rhyme label="b" type="m">tracht</rhyme>;</l>
        <l id="s2:l3" met="2" type="+-">Agh men <rhyme label="a" type="f" corresp="#s2:l1">smartje</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'k Heb soo langh op jou <rhyme label="b" type="m" corresp="#s2:l2">ghewacht</rhyme>:</l>
        <l id="s2:l5" met="2" type="+-">Mijn <rhyme label="c" type="f">beminde</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Saeghje my eens in de <rhyme label="d" type="m">grondt</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Jy soud' <rhyme label="c" type="f" corresp="#s2:l5">vinden</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Datje al gheschreven <rhyme label="d" type="m" corresp="#s2:l6">stondt</rhyme></l>
        <l id="s2:l9" met="7" type="+-">In mijn Hart en Nieren: soume niet wel meenen <rhyme label="e" type="m">recht</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">Maer haer <rhyme label="f" type="f">wercken</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Doen <rhyme label="f" type="f" corresp="#s2:l10">bemercken</rhyme></l>
        <l id="s2:l12" met="4" type="+-">Datse spotten met de <rhyme label="e" type="m" corresp="#s2:l9">Knecht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Die sal <rhyme label="a" type="f">toonen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Wrevel-moedt, en stuersheydt <rhyme label="b" type="m">quaedt</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s3:l1">beloonen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Reyne Liefd' met bitt're <rhyme label="b" type="m" corresp="#s3:l2">haet</rhyme>:</l>
        <l id="s3:l5" met="2" type="+-">Voor wel <rhyme label="c" type="f">spreecken</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Groeten met een nors <rhyme label="d" type="m">ghekijf</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Voor soet <rhyme label="c" type="f" corresp="#s3:l5">smeecken</rhyme></l>
        <l id="s3:l8" met="4" type="+-">V schier vallen op het <rhyme label="d" type="m" corresp="#s3:l6">lijf</rhyme>:</l>
        <l id="s3:l9" met="7" type="+-">'t Waer noch al te lyen nam't in 't eynde maer een <rhyme label="e" type="m">keer</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Maer met <rhyme label="f" type="f">klappen</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">En <rhyme label="f" type="f" corresp="#s3:l10">besnappen</rhyme></l>
        <l id="s3:l12" met="4" type="+-">Nemensy dan noch u <rhyme label="e" type="m" corresp="#s3:l9">eer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">'k Sou wel <rhyme label="a" type="f">wenschen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dit vermomde <rhyme label="b" type="m">onghediert</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Dat als <rhyme label="a" type="f" corresp="#s4:l1">Menschen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Soo in onse ooghen <rhyme label="b" type="m" corresp="#s4:l2">swierdt</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Hare <rhyme label="c" type="f">mommen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Van haer aensicht wierdt <rhyme label="d" type="m">ghedaen</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Soo sou <rhyme label="c" type="f" corresp="#s4:l5">kommen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Dat men eerst mocht veylligh <rhyme label="d" type="m" corresp="#s4:l6">gaen</rhyme>,</l>
        <l id="s4:l9" met="7" type="+-">Sonder eenigh vresen van bedrogh of <rhyme label="e" type="m">klapperny</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">Daer och <rhyme label="f" type="f">armen</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Droevich <rhyme label="f" type="f" corresp="#s4:l10">karmen</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Die nu legghen in de <rhyme label="e" type="m" corresp="#s4:l9">ly</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">'k Wil noch <rhyme label="a" type="f">rommen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Eerbaer Maeghden, schoons' haer <rhyme label="b" type="m">sin</rhyme></l>
        <l id="s5:l3" met="2" type="+-">Wat <rhyme label="a" type="f" corresp="#s5:l1">verblommen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Want dat voeght wel by de <rhyme label="b" type="m" corresp="#s5:l2">Min</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">En het <rhyme label="c" type="f">vryen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Wordt door Jae en Neen <rhyme label="d" type="m">ghevoedt</rhyme>:</l>
        <l id="s5:l7" met="2" type="+-">'t Is te <rhyme label="c" type="f" corresp="#s5:l5">lyen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">In een toe-gheneyght <rhyme label="d" type="m" corresp="#s5:l6">ghemoedt</rhyme>.</l>
        <l id="s5:l9" met="7" type="+-">Dus verliefde Minnaers, houdt dit voor een <rhyme label="e" type="m">regel-maet</rhyme>;</l>
        <l id="s5:l10" met="2" type="+-">Wilje <rhyme label="f" type="f">Vryen</rhyme></l>
        <l id="s5:l11" met="2" type="+-">En <rhyme label="f" type="f" corresp="#s5:l10">bedyen</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">Aght gheen Dochters eerste <rhyme label="e" type="m" corresp="#s5:l9">praet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
