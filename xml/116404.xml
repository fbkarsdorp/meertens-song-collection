<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>116404</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>alkmaar</orginal-name>
      <standard-name>Alkmaar, The Netherlands</standard-name>
      <coordinates>
        <lon>52.6312592</lon>
        <lat>4.7503798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Est-ce Mars</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALder-grootste God en <rhyme label="a" type="f">Kooning</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Van ons <rhyme label="b" type="m">all'</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die den Heemel hebt tot <rhyme label="a" type="f" corresp="#s1:l1">wooning</rhyme></l>
        <l id="s1:l4" met="2" type="+-">En dit <rhyme label="b" type="m" corresp="#s1:l2">dal</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Tot <rhyme label="c" type="m">voet-bank</rhyme>;</l>
        <l id="s1:l6" met="2" type="+-">U zy <rhyme label="c" type="m" corresp="#s1:l5">dank</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Voor uw <rhyme label="d" type="m">weldaadigheid</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">In <rhyme label="d" type="m" corresp="#s1:l7">eeuwigheid</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Tot U is mijn ernstig <rhyme label="a" type="f">smeeken</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Goede <rhyme label="b" type="m">God</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Laat uw gunst my niet <rhyme label="a" type="f" corresp="#s2:l1">ontbreeken</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Want ik <rhyme label="b" type="m" corresp="#s2:l2">tot</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Deugd my <rhyme label="c" type="m">wend</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">En het <rhyme label="c" type="m" corresp="#s2:l5">end</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Der vroomen heb op 't <rhyme label="d" type="m">oog</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En daar naa <rhyme label="d" type="m" corresp="#s2:l7">poog'</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">maar op op die smalle <rhyme label="a" type="f">weegen</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Wel te <rhyme label="b" type="m">gaan</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Moet voor al uw milde <rhyme label="a" type="f" corresp="#s3:l1">zeegen</rhyme></l>
        <l id="s3:l4" met="2" type="+-">My by <rhyme label="b" type="m" corresp="#s3:l2">staan</rhyme>;</l>
        <l id="s3:l5" met="2" type="+-">Want ik <rhyme label="c" type="m">mag</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Niet een <rhyme label="c" type="m" corresp="#s3:l5">dag</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Bestaan, in dien gy <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s3:l8" met="2" type="-+">My gunste <rhyme label="d" type="m" corresp="#s3:l7">bied</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zie ganaadig aan mijn <rhyme label="a" type="f">jonkheid</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">En mijn <rhyme label="b" type="m">stand</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">'k Voel een vonkje, Heer! ontfonk gy <rhyme label="a" type="f" corresp="#s4:l1">'t</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Dat het <rhyme label="b" type="m" corresp="#s4:l2">brand</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Door uw <rhyme label="c" type="m">gloed</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Die gy <rhyme label="c" type="m" corresp="#s4:l5">voed</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">In yeder die U <rhyme label="d" type="m">vreezd</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Op-recht van <rhyme label="d" type="m" corresp="#s4:l7">Geest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wild mijn tong en pen <rhyme label="a" type="f">betoomen</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Datz' het <rhyme label="b" type="m">quaad</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daar men door ten val kan <rhyme label="a" type="f" corresp="#s5:l1">koomen</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Al-tijd <rhyme label="b" type="m" corresp="#s5:l2">haat</rhyme>:</l>
        <l id="s5:l5" met="2" type="+-">Datz', o <rhyme label="c" type="m">Heer</rhyme>!</l>
        <l id="s5:l6" met="2" type="+-">Al-tijd <rhyme label="c" type="m" corresp="#s5:l5">leer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Het geen uw wet <rhyme label="d" type="m">gebied</rhyme>;</l>
        <l id="s5:l8" met="2" type="-+">En anders <rhyme label="d" type="m" corresp="#s5:l7">niet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hoor my toch, genaadig' <rhyme label="a" type="f">Heere</rhyme>!</l>
        <l id="s6:l2" met="2" type="+-">Laat uw <rhyme label="b" type="m">Geest</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">My geleiden, en <rhyme label="a" type="f" corresp="#s6:l1">regeeren</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Dat ik <rhyme label="b" type="m" corresp="#s6:l2">meest</rhyme></l>
        <l id="s6:l5" met="2" type="+-">Naa het <rhyme label="c" type="m">goed</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">'t Welk men <rhyme label="c" type="m" corresp="#s6:l5">moet</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Naa jaagen, yv'rig <rhyme label="d" type="m">jaag</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">En noit <rhyme label="d" type="m" corresp="#s6:l7">verzaag</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
