<song>
  <meta>
    <id>100095</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Verdwaalde koningin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EY bracke wilde <rhyme label="a" type="f">stroomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Stilt nu j'onst <rhyme label="b" type="m">uymicheyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Vermits mijn Lief nu <rhyme label="a" type="f" corresp="#s1:l1">ontnomen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En vast gekluystert <rhyme label="b" type="m" corresp="#s1:l2">leyt</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Mijn levens lust, en heeft geen <rhyme label="c" type="m">rust</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En 'k moet gantsch quijnent <rhyme label="d" type="f">sterven</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Want Ziel en Zin // woont by'er <rhyme label="e" type="m">in</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Hoe kan ick haer dan <rhyme label="d" type="f" corresp="#s1:l6">derven</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dies spring ick inde <rhyme label="a" type="f">Baren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">'k En vrees geen Zee <rhyme label="b" type="m">gedrocht</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Of Garon schoon quam <rhyme label="a" type="f" corresp="#s2:l1">varen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En my te vangen <rhyme label="b" type="m" corresp="#s2:l2">socht</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">My [...]isse Leen:/ hebben de <rhyme label="c" type="m">Zee'n</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Soo vaeck de Doot <rhyme label="d" type="f">ontzwommen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Ha wat een soet // my Geest <rhyme label="e" type="m">ontmoet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Als wy by d'ander <rhyme label="d" type="f" corresp="#s2:l6">kommen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O! Goon wat krampt mijn <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ach Hero lief ik <rhyme label="b" type="m">smoor</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ick schey en daal <rhyme label="a" type="f" corresp="#s3:l1">beneden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daer 'k noyt u soetheyt <rhyme label="b" type="m" corresp="#s3:l2">hoor</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ey gaet aen 's strant // daer ghy in' aen 't <rhyme label="c" type="m">Lant</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ganis leven-loos sult <rhyme label="d" type="f">vinden</rhyme></l>
        <l id="s3:l7" met="4" type="-+">En hout aldaer // in een <rhyme label="e" type="m">Pylaer</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Dees sterft voor Zijn <rhyme label="d" type="f" corresp="#s3:l6">Beminde</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
