  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Laet ons nu met vreughde <rhyme label="a" type="f">rapen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ter eeren Godes <rhyme label="b" type="m">Woort</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Om dat de Roomsche <rhyme label="a" type="f" corresp="#s1:l1">Papen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Uyt Wesel sijn <rhyme label="b" type="m" corresp="#s1:l2">verstoort</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Door dese vrome <rhyme label="c" type="f">Helden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En Bataviers <rhyme label="d" type="m">ghetrouw</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die men hun Lof moet <rhyme label="c" type="f" corresp="#s1:l5">melden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ter eeren van <rhyme label="d" type="m" corresp="#s1:l6">Nassouw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ons Prince van <rhyme label="a" type="f">Orangien</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Als een Heldt <rhyme label="b" type="m">onvertsaeght</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Teghen het wreet <rhyme label="a" type="f" corresp="#s2:l1">Hispaengien</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Aentast die Bossche <rhyme label="b" type="m" corresp="#s2:l2">Maeght</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Een Peerle aen de <rhyme label="c" type="f">Kroone</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van al het Roomsche <rhyme label="d" type="m">rijck</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Geen krachtigh werck soo <rhyme label="c" type="f" corresp="#s2:l5">schoone</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En vont men <rhyme label="d" type="m" corresp="#s2:l6">diesgelijck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En sijn soo sterck <rhyme label="a" type="f">beleghen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Te water en te <rhyme label="b" type="m">Landt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Den goeden Godt te <rhyme label="a" type="f" corresp="#s3:l1">deghen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Biedt daegh'lijcks ons de <rhyme label="b" type="m" corresp="#s3:l2">handt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Als men door veel <rhyme label="c" type="f">op-merckens</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Kan singhen <rhyme label="d" type="m">overluyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat al de wilde <rhyme label="c" type="f" corresp="#s3:l5">Verckens</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Ten Bos haest moeten <rhyme label="d" type="m" corresp="#s3:l6">uyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al de wreede <rhyme label="a" type="f">Soldaten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van Keysers Konincx <rhyme label="b" type="m">pracht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met Heydense <rhyme label="a" type="f" corresp="#s4:l1">Krabaten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Sijn hier in 't Lant <rhyme label="b" type="m" corresp="#s4:l2">gebracht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Om rooven, moorden, <rhyme label="c" type="f">branden</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">Doch die Verraders <rhyme label="d" type="m">kloeck</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Vluchten met groote <rhyme label="c" type="f" corresp="#s4:l5">schanden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Een yder om  een <rhyme label="d" type="m" corresp="#s4:l6">hoeck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Soo schandelijck sy 't <rhyme label="a" type="f">maken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat het den Heer <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hun straf moet nu <rhyme label="a" type="f" corresp="#s5:l1">ghenaken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">'t Beginsel men doch <rhyme label="b" type="m" corresp="#s5:l2">siet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Het Weselken <rhyme label="c" type="f">verheven</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat Rat draeyt <rhyme label="d" type="m">ongefaelt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Kan hun geen kost meer <rhyme label="c" type="f" corresp="#s5:l5">geven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Men backt niet noch men <rhyme label="d" type="m" corresp="#s5:l6">maelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Lof sy de vrome <rhyme label="a" type="f">Helden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En Edel Heeren <rhyme label="b" type="m">trou</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die men hun lof moet <rhyme label="a" type="f" corresp="#s6:l1">melden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ter eeren 't Huys <rhyme label="b" type="m" corresp="#s6:l2">Nassou</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Als Helden kloeck en <rhyme label="c" type="f">vromen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Tot vreught van 't <rhyme label="d" type="m">Vaderlant</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Js Wesel <rhyme label="c" type="f" corresp="#s6:l5">inghenomen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Gods Woort daer in <rhyme label="d" type="m" corresp="#s6:l6">geplant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Den vromen Heer van <rhyme label="a" type="f">Dijden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Heeft manlijck hem <rhyme label="b" type="m">vertoont</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Den Wolf seer kloeck int <rhyme label="a" type="f" corresp="#s7:l1">strijden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Gheen Vyant heeft <rhyme label="b" type="m" corresp="#s7:l2">verschoont</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">De deughtsame <rhyme label="c" type="f">Borgeryen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Sijn onbeschadicht <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Want men gingh hun <rhyme label="c" type="f" corresp="#s7:l5">bevrijen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Looft Godt in 't aertse <rhyme label="d" type="m" corresp="#s7:l6">dal</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Gheen heerlijcker <rhyme label="a" type="f">Victory</rhyme></l>
        <l id="s8:l2" met="3" type="-+">By menschen leven <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sal blijven in <rhyme label="a" type="f" corresp="#s8:l1">memory</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Als dees die nu <rhyme label="b" type="m" corresp="#s8:l2">gheschiet</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Dewijl sy in ons <rhyme label="c" type="f">Landen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Verrasschen wilden <rhyme label="d" type="m">saen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Hebben wy tot haer <rhyme label="c" type="f" corresp="#s8:l5">schanden</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Het Weselken <rhyme label="d" type="m" corresp="#s8:l6">gevaen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Het Tresoor van haer <rhyme label="a" type="f">eten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat sijn sy nu meest <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Veel Schatten <rhyme label="a" type="f" corresp="#s9:l1">ongemeten</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Orangien brenght <rhyme label="b" type="m" corresp="#s9:l2">profijt</rhyme></l>
        <l id="s9:l5" met="3" type="-+">En al de Heeren <rhyme label="c" type="f">Staten</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Ja Bataviers <rhyme label="d" type="m">konstant</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Die lijf en leven <rhyme label="c" type="f" corresp="#s9:l5">laten</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Voor 't vrome <rhyme label="d" type="m" corresp="#s9:l6">Vaderlandt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Heel en halve <rhyme label="a" type="f">kartouwen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">En ander stucken <rhyme label="b" type="m">meer</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Krijgen wy voor <rhyme label="a" type="f" corresp="#s10:l1">Nassouwen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Tot Godes lof en <rhyme label="b" type="m" corresp="#s10:l2">eer</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Met menighte van <rhyme label="c" type="f">Granen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Kruyt, Loot in <rhyme label="d" type="m">overvloet</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Paep Jan wast in u <rhyme label="c" type="f" corresp="#s10:l5">tranen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Ghy raeckt onder de <rhyme label="d" type="m" corresp="#s10:l6">voet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Het moet u rouwen <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Dat ghy in onse <rhyme label="b" type="m">Landt</rhyme></l>
        <l id="s11:l3" met="3" type="-+">De Veluw sijt <rhyme label="a" type="f" corresp="#s11:l1">getreden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Haes op, haes op, met <rhyme label="b" type="m" corresp="#s11:l2">schant</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Hoe kondy nu met <rhyme label="c" type="f">eeren</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Vluchten en sijn <rhyme label="d" type="m">gepaeyt</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Men sal u beter <rhyme label="c" type="f" corresp="#s11:l5">leeren</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Alst gat is <rhyme label="d" type="m" corresp="#s11:l6">toe-genaeyt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Den Bos moet nu oock <rhyme label="a" type="f">Geusen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Daer is geen aflaet <rhyme label="b" type="m">veur</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Gods Woort salse wel <rhyme label="a" type="f" corresp="#s12:l1">kneusen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Het staet recht voor de <rhyme label="b" type="m" corresp="#s12:l2">deur</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">De vuyle woeste <rhyme label="c" type="f">swijnen</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Moeten ruymen hun <rhyme label="d" type="m">schot</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Men sal daer haest in <rhyme label="c" type="f" corresp="#s12:l5">vijnen</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Schaepkens dat gun ons <rhyme label="d" type="m" corresp="#s12:l6">Godt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ons eygen <rhyme label="a" type="f">Jn-lantsaten</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Vluchten met groot <rhyme label="b" type="m">hert-seer</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Door duyvelse <rhyme label="a" type="f" corresp="#s13:l1">Crabaten</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Maer Godt segent ons <rhyme label="b" type="m" corresp="#s13:l2">weer</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Door dees heerlijcke <rhyme label="c" type="f">Victory</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Die ons is <rhyme label="d" type="m">toe-gebracht</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Tot een eeuwigh <rhyme label="c" type="f" corresp="#s13:l5">memory</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Door Godes arm vol <rhyme label="d" type="m" corresp="#s13:l6">kracht</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Met harpen en <rhyme label="a" type="f">tamboeren</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Verkondight Godes <rhyme label="b" type="m">naem</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Koop-lieden, Borgers en <rhyme label="a" type="f" corresp="#s14:l1">Boeren</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Looft danckt den Heer <rhyme label="b" type="m" corresp="#s14:l2">bequaem</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Die ons nu in dit <rhyme label="c" type="f">Leven</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Victory geeft <rhyme label="d" type="m">playsant</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">'s Hertogen-bos <rhyme label="c" type="f" corresp="#s14:l5">verheven</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Jn Prins en Staten <rhyme label="d" type="m" corresp="#s14:l6">hand</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Prinslijcke God <rhyme label="a" type="f">Almachtigh</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Die alleen hebt de <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Want uwen arm seer <rhyme label="a" type="f" corresp="#s15:l1">krachtigh</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Beschermt ons langhs soo <rhyme label="b" type="m" corresp="#s15:l2">meer</rhyme>;</l>
        <l id="s15:l5" met="3" type="-+">Heer wilt doch helpen <rhyme label="c" type="f">strijden</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Ons Josaphat <rhyme label="d" type="m">getrouw</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Als ghy deed' in <rhyme label="c" type="f" corresp="#s15:l5">voor-tijden</rhyme>:</l>
        <l id="s15:l8" met="3" type="-+">Bewaert 't Huys van <rhyme label="d" type="m" corresp="#s15:l6">Nassouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
