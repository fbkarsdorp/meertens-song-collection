<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182561</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Hier ligt mijn Damon in het graf</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Myn minnaar is een <rhyme label="a" type="m">jongeling</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Geschaapen tot den <rhyme label="b" type="m">stryd</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Zyn arm heeft aan 't <rhyme label="a" type="m" corresp="#s1:l1">Vaderland</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Zyn min en hart <rhyme label="c" type="m">gewyd</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De dag is daar, waar op myn <rhyme label="a" type="m">moed</rhyme></l>
        <l id="s2:l2" met="3" type="-+">word op den proef <rhyme label="b" type="m">gesteld</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Het Vaderland eist <rhyme label="a" type="m" corresp="#s2:l1">Eduward</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ten stryd in 't <rhyme label="c" type="m">Oorlogsveld</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De kreet ter stryd, ô <rhyme label="a" type="m">Jongeling</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">Roept zyne fierheid <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hy zweert: ik red u <rhyme label="a" type="m" corresp="#s3:l1">Vaderland</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Of ik wil met uw <rhyme label="c" type="m">vergaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Geen zugt die mynen Boesem <rhyme label="a" type="m">loost</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Verdooft myn minnaars <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Het Vaderland heeft <rhyme label="a" type="m" corresp="#s4:l1">Eduward</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met Leeuwen melk <rhyme label="c" type="m">gevoed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Niet stuit my in myn eerste <rhyme label="a" type="m">pligt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dus luid zyn fiere <rhyme label="b" type="m">taal</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Het Vaderland gebied myn <rhyme label="a" type="m" corresp="#s5:l1">arm</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Myn moed den <rhyme label="c" type="m">zegenpraal</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zyn Oorlogsdrift, zyn <rhyme label="a" type="m">heldenmoed</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zyn kragt, 't groeit alles <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Hy zegt: kan ik myn <rhyme label="a" type="m" corresp="#s6:l1">Vaderland</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vernederd zien <rhyme label="c" type="m">vergaan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dan waar myn min u hart <rhyme label="a" type="m">onwaard</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Geliefde <rhyme label="b" type="m">Julia</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">waar 't Vaderland zyn stryders <rhyme label="a" type="m" corresp="#s7:l1">roept</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Klopt myn 't hart te <rhyme label="c" type="m">ga</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Den heldentaal des <rhyme label="a" type="m">Jongelings</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">verdryft myn angst en <rhyme label="b" type="m">leet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Myn Eduward heeft voor den <rhyme label="a" type="m" corresp="#s8:l1">tocht</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Myn 't hart reeds <rhyme label="c" type="m">overreed</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat hart klopt als 't edel <rhyme label="a" type="m">hart</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Van mynen fieren <rhyme label="b" type="m">held</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">My dunkt ik zie myn minnaar <rhyme label="a" type="m" corresp="#s9:l1">reeds</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Aan 't Oorlogs spits <rhyme label="c" type="m">gesteld</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ik gespe zelfs het Oorlogs <rhyme label="a" type="m">staal</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Hem om de lendenen <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Zijn moed bezweek niet maar zijn <rhyme label="a" type="m" corresp="#s10:l1">min</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Schonk myne min een <rhyme label="c" type="m">traan</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Myn arm die hem als staal <rhyme label="a" type="m">omvat</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Wringt onmerkbaar <rhyme label="b" type="m">los</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Sinkt op myn boesem loost een <rhyme label="a" type="m" corresp="#s11:l1">zugt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">En stapt op 't Oorlogs <rhyme label="c" type="m">ros</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Den jongeling dus ten togt <rhyme label="a" type="m">gereed</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Rykt mij nog eens de <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Hy zegt: vaarwel, ô <rhyme label="a" type="m" corresp="#s12:l1">Julia</rhyme>!</l>
        <l id="s12:l4" met="3" type="-+">ô Min! ô <rhyme label="c" type="m">Vaderland</rhyme>!</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Gelijk een snellen Blixem <rhyme label="a" type="m">straal</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Vliegt hij na de Oorlogs <rhyme label="b" type="m">drom</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Mijn hart mijn oog roept: <rhyme label="a" type="m" corresp="#s13:l1">Eduward</rhyme>!</l>
        <l id="s13:l4" met="3" type="-+">Maar mijne tong blijft <rhyme label="c" type="m">stom</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">ô God! bescherm mijn <rhyme label="a" type="m">Vaderland</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Waakt voor mijn Jonglings <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">'k Heb liever geene <rhyme label="a" type="m" corresp="#s14:l1">Jongeling</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Dan eene zonder <rhyme label="c" type="m">eer</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Keert hij bekranst in mijne <rhyme label="a" type="m">arm</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Verwinnaar in den <rhyme label="b" type="m">strijd</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Dan word door 't dierbaar <rhyme label="a" type="m" corresp="#s15:l1">Vaderland</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Ons Bruilofts maal <rhyme label="c" type="m">bereid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
