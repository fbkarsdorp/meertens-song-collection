  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Die vast'lyck op den <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Stelt zijn vertrouwen <rhyme label="a" type="m" corresp="#s1:l1">neer</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Syn druck en <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In vreucht verandert <rhyme label="b" type="m" corresp="#s1:l3">wert</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Tot uytterst' wert hy noyt <rhyme label="c" type="m">benout</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Die op den Heere grondig <rhyme label="c" type="m" corresp="#s1:l5">bout</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Die het goet,, Geerne <rhyme label="d" type="m">doet</rhyme>,,</l>
        <l id="s1:l8" met="4" type="+-">En het quaet,, Tegen <rhyme label="e" type="m">staet</rhyme>,,</l>
        <l id="s1:l9" met="4" type="+-">Sal't op 'tlest,, Hebben <rhyme label="f" type="m">best</rhyme>.</l>
      </lg>
    </lg>
  </text>
