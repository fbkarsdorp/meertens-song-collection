  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NA hier ons langh <rhyme label="a" type="f">versamen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Moet eenmael ghescheyden <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Eerelijck na <rhyme label="a" type="f" corresp="#s1:l1">'tbetamen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Van 'tSpeel coren in conste <rhyme label="b" type="m" corresp="#s1:l2">fijn</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Hoewel <rhyme label="c" type="f">'tbedroeflick</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Een constich herte <rhyme label="d" type="m">smaeckt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hoopt dat d'oud' mensch <rhyme label="c" type="f" corresp="#s1:l5">behoeflick</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Door 'sconsts troost is <rhyme label="d" type="m" corresp="#s1:l6">vermaeckt</rhyme>.</l>
        <l id="s1:l9" met="4" type="-+">Oorlof Pelcaen, met const <rhyme label="e" type="m">belaen</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Aensiet de jonckheyt cleyn <rhyme label="f" type="m">certeyn</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">V Trou vermaen blijckt t'ons <rhyme label="e" type="m" corresp="#s1:l9">ghedaen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Dancken wy edel <rhyme label="f" type="m" corresp="#s1:l10">greyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
