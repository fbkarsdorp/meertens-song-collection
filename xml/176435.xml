  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">‘k Zing gedreeven door de <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">’t Schoon van myne <rhyme label="a" type="m" corresp="#s1:l1">zielsvriendin</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ja, ja, Mietje <rhyme label="b" type="m">ja</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Zing dit lietje, lieve <rhyme label="c" type="f">Mietje</rhyme>,</l>
        <l id="s1:l5" met="6" type="+-">Ja, ja, Mietje <rhyme label="b" type="m" corresp="#s1:l3">ja</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zing het my uit liefde <rhyme label="b" type="m" corresp="#s1:l3">na</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zie ik ook uw schoone <rhyme label="a" type="m">geest</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Op uw aangenaame <rhyme label="a" type="m" corresp="#s2:l1">leest</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dan, dan, Mietje <rhyme label="b" type="m">dan</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Door verrukking, voel ik <rhyme label="c" type="f">drukking</rhyme>,</l>
        <l id="s2:l5" met="6" type="+-">Dan, dan, Mietje <rhyme label="b" type="m" corresp="#s2:l3">dan</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Roep ik; ach was ik haar <rhyme label="b" type="m" corresp="#s2:l3">Man</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">s’Avonds als den <rhyme label="a" type="m">dageraad</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Is verdweenen vroeg en <rhyme label="a" type="m" corresp="#s3:l1">laat</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Lief, lief, Mietje <rhyme label="b" type="m">lief</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Ô! Dan spreek ik, ô! dan smeek <rhyme label="c" type="f">ik</rhyme>,</l>
        <l id="s3:l5" met="6" type="+-">Lief, lief, Mietje <rhyme label="b" type="m" corresp="#s3:l3">lief</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Van en om u harten <rhyme label="b" type="m" corresp="#s3:l3">dief</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">‘k Roep dan vaak, ô! groote <rhyme label="a" type="m">goon</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">O wat staan die oogen <rhyme label="a" type="m" corresp="#s4:l1">schoon</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ach, ach, Mietje, <rhyme label="b" type="m">ach</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">‘k Ben gevangen, door ‘t <rhyme label="c" type="f">verlangen</rhyme>,</l>
        <l id="s4:l5" met="6" type="+-">Ach, ach, Mietje <rhyme label="b" type="m" corresp="#s4:l3">ach</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat ik u bezitten <rhyme label="b" type="m" corresp="#s4:l3">mag</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Neus en mond en koon en <rhyme label="a" type="m">kin</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ales stont verukking <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Hart hand, voet en <rhyme label="b" type="m">tand</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Even netjes, en de <rhyme label="c" type="f">tetjes</rhyme>,</l>
        <l id="s5:l5" met="6" type="+-">Zo zo, o! <rhyme label="b" type="m" corresp="#s5:l3">o</rhyme>!</l>
        <l id="s5:l6" met="4" type="+-">‘k Vondze aan geen schoonheid <rhyme label="b" type="m" corresp="#s5:l3">zo</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Geef my nu een lieve <rhyme label="a" type="m">kus</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">O gy weet dat ik hem <rhyme label="a" type="m" corresp="#s6:l1">lus</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Mie, Mie, lieve <rhyme label="b" type="m">Mie</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Hoor myn toone, lieve <rhyme label="c" type="f">schoone</rhyme>,</l>
        <l id="s6:l5" met="6" type="+-">Mie, Mie, lieve <rhyme label="b" type="m" corresp="#s6:l3">Miet</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Buiten u bemin ik <rhyme label="b" type="m" corresp="#s6:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Meisje lief geloof het <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Dat uw vriend hoe het ook <rhyme label="a" type="m" corresp="#s7:l1">zy</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Schreid, schreid, dikwils <rhyme label="b" type="m">schreid</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">By de stormen, die ‘er <rhyme label="c" type="f">wormen</rhyme>,</l>
        <l id="s7:l5" met="6" type="+-">Schreid, schreid, dikwils <rhyme label="b" type="m" corresp="#s7:l3">schreid</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Als hy s’avonds van uw <rhyme label="b" type="m" corresp="#s7:l3">scheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
