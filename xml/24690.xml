    <melody>Gij die Jezus' wijngaard plant</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Ghij die Ihesus wijngaert <rhyme label="a" type="m">plant</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Verblijt v op dat soete <rhyme label="a" type="m" corresp="#s1:l1">lant</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Daer ghij toe zijt <rhyme label="b" type="f">vercoren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die chierheit es <rhyme label="c" type="m">onsprekelijc</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Die blijscap <rhyme label="c" type="m" corresp="#s1:l4">onbegrijpelijc</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die v daer sal <rhyme label="b" type="f" corresp="#s1:l3">ghebueren</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Die claerheit. vruecht. ure <rhyme label="d" type="m">zuuerheit</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Die Ihesus v daer heeft <rhyme label="d" type="m" corresp="#s1:l7">bereyt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">En es v niet te <rhyme label="e" type="f">noemen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Iherusalem so es zijn <rhyme label="a" type="m">naeme</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Vul helicheden scoon <rhyme label="a" type="m" corresp="#s2:l1">bequaem</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Veruult met alle <rhyme label="b" type="f">vrueghden</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Van hier en comt daer niemant <rhyme label="c" type="m">in</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Dan die int hart dracht Ihesus <rhyme label="c" type="m" corresp="#s2:l4">min</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En es verchiert met <rhyme label="b" type="f" corresp="#s2:l3">dueghden</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Dus es hij die zijn wijngaert <rhyme label="d" type="m">plant</rhyme></l>
        <l id="s2:l8" met="4" type="-+">En nerstich dient hem zeere <rhyme label="d" type="m" corresp="#s2:l7">verlanct</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Om corts bij hem te <rhyme label="e" type="f">commen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Tsal eewich dueren sonder <rhyme label="a" type="m">oude</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Tes scoender vele dat waer van <rhyme label="a" type="m" corresp="#s3:l1">goude</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Verchiert met gulden <rhyme label="b" type="f">boemen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Die clader blincken dan die <rhyme label="c" type="m">maen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">De sonne zij te bouen <rhyme label="c" type="m" corresp="#s3:l4">gaen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Met haren zoeten <rhyme label="b" type="f" corresp="#s3:l3">bloemen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Van puren goude eist scoen <rhyme label="d" type="m">paueyt</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Met zoeten roesen <rhyme label="d" type="m" corresp="#s3:l7">ouerleyt</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Die sullen eewich <rhyme label="e" type="f">dueren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Och hoe onsprekelike <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Dat god daer es v lieue <rhyme label="a" type="m" corresp="#s4:l1">vaer</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Es wonder meer dan <rhyme label="b" type="f">wonder</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Hi es die eewich heeft <rhyme label="c" type="m">gheleeft</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ent allen dinghen wesen <rhyme label="c" type="m" corresp="#s4:l4">gheeft</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Daer bouen ooc hier <rhyme label="b" type="f" corresp="#s4:l3">onder</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Vut hem vloeyt alle <rhyme label="d" type="m">zoeticheit</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Die haer so verre ende wijde <rhyme label="d" type="m" corresp="#s4:l7">spreit</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Al toots lants guldijn <rhyme label="e" type="f">mueren</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sijn eewich kint marien <rhyme label="a" type="m">soon</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Es daer so wonderlike <rhyme label="a" type="m" corresp="#s5:l1">scoen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Die dlant v heeft <rhyme label="b" type="f">ghewonnen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Hij sijt daer in dat zoete <rhyme label="c" type="m">lant</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Zijns lieuen vaders rechter <rhyme label="c" type="m" corresp="#s5:l4">hant</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Voor alle <rhyme label="b" type="f" corresp="#s5:l3">creatueren</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Die hem aenscout es zo <rhyme label="d" type="m">verblijt</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Tien .M. jaer druct hem gheen <rhyme label="d" type="m" corresp="#s5:l7">tijt</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Tes waer diet can <rhyme label="e" type="f">versinnen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Want die tijt en es daer <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Daer nemmermeer en es <rhyme label="a" type="m" corresp="#s6:l1">verdriet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Van buten noch van <rhyme label="b" type="f">binnen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Men es daer eewich vreen en <rhyme label="c" type="m">blij</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Van allen zorghen es men <rhyme label="c" type="m" corresp="#s6:l4">vri</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Daer en mach gheen weelde <rhyme label="b" type="f" corresp="#s6:l3">ghebreken</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Maria es daer <rhyme label="d" type="m">conincghin</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Alle reyne maegden haer <rhyme label="d" type="m" corresp="#s6:l7">ghesin</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Die ihesu zijn <rhyme label="e" type="f">vercoren</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maer zij es bouen al <rhyme label="a" type="m">ghemint</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Van tsconinx sone haer lieue <rhyme label="a" type="m" corresp="#s7:l1">kint</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Als balsem onder <rhyme label="b" type="f">doren</rhyme></l>
        <l id="s7:l4" met="4" type="-+">So claer scoen frisch en <rhyme label="c" type="m">zuuerlijc</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Dat sij daer es in haer kints <rhyme label="c" type="m" corresp="#s7:l4">rijc</rhyme></l>
        <l id="s7:l6" met="3" type="-+">En es niet vut te <rhyme label="b" type="f" corresp="#s7:l3">spreken</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Haer croene blinct wonderlike <rhyme label="d" type="m">zeere</rhyme></l>
        <l id="s7:l8" met="4" type="-+">Als guldin lelyen zijn haer <rhyme label="d" type="m" corresp="#s7:l7">cleere</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Gout roosen haer <rhyme label="e" type="f">sampielen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Recht als die sterren en die <rhyme label="a" type="m">maen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Vander sonnen haer licht <rhyme label="a" type="m" corresp="#s8:l1">ontfaen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Soe doen van haer die <rhyme label="b" type="f">sielen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Haer roeke gaet bouen alle <rhyme label="c" type="m">cruut</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Ende bouen cymbalen haer <rhyme label="c" type="m" corresp="#s8:l4">gheluut</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Ent al tghescal der <rhyme label="b" type="f" corresp="#s8:l3">pipen</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Die borghers draghen al een <rhyme label="d" type="m">croon</rhyme></l>
        <l id="s8:l8" met="4" type="-+">Sij singhen dat clinket inden <rhyme label="d" type="m" corresp="#s8:l7">troen</rhyme></l>
        <l id="s8:l9" met="3" type="-+">O zanna <rhyme label="e" type="f">alleluya</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer och der maeghden sanc es <rhyme label="a" type="m">scoon</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Zij singhen met des coninx <rhyme label="a" type="m" corresp="#s9:l1">soon</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Ende metter maeght <rhyme label="b" type="f">Marien</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Hoe dat dat luyt ouer <rhyme label="c" type="m">hemelrijc</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Hoe scoen zoet claer en <rhyme label="c" type="m" corresp="#s9:l4">wonderlijc</rhyme></l>
        <l id="s9:l6" met="3" type="-+">En mach gheen mensche <rhyme label="b" type="f" corresp="#s9:l3">begripen</rhyme></l>
        <l id="s9:l7" met="4" type="-+">Haer croon heeft om een rosen <rhyme label="d" type="m">crans</rhyme></l>
        <l id="s9:l8" met="4" type="-+">Ende zijn met ihesu anden <rhyme label="d" type="m" corresp="#s9:l7">dans</rhyme></l>
        <l id="s9:l9" met="3" type="-+">En louen sonder <rhyme label="e" type="f">merren</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dat cranskin heeft so groot <rhyme label="a" type="m">virtuut</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Elc roeskin gheeft meerd claerheyts <rhyme label="a" type="m" corresp="#s10:l1">vut</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Dan sonne mane oft <rhyme label="b" type="f">sterren</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Gheen cruut op eertrijc es so <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Dat rieken mach als tcranskin <rhyme label="c" type="m" corresp="#s10:l4">doet</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Al ware al tcruut <rhyme label="b" type="f" corresp="#s10:l3">vyolen</rhyme></l>
        <l id="s10:l7" met="4" type="-+">Sij hooren ooc dat zoet <rhyme label="d" type="m">gheluut</rhyme></l>
        <l id="s10:l8" met="4" type="-+">Comt mijn gheminde zoete <rhyme label="d" type="m" corresp="#s10:l7">bruut</rhyme></l>
        <l id="s10:l9" met="3" type="-+">En rust v in mijn <rhyme label="e" type="f">aermen</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Sij zijn beuaen met Ihesus <rhyme label="a" type="m">min</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Als cerubin en <rhyme label="a" type="m" corresp="#s11:l1">cheraphin</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Van grooter lieften sij <rhyme label="b" type="f">berren</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Ghelijc der zonnen es haer <rhyme label="c" type="m">aenschijn</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Sij volghen tzoete <rhyme label="c" type="m" corresp="#s11:l4">lammekin</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Ghecleet met witten <rhyme label="b" type="f" corresp="#s11:l3">stoelen</rhyme></l>
        <l id="s11:l7" met="4" type="-+">Die inghelen end zielen zijn <rhyme label="d" type="m">ghemeyn</rhyme></l>
        <l id="s11:l8" met="4" type="-+">En zinghen blidelic ouer <rhyme label="d" type="m" corresp="#s11:l7">een</rhyme></l>
        <l id="s11:l9" met="3" type="-+">Den coninc vanden <rhyme label="e" type="f">troone</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Lof eer sij v heer van <rhyme label="a" type="m">sabaoth</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Almachtich eewich helich <rhyme label="a" type="m" corresp="#s12:l1">god</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Een wesen drie <rhyme label="b" type="f">persoenen</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Ghi sijt die eewich hebt <rhyme label="c" type="m">gheweest</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Ent hemel aertrijc met v <rhyme label="c" type="m" corresp="#s12:l4">gheest</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Doet vroechdelic <rhyme label="b" type="f" corresp="#s12:l3">iubeleren</rhyme></l>
        <l id="s12:l7" met="4" type="-+">Och tes hem zeere <rhyme label="d" type="m">behaechelijc</rhyme></l>
        <l id="s12:l8" met="4" type="-+">Te hoorne van dat zoete <rhyme label="d" type="m" corresp="#s12:l7">rijc</rhyme></l>
        <l id="s12:l9" met="3" type="-+">Die eerbaerlike <rhyme label="e" type="f">leuen</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ende dienen ihesu <rhyme label="a" type="m">ghetrouwelijc</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Sijnen wijngaert plant si <rhyme label="a" type="m" corresp="#s13:l1">bouwelijc</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Die weerelt sij <rhyme label="b" type="f">begheuen</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Want als haer tijt hier es <rhyme label="c" type="m">ghedaen</rhyme></l>
        <l id="s13:l5" met="4" type="-+">Sal hijse lieflijc daer <rhyme label="c" type="m" corresp="#s13:l4">ontfaen</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Ghelijc die zonne <rhyme label="b" type="f" corresp="#s13:l3">verchieren</rhyme></l>
        <l id="s13:l7" met="4" type="-+">En hebbe so vele niet <rhyme label="d" type="m">gheseit</rhyme></l>
        <l id="s13:l8" met="4" type="-+">Dat lant en es noch bat <rhyme label="d" type="m" corresp="#s13:l7">bereyt</rhyme></l>
        <l id="s13:l9" met="3" type="-+">Dan eenich mensche can <rhyme label="e" type="f">dincken</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Plant vri ghij sult daer zijn <rhyme label="a" type="m">verblijt</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Maer ziet dat ghi ghestadich <rhyme label="a" type="m" corresp="#s14:l1">sijt</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Hu tijt die naect al <rhyme label="b" type="f">enken</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Als v ihesus daer sal <rhyme label="c" type="m">ontbien</rhyme></l>
        <l id="s14:l5" met="4" type="-+">V werken sal hij ouer <rhyme label="c" type="m" corresp="#s14:l4">zien</rhyme></l>
        <l id="s14:l6" met="3" type="-+">En na v due dueghden <rhyme label="b" type="f" corresp="#s14:l3">loenen</rhyme></l>
        <l id="s14:l7" met="4" type="-+">Och waerlijc hij es zeere <rhyme label="d" type="m">verblent</rhyme></l>
        <l id="s14:l8" met="4" type="-+">Die tijdelijc voer eewich <rhyme label="d" type="m" corresp="#s14:l7">mint</rhyme></l>
        <l id="s14:l9" met="3" type="-+">En wille dit lant <rhyme label="e" type="f">vergheten</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
