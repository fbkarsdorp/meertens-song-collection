<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4103</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>De busonnette</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="8" type="-+">BEdrijft nu vruecht / bedrijft nu melodie / Bedrijft nu <rhyme label="a" type="m">melodie</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Loeft die maecht <rhyme label="b" type="m">ghebenedijt</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Die ons allen heeft <rhyme label="b" type="m" corresp="#s1:l2">verblijt</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Ghequijt. Wient <rhyme label="b" type="m" corresp="#s1:l2">spijt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Sy heeft veruult het woort van <rhyme label="b" type="m" corresp="#s1:l2">prophecie</rhyme></l>
        <l id="s1:l6" met="5" type="-+">Gheloeft sidy <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s1:l6">ierarchie</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="8" type="-+">Mids dese schone ionge dracht / schone ionge <rhyme label="a" type="m">dracht</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Gheboren opten alder coutsten <rhyme label="b" type="m">nacht</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Bewacht <rhyme label="b" type="m" corresp="#s2:l2">Voerdacht</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Albi des vaders <rhyme label="b" type="m" corresp="#s2:l2">cracht</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ghebaert hebdy v kint wel schoone <rhyme label="b" type="m" corresp="#s2:l2">suyuerie</rhyme>.</l>
        <l id="s2:l6" met="5" type="-+">Gheloeft sidy <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s2:l6">ierarchie</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="8" type="-+">Die herdekens opt velt waren seer verblijt / si waren seer <rhyme label="a" type="m">verblijt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Sij songhen / si spronghen / sij dansten <rhyme label="b" type="m">onueruaert</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Si liepen seer blidelijc te bethleem <rhyme label="b" type="m" corresp="#s3:l2">waert</rhyme>.</l>
        <l id="s3:l4" met="2" type="-+">Vermaert <rhyme label="b" type="m" corresp="#s3:l2">Verclaert</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Het was hen gheopenbaert / Dies waren si vry van <rhyme label="b" type="m" corresp="#s3:l2">melancolie</rhyme></l>
        <l id="s3:l6" met="5" type="-+">Gheloeft sidy <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s3:l6">ierarchie</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="8" type="-+">Besneden wert Iesus gods liefste tresorie / gods liefste <rhyme label="a" type="m">tresorie</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ten achtsten dage vol alder <rhyme label="b" type="m">ootmoet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Symeons woerden Maria wel <rhyme label="b" type="m" corresp="#s4:l2">verstoet</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Doer dbloet Seer <rhyme label="b" type="m" corresp="#s4:l2">goet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Was tstorten ons behoet / Want het heeft ons behoet voer die helsche <rhyme label="b" type="m" corresp="#s4:l2">partie</rhyme></l>
        <l id="s4:l6" met="5" type="-+">Gheloeft sidy <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s4:l6">ierarchie</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="8" type="-+">Drie coninghen van groter seignorien / van groter <rhyme label="a" type="m">seignorien</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Sochten dit kindeken / elck wt enen vreemden <rhyme label="b" type="m">hoecke</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Si haddent ghelesen in propheten <rhyme label="b" type="m" corresp="#s5:l2">boecke</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Tbesoeck Seer <rhyme label="b" type="m" corresp="#s5:l2">cloeck</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Deden si sonder vloeck / De clare sterre inden hemel <rhyme label="b" type="m" corresp="#s5:l2">zye</rhyme></l>
        <l id="s5:l6" met="5" type="-+">Gheloeft sidy <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s5:l6">ierarchie</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="8" type="-+">Herodes wreet sijnde vol der hatie / vol der <rhyme label="a" type="m">hatie</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Die ionghe kinderen wt nide <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Sijn tyrannen te doden <rhyme label="b" type="m" corresp="#s6:l2">gheboot</rhyme>.</l>
        <l id="s6:l4" met="2" type="-+">Dies schoot Ende <rhyme label="b" type="m" corresp="#s6:l2">vloot</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Maria wt sulcker noot / Vragende Ioseph waer yst best dat ic <rhyme label="b" type="m" corresp="#s6:l2">vlie</rhyme></l>
        <l id="s6:l6" met="5" type="-+">Gheloeft sidy <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s6:l6">ierarchie</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="8" type="-+">In egipten dat ons die vlucht geschiede / dat ons die vlucht <rhyme label="a" type="m">gheschiede</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Een lantman sijn koorne heeft <rhyme label="b" type="m">ghesaeyt</rhyme></l>
        <l id="s7:l3" met="4" type="-+">By miraculen niet langer <rhyme label="b" type="m" corresp="#s7:l2">ghedelaeyt</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Ghepaeyt <rhyme label="b" type="m" corresp="#s7:l2">Verfraeyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Heeft hi terstont ghemaeyt / Sulc wonder en geschiede <rhyme label="b" type="m" corresp="#s7:l2">nie</rhyme></l>
        <l id="s7:l6" met="5" type="-+">Gheloeft sidy <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s7:l6">ierarchie</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="8" type="-+">Princesselike maecht regente der hellen prijs / regente der hellen <rhyme label="a" type="m">prijs</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Wij bidden v met deuoter <rhyme label="b" type="m">zede</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Kaerle onsen Keyser verleent pays en <rhyme label="b" type="m" corresp="#s8:l2">vrede</rhyme>.</l>
        <l id="s8:l4" met="2" type="-+">Onse stede Daer <rhyme label="b" type="m" corresp="#s8:l2">mede</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Dit nu is onse bede / Beschermen wilt voer <rhyme label="b" type="m" corresp="#s8:l2">verraderie</rhyme></l>
        <l id="s8:l6" met="5" type="-+">Bidden wij v <rhyme label="c" type="m">Marie</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Si heeft verblijt der engelen ierarchie / der engelen <rhyme label="c" type="m" corresp="#s8:l6">ierarchie</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
