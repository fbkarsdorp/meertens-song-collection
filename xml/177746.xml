    <tags/>
    <melody>Psalm 061 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wil, o God! mijn bede <rhyme label="a" type="f">hooren</rhyme>;</l>
        <l id="s1:l2" met="2" type="+-">Neig uw <rhyme label="a" type="f" corresp="#s1:l1">ooren</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Naar mijn zuchten en <rhyme label="b" type="m">geween</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">In verafgelegen <rhyme label="c" type="f">streeken</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Schier <rhyme label="c" type="f" corresp="#s1:l4">bezweeken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zoek ik heul bij U <rhyme label="b" type="m" corresp="#s1:l3">alleen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Leid mij, Heer, ik zou in 't <rhyme label="a" type="f">stijgen</rhyme></l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s2:l1">Nederzijgen</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Leid mij op een hooge <rhyme label="b" type="m">rots</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Wil mij tot een toevlugt <rhyme label="c" type="f">weezen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Als <rhyme label="c" type="f" corresp="#s2:l4">voordezen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">'s Vijands wreed geweld ten <rhyme label="b" type="m" corresp="#s2:l3">trots</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Zal in uwe tent <rhyme label="a" type="f">verkeeren</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Heer der <rhyme label="a" type="f" corresp="#s3:l1">heeren</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Voor uw oog, in <rhyme label="b" type="m">eeuwigheid</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">'k Zal op U mijn vast <rhyme label="c" type="f">vertrouwen</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Altoos <rhyme label="c" type="f" corresp="#s3:l4">bouwen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Door uw vleuglen <rhyme label="b" type="m" corresp="#s3:l3">overspreid</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Want uw goedheid, die wij <rhyme label="a" type="f">looven</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Heeft van <rhyme label="a" type="f" corresp="#s4:l1">boven</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Mijn geloft' en beê <rhyme label="b" type="m">gehoord</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Gij deedt mij tot d' erfnis <rhyme label="c" type="f">komen</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Van de <rhyme label="c" type="f" corresp="#s4:l4">vroomen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Wien de vrees uws naams <rhyme label="b" type="m" corresp="#s4:l3">bekoort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gij zult nieuwe dagen <rhyme label="a" type="f">voegen</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Vol <rhyme label="a" type="f" corresp="#s5:l1">genoegen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Bij des Konings <rhyme label="b" type="m">levenstijd</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Zijner jaaren tal <rhyme label="c" type="f">vermeêren</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">In 't <rhyme label="c" type="f" corresp="#s5:l4">regeeren</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Door uw gunst van ramp <rhyme label="b" type="m" corresp="#s5:l3">bevrijd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hij zal eeuwig in <rhyme label="a" type="f">vermogen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Voor uw <rhyme label="a" type="f" corresp="#s6:l1">oogen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Zitten op zijn' troon, o <rhyme label="b" type="m">Heer</rhyme>!</l>
        <l id="s6:l4" met="4" type="+-">Zend uw waarheid, uw <rhyme label="c" type="f">ontferming</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Ter <rhyme label="c" type="f" corresp="#s6:l4">bescherming</rhyme>;</l>
        <l id="s6:l6" met="4" type="+-">Zend ze tot zijn wachters <rhyme label="b" type="m" corresp="#s6:l3">neêr</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'k Zal dan door mijn blijde <rhyme label="a" type="f">galmen</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Door mijn <rhyme label="a" type="f" corresp="#s7:l1">psalmen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Looven uwe <rhyme label="b" type="m">majesteit</rhyme>;</l>
        <l id="s7:l4" met="4" type="+-">Mijn geloften U <rhyme label="c" type="f">betaalen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-"><rhyme label="c" type="f" corresp="#s7:l4">Menigmaalen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Plegtig aan U <rhyme label="b" type="m" corresp="#s7:l3">toegezeid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
