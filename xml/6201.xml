<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6201</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het ging een meisje aan de kant</melody>
  <author>C.P. van Wesbusch</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs my Flora heeft van <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">In haer schoone <rhyme label="b" type="f">Bloeme-dalen</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Send ick op 't bedaude <rhyme label="a" type="m" corresp="#s1:l1">groen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Seghen-rijcke gulde <rhyme label="b" type="f" corresp="#s1:l2">stralen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'t Welck de Knopjens haest <rhyme label="c" type="m">ontluyckt</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Dat elck Bloempjen lieff'lijck <rhyme label="c" type="m" corresp="#s1:l5">ruyckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Bloempjens door mijn Floraes <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Schoon ghewrocht, en soet van <rhyme label="b" type="f">geure</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">In den Bloemen-dael <rhyme label="a" type="m" corresp="#s2:l1">gheplant</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Met verscheyden schoone <rhyme label="b" type="f" corresp="#s2:l2">kleuren</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Sietmen nae mijn <rhyme label="c" type="m">ondergaen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Voor de Nacht gheslooten <rhyme label="c" type="m" corresp="#s2:l5">staen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer, wanneer ick weder <rhyme label="a" type="m">rijs</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En nae 't hooghe blaeu kom <rhyme label="b" type="f">loopen</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Yder Bloempjen stelt, om <rhyme label="a" type="m" corresp="#s3:l1">prijs</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Weer zijn lieve Kruyntjen <rhyme label="b" type="f" corresp="#s3:l2">open</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dat nae mijne komst <rhyme label="c" type="m">verlanght</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En mijn straeltjes bly <rhyme label="c" type="m" corresp="#s3:l5">ontfanght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Flora kan niet sonder <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ick niet sonder haer doen <rhyme label="b" type="f">bloeyen</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Sy maeckt Bloemen, ick gheef <rhyme label="a" type="m" corresp="#s4:l1">schijn</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Om de selve te doen <rhyme label="b" type="f" corresp="#s4:l2">groeyen</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Dies ick Flora (door de <rhyme label="c" type="m">Trouw</rhyme>)</l>
        <l id="s4:l6" met="4" type="+-">Wil verkiesen tot mijn <rhyme label="c" type="m" corresp="#s4:l5">Vrouw</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Flora zijde een <rhyme label="a" type="m">Goddin</rhyme>:</l>
        <l id="s5:l2" met="4" type="+-">Ick een Godt van aen 't <rhyme label="b" type="f">beginne</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Beyd' versien met soete <rhyme label="a" type="m" corresp="#s5:l1">Min</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Om malkand'ren te <rhyme label="b" type="f" corresp="#s5:l2">beminnen</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Daphne die my ginck <rhyme label="c" type="m">versmaen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Wil ick Boom-wijs laten <rhyme label="c" type="m" corresp="#s5:l5">staen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
