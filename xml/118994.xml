      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CHarmantes Englisch <rhyme label="a" type="m">Kind</rhyme>/</l>
        <l id="s1:l2" met="3" type="-+">Dein strahl hat mich <rhyme label="a" type="m" corresp="#s1:l1">entzund</rhyme>/</l>
        <l id="s1:l3" met="3" type="-+">Dein blich hat mich <rhyme label="b" type="f">entbunden</rhyme>/</l>
        <l id="s1:l4" met="3" type="-+">Du schlägest hebes <rhyme label="b" type="f" corresp="#s1:l3">wunden</rhyme>/</l>
        <l id="s1:l5" met="3" type="-+">Die meine seel <rhyme label="a" type="m" corresp="#s1:l1">empfind</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Recht Englisch komts du <rhyme label="a" type="m">mir</rhyme>/</l>
        <l id="s2:l2" met="3" type="-+">Du meine schönheit <rhyme label="a" type="m" corresp="#s2:l1">fur</rhyme>/</l>
        <l id="s2:l3" met="3" type="-+">Du hast mich recht <rhyme label="b" type="f">begeistert</rhyme>/</l>
        <l id="s2:l4" met="3" type="-+">Vnd meinen sinn <rhyme label="b" type="f" corresp="#s2:l3">bemeistert</rhyme>/</l>
        <l id="s2:l5" met="3" type="-+">Drum sehn ich nich nach <rhyme label="a" type="m" corresp="#s2:l1">dir</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doch wilst du grausam <rhyme label="a" type="m">seyn</rhyme>/</l>
        <l id="s3:l2" met="3" type="-+">Disz bringet mir viel <rhyme label="a" type="m" corresp="#s3:l1">pein</rhyme>/</l>
        <l id="s3:l3" met="3" type="-+">Wenn ich nach meinen <rhyme label="b" type="f">treibe</rhyme>/</l>
        <l id="s3:l4" met="3" type="-+">Dich bitten um die <rhyme label="b" type="f" corresp="#s3:l3">liebe</rhyme>/</l>
        <l id="s3:l5" met="3" type="-+">So sagst du immer <rhyme label="a" type="m" corresp="#s3:l1">nein</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ach! sag mir einmall <rhyme label="a" type="m">ja</rhyme>/</l>
        <l id="s4:l2" met="3" type="-+">So ist mein hertze <rhyme label="a" type="m" corresp="#s4:l1">da</rhyme>/</l>
        <l id="s4:l3" met="3" type="-+">Dich stetig su <rhyme label="b" type="f">verehren</rhyme>/</l>
        <l id="s4:l4" met="3" type="-+">Tzu folgen deinen <rhyme label="b" type="f" corresp="#s4:l3">lehren</rhyme>/</l>
        <l id="s4:l5" met="3" type="-+">Denn ist vergnugen <rhyme label="a" type="m" corresp="#s4:l1">nah</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Weil du mich nun <rhyme label="a" type="m">verwundt</rhyme>/</l>
        <l id="s5:l2" met="3" type="-+">So thu mir dieses <rhyme label="a" type="m" corresp="#s5:l1">kundt</rhyme>/</l>
        <l id="s5:l3" met="3" type="-+">Mich kunftig treu zu <rhyme label="b" type="f">lieben</rhyme>/</l>
        <l id="s5:l4" met="3" type="-+">Mich nich mehr zu <rhyme label="b" type="f" corresp="#s5:l3">betruben</rhyme>/</l>
        <l id="s5:l5" met="3" type="-+">Den truk ich mundt auf <rhyme label="a" type="m" corresp="#s5:l1">mundt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
