  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Den suarten nacht bedeckt het <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="4" type="-+">wat is geworuet int aertsche <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme></l>
        <l id="s1:l3" met="4" type="-+">door ons belijd wij bidden <rhyme label="b" type="m">heer</rhyme></l>
        <l id="s1:l4" met="4" type="-+">rechter oprecht der herten <rhyme label="b" type="m" corresp="#s1:l3">teer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">dat ghij van ons wech neempt die <rhyme label="a" type="m">sond</rhyme></l>
        <l id="s2:l2" met="4" type="-+">en sharten vuijl affwascht dees <rhyme label="a" type="m" corresp="#s2:l1">stond</rhyme></l>
        <l id="s2:l3" met="4" type="-+">v geeft Christe oock in ons <rhyme label="b" type="m">stort</rhyme></l>
        <l id="s2:l4" met="4" type="-+">waer me het quaet verdreuen <rhyme label="b" type="m" corresp="#s2:l3">wort</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Aensiet thert boes, dat is seer <rhyme label="a" type="m">flaeu</rhyme></l>
        <l id="s3:l2" met="4" type="-+">gebeten vanden sonden <rhyme label="a" type="m" corresp="#s3:l1">graeu</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Begeert van doncker sijn <rhyme label="b" type="m">behoet</rhyme></l>
        <l id="s3:l4" met="4" type="-+">en soeken v verlosser <rhyme label="b" type="m" corresp="#s3:l3">goet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Van ons alle blintheijt <rhyme label="a" type="m">verset</rhyme></l>
        <l id="s4:l2" met="4" type="-+">meest die van binnen doet <rhyme label="a" type="m" corresp="#s4:l1">belet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">dat thart int licht salich <rhyme label="b" type="m">verhoecht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">en wort gestelt in shemels <rhyme label="b" type="m" corresp="#s4:l3">vroocht</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
