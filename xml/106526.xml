<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>106526</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb de bras van het luizige vrijen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'k HEb den bras van't luysige <rhyme label="a" type="f">Vrijen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Meysies ick packer mijn spillities <rhyme label="b" type="m">op</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Truytien is wel een Geestige <rhyme label="b" type="m" corresp="#s1:l2">Pop</rhyme></l>
        <l id="s1:l4" met="4" type="+-">En ick hebse wel menge <rhyme label="a" type="f" corresp="#s1:l1">lijen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Maar de Kermis is <rhyme label="c" type="m">gedaan</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">'k Heb den bras van 't Vrijen <rhyme label="c" type="m" corresp="#s1:l5">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Settent mijn Truytie opper <rhyme label="a" type="f">Consienti</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En ic sey je moeter <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Maar se zey ie meucht wel <rhyme label="b" type="m" corresp="#s2:l2">gaan</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Trouwe dat is maar <rhyme label="a" type="f" corresp="#s2:l1">Penetenti</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Als de Bruydt is inde <rhyme label="c" type="m">Schuyt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Is 't Vleijen en Beloven <rhyme label="c" type="m" corresp="#s2:l5">uyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Sey mijn Lief mijn Blaasie met <rhyme label="a" type="f">Boonen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Mijn Gesuyckerden Rijst <rhyme label="b" type="m">en-Bry</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Datie wist wat dat ick <rhyme label="b" type="m" corresp="#s3:l2">ly</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Om u schoone Karnate <rhyme label="a" type="f" corresp="#s3:l1">koonen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">'k Sucht, ick Ween, ick Klaag, ick <rhyme label="c" type="m">Snick</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Al mijn Ingewant aan <rhyme label="c" type="m" corresp="#s3:l5">stick</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maar de Meyt en woume niet <rhyme label="a" type="f">hooren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">En zy sette haer mondt soo <rhyme label="b" type="m">scheef</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Offer een Vis teef op haar <rhyme label="b" type="m" corresp="#s4:l2">keef</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En doe gaf ic de moet <rhyme label="a" type="f" corresp="#s4:l1">verlooren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En ick dacht is 't al van 't <rhyme label="c" type="m">mal</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die niet en wil, die niet en <rhyme label="c" type="m" corresp="#s4:l5">sal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Sette mijn Hertje op eene <rhyme label="a" type="f">Goegront</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">'k Settent Hylijcken uyt mijn <rhyme label="b" type="m">Zin</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En ick denck soo veel om de <rhyme label="b" type="m" corresp="#s5:l2">Min</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Even als om een gedroochde <rhyme label="a" type="f" corresp="#s5:l1">Koe-stront</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'k Settent Vrije, <rhyme label="c" type="m">Brassery</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Met al dat resje aen een <rhyme label="c" type="m" corresp="#s5:l5">zy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Vrijers, die gaen suchten, en <rhyme label="a" type="f">treuren</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Datser nyt sien als een magere <rhyme label="b" type="m">sprot</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Of een beschimmelde Rommel <rhyme label="b" type="m" corresp="#s6:l2">pot</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Denckt niet om de leuren en <rhyme label="a" type="f" corresp="#s6:l1">seuren</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Die de Meysjes u brengen int <rhyme label="c" type="m">Hooft</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Datse ie Sinnetjes niet en <rhyme label="c" type="m" corresp="#s6:l5">berooft</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'k Sagger gisteren die der sijn <rhyme label="a" type="f">Leden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Handen, en Voeten, en Hooft, en <rhyme label="b" type="m">Staert</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Heele Jaartjes hadde <rhyme label="b" type="m" corresp="#s7:l2">gespaert</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Om een moy Meysie die hy met <rhyme label="a" type="f" corresp="#s7:l1">eeden</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Vierichlijck Vrijden, en hadde <rhyme label="c" type="m">bemint</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Daarmen stoockt daer viertet <rhyme label="c" type="m" corresp="#s7:l5">Kint</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">En hy creeg de sack deur sen <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">En hy smeeter zijn trane <rhyme label="b" type="m">Bron</rhyme></l>
        <l id="s8:l3" met="4" type="+-">En hy Claegde soo veel als hy <rhyme label="b" type="m" corresp="#s8:l2">kon</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Snote traanen met heele <rhyme label="a" type="f" corresp="#s8:l1">stoopen</rhyme></l>
        <l id="s8:l5" met="4" type="+-">'t Was een deernis om te <rhyme label="c" type="m">sien</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Al was een Mensch soo Hart als <rhyme label="c" type="m" corresp="#s8:l5">stien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
