  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DIrckje maet ay wat een <rhyme label="a" type="m">pret</rhyme></l>
        <l id="s1:l2" met="4" type="+-">DIrckje maet ay wat een <rhyme label="a" type="m" corresp="#s1:l1">pret</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Ick lach mijn half slap <rhyme label="a" type="m" corresp="#s1:l1">byget</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Om de kluyt, om de <rhyme label="b" type="m">kluyt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">De kleuters hadden mijn <rhyme label="a" type="m" corresp="#s1:l1">beset</rhyme></l>
        <l id="s1:l6" met="2" type="-+">En riepen <rhyme label="b" type="m" corresp="#s1:l4">luyt</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wy hebben hem nou inde <rhyme label="a" type="m">kou</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Wy hebben hem nou inde <rhyme label="a" type="m" corresp="#s2:l1">kou</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Die deuntjes dicht van Mans en <rhyme label="a" type="m" corresp="#s2:l1">Vrou</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En oock van ongs, en oock van <rhyme label="b" type="m">ongs</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Hoe willen wy hem krabben <rhyme label="a" type="m" corresp="#s2:l1">nou</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Riep Juffrou <rhyme label="b" type="m" corresp="#s2:l4">mongs</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Aensicht quam ierst voor den <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Aensicht quam ierst voor den <rhyme label="a" type="m" corresp="#s3:l1">dach</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Ick schrickten doe k’die Meyt <rhyme label="a" type="m" corresp="#s3:l1">aensach</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En dit fatsoen, en dit <rhyme label="b" type="m">fatsoen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Die voerde dapper daer de <rhyme label="a" type="m" corresp="#s3:l1">vlach</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Met snappen <rhyme label="b" type="m" corresp="#s3:l4">doen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">En sey ghy overgheven <rhyme label="a" type="m">guyt</rhyme></l>
        <l id="s4:l2" met="4" type="+-">En sey ghy overgheven <rhyme label="a" type="m" corresp="#s4:l1">guyt</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Ick wed jou om een dicke <rhyme label="a" type="m" corresp="#s4:l1">Duyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Met munt en kruys, met munt en <rhyme label="b" type="m">kruys</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ick maeck dat ghy de deur comt <rhyme label="a" type="m" corresp="#s4:l1">uyt</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Al vant <rhyme label="b" type="m" corresp="#s4:l4">Stadthuys</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Voorts quammer noch een swart gat <rhyme label="a" type="m">in</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Voorts quammer noch een swart gat <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Al met een kromme kevel <rhyme label="a" type="m" corresp="#s5:l1">kin</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">De swarte meer // de swarte <rhyme label="b" type="m">meer</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Die kreech int kyven mee <rhyme label="a" type="m" corresp="#s5:l1">een</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Daer hadjet <rhyme label="b" type="m" corresp="#s5:l4">weer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Jou schelm jou dief jou <rhyme label="a" type="m">Ouwevaer</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Jou schelm jou dief jou <rhyme label="a" type="m" corresp="#s6:l1">Ouwevaer</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Com maeck my dat nou oock eens <rhyme label="a" type="m" corresp="#s6:l1">waer</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Dat ghy al rymt, dat ghy al <rhyme label="b" type="m">rymt</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Of ick sal jou ick sweer sie <rhyme label="a" type="m" corresp="#s6:l1">daer</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Slaen datje <rhyme label="b" type="m" corresp="#s6:l4">swymt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ick worde doen al vry wat <rhyme label="a" type="m">schuw</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Ick worde doen al vry wat <rhyme label="a" type="m" corresp="#s7:l1">schuw</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Ick kreech met ien een dof of <rhyme label="a" type="m" corresp="#s7:l1">duw</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Al van dit goet, al van dit <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Ick dacht by lo waer kruyp ick <rhyme label="a" type="m" corresp="#s7:l1">nu</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Heer stongh soo <rhyme label="b" type="m" corresp="#s7:l4">soet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ick nam de een en dan de <rhyme label="a" type="m">aer</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Ick nam de een en dan de <rhyme label="a" type="m" corresp="#s8:l1">aer</rhyme></l>
        <l id="s8:l3" met="4" type="+-">En leyde moytjes op men <rhyme label="a" type="m" corresp="#s8:l1">kaer</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Al dit ghespuys, al dit <rhyme label="b" type="m">ghespuys</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Doen quam de Moer en oock de <rhyme label="a" type="m" corresp="#s8:l1">Vaer</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Al van den <rhyme label="b" type="m" corresp="#s8:l4">Huys</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Het gingder doen soo hol o <rhyme label="a" type="m">bloet</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Het gingder doen soo hol o <rhyme label="a" type="m" corresp="#s9:l1">bloet</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Ick dacht ick raeck onder de <rhyme label="a" type="m" corresp="#s9:l1">voet</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dus laet ick gaen // dus laet ick <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Het sal myn wesen wel soo <rhyme label="a" type="m" corresp="#s9:l1">goet</rhyme></l>
        <l id="s9:l6" met="2" type="-+">Als blyven <rhyme label="b" type="m" corresp="#s9:l4">staen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">De handen raeckten inde <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s10:l2" met="4" type="+-">De handen raeckten inde <rhyme label="a" type="m" corresp="#s10:l1">sy</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Ja ja t’was doen ghedaen met <rhyme label="a" type="m" corresp="#s10:l1">my</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Ick koos veur keur, ick koos, <rhyme label="b" type="m">etc</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">En raeckten van dit goetjen <rhyme label="a" type="m" corresp="#s10:l1">vry</rhyme></l>
        <l id="s10:l6" met="2" type="-+">t’Gat vande <rhyme label="b" type="m" corresp="#s10:l4">deur</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Wat dunckje Dirckje van de <rhyme label="a" type="m">kluyt</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Wat dunckje Dirckje van de <rhyme label="a" type="m" corresp="#s11:l1">kluyt</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Ick peurden ras ten Huysen <rhyme label="a" type="m" corresp="#s11:l1">uyt</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">En speulde wip, en speulde <rhyme label="b" type="m">wip</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Ick dacht of ghy naet Vinckje <rhyme label="a" type="m" corresp="#s11:l1">fluyt</rhyme></l>
        <l id="s11:l6" met="2" type="-+">Tis uyt jou <rhyme label="b" type="m" corresp="#s11:l4">knip</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
