    <melody>Belgica loof de heer</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Schoen lief eerelick <rhyme label="a" type="m">befaemt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick souck u te beminne <rhyme label="b" type="f">met</rhyme></l>
        <l id="s1:l3" met="3" type="-+">met liefde <rhyme label="a" type="m" corresp="#s1:l1">ongeblaent</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Want gy staet heel in myn <rhyme label="b" type="f" corresp="#s1:l2">sinne</rhyme></l>
        <l id="s1:l5" met="3" type="-+">soe dat myn lyde <rhyme label="c" type="m">fel</rhyme></l>
        <l id="s1:l6" met="3" type="-+">nu komt bestryde <rhyme label="c" type="m" corresp="#s1:l5">snel</rhyme></l>
        <l id="s1:l7" met="2" type="-+">en truert <rhyme label="d" type="m">voerwaer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">om u reyn maecht <rhyme label="d" type="m" corresp="#s1:l7">eerbaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die orsaeck wat haest <rhyme label="a" type="m">voersint</rhyme></l>
        <l id="s2:l2" met="3" type="-+">waerom dat gy seer <rhyme label="b" type="f">spoedich</rhyme></l>
        <l id="s2:l3" met="3" type="-+">myn soe wel niet en <rhyme label="a" type="m" corresp="#s2:l1">bemint</rhyme></l>
        <l id="s2:l4" met="3" type="-+">als Ick u doe seer <rhyme label="b" type="f" corresp="#s2:l2">goedich</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Waer Ick van goede <rhyme label="c" type="m">veel</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daer toe van bloede <rhyme label="c" type="m" corresp="#s2:l5">eel</rhyme></l>
        <l id="s2:l7" met="2" type="-+">soe soude Ick <rhyme label="d" type="m">fyn</rhyme></l>
        <l id="s2:l8" met="3" type="-+">seer haest u minnaer <rhyme label="d" type="m" corresp="#s2:l7">syn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Men vraech nu ter tyt niet <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">nae persoen ofte nae <rhyme label="b" type="f">geslachte</rhyme></l>
        <l id="s3:l3" met="3" type="-+">noch oeck nae duecht of <rhyme label="a" type="m" corresp="#s3:l1">eer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">maer nae ryckdom ende <rhyme label="b" type="f" corresp="#s3:l2">machte</rhyme></l>
        <l id="s3:l5" met="3" type="-+">sy een seer groet van <rhyme label="c" type="m">hof</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dochter of Ionckman <rhyme label="c" type="m" corresp="#s3:l5">grof</rhyme></l>
        <l id="s3:l7" met="2" type="-+">liefde schint <rhyme label="d" type="m">vast</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">maer tende draecht de <rhyme label="d" type="m" corresp="#s3:l7">last</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die opt sweerels goedt <rhyme label="a" type="m">betrout</rhyme></l>
        <l id="s4:l2" met="3" type="-+">vint hem dickwils <rhyme label="b" type="f">bedroegen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">soemen daegelick <rhyme label="a" type="m" corresp="#s4:l1">aenschout</rhyme></l>
        <l id="s4:l4" met="3" type="-+">en syn mach <rhyme label="b" type="f" corresp="#s4:l2">ongeloegen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">want waer liefie Is <rhyme label="c" type="m">kranck</rhyme></l>
        <l id="s4:l6" met="3" type="-+">en krych noch een <rhyme label="c" type="m" corresp="#s4:l5">misganck</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Duer gelt of <rhyme label="d" type="m">schadt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">men mach beklaegen <rhyme label="d" type="m" corresp="#s4:l7">dat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ontfanck prinsesse <rhyme label="a" type="m">goedt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">van myn dit liet <rhyme label="b" type="f">gepreesen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">en nae myn lesse <rhyme label="a" type="m" corresp="#s5:l1">doet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">laet laet liefde syn <rhyme label="b" type="f" corresp="#s5:l2">gereesen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Want thart In Ionste <rhyme label="c" type="m">groeit</rhyme></l>
        <l id="s5:l6" met="3" type="-+">maeckt dat u Ionste <rhyme label="c" type="m" corresp="#s5:l5">bloeit</rhyme></l>
        <l id="s5:l7" met="2" type="-+">tot myn met <rhyme label="d" type="m">vlyt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Ick blyf getrou <rhyme label="d" type="m" corresp="#s5:l7">altyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
