    <melody>Ik heb de groene straten</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ick heb die groene <rhyme label="a" type="f">straete</rhyme></l>
        <l id="s1:l2" met="9" type="-+">soe dickmaels ten ende gegaen/ <rhyme label="b" type="m">Ick</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat Ick myn liefie moet <rhyme label="a" type="f" corresp="#s1:l1">laeten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat hebben myn vrienden <rhyme label="b" type="m" corresp="#s1:l2">gedaen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick en salse niet <rhyme label="a" type="f">voerlaeten</rhyme></l>
        <l id="s2:l2" met="9" type="-+">Al waeren sy noch soe gram/ <rhyme label="b" type="m">Ick</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ick salse gedachtich <rhyme label="a" type="f" corresp="#s2:l1">weesen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dien dach die Ick sterven <rhyme label="b" type="m" corresp="#s2:l2">sal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick stont op hooge <rhyme label="a" type="f">bergen</rhyme></l>
        <l id="s3:l2" met="9" type="-+">Ick sach te seewaert In/ <rhyme label="b" type="m">Ick</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ick sach een scheepie <rhyme label="a" type="f" corresp="#s3:l1">seilen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer wasser myn liefste <rhyme label="b" type="m" corresp="#s3:l2">In</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat trock hy van sin <rhyme label="a" type="f">hande</rhyme></l>
        <l id="s4:l2" met="9" type="-+">Van goude een <rhyme label="b" type="m">vingerlinck</rhyme>/</l>
        <l id="s4:l3" met="3" type="-+">wat hoe daer myn <rhyme label="a" type="f" corresp="#s4:l1">alderliefste</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer Isser de trouwe van <rhyme label="b" type="m" corresp="#s4:l2">myn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wat sou Ick met dat vingerlinck <rhyme label="a" type="f">doen</rhyme></l>
        <l id="s5:l2" met="9" type="-+">als Ick het niet draegen en mach/ <rhyme label="b" type="m">wat</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Secht dat gyt hebt <rhyme label="a" type="f" corresp="#s5:l1">gevonden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">by die koetges Int groene <rhyme label="b" type="m" corresp="#s5:l2">gras</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sou u vrou moeder <rhyme label="a" type="f">vraege</rhyme></l>
        <l id="s6:l2" met="9" type="-+">Wie u dat vingerlinck <rhyme label="b" type="m">gaf</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Antwoerse met soe <rhyme label="a" type="f" corresp="#s6:l1">woerde</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die eens siaers die liefste <rhyme label="b" type="m" corresp="#s6:l2">was</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Als alle die korsies <rhyme label="a" type="f">bloeyen</rhyme></l>
        <l id="s7:l2" met="9" type="-+">Sy rypen aen eenen <rhyme label="b" type="m">kant</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Soe sal Ick u soetelief <rhyme label="a" type="f" corresp="#s7:l1">trouwen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En neemen u byder <rhyme label="b" type="m" corresp="#s7:l2">handt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Als alle die korsies <rhyme label="a" type="f">rypen</rhyme></l>
        <l id="s8:l2" met="9" type="-+">sy rypen aen eenen <rhyme label="b" type="m">steel</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Soe sal Ick u soetelief <rhyme label="a" type="f" corresp="#s8:l1">trouwen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">nu noch nimmer <rhyme label="b" type="m" corresp="#s8:l2">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
