<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>52284</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags>
      <tag standard-name="zeemanslied" translation="sailor's song">zeemanslied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Laatstmaal ging ik ter jacht</melody>
    <categories>
      <category translation="work">werk</category>
      <category translation="emotions">emoties</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Eylaas wat droevig <rhyme label="a" type="m">lot</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Wat is my <rhyme label="b" type="f">overkomen</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Hoe kom ik dog zoo <rhyme label="a" type="m" corresp="#s1:l1">zot</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat ik heb dienst <rhyme label="b" type="f" corresp="#s1:l2">genomen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat ik op zoo een houten <rhyme label="c" type="m">klomp</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat ik op zoo een houten <rhyme label="c" type="m" corresp="#s1:l5">klomp</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Ging begeven myne <rhyme label="c" type="m" corresp="#s1:l5">romp</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik heb myn jonge <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En in myn lente <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Zoo meenigmaal <rhyme label="a" type="m" corresp="#s2:l1">gezyt</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Geen dienst kan myn <rhyme label="b" type="f" corresp="#s2:l2">behagen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Maar ik zie dat men niet <rhyme label="c" type="m">weet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Maar ik zie dat men niet <rhyme label="c" type="m" corresp="#s2:l5">weet</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Wat voor ramp ons staat <rhyme label="c" type="m" corresp="#s2:l5">gereet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Kan dan een weinig <rhyme label="a" type="m">goud</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Myn vryheid <rhyme label="b" type="f">overwinnen</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Wat of 't geld niet <rhyme label="a" type="m" corresp="#s3:l1">brouwt</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">Waar zyn myn vrye <rhyme label="b" type="f" corresp="#s3:l2">zinnen</rhyme>?</l>
        <l id="s3:l5" met="4" type="+-">Daar ik myn zie om 't <rhyme label="c" type="m">geld</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Daar ik myn zie om 't <rhyme label="c" type="m" corresp="#s3:l5">geld</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Aan een ysere boey <rhyme label="c" type="m" corresp="#s3:l5">geknelt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wie is 'er de oorzaak <rhyme label="a" type="m">van</rhyme>?</l>
        <l id="s4:l2" met="3" type="-+">Wie zal ik het <rhyme label="b" type="f">verwyten</rhyme>?</l>
        <l id="s4:l3" met="3" type="-+">De Liefde of de <rhyme label="a" type="m" corresp="#s4:l1">Kan</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Moet ik de schuld op <rhyme label="b" type="f" corresp="#s4:l2">smyten</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Want de Liefde en de <rhyme label="c" type="m">Wyn</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Want de Liefde en de <rhyme label="c" type="m" corresp="#s4:l5">Wyn</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Moet daar van de oorzaak <rhyme label="c" type="m" corresp="#s4:l5">zyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Aan Jajem en aan <rhyme label="a" type="m">Wyn</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Was ik te zeer <rhyme label="b" type="f">verbonden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ik dagt 't was <rhyme label="a" type="m" corresp="#s5:l1">Medicyn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dog ik heb <rhyme label="b" type="f" corresp="#s5:l2">ondervonden</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Dat het veel gebruyk daar <rhyme label="c" type="m">van</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dat het veel gebruyk daar <rhyme label="c" type="m" corresp="#s5:l5">van</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Aan de jonkheid schaade <rhyme label="c" type="m" corresp="#s5:l5">kan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Myn Moeders <rhyme label="a" type="m">evenbeeld</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Kon my te zeer <rhyme label="b" type="f">behagen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ik kon myn liefdens <rhyme label="a" type="m" corresp="#s6:l1">sweet</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Niet sterk genoeg <rhyme label="b" type="f" corresp="#s6:l2">verdragen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">En ik was niet wel te <rhyme label="c" type="m">vreên</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En ik was niet wel te <rhyme label="c" type="m" corresp="#s6:l5">vreên</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Als ik slapen moest <rhyme label="c" type="m" corresp="#s6:l5">alleen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Had ik na vrienden <rhyme label="a" type="m">raat</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wat meer gehoor <rhyme label="b" type="f">gegeven</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Dan had ik al dat <rhyme label="a" type="m" corresp="#s7:l1">kwaat</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wel van den hals <rhyme label="b" type="f" corresp="#s7:l2">gedreven</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Men dempt de put gemeenlyk <rhyme label="c" type="m">wis</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Men dempt de put gemeenlyk <rhyme label="c" type="m" corresp="#s7:l5">wis</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Als 't Kalf verdronken <rhyme label="c" type="m" corresp="#s7:l5">is</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Had ik van vis of <rhyme label="a" type="m">vlees</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Voor deze wel <rhyme label="b" type="f">gegeten</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Nu maar de been of <rhyme label="a" type="m" corresp="#s8:l1">graat</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Die ik heb weg <rhyme label="b" type="f" corresp="#s8:l2">gesmeten</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Ik vloog 'er aan met <rhyme label="c" type="m">appetyt</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Ik vloog 'er aan met <rhyme label="c" type="m" corresp="#s8:l5">appetyt</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Of het was het zoetst <rhyme label="c" type="m" corresp="#s8:l5">confyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Nu moet myn lekkere <rhyme label="a" type="m">bek</rhyme>:</l>
        <l id="s9:l2" met="3" type="-+">Met Gort en Snert <rhyme label="b" type="f">vervullen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wat Stokvis en wat <rhyme label="a" type="m" corresp="#s9:l1">Spek</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Is dat geen lekker <rhyme label="b" type="f" corresp="#s9:l2">smullen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Boter Kaas en <rhyme label="c" type="m">Scheepsbeschuyt</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Boter Kaas en <rhyme label="c" type="m" corresp="#s9:l5">Scheepsbeschuyt</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Daar toe wat slagen op myn <rhyme label="c" type="m" corresp="#s9:l5">huyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ik kon ook aan de <rhyme label="a" type="m">wal</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">De vreugd van ruste <rhyme label="b" type="f">rapen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">'k Wist van geen <rhyme label="a" type="m" corresp="#s10:l1">Overal</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Wanneer ik lag te <rhyme label="b" type="f" corresp="#s10:l2">slapen</rhyme>.</l>
        <l id="s10:l5" met="4" type="+-">'k Sliep op bed op myn <rhyme label="c" type="m">gemak</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">'k Sliep op bed op myn <rhyme label="c" type="m" corresp="#s10:l5">gemak</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Tot dat de lieve dag aan <rhyme label="c" type="m" corresp="#s10:l5">brak</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Komt nu den avond <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Ik hoor den klokke <rhyme label="b" type="f">Luyen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Men moet na kooy toe <rhyme label="a" type="m" corresp="#s11:l1">gaan</rhyme>;</l>
        <l id="s11:l4" met="3" type="-+">Maar komt 'er wind of <rhyme label="b" type="f" corresp="#s11:l2">buyen</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">'t Is aanstonds overal maak <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">'t Is aanstonds overal maak <rhyme label="c" type="m" corresp="#s11:l5">voort</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Zoo word men in zyn rust <rhyme label="c" type="m" corresp="#s11:l5">gestoord</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ik leefde aan de <rhyme label="a" type="m">wal</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ook altoos wel te <rhyme label="b" type="f">vreeden</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Ik wist van geen klip of <rhyme label="a" type="m" corresp="#s12:l1">bank</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Of andere <rhyme label="b" type="f" corresp="#s12:l2">zwarigheden</rhyme>;</l>
        <l id="s12:l5" met="4" type="+-">'k Was voor geen Oorlogs Vuur of <rhyme label="c" type="m">zwaart</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">'k Was voor geen Oorlogs Vuur of <rhyme label="c" type="m" corresp="#s12:l5">zwaart</rhyme>,</l>
        <l id="s12:l7" met="4" type="+-">Nooit bevreest maar wel <rhyme label="c" type="m" corresp="#s12:l5">bewaart</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Wie weet in welke <rhyme label="a" type="m">staat</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Wy verders zullen <rhyme label="b" type="f">komen</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Misschien nog scheel of <rhyme label="a" type="m" corresp="#s13:l1">mank</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Of schielyk weg <rhyme label="b" type="f" corresp="#s13:l2">genomen</rhyme>;</l>
        <l id="s13:l5" met="4" type="+-">Daar ik gezond en <rhyme label="c" type="m">welgemaakt</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Daar ik gezond en <rhyme label="c" type="m" corresp="#s13:l5">welgemaakt</rhyme>,</l>
        <l id="s13:l7" met="4" type="+-">Ben op het Oorlogs Schip <rhyme label="c" type="m" corresp="#s13:l5">geraakt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">In al myn <rhyme label="a" type="m">arremoed</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">In al myn bitter <rhyme label="b" type="f">lyden</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Kryg ik nog <rhyme label="a" type="m" corresp="#s14:l1">tegenspoed</rhyme>;</l>
        <l id="s14:l4" met="3" type="-+">Komt nu nog eenig <rhyme label="b" type="f" corresp="#s14:l2">byden</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">'t Is beter op een Schip het <rhyme label="c" type="m">lyf</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">'t Is beter op een Schip het <rhyme label="c" type="m" corresp="#s14:l5">lyf</rhyme>,</l>
        <l id="s14:l7" met="4" type="+-">Als getrouwt aan een kwaat <rhyme label="c" type="m" corresp="#s14:l5">wyf</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Gy Nederlanders <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Wy gaan nu voor u <rhyme label="b" type="f">stryden</rhyme>;</l>
        <l id="s15:l3" met="3" type="-+">Tot glorie van ons <rhyme label="a" type="m" corresp="#s15:l1">land</rhyme>!</l>
        <l id="s15:l4" met="3" type="-+">Om u zoo te <rhyme label="b" type="f" corresp="#s15:l2">bevryden</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Voor Kyzers snoode <rhyme label="c" type="m">dwingelandy</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">Voor Kyzers snoode <rhyme label="c" type="m" corresp="#s15:l5">dwingelandy</rhyme>,</l>
        <l id="s15:l7" met="4" type="+-">Om u te maken vrank en <rhyme label="c" type="m" corresp="#s15:l5">vry</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
