<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20436</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Voeg u aan de krans</melody>
  <author>B. Targier, Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">VOeght u aen de <rhyme label="a" type="m">Krans</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Van ons <rhyme label="a" type="m" corresp="#s1:l1">Dooden-dans</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Overleefde <rhyme label="a" type="m" corresp="#s1:l1">Mans</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Die kreupel <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Stel u stockjes <rhyme label="c" type="m">ree</rhyme></l>
        <l id="s1:l6" met="3" type="+-">En u kruckjes <rhyme label="c" type="m" corresp="#s1:l5">mee</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Die u oude <rhyme label="c" type="m" corresp="#s1:l5">wee</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Ons wijsen <rhyme label="b" type="m" corresp="#s1:l4">aen</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Hippel homp, homp, <rhyme label="d" type="m">homp</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Met een dwarsse <rhyme label="d" type="m" corresp="#s1:l9">stomp</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Laet u oude <rhyme label="d" type="m" corresp="#s1:l9">romp</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Ons Rey <rhyme label="e" type="m">versien</rhyme>,</l>
        <l id="s1:l13" met="3" type="+-">Yder stel hem <rhyme label="f" type="m">rat</rhyme></l>
        <l id="s1:l14" met="3" type="+-">Met zijn ouwe <rhyme label="f" type="m" corresp="#s1:l13">gat</rhyme>,</l>
        <l id="s1:l15" met="4" type="+-">En hy vat // nae de <rhyme label="g" type="m">schat</rhyme></l>
        <l id="s1:l16" met="2" type="-+">Dit wy hem <rhyme label="e" type="m" corresp="#s1:l12">bien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ouwe Wijfjes <rhyme label="a" type="m">kom</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Hippel mee <rhyme label="a" type="m" corresp="#s2:l1">rondtom</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Slae de <rhyme label="a" type="m" corresp="#s2:l1">Ketel-trom</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Tsa, dob, dob, <rhyme label="b" type="m">dob</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Tree al hinckend' <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Laetje spin-tuygh <rhyme label="c" type="m" corresp="#s2:l5">staen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Van u <rhyme label="c" type="m" corresp="#s2:l5">Tooverwaen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">En rijter <rhyme label="b" type="m" corresp="#s2:l4">op</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">Schoor u bonte <rhyme label="d" type="m">Rock</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Gheef u op de <rhyme label="d" type="m" corresp="#s2:l9">Stock</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Luy de swarte <rhyme label="d" type="m" corresp="#s2:l9">Klock</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">In Plutoos <rhyme label="e" type="m">Naem</rhyme>:</l>
        <l id="s2:l13" met="3" type="+-">Onse gantsche <rhyme label="f" type="m">Rey</rhyme>,</l>
        <l id="s2:l14" met="3" type="+-">Sal u doen <rhyme label="f" type="m" corresp="#s2:l13">gheley</rhyme>,</l>
        <l id="s2:l15" met="4" type="+-">Onder d'Aerdt // daer de <rhyme label="g" type="m">Waerdt</rhyme></l>
        <l id="s2:l16" met="2" type="-+">V acht <rhyme label="e" type="m" corresp="#s2:l12">bequaem</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Muffe Jonghmans <rhyme label="a" type="m">hey</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Eer ons Dansse <rhyme label="a" type="m" corresp="#s3:l1">schey</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">V berouwt <rhyme label="a" type="m" corresp="#s3:l1">gheley</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Begheeren <rhyme label="b" type="m">wy</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Brengh ons nu 't <rhyme label="c" type="m">ghemack</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Dat u troutje <rhyme label="c" type="m" corresp="#s3:l5">brack</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">In de <rhyme label="c" type="m" corresp="#s3:l5">Leere-sack</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Aen brockjes <rhyme label="b" type="m" corresp="#s3:l4">by</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Spoed' u loome <rhyme label="d" type="m">tredt</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Naer het doode <rhyme label="d" type="m" corresp="#s3:l9">bedt</rhyme>.</l>
        <l id="s3:l11" met="3" type="+-">In de kringh <rhyme label="d" type="m" corresp="#s3:l9">gheset</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Voor laet <rhyme label="e" type="m">berouw</rhyme>:</l>
        <l id="s3:l13" met="3" type="+-">Houwer vry wat <rhyme label="f" type="m">plaets</rhyme>,</l>
        <l id="s3:l14" met="3" type="+-">Voorje ouwe <rhyme label="f" type="m" corresp="#s3:l13">maets</rhyme>,</l>
        <l id="s3:l15" met="4" type="+-">Die de noodt // van de <rhyme label="g" type="m">doodt</rhyme></l>
        <l id="s3:l16" met="2" type="-+">Ons streckt een <rhyme label="e" type="m" corresp="#s3:l12">jou</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Kom al mee op 't <rhyme label="a" type="m">lest</rhyme></l>
        <l id="s4:l2" met="3" type="+-">En vervol de <rhyme label="a" type="m" corresp="#s4:l1">rest</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Ouwe Vrijsters, <rhyme label="a" type="m" corresp="#s4:l1">prest</rhyme></l>
        <l id="s4:l4" met="2" type="-+">V aen ons <rhyme label="b" type="m">zy</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Schoon u d'Aerdt <rhyme label="c" type="m">ontseydt</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Alle <rhyme label="c" type="m" corresp="#s4:l5">vruchtbaerheydt</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Yder is <rhyme label="c" type="m" corresp="#s4:l5">bereydt</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Wy staen u <rhyme label="b" type="m" corresp="#s4:l4">by</rhyme>:</l>
        <l id="s4:l9" met="3" type="+-">Zoo u Maeghdom <rhyme label="d" type="m">huylt</rhyme></l>
        <l id="s4:l10" met="3" type="+-">En in kreuckjes <rhyme label="d" type="m" corresp="#s4:l9">pruylt</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Gheef ons watter <rhyme label="d" type="m" corresp="#s4:l9">schuylt</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+">Wy smoorent <rhyme label="e" type="m">uyt</rhyme>,</l>
        <l id="s4:l13" met="3" type="+-">Hey! tsa wacker <rhyme label="f" type="m">aen</rhyme>,</l>
        <l id="s4:l14" met="3" type="+-">Laet nu <rhyme label="f" type="m" corresp="#s4:l13">ommegaen</rhyme></l>
        <l id="s4:l15" met="4" type="+-">Onse rey // heeft <rhyme label="g" type="m">gheley</rhyme>,</l>
        <l id="s4:l16" met="2" type="-+">Dus gaen wy <rhyme label="e" type="m" corresp="#s4:l12">uyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
