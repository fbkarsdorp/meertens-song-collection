<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179652</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GOd een God vol <rhyme label="a" type="f">Wonderheeden</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Die hy doet en heeft <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Het gaet boven ’s menschen <rhyme label="a" type="f" corresp="#s1:l1">reden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En geen schepzel kan <rhyme label="b" type="m" corresp="#s1:l2">verstaen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Alle Schepzels groot en <rhyme label="c" type="m">kleyn</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">In Gods handen nietig <rhyme label="c" type="m" corresp="#s1:l5">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Denk aen al de <rhyme label="a" type="f">Wonderheeden</rhyme></l>
        <l id="s2:l2" met="4" type="+-">In Egipten <rhyme label="b" type="m">voortgezet</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Aen het Onweer dat hy <rhyme label="a" type="f" corresp="#s2:l1">smeeden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">By het geven van zyn <rhyme label="b" type="m" corresp="#s2:l2">Wet</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Toen hy viel op Sina <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Als een schriklyk <rhyme label="c" type="m" corresp="#s2:l5">Opper-heer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doet de Schrift het ons niet <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">’t Oordeel op een Adams <rhyme label="b" type="m">val</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Aen een Babels stoute <rhyme label="a" type="f" corresp="#s3:l1">Tooren</rhyme>?</l>
        <l id="s3:l4" met="4" type="+-">Die neerstord met en grond <rhyme label="b" type="m" corresp="#s3:l2">wal</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Om de trotze <rhyme label="c" type="m">hovaerdy</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Dat het raekte in de <rhyme label="c" type="m" corresp="#s3:l5">ly</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Corach en zyn Maets <rhyme label="a" type="f">beneeden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Zonken neder in der <rhyme label="b" type="m">Aerd</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om haer <rhyme label="a" type="f" corresp="#s4:l1">Wederspannigheden</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Waren zy Gods Tooren <rhyme label="b" type="m" corresp="#s4:l2">waerd</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En zyne gedugte <rhyme label="c" type="m">magt</rhyme>!</l>
        <l id="s4:l6" met="4" type="+-">Toond alom zyn groote <rhyme label="c" type="m" corresp="#s4:l5">kragt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Al de wyze <rhyme label="a" type="f">handelweegen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Die de Godheid houd <rhyme label="b" type="m">beneen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Dan eens vuur en zulpher <rhyme label="a" type="f" corresp="#s5:l1">Reegen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">doet hy komen op ’t <rhyme label="b" type="m" corresp="#s5:l2">Gemeen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">En de straf op boos en <rhyme label="c" type="m">goed</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">hy de hemel eenig <rhyme label="c" type="m" corresp="#s5:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Schuddingen op Aerd <rhyme label="a" type="f">beneeden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Groot en ongemeene <rhyme label="b" type="m">vloed</rhyme>!</l>
        <l id="s6:l3" met="4" type="+-">hy de hemel veelmael <rhyme label="a" type="f" corresp="#s6:l1">deeden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Koude Winters, <rhyme label="b" type="m" corresp="#s6:l2">Tegenspoet</rhyme>!</l>
        <l id="s6:l5" met="4" type="+-">Gaf de Hemel door zyn <rhyme label="c" type="m">Hand</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Veelmael in ons <rhyme label="c" type="m" corresp="#s6:l5">Nederland</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Als men leest Godfrieds <rhyme label="a" type="f">Kronyken</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Net Gedrukt met Plaet en <rhyme label="b" type="m">Print</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Ziet men veel Oordeelen <rhyme label="a" type="f" corresp="#s7:l1">blyken</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Waer door Mensch en Vee <rhyme label="b" type="m" corresp="#s7:l2">verslind</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">God heeft Wond’ren in zyn <rhyme label="c" type="m">Hand</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Als hy zyne Vierschaer <rhyme label="c" type="m" corresp="#s7:l5">spant</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Deeze Maend doet ons <rhyme label="a" type="f">beleeven</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Uit de Tyding van de <rhyme label="b" type="m">Crand</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Wat de Hemel ons kan <rhyme label="a" type="f" corresp="#s8:l1">geven</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Door de grootheid van zyn <rhyme label="b" type="m" corresp="#s8:l2">Hand</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">En wen hy zyn Almagt <rhyme label="c" type="m">toont</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Werd nog Oud, nog Jong <rhyme label="c" type="m" corresp="#s8:l5">verschoont</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Men hoord uit de Stad van <rhyme label="a" type="f">Cleve</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Van een Zoogenaemde <rhyme label="b" type="m">Vogt</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Die den Hemel ons kwam <rhyme label="a" type="f" corresp="#s9:l1">geven</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Daer hy hun aldaer <rhyme label="b" type="m" corresp="#s9:l2">bezogt</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Met een Regen, Rood als <rhyme label="c" type="m">Bloed</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dat ontzette het <rhyme label="c" type="m" corresp="#s9:l5">Gemoed</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Ook zoo meld de Crand van <rhyme label="a" type="f">Reenen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Agt Uur van Uregt de <rhyme label="b" type="m">Stad</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Daer deez’ Regen is <rhyme label="a" type="f" corresp="#s10:l1">verscheenen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Dat de reden heeft <rhyme label="b" type="m" corresp="#s10:l2">gevat</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">En waer over veele <rhyme label="c" type="m">reen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Zyn gevoert van groot en <rhyme label="c" type="m" corresp="#s10:l5">kleen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Wat deez’ Regen zal <rhyme label="a" type="f">beduyden</rhyme>:</l>
        <l id="s11:l2" met="4" type="+-">Oorlog, Sterfte, <rhyme label="b" type="m">Hongersnoot</rhyme>!</l>
        <l id="s11:l3" met="4" type="+-">Geven stoffe aen veel <rhyme label="a" type="f" corresp="#s11:l1">Luyden</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Van gevaren veel en <rhyme label="b" type="m" corresp="#s11:l2">groot</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Best dat yder zwyge <rhyme label="c" type="m">stil</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Want de Hemel doet zyn <rhyme label="c" type="m" corresp="#s11:l5">wil</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Yder val tog in Gods <rhyme label="a" type="f">Hoede</rhyme>!</l>
        <l id="s12:l2" met="4" type="+-">Leg de Handen op de <rhyme label="b" type="m">Mond</rhyme>;</l>
        <l id="s12:l3" met="4" type="+-">Myd het quade, zoek het <rhyme label="a" type="f" corresp="#s12:l1">goede</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">En hy laete af van <rhyme label="b" type="m" corresp="#s12:l2">zond</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Met een Bede dat ons <rhyme label="c" type="m">Godt</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Alle geef een Zalig <rhyme label="c" type="m" corresp="#s12:l5">Lot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
