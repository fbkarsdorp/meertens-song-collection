<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180708</id>
    <date>
      <year-start>1688</year-start>
      <year-end>1688</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Amo te benigne Jesu</melody>
  <author>Philippus van Sorgen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Eeuwig, Heylig Hemels <rhyme label="a" type="f">Koning</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die door u groot <rhyme label="b" type="m">beleyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Schiept den Hemel tot u <rhyme label="a" type="f" corresp="#s1:l1">woning</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En al u Scheps’len <rhyme label="b" type="m" corresp="#s1:l2">leyt</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Gy regeert in ‘t Aerdsche <rhyme label="c" type="m">dal</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Door u Wijsheyt over <rhyme label="c" type="m" corresp="#s1:l5">al</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s1:l7">Heyligheyt</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s1:l7">zijt</rhyme>.</l>
        <l id="s1:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s1:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Uw’ Wijsheyt voegt ons hier <rhyme label="a" type="f">t’same</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En ons u wijsheyt <rhyme label="b" type="m">scheyt</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Soo dat wonder is u <rhyme label="a" type="f" corresp="#s2:l1">Name</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En wonder u <rhyme label="b" type="m" corresp="#s2:l2">beleyt</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Al wat uwe wijsheyt <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Zy dog in ons’ oogen <rhyme label="c" type="m" corresp="#s2:l5">goet</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">Heyliogheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Heyliogheyt, <rhyme label="d" type="m" corresp="#s2:l7">Heyligheyt</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s2:l7">zijt</rhyme>.</l>
        <l id="s2:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s2:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als wy met ons Lighaam <rhyme label="a" type="f">scheyden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Schept Heere nimmer <rhyme label="b" type="m">op</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Wilt ons door u Geest <rhyme label="a" type="f" corresp="#s3:l1">geleyden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Blijft ons dog altijt <rhyme label="b" type="m" corresp="#s3:l2">by</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Send u Engel voor ons <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat ons quaat aantreffe <rhyme label="c" type="m" corresp="#s3:l5">geen</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s3:l7">Heyligheyt</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s3:l7">zijt</rhyme>.</l>
        <l id="s3:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s3:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Of wy blijven, of wy <rhyme label="a" type="f">scheyden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gy Iesu zijt ons <rhyme label="b" type="m">heyl</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hier in wy ons steeds <rhyme label="a" type="f" corresp="#s4:l1">verblijden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Want wy zijn in u <rhyme label="b" type="m" corresp="#s4:l2">veyl</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Waar dat gy ons lighaam <rhyme label="c" type="m">voert</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">In u zijn wy <rhyme label="c" type="m" corresp="#s4:l5">onberoert</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s4:l7">Heyligheyt</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s4:l7">zijt</rhyme>.</l>
        <l id="s4:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s4:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Iesu, blijft met u <rhyme label="a" type="f">genade</rhyme></l>
        <l id="s5:l2" met="3" type="-+">U Lievelingen <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Laatse in u vroeg en <rhyme label="a" type="f" corresp="#s5:l1">spade</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dog altijt wesen <rhyme label="b" type="m" corresp="#s5:l2">bly</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Leert haar dat is <rhyme label="c" type="m">Majesteyt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Al u doen en <rhyme label="c" type="m" corresp="#s5:l5">Heerlikheyt</rhyme>:</l>
        <l id="s5:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s5:l7">Heyligheyt</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s5:l7">zijt</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s5:l7">zijt</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Iesu, wilt gy voor haar <rhyme label="a" type="f">sorgen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Woont altijt in haar <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Laatse in u zijn <rhyme label="a" type="f" corresp="#s6:l1">verborgen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat haar geen vrees <rhyme label="b" type="m" corresp="#s6:l2">benert</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Iesu, grijpt haar Zielen <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Doets’ in brand van liefde <rhyme label="c" type="m" corresp="#s6:l5">staan</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s6:l7">Heyligheyt</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s6:l7">zijt</rhyme>.</l>
        <l id="s6:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s6:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Iesus soo de Hel haar <rhyme label="a" type="f">kaken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Tegens haar spede <rhyme label="b" type="m">op</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Doet haer dog in iver <rhyme label="a" type="f" corresp="#s7:l1">blaken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Veetreed den Slang den <rhyme label="b" type="m" corresp="#s7:l2">kop</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Iesus maakt (gelijk gy <rhyme label="c" type="m">pleegt</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Al u Kind’ren <rhyme label="c" type="m" corresp="#s7:l5">onbeweegt</rhyme>)</l>
        <l id="s7:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s7:l7">Heyligheyt</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s7:l7">zijt</rhyme>.</l>
        <l id="s7:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s7:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Brengt ons in die Stadt hier <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En in u <rhyme label="b" type="m">Heyligdom</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daar wy steeds u sullen <rhyme label="a" type="f" corresp="#s8:l1">loven</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Prijsen, en leven, <rhyme label="b" type="m" corresp="#s8:l2">om</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Iuychende den lof en <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Toe te schrijven u, ô <rhyme label="c" type="m" corresp="#s8:l5">Heer</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s8:l7">Heyligheyt</rhyme>,</l>
        <l id="s8:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s8:l7">zijt</rhyme>.</l>
        <l id="s8:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s8:l7">zijt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hier onnut, dog nauw aan <rhyme label="a" type="f">Iesus</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En hier gantz <rhyme label="b" type="m">veragt</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Hier verjaagt en dat om <rhyme label="a" type="f" corresp="#s9:l1">Iesus</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Hier na in eer <rhyme label="b" type="m" corresp="#s9:l2">gebragt</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Iesus is ons eer, ons <rhyme label="c" type="m">kragt</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Iesus is ons stercke <rhyme label="c" type="m" corresp="#s9:l5">magt</rhyme>!</l>
        <l id="s9:l7" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m">Heyligheyt</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Heyligheyt, <rhyme label="d" type="m" corresp="#s9:l7">Heyligheyt</rhyme>,</l>
        <l id="s9:l9" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s9:l7">zijt</rhyme>.</l>
        <l id="s9:l10" met="4" type="+-">Al ons sugt en wens gy <rhyme label="d" type="m" corresp="#s9:l7">zijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
