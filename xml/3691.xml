    <melody>Psalm 088 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEER die my duslangh' hebt <rhyme label="a" type="m">behoedt</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">Ick roep' altyt met groot <rhyme label="b" type="f">verlanghen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Laet tot v kommen, wilt <rhyme label="b" type="f" corresp="#s1:l2">ontfanghen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Myn ghebedt wt ghenaeden <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wilt vriendelick neyghen v' <rhyme label="c" type="f">ooren</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">End myn ghestaedigh klaeghen <rhyme label="c" type="f" corresp="#s1:l5">hooren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want myn siel' is vol angst end <rhyme label="a" type="m">noot</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ter hellen is ghedaelt myn <rhyme label="b" type="f">leuen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ick magh met hen wel sijn <rhyme label="b" type="f" corresp="#s2:l2">gheschreuen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die t'ondergaen doer lyden <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Men magh my hem wel ghelyck <rhyme label="c" type="f">achten</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die hulpeloos is, sonder <rhyme label="c" type="f" corresp="#s2:l5">krachten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick ben onder den dooden <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Versteken end oock gantsch <rhyme label="b" type="f">verlaeten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Als een die vermoort is op <rhyme label="b" type="f" corresp="#s3:l2">straeten</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dien men seer haest begraeuen <rhyme label="a" type="m" corresp="#s3:l1">sal</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Want ghy ghedenckt myner niet <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">V' handt hebt ghy afghekeert <rhyme label="c" type="f" corresp="#s3:l5">seere</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy werpt my diep' in den <rhyme label="a" type="m">afgrondt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die duyster end diep' is, met <rhyme label="b" type="f">suchten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">V' gramschap die seer is te <rhyme label="b" type="f" corresp="#s4:l2">vruchten</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Druckt my seer hart tot deser <rhyme label="a" type="m" corresp="#s4:l1">stondt</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Ghy ouervalt my met die <rhyme label="c" type="f">roeden</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">End met toorn' als met <rhyme label="c" type="f" corresp="#s4:l5">watervloeden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mynen vrienden ben ick <rhyme label="a" type="m">ghemaeckt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Vremdt end veracht, jae een <rhyme label="b" type="f">afgrysen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Met vingheren nae my sy <rhyme label="b" type="f" corresp="#s5:l2">wysen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ick ben ouervallen, gaer <rhyme label="a" type="m" corresp="#s5:l1">naeckt</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Alle middlen sijn my <rhyme label="c" type="f">benomen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Wt den banden kan ick niet <rhyme label="c" type="f" corresp="#s5:l5">komen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Door t'kruys is myn ghesicht <rhyme label="a" type="m">ontstelt</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Ick bidd' v daeghlicks Heer <rhyme label="b" type="f">almachtigh</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">End streck' wt steedts myn handen <rhyme label="b" type="f" corresp="#s6:l2">klachtigh</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Wanneer toont ghy eens v' <rhyme label="a" type="m" corresp="#s6:l1">ghewelt</rhyme>?</l>
        <l id="s6:l5" met="4" type="-+">Wanneer sullen wy doch <rhyme label="c" type="f">aenmercken</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Die schier doodt sijn, v' <rhyme label="c" type="f" corresp="#s6:l5">wonderwercken</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sullen die dooden hervoort <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">End v' wonderdaeden <rhyme label="b" type="f">vermonden</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">End uwer goetheit niet om <rhyme label="b" type="f" corresp="#s7:l2">gronden</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">In haeren graeuendoen <rhyme label="a" type="m" corresp="#s7:l1">vermaen</rhyme>?</l>
        <l id="s7:l5" met="4" type="-+">Sullen se van v' waerheit <rhyme label="c" type="f">spreken</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Die doodt sijnde noch in't graf <rhyme label="c" type="f" corresp="#s7:l5">steken</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Werden uwe wondren <rhyme label="a" type="m">bedacht</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">In't duyster daer men niets kan <rhyme label="b" type="f">weten</rhyme>?</l>
        <l id="s8:l3" met="4" type="-+">Sal men daer all' dingh wert <rhyme label="b" type="f" corresp="#s8:l2">vergheten</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Op v'gherechtheit hebben <rhyme label="a" type="m" corresp="#s8:l1">acht</rhyme>?</l>
        <l id="s8:l5" met="4" type="-+">Nochtans bidd' ick v Heer <rhyme label="c" type="f">ghestaedigh</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Van s'morghens vroegh, weest my <rhyme label="c" type="f" corresp="#s8:l5">genaedigh</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Waerom O Heer verstoott ghy <rhyme label="a" type="m">my</rhyme>?</l>
        <l id="s9:l2" met="4" type="-+">Waerom is v' aensicht <rhyme label="b" type="f">verborghen</rhyme>?</l>
        <l id="s9:l3" met="4" type="-+">Van der jonckheit ben ick vol <rhyme label="b" type="f" corresp="#s9:l2">sorghen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Duysentfout beangst seer <rhyme label="a" type="m" corresp="#s9:l1">onvry</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Ick ben doer schricken gantsch <rhyme label="c" type="f">verslaeghen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Benauwt vol vreese met <rhyme label="c" type="f" corresp="#s9:l5">vertzaeghen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Vwe gramschap ouer my <rhyme label="a" type="m">gaet</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Vwe verschrickinghen my <rhyme label="b" type="f">drucken</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Waterstroomen my gantsch wegh <rhyme label="b" type="f" corresp="#s10:l2">rucken</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">In desen seer benauwden <rhyme label="a" type="m" corresp="#s10:l1">staet</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Alles wat my angst kan <rhyme label="c" type="f">toebringhen</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Wil my nu al met een <rhyme label="c" type="f" corresp="#s10:l5">omringhen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ghy hebt van my ghedaen seer <rhyme label="a" type="m">wydt</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Myn beste vrienden al met <rhyme label="b" type="f">eene</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Myn secreetste vrienden <rhyme label="b" type="f" corresp="#s11:l2">ghemeene</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Sijn wydt van my in desen <rhyme label="a" type="m" corresp="#s11:l1">strydt</rhyme>.</l>
        <l id="s11:l5" met="4" type="-+">In alle myn swaere <rhyme label="c" type="f">elenden</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Van my wycken se die my <rhyme label="c" type="f" corresp="#s11:l5">kenden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
