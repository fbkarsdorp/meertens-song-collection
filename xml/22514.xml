  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wt harteliker <rhyme label="a" type="f">gheren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">uut rechter <rhyme label="b" type="m">vrolicheit</rhyme></l>
        <l id="s1:l3" met="3" type="-+">so wil wi <rhyme label="a" type="f" corresp="#s1:l1">iubileren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">mit rechter <rhyme label="b" type="m" corresp="#s1:l2">soeticheit</rhyme></l>
        <l id="s1:l5" met="4" type="-+">al wair mijn leit al hier <rhyme label="b" type="m" corresp="#s1:l2">verbreit</rhyme></l>
        <l id="s1:l6" met="3" type="-+">in sorghen ende in <rhyme label="a" type="f" corresp="#s1:l1">seren</rhyme></l>
        <l id="s1:l7" met="3" type="-+">ten is gheen <rhyme label="b" type="m" corresp="#s1:l2">ewicheit</rhyme></l>
        <l id="s1:l8" met="4" type="-+">tis dic gheseit een goet <rhyme label="b" type="m" corresp="#s1:l2">verbeit</rhyme></l>
        <l id="s1:l9" met="3" type="+-">dat staet te prisen <rhyme label="a" type="f" corresp="#s1:l1">meren</rhyme></l>
        <l id="s1:l10" met="3" type="-+">dan grote <rhyme label="b" type="m" corresp="#s1:l2">haesticheit</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">War om so wil ic <rhyme label="a" type="f">truren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">mit sorghen sijn <rhyme label="b" type="m">beuaen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">stont ic ter <rhyme label="a" type="f" corresp="#s2:l1">auonturen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">als menich heeft <rhyme label="b" type="m" corresp="#s2:l2">ghedaen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">so mocht ic saen mi seer <rhyme label="b" type="m" corresp="#s2:l2">verslaen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">ende duchten tallen <rhyme label="a" type="f" corresp="#s2:l1">vren</rhyme></l>
        <l id="s2:l7" met="3" type="-+">dat hi mi mocht <rhyme label="b" type="m" corresp="#s2:l2">ontgaen</rhyme></l>
        <l id="s2:l8" met="4" type="-+">op wien ic haen so lang <rhyme label="b" type="m" corresp="#s2:l2">gestaen</rhyme></l>
        <l id="s2:l9" met="3" type="+-">dien ic uut minnen <rhyme label="a" type="f" corresp="#s2:l1">puren</rhyme></l>
        <l id="s2:l10" met="3" type="-+">so hartelic heb <rhyme label="b" type="m" corresp="#s2:l2">omvaen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">In desen stae ic <rhyme label="a" type="f">vride</rhyme></l>
        <l id="s3:l2" met="3" type="-+">bevreet in tharte <rhyme label="b" type="m">mijn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">ten sal niet langhe <rhyme label="a" type="f" corresp="#s3:l1">lide</rhyme></l>
        <l id="s3:l4" met="3" type="-+">ic sal verlichtet <rhyme label="b" type="m" corresp="#s3:l2">sijn</rhyme></l>
        <l id="s3:l5" met="4" type="-+">uut gods aenscijn dair putmen <rhyme label="b" type="m" corresp="#s3:l2">wijn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">men drinct tot allen <rhyme label="a" type="f" corresp="#s3:l1">tide</rhyme></l>
        <l id="s3:l7" met="3" type="-+">ende nymmermeer vol te <rhyme label="b" type="m" corresp="#s3:l2">sijn</rhyme></l>
        <l id="s3:l8" met="4" type="-+">na corte pijn so lang <rhyme label="b" type="m" corresp="#s3:l2">termijn</rhyme></l>
        <l id="s3:l9" met="3" type="+-">te wesen altijt <rhyme label="a" type="f" corresp="#s3:l1">blide</rhyme></l>
        <l id="s3:l10" met="3" type="-+">verslonden in vroechden <rhyme label="b" type="m" corresp="#s3:l2">sijn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Lof si god van hier <rhyme label="a" type="f">bouen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">al inder <rhyme label="b" type="m">ewicheit</rhyme></l>
        <l id="s4:l3" met="3" type="-+">dien al die enghelen <rhyme label="a" type="f" corresp="#s4:l1">louen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">mit groter <rhyme label="b" type="m" corresp="#s4:l2">nersticheit</rhyme></l>
        <l id="s4:l5" met="4" type="-+">die al mijn leit verdreuen <rhyme label="b" type="m" corresp="#s4:l2">heit</rhyme></l>
        <l id="s4:l6" met="3" type="-+">so veer van mi <rhyme label="a" type="f" corresp="#s4:l1">verscouen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">verwandelt in <rhyme label="b" type="m" corresp="#s4:l2">soeticheit</rhyme></l>
        <l id="s4:l8" met="4" type="-+">o triniteit daert al aen <rhyme label="b" type="m" corresp="#s4:l2">steit</rhyme></l>
        <l id="s4:l9" met="3" type="+-">noed ons tot dynen <rhyme label="a" type="f" corresp="#s4:l1">houen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">der ewigher <rhyme label="b" type="m" corresp="#s4:l2">salicheit</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoe vast hout mi <rhyme label="a" type="f">geuanghen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">dij min o edel <rhyme label="b" type="m">greyn</rhyme></l>
        <l id="s5:l3" met="3" type="-+">laet mi dijn troest <rhyme label="a" type="f" corresp="#s5:l1">onfanghen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">onsluyt die soete <rhyme label="b" type="m" corresp="#s5:l2">fonteyn</rhyme></l>
        <l id="s5:l5" met="4" type="-+">die mi alleyn mach maken <rhyme label="b" type="m" corresp="#s5:l2">reyn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">ende corten swaer <rhyme label="a" type="f" corresp="#s5:l1">verlanghen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">mit v te sijn <rhyme label="b" type="m" corresp="#s5:l2">ghemeyn</rhyme></l>
        <l id="s5:l8" met="4" type="-+">o edel eyn der minnen <rhyme label="b" type="m" corresp="#s5:l2">screyn</rhyme></l>
        <l id="s5:l9" met="3" type="+-">ghi maect mijn hert so <rhyme label="a" type="f" corresp="#s5:l1">banghen</rhyme></l>
        <l id="s5:l10" met="3" type="-+">och eyn ende anders <rhyme label="b" type="m" corresp="#s5:l2">gheyn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Een tijt was hier te <rhyme label="a" type="f">voren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">dien ic in corter <rhyme label="b" type="m">stont</rhyme></l>
        <l id="s6:l3" met="3" type="-+">so deerlic heb <rhyme label="a" type="f" corresp="#s6:l1">verloren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">des bin ic seer <rhyme label="b" type="m" corresp="#s6:l2">ghewont</rhyme></l>
        <l id="s6:l5" met="4" type="-+">in thertse gront als god <rhyme label="b" type="m" corresp="#s6:l2">orcont</rhyme></l>
        <l id="s6:l6" met="3" type="-+">van eenre maecht <rhyme label="a" type="f" corresp="#s6:l1">gheboren</rhyme></l>
        <l id="s6:l7" met="3" type="-+">der heelre werelt <rhyme label="b" type="m" corresp="#s6:l2">ront</rhyme></l>
        <l id="s6:l8" met="4" type="-+">o edel font maec mi <rhyme label="b" type="m" corresp="#s6:l2">gesont</rhyme></l>
        <l id="s6:l9" met="3" type="+-">op dat ic werd <rhyme label="a" type="f" corresp="#s6:l1">vercoren</rhyme></l>
        <l id="s6:l10" met="3" type="-+">daer ghijt al naeste <rhyme label="b" type="m" corresp="#s6:l2">gront</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laet ons mit hert ende <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">mit vroechden ende <rhyme label="b" type="m">iolijt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ons keren tsijnre <rhyme label="a" type="f" corresp="#s7:l1">minnen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">in onser iongher <rhyme label="b" type="m" corresp="#s7:l2">tijt</rhyme></l>
        <l id="s7:l5" met="4" type="-+">die ver ende wijt elc hert <rhyme label="b" type="m" corresp="#s7:l2">verblijt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">ontfonct mit <rhyme label="a" type="f" corresp="#s7:l1">seraphinnen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">die ons so wel <rhyme label="b" type="m" corresp="#s7:l2">beurijt</rhyme></l>
        <l id="s7:l8" met="4" type="-+">voer thelsche spijt dat ons <rhyme label="b" type="m" corresp="#s7:l2">benijt</rhyme></l>
        <l id="s7:l9" met="3" type="+-">dat wi ter hogher <rhyme label="a" type="f" corresp="#s7:l1">tinnen</rhyme></l>
        <l id="s7:l10" met="3" type="-+">der glorien sijn <rhyme label="b" type="m" corresp="#s7:l2">ghedijt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Mijn hert sin siel mit <rhyme label="a" type="f">ene</rhyme></l>
        <l id="s8:l2" met="3" type="-+">ende al mijn erflic <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s8:l3" met="3" type="-+">dat ic ontfinc te <rhyme label="a" type="f" corresp="#s8:l1">lene</rhyme></l>
        <l id="s8:l4" met="3" type="-+">van minen troester <rhyme label="b" type="m" corresp="#s8:l2">soet</rhyme></l>
        <l id="s8:l5" met="4" type="-+">dat ic ter spoet hier laten <rhyme label="b" type="m" corresp="#s8:l2">moet</rhyme></l>
        <l id="s8:l6" met="3" type="-+">ic gheeft hem al <rhyme label="a" type="f" corresp="#s8:l1">ghemene</rhyme></l>
        <l id="s8:l7" met="3" type="-+">wiens troest mi hopen <rhyme label="b" type="m" corresp="#s8:l2">doet</rhyme></l>
        <l id="s8:l8" met="4" type="-+">op sijn oetmoet die voer mi <rhyme label="b" type="m" corresp="#s8:l2">stoet</rhyme></l>
        <l id="s8:l9" met="3" type="+-">aen tcruys gerect mit <rhyme label="a" type="f" corresp="#s8:l1">wene</rhyme></l>
        <l id="s8:l10" met="3" type="-+">so iammerlijc <rhyme label="b" type="m" corresp="#s8:l2">bebloet</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ut hem is al mijn <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">mijn hoep mijn <rhyme label="b" type="m">toeuerlaet</rhyme></l>
        <l id="s9:l3" met="3" type="-+">aen hem bin icks <rhyme label="a" type="f" corresp="#s9:l1">gebleuen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">die mi niet af en <rhyme label="b" type="m" corresp="#s9:l2">gaet</rhyme></l>
        <l id="s9:l5" met="4" type="-+">noch scae noch baet en <rhyme label="b" type="m" corresp="#s9:l2">valscht</rhyme></l>
        <l id="s9:l6" met="3" type="-+">die also vast gescreuen sijn <rhyme label="a" type="f" corresp="#s9:l1">maet</rhyme></l>
        <l id="s9:l7" met="3" type="-+">in mijnre harten <rhyme label="b" type="m" corresp="#s9:l2">staet</rhyme></l>
        <l id="s9:l8" met="4" type="-+">die vroech ende spaet elx hert <rhyme label="b" type="m" corresp="#s9:l2">beraet</rhyme></l>
        <l id="s9:l9" met="3" type="+-">hi sal mi niet <rhyme label="a" type="f" corresp="#s9:l1">begheuen</rhyme></l>
        <l id="s9:l10" met="3" type="-+">die hemel ende aerd <rhyme label="b" type="m" corresp="#s9:l2">versaet</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Stelt v mijns raets te <rhyme label="a" type="f">pleghen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">ic sal v raden <rhyme label="b" type="m">wel</rhyme></l>
        <l id="s10:l3" met="3" type="-+">van als te sijn <rhyme label="a" type="f" corresp="#s10:l1">onsleghen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">mit iesu vrientscap <rhyme label="b" type="m" corresp="#s10:l2">stel</rhyme></l>
        <l id="s10:l5" met="4" type="-+">mit hem versel ende nymont <rhyme label="b" type="m" corresp="#s10:l2">el</rhyme></l>
        <l id="s10:l6" met="3" type="-+">den bant mit hem te <rhyme label="a" type="f" corresp="#s10:l1">dreghen</rhyme></l>
        <l id="s10:l7" met="3" type="-+">dat is een vrolic <rhyme label="b" type="m" corresp="#s10:l2">spel</rhyme></l>
        <l id="s10:l8" met="4" type="-+">die tijt is snel die doot is <rhyme label="b" type="m" corresp="#s10:l2">fel</rhyme></l>
        <l id="s10:l9" met="3" type="+-">reyt v ghi moet van <rhyme label="a" type="f" corresp="#s10:l1">weghen</rhyme></l>
        <l id="s10:l10" met="3" type="-+">vreest v voor thelsche <rhyme label="b" type="m" corresp="#s10:l2">quel</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
