    <tags/>
    <melody>Wanneer ik slaap</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Wilt doch o Godt, die door u stercke <rhyme label="a" type="m">cracht</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Al watmen siet uyt niet hebt <rhyme label="a" type="m" corresp="#s1:l1">voortghebracht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dit paer vroech ende <rhyme label="b" type="m">laet</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">In desen <rhyme label="b" type="m" corresp="#s1:l3">staet</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Gheven niet dan <rhyme label="c" type="m">vreucht</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Sonder <rhyme label="c" type="m" corresp="#s1:l5">ongheneucht</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Gheven niet dan <rhyme label="c" type="m" corresp="#s1:l5">vreucht</rhyme></l>
        <l id="s1:l8" met="1" type="-+"><rhyme label="d" type="f">Gestadich</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">O Godt o goede Godt <rhyme label="d" type="f" corresp="#s1:l8">ghenadich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Behoedt haer Heer door uwe milde <rhyme label="a" type="m">handr</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Voor ongheval, voor smaetheydt ende <rhyme label="a" type="m" corresp="#s2:l1">schandt</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Gheeft dat al haren <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s2:l4" met="2" type="-+">In vrede <rhyme label="b" type="m" corresp="#s2:l3">slijt</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Gheeft haer <rhyme label="c" type="m">eenicheyt</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Door u soet <rhyme label="c" type="m" corresp="#s2:l5">beleyt</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Gheeft haer <rhyme label="c" type="m" corresp="#s2:l5">eenicheyt</rhyme></l>
        <l id="s2:l8" met="1" type="-+">O <rhyme label="d" type="f">Heere</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Tot hare salicheyt u <rhyme label="d" type="f" corresp="#s2:l8">eere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
