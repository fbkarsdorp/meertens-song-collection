    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">AEs van mijn <rhyme label="a" type="m">ghemoedt</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Brandt die Liefde <rhyme label="a" type="m" corresp="#s1:l1">voedt</rhyme></l>
        <l id="s1:l3" met="5" type="+-">Center daer mij hert en ziel naer <rhyme label="b" type="m">sinck</rhyme>,</l>
        <l id="s1:l4" met="5" type="+-">Diamant van mijnen Trouwen <rhyme label="b" type="m" corresp="#s1:l3">Rinck</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Eer van 't Mannelijck <rhyme label="c" type="m">gheslacht</rhyme>,</l>
        <l id="s1:l6" met="5" type="+-">Fleur der jongmans daer mijn ziel na <rhyme label="c" type="m" corresp="#s1:l5">tracht</rhyme></l>
        <l id="s1:l7" met="5" type="+-">Gayken dat my Godt gegeven <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s1:l8" met="5" type="+-">Hert waer door mijn jeughdig hert <rhyme label="d" type="m" corresp="#s1:l7">herleeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Jeught die 't al <rhyme label="a" type="m">verdooft</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Kroone van mijn <rhyme label="a" type="m" corresp="#s2:l1">Hooft</rhyme></l>
        <l id="s2:l3" met="5" type="+-">Leven dat mijn leven bloeyen <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s2:l4" met="5" type="+-">Minnaer die mijn hert in liefde <rhyme label="b" type="m" corresp="#s2:l3">voet</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Nardus lieffelijck van <rhyme label="c" type="m">geur</rhyme></l>
        <l id="s2:l6" met="5" type="+-">Oorspronck van mijn levendigh <rhyme label="c" type="m" corresp="#s2:l5">couleur</rhyme></l>
        <l id="s2:l7" met="5" type="+-">Perel die mijn borst met glans <rhyme label="d" type="m">verciert</rhyme></l>
        <l id="s2:l8" met="5" type="+-">Quecker die mijn jeught op-queckt en <rhyme label="d" type="m" corresp="#s2:l7">viert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Rust-plaets van mijn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Salvingh van mijn <rhyme label="a" type="m" corresp="#s3:l1">smert</rhyme></l>
        <l id="s3:l3" met="5" type="+-">Tweede ziel vereenight in een <rhyme label="b" type="m">lijf</rhyme></l>
        <l id="s3:l4" met="5" type="+-">Vreught waer door ick al mijn pijn <rhyme label="b" type="m" corresp="#s3:l3">verdryf</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Wensch, en wit van mijn <rhyme label="c" type="m">jeught</rhyme></l>
        <l id="s3:l6" met="5" type="+-">Ynde, en beginsel van mijn <rhyme label="c" type="m" corresp="#s3:l5">vreught</rhyme></l>
        <l id="s3:l7" met="5" type="+-">Ziel van mijn ziel, licht van <rhyme label="d" type="m">licht</rhyme></l>
        <l id="s3:l8" met="5" type="+-">Hert aen mijn hert door liefdes bant <rhyme label="d" type="m" corresp="#s3:l7">verplicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Hier eyndt mijn A. B. <rhyme label="a" type="m">C</rhyme>.</l>
        <l id="s4:l2" met="3" type="+-">Nochtans de <rhyme label="a" type="m" corresp="#s4:l1">renome</rhyme></l>
        <l id="s4:l3" met="5" type="+-">Van uwen lof en is daer med' niet <rhyme label="b" type="m">uyt</rhyme></l>
        <l id="s4:l4" met="5" type="+-">En even-wel maeck ick hier een <rhyme label="b" type="m" corresp="#s4:l3">besluyt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Want ick en heb geen letters <rhyme label="c" type="m">meer</rhyme>,</l>
        <l id="s4:l6" met="5" type="+-">Om voorders iet te beschrijven tot uw' <rhyme label="c" type="m" corresp="#s4:l5">eer</rhyme>,</l>
        <l id="s4:l7" met="5" type="+-">Ben ick noch bot, peyst dat ick leeren <rhyme label="d" type="m">sal</rhyme>,</l>
        <l id="s4:l8" met="5" type="+-">En u hoe langhs hoe meer vereeren <rhyme label="d" type="m" corresp="#s4:l7">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
