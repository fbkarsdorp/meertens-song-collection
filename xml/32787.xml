      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="praise song">loflied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LOff Iehova <rhyme label="a" type="m">Sabaoth</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Eeuwigh <rhyme label="a" type="m" corresp="#s1:l1">Godt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Die door liefd' tot ons <rhyme label="b" type="f">ghedreven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hebt u lichaem met <rhyme label="c" type="m">bewijs</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Tot een <rhyme label="c" type="m" corresp="#s1:l4">spijs</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Onse zielen hier <rhyme label="b" type="f" corresp="#s1:l3">ghegheven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Teer-spijs die ons inden <rhyme label="a" type="m">noot</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Vande <rhyme label="a" type="m" corresp="#s2:l1">doodt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Stadich vrijt voor 't eeuwich <rhyme label="b" type="f">sterven</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Hemels Manna, weerdich <rhyme label="c" type="m">pandt</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Dat ons 't <rhyme label="c" type="m" corresp="#s2:l4">landt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Der beloften doet <rhyme label="b" type="f" corresp="#s2:l3">be-erven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Nut-baer voetsel vol van <rhyme label="a" type="m">deught</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Dat <rhyme label="a" type="m" corresp="#s3:l1">verheught</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Ziel en lichaem bey te <rhyme label="b" type="f">samen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Als wy u, o wonder <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Onder <rhyme label="c" type="m" corresp="#s3:l4">t'broot</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Weerdich nutten naer 't <rhyme label="b" type="f" corresp="#s3:l3">betamen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Broodt der Enghelen wonder <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Dat ons <rhyme label="a" type="m" corresp="#s4:l1">voet</rhyme></l>
        <l id="s4:l3" met="4" type="+-">En kan vrijden voor de <rhyme label="b" type="f">sonden</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Medecijn en goeden <rhyme label="c" type="m">raet</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">T' Onser <rhyme label="c" type="m" corresp="#s4:l4">baet</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Vol onsterf'lijckheydt <rhyme label="b" type="f" corresp="#s4:l3">bevonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wacht u menschen wie ghy <rhyme label="a" type="m">sijt</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Dat ghy <rhyme label="a" type="m" corresp="#s5:l1">mijt</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Dit onweerdich te <rhyme label="b" type="f">ontfanghen</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Treckt u Bruylofts kleed'ren <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Om te <rhyme label="c" type="m" corresp="#s5:l4">gaen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Godt die heeft naer u <rhyme label="b" type="f" corresp="#s5:l3">verlanghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
