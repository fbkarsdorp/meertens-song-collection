  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Lustig alle Vrees <rhyme label="a" type="f">verbannen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">En 't benaauwde hart <rhyme label="b" type="m">ontschroefd</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Lustig de oude Snaar <rhyme label="a" type="f" corresp="#s1:l1">gespannen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Koning WILLEM'S deugd <rhyme label="b" type="m" corresp="#s1:l2">beproefd</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Eeuwig moet ORANJE <rhyme label="c" type="f">leven</rhyme>!</l>
        <l id="s1:l6" met="4" type="+-">Neêrland ziet den dag <rhyme label="c" type="f" corresp="#s1:l5">herleven</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dat op aller Braaven <rhyme label="d" type="m">beê</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Elk zig met Oranje <rhyme label="e" type="f">tooije</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">d'Oude Vlag zig weêr <rhyme label="e" type="f" corresp="#s1:l8">ontplooije</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">En het Land dreund van <rhyme label="d" type="m" corresp="#s1:l7">Hoezéé</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
