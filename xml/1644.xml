    <melody>Fortuin wat heb dij gebrouwen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">FOrtuyne wat hebdy <rhyme label="a" type="f">ghebrouwen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick en weet mi gheenen <rhyme label="b" type="m">raet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Waer sal ick mi <rhyme label="a" type="f" corresp="#s1:l1">onthouwen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mijn herte wert geheel <rhyme label="b" type="m" corresp="#s1:l2">desperaet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dat ick nv Brugghe moet <rhyme label="c" type="f">laten</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die schone stede <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Rijck god coemt mi te <rhyme label="c" type="f" corresp="#s1:l5">baten</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Mijn herte verbrant al inden <rhyme label="d" type="m" corresp="#s1:l6">gloet</rhyme></l>
        <l id="s1:l9" met="2" type="-+">O lieffelijck <rhyme label="e" type="m">lief</rhyme></l>
        <l id="s1:l10" met="3" type="-+">door v schoon aenschijn <rhyme label="d" type="m" corresp="#s1:l6">soet</rhyme></l>
        <l id="s1:l11" met="3" type="-+">Dat mach ick wel <rhyme label="f" type="f">beclaghen</rhyme></l>
        <l id="s1:l12" met="3" type="-+">dat ick v nv laten <rhyme label="d" type="m" corresp="#s1:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat sullen wi nv gaen <rhyme label="a" type="f">beghinnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wi ghildekens al <rhyme label="b" type="m">ghemeyn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Si woont te Brugghe <rhyme label="a" type="f" corresp="#s2:l1">binnen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">dat alder waertste <rhyme label="b" type="m" corresp="#s2:l2">greyn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Si heeft mi nv <rhyme label="c" type="f">begheuen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ende ghelaten inden <rhyme label="d" type="m">noot</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Och bouen alle die ter werelt <rhyme label="c" type="f" corresp="#s2:l5">leuen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">So sterue ick ia die <rhyme label="d" type="m" corresp="#s2:l6">doot</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">O lieffelijck <rhyme label="e" type="m">lief</rhyme></l>
        <l id="s2:l10" met="3" type="-+">door v schoon aenschijn <rhyme label="d" type="m" corresp="#s2:l6">soet</rhyme></l>
        <l id="s2:l11" met="3" type="-+">Dat mach ick wel <rhyme label="f" type="f">beclaghen</rhyme></l>
        <l id="s2:l12" met="3" type="-+">dat ick v nv laten <rhyme label="d" type="m" corresp="#s2:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dat ick oyt was <rhyme label="a" type="f">gheboren</rhyme> |</l>
        <l id="s3:l2" met="3" type="-+">Van moeder lijf <rhyme label="b" type="m">ontfinck</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">daer om so moet ick <rhyme label="a" type="f" corresp="#s3:l1">tueren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Het is mi een droeuich <rhyme label="b" type="m" corresp="#s3:l2">dinck</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Vrou Venus wat condi <rhyme label="c" type="f">maken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ick en weets mi gheenen <rhyme label="d" type="m">raet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Het zijn vreemde <rhyme label="c" type="f" corresp="#s3:l5">saken</rhyme></l>
        <l id="s3:l8" met="3" type="-+">die ghi nv aen <rhyme label="d" type="m" corresp="#s3:l6">gaet</rhyme></l>
        <l id="s3:l9" met="2" type="-+">O lieffelijck <rhyme label="e" type="m">lief</rhyme></l>
        <l id="s3:l10" met="3" type="-+">door v schoon aenschijn <rhyme label="d" type="m" corresp="#s3:l6">soet</rhyme></l>
        <l id="s3:l11" met="3" type="-+">Dat mach ick wel <rhyme label="f" type="f">beclaghen</rhyme></l>
        <l id="s3:l12" met="3" type="-+">dat ick v nv laten <rhyme label="d" type="m" corresp="#s3:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dit liedeken is eerst <rhyme label="a" type="f">ghesongen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Te Brugghe al op die <rhyme label="b" type="m">steen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">In spijte van alle nijders <rhyme label="a" type="f" corresp="#s4:l1">tonghen</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Ter eeren van vroukens <rhyme label="b" type="m" corresp="#s4:l2">reyn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Een Mechelaer heuet <rhyme label="c" type="f">geschreuen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">daer hi lach in der <rhyme label="d" type="m">noot</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Nv moet hi haer <rhyme label="c" type="f" corresp="#s4:l5">begheuen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">dat sal hem doen den <rhyme label="d" type="m" corresp="#s4:l6">doot</rhyme></l>
        <l id="s4:l9" met="2" type="-+">O lieffelick <rhyme label="e" type="m">lief</rhyme>.</l>
        <l id="s4:l10" met="3" type="-+">door v schoon aenschijn <rhyme label="d" type="m" corresp="#s4:l6">soet</rhyme></l>
        <l id="s4:l11" met="3" type="-+">Dat mach ick wel <rhyme label="f" type="f">beclaghen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">dat ick v nv laten <rhyme label="d" type="m" corresp="#s4:l6">moet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
