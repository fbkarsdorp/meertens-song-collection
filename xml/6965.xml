  <author>Johan van Heemskerck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">EEns als Ph≈ìbus <rhyme label="a" type="f">(duycken</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Gaend' in <rhyme label="b" type="m">d'Oceaen</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Sijn vergulde <rhyme label="a" type="f" corresp="#s1:l1">pruycken</rhyme>)</l>
        <l id="s1:l4" met="3" type="+-">Had zyn reys <rhyme label="b" type="m" corresp="#s1:l2">gedaen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Tusschen licht en <rhyme label="a" type="f">duyster</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Als nu 's nachts <rhyme label="b" type="m">Boodin</rhyme></l>
        <l id="s2:l3" met="3" type="+-">d' Avont-ster haer <rhyme label="a" type="f" corresp="#s2:l1">luyster</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Schoot te venst'ren <rhyme label="b" type="m" corresp="#s2:l2">in</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">En de swarte <rhyme label="a" type="f">wolcken</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Haer bruyn-verwigh <rhyme label="b" type="m">kleet</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Over land en <rhyme label="a" type="f" corresp="#s3:l1">volcken</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Hadden uyt <rhyme label="b" type="m" corresp="#s3:l2">gespreet</rhyme>;</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Ginck ick my <rhyme label="a" type="f">vertreden</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Lancks een <rhyme label="b" type="m">water-kant</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Om myn moede <rhyme label="a" type="f" corresp="#s4:l1">leden</rhyme></l>
        <l id="s4:l4" met="3" type="+-">En versuft <rhyme label="b" type="m" corresp="#s4:l2">verstant</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Naer het lange <rhyme label="a" type="f">lesen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">VVat vermaecks te <rhyme label="b" type="m">doen</rhyme>;</l>
        <l id="s5:l3" met="3" type="+-">En 't wou juyst so <rhyme label="a" type="f" corresp="#s5:l1">wesen</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Dat ick vond in 't <rhyme label="b" type="m" corresp="#s5:l2">groen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">By geval, <rhyme label="a" type="f">geseten</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Voor haer deur <rhyme label="b" type="m">alleen</rhyme></l>
        <l id="s6:l3" met="3" type="+-">'t Schoonst' so ic kan <rhyme label="a" type="f" corresp="#s6:l1">weten</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Dat my oyt <rhyme label="b" type="m" corresp="#s6:l2">verscheen</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">'t Sy de nacht my <rhyme label="a" type="f">maecken</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Moedigh ginck en <rhyme label="b" type="m">stout</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Dat ick dorst <rhyme label="a" type="f" corresp="#s7:l1">genaecken</rhyme></l>
        <l id="s7:l4" met="3" type="+-">'t Geen ick had <rhyme label="b" type="m" corresp="#s7:l2">beschouwt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">'t Sy dat hare <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Met een soet <rhyme label="b" type="m">gestrael</rhyme></l>
        <l id="s8:l3" met="3" type="+-">My te haer-waerts <rhyme label="a" type="f" corresp="#s8:l1">toogen</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Als de zeyl-steen 't <rhyme label="b" type="m" corresp="#s8:l2">stael</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Even als de <rhyme label="a" type="f">vliegen</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Om de kaers en <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Dom en roeckloos <rhyme label="a" type="f" corresp="#s9:l1">vliegen</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Tot zy zyn <rhyme label="b" type="m" corresp="#s9:l2">gevaen</rhyme>:</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">So oock heen en <rhyme label="a" type="f">weder</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Keerden ick myn <rhyme label="b" type="m">tret</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Tot dat ick my <rhyme label="a" type="f" corresp="#s10:l1">neder</rhyme></l>
        <l id="s10:l4" met="3" type="+-">By haer had <rhyme label="b" type="m" corresp="#s10:l2">geset</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">VVat heeft haer <rhyme label="a" type="f">aenschouwen</rhyme></l>
        <l id="s11:l2" met="3" type="+-">In my niet <rhyme label="b" type="m">gewracht</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">En het lieflijck <rhyme label="a" type="f" corresp="#s11:l1">douwen</rhyme></l>
        <l id="s11:l4" met="3" type="+-">Van haer hantjes <rhyme label="b" type="m" corresp="#s11:l2">sacht</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Doch wanneer ick <rhyme label="a" type="f">pooghde</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Haer wel te <rhyme label="b" type="m">besien</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">En haer scherp <rhyme label="a" type="f" corresp="#s12:l1">be-ooghde</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">Ginck de Maen wegh <rhyme label="b" type="m" corresp="#s12:l2">vlien</rhyme>:</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Phoebe scheen te <rhyme label="a" type="f">vresen</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Dat men dees <rhyme label="b" type="m">Ionck-Vrouw</rhyme></l>
        <l id="s13:l3" met="3" type="+-">Neven haer na <rhyme label="a" type="f" corresp="#s13:l1">desen</rhyme></l>
        <l id="s13:l4" met="3" type="+-">Of meer achten <rhyme label="b" type="m" corresp="#s13:l2">sou</rhyme>:</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">Dies sy ginck <rhyme label="a" type="f">verbergen</rhyme></l>
        <l id="s14:l2" met="3" type="+-">'t Nydigh bleecke <rhyme label="b" type="m">hooft</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">En om my te <rhyme label="a" type="f" corresp="#s14:l1">tergen</rhyme></l>
        <l id="s14:l4" met="3" type="+-">Heeft 's haer licht <rhyme label="b" type="m" corresp="#s14:l2">verdooft</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="+-">Koomt de wolcken <rhyme label="a" type="f">breecken</rhyme></l>
        <l id="s15:l2" met="3" type="+-">Luna met u <rhyme label="b" type="m">schijn</rhyme>,</l>
        <l id="s15:l3" met="3" type="+-">So ghy wilt <rhyme label="a" type="f" corresp="#s15:l1">geleecken</rhyme></l>
        <l id="s15:l4" met="3" type="+-">By mijn CLORIS <rhyme label="b" type="m" corresp="#s15:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="+-">Of soo u te <rhyme label="a" type="f">toonen</rhyme></l>
        <l id="s16:l2" met="3" type="+-">Ghy door vrees u <rhyme label="b" type="m">wacht</rhyme>,</l>
        <l id="s16:l3" met="3" type="+-">So wilt my <rhyme label="a" type="f" corresp="#s16:l1">verschoonen</rhyme></l>
        <l id="s16:l4" met="3" type="+-">Dat ick haer meer <rhyme label="b" type="m" corresp="#s16:l2">acht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
