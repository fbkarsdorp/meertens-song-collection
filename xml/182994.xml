<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182994</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Juicht nog ons eerlyk <rhyme label="b" type="m">hart</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Al moet Europa <rhyme label="a" type="f" corresp="#s1:l1">aanschouwen</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Hoe men zyn Naneef <rhyme label="b" type="m" corresp="#s1:l2">tart</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dien Prince van <rhyme label="c" type="f">Oranje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door zyne deugd <rhyme label="d" type="m">bekend</rhyme>!</l>
        <l id="s1:l7" met="3" type="-+">Nooit heeft hy om <rhyme label="c" type="f" corresp="#s1:l5">Brittanje</rhyme>!</l>
        <l id="s1:l8" met="3" type="-+">Zyn eed en plicht <rhyme label="d" type="m" corresp="#s1:l6">geschend</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In God is zyn <rhyme label="a" type="f">vertrouwen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zyn hart voor 't <rhyme label="b" type="m">Vaderland</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Laat vry dien Vorst dan <rhyme label="a" type="f" corresp="#s2:l1">bouwen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Op 's Hemels <rhyme label="b" type="m" corresp="#s2:l2">onderstand</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">'Er zyn in onze <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Nog Mannen kloek en <rhyme label="d" type="m">fier</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die alles zullen <rhyme label="c" type="f" corresp="#s2:l5">waagen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Voor 't vrye <rhyme label="d" type="m" corresp="#s2:l6">Staatsbestier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laat vry den helhond <rhyme label="a" type="f">bassen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wy schroomen geen <rhyme label="b" type="m">geschreeuw</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Geen hond is <rhyme label="a" type="f" corresp="#s3:l1">opgewassen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Te stryden met een <rhyme label="b" type="m" corresp="#s3:l2">Leeuw</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">W'erkenne geen <rhyme label="c" type="f">Landverraders</rhyme></l>
        <l id="s3:l6" met="3" type="-+">In een geschoor dat <rhyme label="d" type="m">woed</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Maar Neêrlands echte <rhyme label="c" type="f" corresp="#s3:l5">Vaders</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In Princen van dat <rhyme label="d" type="m" corresp="#s3:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Laat nu de duivel <rhyme label="a" type="f">bloozen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die 't zaat van oproer <rhyme label="b" type="m">zaait</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hier worden schoone <rhyme label="a" type="f" corresp="#s4:l1">roozen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In Hollands tuin <rhyme label="b" type="m" corresp="#s4:l2">gemaait</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Nu wy een <rhyme label="c" type="f">Vaderlander</rhyme></l>
        <l id="s4:l6" met="3" type="-+">De Vorstelyke <rhyme label="d" type="m">jeugd</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">By onzen <rhyme label="c" type="f" corresp="#s4:l5">Eendragtsstander</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zien leiden tot ons <rhyme label="d" type="m" corresp="#s4:l6">vreugd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een Ed'le <rhyme label="a" type="f">Hooggebooren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Een Prins zoer vroom en <rhyme label="b" type="m">trouw</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Een vriend der <rhyme label="a" type="f" corresp="#s5:l1">Tempelchooren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Een Zuil van 't Staat <rhyme label="b" type="m" corresp="#s5:l2">gebouw</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zal voor de Zoonen <rhyme label="c" type="f">waaken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Geteeld uit Nassouws <rhyme label="d" type="m">Bloed</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En Willems lof <rhyme label="c" type="f" corresp="#s5:l5">volmaaken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat dit geen Vreemdeling <rhyme label="d" type="m" corresp="#s5:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ofschoon men 's Prinsen <rhyme label="a" type="f">Vrinden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Veracht tot in de <rhyme label="b" type="m">Hel</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Geen vrees moet lippen <rhyme label="a" type="f" corresp="#s6:l1">binden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Die 't egter meenen <rhyme label="b" type="m" corresp="#s6:l2">wel</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Wil men de Prins <rhyme label="c" type="f">bedanken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Voor zyn getrouwe <rhyme label="d" type="m">plicht</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Vervangt het door u <rhyme label="c" type="f" corresp="#s6:l5">klanken</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En zingt dit vrolyk <rhyme label="d" type="m" corresp="#s6:l6">Dicht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Aan Steedsche <rhyme label="a" type="f">Magistraaten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Staat u wel danken <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zoud gy Oranje <rhyme label="a" type="f" corresp="#s7:l1">haaten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Daar hy u redder <rhyme label="b" type="m" corresp="#s7:l2">zy</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">Neen, trouwe <rhyme label="c" type="f">Burgerye</rhyme>!</l>
        <l id="s7:l6" met="3" type="-+">Voldoe uw dankbaar <rhyme label="d" type="m">hart</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wil hem uw zangen <rhyme label="c" type="f" corresp="#s7:l5">wye</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Vervloekt al wie hem <rhyme label="d" type="m" corresp="#s7:l6">smart</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">ô God! wil ons <rhyme label="a" type="f">ontdekken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Wie dat de Monsters <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Die Hem zyn leed <rhyme label="a" type="f" corresp="#s8:l1">verwekken</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hem, Neêrlands <rhyme label="b" type="m" corresp="#s8:l2">oogenlyn</rhyme></l>
        <l id="s8:l5" met="3" type="-+">En ach! verschoon deez' <rhyme label="c" type="f">galmen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Niet ik, zy zyn in <rhyme label="d" type="m">schuld</rhyme></l>
        <l id="s8:l7" met="3" type="-+">'Er zyn ook nog Vloek <rhyme label="c" type="f" corresp="#s8:l5">Psalmen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">In uwe Kerk <rhyme label="d" type="m" corresp="#s8:l6">geduld</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Roey uit, roey uit de <rhyme label="a" type="f">boozen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">De haters van zyn <rhyme label="b" type="m">Huys</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Straf, straf de <rhyme label="a" type="f" corresp="#s9:l1">Goddeloozen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Verdelg dat <rhyme label="b" type="m" corresp="#s9:l2">Vloekgespuis</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Geen Prins is ooit <rhyme label="c" type="f">gebooren</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die Neêrland meer <rhyme label="d" type="m">bemint</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Laat dan uw blydschap <rhyme label="c" type="f" corresp="#s9:l5">hooren</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Weest Prinselyk <rhyme label="d" type="m" corresp="#s9:l6">gezind</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Neem hem voor echten <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En Grootvaars echten <rhyme label="b" type="m">Zoon</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Wie, Vrienden! is ons <rhyme label="a" type="f" corresp="#s10:l1">nader</rhyme>?</l>
        <l id="s10:l4" met="3" type="-+">Wie helpt de Deugd ten <rhyme label="b" type="m" corresp="#s10:l2">throon</rhyme>?</l>
        <l id="s10:l5" met="3" type="-+">De Vryheid gaat <rhyme label="c" type="f">verlooren</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Waakt hy voor niet dat <rhyme label="d" type="m">pand</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Waar WILLEM 't snoer wil <rhyme label="c" type="f" corresp="#s10:l5">schooren</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Daar bloeyt het <rhyme label="d" type="m" corresp="#s10:l6">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ja Godsdienst gaat te <rhyme label="a" type="f">gronden</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Door veel <rhyme label="b" type="m">toegeeflykheid</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Het hart verlokt tot <rhyme label="a" type="f" corresp="#s11:l1">zonden</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Dient in een band <rhyme label="b" type="m" corresp="#s11:l2">geleid</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Alleen in WILLEMS <rhyme label="c" type="f">handen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Is 't kleinood wel <rhyme label="d" type="m">bewaard</rhyme>:</l>
        <l id="s11:l7" met="3" type="-+">Wilt vreugde vuuren <rhyme label="c" type="f" corresp="#s11:l5">branden</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Prins WILLEM is het <rhyme label="d" type="m" corresp="#s11:l6">waard</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Eeuwige! ai hoor deez' <rhyme label="a" type="f">bede</rhyme>!</l>
        <l id="s12:l2" met="3" type="-+">Bescherm ons <rhyme label="b" type="m">Vaderland</rhyme>!</l>
        <l id="s12:l3" met="3" type="-+">In Oorlog en in <rhyme label="a" type="f" corresp="#s12:l1">Vreede</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Lei WILLEM by der <rhyme label="b" type="m" corresp="#s12:l2">hand</rhyme>!</l>
        <l id="s12:l5" met="3" type="-+">Ja, Hemel! elk roep <rhyme label="c" type="f">Amen</rhyme>!</l>
        <l id="s12:l6" met="3" type="-+">Elk woif den vryen <rhyme label="d" type="m">hoed</rhyme>!</l>
        <l id="s12:l7" met="3" type="-+">Godsdienst, Prins, Vryheid <rhyme label="c" type="f" corresp="#s12:l5">zamen</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Zyn Een by 't echte <rhyme label="d" type="m" corresp="#s12:l6">bloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
