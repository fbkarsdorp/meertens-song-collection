<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25080</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="saint song">heiligenlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Na dat sint ian daer <rhyme label="a" type="m">sach</rhyme>.</l>
        <l id="s1:l2" met="2" type="+-">Eest daer altoes eewich <rhyme label="a" type="m" corresp="#s1:l1">dach</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Daer en es anxte noch genen <rhyme label="a" type="m" corresp="#s1:l1">noet</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Maer vroude <rhyme label="b" type="m">groet</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Hi es daer diet al <rhyme label="b" type="m" corresp="#s1:l4">vermach</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s1:l6">fijn</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s1:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Si sueken raste in haer <rhyme label="a" type="m">begin</rhyme>.</l>
        <l id="s2:l2" met="2" type="+-">En goede weyde es haer <rhyme label="a" type="m" corresp="#s2:l1">gewin</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">In sijn suete menscheit <rhyme label="a" type="m" corresp="#s2:l1">daer</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Ende godheit <rhyme label="b" type="m">claerheit</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Daer gaense altoes vut ende <rhyme label="b" type="m" corresp="#s2:l4">in</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s2:l6">fijn</rhyme>.</l>
        <l id="s2:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s2:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Si sugen vut siere herten <rhyme label="a" type="m">fijn</rhyme>.</l>
        <l id="s3:l2" met="2" type="+-">Balseme ende ouer sueten <rhyme label="a" type="m" corresp="#s3:l1">wijn</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Die daer inder minnen <rhyme label="a" type="m" corresp="#s3:l1">groeyt</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Ende <rhyme label="b" type="m">ouervloeyt</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Doer alle heilegen die daer <rhyme label="b" type="m" corresp="#s3:l4">sijn</rhyme>.</l>
        <l id="s3:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s3:l6">fijn</rhyme>.</l>
        <l id="s3:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s3:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hi biet daer sinen sconen <rhyme label="a" type="m">mont</rhyme>.</l>
        <l id="s4:l2" met="2" type="+-">Sijn claer aenscijn sijn herte <rhyme label="a" type="m" corresp="#s4:l1">gront</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Die wilt die mach genade <rhyme label="a" type="m" corresp="#s4:l1">ontfaen</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Het es al <rhyme label="b" type="m">voldaen</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Hi es van minnen al <rhyme label="b" type="m" corresp="#s4:l4">doerwont</rhyme>.</l>
        <l id="s4:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s4:l6">fijn</rhyme>.</l>
        <l id="s4:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s4:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Si eten daer dat hemelsche <rhyme label="a" type="m">broet</rhyme>.</l>
        <l id="s5:l2" met="2" type="+-">Sijn herte es al bloedich <rhyme label="a" type="m" corresp="#s5:l1">roet</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Die si minnen sijnse <rhyme label="a" type="m" corresp="#s5:l1">bi</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">ende altoes <rhyme label="b" type="m">vri</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Si vallen al in sinen <rhyme label="b" type="m" corresp="#s5:l4">scoet</rhyme>.</l>
        <l id="s5:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s5:l6">fijn</rhyme>.</l>
        <l id="s5:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s5:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hi sach dat edel weerde <rhyme label="a" type="m">lam</rhyme>.</l>
        <l id="s6:l2" met="2" type="+-">Hoet ons menscheit ane <rhyme label="a" type="m" corresp="#s6:l1">nam</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Dat doer ons verslegen <rhyme label="a" type="m" corresp="#s6:l1">was</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Ons sonden <rhyme label="b" type="m">genas</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Ende tot sijnder vrouden <rhyme label="b" type="m" corresp="#s6:l4">quam</rhyme>.</l>
        <l id="s6:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s6:l6">fijn</rhyme>.</l>
        <l id="s6:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s6:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hi versach den hoechsten <rhyme label="a" type="m">trone</rhyme>.</l>
        <l id="s7:l2" met="2" type="+-">Dat god was hare alre <rhyme label="a" type="m" corresp="#s7:l1">crone</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Sittende in sijn <rhyme label="a" type="m" corresp="#s7:l1">conincrike</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Sonder <rhyme label="b" type="m">gelike</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Dat eewich woert des vaders <rhyme label="b" type="m" corresp="#s7:l4">sone</rhyme>.</l>
        <l id="s7:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s7:l6">fijn</rhyme>.</l>
        <l id="s7:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s7:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die scone coninginne es daer <rhyme label="a" type="m">gecleet</rhyme>.</l>
        <l id="s8:l2" met="2" type="+-">Al metter sonnen <rhyme label="a" type="m" corresp="#s8:l1">claer</rhyme>.</l>
        <l id="s8:l3" met="4" type="+-">Haer bruydegoem wel <rhyme label="a" type="m" corresp="#s8:l1">bereet</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Scone gecleet <rhyme label="b" type="m">aldaer</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Gecroent al <rhyme label="b" type="m" corresp="#s8:l4">openbaer</rhyme>.</l>
        <l id="s8:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s8:l6">fijn</rhyme>.</l>
        <l id="s8:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s8:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Si es daer mechtich ende alte <rhyme label="a" type="m">groot</rhyme>.</l>
        <l id="s9:l2" met="2" type="+-">Si heeft getorden des draken <rhyme label="a" type="m" corresp="#s9:l1">hoot</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Ende gebonden inden bant. met <rhyme label="a" type="m" corresp="#s9:l1">hare</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Hant. Si huede ons in die <rhyme label="b" type="m">vre</rhyme></l>
        <l id="s9:l5" met="4" type="-+">der <rhyme label="b" type="m" corresp="#s9:l4">doot</rhyme>.</l>
        <l id="s9:l6" met="4" type="-+">Het es daer inder sonnen <rhyme label="c" type="m">scijn</rhyme></l>
        <l id="s9:l7" met="2" type="-+">Claer en <rhyme label="c" type="m" corresp="#s9:l6">fijn</rhyme>.</l>
        <l id="s9:l8" met="4" type="-+">Daer die weerde heilege <rhyme label="b" type="m" corresp="#s9:l4">sijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
