        <l id="s5:l5" met="3" type="-+">Last man vngl ck <rhyme label="c" type="f">waltenn</rhyme> /</l>
        <l id="s5:l6" met="3" type="-+">Wehr nitt hatt groß geltt vnnd g <rhyme label="d" type="m">itt</rhyme> /</l>
        <l id="s5:l7" met="3" type="-+">Dan ist die lieb <rhyme label="c" type="f" corresp="#s5:l5">zerspaltenn</rhyme> /</l>
        <l id="s5:l8" met="3" type="-+">Alß manich befinden th <rhyme label="d" type="m" corresp="#s5:l6">itt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ade ade mein schones <rhyme label="a" type="f">lieb</rhyme></l>
