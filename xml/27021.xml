  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ich hatt mich a <rhyme label="a" type="f">ßerkorn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">ein feins lieb <rhyme label="b" type="m">wolgethann</rhyme></l>
