        <l id="s1:l4" met="3" type="-+">meinenn dienst ich ihr <rhyme label="b" type="m" corresp="#s1:l2">geschworen</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Jn rechter lieb vnnd <rhyme label="c" type="f">trewenn</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">whar sei jn mein hertz <rhyme label="d" type="m">geneigtt</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">Kein dienst mich th itt gera <rhyme label="c" type="f" corresp="#s1:l5">wenn</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">Denn ich jr hatt <rhyme label="d" type="m" corresp="#s1:l6">erzeigtt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sei th tt mich offt <rhyme label="a" type="f">anblicken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">mitt manchem s chtenn <rhyme label="b" type="m">diep</rhyme></l>
        <l id="s2:l3" met="3" type="-+">So a ch ir fre ndtlich <rhyme label="a" type="f" corresp="#s2:l1">sprechenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Ob ich ir die liebste <rhyme label="b" type="m" corresp="#s2:l2">wehr</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Jch hatt es gar woll <rhyme label="c" type="f">geschworenn</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">Mein sach wehr eithell <rhyme label="d" type="m">feill</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Sie hett mich a <rhyme label="c" type="f" corresp="#s2:l5">ßerkornn</rhyme> /</l>
