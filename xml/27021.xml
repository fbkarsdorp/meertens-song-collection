        <l id="s3:l5" met="3" type="-+">Die ich so lang gethragenn <rhyme label="c" type="f">hab</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">F rwar ohn lieb vnnd g <rhyme label="d" type="m">nst</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Wiewoll sei th itt <rhyme label="c" type="f" corresp="#s3:l5">dergleichenn</rhyme> /</l>
