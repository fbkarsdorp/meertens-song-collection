        <l id="s1:l4" met="4" type="-+">Ik bid dat gy my sterven <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Of liever wilt die zoete <rhyme label="c" type="m">pyn</rhyme>,</l>
