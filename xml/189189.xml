        <l id="s1:l7" met="2" type="-+">Daar ik om <rhyme label="c" type="m" corresp="#s1:l5">kwyn</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Helaas, ten <rhyme label="d" type="f">minsten</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Helaas, ten minsten wat <rhyme label="b" type="f" corresp="#s1:l3">verlichten</rhyme>.</l>
