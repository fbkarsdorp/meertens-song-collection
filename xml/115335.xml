    <melody>Psalm 034 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Alle oogen met <rhyme label="a" type="m">vlijt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Wachten, o Heer, op uwen <rhyme label="b" type="m">Naem</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Op dat ghy hem spijse <rhyme label="b" type="m" corresp="#s1:l2">bequaem</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Geeft t’aengenamer <rhyme label="a" type="m" corresp="#s1:l1">tijdt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Almachtigh Vader <rhyme label="c" type="m">goedt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die ‘t al geschapen hebt uyt <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">En noch stedes alsoomen <rhyme label="d" type="m" corresp="#s1:l6">siet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Onderhoudt en <rhyme label="c" type="m" corresp="#s1:l5">behoedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die u volck <rhyme label="a" type="m">Israël</rhyme></l>
        <l id="s2:l2" met="4" type="-+">In ‘t woest spijsde met Manna <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En uyt den Steen met Water <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Lested’ haer dorst <rhyme label="a" type="m" corresp="#s2:l1">wel</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Tot ons u zegen <rhyme label="c" type="m">streckt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dat wy uw’ heylige gaven <rhyme label="d" type="m">fijn</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Gebruycken so wy schuldigh <rhyme label="d" type="m" corresp="#s2:l6">zijn</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Met danckbaerheydt <rhyme label="c" type="m" corresp="#s2:l5">perfect</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daer door wy u <rhyme label="a" type="m">alleyn</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Eeren, met vreesen <rhyme label="b" type="m">kinderlijck</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En kennen <rhyme label="b" type="m" corresp="#s3:l2">onverhinderlijck</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Aller goeden <rhyme label="a" type="m" corresp="#s3:l1">Fonteyn</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Voor al dijn heyligh <rhyme label="c" type="m">Woordt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Onser zielen eeuwige <rhyme label="d" type="m">broodt</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Bereyd door Jesu Christi <rhyme label="d" type="m" corresp="#s3:l6">doot</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Laet ons rusten nu <rhyme label="c" type="m" corresp="#s3:l5">voort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
