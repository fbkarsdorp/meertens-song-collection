<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>12138</id>
    <date>
      <year-start>1617</year-start>
      <year-end>1617</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Sus muse d'un bien humble</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SVs Muse, d’vn bien humble <rhyme label="a" type="m">vers</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Le sinct los de Marie <rhyme label="b" type="f">entonne</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Et toy Royne de <rhyme label="a" type="m" corresp="#s1:l1">l’vniuers</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Entendz ma Lyre qui te <rhyme label="b" type="f" corresp="#s1:l2">sonne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Il faict beau veoir dans vn <rhyme label="a" type="m">iardin</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Au premier resueil de <rhyme label="b" type="f">l’Aurore</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">La Rose fille du <rhyme label="a" type="m" corresp="#s2:l1">matin</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Peu à peu son beau sein <rhyme label="b" type="f" corresp="#s2:l2">esclore</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Il faict beau veoir aussi le <rhyme label="a" type="m">Lis</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ouurir ses leures <rhyme label="b" type="f">blanchissantes</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Et les retordre en diuers <rhyme label="a" type="m" corresp="#s3:l1">plis</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Pour mostrer ses fleurs <rhyme label="b" type="f" corresp="#s3:l2">iaunissantes</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Mais nostre Virginale <rhyme label="a" type="m">Fleur</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Marie gracieuse &amp; <rhyme label="b" type="f">belle</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En teinct, odeur plein de <rhyme label="a" type="m" corresp="#s4:l1">douceur</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Passe toute beauté <rhyme label="b" type="f" corresp="#s4:l2">mortelle</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Au jardin clos de toute <rhyme label="a" type="m">part</rhyme></l>
        <l id="s5:l2" met="4" type="-+">De la diuine <rhyme label="b" type="f">prescience</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Fut plantée seule à <rhyme label="a" type="m" corresp="#s5:l1">l’escart</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Veste belle fleur <rhyme label="b" type="f" corresp="#s5:l2">d’excellence</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ce Iardinier <rhyme label="a" type="m">tres-souuerain</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Qui de rien ce grand tout fit <rhyme label="b" type="f">naistre</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">La planta de sa proper <rhyme label="a" type="m" corresp="#s6:l1">main</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Digne chef d’oevre d’vn tel <rhyme label="b" type="f" corresp="#s6:l2">Maitre</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nostre printemps n’estoit pas <rhyme label="a" type="m">né</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Ceste fleur estoit desia <rhyme label="b" type="f">née</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Desia Dieu luy auoit <rhyme label="a" type="m" corresp="#s7:l1">donné</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Sa grace, à  null’ autre <rhyme label="b" type="f" corresp="#s7:l2">donnée</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ie te salue saincte <rhyme label="a" type="m">fleur</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Fleur plus vermeille que la <rhyme label="b" type="f">Rose</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Fleur plus sincere en ta <rhyme label="a" type="m" corresp="#s8:l1">candeur</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Que le Lis &amp; toute autre <rhyme label="b" type="f" corresp="#s8:l2">chose</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Fleur plus doucette que tout <rhyme label="a" type="m">miel</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Fleur plus que tout musq <rhyme label="b" type="f">doux-flairante</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Plus haute que le plus haut <rhyme label="a" type="m" corresp="#s9:l1">ciel</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Fleur des fleurs touiours <rhyme label="b" type="f" corresp="#s9:l2">fleurissantes</rhyme>:</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Accorde moy ceste <rhyme label="a" type="m">faueur</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Fleur, l’heur &amp; l’honneur de ta <rhyme label="b" type="f">race</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Que tousiours fleurisse en mon <rhyme label="a" type="m" corresp="#s10:l1">coeur</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Quelque reiecton de ta <rhyme label="b" type="f" corresp="#s10:l2">grace</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
