    <melody>Bedrijf jolijt in des werelds krijt</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Comt al ter baen die placht te <rhyme label="a" type="m">staen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">met kou belaen sin nu <rhyme label="a" type="m" corresp="#s1:l1">voortaen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">veerdweenen plaen die straffe <rhyme label="b" type="f">luchten</rhyme></l>
        <l id="s1:l4" met="4" type="-+">komt nu vrij uit den houck <rhyme label="c" type="m">huisdruit</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Taerrijck ontsluit ende <rhyme label="c" type="m" corresp="#s1:l4">ontspruit</rhyme></l>
        <l id="s1:l6" met="4" type="-+">met liefelick kruit en schoene <rhyme label="b" type="f" corresp="#s1:l3">vruchten</rhyme></l>
        <l id="s1:l7" met="4" type="-+">geen quale staen u nu te <rhyme label="b" type="f" corresp="#s1:l3">duchten</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Die scharpe winden wel <rhyme label="d" type="f">kauseeren</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Want ons niet <rhyme label="e" type="m">meer</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Eenich stranck <rhyme label="e" type="m" corresp="#s1:l9">weer</rhyme></l>
        <l id="s1:l11" met="2" type="-+">twelck tsmenschen <rhyme label="e" type="m" corresp="#s1:l9">teer</rhyme></l>
        <l id="s1:l12" met="2" type="-+">veel vleys Jae <rhyme label="e" type="m" corresp="#s1:l9">smeer</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Doet smerten <rhyme label="e" type="m" corresp="#s1:l9">seer</rhyme></l>
        <l id="s1:l14" met="2" type="-+">Comt <rhyme label="d" type="f" corresp="#s1:l8">tormenteeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gij muecht nu vrij In dit <rhyme label="a" type="m">getij</rhyme></l>
        <l id="s2:l2" met="4" type="-+">neffens den sij van u liefken <rhyme label="a" type="m" corresp="#s2:l1">blij</rhyme></l>
        <l id="s2:l3" met="4" type="-+">u gaen <rhyme label="b" type="f">voormengen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">En sien hoe stout en <rhyme label="c" type="m">onberout</rhyme></l>
        <l id="s2:l5" met="4" type="-+">seer meenich fout buiten het <rhyme label="c" type="m" corresp="#s2:l4">hout</rhyme></l>
        <l id="s2:l6" met="4" type="-+">[...] bedout tgedierte gaet <rhyme label="b" type="f" corresp="#s2:l3">weyen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">En hoe die haeskens en <rhyme label="b" type="f" corresp="#s2:l3">lampreyen</rhyme></l>
        <l id="s2:l8" met="4" type="-+">haer Jeuchdich In de son <rhyme label="d" type="f">verquicken</rhyme></l>
        <l id="s2:l9" met="2" type="-+">En des <rhyme label="e" type="m">gelijcken</rhyme></l>
        <l id="s2:l10" met="2" type="-+">hoe <rhyme label="e" type="m" corresp="#s2:l9">maenyfijck</rhyme></l>
        <l id="s2:l11" met="2" type="-+">Diaenae <rhyme label="e" type="m" corresp="#s2:l9">rijck</rhyme></l>
        <l id="s2:l12" met="2" type="-+">In arch <rhyme label="e" type="m" corresp="#s2:l9">pratijck</rhyme></l>
        <l id="s2:l13" met="2" type="-+">Duer sant en <rhyme label="e" type="m" corresp="#s2:l9">slijck</rhyme></l>
        <l id="s2:l14" met="2" type="-+">twilt gaet <rhyme label="d" type="f" corresp="#s2:l8">voorklicken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Certey tplaesier twelck elck aers <rhyme label="a" type="m">dier</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Jae tleyne mier met soet <rhyme label="a" type="m" corresp="#s3:l1">getier</rhyme></l>
        <l id="s3:l3" met="4" type="-+">naeck passeert schier als <rhyme label="b" type="f">onbedachte</rhyme></l>
        <l id="s3:l4" met="4" type="-+">tgevoegelt kleyn en groet <rhyme label="c" type="m">gemeyn</rhyme></l>
        <l id="s3:l5" met="4" type="-+">lieflick en reyn nu queelt <rhyme label="c" type="m" corresp="#s3:l4">certeyn</rhyme></l>
        <l id="s3:l6" met="4" type="-+">men hoort allein daer te <rhyme label="b" type="f" corresp="#s3:l3">voornachten</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Daer soutman sonder lanck te <rhyme label="b" type="f" corresp="#s3:l3">wachten</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Dan nachtegael luit singen om <rhyme label="d" type="f">hooren</rhyme></l>
        <l id="s3:l9" met="2" type="-+">men sou oock <rhyme label="e" type="m">daer</rhyme></l>
        <l id="s3:l10" met="2" type="-+">aenschouwen <rhyme label="e" type="m" corresp="#s3:l9">klaer</rhyme></l>
        <l id="s3:l11" met="2" type="-+">een <rhyme label="e" type="m" corresp="#s3:l9">exsemplaer</rhyme></l>
        <l id="s3:l12" met="2" type="-+">hoe elck daer <rhyme label="e" type="m" corresp="#s3:l9">swaer</rhyme></l>
        <l id="s3:l13" met="2" type="-+">met sijn <rhyme label="e" type="m" corresp="#s3:l9">weerpaer</rhyme></l>
        <l id="s3:l14" met="2" type="-+">vruecht gaet <rhyme label="d" type="f" corresp="#s3:l8">ver(pachten</rhyme>)</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Juech wilt u nu spoen te gaen Int <rhyme label="a" type="m">groen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">smergens nae noen om daer te <rhyme label="a" type="m" corresp="#s4:l1">Doen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">als helten koen ws herts <rhyme label="b" type="f">voormaeken</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Kuibydoes aert daer <rhyme label="c" type="m">oopenbaert</rhyme></l>
        <l id="s4:l5" met="4" type="-+">troost onbeswaert u lief en <rhyme label="c" type="m" corresp="#s4:l4">paert</rhyme></l>
        <l id="s4:l6" met="4" type="-+">eert vooriaert voor alle <rhyme label="b" type="f" corresp="#s4:l3">saecken</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Wilt daer elck ander lief doen <rhyme label="b" type="f" corresp="#s4:l3">smaeken</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Met alle vrienschap te <rhyme label="d" type="f">bedrijven</rhyme></l>
        <l id="s4:l9" met="2" type="-+">In eer en <rhyme label="e" type="m">duecht</rhyme></l>
        <l id="s4:l10" met="2" type="-+">met goe <rhyme label="e" type="m" corresp="#s4:l9">genuecht</rhyme></l>
        <l id="s4:l11" met="2" type="-+">en wilt oe <rhyme label="e" type="m" corresp="#s4:l9">Juecht</rhyme></l>
        <l id="s4:l12" met="2" type="-+">so gij best <rhyme label="e" type="m" corresp="#s4:l9">muegt</rhyme></l>
        <l id="s4:l13" met="2" type="-+">u [...] tsaem aen <rhyme label="e" type="m" corresp="#s4:l9">(c)lijven</rhyme>.</l>
        <l id="s4:l14" met="2" type="-+">[...] troost veel <rhyme label="d" type="f" corresp="#s4:l8">vruachden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
