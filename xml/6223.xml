<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6223</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Un satyre cornu</melody>
  <author>C.P. van Wesbusch</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHy die de Kroone <rhyme label="a" type="m">voert</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Op d'uytghekamde <rhyme label="b" type="f">Tuytjes</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En vriend'lijck sit en <rhyme label="a" type="m" corresp="#s1:l1">loert</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">O puyck van alle <rhyme label="b" type="f" corresp="#s1:l2">Bruytjes</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Ghy zijt d'aerts hooghste <rhyme label="c" type="m">wensch</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Voor een recht beweeg'lijck <rhyme label="c" type="m" corresp="#s1:l5">mensch</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Soet Beckjen waert ghy <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ick liet mijn niet <rhyme label="b" type="f">bedaren</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Ghy soud niet langhe <rhyme label="a" type="m" corresp="#s2:l1">zijn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">By die u nu <rhyme label="b" type="f" corresp="#s2:l2">bewaren</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Ick soud u met <rhyme label="c" type="m">gheweldt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Haest by komen als een <rhyme label="c" type="m" corresp="#s2:l5">Heldt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy die nae langh <rhyme label="a" type="m">gheslaef</rhyme></l>
        <l id="s3:l2" met="3" type="-+">V eynd'lijck heeft <rhyme label="b" type="f">verkreghen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als voor de hooghste <rhyme label="a" type="m" corresp="#s3:l1">Gaef</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van wereldtlijcke <rhyme label="b" type="f" corresp="#s3:l2">Zeghen</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Werdt van u af <rhyme label="c" type="m">gheleydt</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">O vuyle <rhyme label="c" type="m" corresp="#s3:l5">onreed'lijckheydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Soud ick een Heere <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van soo een waerdigh <rhyme label="b" type="f">pantjen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En missen mijn <rhyme label="a" type="m" corresp="#s4:l1">Robijn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Mijn aerdigh <rhyme label="b" type="f" corresp="#s4:l2">Diamantjen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die met haer lief <rhyme label="c" type="m">ghesicht</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Yder Minnaers hart <rhyme label="c" type="m" corresp="#s4:l5">verlicht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Neen seecker ick en <rhyme label="a" type="m">sou</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Gheen Flickeflopsters <rhyme label="b" type="f">lyden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">By mijn ghewenschte <rhyme label="a" type="m" corresp="#s5:l1">Vrou</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Aen hare groene <rhyme label="b" type="f" corresp="#s5:l2">zijden</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Maer setten als een <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s5:l6" met="4" type="+-">My by mijn soet Beckjen <rhyme label="c" type="m" corresp="#s5:l5">neer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Tsa! waerden <rhyme label="a" type="m">Bruydegom</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Laet u dus niet <rhyme label="b" type="f">verdrucken</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wilt dijne rijpe <rhyme label="a" type="m" corresp="#s6:l1">Blom</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Wt groene Knopjes <rhyme label="b" type="f" corresp="#s6:l2">plucken</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Voert haer tot dijn <rhyme label="c" type="m">Cieraet</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Waer ghy heenen gaet of <rhyme label="c" type="m" corresp="#s6:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Stelt haer in een <rhyme label="a" type="m">Salet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">By al dijn <rhyme label="b" type="f">Pronckeryen</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Voert haer op 't cierlijck <rhyme label="a" type="m" corresp="#s7:l1">Bedt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Om Minne-kamp te <rhyme label="b" type="f" corresp="#s7:l2">stryen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Weerdt u als <rhyme label="c" type="m">Kampioen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Met een vriendelijcke <rhyme label="c" type="m" corresp="#s7:l5">soen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Vaert wel, manhaftigh <rhyme label="a" type="m">Heldt</rhyme>:</l>
        <l id="s8:l2" met="3" type="-+">Ick wensch u Heyl en <rhyme label="b" type="f">Zegen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Slaet alles uyt het <rhyme label="a" type="m" corresp="#s8:l1">veldt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Wat u komt rennen <rhyme label="b" type="f" corresp="#s8:l2">teghen</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Siet dat ghy krijgt, en <rhyme label="c" type="m">houdt</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Dees, die ghy nu hebt <rhyme label="c" type="m" corresp="#s8:l5">Ghetrout</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
