<song>
  <meta>
    <id>177929</id>
    <date>
      <year-start>1837</year-start>
      <year-end>1837</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mars van Dessau</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Vrouw, Kom <rhyme label="a" type="m">nou</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Zing eens van luchtig <rhyme label="b" type="f">Fransje</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Kom leg met mij een <rhyme label="b" type="f" corresp="#s1:l2">dansje</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Het deeg ligt in de <rhyme label="c" type="m">trog</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Help dan ook wat <rhyme label="d" type="f">afwegen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Tot hulp van uwen <rhyme label="e" type="m">man</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dan kunt gij platen <rhyme label="d" type="f" corresp="#s1:l5">vegen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Intusschen build klein <rhyme label="e" type="m" corresp="#s1:l6">jan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wel man, Wat <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Als ik heb <rhyme label="b" type="f">afgewogen</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Wat zet gij een paar <rhyme label="b" type="f" corresp="#s2:l2">oogen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Op, tegen lijs uw <rhyme label="c" type="m">vrouw</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">Of heb ik wat <rhyme label="d" type="f">bedreven</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat u niet aan en <rhyme label="e" type="m">staat</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maak nu dan toch geen <rhyme label="d" type="f" corresp="#s2:l5">leven</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">o! Hoe zijt gij toch zoo <rhyme label="e" type="m" corresp="#s2:l6">kwaad</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Loop weg, loop <rhyme label="a" type="m">weg</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">g' Ontsteelt mij in mijn <rhyme label="b" type="f">zaken</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Kunt gij 't niet ligter <rhyme label="b" type="f" corresp="#s3:l2">maker</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gij weegt het veel te <rhyme label="c" type="m">zwaar</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">'k Dagt dat het in den <rhyme label="d" type="f">oven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Veel ligter werd door 't <rhyme label="e" type="m">vuur</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat moet gij niet <rhyme label="d" type="f" corresp="#s3:l5">gelooven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Al zegt dit ook klaas <rhyme label="e" type="m" corresp="#s3:l6">buur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ik zal, Dan <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Mijn deeg een beetje <rhyme label="b" type="f">knijpen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Als gij maar kunt <rhyme label="b" type="f" corresp="#s4:l2">begrijpen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Een half once <rhyme label="c" type="m">maar</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">De gist zal 't wel <rhyme label="d" type="f">uitzetten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dit is maar voor het <rhyme label="e" type="m">oog</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wie zal er ook op <rhyme label="d" type="f" corresp="#s4:l5">letten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Als ik hun ook <rhyme label="e" type="m" corresp="#s4:l6">bedroog</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Nu vrouw, 'k Wil <rhyme label="a" type="m">jouw</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Dan altoos laten <rhyme label="b" type="f">wegen</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">'t Is een verborgen <rhyme label="b" type="f" corresp="#s5:l2">zegen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">'t Geeft, voordel in mijn <rhyme label="c" type="m">zak</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Maar als de wegers <rhyme label="d" type="f">komen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Zijn w' dan niet in <rhyme label="e" type="m">gevaar</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wil daar maar niet voor <rhyme label="d" type="f" corresp="#s5:l5">schromen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat is ook alles <rhyme label="e" type="m" corresp="#s5:l6">klaar</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
