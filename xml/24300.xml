        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="rederijkerslied" translation="rhetorician song">rederijkerslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>O bloeiende jeugd notabel wijs van zinnen</melody>
    <categories>
      <category translation="groups">groepen</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">V louick o <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van dijne groote <rhyme label="b" type="f">daden</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Lanck soo meer en <rhyme label="a" type="m" corresp="#s1:l1">meer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Om u soete <rhyme label="b" type="f" corresp="#s1:l2">ghenaden</rhyme></l>
        <l id="s1:l5" met="4" type="-+">U louick die daer hebt <rhyme label="c" type="m">ghemaeckt</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Die weerelt wonderlick <rhyme label="c" type="m" corresp="#s1:l5">ghetaeckt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Sonder yemants <rhyme label="b" type="f" corresp="#s1:l2">beraden</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Het licht gesticht daer elck naer <rhyme label="c" type="m" corresp="#s1:l5">haeckt</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Den nacht reyn vanden dach <rhyme label="c" type="m" corresp="#s1:l5">gestaeckt</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Sonder eenich <rhyme label="b" type="f" corresp="#s1:l2">verspaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">U die <rhyme label="a" type="m">t'Firmament</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Seer constich cost <rhyme label="b" type="f">bereeden</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Louick: En die <rhyme label="a" type="m" corresp="#s2:l1">gent</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dwater cost <rhyme label="b" type="f" corresp="#s2:l2">onderscheeden</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">U louick die maecte gras en <rhyme label="c" type="m">cruyt</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Vruchten en boomen van soet <rhyme label="c" type="m" corresp="#s2:l5">fruyt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dwelck can sijn saet wt <rhyme label="b" type="f" corresp="#s2:l2">leeden</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Voor den dach/ bracht der Sonnen <rhyme label="c" type="m" corresp="#s2:l5">spruyt</rhyme></l>
        <l id="s2:l9" met="4" type="-+">Voor den nacht Maen en sterren <rhyme label="c" type="m" corresp="#s2:l5">wt</rhyme></l>
        <l id="s2:l10" met="3" type="-+">U sal ick lof <rhyme label="b" type="f" corresp="#s2:l2">verbreeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">U (die de <rhyme label="a" type="m">dieren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Op eerden hebt <rhyme label="b" type="f">gheschapen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Vischs in <rhyme label="a" type="m" corresp="#s3:l1">Riuieren</rhyme>)</l>
        <l id="s3:l4" met="3" type="-+">Louick nu <rhyme label="b" type="f" corresp="#s3:l2">onvernepen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Tgheuogelt elck naer sijnen <rhyme label="c" type="m">aert</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Al wat op eerden roert <rhyme label="c" type="m" corresp="#s3:l5">ghepaert</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Wormen diueers van <rhyme label="b" type="f" corresp="#s3:l2">strepen</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Die my schiept naer dijn <rhyme label="c" type="m" corresp="#s3:l5">vermaert</rhyme></l>
        <l id="s3:l9" met="4" type="-+">En al dat leeft voor my <rhyme label="c" type="m" corresp="#s3:l5">ghebaert</rhyme></l>
        <l id="s3:l10" met="3" type="-+">U lof  dient my <rhyme label="b" type="f" corresp="#s3:l2">begrepen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">U louick nu <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Maerd' Enghels inden <rhyme label="b" type="f">troone</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Want ghy schonct ons <rhyme label="a" type="m" corresp="#s4:l1">fier</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Vier Elementen <rhyme label="b" type="f" corresp="#s4:l2">schoone</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Als eerde water vier en <rhyme label="c" type="m">lucht</rhyme></l>
        <l id="s4:l6" met="4" type="-+">En deerde gheeft ons alle <rhyme label="c" type="m" corresp="#s4:l5">vrucht</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Met dwater zeer <rhyme label="b" type="f" corresp="#s4:l2">idoone</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Dwelck alle vuylheyt drijft <rhyme label="c" type="m" corresp="#s4:l5">tervlucht</rhyme></l>
        <l id="s4:l9" met="4" type="-+">Tvier dwelck saecht den couden <rhyme label="c" type="m" corresp="#s4:l5">sucht</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Lucht gheeft groeysel te <rhyme label="b" type="f" corresp="#s4:l2">loone</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Met my (Princen <rhyme label="a" type="m">ras</rhyme>)</l>
        <l id="s5:l2" met="3" type="-+">Looft: met Hertoghen <rhyme label="b" type="f">Grauen</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Looft hem op elck <rhyme label="a" type="m" corresp="#s5:l1">pas</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Looft hem vrye en <rhyme label="b" type="f" corresp="#s5:l2">slauen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Looft hem my Joden breed/ en <rhyme label="c" type="m">smal</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Looft met mi Heydensche <rhyme label="c" type="m" corresp="#s5:l5">ghetal</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Looft al die hy wilt <rhyme label="b" type="f" corresp="#s5:l2">lauen</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Looft met my wat leeft in dit <rhyme label="c" type="m" corresp="#s5:l5">dal</rhyme></l>
        <l id="s5:l9" met="4" type="-+">Looft om sijn Elementen <rhyme label="c" type="m" corresp="#s5:l5">al</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Looft hem om al dees <rhyme label="b" type="f" corresp="#s5:l2">gauen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
