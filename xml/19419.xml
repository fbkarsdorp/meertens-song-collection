<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19419</id>
    <date>
      <year-start>1642</year-start>
      <year-end>1642</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Branle Parthenice</melody>
    <categories>
      <category translation="emotions">emoties</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'t NAtuirlijk quaat, dats drinken <rhyme label="a" type="f">speelen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">En <rhyme label="b" type="m">Hoerety</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Verraen bedrieghen woek'ren <rhyme label="a" type="f" corresp="#s1:l1">steelen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">En <rhyme label="b" type="m" corresp="#s1:l2">sropery</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sulke' en dierghelyke <rhyme label="c" type="f">daaden</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Meer als te <rhyme label="d" type="m">quaat</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Geschien ten deel om lust te <rhyme label="c" type="f" corresp="#s1:l5">zaaden</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Ten deel om <rhyme label="d" type="m" corresp="#s1:l6">baat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maar snooder boosenvint-men <rhyme label="a" type="f">zelken</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Hoe bits en <rhyme label="b" type="m">wreet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Als dte het goet met quaar <rhyme label="a" type="f" corresp="#s2:l1">vergelden</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">En't soet met <rhyme label="b" type="m" corresp="#s2:l2">loet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dat is een boosheyt at <rhyme label="c" type="f">gesondert</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">En <rhyme label="d" type="m">uyt-geroeyt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Waar van sich self een Dief <rhyme label="c" type="f" corresp="#s2:l5">verwonedrt</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En dat <rhyme label="d" type="m" corresp="#s2:l6">verfoeyr</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Loopers, Stroopers, Guyten en <rhyme label="a" type="f">Fielen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Of sulk <rhyme label="b" type="m">ghebroedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die hebben vaak veel eelder <rhyme label="a" type="f" corresp="#s3:l1">Zielen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">In haar <rhyme label="b" type="m" corresp="#s3:l2">ghemoedt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">De sulke dikwijls op de <rhyme label="c" type="f">weghen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Gebruykten <rhyme label="d" type="m">re'en</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Datse baar wel-daaders <rhyme label="c" type="f" corresp="#s3:l5">kreegen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">En vrindschap <rhyme label="d" type="m" corresp="#s3:l6">de'en</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Heyd'nen die int wilde <rhyme label="a" type="f">woonen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Barbaars en <rhyme label="b" type="m">stuyr</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">De deughdt mea deughde te <rhyme label="a" type="f" corresp="#s4:l1">beloonen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Leert haar <rhyme label="b" type="m" corresp="#s4:l2">natuyr</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Veel stomme beeften dankbaar <rhyme label="c" type="f">spreeken</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Door blyk en <rhyme label="d" type="m">schyn</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Betoonende met trek en <rhyme label="c" type="f" corresp="#s4:l5">teeken</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Verschuldt te <rhyme label="d" type="m" corresp="#s4:l6">syn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een Roomsche Slaaf heeft dank <rhyme label="a" type="f">bevonden</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">In ouder <rhyme label="b" type="m">eeuw</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">De welke had' den Poot <rhyme label="a" type="f" corresp="#s5:l1">verbonden</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Van eenen <rhyme label="b" type="m" corresp="#s5:l2">Leeuw</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Die't hemwel dubbel weer <rhyme label="c" type="f">beloonde</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">In synnen <rhyme label="d" type="m">noot</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">En hem gestadigh vrindt schap <rhyme label="c" type="f" corresp="#s5:l5">toonden</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Tot inder <rhyme label="d" type="m" corresp="#s5:l6">doodt</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ont menscht verbastert en <rhyme label="a" type="f">versteeken</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">En heel <rhyme label="b" type="m">veraerdt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Van schaamt van eer van deughdt <rhyme label="a" type="f" corresp="#s6:l1">geweeken</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">En gansch <rhyme label="b" type="m" corresp="#s6:l2">vervaert</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Een oncruyt schuym van al het <rhyme label="c" type="f">boose</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">En quaat <rhyme label="d" type="m">beleyt</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Is d'oversnoode <rhyme label="c" type="f" corresp="#s6:l5">Goddeloose</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Ondank <rhyme label="d" type="m" corresp="#s6:l6">baarheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Veel boosheyt siermen billik <rhyme label="a" type="f">straffen</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">(Tot nut der <rhyme label="b" type="m">go√¢n</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Om die (waert moog'lijk) <rhyme label="a" type="f" corresp="#s7:l1">afteschaffen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">En wegh te <rhyme label="b" type="m" corresp="#s7:l2">doen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Maar dees verhaare pest op <rhyme label="c" type="f">aarden</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Dit Helsche <rhyme label="d" type="m">zaat</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">En vreest geen Roeden Strop noch <rhyme label="c" type="f" corresp="#s7:l5">Swaarden</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Noch wet noch <rhyme label="d" type="m" corresp="#s7:l6">raadt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Vermits 'tondankbaar hart na <rhyme label="a" type="f">rechten</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Verbeurt geen <rhyme label="b" type="m">lijf</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dies is onmog'lijk te <rhyme label="a" type="f" corresp="#s8:l1">beslechten</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Dit valsch <rhyme label="b" type="m" corresp="#s8:l2">bedrijf</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En daarom sulke lien <rhyme label="c" type="f">verharden</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">In haar <rhyme label="d" type="m">fenijn</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Maar streekmen die met scharpe <rhyme label="c" type="f" corresp="#s8:l5">Garden</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">'tSou beter <rhyme label="d" type="m" corresp="#s8:l6">syn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wat grouwel sietmen niet <rhyme label="a" type="f">gebeuren</rhyme>?</l>
        <l id="s9:l2" met="2" type="-+">Dat rijke <rhyme label="b" type="m">lien</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Na haar verarmde Ouders <rhyme label="a" type="f" corresp="#s9:l1">treuren</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Niet om en <rhyme label="b" type="m" corresp="#s9:l2">sien</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">En sluyten handen oor en <rhyme label="c" type="f">ooghen</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">En haar ge <rhyme label="d" type="m">moet</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Voor die haar hebben op <rhyme label="c" type="f" corresp="#s9:l5">getogen</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">En <rhyme label="d" type="m" corresp="#s9:l6">op-gevoedt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">En ook hoe veel verscheyden <rhyme label="a" type="f">vroomen</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Om Godes <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Wel armen hebben <rhyme label="a" type="f" corresp="#s10:l1">in-genomen</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">By haar <rhyme label="b" type="m" corresp="#s10:l2">gesin</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">En op-gequeekt als haare <rhyme label="c" type="f">kinders</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Veel jaeren <rhyme label="d" type="m">lank</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Die daar van leeden veele <rhyme label="c" type="f" corresp="#s10:l5">hinders</rhyme>,</l>
        <l id="s10:l8" met="2" type="-+">In plaets' van <rhyme label="d" type="m" corresp="#s10:l6">dank</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ick weet van sekere <rhyme label="a" type="f">gevangen</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Om <rhyme label="b" type="m">schendery</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">De welk, sond' hebben moet en <rhyme label="a" type="f" corresp="#s11:l1">hangen</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">'tEn waar door <rhyme label="b" type="m" corresp="#s11:l2">my</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Mits ik hem abelijk <rhyme label="c" type="f">verschooden</rhyme>,</l>
        <l id="s11:l6" met="2" type="-+">Met goedt <rhyme label="d" type="m">beleyt</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Die't my ook naderhant <rhyme label="c" type="f" corresp="#s11:l5">beloonden</rhyme>,</l>
        <l id="s11:l8" met="2" type="-+">Met <rhyme label="d" type="m" corresp="#s11:l6">spytigheyt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">O grooten Schepper hoogh <rhyme label="a" type="f">gepreesen</rhyme>,</l>
        <l id="s12:l2" met="2" type="-+">O eewigh <rhyme label="b" type="m">al</rhyme>:</l>
        <l id="s12:l3" met="4" type="-+">Hoe weyningh dank wert u <rhyme label="a" type="f" corresp="#s12:l1">beweesen</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Int droeve <rhyme label="b" type="m" corresp="#s12:l2">dal</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Die over-vloedighli laat <rhyme label="c" type="f">vlieten</rhyme>,</l>
        <l id="s12:l6" met="2" type="-+">Des lichaams <rhyme label="d" type="m">spys</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">En Cryght van die het meest <rhyme label="c" type="f" corresp="#s12:l5">genieten</rhyme>,</l>
        <l id="s12:l8" met="2" type="-+">De minste <rhyme label="d" type="m" corresp="#s12:l6">prijs</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Ghy haater van d'ondankb're <rhyme label="a" type="f">Zielen</rhyme>!</l>
        <l id="s13:l2" met="2" type="-+">Buygh ons <rhyme label="b" type="m">ghemoedt</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Ten eynde dat ons hart mach <rhyme label="a" type="f" corresp="#s13:l1">knielen</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">Tot waare <rhyme label="b" type="m" corresp="#s13:l2">boet</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">En geeven u staegh lofen <rhyme label="c" type="f">eere</rhyme>,</l>
        <l id="s13:l6" met="2" type="-+">In al ons <rhyme label="d" type="m">doen</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">Door Jesum Christum onsen <rhyme label="c" type="f" corresp="#s13:l5">Heere</rhyme>,</l>
        <l id="s13:l8" met="2" type="-+">Der sonden <rhyme label="d" type="m" corresp="#s13:l6">soen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
