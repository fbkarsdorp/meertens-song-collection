<song>
  <meta>
    <id>25248</id>
    <date>
      <year-start>1582</year-start>
      <year-end>1582</year-end>
    </date>
    <location>
      <orginal-name>bisdom utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
