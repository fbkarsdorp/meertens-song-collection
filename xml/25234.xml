  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O groote scepper van <rhyme label="a" type="m">aertrijck</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die d'aerd haer grond went om als <rhyme label="a" type="m" corresp="#s1:l1">slijck</rhyme></l>
        <l id="s1:l3" met="4" type="-+">verset die wateren <rhyme label="b" type="m">onstaech</rhyme></l>
        <l id="s1:l4" met="4" type="-+">hebt vast gemaect die aerde <rhyme label="b" type="m" corresp="#s1:l3">laech</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Om dat die sou voorbrengen <rhyme label="a" type="m">ras</rhyme></l>
        <l id="s2:l2" met="4" type="-+">der besten voijer, hoij, stroo, en <rhyme label="a" type="m" corresp="#s2:l1">gras</rhyme></l>
        <l id="s2:l3" met="4" type="-+">oock bloemen cierlijck van <rhyme label="b" type="m">aensien</rhyme></l>
        <l id="s2:l4" met="4" type="-+">met vruchten die ons doen <rhyme label="b" type="m" corresp="#s2:l3">verblien</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die aen gebrande wonden <rhyme label="a" type="m">coen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">der harten wilt doer gracij <rhyme label="a" type="m" corresp="#s3:l1">groen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">maken suijuer, doer traen het <rhyme label="b" type="m">werck</rhyme></l>
        <l id="s3:l4" met="4" type="-+">dat quae beroert oeck word <rhyme label="b" type="m" corresp="#s3:l3">onsterck</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maeck dat dees sijn gehoorsaem <rhyme label="a" type="m">v</rhyme></l>
        <l id="s4:l2" met="4" type="-+">gebot, ende haer wachten <rhyme label="a" type="m" corresp="#s4:l1">nu</rhyme></l>
        <l id="s4:l3" met="4" type="-+">van allen werk wat is <rhyme label="b" type="m">doetlijck</rhyme></l>
        <l id="s4:l4" met="4" type="-+">veruult met gratij <rhyme label="b" type="m" corresp="#s4:l3">ewelijck</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
