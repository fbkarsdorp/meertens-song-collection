<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>166059</id>
    <date>
      <year-start>1620</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag>new year song</tag>
    </tags>
    <melody>Ik lijd in het hart pijn ongewoon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O heer hout op / Drije plagen <rhyme label="a" type="m">zwaer</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Tot straff <rhyme label="b" type="f">beneden</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">twert u <rhyme label="b" type="f" corresp="#s1:l2">gebeden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Als oorloogh Fel, In dit nieuw <rhyme label="a" type="m" corresp="#s1:l1">Iaer</rhyme></l>
        <l id="s1:l5" met="2" type="-+">ver Iaecht om <rhyme label="b" type="f" corresp="#s1:l2">reden</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Die land en <rhyme label="b" type="f" corresp="#s1:l2">steden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Berooft, en brengt tot <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s1:l8" met="2" type="-+">vell schoone <rhyme label="d" type="f">vruchten</rhyme></l>
        <l id="s1:l9" met="3" type="-+">alsmen twaerom <rhyme label="c" type="m" corresp="#s1:l7">doorsiet</rhyme></l>
        <l id="s1:l10" met="3" type="-+">tvalt over al <rhyme label="c" type="m" corresp="#s1:l7">verdriet</rhyme></l>
        <l id="s1:l11" met="2" type="+-">weenen en <rhyme label="d" type="f" corresp="#s1:l8">suchten</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Tot straff doet godt, vuerige <rhyme label="a" type="m">pest</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Dagelicx <rhyme label="b" type="f">vermeeren</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Om noch <rhyme label="b" type="f" corresp="#s2:l2">bekeren</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Eer dat de Doot, stryckt al de <rhyme label="a" type="m" corresp="#s2:l1">rest</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Die t'quaet voort <rhyme label="b" type="f" corresp="#s2:l2">leeren</rhyme></l>
        <l id="s2:l6" met="2" type="-+">oock wel <rhyme label="b" type="f" corresp="#s2:l2">begeeren</rhyme></l>
        <l id="s2:l7" met="3" type="-+">hier In <rhyme label="c" type="m">wellusticheyt</rhyme></l>
        <l id="s2:l8" met="2" type="-+">altyts te <rhyme label="d" type="f">blijven</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Daer staet u huijs <rhyme label="c" type="m" corresp="#s2:l7">bereyt</rhyme></l>
        <l id="s2:l10" met="3" type="-+">langtyt niet <rhyme label="c" type="m" corresp="#s2:l7">toegeseyt</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Tot vreuchts <rhyme label="d" type="f" corresp="#s2:l8">bedrijven</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De klacht over den Dieren <rhyme label="a" type="m">tyt</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Rycken nyet <rhyme label="b" type="f">hooren</rhyme></l>
        <l id="s3:l3" met="2" type="-+">stoppen de <rhyme label="b" type="f" corresp="#s3:l2">Ooren</rhyme></l>
        <l id="s3:l4" met="4" type="-+">geen acht op die den honger <rhyme label="a" type="m" corresp="#s3:l1">lijt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">winters te <rhyme label="b" type="f" corresp="#s3:l2">vooren</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Laten <rhyme label="b" type="f" corresp="#s3:l2">versmoren</rhyme></l>
        <l id="s3:l7" met="3" type="-+">In Bethlehem by <rhyme label="c" type="m">nacht</rhyme></l>
        <l id="s3:l8" met="2" type="-+">veel arme <rhyme label="d" type="f">Dieren</rhyme></l>
        <l id="s3:l9" met="3" type="-+">wat loon nae wort <rhyme label="c" type="m" corresp="#s3:l7">verwacht</rhyme></l>
        <l id="s3:l10" met="3" type="-+">wilt houden In <rhyme label="c" type="m" corresp="#s3:l7">gedacht</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Traeckt hart en <rhyme label="d" type="f" corresp="#s3:l8">nieren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Nae wensch keert godt t spaensche <rhyme label="a" type="m">gewelt</rhyme></l>
        <l id="s4:l2" met="2" type="-+">met wint en <rhyme label="b" type="f">regen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">nemt waer gods <rhyme label="b" type="f" corresp="#s4:l2">segen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Eer dat de vorst wateren <rhyme label="a" type="m" corresp="#s4:l1">velt</rhyme></l>
        <l id="s4:l5" met="2" type="-+">met ijs comt <rhyme label="b" type="f" corresp="#s4:l2">legen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">en vast <rhyme label="b" type="f" corresp="#s4:l2">belegen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">hout wacht voor swaer <rhyme label="c" type="m">aenstoot</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Op u <rhyme label="d" type="f">bolwercken</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">vreest voor geen vyand <rhyme label="c" type="m" corresp="#s4:l7">groot</rhyme></l>
        <l id="s4:l10" met="3" type="-+">maer het hoe haest de <rhyme label="c" type="m" corresp="#s4:l7">Doot</rhyme></l>
        <l id="s4:l11" met="2" type="+-">verwint den <rhyme label="d" type="f" corresp="#s4:l8">stercken</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Prins almachtich van <rhyme label="a" type="m">tbegin</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Diet al doet <rhyme label="b" type="f">vresen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">sal rechter <rhyme label="b" type="f" corresp="#s5:l2">wesen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Off werelt gierich om <rhyme label="a" type="m" corresp="#s5:l1">gewin</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Tnieuw Iaer wilt <rhyme label="b" type="f" corresp="#s5:l2">lezen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">straff blyckt by <rhyme label="b" type="f" corresp="#s5:l2">desen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">aen plaegen Drye <rhyme label="c" type="m">bekent</rhyme></l>
        <l id="s5:l8" met="2" type="-+">De <rhyme label="d" type="f">Nederlanden</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Daer lieffd'es <rhyme label="c" type="m" corresp="#s5:l7">t'Fondament</rhyme></l>
        <l id="s5:l10" met="3" type="-+">wenscht d'acoleijen <rhyme label="c" type="m" corresp="#s5:l7">jent</rhyme></l>
        <l id="s5:l11" met="2" type="+-">vrede voor <rhyme label="d" type="f" corresp="#s5:l8">handen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
