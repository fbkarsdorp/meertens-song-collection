  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Schoone <rhyme label="a" type="m">beelt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Iou blicken hebben <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Zoo dat'et my <rhyme label="a" type="m" corresp="#s1:l1">verveelt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wanneer myn Orsel <rhyme label="b" type="m" corresp="#s1:l2">lacht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Want ziet jou <rhyme label="c" type="m">zoetebeck</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Want ziet jou <rhyme label="c" type="m" corresp="#s1:l5">zoetebeck</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Want ziet jou <rhyme label="c" type="m" corresp="#s1:l5">zoetebeck</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Mackt Ioseps hart schier <rhyme label="c" type="m" corresp="#s1:l5">geck</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Iou schoone <rhyme label="a" type="m">snats</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Acht ick meer weert te <rhyme label="b" type="m">zyn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Als hondert tonnen <rhyme label="a" type="m" corresp="#s2:l1">schats</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Elk lipjen als <rhyme label="b" type="m" corresp="#s2:l2">Robyn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Of eer gelyk la <rhyme label="c" type="m">Lazuur</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Of eer gelyk la <rhyme label="c" type="m" corresp="#s2:l5">Lazuur</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Of eer gelyk la <rhyme label="c" type="m" corresp="#s2:l5">Lazuur</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Is my een paartjens <rhyme label="c" type="m" corresp="#s2:l5">vuur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Iou schoone <rhyme label="a" type="m">blos</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Geeft kraproot weinich <rhyme label="b" type="m">toe</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Haer veur hooft als een <rhyme label="a" type="m" corresp="#s3:l1">os</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer in sie ick me <rhyme label="b" type="m" corresp="#s3:l2">moe</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Door rimpel keuck en <rhyme label="c" type="m">vouw</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Door rimpel keuck en <rhyme label="c" type="m" corresp="#s3:l5">vouw</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Door rimpel keuck en <rhyme label="c" type="m" corresp="#s3:l5">vouw</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Lyckt ghy een wyse <rhyme label="c" type="m" corresp="#s3:l5">vrouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Iou schoone <rhyme label="a" type="m">hair</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Daer vang jy harten <rhyme label="b" type="m">meê</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Lyk ik en lyk een <rhyme label="a" type="m" corresp="#s4:l1">aer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Wel wisten wat men <rhyme label="b" type="m" corresp="#s4:l2">leê</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wanneer m'een soentje <rhyme label="c" type="m">kreeg</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wanneer m'een soentje <rhyme label="c" type="m" corresp="#s4:l5">kreeg</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wanneer m'een soentje <rhyme label="c" type="m" corresp="#s4:l5">kreeg</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Jou hayr was in de <rhyme label="c" type="m" corresp="#s4:l5">weeg</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Iou schoone <rhyme label="a" type="m">kleur</rhyme></l>
        <l id="s5:l2" met="3" type="-+">gelyck een <rhyme label="b" type="m">regen-boogh</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Maeckt my jou <rhyme label="a" type="m" corresp="#s5:l1">serviteur</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daer op viel eerst myn <rhyme label="b" type="m" corresp="#s5:l2">oogh</rhyme></l>
        <l id="s5:l5" met="3" type="-+">En dan jou korten <rhyme label="c" type="m">hals</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En dan jou korten <rhyme label="c" type="m" corresp="#s5:l5">hals</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En dan jou korten <rhyme label="c" type="m" corresp="#s5:l5">hals</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die is volmaeckt in <rhyme label="c" type="m" corresp="#s5:l5">als</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Het lyckt een <rhyme label="a" type="m">way</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Voor 't voetige <rhyme label="b" type="m">gediert</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Dat zonder veel <rhyme label="a" type="m" corresp="#s6:l1">geschray</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Zoo deur malkander <rhyme label="b" type="m" corresp="#s6:l2">zwiert</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Als d'Engelen op de <rhyme label="c" type="m">leer</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Als d'Engelen op de <rhyme label="c" type="m" corresp="#s6:l5">leer</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Als d'Engelen op de <rhyme label="c" type="m" corresp="#s6:l5">leer</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Van Iacob op en <rhyme label="c" type="m" corresp="#s6:l5">neer</rhyme>.</l>
      </lg>
    </lg>
  </text>
