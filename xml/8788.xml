<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8788</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ach Gott vom Himmel</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ACh GODt van hemel siet daer <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En laet u dat <rhyme label="b" type="f">ERbarmen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hoe weynich syn de Heylgen <rhyme label="a" type="m" corresp="#s1:l1">dijn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verlaten syn wy <rhyme label="b" type="f" corresp="#s1:l2">Armen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dijn Woort en laet men niet syn <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">T'gelooff is wt gheblusschet <rhyme label="c" type="m" corresp="#s1:l5">gaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">By allen Menschen <rhyme label="d" type="f">Kindren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sy leeren ydel valsche <rhyme label="a" type="m">List</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wat eyghen Wijsheyt <rhyme label="b" type="f">dichtet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Haer Hert niet eens ghesinnens <rhyme label="a" type="m" corresp="#s2:l1">is</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In GOdes Woort <rhyme label="b" type="f" corresp="#s2:l2">ghestichtet</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Die Een kiest dit, die Ander <rhyme label="c" type="m">dat</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Verstroyen ons al boven <rhyme label="c" type="m" corresp="#s2:l5">maet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En blincken schoon van <rhyme label="d" type="f">buyten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Godt wils' wtroepen alle <rhyme label="a" type="m">gaer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die Valschen schijn ons <rhyme label="b" type="f">leeren</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">En noch haer Tongh' stout <rhyme label="a" type="m" corresp="#s3:l1">openbaer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Spreeckt trots, wie wilt ons <rhyme label="b" type="f" corresp="#s3:l2">keeren</rhyme>?</l>
        <l id="s3:l5" met="4" type="-+">Wy hebben Recht en Macht <rhyme label="c" type="m">alleyn</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Wat wy setten dat gelt <rhyme label="c" type="m" corresp="#s3:l5">ghemeyn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wie is die ons sou <rhyme label="d" type="f">meyst'ren</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daerom spreeckt God: Ick moet op <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die Armen syn <rhyme label="b" type="f">verstooret</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Haer suchten dringt tot mijn hier <rhyme label="a" type="m" corresp="#s4:l1">in</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ick heb haer clagh' <rhyme label="b" type="f" corresp="#s4:l2">verhooret</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Mijn heylsaem woort sal op de <rhyme label="c" type="m">baen</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Ghetroost en frisch sy grijpen <rhyme label="c" type="m" corresp="#s4:l5">aen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En zijn die cracht der <rhyme label="d" type="f">Armen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat Silver door t'vyer <rhyme label="a" type="m">sevenmael</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Beproeft, wort net <rhyme label="b" type="f">bevonden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Aen GOdes woort men houden <rhyme label="a" type="m" corresp="#s5:l1">sal</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Vast blijven t'allen <rhyme label="b" type="f" corresp="#s5:l2">stonden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Het wil door t'Cruys beproevet <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Dan wort ghekent syn cracht en <rhyme label="c" type="m" corresp="#s5:l5">schijn</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En leucht sterck in den <rhyme label="d" type="f">Lande</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dat wilt ghy Godt bewaren <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Al voor dat Argh' <rhyme label="b" type="f">gheslechte</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En laet ons dy bevolen <rhyme label="a" type="m" corresp="#s6:l1">zijn</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat hem in ons niet <rhyme label="b" type="f" corresp="#s6:l2">vlechte</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Die Godloos' hoop hem altoos <rhyme label="c" type="m">vindt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Waer dese loose lieden <rhyme label="c" type="m" corresp="#s6:l5">sindt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">In dijnen Volck <rhyme label="d" type="f">verheven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
