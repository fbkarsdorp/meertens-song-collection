    <melody>Al mijn geluk</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hemel welk een <rhyme label="a" type="m">smart</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gevoel ik dag en <rhyme label="b" type="f">nagten</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Myn jeugdig <rhyme label="a" type="m" corresp="#s1:l1">hart</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Moet hier in druk <rhyme label="b" type="f" corresp="#s1:l2">versmagten</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">O welk een <rhyme label="a" type="m" corresp="#s1:l1">smart</rhyme>!</l>
        <l id="s1:l6" met="2" type="-+">Een steenen <rhyme label="a" type="m" corresp="#s1:l1">hart</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Beweegd zelfs door myn <rhyme label="b" type="f" corresp="#s1:l2">klagten</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Maar Hemel wat <rhyme label="c" type="m">verdriet</rhyme>!</l>
        <l id="s1:l9" met="3" type="-+">Myn voogden achten 't <rhyme label="c" type="m" corresp="#s1:l8">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Komt Leeuwen <rhyme label="a" type="m">wreed</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wilt myn jong hert <rhyme label="b" type="f">verscheuren</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Den Hemel <rhyme label="a" type="m" corresp="#s2:l1">weet</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Wat my nog zal <rhyme label="b" type="f" corresp="#s2:l2">gebeuren</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Myn jeugdig <rhyme label="a" type="m" corresp="#s2:l1">hert</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Vergaat in <rhyme label="a" type="m" corresp="#s2:l1">smert</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">'t Moet om een minnaar <rhyme label="b" type="f" corresp="#s2:l2">treuren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Een minnaar, ja een <rhyme label="c" type="m">vrind</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Die my zo teder <rhyme label="c" type="m" corresp="#s2:l8">mind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy is 't die ik <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mogt ik hem nog eens <rhyme label="b" type="f">groeten</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Ik wierd uw <rhyme label="a" type="m" corresp="#s3:l1">slavin</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ik vial neder voor uw <rhyme label="b" type="f" corresp="#s3:l2">voeten</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ach Oom en <rhyme label="a" type="m" corresp="#s3:l1">Voogd</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Dit tog <rhyme label="a" type="m" corresp="#s3:l1">gedoogt</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Gy kunt myn leed <rhyme label="b" type="f" corresp="#s3:l2">verzoeten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Geeft my myn minnaar <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Die ik bemin zo <rhyme label="c" type="m" corresp="#s3:l8">teer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Moeder lief <rhyme label="a" type="m">bemind</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Zo ik iets heb <rhyme label="b" type="f">misdreven</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Spaar tog uw <rhyme label="a" type="m" corresp="#s4:l1">kind</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Wil haar die fout <rhyme label="b" type="f" corresp="#s4:l2">vergeven</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">Beweegt uw <rhyme label="a" type="m" corresp="#s4:l1">hert</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Verligt myn <rhyme label="a" type="m" corresp="#s4:l1">smert</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Vergunt my vry te <rhyme label="b" type="f" corresp="#s4:l2">leven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Of neemt my 't leven <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Begraaft my in het <rhyme label="c" type="m" corresp="#s4:l8">graf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Pardonneer my <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En wilt myn een niet <rhyme label="b" type="f">breeken</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Als 't wezen <rhyme label="a" type="m" corresp="#s5:l1">kan</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Beweegt door minzaam <rhyme label="b" type="f" corresp="#s5:l2">smeeken</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">En schenk aan <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Dat ik weer <rhyme label="a" type="m" corresp="#s5:l1">vry</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Mag met myn minnaar <rhyme label="b" type="f" corresp="#s5:l2">spreeken</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Ach helpt myn uit dit <rhyme label="c" type="m">slot</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Verbreek myn treurig <rhyme label="c" type="m" corresp="#s5:l8">lot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
