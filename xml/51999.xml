<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>51999</id>
    <date>
      <year-start>1820</year-start>
      <year-end>1820</year-end>
    </date>
    <tags>
      <tag translation="love song">liefdeslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Een Brabantsch <rhyme label="a" type="f">boerinneke</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Ging eens naar de <rhyme label="b" type="m">stad</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Om kermis te <rhyme label="c" type="f">houden</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als ze een vrijer <rhyme label="b" type="m" corresp="#s1:l2">had</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Jan was aan t <rhyme label="d" type="f">wandelen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Zij stapt uit den <rhyme label="e" type="m">tram</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">En hij vroeg: <rhyme label="d" type="f" corresp="#s1:l5">‘boerinneke</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Zoekt gij een <rhyme label="e" type="m" corresp="#s1:l6">jongman</rhyme>?’</l>
        <l id="s1:l9" met="3" type="+-">Zoo dansen we eer 't op <rhyme label="f" type="f">is</rhyme> -</l>
        <l id="s1:l10" met="2" type="+-">Eer 't op is - eer 't op <rhyme label="f" type="f" corresp="#s1:l9">is</rhyme>!</l>
        <l id="s1:l11" met="3" type="-+">Zoo dansen we eer 't op <rhyme label="f" type="f" corresp="#s1:l9">is</rhyme> -</l>
        <l id="s1:l12" met="3" type="-+">Als t op is dansen we <rhyme label="g" type="m">nog</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Zij kwam hem toen te <rhyme label="a" type="f">spreken</rhyme></l>
        <l id="s2:l2" met="3" type="+-">En Jan was <rhyme label="b" type="m">ontsteld</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">'k Zou t kansje <rhyme label="c" type="f">waarnemen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Maar mij ontbreekt <rhyme label="b" type="m" corresp="#s2:l2">geld</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Zij kwam hem te <rhyme label="d" type="f">geven</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Haar <rhyme label="e" type="m">portemonnaie</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Voorzien van <rhyme label="d" type="f" corresp="#s2:l5">schijven</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En Jan lacht <rhyme label="e" type="m" corresp="#s2:l6">mee</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">Zoo dansen we eer 't op <rhyme label="f" type="f">is</rhyme> -</l>
        <l id="s2:l10" met="2" type="+-">Eer 't op is - eer 't op <rhyme label="f" type="f" corresp="#s2:l9">is</rhyme>!</l>
        <l id="s2:l11" met="3" type="-+">Zoo dansen we eer 't op <rhyme label="f" type="f" corresp="#s2:l9">is</rhyme> -</l>
        <l id="s2:l12" met="3" type="-+">Als t op is dansen we <rhyme label="g" type="m">nog</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Ze ging met hem <rhyme label="a" type="f">wandelen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">En hing aan zijn <rhyme label="b" type="m">zij</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Zoo kwamen ze op de <rhyme label="c" type="f">kermis</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Veel gingen <rhyme label="b" type="m" corresp="#s3:l2">voorbij</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Daar gaat Jan den <rhyme label="d" type="f">Snijder</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Met zijn kalen <rhyme label="e" type="m">jas</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">Ja zoo een <rhyme label="d" type="f" corresp="#s3:l5">boerinneke</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Komt hem van <rhyme label="e" type="m" corresp="#s3:l6">pas</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">Zoo dansen we eer 't op <rhyme label="f" type="f">is</rhyme> -</l>
        <l id="s3:l10" met="2" type="+-">Eer 't op is - eer 't op <rhyme label="f" type="f" corresp="#s3:l9">is</rhyme>!</l>
        <l id="s3:l11" met="3" type="-+">Zoo dansen we eer 't op <rhyme label="f" type="f" corresp="#s3:l9">is</rhyme> -</l>
        <l id="s3:l12" met="3" type="-+">Als t op is dansen we <rhyme label="g" type="m">nog</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Laarzen zonder <rhyme label="a" type="f">zolen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Een broek gelapt <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Zijn jaske dood <rhyme label="c" type="f">versleten</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Een papieren kraagje <rhyme label="b" type="m" corresp="#s4:l2">aan</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Maar zij ging hem <rhyme label="d" type="f">koopen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Een broek en een <rhyme label="e" type="m">jas</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Doch om te gaan <rhyme label="d" type="f" corresp="#s4:l5">dansen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Komen laarzen te <rhyme label="e" type="m" corresp="#s4:l6">pas</rhyme>.</l>
        <l id="s4:l9" met="3" type="+-">Zoo dansen we eer 't op <rhyme label="f" type="f">is</rhyme> -</l>
        <l id="s4:l10" met="2" type="+-">Eer 't op is - eer 't op <rhyme label="f" type="f" corresp="#s4:l9">is</rhyme>!</l>
        <l id="s4:l11" met="3" type="-+">Zoo dansen we eer 't op <rhyme label="f" type="f" corresp="#s4:l9">is</rhyme> -</l>
        <l id="s4:l12" met="3" type="-+">Als t op is dansen we <rhyme label="g" type="m">nog</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Zij dansen tot dat <rhyme label="a" type="f">er</rhyme></l>
        <l id="s5:l2" met="3" type="+-">De avond <rhyme label="b" type="m">aankwam</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">En zij sprak: mijn <rhyme label="c" type="f">Jantje</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Breng mij naar den <rhyme label="b" type="m" corresp="#s5:l2">tram</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Ik ben eenige <rhyme label="d" type="f">dochter</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Met veel geld en <rhyme label="e" type="m">goed</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Wilt gij met me <rhyme label="d" type="f" corresp="#s5:l5">trouwen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Doe het dan met <rhyme label="e" type="m" corresp="#s5:l6">spoed</rhyme>.</l>
        <l id="s5:l9" met="3" type="+-">Zoo dansen we eer 't op <rhyme label="f" type="f">is</rhyme> -</l>
        <l id="s5:l10" met="2" type="+-">Eer 't op is - eer 't op <rhyme label="f" type="f" corresp="#s5:l9">is</rhyme>!</l>
        <l id="s5:l11" met="3" type="-+">Zoo dansen we eer 't op <rhyme label="f" type="f" corresp="#s5:l9">is</rhyme> -</l>
        <l id="s5:l12" met="3" type="-+">Als t op is dansen we <rhyme label="g" type="m">nog</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Twee dagen <rhyme label="a" type="f">geleden</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Kreeg Jantje een <rhyme label="b" type="m">brief</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Waarin was <rhyme label="c" type="f">geschreven</rhyme>;</l>
        <l id="s6:l4" met="3" type="+-">O Jan 'k heb uw <rhyme label="b" type="m" corresp="#s6:l2">lief</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Uw dansen, uw <rhyme label="d" type="f">springen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Heeft u niet <rhyme label="e" type="m">berouwd</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">En binnen vier <rhyme label="d" type="f" corresp="#s6:l5">weken</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Was de snijder <rhyme label="e" type="m" corresp="#s6:l6">getrouwd</rhyme>.</l>
        <l id="s6:l9" met="3" type="+-">Zoo dansen we eer 't op <rhyme label="f" type="f">is</rhyme> -</l>
        <l id="s6:l10" met="2" type="+-">Eer t op is - eer 't op <rhyme label="f" type="f" corresp="#s6:l9">is</rhyme> -</l>
        <l id="s6:l11" met="3" type="-+">Zoo dansen we eer t op <rhyme label="f" type="f" corresp="#s6:l9">is</rhyme> -</l>
        <l id="s6:l12" met="3" type="-+">Als 't op is dansen we <rhyme label="g" type="m">nog</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
