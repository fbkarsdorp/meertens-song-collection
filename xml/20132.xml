  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">SOete zieltje, meesteresje van mijn <rhyme label="a" type="m">hart</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Verhoor u Minnaer, ach! versacht dien <rhyme label="a" type="m" corresp="#s1:l1">smart</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Verhoor u Minnaer, ach! versacht dien <rhyme label="a" type="m" corresp="#s1:l1">smart</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die ghy door ’t wreede strijen, doet <rhyme label="b" type="f">lijen</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Mijn Zieltje, och! u stuursheydt valt my <rhyme label="c" type="m">hart</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Mijn zieltje, och! <rhyme label="d" type="f">u</rhyme></l>
        <l id="s1:l7" met="5" type="+-">Mijn zieltje, och! u stuursheyt valt my <rhyme label="c" type="m" corresp="#s1:l5">hart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Segh wat reden (roemwaerde <rhyme label="a" type="m">Engelin</rhyme>)</l>
        <l id="s2:l2" met="5" type="-+">Dat ghy met strafheydt loont mijn trouwe <rhyme label="a" type="m" corresp="#s2:l1">min</rhyme>?</l>
        <l id="s2:l3" met="5" type="-+">Dat ghy met strafheydt loont mijn trouwe <rhyme label="a" type="m" corresp="#s2:l1">min</rhyme>?</l>
        <l id="s2:l4" met="4" type="-+">Laet ick u hart door ’t smeecken, eens <rhyme label="b" type="f">breecken</rhyme>;</l>
        <l id="s2:l5" met="5" type="+-">Ay! toont my weder liefde <rhyme label="c" type="m">(Aerdsch-godin</rhyme>)</l>
        <l id="s2:l6" met="2" type="-+">Ay! toont, my <rhyme label="d" type="f">weder</rhyme></l>
        <l id="s2:l7" met="5" type="+-">Ay! toont my weder liefde <rhyme label="c" type="m" corresp="#s2:l5">(Aerdsch-godin</rhyme>)</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Vraeght ghy d’ oorsaeck? ’t is dat u reden <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Als windt, of als beloften by de <rhyme label="a" type="m" corresp="#s3:l1">Wijn</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Als windt, of als beloften by de <rhyme label="a" type="m" corresp="#s3:l1">Wijn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">V woorden ’t hart niet raken, wilt <rhyme label="b" type="f">staken</rhyme></l>
        <l id="s3:l5" met="5" type="+-">Dit smeken, treckt u sinnen vry van <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dit smeken, treckt <rhyme label="d" type="f">u</rhyme></l>
        <l id="s3:l7" met="5" type="+-">Dit smeken, treckt u sinnen vry van <rhyme label="c" type="m" corresp="#s3:l5">mijn</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Wel dan (Waerde) aenvaert dit gladde <rhyme label="a" type="m">stael</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Doorstoot mijn boesem nu, opdat ick <rhyme label="a" type="m" corresp="#s4:l1">dael</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Doorstoot mijn boesem nu, opdat ick <rhyme label="a" type="m" corresp="#s4:l1">dael</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">By de verdoemde Ziele, die <rhyme label="b" type="f">krielen</rhyme></l>
        <l id="s4:l5" met="5" type="+-">In nare naerheydt, daer noyt Son en <rhyme label="c" type="m">strael</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">In nare <rhyme label="d" type="f">naerheydt</rhyme></l>
        <l id="s4:l7" met="5" type="+-">In nare naerheydt, daer noyt Son en <rhyme label="c" type="m" corresp="#s4:l5">strael</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Dan sal zweven mijn schaduw steedts om <rhyme label="a" type="m">hoogh</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">En tuygen van mijn trouwe Min, <rhyme label="a" type="m" corresp="#s5:l1">gedoogh</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">En tuygen van mijn trouwe Min, <rhyme label="a" type="m" corresp="#s5:l1">gedoogh</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dat hy eens u kake, <rhyme label="b" type="f">genake</rhyme>,</l>
        <l id="s5:l5" met="5" type="+-">Of slae ten minsten eens op hem u <rhyme label="c" type="m">oogh</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Of slae ten <rhyme label="d" type="f">minsten</rhyme></l>
        <l id="s5:l7" met="5" type="+-">Of slae ten minsten eens op hem u <rhyme label="c" type="m" corresp="#s5:l5">oogh</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="+-">Hy sal dalen gerust weer na <rhyme label="a" type="m">beneen</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Soud’ ick u moorden, Felicks, neen, o <rhyme label="a" type="m" corresp="#s6:l1">neen</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Soud’ ick u moorden, Felicks, neen, o <rhyme label="a" type="m" corresp="#s6:l1">neen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Staet op, ick proefde u minnen: <rhyme label="b" type="f">verwinnen</rhyme></l>
        <l id="s6:l5" met="5" type="+-">Keunt ghy mijn hartje door u gegronde <rhyme label="c" type="m">reen</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Keunt ghy mijn <rhyme label="d" type="f">hartje</rhyme>,</l>
        <l id="s6:l7" met="5" type="+-">Keunt ghy mijn hartje door u gegronde <rhyme label="c" type="m" corresp="#s6:l5">reen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
