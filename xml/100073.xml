  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn Engel is vol <rhyme label="a" type="f">deuchden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van d' Engelen <rhyme label="b" type="m">bemindt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En 's Heemels hoogste <rhyme label="a" type="f" corresp="#s1:l1">vreuchden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En 's weerelts beste <rhyme label="b" type="m" corresp="#s1:l2">kindt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Wil ick daer een kusje <rhyme label="c" type="f">geven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">S' is bereyt, en wel te <rhyme label="d" type="m">vreen</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Maer soo ick <rhyme label="e" type="m">meer</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Van haer <rhyme label="e" type="m" corresp="#s1:l7">begeer</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Comje weer soo meugje <rhyme label="c" type="f" corresp="#s1:l5">vresen</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Seytse en geeft m'er <rhyme label="d" type="m" corresp="#s1:l6">twee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick heb een les <rhyme label="a" type="f">gekregen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van Venus dertel <rhyme label="b" type="m">kindt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Geen liefde meer te <rhyme label="a" type="f" corresp="#s2:l1">plegen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Daer men geen weer lieffd' <rhyme label="b" type="m" corresp="#s2:l2">vindt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Roosjens wilt ick sonder <rhyme label="c" type="f">doorne</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Kusjes wil ick sonder <rhyme label="d" type="m">pijn</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Vraegje wat <rhyme label="e" type="m">mondt</rhyme>?</l>
        <l id="s2:l8" met="2" type="-+">Dat mijn die <rhyme label="e" type="m" corresp="#s2:l7">jondt</rhyme>:</l>
        <l id="s2:l9" met="4" type="+-">'t Is de schoone wel <rhyme label="c" type="f" corresp="#s2:l5">geboorne</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">'t Is by Phillis waert te <rhyme label="d" type="m" corresp="#s2:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick sie in Phillis <rhyme label="a" type="f">Oogen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De eeuwige Sterre <rhyme label="b" type="m">strael</rhyme></l>
        <l id="s3:l3" met="3" type="-+">En dat Iupijn <rhyme label="a" type="f" corresp="#s3:l1">bewoogen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Heeft uyt zijn Throon te <rhyme label="b" type="m" corresp="#s3:l2">gaen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ick sie een Hemel vol van <rhyme label="c" type="f">soetheyt</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">In haer Corale <rhyme label="d" type="m">monde</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ach wat een <rhyme label="e" type="m">pracht</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Ach wat een <rhyme label="e" type="m" corresp="#s3:l7">macht</rhyme></l>
        <l id="s3:l9" met="4" type="+-">'k Sie een Hemel vol van <rhyme label="c" type="f" corresp="#s3:l5">goetheyt</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Voor die Phillis heeft <rhyme label="d" type="m" corresp="#s3:l6">gewont</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
