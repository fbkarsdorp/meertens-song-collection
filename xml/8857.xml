<song>
  <meta>
    <id>8857</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Herr es sind Heiden</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEEr Heyden syn in u <rhyme label="a" type="m">Erfdeel</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met grooten grim <rhyme label="b" type="f">ghevallen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Sy hebben dijnen Tempel <rhyme label="a" type="m" corresp="#s1:l1">heel</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ontheylicht GOdt in <rhyme label="b" type="f" corresp="#s1:l2">Allen</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Want waer dijn Woort te woonen <rhyme label="c" type="m">plach</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Daer hoort men nu niet All' den <rhyme label="c" type="m" corresp="#s1:l5">Dach</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dan valsche Duyvels <rhyme label="d" type="f">Leere</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ach Godt staet op en <rhyme label="d" type="f" corresp="#s1:l7">keere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die Arme Christen gins en <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">De Wijven en die <rhyme label="b" type="f">Kinder</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die houwen sy doch al ter <rhyme label="a" type="m" corresp="#s2:l1">Neer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Verschoonend' haer veel <rhyme label="b" type="f" corresp="#s2:l2">minder</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dan Eenich Wilt verwoestet <rhyme label="c" type="m">Dier</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ach Godt staet op, End helpt ons <rhyme label="c" type="m" corresp="#s2:l5">schier</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Anders so ist <rhyme label="d" type="f">verlooren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Tot Moord' zijn sy <rhyme label="d" type="f" corresp="#s2:l7">ghebooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy hebben dijner Christen <rhyme label="a" type="m">Bloet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Aen allen Oort <rhyme label="b" type="f">vergooten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">(Daer by bekennen wy dijn <rhyme label="a" type="m" corresp="#s3:l1">Roed'</rhyme>)</l>
        <l id="s3:l4" met="3" type="-+">Alst Water ist <rhyme label="b" type="f" corresp="#s3:l2">ghevlooten</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Een schouw van ons heeft Ellick <rhyme label="c" type="m">een</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Als deeden Onrecht haerder <rhyme label="c" type="m" corresp="#s3:l5">gheen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat moeten wy dy <rhyme label="d" type="f">Claghen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ach GOdt help van den <rhyme label="d" type="f" corresp="#s3:l7">Plaghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wy mercken Heer dijn Thoorne <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hy brandt ons als een <rhyme label="b" type="f">Viere</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Daerom een yeder Christen <rhyme label="a" type="m" corresp="#s4:l1">sel</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Teghen des Vyants <rhyme label="b" type="f" corresp="#s4:l2">tieren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dy bidden, dat ghy uwen <rhyme label="c" type="m">grim</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Afwenden wilt van ons op <rhyme label="c" type="m" corresp="#s4:l5">hem</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">End' op de Boeven <rhyme label="d" type="f">tsame</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die niet achten dijn <rhyme label="d" type="f" corresp="#s4:l7">Name</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want sy nu dyne <rhyme label="a" type="m">Christenheyt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Schier hebben gantsch <rhyme label="b" type="f">verslonden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">End' ons oock tselve is <rhyme label="a" type="m" corresp="#s5:l1">bereyt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Al nu tot deser <rhyme label="b" type="f" corresp="#s5:l2">stonden</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Soo dijn handt sterck ons niet <rhyme label="c" type="m">bevrijdt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Schelt ons Heer onse sonden <rhyme label="c" type="m" corresp="#s5:l5">quijt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ontfermt dy haest van t <rhyme label="d" type="f">quade</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ons helpt door dijn <rhyme label="d" type="f" corresp="#s5:l7">ghenade</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Helpt ghy ons doch O Heere <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Alleen om dyns Naems <rhyme label="b" type="f">eere</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Verlost ons nu wt deser <rhyme label="a" type="m" corresp="#s6:l1">noot</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Al den godloosen <rhyme label="b" type="f" corresp="#s6:l2">weere</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Ons sonden hebbent wel <rhyme label="c" type="m">verschult</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Maer decktse, Heer, met dyner <rhyme label="c" type="m" corresp="#s6:l5">huld</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Doet dat om dynen <rhyme label="d" type="f">Name</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Keert de Vyanden <rhyme label="d" type="f" corresp="#s6:l7">tsame</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Waerom laet ghy die Heyden, <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Bespotten ons dus <rhyme label="b" type="f">seere</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Laet roemen haer, als hadden <rhyme label="a" type="m" corresp="#s7:l1">wy</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Gheen Godt die ons <rhyme label="b" type="f" corresp="#s7:l2">salveere</rhyme>?</l>
        <l id="s7:l5" met="4" type="-+">Ach Heer, gheeft yeder te <rhyme label="c" type="m">verstaen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Hoe veel de Vyandt heeft <rhyme label="c" type="m" corresp="#s7:l5">misdaen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Die syn Volck heeft door <rhyme label="d" type="f">steken</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Ws Knechten bloet wilt <rhyme label="d" type="f" corresp="#s7:l7">wreken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Laet comen lieve (HEEr) voor <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat suchten der <rhyme label="b" type="f">Ghevanghen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ghy zijt veel stercker doch dan <rhyme label="a" type="m" corresp="#s8:l1">sy</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die ons al willen <rhyme label="b" type="f" corresp="#s8:l2">hanghen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Den arghen Vyandt dat <rhyme label="c" type="m">verghelt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Die niet en doet dan dijn Volck <rhyme label="c" type="m" corresp="#s8:l5">quelt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Betaelt hem sulcke <rhyme label="d" type="f">smade</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Met Wraeck en <rhyme label="d" type="f" corresp="#s8:l7">Onghenade</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer wy dijn Volck dy danken <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s9:l2" met="3" type="-+">(Wy Schapen dijner <rhyme label="b" type="f">Weyden</rhyme>)</l>
        <l id="s9:l3" met="4" type="-+">In eewicheyt, en altijdt, <rhyme label="a" type="m" corresp="#s9:l1">HEER</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Dyn hulpe wy <rhyme label="b" type="f" corresp="#s9:l2">verbeyden</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">O Godt helpt ons door uwen <rhyme label="c" type="m">Christ</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Die ons van dy gheschoncken <rhyme label="c" type="m" corresp="#s9:l5">is</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Te vryden ons van <rhyme label="d" type="f">schanden</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Vertreden dyn <rhyme label="d" type="f" corresp="#s9:l7">Vyanden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
