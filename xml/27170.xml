      <tag>love song</tag>
      <tag>praise</tag>
    </tags>
    <melody>Schoon lief wil mij troost geven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Myn hert dat leyt <rhyme label="a" type="f">beuanghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">na v staet myn <rhyme label="a" type="f" corresp="#s1:l1">verlanghen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">dus lyd'ick groote <rhyme label="b" type="m">pyn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">och waer ick v <rhyme label="a" type="f" corresp="#s1:l1">geuanghen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">al hielt ghy my wat <rhyme label="a" type="f" corresp="#s1:l1">stranghe</rhyme></l>
        <l id="s1:l6" met="3" type="-+">ick coos gheen <rhyme label="b" type="m" corresp="#s1:l3">medezyn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon eedele <rhyme label="a" type="f">goddinne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">dien ick soe seer <rhyme label="a" type="f" corresp="#s2:l1">beminne</rhyme></l>
        <l id="s2:l3" met="3" type="-+">wilt toch gedachtich <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">myn hert geheel van <rhyme label="a" type="f" corresp="#s2:l1">binne</rhyme></l>
        <l id="s2:l5" met="3" type="-+">dat is oock om te <rhyme label="a" type="f" corresp="#s2:l1">winne</rhyme></l>
        <l id="s2:l6" met="3" type="-+">schoon lieff v claer <rhyme label="b" type="m" corresp="#s2:l3">aenschyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wilt toch myn bitter <rhyme label="a" type="f">karmen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">o reyn bloemken <rhyme label="a" type="f" corresp="#s3:l1">ontfarmen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">en weest niet <rhyme label="b" type="m">opstynaet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">mocht ick in vwen <rhyme label="a" type="f" corresp="#s3:l1">armen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">o liefste my <rhyme label="a" type="f" corresp="#s3:l1">verwarmen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">ghy sout my geuen <rhyme label="b" type="m" corresp="#s3:l3">baet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O maeghet <rhyme label="a" type="f">wtgheleesen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">v suyuerlycke <rhyme label="a" type="f" corresp="#s4:l1">weesen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">myn liefde groeyen <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">ghy wert van my <rhyme label="a" type="f" corresp="#s4:l1">gepreesen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">dat segh ick v by <rhyme label="a" type="f" corresp="#s4:l1">deesen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">wilt het nemen int <rhyme label="b" type="m" corresp="#s4:l3">goet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het is om v <rhyme label="a" type="f">courage</rhyme></l>
        <l id="s5:l2" met="3" type="-+">en v schoone <rhyme label="a" type="f" corresp="#s5:l1">visage</rhyme></l>
        <l id="s5:l3" met="3" type="-+">en om v borstkens <rhyme label="b" type="m">ront</rhyme></l>
        <l id="s5:l4" met="3" type="-+">ick synde vwe <rhyme label="a" type="f" corresp="#s5:l1">pagie</rhyme></l>
        <l id="s5:l5" met="3" type="-+">int hert hebt ghy <rhyme label="a" type="f" corresp="#s5:l1">loyagie</rhyme></l>
        <l id="s5:l6" met="3" type="-+">laet my cussen v <rhyme label="b" type="m" corresp="#s5:l3">mont</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nu wil ick oock by <rhyme label="a" type="f">deesen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">onder v dienste <rhyme label="a" type="f" corresp="#s6:l1">wesen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">en blyuen v <rhyme label="b" type="m">getrou</rhyme></l>
        <l id="s6:l4" met="3" type="-+">wilt ghy o maecht <rhyme label="a" type="f" corresp="#s6:l1">gepreesen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">so sal ick v oock <rhyme label="a" type="f" corresp="#s6:l1">weesen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">en wilt blyuen myn <rhyme label="b" type="m" corresp="#s6:l3">vrou</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dus wil ick by v <rhyme label="a" type="f">blyuen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">wilt my hier in <rhyme label="a" type="f" corresp="#s7:l1">geryuen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">myn dienste niet <rhyme label="b" type="m">versmaen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">al sou ick heel <rhyme label="a" type="f" corresp="#s7:l1">verstyuen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">soo sal ick v <rhyme label="a" type="f" corresp="#s7:l1">beclyuen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">of vreucht sou syn <rhyme label="b" type="m" corresp="#s7:l3">gedaen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Princesse schoon <rhyme label="a" type="f">goddinne</rhyme></l>
        <l id="s8:l2" met="3" type="-+">myn herte is vol <rhyme label="a" type="f" corresp="#s8:l1">minnen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">van v lieff <rhyme label="b" type="m">delicaet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">pirames wilt <rhyme label="a" type="f" corresp="#s8:l1">versinnen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">noch heeter is die <rhyme label="a" type="f" corresp="#s8:l1">minne</rhyme></l>
        <l id="s8:l6" met="3" type="-+">dus bid ick godt om <rhyme label="b" type="m" corresp="#s8:l3">baet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
