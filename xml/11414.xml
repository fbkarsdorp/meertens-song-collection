    <melody>Ne vous offensez madame</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIe doch souw de glory <rhyme label="a" type="f">strijcken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Soo nu quamen in 't <rhyme label="b" type="m">geschil</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die Goddinnen te <rhyme label="a" type="f" corresp="#s1:l1">gelijcken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Kampend om u schoonheydts <rhyme label="b" type="m" corresp="#s1:l2">wil</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Paris souw versuft gaen <rhyme label="a" type="f">deynsen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ia oock selfs de hooghste <rhyme label="b" type="m">Goon</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Venus om geen prijs meer <rhyme label="a" type="f" corresp="#s2:l1">peynsen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Want ghy zijt haer veel te <rhyme label="b" type="m" corresp="#s2:l2">schoon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ay Goddinne! laet vry <rhyme label="a" type="f">roemen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Alles wat daer schoonheyd <rhyme label="b" type="m">heeft</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">'t Is by u niet weerd om <rhyme label="a" type="f" corresp="#s3:l1">noemen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">'t Zy oock waer sy roert of <rhyme label="b" type="m" corresp="#s3:l2">leeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dat Homerus nu quam <rhyme label="a" type="f">kijcken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Waer bleef sijn Helena, <rhyme label="b" type="m">ach</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Pallas kan u niet <rhyme label="a" type="f" corresp="#s4:l1">gelijcken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Hoe veel min dan Juno <rhyme label="b" type="m" corresp="#s4:l2">mach</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Staeckt Angelica het <rhyme label="a" type="f">prysen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ghy Arminde oock met <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ick wil u een schoonder <rhyme label="a" type="f" corresp="#s5:l1">wysen</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Dat's mijn Nymph', en anders <rhyme label="b" type="m" corresp="#s5:l2">gheen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'t Is goet dat de kouwe <rhyme label="a" type="f">Aerde</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Laura nu bedeckt u <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Want ziet mijn Goddinne <rhyme label="a" type="f" corresp="#s6:l1">baerde</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Licht'lijck een Petrarcha <rhyme label="b" type="m" corresp="#s6:l2">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
