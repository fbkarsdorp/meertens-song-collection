    <melody>Fortuin helaas</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IVdea weest <rhyme label="a" type="m">verblijdt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Jerusalem maeckt <rhyme label="b" type="m">vreught</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Op desen gulden <rhyme label="a" type="m" corresp="#s1:l1">tijdt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Bekleedt u ziel met <rhyme label="b" type="m" corresp="#s1:l2">deught</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Allendigh Adams <rhyme label="c" type="m">zaet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Op dese hooghe <rhyme label="d" type="m">feest</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Toont vry een bly <rhyme label="c" type="m" corresp="#s1:l5">ghelaet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Verheught u in den <rhyme label="d" type="m" corresp="#s1:l6">gheest</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Rijst wt den diepen <rhyme label="a" type="m">slaep</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die u in sonden <rhyme label="b" type="m">hiel</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Want dat onnoosel <rhyme label="a" type="m" corresp="#s2:l1">Schaep</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Komt lossen uwe <rhyme label="b" type="m" corresp="#s2:l2">ziel</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Hy komt als eenen <rhyme label="c" type="m">mensch</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die Godt is vol <rhyme label="d" type="m">ghedult</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Om (naer u herten <rhyme label="c" type="m" corresp="#s2:l5">wensch</rhyme>)</l>
        <l id="s2:l8" met="3" type="-+">Te draeghen uwe <rhyme label="d" type="m" corresp="#s2:l6">schuldt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy Dochters (net, en <rhyme label="a" type="m">reyn</rhyme>)</l>
        <l id="s3:l2" met="3" type="-+">Der stadt <rhyme label="b" type="m">Jerusalem</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Gaet, ende siet hoe <rhyme label="a" type="m" corresp="#s3:l1">kleyn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat binnen <rhyme label="b" type="m" corresp="#s3:l2">BethleÃ«m</rhyme></l>
        <l id="s3:l5" met="3" type="-+">V Koningh leyt in't <rhyme label="c" type="m">stal</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En hoe sijn Moeder <rhyme label="d" type="m">schoon</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Heeft hem een kribbe <rhyme label="c" type="m" corresp="#s3:l5">smal</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Ghegheven voor sijn <rhyme label="d" type="m" corresp="#s3:l6">kroon'</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Siet, uwen Koningh <rhyme label="a" type="m">komt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Tot u <rhyme label="b" type="m">sacht-sinnelijck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">(Soo den Prophete <rhyme label="a" type="m" corresp="#s4:l1">romt</rhyme>)</l>
        <l id="s4:l4" met="3" type="-+">Ootmoedigh, <rhyme label="b" type="m" corresp="#s4:l2">minnelijck</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Siet, hoe dat <rhyme label="c" type="m">Opper-Goedt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat eeuwigh Vaders <rhyme label="d" type="m">Woordt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Bekleedt met vleesch en <rhyme label="c" type="m" corresp="#s4:l5">bloedt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Komt uyt een Maghet <rhyme label="d" type="m" corresp="#s4:l6">voort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die Moeder reyn en <rhyme label="a" type="m">net</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Naer dat sy heeft <rhyme label="b" type="m">ghebaert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Blijft Maghet <rhyme label="a" type="m" corresp="#s5:l1">onbesmet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">(Soo den Propheet <rhyme label="b" type="m" corresp="#s5:l2">verklaert</rhyme>)</l>
        <l id="s5:l5" met="3" type="-+">Want Godt der Heeren <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">(Wie suyverheydt <rhyme label="d" type="m">behaeght</rhyme>)</l>
        <l id="s5:l7" met="3" type="-+">Wauw doen sijn Moeder <rhyme label="c" type="m" corresp="#s5:l5">d'eer</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Altijdt te blijven <rhyme label="d" type="m" corresp="#s5:l6">Maeght</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dees' alder-eelste <rhyme label="a" type="m">Dracht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En Godt, en mensch te <rhyme label="b" type="m">gaer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Voor 'tmenschelijck <rhyme label="a" type="m" corresp="#s6:l1">gheslacht</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Komt wesen <rhyme label="b" type="m" corresp="#s6:l2">Middelaer</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Sijn weeldigh Jacobs <rhyme label="c" type="m">huys</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Daer hy bevelen <rhyme label="d" type="m">sal</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Werdt een seer schandigh <rhyme label="c" type="m" corresp="#s6:l5">kruys</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Voor onse sonden <rhyme label="d" type="m" corresp="#s6:l6">al</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Gaet nu en wilt <rhyme label="a" type="m">aen-sien</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die uwe ziele <rhyme label="b" type="m">mint</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Maer valt op uwe <rhyme label="a" type="m" corresp="#s7:l1">knien</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Voor dat gheboren <rhyme label="b" type="m" corresp="#s7:l2">Kindt</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Gheeft eenen soeten <rhyme label="c" type="m">kus</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Aen Iesus rooden <rhyme label="d" type="m">mondt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Op dat u ziel' <rhyme label="c" type="m" corresp="#s7:l5">aldus</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Door liefde zy <rhyme label="d" type="m" corresp="#s7:l6">ghewondt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Looft hem op dit <rhyme label="a" type="m">termijn</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die uyt een Maghets <rhyme label="b" type="m">slot</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Komt uwen broeder <rhyme label="a" type="m" corresp="#s8:l1">zijn</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Al is hy uwen <rhyme label="b" type="m" corresp="#s8:l2">Godt</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Dat eeuwigh Licht is <rhyme label="c" type="m">hy</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die nu gheboren <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Hy komt u maken <rhyme label="c" type="m" corresp="#s8:l5">vry</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Van alle <rhyme label="d" type="m" corresp="#s8:l6">duysternis</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
