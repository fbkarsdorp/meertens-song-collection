  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEldig Hansken die quam <rhyme label="a" type="m">my</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Gister-avont <rhyme label="b" type="f">vryen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maer 't quam uyt op <rhyme label="a" type="m" corresp="#s1:l1">filtery</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hy socht my’ Als een <rhyme label="a" type="m" corresp="#s1:l1">pry</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Schandigh te <rhyme label="b" type="f" corresp="#s1:l2">verleyen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hansken loerden als een <rhyme label="a" type="m">Muys</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Naer een lecker <rhyme label="b" type="f">bettjen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Maer syn doen en was niet <rhyme label="a" type="m" corresp="#s2:l1">pluys</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hanskens beck’ Socht het <rhyme label="a" type="m" corresp="#s2:l1">speck</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Uyt de val langhs 't <rhyme label="b" type="f" corresp="#s2:l2">splettjen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hans komt in de <rhyme label="a" type="m">Muyse-val</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Soo ghy speck wilt <rhyme label="b" type="f">fressen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Anders kryghdy <rhyme label="a" type="m" corresp="#s3:l1">niet-met-al</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Wordt eerst vast’ Oft wel <rhyme label="a" type="m" corresp="#s3:l1">vast</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">En soeckt vuyl <rhyme label="b" type="f" corresp="#s3:l2">Matressen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hans ick wil eerst zyn u <rhyme label="a" type="m">Bruyt</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Eer ick word' uw <rhyme label="b" type="f">Vrouwe</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Eer het speck wordt uwen <rhyme label="a" type="m" corresp="#s4:l1">buyt</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Moet de Maeght’ Die ghy <rhyme label="a" type="m" corresp="#s4:l1">vraeght</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Met u bruyloft hou <rhyme label="b" type="f" corresp="#s4:l2">wen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Weldig Hansken wou <rhyme label="a" type="m">nochtans</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Dat ich hem <rhyme label="b" type="f">gheriefde</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Maer syn liefden is <rhyme label="a" type="m" corresp="#s5:l1">ongans</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Synen brandt’ Streckt tot <rhyme label="a" type="m" corresp="#s5:l1">schant</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">'t Is vuyl Kalver <rhyme label="b" type="f" corresp="#s5:l2">liefde</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'t Kalf wilt ongebonden <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Hans en wilt niet <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Welden en staen Hans maer <rhyme label="a" type="m" corresp="#s6:l1">aen</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">Maer den geck’ En sal 't <rhyme label="a" type="m" corresp="#s6:l1">speck</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Uyt myn val niet <rhyme label="b" type="f" corresp="#s6:l2">krouwen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dat ick Hans de blom-kool <rhyme label="a" type="m">liet</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Van haer steltjen <rhyme label="b" type="f">stelen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Hansken liet my in 't <rhyme label="a" type="m" corresp="#s7:l1">verdriet</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En wat Man’ Sou <rhyme label="a" type="m" corresp="#s7:l1">als-dan</rhyme></l>
        <l id="s7:l5" met="3" type="+-">Trouwen dorre <rhyme label="b" type="f" corresp="#s7:l2">stelen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Semels daer de blom is <rhyme label="a" type="m">uyt</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Worden Verckens <rhyme label="b" type="f">koren</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Wie siet-uyt naer een vuyl' <rhyme label="a" type="m" corresp="#s8:l1">Bruyt</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Die den roem’ van haer <rhyme label="a" type="m" corresp="#s8:l1">bloem</rhyme></l>
        <l id="s8:l5" met="3" type="+-">Schandigh heeft <rhyme label="b" type="f" corresp="#s8:l2">verloren</rhyme>.</l>
      </lg>
    </lg>
  </text>
