<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>122537</id>
    <date>
      <year-start>1854</year-start>
      <year-end>1864</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>dialog song</tag>
      <tag>secular song</tag>
      <tag>love song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">In 't schuduw van de <rhyme label="a" type="f">boomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mag ik u dan <rhyme label="b" type="m">alleen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ter liefde <rhyme label="a" type="f" corresp="#s1:l1">bekroonen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Bekoorlijke <rhyme label="b" type="m" corresp="#s1:l2">Annet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Nu zing ik uit het <rhyme label="c" type="f">boekje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Een liedje van de <rhyme label="d" type="m">min</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat streelt bij dit <rhyme label="c" type="f" corresp="#s1:l5">bezoekje</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Op 't aangenaamst mijn <rhyme label="d" type="m" corresp="#s1:l6">zin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Neen, vleijende <rhyme label="a" type="f">Lubijn</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Ik zong daar op mijn <rhyme label="b" type="m">floort</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat nooit een maagd te <rhyme label="a" type="f" corresp="#s2:l1">zijn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wanneer ik heb <rhyme label="b" type="m" corresp="#s2:l2">gehoord</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Een minnaar te <rhyme label="c" type="f">smeken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">In wantrouw op zijn <rhyme label="d" type="m">woord</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Eer hij door looze <rhyme label="c" type="f" corresp="#s2:l5">streken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Mij al te ligt <rhyme label="d" type="m" corresp="#s2:l6">bekoord</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wantrouwt gij dan mijn <rhyme label="a" type="f">bede</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Belooft 't woord van <rhyme label="b" type="m">ja</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met duurzame <rhyme label="a" type="f" corresp="#s3:l1">eden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Slaat de smart niet <rhyme label="b" type="m" corresp="#s3:l2">gâ</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die ik om u moet <rhyme label="c" type="f">voelen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Door liefdesvuur <rhyme label="d" type="m">geblaakt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Gij kunt alleen mij <rhyme label="c" type="f" corresp="#s3:l5">koelen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wordt er toch door <rhyme label="d" type="m" corresp="#s3:l6">geraakt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik wil uw vuur niet <rhyme label="a" type="f">voeden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Opdat het mij niet <rhyme label="b" type="m">deer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'t Zal mij voor branden <rhyme label="a" type="f" corresp="#s4:l1">hoeden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gelooft mij op mijn <rhyme label="b" type="m" corresp="#s4:l2">eer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zou ik met losse <rhyme label="c" type="f">zinnen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Mijn blijde vrije <rhyme label="d" type="m">staat</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Verwisselen voor het <rhyme label="c" type="f" corresp="#s4:l5">minnen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Te zot is toch die <rhyme label="d" type="m" corresp="#s4:l6">praat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Veel zotter zijn de <rhyme label="a" type="f">gaven</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De schoonheid zonder <rhyme label="b" type="m">min</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Door tijd te zien <rhyme label="a" type="f" corresp="#s5:l1">begraven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En dat zonder <rhyme label="b" type="m" corresp="#s5:l2">gewin</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">De jeugd te zien <rhyme label="c" type="f">verbloeijen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Annette! och wat <rhyme label="d" type="m">verdriet</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Zou geen liefde in u <rhyme label="c" type="f" corresp="#s5:l5">groeijen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ach, schuw de min toch <rhyme label="d" type="m" corresp="#s5:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Zou ik dwang <rhyme label="a" type="f">gedoogen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hij smeed mijn <rhyme label="b" type="m">huwlijksjuk</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Als de liefde is <rhyme label="a" type="f" corresp="#s6:l1">vervlogen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Leeft men te veel in <rhyme label="b" type="m" corresp="#s6:l2">druk</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Ik zal uw eeuwig <rhyme label="c" type="f">minnen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Brengen u 't duurzaamst <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat liefde schenkt te <rhyme label="c" type="f" corresp="#s6:l5">binnen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Annette! gelijk gij <rhyme label="d" type="m" corresp="#s6:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ach, laat mij eenzaam <rhyme label="a" type="f">peinzen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Lubijn! laat mij <rhyme label="b" type="m">alleen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Kunt gij de min <rhyme label="a" type="f" corresp="#s7:l1">ontveinzen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">De liefde, o schoone! <rhyme label="b" type="m" corresp="#s7:l2">neen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Zou gij mij kunnen <rhyme label="c" type="f">haten</rhyme>?</l>
        <l id="s7:l6" met="3" type="-+">Ach, vraag mij nu niet <rhyme label="d" type="m">meer</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ik moet het veinzen <rhyme label="c" type="f" corresp="#s7:l5">laten</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Lubijn! ik min uw <rhyme label="d" type="m" corresp="#s7:l6">weêr</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Zie hoe de Goôn 't kan <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Altoos in mijn <rhyme label="b" type="m">heil</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">De deugd moet <rhyme label="a" type="f" corresp="#s8:l1">triumfeeren</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Schoon veracht een korte <rhyme label="b" type="m" corresp="#s8:l2">wijl</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Laat ik 't nu <rhyme label="c" type="f">aanhooren</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dus vol luister uit zijn <rhyme label="d" type="m">troon</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Wat voor mij is <rhyme label="c" type="f" corresp="#s8:l5">beschooren</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Annette tot mijn <rhyme label="d" type="m" corresp="#s8:l6">loon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
