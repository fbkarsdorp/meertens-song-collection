<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185834</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De tijd is hier</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Feestelijcken <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Gegroet so moet ghy <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Sonder <rhyme label="a" type="m" corresp="#s1:l1">geclach</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Tot alder tijt <rhyme label="c" type="m">geeert</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Weerdich <rhyme label="b" type="f" corresp="#s1:l2">gepresen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Al door den heer <rhyme label="b" type="f" corresp="#s1:l2">verresen</rhyme></l>
        <l id="s1:l7" met="2" type="-+"><rhyme label="c" type="m" corresp="#s1:l4">Gecelebreert</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En zijn <rhyme label="c" type="m" corresp="#s1:l4">gefestiveert</rhyme></l>
        <l id="s1:l9" met="2" type="-+"><rhyme label="c" type="m" corresp="#s1:l4">Getriumpheert</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Heeft Godt seer <rhyme label="d" type="m">vayliant</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Over de helsche <rhyme label="e" type="f">natie</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Nu sidt met grote <rhyme label="e" type="f" corresp="#s1:l11">statie</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Seer <rhyme label="d" type="m" corresp="#s1:l10">triumphant</rhyme></l>
        <l id="s1:l14" met="2" type="-+">Aen s’vaders <rhyme label="d" type="m" corresp="#s1:l10">rechterhant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Aensiet hoe <rhyme label="a" type="m">dray</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dees werelts lieflijck <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Lustich en <rhyme label="a" type="m" corresp="#s2:l1">fray</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Betuycht, en ons <rhyme label="c" type="m">beduyt</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Alle gaven mits <rhyme label="b" type="f" corresp="#s2:l2">desen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met haren Heer <rhyme label="b" type="f" corresp="#s2:l2">verresen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Doet spruyten <rhyme label="c" type="m" corresp="#s2:l4">uyt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dees Lenten tijt <rhyme label="c" type="m" corresp="#s2:l4">virtuyt</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Lover en <rhyme label="c" type="m" corresp="#s2:l4">cruyt</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Bewijst jolijt en <rhyme label="d" type="m">vruecht</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Om dat den Heere der <rhyme label="e" type="f">Heeren</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Men siet nu <rhyme label="e" type="f" corresp="#s2:l11">triumpheeren</rhyme></l>
        <l id="s2:l13" met="2" type="-+">T’wort al <rhyme label="d" type="m" corresp="#s2:l10">veriuecht</rhyme></l>
        <l id="s2:l14" met="2" type="-+">Met blijschap en <rhyme label="d" type="m" corresp="#s2:l10">gheneucht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dat groene <rhyme label="a" type="m">wout</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die Zee met haren <rhyme label="b" type="f">baren</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Die lichten <rhyme label="a" type="m" corresp="#s3:l1">menichvout</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Des hemels hier op <rhyme label="c" type="m">let</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Gods lof <rhyme label="b" type="f" corresp="#s3:l2">verclaeren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die gaet ten hemel <rhyme label="b" type="f" corresp="#s3:l2">varen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Als hy die <rhyme label="c" type="m" corresp="#s3:l4">wet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Der hellen heeft <rhyme label="c" type="m" corresp="#s3:l4">verplet</rhyme></l>
        <l id="s3:l9" met="2" type="-+">T’lam sonder <rhyme label="c" type="m" corresp="#s3:l4">smet</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Een offerhande <rhyme label="d" type="m">puer</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Heeftover al <rhyme label="e" type="f">regnatie</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Des looft met <rhyme label="e" type="f" corresp="#s3:l11">iubilatie</rhyme></l>
        <l id="s3:l13" met="2" type="-+">Alle <rhyme label="d" type="m" corresp="#s3:l10">creatuer</rhyme></l>
        <l id="s3:l14" met="2" type="-+">Haer schepper tot alder <rhyme label="d" type="m" corresp="#s3:l10">uer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
