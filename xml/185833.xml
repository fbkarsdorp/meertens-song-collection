<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185833</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De tijd is hier</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als Christus <rhyme label="a" type="m">triumphant</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Der glorien Coninck <rhyme label="b" type="f">machtich</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Sijn <rhyme label="a" type="m" corresp="#s1:l1">offerhant</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Volbracht had en <rhyme label="c" type="m">bestelt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hy daelde neder <rhyme label="b" type="f" corresp="#s1:l2">warachtich</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Den hel bestormde <rhyme label="b" type="f" corresp="#s1:l2">crachtich</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Met groot <rhyme label="c" type="m" corresp="#s1:l4">gewelt</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Als een dapper <rhyme label="c" type="m" corresp="#s1:l4">helt</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Neder <rhyme label="c" type="m" corresp="#s1:l4">gevelt</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Haer poorten hier op <rhyme label="d" type="m">acht</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Die eewich waren <rhyme label="e" type="f">gesloten</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Als Sampson <rhyme label="e" type="f" corresp="#s1:l11">onverdroten</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Door Godes <rhyme label="d" type="m" corresp="#s1:l10">cracht</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Had op den Berch <rhyme label="d" type="m" corresp="#s1:l10">gebracht</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Voor zijn aenschijn <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die hemelsche <rhyme label="b" type="f">consoorten</rhyme></l>
        <l id="s2:l3" met="2" type="-+">D’engelsche <rhyme label="a" type="m" corresp="#s2:l1">schaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Spraecken met woorden <rhyme label="c" type="m">heus</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ghy prinsen <rhyme label="b" type="f" corresp="#s2:l2">persoorten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Doet op v helsche <rhyme label="b" type="f" corresp="#s2:l2">poorten</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Dat den Koninck <rhyme label="c" type="m" corresp="#s2:l4">glorieus</rhyme></l>
        <l id="s2:l8" met="2" type="-+">In gae <rhyme label="c" type="m" corresp="#s2:l4">victorieus</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Wie is <rhyme label="c" type="m" corresp="#s2:l4">pompeus</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Dees Coninck <rhyme label="d" type="m">glorieus</rhyme>:</l>
        <l id="s2:l11" met="3" type="+-">Den Heer sterck en <rhyme label="e" type="f">machtich</rhyme></l>
        <l id="s2:l12" met="3" type="-+">En om strijden <rhyme label="e" type="f" corresp="#s2:l11">crachtich</rhyme>,</l>
        <l id="s2:l13" met="2" type="-+">Seer <rhyme label="d" type="m" corresp="#s2:l10">corageus</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Lieflijck en <rhyme label="d" type="m" corresp="#s2:l10">gratieus</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Der Heyligher <rhyme label="a" type="m">schaer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Zeer zwaerlijcken lach <rhyme label="b" type="f">ghevanghen</rhyme></l>
        <l id="s3:l3" met="2" type="-+">In s’doots <rhyme label="a" type="m" corresp="#s3:l1">vaer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sy maeckten groot <rhyme label="c" type="m">gheclach</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Met groot <rhyme label="b" type="f" corresp="#s3:l2">verlanghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die laghen in <rhyme label="b" type="f" corresp="#s3:l2">verstranghen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Sy riepen te <rhyme label="c" type="m" corresp="#s3:l4">gaer</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Och willecoom zijt ghy <rhyme label="c" type="m" corresp="#s3:l4">daer</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Ons <rhyme label="c" type="m" corresp="#s3:l4">middelaer</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Daer wy hebben na <rhyme label="d" type="m">ghewacht</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Dat ghy sout <rhyme label="e" type="f">remedieeren</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Ons arme <rhyme label="e" type="f" corresp="#s3:l11">prissonieren</rhyme></l>
        <l id="s3:l13" met="2" type="-+">In deser <rhyme label="d" type="m" corresp="#s3:l10">nacht</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Al wter hellen <rhyme label="d" type="m" corresp="#s3:l10">gracht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al ons <rhyme label="a" type="m">geroep</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ons kermen ende <rhyme label="b" type="f">klaghen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Tot u ons <rhyme label="a" type="m" corresp="#s4:l1">hoop</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In desen allendighen <rhyme label="c" type="m">staet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die ghecluystert <rhyme label="b" type="f" corresp="#s4:l2">laghen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Inder hellen seer <rhyme label="b" type="f" corresp="#s4:l2">verslaghen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Heel <rhyme label="c" type="m" corresp="#s4:l4">desolaet</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Ons troost ons <rhyme label="c" type="m" corresp="#s4:l4">toeverlaet</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Ghy metter <rhyme label="c" type="m" corresp="#s4:l4">daet</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Nu hier ghecomen <rhyme label="d" type="m">zijt</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Om ons te <rhyme label="e" type="f">consoleren</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Int clachtich <rhyme label="e" type="f" corresp="#s4:l11">tormenteeren</rhyme></l>
        <l id="s4:l13" met="2" type="-+">En s’doots <rhyme label="d" type="m" corresp="#s4:l10">strijt</rhyme></l>
        <l id="s4:l14" met="3" type="-+">O heer <rhyme label="d" type="m" corresp="#s4:l10">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy s’hemels Princen <rhyme label="a" type="m">heus</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ontsluyt u hooghe <rhyme label="b" type="f">poorten</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Der Enghelen <rhyme label="a" type="m" corresp="#s5:l1">veus</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Luyden zeer <rhyme label="c" type="m">gratieus</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ghy eewige <rhyme label="b" type="f" corresp="#s5:l2">poorten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Wort verheven met <rhyme label="b" type="f" corresp="#s5:l2">consoorten</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Dat den Coninck <rhyme label="c" type="m" corresp="#s5:l4">glorieus</rhyme></l>
        <l id="s5:l8" met="2" type="-+">In gae <rhyme label="c" type="m" corresp="#s5:l4">victorieus</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Wie is <rhyme label="c" type="m" corresp="#s5:l4">glorieus</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Dees Coninck <rhyme label="d" type="m">victorieus</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Den Heer der <rhyme label="e" type="f">heyrcrachten</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Een Coninck van grooter <rhyme label="e" type="f" corresp="#s5:l11">machten</rhyme></l>
        <l id="s5:l13" met="2" type="-+">Seer <rhyme label="d" type="m" corresp="#s5:l10">coragieus</rhyme></l>
        <l id="s5:l14" met="3" type="-+">Lieflijck en <rhyme label="d" type="m" corresp="#s5:l10">amoreus</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
