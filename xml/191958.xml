<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191958</id>
    <date>
      <year-start>1753</year-start>
      <year-end>1753</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Goedenavond malse Neeltje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn vrindelyke <rhyme label="a" type="f">Buurtje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe dus in <rhyme label="b" type="m">ienigheit</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Zeg zoete <rhyme label="b" type="m" corresp="#s1:l2">meit</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Mag ik wel in jou <rhyme label="a" type="f" corresp="#s1:l1">schuurtje</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Eens treeden, met <rhyme label="b" type="m" corresp="#s1:l2">bescheit</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">'k Zal maer ien <rhyme label="a" type="f" corresp="#s1:l1">uurtje</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Wat praeten lieve <rhyme label="c" type="m">Syt</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">'k Ga dan stal,, 't vee pleegen <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Op Melkens <rhyme label="c" type="m" corresp="#s1:l7">tyd</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wel Jochem 'k zal 't <rhyme label="a" type="f">verdraegen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Praet maer zoo als 't <rhyme label="b" type="m">behoort</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Of aers, ga <rhyme label="b" type="m" corresp="#s2:l2">voort</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Maer mag ik jou wel <rhyme label="a" type="f" corresp="#s2:l1">vraegen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Waer hebje <rhyme label="b" type="m" corresp="#s2:l2">regtevoort</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Eweest, dat jou de <rhyme label="a" type="f" corresp="#s2:l1">vlaegen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Van 't praeten <rhyme label="c" type="m">overkomt</rhyme>?</l>
        <l id="s2:l8" met="4" type="-+">'k Loof in Stadt,,, daer jy uit 't <rhyme label="d" type="m">Vat</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Dronkt tot het <rhyme label="c" type="m" corresp="#s2:l7">bomd'</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ja Sytje dat 's de <rhyme label="a" type="f">waerheit</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'k Was daer om fransse <rhyme label="b" type="m">Stroop</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">'k Stont op de <rhyme label="b" type="m" corresp="#s3:l2">Stoop</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daar hoorden ik met <rhyme label="a" type="f" corresp="#s3:l1">raerheit</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van verre ien <rhyme label="b" type="m" corresp="#s3:l2">geroop</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van Wyven, 'k zegt u, maer <rhyme label="a" type="f" corresp="#s3:l1">meit</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">'t Was ierst ien <rhyme label="c" type="m">mosselpoop</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Die riep groot,,, wit als de <rhyme label="d" type="m">doot</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Koop Vollek <rhyme label="c" type="m" corresp="#s3:l7">koop</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Toen kwamper ien mank <rhyme label="a" type="f">Ventje</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die riep, hiet <rhyme label="b" type="m">garrenael</rhyme>!</l>
        <l id="s4:l3" met="2" type="-+">Met raere <rhyme label="b" type="m" corresp="#s4:l2">tael</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Heet! heet! wie koopt myn <rhyme label="a" type="f" corresp="#s4:l1">entje</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">Gins komt 'er ien met <rhyme label="b" type="m" corresp="#s4:l2">Ael</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En Paeling, koop wet <rhyme label="a" type="f" corresp="#s4:l1">Wentje</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">'k Heb nou een schoone <rhyme label="c" type="m">zoo</rhyme>.</l>
        <l id="s4:l8" met="4" type="-+">Stoof of braen,,, kom wiljer <rhyme label="d" type="m">aen</rhyme>?</l>
        <l id="s4:l9" met="2" type="-+">'k Heb geld van <rhyme label="c" type="m" corresp="#s4:l7">noo</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wie wil Rygsnoeren <rhyme label="a" type="f">koopen</rhyme>?</l>
        <l id="s5:l2" met="3" type="-+">Ze zyn drie ellen <rhyme label="b" type="m">lang</rhyme>.</l>
        <l id="s5:l3" met="2" type="-+">En ook, hiel <rhyme label="b" type="m" corresp="#s5:l2">strang</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Gins komt 'er ien <rhyme label="a" type="f" corresp="#s5:l1">geloopen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die roept met heesche <rhyme label="b" type="m" corresp="#s5:l2">zang</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Schuurzant! schuurzant! met <rhyme label="a" type="f" corresp="#s5:l1">hoopen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Een aer roept <rhyme label="c" type="m">Alykruik</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Vers en, wit,,, En vol van <rhyme label="d" type="m">pit</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Met witte <rhyme label="c" type="m" corresp="#s5:l7">buik'</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Fran√ßois met Mars en <rhyme label="a" type="f">Brillen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En nieuwen <rhyme label="b" type="m">Almanak</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Roept even <rhyme label="b" type="m" corresp="#s6:l2">strak</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Zeg wie se koopen <rhyme label="a" type="f" corresp="#s6:l1">willen</rhyme>?</l>
        <l id="s6:l5" met="3" type="-+">Het is een groot <rhyme label="b" type="m" corresp="#s6:l2">gemak</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Met vremde <rhyme label="a" type="f" corresp="#s6:l1">grillen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Roept Schoorsteen Veger, <rhyme label="c" type="m">Hoo</rhyme>!</l>
        <l id="s6:l8" met="4" type="-+">Een aer weer,,, 'k slyp mes en <rhyme label="d" type="m">scheer</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Wie haver <rhyme label="c" type="m" corresp="#s6:l7">stroo</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Drie stuivers ien El <rhyme label="a" type="f">laeken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Voor dweilen is 't heel <rhyme label="b" type="m">goet</rhyme>.</l>
        <l id="s7:l3" met="2" type="-+">Wie warme <rhyme label="b" type="m" corresp="#s7:l2">kroet'</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Wie wilder neuten <rhyme label="a" type="f" corresp="#s7:l1">kraeken</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Met pit in <rhyme label="b" type="m" corresp="#s7:l2">overvloed</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Pastei, se <rhyme label="a" type="f" corresp="#s7:l1">smaeken</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Zoo lekker als <rhyme label="c" type="m">Confyt</rhyme>.</l>
        <l id="s7:l8" met="4" type="-+">Boek weit broot,,, en Broeders <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Roept elk om <rhyme label="c" type="m" corresp="#s7:l7">stryt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Een wyf roept met een <rhyme label="a" type="f">waegen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Wie pootris <rhyme label="b" type="m">Applesyn</rhyme>.</l>
        <l id="s8:l3" met="2" type="-+">Wie wil roept <rhyme label="b" type="m" corresp="#s8:l2">Tryn</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Een mooi net kantje <rhyme label="a" type="f" corresp="#s8:l1">draegen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Goet koop, en sterk en <rhyme label="b" type="m" corresp="#s8:l2">fyn</rhyme>!</l>
        <l id="s8:l6" met="3" type="-+">Wie het <rhyme label="a" type="f" corresp="#s8:l1">behaegen</rhyme></l>
        <l id="s8:l7" met="3" type="-+">In hylikmaeker <rhyme label="c" type="m">roept</rhyme></l>
        <l id="s8:l8" met="4" type="-+">Raere Koos,,, myn <rhyme label="d" type="m">suikerdoos</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Ey, komt eens <rhyme label="c" type="m" corresp="#s8:l7">snoept</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Lamoenen! laetje <rhyme label="a" type="f">zoenen</rhyme>!</l>
        <l id="s9:l2" met="3" type="-+">Wie Rot, of <rhyme label="b" type="m">Muizeval</rhyme>!</l>
        <l id="s9:l3" met="2" type="-+">Wie koopt een <rhyme label="b" type="m" corresp="#s9:l2">Bal</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Om vlakken uit te <rhyme label="a" type="f" corresp="#s9:l1">boenen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">'t Smet 't Laeken <rhyme label="b" type="m" corresp="#s9:l2">niemendal</rhyme>.</l>
        <l id="s9:l6" met="3" type="-+">Wie koopt out Nieumaek <rhyme label="a" type="f" corresp="#s9:l1">schoenen</rhyme>!</l>
        <l id="s9:l7" met="3" type="-+">Wie nou ien <rhyme label="c" type="m">Uilebort</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Dobbelstien,,, krygt gy met <rhyme label="d" type="m">ien</rhyme>!</l>
        <l id="s9:l9" met="2" type="-+">Roept elk al <rhyme label="c" type="m" corresp="#s9:l7">vort</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wie koopt myn leste <rhyme label="a" type="f">zootje</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Bak Schollen, of ien <rhyme label="b" type="m">snees</rhyme>?</l>
        <l id="s10:l3" met="2" type="-+">Wie wil roept <rhyme label="b" type="m" corresp="#s10:l2">Kees</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Twie Bokkems om ien <rhyme label="a" type="f" corresp="#s10:l1">grootje</rhyme>!</l>
        <l id="s10:l5" met="3" type="-+">Wie Sprot? ik roep my <rhyme label="b" type="m" corresp="#s10:l2">hees</rhyme>.</l>
        <l id="s10:l6" met="3" type="-+">By stuk, of wel by 't <rhyme label="a" type="f" corresp="#s10:l1">strootje</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Wie App'len hiet, of <rhyme label="c" type="m">raeuw</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">Knollen zoet,,, Aerd-appels <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s10:l9" met="2" type="-+">En mals van <rhyme label="c" type="m" corresp="#s10:l7">kaeuw</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Kees Teunens Aey, roept <rhyme label="a" type="f">Maertje</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En Neeltje moet je <rhyme label="b" type="m">Melk</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Straks liep 'er <rhyme label="b" type="m" corresp="#s11:l2">elk</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Rondom den Boer zyn <rhyme label="a" type="f" corresp="#s11:l1">Kaertje</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Met Pot en Pan en <rhyme label="b" type="m" corresp="#s11:l2">Kelk</rhyme>.</l>
        <l id="s11:l6" met="3" type="-+">'t Groenwyf roept, <rhyme label="a" type="f" corresp="#s11:l1">Saertje</rhyme></l>
        <l id="s11:l7" met="3" type="-+">'k Heb schoone vette <rhyme label="c" type="m">kous</rhyme>!</l>
        <l id="s11:l8" met="4" type="-+">Hebje iets van doen? 'k ruil ouwe <rhyme label="d" type="m">hoen</rhyme></l>
        <l id="s11:l9" met="2" type="-+">En kleeren roept een <rhyme label="c" type="m" corresp="#s11:l7">Smous</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Vier in 't oortje ien paer <rhyme label="a" type="f">klompen</rhyme>!</l>
        <l id="s12:l2" met="3" type="-+">Wie koopt 'er nou ien <rhyme label="b" type="m">Paer</rhyme>!</l>
        <l id="s12:l3" met="2" type="-+">Wie (roept ien <rhyme label="b" type="m" corresp="#s12:l2">aer</rhyme>.)</l>
        <l id="s12:l4" met="3" type="-+">Koopt nou myn leste <rhyme label="a" type="f" corresp="#s12:l1">lompen</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Of zoute Vis, ze is <rhyme label="b" type="m" corresp="#s12:l2">klaer</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Geweekt, en ook <rhyme label="a" type="f" corresp="#s12:l1">gekrompen</rhyme>.</l>
        <l id="s12:l7" met="3" type="-+">Wie mostert, mostert <rhyme label="c" type="m">goet</rhyme>!</l>
        <l id="s12:l8" met="4" type="-+">Wie een stik,,, hooy Butter <rhyme label="d" type="m">dik</rhyme>!</l>
        <l id="s12:l9" met="2" type="-+">Roept Kees <rhyme label="c" type="m" corresp="#s12:l7">Hooghoet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Hier heb ik voor drie <rhyme label="a" type="f">duiten</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Twie blaen met Liedjes. <rhyme label="b" type="m">die</rhyme></l>
        <l id="s13:l3" met="2" type="-+">(Als yder <rhyme label="b" type="m" corresp="#s13:l2">zie</rhyme>.)</l>
        <l id="s13:l4" met="3" type="-+">Zyn nieuw, en die wel <rhyme label="a" type="f" corresp="#s13:l1">sluiten</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Op scheele Piet en <rhyme label="b" type="m" corresp="#s13:l2">Mie</rhyme>;</l>
        <l id="s13:l6" met="3" type="-+">'k Heb ook voor <rhyme label="a" type="f" corresp="#s13:l1">Guiten</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Die vryen roept het <rhyme label="c" type="m">Wyf</rhyme>,</l>
        <l id="s13:l8" met="4" type="-+">Wat de Meid,,, al dient <rhyme label="d" type="m">gezeit</rhyme>,</l>
        <l id="s13:l9" met="2" type="-+">Als hy vryd <rhyme label="c" type="m" corresp="#s13:l7">styf</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wel Jochem is dat <rhyme label="a" type="f">raesen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Wat scheelt my al die <rhyme label="b" type="m">praet</rhyme>?</l>
        <l id="s14:l3" met="2" type="-+">Loop 't is al <rhyme label="b" type="m" corresp="#s14:l2">laet</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Gaet heen uw Koeijen <rhyme label="a" type="f" corresp="#s14:l1">aezen</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Eer dat jou Vaer word <rhyme label="b" type="m" corresp="#s14:l2">kwaet</rhyme>;</l>
        <l id="s14:l6" met="3" type="-+">Maer jonge <rhyme label="a" type="f" corresp="#s14:l1">Baezen</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Vereeren altyd <rhyme label="c" type="m">wat</rhyme>,</l>
        <l id="s14:l8" met="4" type="-+">Wat heb jy,,, gekogt voor <rhyme label="d" type="m">my</rhyme>,</l>
        <l id="s14:l9" met="2" type="-+">Nou in de <rhyme label="c" type="m" corresp="#s14:l7">Stadt</rhyme>?</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Voor jou myn lieve <rhyme label="a" type="f">Sytje</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Wat Hylikmaker, <rhyme label="b" type="m">daer</rhyme>!</l>
        <l id="s15:l3" met="2" type="-+">Nou neemt het <rhyme label="b" type="m" corresp="#s15:l2">maer</rhyme>.</l>
        <l id="s15:l4" met="3" type="-+">Voor my myn lieve <rhyme label="a" type="f" corresp="#s15:l1">prytje</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Een Boekje, dat is <rhyme label="b" type="m" corresp="#s15:l2">raer</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Van 't vryen, want hy <rhyme label="a" type="f" corresp="#s15:l1">vrydje</rhyme></l>
        <l id="s15:l7" met="3" type="-+">Die jou de Koek nou <rhyme label="c" type="m">geeft</rhyme>:</l>
        <l id="s15:l8" met="4" type="-+">Neemt het aen,,, want ik moet <rhyme label="d" type="m">gaen</rhyme></l>
        <l id="s15:l9" met="2" type="-+">ZOO! 'T ZOENTJE <rhyme label="c" type="m" corresp="#s15:l7">KLEEFT</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
