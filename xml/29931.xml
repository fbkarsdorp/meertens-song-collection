  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Droeve Maagd droog af uwe <rhyme label="a" type="f">oogen</rhyme>:</l>
        <l id="s1:l2" met="4" type="+-">Vreede en Welvaart komt <rhyme label="b" type="m">weerom</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">'s Hemels gunst met u <rhyme label="a" type="f" corresp="#s1:l1">bewogen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dooft Trompet, en <rhyme label="b" type="m" corresp="#s1:l2">Oorlogs-trom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">De Overvloed, nu weg <rhyme label="a" type="f">geweken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Zal eerlang met grooter <rhyme label="b" type="m">pracht</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'t Hooft in uwe staat <rhyme label="a" type="f" corresp="#s2:l1">opsteken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En zich geven in uvv <rhyme label="b" type="m" corresp="#s2:l2">macht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doch laat nimmer haar <rhyme label="a" type="f">bestieren</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Door de Weelde, en <rhyme label="b" type="m">Zorgeloosheid</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Maat, en tucht moet teugel <rhyme label="a" type="f" corresp="#s3:l1">vieren</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Aan de schat uw <rhyme label="b" type="m" corresp="#s3:l2">toegeleit</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wilt dan voor ons beide <rhyme label="a" type="f">waken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Gy zult u gehulpen <rhyme label="b" type="m">zien</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wy u meer gelukkig <rhyme label="a" type="f" corresp="#s4:l1">maken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Gy met rust uw staat <rhyme label="b" type="m" corresp="#s4:l2">gebien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
