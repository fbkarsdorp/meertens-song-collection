<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156813</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O God gy hebt ons noyt <rhyme label="a" type="f">besweken</rhyme></l>
        <l id="s1:l2" met="4" type="-+">en ic hope dat gy ooc niet en <rhyme label="b" type="m">sult</rhyme></l>
        <l id="s1:l3" met="4" type="-+">al sijn wy met veel quaet <rhyme label="b" type="m" corresp="#s1:l2">vervult</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">ghy kondt genesen al ons <rhyme label="a" type="f" corresp="#s1:l1">gebreken</rhyme></l>
        <l id="s1:l5" met="4" type="-+">en vergeeft deur genade onse <rhyme label="b" type="m" corresp="#s1:l2">schult</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Waer vintmen sulck eenen Godt van <rhyme label="a" type="f">weerde</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die 't daer al regeerdt naer zijn <rhyme label="b" type="m">bemerc</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Hy is alleene wijs en <rhyme label="b" type="m" corresp="#s2:l2">sterck</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">sijns gelijck en is niet op <rhyme label="a" type="f" corresp="#s2:l1">Eerde</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">die daer so besorcht zijn handen <rhyme label="b" type="m" corresp="#s2:l2">were</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daeromme heeft hy ons <rhyme label="a" type="f">gheschapen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dat wy hem souden sijn <rhyme label="b" type="m">onderdaen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">en hoorende zijns woorts <rhyme label="b" type="m" corresp="#s3:l2">vermaen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Op dat wy nimmermeer en <rhyme label="a" type="f" corresp="#s3:l1">slapen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Inde doot en alsoo niet <rhyme label="b" type="m" corresp="#s3:l2">vergaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer dat wy ons doch souden <rhyme label="a" type="f">wachten</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Van de sonde die daer wondt en <rhyme label="b" type="m">schendt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">O Heere ons kracht van boven <rhyme label="b" type="m" corresp="#s4:l2">sent</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Want wy hebben geen <rhyme label="a" type="f" corresp="#s4:l1">ghedachten</rhyme></l>
        <l id="s4:l5" met="4" type="-+">zy en sijn u altsamen <rhyme label="b" type="m" corresp="#s4:l2">bekent</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Heer ghy weet ons doen en <rhyme label="a" type="f">pooghen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">voor u en is verborghen <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">ghy wetet al naer schrifs <rhyme label="b" type="m" corresp="#s5:l2">bediet</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Gelijck vyer vlammen sijn u <rhyme label="a" type="f" corresp="#s5:l1">oogen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">dat ghy in alle duyster hoecken <rhyme label="b" type="m" corresp="#s5:l2">siet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy hebt o Heere Godt <rhyme label="a" type="f">eersame</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Mijne Nieren in u <rhyme label="b" type="m">gewelt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Mijn gebeenten hebt ghy <rhyme label="b" type="m" corresp="#s6:l2">getelt</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Doen ick noch was in dat <rhyme label="a" type="f" corresp="#s6:l1">Lichame</rhyme></l>
        <l id="s6:l5" met="4" type="-+">van myne Moeder <rhyme label="b" type="m" corresp="#s6:l2">ghestelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Spreec ick: duysterheyt mach my <rhyme label="a" type="f">bedecken</rhyme></l>
        <l id="s7:l2" met="4" type="-+">soo isser doch light rontomme <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Voor u en ben ic nerghens <rhyme label="b" type="m" corresp="#s7:l2">vry</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Ghy kondt aenschouwen al mijn <rhyme label="a" type="f" corresp="#s7:l1">vlecken</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Want ghy siedet al veer ende <rhyme label="b" type="m" corresp="#s7:l2">by</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Men kan voor u Heere niet <rhyme label="a" type="f">versteken</rhyme></l>
        <l id="s8:l2" met="4" type="-+">daerom datmen u wel vreesen <rhyme label="b" type="m">magh</rhyme></l>
        <l id="s8:l3" met="4" type="-+">ghy wetet al naer schrifs <rhyme label="b" type="m" corresp="#s8:l2">gewagh</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Soo wat wy dencken ende <rhyme label="a" type="f" corresp="#s8:l1">spreken</rhyme></l>
        <l id="s8:l5" met="4" type="-+">den nacht is voor u gelijc den <rhyme label="b" type="m" corresp="#s8:l2">dagh</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Waer sal ic my voor u <rhyme label="a" type="f">verbergen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">oft henen vlieden voor u <rhyme label="b" type="m">aenschijn</rhyme>?</l>
        <l id="s9:l3" met="4" type="-+">Ghy zijt my altijt rontomme <rhyme label="b" type="m" corresp="#s9:l2">fijn</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Wee alle die den Heere <rhyme label="a" type="f" corresp="#s9:l1">tergen</rhyme></l>
        <l id="s9:l5" met="4" type="-+">en voor God willen verborgen <rhyme label="b" type="m" corresp="#s9:l2">sijn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Al hadde ic vlogelen dat ic <rhyme label="a" type="f">vloge</rhyme></l>
        <l id="s10:l2" met="4" type="-+">gelijc als den morgenstont seer <rhyme label="b" type="m">klaer</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Tot aen d' uytter Zee <rhyme label="b" type="m" corresp="#s10:l2">voorwaer</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">so sal my u hant groot van <rhyme label="a" type="f" corresp="#s10:l1">vermogen</rhyme></l>
        <l id="s10:l5" met="4" type="-+">selve voeren en behouden <rhyme label="b" type="m" corresp="#s10:l2">daer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Al waerdt dat ick inden Hemel <rhyme label="a" type="f">klomme</rhyme></l>
        <l id="s11:l2" met="4" type="-+">soo sout ghy my Heere vinden <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Al maect ic mijn Bedde inde <rhyme label="b" type="m" corresp="#s11:l2">Hel</rhyme></l>
        <l id="s11:l4" met="4" type="-+">so sout ghy daer o Heere wel <rhyme label="a" type="f" corresp="#s11:l1">kommen</rhyme></l>
        <l id="s11:l5" met="4" type="-+">En my weder uyt halen <rhyme label="b" type="m" corresp="#s11:l2">snel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Al sprake ick  bedect my  ghy <rhyme label="a" type="f">afgronden</rhyme></l>
        <l id="s12:l2" met="4" type="-+">en dat ic in haer waer <rhyme label="b" type="m">geraeckt</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">So waer ic voor u bloot en <rhyme label="b" type="m" corresp="#s12:l2">naect</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">En lichtelijc van u <rhyme label="a" type="f" corresp="#s12:l1">ghevonden</rhyme></l>
        <l id="s12:l5" met="4" type="-+">want u hant hevet doch al <rhyme label="b" type="m" corresp="#s12:l2">gemaect</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Die dagen sijn al Heere <rhyme label="a" type="f">geschreven</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Die sonde worden in u <rhyme label="b" type="m">boeck</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">waer vintmen sulken God soo <rhyme label="b" type="m" corresp="#s13:l2">kloec</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Alleene u moetmen de eere <rhyme label="a" type="f" corresp="#s13:l1">geven</rhyme></l>
        <l id="s13:l5" met="4" type="-+">want geen uws gelijc in <rhyme label="b" type="m" corresp="#s13:l2">ondersoec</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Hoe en soudemen u niet <rhyme label="a" type="f">prijsen</rhyme></l>
        <l id="s14:l2" met="4" type="-+">O Princelijcke God diet al <rhyme label="b" type="m">geneest</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Ghy sult ooc noch alsoo men <rhyme label="b" type="m" corresp="#s14:l2">leest</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Alle menschen doen <rhyme label="a" type="f" corresp="#s14:l1">verrijsen</rhyme></l>
        <l id="s14:l5" met="4" type="-+">ooc die vant begin hebben <rhyme label="b" type="m" corresp="#s14:l2">geweest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
