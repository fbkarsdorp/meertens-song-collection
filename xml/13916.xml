    <melody>Ik ben tot Amsterdam geweest hu hu</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">SEgh Harderinnen ach! waer heen, Hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">En laet u Cloris hier alleen, Hu, <rhyme label="a" type="m" corresp="#s1:l1">hu</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ick bid’ u, houdt een weynigh <rhyme label="b" type="m">stee</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zoo zingh ick van fa, la, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s1:l5" met="6" type="+-">Ach! Filidaetje ick wil mee, Fa, la, <rhyme label="c" type="m" corresp="#s1:l4">la</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Wel Cloris zijt ghy daer, ey kom, Hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Mijn Moeder wachtme, ‘k moet weer om Hu, <rhyme label="a" type="m" corresp="#s2:l1">hu</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Nae huys toe gaen, het wordt al <rhyme label="b" type="m">laet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Zoo zingh ick van fa, la, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s2:l5" met="6" type="+-">Ay siet waer dat de Zon al staet, Fa, la, <rhyme label="c" type="m" corresp="#s2:l4">la</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Ey Harderinne eerst een Kus, Hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Hoe staet mijn Filidaetje dus, Hu, <rhyme label="a" type="m" corresp="#s3:l1">hu</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hoe! is de oude Min uyt ‘t <rhyme label="b" type="m">Hart</rhyme>?</l>
        <l id="s3:l4" met="4" type="+-">Zoo zingh ick van fa, la, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s3:l5" met="6" type="+-">Komt Enghel, en gheneest mijn smart, Fa, la, <rhyme label="c" type="m" corresp="#s3:l4">la</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ben ick de oorsaeck van u pijn, Hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Zoo wil ick die niet langher zijn, Hu, <rhyme label="a" type="m" corresp="#s4:l1">hu</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Zoo komt dan Cloris, neemt een <rhyme label="b" type="m">soen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Soo zingh ick van fa, la, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s4:l5" met="6" type="+-">Dat’s twee voor een, ick wil ‘t niet doen, Fa, la, <rhyme label="c" type="m" corresp="#s4:l4">la</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Hoe soo mijn overschoone Meyt, Hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Ick zwem in alle dartelheydt, Hu, <rhyme label="a" type="m" corresp="#s5:l1">hu</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Nu ick u lipjes heb <rhyme label="b" type="m">ghekust</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Zoo zingh ick van fa, la, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s5:l5" met="6" type="+-">Nu is de brandt wat uytgheblust, Fa, la, <rhyme label="c" type="m" corresp="#s5:l4">la</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Komt Cloris, brenght my op het padt, Hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">En drijft mijn Schaepjes nae de stadt, Hu, <rhyme label="a" type="m" corresp="#s6:l1">hu</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Kust dan wat dat ghy kussen <rhyme label="b" type="m">meught</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Zoo zingh ick van fa, la, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s6:l5" met="6" type="+-">Alst maer geschiedt in eer en deught, Fa, la, <rhyme label="c" type="m" corresp="#s6:l4">la</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Wel Harderin, alst u gelieft, Hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Ick liet noyt Dochter ongherieft, Hu, <rhyme label="a" type="m" corresp="#s7:l1">hu</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Doch als de Kusjes blijven <rhyme label="b" type="m">vast</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Zoo zingh ick van fa, la, <rhyme label="c" type="m">la</rhyme>,</l>
        <l id="s7:l5" met="6" type="+-">Daer sal van my zijn op ghepast, Fa, la, <rhyme label="c" type="m" corresp="#s7:l4">la</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
