    <melody>Ik had een lief verkoren zij is mij worden schuw</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ic hadde een lief <rhyme label="a" type="f">vercoren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Soe es mi worden <rhyme label="b" type="m">scu</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Daer toe hebbict <rhyme label="a" type="f" corresp="#s1:l1">verloren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">In weet waer zouken <rhyme label="b" type="m" corresp="#s1:l2">nu</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Soe gheift mi enen wreeden <rhyme label="b" type="m" corresp="#s1:l2">vu</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In hebbe haer niet <rhyme label="c" type="m">mesdaen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Wel op wel <rhyme label="c" type="m" corresp="#s1:l6">aen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Wat saelt <rhyme label="d" type="m">ghesneift</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Die sijn lief verloren <rhyme label="d" type="m" corresp="#s1:l8">heift</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Wi willent zouken <rhyme label="c" type="m" corresp="#s1:l6">gaen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat baet ons daer om <rhyme label="a" type="f">trueren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat ons niet werden <rhyme label="b" type="m">mach</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat mi niet mach <rhyme label="a" type="f" corresp="#s2:l1">gheburen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Des hebbic goet <rhyme label="b" type="m" corresp="#s2:l2">verdrach</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Ic hebbe ghetruert so menighen <rhyme label="b" type="m" corresp="#s2:l2">dach</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ic wilt voort <rhyme label="c" type="m">avelaen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Wel op wel <rhyme label="c" type="m" corresp="#s2:l6">aen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Wat saelt <rhyme label="d" type="m">ghesneift</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Die sijn lief verloren <rhyme label="d" type="m" corresp="#s2:l8">heift</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Wi willent zouken <rhyme label="c" type="m" corresp="#s2:l6">gaen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wie hem daer toe wil <rhyme label="a" type="f">gheven</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Daer mens niet vele en <rhyme label="b" type="m">acht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hi moet met zorghen <rhyme label="a" type="f" corresp="#s3:l1">leven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hem en baet const no <rhyme label="b" type="m" corresp="#s3:l2">cracht</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Maer brinct hi ghelt men es <rhyme label="b" type="m" corresp="#s3:l2">bedacht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ten can hem niet <rhyme label="c" type="m">ontstaen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Wel op wel <rhyme label="c" type="m" corresp="#s3:l6">aen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Wat saelt <rhyme label="d" type="m">ghesneift</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Die sijn lief verloren <rhyme label="d" type="m" corresp="#s3:l8">heift</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Wi willent zouken <rhyme label="c" type="m" corresp="#s3:l6">gaen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dat ghelt dat es die <rhyme label="a" type="f">bloume</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die elkerlijc <rhyme label="b" type="m">begheert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dies ic mi niet <rhyme label="a" type="f" corresp="#s4:l1">beroume</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Mijn ghelt es al <rhyme label="b" type="m" corresp="#s4:l2">verteert</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Wie niet ne liecht bedriecht ende <rhyme label="b" type="m" corresp="#s4:l2">zweert</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Men acht sijns niet een <rhyme label="c" type="m">spaen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Wel op wel <rhyme label="c" type="m" corresp="#s4:l6">aen</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Wat saelt <rhyme label="d" type="m">ghesneift</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Die sijn lief verloren <rhyme label="d" type="m" corresp="#s4:l8">heift</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Wi willent zouken <rhyme label="c" type="m" corresp="#s4:l6">gaen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu wil ics al <rhyme label="a" type="f">ontberen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Want ics ontberen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">In can mi niet <rhyme label="a" type="f" corresp="#s5:l1">gheneeren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In hadde ghelt of <rhyme label="b" type="m" corresp="#s5:l2">goet</rhyme></l>
        <l id="s5:l5" met="4" type="-+">So wat ic doe dats jeghen <rhyme label="b" type="m" corresp="#s5:l2">spoet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Men wil mi niet <rhyme label="c" type="m">ontfaen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Wel op wel <rhyme label="c" type="m" corresp="#s5:l6">aen</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Wat saelt <rhyme label="d" type="m">ghesneift</rhyme></l>
        <l id="s5:l9" met="4" type="+-">Die sijn lief verloren <rhyme label="d" type="m" corresp="#s5:l8">heift</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Wi willent zouken <rhyme label="c" type="m" corresp="#s5:l6">gaen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nu wilwi vroilic <rhyme label="a" type="f">zinghen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Scamel sonder <rhyme label="b" type="m">ghelt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Cuenwijt also <rhyme label="a" type="f" corresp="#s6:l1">duerbringhen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">So hebben wijt wel <rhyme label="b" type="m" corresp="#s6:l2">bestelt</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Here God die allen commer <rhyme label="b" type="m" corresp="#s6:l2">velt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Wilt ons van zorghen <rhyme label="c" type="m">dwaen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Wel op wel <rhyme label="c" type="m" corresp="#s6:l6">aen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Wat saelt <rhyme label="d" type="m">ghesneift</rhyme></l>
        <l id="s6:l9" met="4" type="+-">Die sijn lief verloren <rhyme label="d" type="m" corresp="#s6:l8">heift</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Wi willent zouken <rhyme label="c" type="m" corresp="#s6:l6">gaen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
