  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat Nieus wil ik u <rhyme label="a" type="f">vertellen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Al met een blyde <rhyme label="b" type="m">aenschyn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Hoe dat de dochters haer <rhyme label="a" type="f" corresp="#s1:l1">stellen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">en soeken de Jonckmans <rhyme label="b" type="m" corresp="#s1:l2">fyn</rhyme></l>
        <l id="s1:l5" met="4" type="-+">dat sy in de verraderen  <rhyme label="b" type="m" corresp="#s1:l2">syn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Men siese drollich <rhyme label="a" type="f">loncken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">als sy ons gaen <rhyme label="b" type="m">verby</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Als brudekens dat sy <rhyme label="a" type="f" corresp="#s2:l1">proncken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">den voorschoot is van <rhyme label="b" type="m" corresp="#s2:l2">sey</rhyme></l>
        <l id="s2:l5" met="4" type="-+">dat is te seggen Jonckmans <rhyme label="b" type="m" corresp="#s2:l2">altenij</rhyme> .</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick moetter wel op <rhyme label="a" type="f">letten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">op de dochters <rhyme label="b" type="m">idoon</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hoe schoon sy gaer <rhyme label="a" type="f" corresp="#s3:l1">bancketten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">versieren haeren <rhyme label="b" type="m" corresp="#s3:l2">persoon</rhyme></l>
        <l id="s3:l5" met="4" type="-+">sy gaen Als Godinnen <rhyme label="b" type="m" corresp="#s3:l2">schoon</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Haer muijtskens wel te <rhyme label="a" type="f">keure</rhyme></l>
        <l id="s4:l2" met="3" type="-+">die sij draghen <rhyme label="b" type="m">hier</rhyme></l>
        <l id="s4:l3" met="3" type="-+">daer loopt wat binnewerck <rhyme label="a" type="f" corresp="#s4:l1">deure</rhyme></l>
        <l id="s4:l4" met="3" type="-+">dat sy coopen sij also <rhyme label="b" type="m" corresp="#s4:l2">dier</rhyme></l>
        <l id="s4:l5" met="4" type="-+">de houders sinctent  veel beter int <rhyme label="b" type="m" corresp="#s4:l2">vier</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sy weten loose treken <rhyme label="a" type="f">dese</rhyme></l>
        <l id="s5:l2" met="3" type="-+">dese meyskens <rhyme label="b" type="m">bley</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Aen thoofdeken draghen sy <rhyme label="a" type="f" corresp="#s5:l1">locken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Over elke  <rhyme label="b" type="m" corresp="#s5:l2">sey</rhyme></l>
        <l id="s5:l5" met="4" type="-+">sy volghen de <rhyme label="b" type="m" corresp="#s5:l2">soldaetereij</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Men seijt op de <rhyme label="a" type="f">kermisse</rhyme></l>
        <l id="s6:l2" met="3" type="-+">hoe sy pronken <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Aen thoofdeken draghen sij <rhyme label="a" type="f" corresp="#s6:l1">locken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">over elcke sey sy <rhyme label="b" type="m" corresp="#s6:l2">vo</rhyme></l>
        <l id="s6:l5" met="4" type="-+">sy volghen de <rhyme label="b" type="m" corresp="#s6:l2">soldaeterey</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Veel dochters haer <rhyme label="a" type="f">opsteken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">het is haer ouders <rhyme label="b" type="m">schult</rhyme></l>
        <l id="s7:l3" met="3" type="-+">sy gaen vel <rhyme label="a" type="f" corresp="#s7:l1">ghestreken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">het aerken is <rhyme label="b" type="m" corresp="#s7:l2">ghecrult</rhyme></l>
        <l id="s7:l5" met="4" type="-+">maer sulcken dierken dient wel <rhyme label="b" type="m" corresp="#s7:l2">ghesmult</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sy doen dat aerken <rhyme label="a" type="f">crullen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">met veel <rhyme label="b" type="m">krunckelen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">en dat doet veel Jonckmans <rhyme label="a" type="f" corresp="#s8:l1">dulle</rhyme></l>
        <l id="s8:l4" met="3" type="-+">do al door de soete <rhyme label="b" type="m" corresp="#s8:l2">min</rhyme></l>
        <l id="s8:l5" met="4" type="-+">dat sy vaeren uet haeren <rhyme label="b" type="m" corresp="#s8:l2">sin</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Daer de Jonckman <rhyme label="a" type="f">vergaeren</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Al dansen en spelen <rhyme label="b" type="m">bley</rhyme></l>
        <l id="s9:l3" met="3" type="-+">daer gaense met groote <rhyme label="a" type="f" corresp="#s9:l1">schaeren</rhyme></l>
        <l id="s9:l4" met="3" type="-+">die herberge <rhyme label="b" type="m" corresp="#s9:l2">verby</rhyme></l>
        <l id="s9:l5" met="4" type="-+">dat is te seggen Jonckman haelt <rhyme label="b" type="m" corresp="#s9:l2">mij</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">siet wat de dochters <rhyme label="a" type="f">draghen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Als sy commen ten <rhyme label="b" type="m">dans</rhyme></l>
        <l id="s10:l3" met="3" type="-+">seer schoon al mode <rhyme label="a" type="f" corresp="#s10:l1">craghen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">en lyfkens op syn <rhyme label="b" type="m" corresp="#s10:l2">frans</rhyme></l>
        <l id="s10:l5" met="4" type="-+">langhe hantschoonnen heb de <rhyme label="b" type="m" corresp="#s10:l2">Jonckmans</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Men hoort de schoenen <rhyme label="a" type="f">cracken</rhyme></l>
        <l id="s11:l2" met="3" type="-+">sy maeken gheheel straten <rhyme label="b" type="m">verstoort</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Rocken van elle <rhyme label="a" type="f" corresp="#s11:l1">laken</rhyme></l>
        <l id="s11:l4" met="3" type="-+">elck myn reden aen <rhyme label="b" type="m" corresp="#s11:l2">hoort</rhyme></l>
        <l id="s11:l5" met="4" type="-+">den rock is tot het gat <rhyme label="b" type="m" corresp="#s11:l2">gheboort</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">So commen sy ten <rhyme label="a" type="f">danse</rhyme></l>
        <l id="s12:l2" met="3" type="-+">proper ende <rhyme label="b" type="m">plaisant</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Jonckman hier ic nu <rhyme label="a" type="f" corresp="#s12:l1">canse</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Nemt een beyder <rhyme label="b" type="m" corresp="#s12:l2">ant</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Die in u herte de croone <rhyme label="b" type="m" corresp="#s12:l2">spant</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Sy dansen en sy <rhyme label="a" type="f">springhen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">met een Jonc <rhyme label="b" type="m">ghesel</rhyme></l>
        <l id="s13:l3" met="3" type="-+">men siese haer gat <rhyme label="a" type="f" corresp="#s13:l1">vringhen</rhyme></l>
        <l id="s13:l4" met="3" type="-+">som gaense fraij <rhyme label="b" type="m" corresp="#s13:l2">deytel</rhyme></l>
        <l id="s13:l5" met="4" type="-+">van dansen syn sy <rhyme label="b" type="m" corresp="#s13:l2">cornel</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Som danschen aen <rhyme label="a" type="f">gheswollen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">lustich pier en <rhyme label="b" type="m">paer</rhyme></l>
        <l id="s14:l3" met="3" type="-+">som gaense <rhyme label="a" type="f" corresp="#s14:l1">cabbekollen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">sy huyrcken al naer de <rhyme label="b" type="m" corresp="#s14:l2">snaer</rhyme></l>
        <l id="s14:l5" met="4" type="-+">veel beter oft een precatie <rhyme label="b" type="m" corresp="#s14:l2">waer</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Op fruyten en op <rhyme label="a" type="f">vylen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">dansen sy <rhyme label="b" type="m">compeer</rhyme></l>
        <l id="s15:l3" met="3" type="-+">men siese met haer <rhyme label="a" type="f" corresp="#s15:l1">spelen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">het gat ga over en <rhyme label="b" type="m" corresp="#s15:l2">d'weer</rhyme></l>
        <l id="s15:l5" met="4" type="-+">deen springht op en dander <rhyme label="b" type="m" corresp="#s15:l2">neer</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Of ick hebbe veel <rhyme label="a" type="f">dochters</rhyme></l>
        <l id="s16:l2" met="3" type="-+">sy en willent niet <rhyme label="b" type="m">verstaen</rhyme></l>
        <l id="s16:l3" met="3" type="-+">sy spelen al over <rhyme label="a" type="f" corresp="#s16:l1">schreven</rhyme></l>
        <l id="s16:l4" met="3" type="-+">ick sie wel de <rhyme label="b" type="m" corresp="#s16:l2">dochters</rhyme></l>
        <l id="s16:l5" met="4" type="-+">syn niet met de hooverdey <rhyme label="b" type="m" corresp="#s16:l2">belaen</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Jonckmans wilt u <rhyme label="a" type="f">bedincken</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Vry en so <rhyme label="b" type="m">excelent</rhyme></l>
        <l id="s17:l3" met="3" type="-+">eer ghy gaet <rhyme label="a" type="f" corresp="#s17:l1">verdrincken</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Verwt pieter liefde <rhyme label="b" type="m" corresp="#s17:l2">blent</rhyme></l>
        <l id="s17:l5" met="4" type="-+">eer ghy het water <rhyme label="b" type="m" corresp="#s17:l2">kent</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
