    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 009 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">UYt over-volle <rhyme label="a" type="m">zielen-kracht</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Danck ick dy, Heer, en roem uw’ <rhyme label="a" type="m" corresp="#s1:l1">macht</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">En wilder tong en pen <rhyme label="b" type="f">toestellen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Om uwe wond’ren te <rhyme label="b" type="f" corresp="#s1:l3">vertellen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In Gode is mijn hert <rhyme label="a" type="m">verheught</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Godt is fonteyn van alle <rhyme label="a" type="m" corresp="#s2:l1">vreuchdt</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Des Alderhooghsten loff te <rhyme label="b" type="f">singen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Gaet my voor alle and’re <rhyme label="b" type="f" corresp="#s2:l3">dingen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Met recht. Want door uws handts <rhyme label="a" type="m">geweldt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Moest ons de vyandt uyt het <rhyme label="a" type="m" corresp="#s3:l1">veldt</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Moest hy uyt sijne <rhyme label="b" type="f">leger-wallen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En schand’lijck voor uw’ voeten <rhyme label="b" type="f" corresp="#s3:l3">vallen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy neemt u mijnes trouwlijck <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Om in mijn recht my voor te <rhyme label="a" type="m" corresp="#s4:l1">staen</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Ghy uyt’t, rechtvaerdigh richter, ’t <rhyme label="b" type="f">oordeel</rhyme></l>
        <l id="s4:l4" met="4" type="-+">uyt uwen throon tot mijn <rhyme label="b" type="f" corresp="#s4:l3">voordeel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Het Heydensch volck, verwaent van <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Bindt ghy sijn pracht en trotsheydt <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Haer naem, verdoemt ter <rhyme label="b" type="f">duysternissen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Moet eeuw’lijck allen luyster <rhyme label="b" type="f" corresp="#s5:l3">missen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sie, trotser, hoe dijn trotsheydt <rhyme label="a" type="m">zijght</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Hebt ghy nu alles <rhyme label="a" type="m" corresp="#s6:l1">uyt-gekrijght</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">Zijn onse Ste’en al heel <rhyme label="b" type="f">verdorven</rhyme>?</l>
        <l id="s6:l4" met="4" type="-+">En, t’saem met haer, haer’ naem <rhyme label="b" type="f" corresp="#s6:l3">gestorven</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O! ver van daen. Daer isser <rhyme label="a" type="m">een</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Die blijft in aller <rhyme label="a" type="m" corresp="#s7:l1">eeuwiche’en</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">De Heer. wiens richt-stoel vast geset <rhyme label="b" type="f">is</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En ’t algemeene recht en wet <rhyme label="b" type="f" corresp="#s7:l3">is</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Al wat hy keurt, is recht en <rhyme label="a" type="m">sluyt</rhyme>.</l>
        <l id="s8:l2" met="4" type="-+">Noyt gaff sijn banck quaedt vonnis <rhyme label="a" type="m" corresp="#s8:l1">uyt</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Den armen staen sijn’ ooren <rhyme label="b" type="f">ope</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Als sy met noodt-be’e tot hem <rhyme label="b" type="f" corresp="#s8:l3">loopen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Te recht vertrouwt dy, in <rhyme label="a" type="m">ellendt</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Al wie uw’ grooten name <rhyme label="a" type="m" corresp="#s9:l1">kent</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Want ghy, Heer, sultse niet <rhyme label="b" type="f">begeven</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Die langs het deuchdt-padt t’uwaerts <rhyme label="b" type="f" corresp="#s9:l3">streven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Nu looft den Heere onsen <rhyme label="a" type="m">God</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Die wooning houdt op Syons <rhyme label="a" type="m" corresp="#s10:l1">slot</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Maeckt sijne eer van eere <rhyme label="b" type="f">vruchtbaer</rhyme></l>
        <l id="s10:l4" met="4" type="-+">En sijnen naem by allen <rhyme label="b" type="f" corresp="#s10:l3">ruchtbaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Des armen bloedt, meest kleyn <rhyme label="a" type="m">geacht</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Is kostelijck in sijn <rhyme label="a" type="m" corresp="#s11:l1">gedacht</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">’tGeschrey van die in kommer <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Staet onverget’lijck <rhyme label="b" type="f" corresp="#s11:l3">opgeschreven</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Genaed’, ach! Heer. sie aff op <rhyme label="a" type="m">m</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Die soo veel quaedts en ellendt <rhyme label="a" type="m" corresp="#s12:l1">ly</rhyme>’</l>
        <l id="s12:l3" met="4" type="-+">Van haer die my vyandigh <rhyme label="b" type="f">naken</rhyme>;</l>
        <l id="s12:l4" met="4" type="-+">En ruck my uyt des doodes <rhyme label="b" type="f" corresp="#s12:l3">kaken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Soo krijght mijn tonge rijcke <rhyme label="a" type="m">stof</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Om uwer daden hoogen <rhyme label="a" type="m" corresp="#s13:l1">loff</rhyme></l>
        <l id="s13:l3" met="4" type="-+">In Syons poorten uyt te <rhyme label="b" type="f">meten</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">En elck ten volsten te doen <rhyme label="b" type="f" corresp="#s13:l3">weten</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Soo swemt mijn hert in <rhyme label="a" type="m">vrolijckheydt</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Wanneer’t aendachtigh <rhyme label="a" type="m" corresp="#s14:l1">overleyt</rhyme></l>
        <l id="s14:l3" met="4" type="-+">Hoe soet uw’ hulp is komen <rhyme label="b" type="f">dalen</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Tot eyndiging van alle <rhyme label="b" type="f" corresp="#s14:l3">qualen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">De Heyd’nen zijn verstrickt in’t <rhyme label="a" type="m">ne</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Van hun voor and’ren <rhyme label="a" type="m" corresp="#s15:l1">toe-geset</rhyme>.</l>
        <l id="s15:l3" met="4" type="-+">Sy vallen in haer eygen <rhyme label="b" type="f">grachten</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Verraden van haers selffs <rhyme label="b" type="f" corresp="#s15:l3">gedachten</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">O! oordeel vol <rhyme label="a" type="m">verwondernis</rhyme>!</l>
        <l id="s16:l2" met="4" type="-+">Getuyge dat Godt Richter <rhyme label="a" type="m" corresp="#s16:l1">is</rhyme>.</l>
        <l id="s16:l3" met="4" type="-+">De boos’ (o! saeck wel waerdt om <rhyme label="b" type="f">mercken</rhyme>!)</l>
        <l id="s16:l4" met="4" type="-+">Bestrickt sich in sijn eygen <rhyme label="b" type="f" corresp="#s16:l3">wercken</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Dus komt in’t endt des ondeuchdts <rhyme label="a" type="m">straff</rhyme>.</l>
        <l id="s17:l2" met="4" type="-+">De boose moetter schielijck <rhyme label="a" type="m" corresp="#s17:l1">aff</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">En wordt, met hen die Godts <rhyme label="b" type="f">vergeten</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Ter eyndelooser doodt <rhyme label="b" type="f" corresp="#s17:l3">versmeten</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Heel anders is’t met ’t ned’righ <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">’tWelck treurt, doch niet verdoft in <rhyme label="a" type="m" corresp="#s18:l1">smert</rhyme>;</l>
        <l id="s18:l3" met="4" type="-+">’tWelck ongeval wel dreyght te <rhyme label="b" type="f">krencken</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">Maer noyt en is uyt Godts <rhyme label="b" type="f" corresp="#s18:l3">gedencken</rhyme>:</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Wiens hoop’ wel duyckt, maer niet en <rhyme label="a" type="m">sterft</rhyme>;</l>
        <l id="s19:l2" met="4" type="-+">Wel uytstel lijdt en <rhyme label="a" type="m" corresp="#s19:l1">omme-swerft</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Maer, niet in eeuwicheydt <rhyme label="b" type="f">verloren</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">In’t endt bereyckt ’tgeen s’ heeft <rhyme label="b" type="f" corresp="#s19:l3">verkoren</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Sta op, mijn Godt. ach! heng het <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s20:l2" met="4" type="-+">Dat hier een Mensch (een worm, een <rhyme label="a" type="m" corresp="#s20:l1">riedt</rhyme>)</l>
        <l id="s20:l3" met="4" type="-+">Sich over alles Meester <rhyme label="b" type="f">make</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">En tot den toom der dingen <rhyme label="b" type="f" corresp="#s20:l3">rake</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Dat alle Heyden, onder <rhyme label="a" type="m">toom</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">In ootmoedt t’uwer vierschaer <rhyme label="a" type="m" corresp="#s21:l1">koom</rhyme>.</l>
        <l id="s21:l3" met="4" type="-+">Dat sich de grootheydt uwer <rhyme label="b" type="f">machten</rhyme></l>
        <l id="s21:l4" met="4" type="-+">Vertoon, en u voor Heer doe <rhyme label="b" type="f" corresp="#s21:l3">achten</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Dat schrick de zielen diep <rhyme label="a" type="m">door-sny</rhyme>’:</l>
        <l id="s22:l2" met="4" type="-+">Op datse die door <rhyme label="a" type="m" corresp="#s22:l1">hoovaerdy</rhyme></l>
        <l id="s22:l3" met="4" type="-+">Na al te groote dingen <rhyme label="b" type="f">rennen</rhyme>,</l>
        <l id="s22:l4" met="4" type="-+">Haer swacke menschlijckheydt <rhyme label="b" type="f" corresp="#s22:l3">erkennen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
