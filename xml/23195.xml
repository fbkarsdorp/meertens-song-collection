    <melody>Nu laat ons zingen het is tijd</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Nu laet ons singhen, het is <rhyme label="a" type="m">tijt</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die ons allen heeft <rhyme label="a" type="m" corresp="#s1:l1">verblijt</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s1:l2">crimine</rhyme></l>
        <l id="s1:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s1:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Vader in der <rhyme label="a" type="m">ewicheit</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ontfermt U onser <rhyme label="a" type="m" corresp="#s2:l1">eellendicheit</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s2:l2">crimine</rhyme></l>
        <l id="s2:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s2:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat Adam in den appel <rhyme label="a" type="m">beet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat mach ons allen wesen <rhyme label="a" type="m" corresp="#s3:l1">leet</rhyme>!</l>
        <l id="s3:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s3:l2">crimine</rhyme></l>
        <l id="s3:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s3:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Want wy daer om hier te <rhyme label="a" type="m">voren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Mosten wy allen sijn <rhyme label="a" type="m" corresp="#s4:l1">verloren</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s4:l2">crimine</rhyme></l>
        <l id="s4:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s4:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die Godes Soen van <rhyme label="a" type="m">hemelrijck</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hi woude worden ons <rhyme label="a" type="m" corresp="#s5:l1">ghelijck</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s5:l2">crimine</rhyme></l>
        <l id="s5:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s5:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Daer om is Hi hier neder <rhyme label="a" type="m">ghecomen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ende heeft ons mensheit <rhyme label="a" type="m" corresp="#s6:l1">aenghenomen</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s6:l2">crimine</rhyme></l>
        <l id="s6:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s6:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Tot eenre Maghet, al te <rhyme label="a" type="m">hant</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Heeft God Sinen enghel neder <rhyme label="a" type="m" corresp="#s7:l1">ghesant</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s7:l2">crimine</rhyme></l>
        <l id="s7:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s7:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">'God gruet U!' sprac die enghel <rhyme label="a" type="m">saen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">'Ghi sijdt vol gracien, al sonder <rhyme label="a" type="m" corresp="#s8:l1">waen'</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s8:l2">crimine</rhyme></l>
        <l id="s8:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s8:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">'Van Gode, soe doen ick U <rhyme label="a" type="m">verstaen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat Ghi den Goods Soen selt <rhyme label="a" type="m" corresp="#s9:l1">ontfaen'</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s9:l2">crimine</rhyme></l>
        <l id="s9:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s9:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">'Want Hi U daer toe heeft <rhyme label="a" type="m">vercoren</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Dat Hi van U wil sijn <rhyme label="a" type="m" corresp="#s10:l1">gheboren'</rhyme>.</l>
        <l id="s10:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s10:l2">crimine</rhyme></l>
        <l id="s10:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s10:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Als Maria dat <rhyme label="a" type="m">verstoet</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Wart Sy vervaert in Haren <rhyme label="a" type="m" corresp="#s11:l1">moet</rhyme>.</l>
        <l id="s11:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s11:l2">crimine</rhyme></l>
        <l id="s11:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s11:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Si sprack den enghel met vresen <rhyme label="a" type="m">aen</rhyme>:</l>
        <l id="s12:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">'Hoe soude dat sijn? Ic en kende nie <rhyme label="a" type="m" corresp="#s12:l1">man</rhyme>!'</l>
        <l id="s12:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s12:l2">crimine</rhyme></l>
        <l id="s12:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s12:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Den enghel sprack Haer toe, <rhyme label="a" type="m">mettien</rhyme>:</l>
        <l id="s13:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">'Maria, en wilt U niet <rhyme label="a" type="m" corresp="#s13:l1">ontsien'</rhyme>.</l>
        <l id="s13:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s13:l2">crimine</rhyme></l>
        <l id="s13:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s13:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">'Die Heilighe Gheest sal U <rhyme label="a" type="m">bevaen</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Met crachten groot salt Ghi Hem <rhyme label="a" type="m" corresp="#s14:l1">ontfaen'</rhyme>.</l>
        <l id="s14:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s14:l2">crimine</rhyme></l>
        <l id="s14:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s14:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">'Goods wil, die moet in My <rhyme label="a" type="m">gheschien</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Sijn Dienstmaghet, die wil Ic gaerne <rhyme label="a" type="m" corresp="#s15:l1">sijn'</rhyme>.</l>
        <l id="s15:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s15:l2">crimine</rhyme></l>
        <l id="s15:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s15:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Doen Sy dat sprack <rhyme label="a" type="m">oetmoedelijck</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Ontfinck Sy God van <rhyme label="a" type="m" corresp="#s16:l1">hemelrijck</rhyme>.</l>
        <l id="s16:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s16:l2">crimine</rhyme></l>
        <l id="s16:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s16:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Sonder pijn of sonder <rhyme label="a" type="m">vaer</rhyme></l>
        <l id="s17:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Ruste Hi neghen maenden in <rhyme label="a" type="m" corresp="#s17:l1">Haer</rhyme>.</l>
        <l id="s17:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s17:l2">crimine</rhyme></l>
        <l id="s17:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s17:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">In den nacht dat Sy <rhyme label="a" type="m">ghelach</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Van vrouwen en was daer gheen <rhyme label="a" type="m" corresp="#s18:l1">ghewach</rhyme>.</l>
        <l id="s18:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s18:l2">crimine</rhyme></l>
        <l id="s18:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s18:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Die enghel Goods van <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Sy songhen allegadaer <rhyme label="a" type="m" corresp="#s19:l1">blydelijck</rhyme>:</l>
        <l id="s19:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s19:l2">crimine</rhyme></l>
        <l id="s19:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s19:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">'Gloria al inder <rhyme label="a" type="m">ewicheit</rhyme></l>
        <l id="s20:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">Sy daer Heiligher <rhyme label="a" type="m" corresp="#s20:l1">Drievoldicheit'</rhyme>.</l>
        <l id="s20:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s20:l2">crimine</rhyme></l>
        <l id="s20:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s20:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Die hardekijns al op die velden <rhyme label="a" type="m">laghen</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">Groote claerheit die sij <rhyme label="a" type="m" corresp="#s21:l1">saghen</rhyme>.</l>
        <l id="s21:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s21:l2">crimine</rhyme></l>
        <l id="s21:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s21:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Met vresen waren sy allen <rhyme label="a" type="m">bevaen</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s22:l3" met="4" type="-+">Want die enghel, die quam by hem <rhyme label="a" type="m" corresp="#s22:l1">staen</rhyme>:</l>
        <l id="s22:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s22:l2">crimine</rhyme></l>
        <l id="s22:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s22:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">'Ghi hardekijns, en wilt u niet <rhyme label="a" type="m">vervaren</rhyme>!</l>
        <l id="s23:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">Van Gode brenghe ic u niewe <rhyme label="a" type="m" corresp="#s23:l1">maren</rhyme>!'</l>
        <l id="s23:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s23:l2">crimine</rhyme></l>
        <l id="s23:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s23:l2">glorie</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">'Want huden is een Kijnt <rhyme label="a" type="m">gheboren</rhyme>,</l>
        <l id="s24:l2" met="4" type="-+">Est pueuer natus <rhyme label="b" type="m">hodie</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Van eenre Maghet <rhyme label="a" type="m" corresp="#s24:l1">uutvercoren'</rhyme>.</l>
        <l id="s24:l4" met="4" type="+-">Pro nostrorum <rhyme label="b" type="m" corresp="#s24:l2">crimine</rhyme></l>
        <l id="s24:l5" met="8" type="+-">Hodie Hodie natus est Rex <rhyme label="b" type="m" corresp="#s24:l2">glorie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
