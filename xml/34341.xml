  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VOlmaeckste <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn Laura die ick <rhyme label="b" type="f">heden</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Heb aenbeden,, als <rhyme label="a" type="m" corresp="#s1:l1">Goddin</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">U Ambrosijnen <rhyme label="c" type="m">vloet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Der liefelijcke <rhyme label="d" type="f">woorden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Heeft te recht ghequetst mijn <rhyme label="c" type="m" corresp="#s1:l4">bloet</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Ghy hebt in <rhyme label="e" type="m">t'ghedacht</rhyme></l>
        <l id="s1:l8" met="2" type="+-">My <rhyme label="e" type="m" corresp="#s1:l7">ghebracht</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Suer en <rhyme label="c" type="m" corresp="#s1:l4">soet</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Waer door ick u beminnen <rhyme label="c" type="m" corresp="#s1:l4">moet</rhyme>,</l>
        <l id="s1:l11" met="3" type="-+">U vrindelijck <rhyme label="f" type="m">ghelaet</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Mijn graghe ziel <rhyme label="d" type="f" corresp="#s1:l5">becoorden</rhyme></l>
        <l id="s1:l13" met="4" type="+-">Oft een blixem t'hert <rhyme label="f" type="m" corresp="#s1:l11">verslaet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer k'bid u waer toe <rhyme label="a" type="m">dint</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Mijn Laura moet ick <rhyme label="b" type="f">vraeghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Al dit plaeghen,, Op u <rhyme label="a" type="m" corresp="#s2:l1">vrint</rhyme>?</l>
        <l id="s2:l4" met="3" type="-+">Die my met ancxt <rhyme label="c" type="m">bevanght</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In bitter <rhyme label="d" type="f">minne-suchten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat als suer in 'thert my <rhyme label="c" type="m" corresp="#s2:l4">vranght</rhyme>:</l>
        <l id="s2:l7" met="3" type="+-">Waer toe niet <rhyme label="e" type="m">vrindin</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Soete <rhyme label="e" type="m" corresp="#s2:l7">min</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Daer mijn <rhyme label="c" type="m" corresp="#s2:l4">smert</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Dus mede haest verdreven <rhyme label="c" type="m" corresp="#s2:l4">wert</rhyme>,</l>
        <l id="s2:l11" met="3" type="-+">Dan can ick oosten <rhyme label="f" type="m">vaeck</rhyme></l>
        <l id="s2:l12" met="3" type="-+">De lieflelijcke <rhyme label="d" type="f" corresp="#s2:l5">vruchten</rhyme>,</l>
        <l id="s2:l13" met="4" type="+-">Daer ick driftelijck naer <rhyme label="f" type="m" corresp="#s2:l11">snaeck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Y vlucht toch niet van <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">O eenigh <rhyme label="b" type="f">uytvercoren</rhyme></l>
        <l id="s3:l3" met="4" type="+-">My gheboren,, Zijde <rhyme label="a" type="m" corresp="#s3:l1">ghy</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Laet vry u heldre <rhyme label="c" type="m">Son</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Nu op een ander <rhyme label="d" type="f">straelen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Op Amint' oft <rhyme label="c" type="m" corresp="#s3:l4">Coridon</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Oft dat Tyters <rhyme label="e" type="m">riet</rhyme></l>
        <l id="s3:l8" met="2" type="+-">Door een <rhyme label="e" type="m" corresp="#s3:l7">liet</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Min <rhyme label="c" type="m" corresp="#s3:l4">begon</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Laura die is voor <rhyme label="c" type="m" corresp="#s3:l4">Celadon</rhyme>;</l>
        <l id="s3:l11" met="3" type="-+">Ick heb in t'minst geen <rhyme label="f" type="m">vrees</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Godt heeft in u doen <rhyme label="d" type="f" corresp="#s3:l5">daelen</rhyme></l>
        <l id="s3:l13" met="4" type="+-">Eene rib mijns eygen <rhyme label="f" type="m" corresp="#s3:l11">vlees</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Princers met cracht my <rhyme label="a" type="m">dwonck</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Cupido u te <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Laet beginnen t'min <rhyme label="a" type="m" corresp="#s4:l1">ghevonck</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">t'Is tijt dat ghy <rhyme label="c" type="m">veriaeght</rhyme></l>
        <l id="s4:l5" met="3" type="-+">De suere-coude <rhyme label="d" type="f">winden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Siet wie dat u liefde <rhyme label="c" type="m" corresp="#s4:l4">draeght</rhyme>:</l>
        <l id="s4:l7" met="3" type="+-">Oft dat ghy <rhyme label="e" type="m">veracht</rhyme></l>
        <l id="s4:l8" met="2" type="+-">Dese <rhyme label="e" type="m" corresp="#s4:l7">clacht</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Die ghy <rhyme label="c" type="m" corresp="#s4:l4">plaeght</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Sal wesen die u t'meest <rhyme label="c" type="m" corresp="#s4:l4">behaeght</rhyme>,</l>
        <l id="s4:l11" met="3" type="-+">Ten compt door geenen <rhyme label="f" type="m">mensch</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Maer Godt die sal ons <rhyme label="d" type="f" corresp="#s4:l5">binden</rhyme>,</l>
        <l id="s4:l13" met="4" type="+-">t'Is sijn wil naer onsen <rhyme label="f" type="m" corresp="#s4:l11">wensch</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
