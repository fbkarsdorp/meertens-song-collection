  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Aeltje datm' <rhyme label="a" type="m">ontwrinkt</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Baersje datm' <rhyme label="a" type="m" corresp="#s1:l1">ontsprinkt</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Cuil die mijn <rhyme label="a" type="m" corresp="#s1:l1">verdrinkt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Droesje dat me <rhyme label="a" type="m" corresp="#s1:l1">dwinkt</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Entje van mijn <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Fretje na men <rhyme label="a" type="m" corresp="#s2:l1">zin</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Gast-huys daar ik <rhyme label="a" type="m" corresp="#s2:l1">in</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Heeltjes graegjes <rhyme label="a" type="m" corresp="#s2:l1">bin</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Iachje van me <rhyme label="a" type="m">jeucht</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Knipje van <rhyme label="a" type="m" corresp="#s3:l1">geneucht</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Laatje van me <rhyme label="a" type="m" corresp="#s3:l1">vreught</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Mantje vol van <rhyme label="a" type="m" corresp="#s3:l1">deught</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Natter veur me <rhyme label="a" type="m">sijl</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Oosje veur me <rhyme label="a" type="m" corresp="#s4:l1">bijl</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Puik doel veur me <rhyme label="a" type="m" corresp="#s4:l1">pijl</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Quikstart van me <rhyme label="a" type="m" corresp="#s4:l1">hijl</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Rotie veur me <rhyme label="a" type="m">spek</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Slakje an me <rhyme label="a" type="m" corresp="#s5:l1">hek</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Troosje van <rhyme label="a" type="m" corresp="#s5:l1">gebrek</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Vatje veur me <rhyme label="a" type="m" corresp="#s5:l1">lek</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Witje van me <rhyme label="a" type="m">wit</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Ysje veur me <rhyme label="a" type="m" corresp="#s6:l1">hit</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Ziet eens hoe ik <rhyme label="a" type="m" corresp="#s6:l1">bit</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Dies gun u voetveeg <rhyme label="a" type="m" corresp="#s6:l1">dit</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Dat hy gelijck een <rhyme label="a" type="m">mol</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Mach vroeten in <rhyme label="a" type="m" corresp="#s7:l1">je</rhyme> ...</l>
        <l id="s7:l3" met="3" type="+-">Ik bidje mannetjes-duit, ey slaat de koop toch <rhyme label="a" type="m" corresp="#s7:l1">toe</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Of ik word, by me 's Jan, dit vryen straks aers <rhyme label="a" type="m" corresp="#s7:l1">moe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
