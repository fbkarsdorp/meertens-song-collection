    <tags/>
    <melody>Als Atalante doden maakte</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">FEbus had zijn goude <rhyme label="a" type="f">wagen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Qualijk <rhyme label="a" type="f" corresp="#s1:l1">aangeslagen</rhyme>,</l>
        <l id="s1:l3" met="7" type="-+">Als ik mijn teere geitjes hoeden, langs den <rhyme label="b" type="m">Amstel-stroom</rhyme>;</l>
        <l id="s1:l4" met="2" type="-+">Daar ik mijn <rhyme label="c" type="m">Zon</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Beschoten <rhyme label="c" type="m" corresp="#s1:l4">von</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">In schaauw van <rhyme label="a" type="f" corresp="#s1:l1">hagen</rhyme>;</l>
        <l id="s1:l7" met="2" type="-+">Ik kusten <rhyme label="d" type="m">haar</rhyme>;</l>
        <l id="s1:l8" met="2" type="-+">Zij wiert 't <rhyme label="d" type="m" corresp="#s1:l7">gewaar</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">En antwoort in haar <rhyme label="b" type="m" corresp="#s1:l3">droom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Kloris laat de <rhyme label="a" type="f">teugel-repen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Van uw weelde <rhyme label="a" type="f" corresp="#s2:l1">slepen</rhyme>,</l>
        <l id="s2:l3" met="7" type="-+">Omhelst, en kust, en blust uw' lust, met troetel-ziek <rhyme label="b" type="m">geswarm</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Gewenste <rhyme label="c" type="m">reên</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">VViens <rhyme label="c" type="m" corresp="#s2:l4">zoetigheên</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Mijn ooren <rhyme label="a" type="f" corresp="#s2:l1">grepen</rhyme>!</l>
        <l id="s2:l7" met="2" type="-+">Mijn harte <rhyme label="d" type="m">swom</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">In weeld', al <rhyme label="d" type="m" corresp="#s2:l7">om</rhyme>;</l>
        <l id="s2:l9" met="3" type="-+">Kupido blies <rhyme label="b" type="m" corresp="#s2:l3">alarm</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
