  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAnneer een Bloemken <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In zijn volle <rhyme label="a" type="m" corresp="#s1:l1">cieraet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dan is yeder Mensch <rhyme label="b" type="m">bereydt</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Om te plucken zijn <rhyme label="b" type="m" corresp="#s1:l3">schoonigheydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Soo gaet het met een <rhyme label="a" type="m">Maeght</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">Soo haest die Bloeysel <rhyme label="a" type="m" corresp="#s2:l1">draeght</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Versoecken de Iong-mans <rhyme label="b" type="m">veel</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Haer te rucken van hare <rhyme label="b" type="m" corresp="#s2:l3">steel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En als dat Bloemken <rhyme label="a" type="m">is</rhyme></l>
        <l id="s3:l2" met="3" type="-+">(Tot zijn <rhyme label="a" type="m" corresp="#s3:l1">verderffenis</rhyme>)</l>
        <l id="s3:l3" met="3" type="-+">Ghekomen in 's menschen <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Soo geraecket wel haest te <rhyme label="b" type="m" corresp="#s3:l3">schant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een Maghet van <rhyme label="a" type="m">ghelijck</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Hoe Eel, hoe Schoon, hoe <rhyme label="a" type="m" corresp="#s4:l1">Rijck</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Wanneer sy is <rhyme label="b" type="m">af-ghepuckt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Werdt haer het schoonste Schoon <rhyme label="b" type="m" corresp="#s4:l3">ontruckt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Doch soo het Bloemjes <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met vocht ghelavet <rhyme label="a" type="m" corresp="#s5:l1">wardt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Soo blyvet van des te <rhyme label="b" type="m">meer</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Noch te langher in zijn <rhyme label="b" type="m" corresp="#s5:l3">eer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Soo men de Maeghden, <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Oock niet ghedienstigh <rhyme label="a" type="m" corresp="#s6:l1">zy</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met dinghen, Natuyr van <rhyme label="b" type="m">nood'</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Soo verwelck'ren sy tot der <rhyme label="b" type="m" corresp="#s6:l3">doodt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hierom: een Bloem, en <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Schier een Natuere <rhyme label="a" type="m" corresp="#s7:l1">draeght</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Van schoon, en van soet; doch <rhyme label="b" type="m">teer</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Qualijck te houden in haer <rhyme label="b" type="m" corresp="#s7:l3">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
