<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>34223</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody>O schone jonkvrouw mocht mij gebeuren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HElaes! nu magh ick wel <rhyme label="a" type="f">verlanghen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Met pijne, ende betraende <rhyme label="a" type="f" corresp="#s1:l1">wanghen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En zwaer <rhyme label="b" type="m">gesucht</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Vol <rhyme label="b" type="m" corresp="#s1:l3">on-ghenucht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Naer u, die mijn hert houdt <rhyme label="a" type="f" corresp="#s1:l1">bevanghen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">En van my <rhyme label="b" type="m" corresp="#s1:l3">vlucht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Doen ick mijn liefd’ u eerst <rhyme label="a" type="f">vertelde</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">V hert ghy doen gantsch op my <rhyme label="a" type="f" corresp="#s2:l1">stelde</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Ghelijck het <rhyme label="b" type="m">scheen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Aen u <rhyme label="b" type="m" corresp="#s2:l3">gheween</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Nu laet ghy my als de <rhyme label="a" type="f" corresp="#s2:l1">ghequelde</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Bedroeft <rhyme label="b" type="m" corresp="#s2:l3">alleen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy zwoert my gheensints te <rhyme label="a" type="f">verlaten</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Nu vliedt ghy als den <rhyme label="a" type="f" corresp="#s3:l1">obstinaten</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Theseus <rhyme label="b" type="m">ontrouw</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Die meed’ in <rhyme label="b" type="m" corresp="#s3:l3">rouw</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Broght Ariadne door sijn <rhyme label="a" type="f" corresp="#s3:l1">praten</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Die schoon <rhyme label="b" type="m" corresp="#s3:l3">Karsouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer Iupiter heeft haer <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En een blinckende kroon <rhyme label="a" type="f" corresp="#s4:l1">ghegheven</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Aen ’t <rhyme label="b" type="m">firmament</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Voor haer <rhyme label="b" type="m" corresp="#s4:l3">ellend</rhyme>’,</l>
        <l id="s4:l5" met="4" type="-+">Sulcx wacht ick oock, als mijn droef <rhyme label="a" type="f" corresp="#s4:l1">leven</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Sal zijn <rhyme label="b" type="m" corresp="#s4:l3">ghe-endt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Als uw’ conscientie u sal <rhyme label="a" type="f">knaghen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Van ’t lijden dat ghy hebt doen <rhyme label="a" type="f" corresp="#s5:l1">draghen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Vw’ trouw’ <rhyme label="b" type="m">slavin</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Voor hare <rhyme label="b" type="m" corresp="#s5:l3">min</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Die ghy hebt gaen van u <rhyme label="a" type="f" corresp="#s5:l1">verjaghen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Met wreeden <rhyme label="b" type="m" corresp="#s5:l3">sin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ontsteeckt nu Cupido, uw’ <rhyme label="a" type="f">brander</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">En brenght daer door eenigh <rhyme label="a" type="f" corresp="#s6:l1">verander</rhyme></l>
        <l id="s6:l3" met="2" type="-+">In mijnen <rhyme label="b" type="m">vrindt</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Dat hy my <rhyme label="b" type="m" corresp="#s6:l3">mindt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Alleen door een, want tot gheen <rhyme label="a" type="f" corresp="#s6:l1">ander</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Ben ick <rhyme label="b" type="m" corresp="#s6:l3">ghesindt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
