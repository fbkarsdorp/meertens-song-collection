  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Eerbaere schoen <rhyme label="a" type="m">kersouwe</rhyme></l>
        <l id="s1:l2" met="3" type="-+">aensiet u dienaers <rhyme label="b" type="m">torment</rhyme></l>
        <l id="s1:l3" met="3" type="-+">verlicht sijn druck en <rhyme label="a" type="m" corresp="#s1:l1">rou</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Eer hij sijn leeven <rhyme label="b" type="m" corresp="#s1:l2">ent</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Sijn hert, lijt <rhyme label="c" type="m">smert</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Twelck u liefde hem <rhyme label="b" type="m" corresp="#s1:l2">Inprent</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Salt u o maeget <rhyme label="a" type="m">fier</rhyme></l>
        <l id="s2:l2" met="3" type="-+">niet voordrieten voor of <rhyme label="b" type="m">naer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">dat gij ter doodt toe <rhyme label="a" type="m" corresp="#s2:l1">schier</rhyme></l>
        <l id="s2:l4" met="3" type="-+">hebt gewont u getrou <rhyme label="b" type="m" corresp="#s2:l2">minnaer</rhyme></l>
        <l id="s2:l5" met="2" type="-+">O wacht, den <rhyme label="c" type="m">dach</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Dat Ick aen sach u aenschin <rhyme label="b" type="m" corresp="#s2:l2">klaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Cuibydoe komt mijn te <rhyme label="a" type="m">baet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">met uwen schichte <rhyme label="b" type="m">fel</rhyme></l>
        <l id="s3:l3" met="3" type="-+">wilt doen nae mijnen <rhyme label="a" type="m" corresp="#s3:l1">raet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">doorschiet mijns liefs herte <rhyme label="b" type="m" corresp="#s3:l2">fel</rhyme></l>
        <l id="s3:l5" met="2" type="-+">dat sij, doch <rhyme label="c" type="m">mij</rhyme></l>
        <l id="s3:l6" met="4" type="+-">mach verlossen van <rhyme label="b" type="m" corresp="#s3:l2">gequel</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Prinselicke liefste <rhyme label="a" type="m">grein</rhyme></l>
        <l id="s4:l2" met="3" type="-+">geeft mijn een troestelick <rhyme label="b" type="m">woert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">bewijst hem liefde <rhyme label="a" type="m" corresp="#s4:l1">reyn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Wiens gij het hert <rhyme label="b" type="m" corresp="#s4:l2">duerwont</rhyme></l>
        <l id="s4:l5" met="2" type="-+">haest u, als <rhyme label="c" type="m">nu</rhyme></l>
        <l id="s4:l6" met="4" type="+-">thert voorlanckt nae u <rhyme label="b" type="m" corresp="#s4:l2">konfoort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
