  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Luistert eens <rhyme label="a" type="f">Jeannette</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Staat iets moois u aan, <rhyme label="a" type="f" corresp="#s1:l1">laridette</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Luister eens <rhyme label="a" type="f" corresp="#s1:l1">jeannette</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Om naar Parijs te <rhyme label="b" type="m">gaan</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Ja doch, mijnheer! ja doch, <rhyme label="c" type="f">mijnheer</rhyme>!</l>
        <l id="s1:l6" met="2" type="-+">Sprak de <rhyme label="a" type="f" corresp="#s1:l1">Bruinette</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Maar waarom mij, maar waarom <rhyme label="d" type="m">mij</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Dit <rhyme label="e" type="m">aangeboón</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Hoe lieve <rhyme label="a" type="f" corresp="#s1:l1">Jeannette</rhyme>,</l>
        <l id="s1:l10" met="5" type="+-">Gij met zoo veel schoon <rhyme label="a" type="f" corresp="#s1:l1">laridette</rhyme>,</l>
        <l id="s1:l11" met="6" type="+-">Hoe lieve Jeannette, vindt dit <rhyme label="d" type="m" corresp="#s1:l7">ongewoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Luistert eens <rhyme label="a" type="f">Jeannette</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">Zoo gij geld bemind <rhyme label="a" type="f" corresp="#s2:l1">laridette</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Luistert eens <rhyme label="a" type="f" corresp="#s2:l1">Jeannette</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Daar neem aan mijn <rhyme label="b" type="m">kind</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Och, och, mijnheer! Och, och, <rhyme label="c" type="f">mijnheer</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">Zei de <rhyme label="a" type="f" corresp="#s2:l1">bruinette</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Om wat te doen? Om wat te <rhyme label="d" type="m">doen</rhyme>?</l>
        <l id="s2:l8" met="2" type="+-">Mij geld <rhyme label="e" type="m">geboón</rhyme>,,</l>
        <l id="s2:l9" met="3" type="+-">Hoe lieve <rhyme label="a" type="f" corresp="#s2:l1">Jeannette</rhyme>.</l>
        <l id="s2:l10" met="5" type="+-">Gij met zoo veel schoon <rhyme label="a" type="f" corresp="#s2:l1">laridette</rhyme>,</l>
        <l id="s2:l11" met="6" type="+-">Hoe lieve Jeannete, vindt dit <rhyme label="d" type="m" corresp="#s2:l7">ongewoon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Luistert eens <rhyme label="a" type="f">Jeannette</rhyme>,</l>
        <l id="s3:l2" met="5" type="+-">Geef me een lekkre zoen <rhyme label="a" type="f" corresp="#s3:l1">laridette</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Luistert eens <rhyme label="a" type="f" corresp="#s3:l1">Jeannette</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Toe dat moest je <rhyme label="b" type="m">doen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Och, och, mijnheer! och, och, <rhyme label="c" type="f">mijnheer</rhyme>!</l>
        <l id="s3:l6" met="2" type="-+">Zei de <rhyme label="a" type="f" corresp="#s3:l1">bruinette</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ik weet niet hoe, ik weet niet <rhyme label="d" type="m">hoe</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Ik 't zeggen <rhyme label="e" type="m">zal</rhyme>;</l>
        <l id="s3:l9" met="3" type="+-">Weet dan dat <rhyme label="a" type="f" corresp="#s3:l1">Jeannette</rhyme>,</l>
        <l id="s3:l10" met="5" type="+-">'t Geeft voor niemendal, <rhyme label="a" type="f" corresp="#s3:l1">laridette</rhyme>,</l>
        <l id="s3:l11" met="6" type="+-">Weet dan dat Jeannette, 't geeft voor <rhyme label="d" type="m" corresp="#s3:l7">niemendal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
