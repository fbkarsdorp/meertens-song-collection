  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">NIet gestadig by een <rhyme label="a" type="m">Meyt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Want gy raakt u zinnen <rhyme label="a" type="m" corresp="#s1:l1">kwyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Nog gestadig by de <rhyme label="b" type="m">Maagt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Want dat maakt u bloed te <rhyme label="b" type="m" corresp="#s1:l3">traag</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maar wilt gy zyn zeer <rhyme label="a" type="m">rustig</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ga by de Wyn zeer <rhyme label="a" type="m" corresp="#s2:l1">reyn</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">En maakt u daar me <rhyme label="b" type="m">lustig</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Het geneest de grootste <rhyme label="b" type="m" corresp="#s2:l3">pyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Za lustig helder <rhyme label="a" type="m">vrolyk</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En drink wat voor de <rhyme label="a" type="m" corresp="#s3:l1">dorst</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Tot dat je rold van <rhyme label="b" type="m">boven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En voldoet zo togt op <rhyme label="b" type="m" corresp="#s3:l3">togt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Za lustig uit uwe <rhyme label="a" type="m">Keelen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Het is Lang genoeg <rhyme label="a" type="m" corresp="#s4:l1">gewagt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ik ga wat zitten <rhyme label="b" type="m">spelen</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Tot dat koomt dan klaren <rhyme label="b" type="m" corresp="#s4:l3">dag</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Za zyt nu lustig <rhyme label="a" type="m">vrolyk</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En helder uit de <rhyme label="a" type="m" corresp="#s5:l1">borst</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">En drink tot dat je <rhyme label="b" type="m">oolyk</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Wat glaazen voor den <rhyme label="b" type="m" corresp="#s5:l3">dorst</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Za drinkt tot datje <rhyme label="a" type="m">rold</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dag zuypt u gat niet <rhyme label="a" type="m" corresp="#s6:l1">vol</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Maar weest gestadig <rhyme label="b" type="m">Cras</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">Dat maakt dat gy blyft de <rhyme label="b" type="m" corresp="#s6:l3">baas</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Za ik schey uit met <rhyme label="a" type="m">spelen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Want ik heb genoeg <rhyme label="a" type="m" corresp="#s7:l1">gezeyt</rhyme>;</l>
        <l id="s7:l3" met="4" type="+-">Gy kund my ligt <rhyme label="b" type="m">vergeven</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Ben ik hier in ook <rhyme label="b" type="m" corresp="#s7:l3">misleyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
