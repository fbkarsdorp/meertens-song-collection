<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6198</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Corydon ontstoken</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SOetst' der Venus <rhyme label="a" type="f">Nichjes</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Pronckjes van ons <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Die de <rhyme label="a" type="f" corresp="#s1:l1">Minne-plichjes</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Onderworpen <rhyme label="b" type="m" corresp="#s1:l2">zijt</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Als ick u <rhyme label="c" type="f">aenschouwe</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Kan ick mijn jonck <rhyme label="d" type="m">hart</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Van u niet <rhyme label="c" type="f" corresp="#s1:l5">onthouden</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Of ick leef in <rhyme label="d" type="m" corresp="#s1:l6">smart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Lieve <rhyme label="a" type="f">Toveresje</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Die mijn hartjen <rhyme label="b" type="m">wondt</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Sachte <rhyme label="a" type="f" corresp="#s2:l1">Meesteresje</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Maeckt my weer <rhyme label="b" type="m" corresp="#s2:l2">ghesont</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Hebt ghy my doen <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Met u lief <rhyme label="d" type="m">aenschijn</rhyme>:</l>
        <l id="s2:l7" met="3" type="+-">Ghy kondt my <rhyme label="c" type="f" corresp="#s2:l5">bevryden</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Van de selve <rhyme label="d" type="m" corresp="#s2:l6">pijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Laet de soete <rhyme label="a" type="f">tipjes</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Van u lieve <rhyme label="b" type="m">Mondt</rhyme>:</l>
        <l id="s3:l3" met="3" type="+-">Reycken tot mijn <rhyme label="a" type="f" corresp="#s3:l1">lipjes</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Maecken my <rhyme label="b" type="m" corresp="#s3:l2">ghesondt</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Met het Sap der <rhyme label="c" type="f">Minnen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Dat u lipjes <rhyme label="d" type="m">laedt</rhyme>;</l>
        <l id="s3:l7" met="3" type="+-">Puyckjen der <rhyme label="c" type="f" corresp="#s3:l5">Goddinnen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Komt my doch te <rhyme label="d" type="m" corresp="#s3:l6">baet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Nu ick u <rhyme label="a" type="f">ontmoete</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Als een dienstbaer <rhyme label="b" type="m">slaef</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Met eerbiedigh <rhyme label="a" type="f" corresp="#s4:l1">groeten</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Vriendelijck en <rhyme label="b" type="m" corresp="#s4:l2">braef</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">Wilt mijn gunst niet <rhyme label="c" type="f">weeren</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Met een stuyr <rhyme label="d" type="m">ghesicht</rhyme>:</l>
        <l id="s4:l7" met="3" type="+-">Maer u tot my <rhyme label="c" type="f" corresp="#s4:l5">keeren</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Om voldoen u <rhyme label="d" type="m" corresp="#s4:l6">plicht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Eyndicht doch mijn <rhyme label="a" type="f">karmen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Laet my dus niet <rhyme label="b" type="m">staen</rhyme>:</l>
        <l id="s5:l3" met="3" type="+-">Opent bijde <rhyme label="a" type="f" corresp="#s5:l1">armen</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Om my te <rhyme label="b" type="m" corresp="#s5:l2">ontfaen</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Suycker-soete <rhyme label="c" type="f">Diefjen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Veynst u niet voor <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Want ghy zijt mijn <rhyme label="c" type="f" corresp="#s5:l5">Liefjen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">En ick weder <rhyme label="d" type="m" corresp="#s5:l6">dijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Sal't moghen <rhyme label="a" type="f">beuren</rhyme></l>
        <l id="s6:l2" met="3" type="+-">'tGheen ick van u <rhyme label="b" type="m">eys</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Soo staeck ick mijn <rhyme label="a" type="f" corresp="#s6:l1">treuren</rhyme>:</l>
        <l id="s6:l4" met="3" type="+-">En ick wil mijn <rhyme label="b" type="m" corresp="#s6:l2">vleys</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Iae mijn ziel daer <rhyme label="c" type="f">neven</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Tot een <rhyme label="d" type="m">Offerhandt</rhyme></l>
        <l id="s6:l7" met="3" type="+-">V ten dienste <rhyme label="c" type="f" corresp="#s6:l5">gheven</rhyme>:</l>
        <l id="s6:l8" met="3" type="+-">Siet daer is mijn <rhyme label="d" type="m" corresp="#s6:l6">Handt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
