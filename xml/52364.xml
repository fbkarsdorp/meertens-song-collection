    <date>
      <year-start>1813</year-start>
      <year-end>1813</year-end>
    </date>
    <location>
      <orginal-name>amsterdam (anjeliersgracht)</orginal-name>
      <standard-name>Anjeliersgracht, Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3774201</lon>
        <lat>4.8816913</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Treurd niet langer, gy ô <rhyme label="a" type="f">braven</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">U verlossing die is <rhyme label="b" type="m">dáár</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wy zyn niet langer Fransche <rhyme label="a" type="f" corresp="#s1:l1">slaven</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Wy zyn gered uit ons <rhyme label="b" type="m" corresp="#s1:l2">gevaar</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het is nu agttien Jaar <rhyme label="a" type="f">geleeden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dat men onder Frankryk <rhyme label="b" type="m">bukt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maar God geeft ons nu de <rhyme label="a" type="f" corresp="#s2:l1">Vreede</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En men word niet meer <rhyme label="b" type="m" corresp="#s2:l2">geplukt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Geen Oorlogszwaard zal men <rhyme label="a" type="f">verheffen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Nog Kanonnen of <rhyme label="b" type="m">Mortier</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Geen duure tyd zal ons meer <rhyme label="a" type="f" corresp="#s3:l1">treffen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ons gantsche land is vol <rhyme label="b" type="m" corresp="#s3:l2">plaizier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoezée! de Franschen zyn <rhyme label="a" type="f">vertrokken</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Met de Douanen hand aan <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Geen Prinsman is nu meer <rhyme label="a" type="f" corresp="#s4:l1">verschrokken</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">'t Is nu Oranje aan alle <rhyme label="b" type="m" corresp="#s4:l2">kant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Laat ons daarom vrolyk <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ter eeren die ons heeft <rhyme label="b" type="m">gered</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En in het rond eens vrolyk <rhyme label="a" type="f" corresp="#s5:l1">springen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Maar dat geen wraakzucht ons <rhyme label="b" type="m" corresp="#s5:l2">besmet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Laat Plunderzugt uw doel niet <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Waar door men niets dan onheil <rhyme label="b" type="m">voed</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dan hebt gy groote straf te <rhyme label="a" type="f" corresp="#s6:l1">vrezen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En 't niets tot uwe voordeel <rhyme label="b" type="m" corresp="#s6:l2">doet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Laat ons nu gehoorzaam <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Aan hem die thans ons Land <rhyme label="b" type="m">regeerd</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ja, dan behoeft men niet te <rhyme label="a" type="f" corresp="#s7:l1">vrezen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Oranjes vreugd word nog <rhyme label="b" type="m" corresp="#s7:l2">vermeerd</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Laat ons nog eens in 't rond gaan <rhyme label="a" type="f">springen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Ter eere van de lieve <rhyme label="b" type="m">Vrêe</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ons Liedje dat is uit <rhyme label="a" type="f" corresp="#s8:l1">gezongen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">'t Is al Oranje, en <rhyme label="b" type="m" corresp="#s8:l2">Hoezée</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
