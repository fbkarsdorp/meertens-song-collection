<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4260</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag>dialog song</tag>
      <tag>secular song</tag>
      <tag>love song</tag>
      <tag>arcostic</tag>
    </tags>
    <melody>Ik peins om een persoon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EErbaer beeldeken <rhyme label="a" type="f">soete</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Peerle gulden <rhyme label="b" type="m">ciraet</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Van herten ick u <rhyme label="a" type="f" corresp="#s1:l1">groete</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Mijn eenich <rhyme label="b" type="m" corresp="#s1:l2">toeverlaet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sonder u by zijn, is geen <rhyme label="c" type="m">vreught</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Sonder u by zijn is geen <rhyme label="c" type="m" corresp="#s1:l5">vreught</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Mijn hert u <rhyme label="d" type="m">mint</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Ghy zijt schoon <rhyme label="d" type="m" corresp="#s1:l7">kint</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Die my troost geuen <rhyme label="c" type="m" corresp="#s1:l5">meught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Laet varen sulck <rhyme label="a" type="f">beginnen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">V woorden zijn <rhyme label="b" type="m">geveynst</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Onder Venus <rhyme label="a" type="f" corresp="#s2:l1">goddinnen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ghy om een ander <rhyme label="b" type="m" corresp="#s2:l2">peynst</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Hier in neem ick een goet <rhyme label="c" type="m">verdrach</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Hier in neem ick een goet <rhyme label="c" type="m" corresp="#s2:l5">verdrach</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Vry elders <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Ken ben doch <rhyme label="d" type="m" corresp="#s2:l7">niet</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Die u troost geven <rhyme label="c" type="m" corresp="#s2:l5">mach</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">In geheel swerelts <rhyme label="a" type="f">ronde</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Zijt ghy mijn sins <rhyme label="b" type="m">verkies</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Dat ick u halen <rhyme label="a" type="f" corresp="#s3:l1">conde</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Als Iason dede <rhyme label="b" type="m" corresp="#s3:l2">t'vlies</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Voor geen dangier waer ick <rhyme label="c" type="m">versaeght</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Voor geen dangier waer ick <rhyme label="c" type="m" corresp="#s3:l5">versaeght</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Mijns herten <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Lijf ende <rhyme label="d" type="m" corresp="#s3:l7">goet</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Ware om u <rhyme label="c" type="m" corresp="#s3:l5">gewaecht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sout ghy zijn soo <rhyme label="a" type="f">volheerdich</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Voor my, arm simpel <rhyme label="b" type="m">maeght</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Ken ben de daet niet <rhyme label="a" type="f" corresp="#s4:l1">weerdich</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat ghy my liefde <rhyme label="b" type="m" corresp="#s4:l2">draeght</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Daer bouen, rijck, noch schoon <rhyme label="c" type="m">genouch</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Daer bouen, rijck, noch schoon <rhyme label="c" type="m" corresp="#s4:l5">genough</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">Stilt u <rhyme label="d" type="m">motijf</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Spaert goet, en <rhyme label="d" type="m" corresp="#s4:l7">lijf</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Ghy coemt my noch te <rhyme label="c" type="m" corresp="#s4:l5">vrough</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Al waerdy my <rhyme label="a" type="f">vyandich</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Als eenen Leew <rhyme label="b" type="m">rebel</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ick acht u soo <rhyme label="a" type="f" corresp="#s5:l1">verstandich</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ghy cont u sachten <rhyme label="b" type="m" corresp="#s5:l2">wel</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ten helpt dan leet, schimp, of <rhyme label="c" type="m">bedroch</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Ten helpt dan leet, schimp, of <rhyme label="c" type="m" corresp="#s5:l5">bedroch</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Ick sal <rhyme label="d" type="m">t'gevecht</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Met een <rhyme label="d" type="m" corresp="#s5:l7">oprecht</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Herte verwinnen <rhyme label="c" type="m" corresp="#s5:l5">noch</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Bewijst ghy sulcken <rhyme label="a" type="f">trouwe</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">In v dienstbaerheyt <rhyme label="b" type="m">swaer</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Can u verdriet, noch <rhyme label="a" type="f" corresp="#s6:l1">rouwe</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Niet maken <rhyme label="b" type="m" corresp="#s6:l2">wanckelbaer</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">T'sy hem dan, die u toont // <rhyme label="c" type="m">fenijn</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">T'sy hem dan, die u toont // <rhyme label="c" type="m" corresp="#s6:l5">fenijn</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Voorwaer ick <rhyme label="d" type="m">seght</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Alsucken <rhyme label="d" type="m" corresp="#s6:l7">knecht</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Behoort gecroont // te // <rhyme label="c" type="m" corresp="#s6:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Edel blomme <rhyme label="a" type="f">verheuen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Sulcx staet in uwer <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">My dooden, of doen <rhyme label="a" type="f" corresp="#s7:l1">leuen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Alleen hebt ghy de <rhyme label="b" type="m" corresp="#s7:l2">cracht</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Dus bid' ick dat ghy my <rhyme label="c" type="m">verschoont</rhyme>:</l>
        <l id="s7:l6" met="4" type="-+">Dus bid' ick dat ghy my <rhyme label="c" type="m" corresp="#s7:l5">verschoont</rhyme>:</l>
        <l id="s7:l7" met="2" type="-+">O soete <rhyme label="d" type="m">ieught</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Noyt geen <rhyme label="d" type="m" corresp="#s7:l7">ondeught</rhyme></l>
        <l id="s7:l9" met="3" type="-+">En heb ick u <rhyme label="c" type="m" corresp="#s7:l5">getoont</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Thoont ghy my deught veel <rhyme label="a" type="f">werfuen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Geen spot, oft schimp, meer <rhyme label="b" type="m">gelt</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Staet u leuen, en <rhyme label="a" type="f" corresp="#s8:l1">steruen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Alleen in mijn <rhyme label="b" type="m" corresp="#s8:l2">gewelt</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Wee mijns, naem ict int quade <rhyme label="c" type="m">deel</rhyme>:</l>
        <l id="s8:l6" met="4" type="-+">Wee mijns, naem ict int quade <rhyme label="c" type="m" corresp="#s8:l5">deel</rhyme>:</l>
        <l id="s8:l7" met="2" type="-+">Jonck heer <rhyme label="d" type="m">valiant</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">V hert <rhyme label="d" type="m" corresp="#s8:l7">constant</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Heeft my verwonnen <rhyme label="c" type="m" corresp="#s8:l5">heel</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Haters, vileyne <rhyme label="a" type="f">quade</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Vaert wech, met al v <rhyme label="b" type="m">doen</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Deur mijn liefkens <rhyme label="a" type="f" corresp="#s9:l1">genade</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Coem ick wt u <rhyme label="b" type="m" corresp="#s9:l2">prisoen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Drijft uwen spot niet meer met <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Drijft uwen spot niet meer met <rhyme label="c" type="m" corresp="#s9:l5">my</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Ick ben <rhyme label="d" type="m">bevrijt</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">Ja so <rhyme label="d" type="m" corresp="#s9:l7">verblijt</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Geen vreught en heefter <rhyme label="c" type="m" corresp="#s9:l5">by</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Coemt nu, treedt met my <rhyme label="a" type="f">binnen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ten leeft geen vromer <rhyme label="b" type="m">helt</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Noyt en was in mijn <rhyme label="a" type="f" corresp="#s10:l1">sinnen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Geen man soo vast <rhyme label="b" type="m" corresp="#s10:l2">gestelt</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Wech nijders, ghy hebt hier <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Wech nijders, ghy hebt hier <rhyme label="c" type="m" corresp="#s10:l5">gedaen</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Leet, en <rhyme label="d" type="m">getruer</rhyme>,</l>
        <l id="s10:l8" met="2" type="-+">Zoet, ende <rhyme label="d" type="m" corresp="#s10:l7">suer</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Wil ick (met u) <rhyme label="c" type="m" corresp="#s10:l5">aengaen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O minnaers <rhyme label="a" type="f">ongeluckich</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Die ongetroost noch <rhyme label="b" type="m">waert</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Ick (die van herten <rhyme label="a" type="f" corresp="#s11:l1">druckich</rhyme>)</l>
        <l id="s11:l4" met="3" type="-+">Was schier ter doot <rhyme label="b" type="m" corresp="#s11:l2">gemaect</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Ben nu vande liefste <rhyme label="c" type="m">begeert</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Ben nu vande liefste <rhyme label="c" type="m" corresp="#s11:l5">begeert</rhyme>,</l>
        <l id="s11:l7" met="2" type="-+">Al mijn <rhyme label="d" type="m">geclach</rhyme></l>
        <l id="s11:l8" met="2" type="-+">Is desen <rhyme label="d" type="m" corresp="#s11:l7">dach</rhyme></l>
        <l id="s11:l9" met="3" type="-+">In grooter vreught <rhyme label="c" type="m" corresp="#s11:l5">verkeert</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ramp, moet hem God nu <rhyme label="a" type="f">geuen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Die u bringt in <rhyme label="b" type="m">geween</rhyme>:</l>
        <l id="s12:l3" met="3" type="-+">Al dat my mach <rhyme label="a" type="f" corresp="#s12:l1">aencleuen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Maeck ick met u <rhyme label="b" type="m" corresp="#s12:l2">gemeen</rhyme>:</l>
        <l id="s12:l5" met="4" type="-+">Dus zijt hier mede wel <rhyme label="c" type="m">vernought</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Dus zijt hier mede wel <rhyme label="c" type="m" corresp="#s12:l5">vernought</rhyme>,</l>
        <l id="s12:l7" met="2" type="-+">Ick blijue <rhyme label="d" type="m">dy</rhyme>,</l>
        <l id="s12:l8" met="2" type="-+">S'gelijcx ghy <rhyme label="d" type="m" corresp="#s12:l7">my</rhyme>,</l>
        <l id="s12:l9" met="3" type="-+">God heeft het soo <rhyme label="c" type="m" corresp="#s12:l5">gevought</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Niet anders, O <rhyme label="a" type="f">Jongvrouwe</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Wensch ick van u, macht <rhyme label="b" type="m">zijn</rhyme>:</l>
        <l id="s13:l3" met="3" type="-+">Liefste, al waer van <rhyme label="a" type="f" corresp="#s13:l1">gouwe</rhyme></l>
        <l id="s13:l4" met="3" type="-+">De gansche weerelt <rhyme label="b" type="m" corresp="#s13:l2">mijn</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Ick gaeft u, als bereyden // <rhyme label="c" type="m">man</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Ick gaeft u, als bereyden // <rhyme label="c" type="m" corresp="#s13:l5">man</rhyme>,</l>
        <l id="s13:l7" met="2" type="-+">Alsoot <rhyme label="d" type="m">betaemt</rhyme>,</l>
        <l id="s13:l8" met="2" type="-+">Dat God <rhyme label="d" type="m" corresp="#s13:l7">versaemt</rhyme>,</l>
        <l id="s13:l9" met="3" type="-+">Geen mensche scheyden // <rhyme label="c" type="m" corresp="#s13:l5">can</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Edel Jongman van <rhyme label="a" type="f">prijse</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Ick moet u dancken <rhyme label="b" type="m">net</rhyme>:</l>
        <l id="s14:l3" met="3" type="-+">Dat ghy u (als de <rhyme label="a" type="f" corresp="#s14:l1">wijse</rhyme>)</l>
        <l id="s14:l4" met="3" type="-+">Tot mijnen dienste <rhyme label="b" type="m" corresp="#s14:l2">set</rhyme>:</l>
        <l id="s14:l5" met="4" type="-+">T'samen sullen wy leuen <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">T'samen sullen wy leuen <rhyme label="c" type="m" corresp="#s14:l5">voort</rhyme>,</l>
        <l id="s14:l7" met="2" type="-+">Sonder <rhyme label="d" type="m">gekijf</rhyme>,</l>
        <l id="s14:l8" met="2" type="-+">Als man, en <rhyme label="d" type="m" corresp="#s14:l7">wijf</rhyme>,</l>
        <l id="s14:l9" met="3" type="-+">Behooren, met <rhyme label="c" type="m" corresp="#s14:l5">accoort</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Liefste, alsulck <rhyme label="a" type="f">verhalen</rhyme>.</l>
        <l id="s15:l2" met="3" type="-+">Houd' ick voor zegel <rhyme label="b" type="m">vast</rhyme>:</l>
        <l id="s15:l3" met="3" type="-+">Wie can met goudt <rhyme label="a" type="f" corresp="#s15:l1">betalen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Dat ghy my hebt <rhyme label="b" type="m" corresp="#s15:l2">ontlast</rhyme>:</l>
        <l id="s15:l5" met="4" type="-+">En ontfangen in uwen <rhyme label="c" type="m">schoot</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">En ontfangen in uwen <rhyme label="c" type="m" corresp="#s15:l5">schoot</rhyme>,</l>
        <l id="s15:l7" met="2" type="-+">Vreest geen <rhyme label="d" type="m">misval</rhyme>,</l>
        <l id="s15:l8" met="2" type="-+">Niemant en <rhyme label="d" type="m" corresp="#s15:l7">sal</rhyme>,</l>
        <l id="s15:l9" met="3" type="-+">Ons scheyden, dan de <rhyme label="c" type="m" corresp="#s15:l5">doot</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Iaeght wech, oorspronck van <rhyme label="a" type="f">grieue</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Laet ons met vreughden <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Ten was noyt lief, by <rhyme label="a" type="f" corresp="#s16:l1">lieue</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Daer quam een scheyden <rhyme label="b" type="m" corresp="#s16:l2">aen</rhyme>:</l>
        <l id="s16:l5" met="4" type="-+">Adieu Prince, en blijft <rhyme label="c" type="m">gesont</rhyme>,</l>
        <l id="s16:l6" met="4" type="-+">Adieu Prince, en blijft <rhyme label="c" type="m" corresp="#s16:l5">gesont</rhyme>,</l>
        <l id="s16:l7" met="2" type="-+">Soot is <rhyme label="d" type="m">geseyt</rhyme>,</l>
        <l id="s16:l8" met="2" type="-+">Ben ick <rhyme label="d" type="m" corresp="#s16:l7">bereyt</rhyme>,</l>
        <l id="s16:l9" met="3" type="-+">Alst zijn sal tijt en <rhyme label="c" type="m" corresp="#s16:l5">stont</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Schoone ionge <rhyme label="a" type="f">Princesse</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Adieu, God sy met <rhyme label="b" type="m">u</rhyme>:</l>
        <l id="s17:l3" met="3" type="-+">Adieu, cloucke <rhyme label="a" type="f" corresp="#s17:l1">maitresse</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Adieu, wy scheyden <rhyme label="b" type="m" corresp="#s17:l2">nu</rhyme>,</l>
        <l id="s17:l5" met="4" type="-+">Adieu, segh ick, sonder <rhyme label="c" type="m">geween</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">Adieu, segh ick, sonder <rhyme label="c" type="m" corresp="#s17:l5">geween</rhyme>,</l>
        <l id="s17:l7" met="2" type="-+">Wy spreken <rhyme label="d" type="m">meer</rhyme>,</l>
        <l id="s17:l8" met="2" type="-+">Ter tijt <rhyme label="d" type="m" corresp="#s17:l7">wanneer</rhyme></l>
        <l id="s17:l9" met="3" type="-+">Wy weder zijn by <rhyme label="c" type="m" corresp="#s17:l5">een</rhyme>.</l>
