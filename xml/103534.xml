    <melody>Repicaban</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">ACh Rosette, voogdesse van mijn <rhyme label="a" type="m">Magt</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Wiens helle schoonheyt my geheel <rhyme label="a" type="m" corresp="#s1:l1">verkragt</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">En my gevangen dus in rampen <rhyme label="a" type="m" corresp="#s1:l1">bragt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ben ick Fenix der <rhyme label="b" type="f">Zonnen</rhyme></l>
        <l id="s1:l5" met="1" type="-+"><rhyme label="b" type="f" corresp="#s1:l4">Verwonnen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Door't minsaam <rhyme label="c" type="f">zwieren</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">van u flonken <rhyme label="d" type="m">oog</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Kan't minsaam <rhyme label="c" type="f" corresp="#s1:l6">zwieren</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Soo kragtich <rhyme label="c" type="f" corresp="#s1:l6">vieren</rhyme>?</l>
        <l id="s1:l10" met="3" type="+-">Daar ickt, Minsaam <rhyme label="d" type="m" corresp="#s1:l7">soogh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Deftig wesen, waar in Natuyr haar <rhyme label="a" type="m">praalt</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Volsiete treckjes, aardig <rhyme label="a" type="m" corresp="#s2:l1">afgemaalt</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Ent' waar Evoor, met rose-blos om <rhyme label="a" type="m" corresp="#s2:l1">haalt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Amber vloeyende <rhyme label="b" type="f">randjes</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">Wiens <rhyme label="b" type="f" corresp="#s2:l4">kantjes</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Vol weelden <rhyme label="c" type="f">stickten</rhyme></l>
        <l id="s2:l7" met="3" type="+-">schier mijn grage <rhyme label="d" type="m">vorst</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Als ick kom <rhyme label="c" type="f" corresp="#s2:l6">licken</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Mort ick schier <rhyme label="c" type="f" corresp="#s2:l6">slicken</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">In die Nectar <rhyme label="d" type="m" corresp="#s2:l7">dorst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Of de Goden mijn riepen in haar <rhyme label="a" type="m">Troon</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">En datmijn lief, daar niet en was te <rhyme label="a" type="m" corresp="#s3:l1">woon</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">K'enagten t'niet, alt' gene zy my <rhyme label="a" type="m" corresp="#s3:l1">boon</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">K'sou hat heersen <rhyme label="b" type="f">hemel</rhyme></l>
        <l id="s3:l5" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s3:l4">Gewemel</rhyme>
        </l>
        <l id="s3:l6" met="2" type="-+"><rhyme label="c" type="f">Ontvlieden</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">En my buygen int <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Want al mijn <rhyme label="c" type="f" corresp="#s3:l6">lusten</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Niet konnen <rhyme label="c" type="f" corresp="#s3:l6">rusten</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Voor zy mijn <rhyme label="d" type="m" corresp="#s3:l7">gebiet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
