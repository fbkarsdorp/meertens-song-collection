<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186322</id>
    <date>
      <year-start>1672</year-start>
      <year-end>1672</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pier is in de hemel</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gelijck de <rhyme label="a" type="f">Roose-knopjes</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wanneer den <rhyme label="b" type="m">dageraed</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Sijn Christallijne <rhyme label="a" type="f" corresp="#s1:l1">dropjes</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Op 't Aerdrijck dalen <rhyme label="b" type="m" corresp="#s1:l2">laet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Haer Bladeren <rhyme label="c" type="f">ontluycken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En vry wat soetlijck <rhyme label="c" type="f" corresp="#s1:l5">ruycken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Als Ambroos en <rhyme label="b" type="m" corresp="#s1:l2">Muscaet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">So is dan <rhyme label="a" type="f">Iuliane</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">De stant van mijn <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Mijn Zieltje smelt in <rhyme label="a" type="f" corresp="#s2:l1">Tranen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Als ick u derven <rhyme label="b" type="m" corresp="#s2:l2">moet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Mijn waerde Ziels <rhyme label="c" type="f">Vooghdesse</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Mijn boesem is vol <rhyme label="c" type="f" corresp="#s2:l5">bresse</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En blakert in een <rhyme label="b" type="m" corresp="#s2:l2">gloet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Van heete <rhyme label="a" type="f">minne-voncken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">U welbespraeckte <rhyme label="b" type="m">Tongh</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Verselt met soete <rhyme label="a" type="f" corresp="#s3:l1">loncken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">'t Was die mijn hert <rhyme label="b" type="m" corresp="#s3:l2">besprongh</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En door gewelt van <rhyme label="c" type="f">stralen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die ghy op mijn liet <rhyme label="c" type="f" corresp="#s3:l5">dalen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Tot uwe Liefde <rhyme label="b" type="m" corresp="#s3:l2">dwongh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">En laet ick soete <rhyme label="a" type="f">Susje</rhyme></l>
        <l id="s4:l2" met="3" type="-+">U cierlijck <rhyme label="b" type="m">Roose-bloos</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">eens drucken met een <rhyme label="a" type="f" corresp="#s4:l1">Kusje</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat Nectar en <rhyme label="b" type="m" corresp="#s4:l2">Ambroos</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">In soete <rhyme label="c" type="f">evenaren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ach mocht ick met u <rhyme label="c" type="f" corresp="#s4:l5">paren</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">In Cipres <rhyme label="b" type="m" corresp="#s4:l2">Minne-bloos</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dan soud ghy sien mijn <rhyme label="a" type="f">Engel</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hoe u Philander <rhyme label="b" type="m">mind</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Of kan die kleyne <rhyme label="a" type="f" corresp="#s5:l1">bengel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die 't alles <rhyme label="b" type="m" corresp="#s5:l2">overwind</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">U onversetbaer <rhyme label="c" type="f">sinnen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Niet dwingen om te <rhyme label="c" type="f" corresp="#s5:l5">minnen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">So is sijn roem maer <rhyme label="b" type="m" corresp="#s5:l2">wint</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Of meent ghy dat <rhyme label="a" type="f">Philander</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Met valsheyt om sou <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En loopen by een <rhyme label="a" type="f" corresp="#s6:l1">ander</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Als hy met <rhyme label="b" type="m" corresp="#s6:l2">Iuliaen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Sijn geyle lust geboet <rhyme label="c" type="f">had</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">U kuysheyt onder voet <rhyme label="c" type="f" corresp="#s6:l5">trad</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Verbant de snode <rhyme label="b" type="m" corresp="#s6:l2">waen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Eer sal de <rhyme label="a" type="f">Sonne-wagen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">In 't Oosten onder <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En in het Westen <rhyme label="a" type="f" corresp="#s7:l1">dagen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Eer ick van <rhyme label="b" type="m" corresp="#s7:l2">Iuliaen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Mijn Liefde af sal <rhyme label="c" type="f">keeren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En soo ick haer <rhyme label="c" type="f" corresp="#s7:l5">onteeren</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Moet het mijn qualijck <rhyme label="b" type="m" corresp="#s7:l2">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
