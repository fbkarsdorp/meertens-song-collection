  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Der dingen ewich scepper <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="4" type="-+">ghij die regeert nacht en <rhyme label="b" type="m">dach</rhyme></l>
        <l id="s1:l3" met="4" type="-+">oock set den tijt, sijn tijts <rhyme label="a" type="m" corresp="#s1:l1">geual</rhyme></l>
        <l id="s1:l4" met="4" type="-+">tot verlichten t'verdriets <rhyme label="b" type="m" corresp="#s1:l2">geclach</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den dach rooper, die geeft <rhyme label="a" type="m">geluijt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">nae hij den nacht heeft <rhyme label="b" type="m">ouerwaeckt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">der nachten licht, voor die gaen <rhyme label="a" type="m" corresp="#s2:l1">vuijt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">de nacht hij vande nacht <rhyme label="b" type="m" corresp="#s2:l2">afstaeckt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hier door de morgenster <rhyme label="a" type="m">ontlaet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">verdrijft de duijster vuijt die <rhyme label="b" type="m">lucht</rhyme></l>
        <l id="s3:l3" met="4" type="-+">alle dualing oeck nu wech <rhyme label="a" type="m" corresp="#s3:l1">gaet</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Toch al dat hindert, neemt die <rhyme label="b" type="m" corresp="#s3:l2">vlucht</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hier me neemt die scipper een <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">die wateren worden <rhyme label="b" type="m">gestilt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Der kercken steen, oock petrus <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme></l>
        <l id="s4:l4" met="4" type="-+">door dees wast aff sijn scults <rhyme label="b" type="m" corresp="#s4:l2">gescilt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dus laet ons al naerstich <rhyme label="a" type="m">opstaen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">den haen reept aen die slaepen <rhyme label="b" type="m">meer</rhyme></l>
        <l id="s5:l3" met="4" type="-+">hij straft den traegen doer <rhyme label="a" type="m" corresp="#s5:l1">vermaen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">berispt die mislaecken den <rhyme label="b" type="m" corresp="#s5:l2">heer</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als singt den haen, ons hoep <rhyme label="a" type="m">genaect</rhyme></l>
        <l id="s6:l2" met="4" type="-+">den crancken compt gesontheijt <rhyme label="b" type="m">blij</rhyme></l>
        <l id="s6:l3" met="4" type="-+">des moorders swaert dat wort <rhyme label="a" type="m" corresp="#s6:l1">geslaeckt</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Tgeloeff comt den versakers <rhyme label="b" type="m" corresp="#s6:l2">bij</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Jesv aensiet die leggen <rhyme label="a" type="m">naer</rhyme></l>
        <l id="s7:l2" met="4" type="-+">ons aensiende verbetert <rhyme label="b" type="m">me</rhyme></l>
        <l id="s7:l3" met="4" type="-+">als ghij aensiet soo staen wij <rhyme label="a" type="m" corresp="#s7:l1">heer</rhyme></l>
        <l id="s7:l4" met="4" type="-+">tgescreij wast aff der sonden <rhyme label="b" type="m" corresp="#s7:l2">we</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Bescijnt o licht ons sinnen <rhyme label="a" type="m">al</rhyme></l>
        <l id="s8:l2" met="4" type="-+">ons herten slaep gering <rhyme label="b" type="m">bedrijft</rhyme></l>
        <l id="s8:l3" met="4" type="-+">dijn eer laet spreecken ons <rhyme label="a" type="m" corresp="#s8:l1">geschal</rhyme></l>
        <l id="s8:l4" met="4" type="-+">voor al ons mont v loff <rhyme label="b" type="m" corresp="#s8:l2">beclijft</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Geeft glorij godt den vader <rhyme label="a" type="m">groet</rhyme></l>
        <l id="s9:l2" met="4" type="-+">oeck sijn enijgen soen <rhyme label="b" type="m">verbreijt</rhyme></l>
        <l id="s9:l3" met="4" type="-+">met den geest heijlich ende <rhyme label="a" type="m" corresp="#s9:l1">goet</rhyme></l>
        <l id="s9:l4" met="4" type="-+">van nw tot inder <rhyme label="b" type="m" corresp="#s9:l2">ewicheijt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
