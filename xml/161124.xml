    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Breek uw pijlen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Niets kan ware   liefden <rhyme label="a" type="f">stuyten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Niemand kan haar kragt <rhyme label="a" type="f" corresp="#s1:l1">besluyten</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Sy verwind het wreed <rhyme label="b" type="m">geweld</rhyme></l>
        <l id="s1:l4" met="4" type="+-">En behoud alom het <rhyme label="b" type="m" corresp="#s1:l3">veld</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Niemand kan de vrye <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Door geweld nog dwang <rhyme label="a" type="f" corresp="#s2:l1">verwinnen</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Hare drift is altoos <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van geweld en <rhyme label="b" type="m" corresp="#s2:l3">slaverny</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Het gevaar van wind en <rhyme label="a" type="f">baren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Kan Leander niet <rhyme label="a" type="f" corresp="#s3:l1">vervaren</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Hij vreesd storm, nog zee, nog <rhyme label="b" type="m">wind</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Tot hy sig by Hero <rhyme label="b" type="m" corresp="#s3:l3">vind</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Piramus, in min <rhyme label="a" type="f">ontsteken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Kan sijn Thisbe selve <rhyme label="a" type="f" corresp="#s4:l1">spreken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Schoon men hem voor <rhyme label="b" type="m">straffigheid</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Haar gesigt en min <rhyme label="b" type="m" corresp="#s4:l3">ontseyd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Twee aan een verknogte <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Vreesen geen gevaar nog <rhyme label="a" type="f" corresp="#s5:l1">smerten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Want de ware <rhyme label="b" type="m">minne-gloed</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Treed al 't onheyl met de <rhyme label="b" type="m" corresp="#s5:l3">voet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
