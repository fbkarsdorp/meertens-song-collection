<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>51960</id>
    <date>
      <year-start>1701</year-start>
      <year-end>1800</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>O lusteloze maagd</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn zoete Ziel <rhyme label="a" type="m">Vrindin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gy zyt die ik <rhyme label="a" type="m" corresp="#s1:l1">bemin</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Gy zyt een Teeren <rhyme label="b" type="m">Maagt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die myn Jonk Hert <rhyme label="c" type="m">behaagt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ik wenste wel dat de <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Myn dit gelukkig <rhyme label="c" type="m" corresp="#s1:l4">gaf</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat ik myn Phieles <rhyme label="e" type="m">Lief</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">By myn ontfangen <rhyme label="c" type="m" corresp="#s1:l4">mag</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ag minnaar staakt uw <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Want ik heb in uw geen <rhyme label="a" type="m" corresp="#s2:l1">zin</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Uw woorden die gy <rhyme label="b" type="m">spreekt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die zyn maar <rhyme label="c" type="m">Vynsery</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Gy zyt een minnaar <rhyme label="d" type="m">vals</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die door een zoet <rhyme label="c" type="m" corresp="#s2:l4">gevley</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Myn zoekt te <rhyme label="e" type="m">bekooren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dan was ik in de <rhyme label="c" type="m" corresp="#s2:l4">ley</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En 'k sweer uw Ziels <rhyme label="a" type="m">vriendin</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat ik uw trouw <rhyme label="a" type="m" corresp="#s3:l1">bemin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Uw woorden die gy <rhyme label="b" type="m">spreekt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Van Valsheyd en <rhyme label="c" type="m">Bedrog</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die hebben myn <rhyme label="d" type="m">Jonkhert</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Tot Treurens <rhyme label="c" type="m" corresp="#s3:l4">toegebrogt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Myn Liefste Lief weest maar <rhyme label="e" type="m">gerust</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ik heb dat nooyt <rhyme label="c" type="m" corresp="#s3:l4">gedagt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ag Liefste Lief toon <rhyme label="a" type="m">gena</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Eer ik door min <rhyme label="a" type="m" corresp="#s4:l1">vergaa</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zoo laat uw minnaar <rhyme label="b" type="m">dog</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Genaade by uw <rhyme label="c" type="m">ontstaan</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Toont dog eens weeder <rhyme label="d" type="m">min</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Myn Aldersoetste <rhyme label="c" type="m" corresp="#s4:l4">lief</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Gy zyt die <rhyme label="e" type="m">geen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die myn Jonkhert <rhyme label="c" type="m" corresp="#s4:l4">belieft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
