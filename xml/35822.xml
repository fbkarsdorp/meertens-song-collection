<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35822</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOdt Hemelsche Vader! met uwe <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ey! wilt ons <rhyme label="b" type="f">ontfermen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Godt Zone! Met u wijsheyt dagh en <rhyme label="a" type="m" corresp="#s1:l1">nacht</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt ons trouw <rhyme label="b" type="f" corresp="#s1:l2">beschermen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ontfermt ons, vertrooster heyligen <rhyme label="c" type="m">Geest</rhyme>!</l>
        <l id="s1:l6" met="4" type="-+">En Godt drievuldigh ons al <rhyme label="c" type="m" corresp="#s1:l5">geneest</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Soete Maria! ontfange <rhyme label="a" type="m">net</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Bidt voor ons <rhyme label="b" type="f">Sondaren</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">O Maeght gebooren sonder zondt, en <rhyme label="a" type="m" corresp="#s2:l1">smet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Bidt voor dese <rhyme label="b" type="f" corresp="#s2:l2">scharen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Want d' Heere is met u, soo <rhyme label="c" type="m">Gabriel</rhyme></l>
        <l id="s2:l6" met="4" type="-+">U heeft geboodtschapt, na Goods <rhyme label="c" type="m" corresp="#s2:l5">bevel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O Alderheylighste! naest Godt <rhyme label="a" type="m">geacht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Bidt voor ons u <rhyme label="b" type="f">Zoone</rhyme></l>
        <l id="s3:l3" met="4" type="-+">O Verpletster van Satans hooft en <rhyme label="a" type="m" corresp="#s3:l1">kracht</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">Bidt sonder <rhyme label="b" type="f" corresp="#s3:l2">verschoone</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Weest gegroet, vol gracy <rhyme label="c" type="m">gebenedijt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Van al Satans lagen, ons <rhyme label="c" type="m" corresp="#s3:l5">bevrijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Moeder van de Salighmaker! en <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Bidt sonder <rhyme label="b" type="f">vertoeven</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Naest Godt komt u toe d'hooghste lof en <rhyme label="a" type="m" corresp="#s4:l1">eer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Laet ons u hulp <rhyme label="b" type="f" corresp="#s4:l2">proeven</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Versoent ons aen onse Schepper; u <rhyme label="c" type="m">Kindt</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Als hy ons wil slaen, sijn handen <rhyme label="c" type="m" corresp="#s4:l5">bindt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Morge-sterre! na u langh <rhyme label="a" type="m">verwacht</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Bidt voor ons, o <rhyme label="b" type="f">Moeder</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">Zaligh genoemt, van al 't goede <rhyme label="a" type="m" corresp="#s5:l1">geslacht</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Krijght van de <rhyme label="b" type="f" corresp="#s5:l2">behoeder</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Dat 't waerachtigh licht, van d'aerde <rhyme label="c" type="m">verdrijft</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Dat Ketters blindtheyt en boosheyt <rhyme label="c" type="m" corresp="#s5:l5">stijft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O Stadt Goods! met al de deughden <rhyme label="a" type="m">vervult</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Bidt voor kleyn als <rhyme label="b" type="f">grooten</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">O Stoel des wijsheydts! sonder sondt en <rhyme label="a" type="m" corresp="#s6:l1">schuldt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wilt ons niet <rhyme label="b" type="f" corresp="#s6:l2">verstooten</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Verkeerster in 't goet Evaes naem, end' <rhyme label="c" type="m">quaedt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Weest ons' hoop naest Godt; ons noyt <rhyme label="c" type="m" corresp="#s6:l5">verlaet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O Gedeons vlies, vol Hemelsch <rhyme label="a" type="m">douw</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">Bidt voor Klerck als <rhyme label="b" type="f">Leecken</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Middelaresse by u Zoone <rhyme label="a" type="m" corresp="#s7:l1">trouw</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">Krijght door u <rhyme label="b" type="f" corresp="#s7:l2">voorspreecken</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Dat Godt en Al in ons hart rust <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Dat w' hem beminnen met ziel en <rhyme label="c" type="m" corresp="#s7:l5">le'en</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O Arck des verbondts! en Salomons <rhyme label="a" type="m">throon</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">Stort voor ons <rhyme label="b" type="f">Gebeden</rhyme></l>
        <l id="s8:l3" met="4" type="-+">O Maeghden cieraet! der heyligen <rhyme label="a" type="m" corresp="#s8:l1">kroon</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Verkrijcht voor ons <rhyme label="b" type="f" corresp="#s8:l2">heden</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Dat d' Hemelsche Bruyd'gom, ons alle <rhyme label="c" type="m">geeft</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Dat yder na u exempel <rhyme label="c" type="m" corresp="#s8:l5">leeft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Zee-sterre! held're Zon, volle <rhyme label="a" type="m">Maen</rhyme>!</l>
        <l id="s9:l2" met="3" type="-+">Laet ons u hulp <rhyme label="b" type="f">smaken</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Hemelsche ladder! die Iacob sach <rhyme label="a" type="m" corresp="#s9:l1">staen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Krijght door u <rhyme label="b" type="f" corresp="#s9:l2">voorspraken</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Dat Goods in sprake, handt, en wil ons <rhyme label="c" type="m">leyt</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Dat niemandt daer tegen doet, noch <rhyme label="c" type="m" corresp="#s9:l5">seyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O Brandent licht! ende goude <rhyme label="a" type="m">autaer</rhyme>!</l>
        <l id="s10:l2" met="3" type="-+">Bidt voor ons de <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Oorsaeck van ons Blijdtschap, en Kercx <rhyme label="a" type="m" corresp="#s10:l1">Pylaer</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Krijght na ons <rhyme label="b" type="f" corresp="#s10:l2">begeere</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Dat u eenige vrucht, ons zoo <rhyme label="c" type="m">versaet</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Dat yder 't doodelijck Aes <rhyme label="c" type="m" corresp="#s10:l5">versmaet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Zondare toevlucht! bekleedt met de <rhyme label="a" type="m">Zon</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Bidt voor ons u <rhyme label="b" type="f">Zoone</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">O Moeder! die Jesum in doecken <rhyme label="a" type="m" corresp="#s11:l1">bon'</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Bidt zonder <rhyme label="b" type="f" corresp="#s11:l2">verschoone</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Als Abigel u Zoons gramschappen <rhyme label="c" type="m">slist</rhyme>.</l>
        <l id="s11:l6" met="4" type="-+">Beminster van Peys; neemt wegh de <rhyme label="c" type="m" corresp="#s11:l5">twist</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">O schoonste pronck van d'Aerd', end' <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Bidt voor dese <rhyme label="b" type="f">Scharen</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Geen Menschen, Noch Engel zijn u <rhyme label="a" type="m" corresp="#s12:l1">gelijck</rhyme>.</l>
        <l id="s12:l4" met="3" type="-+">Bidt voor ons <rhyme label="b" type="f" corresp="#s12:l2">Sondaren</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Ende toont dat ghy onse Moeder <rhyme label="c" type="m">zijt</rhyme>.</l>
        <l id="s12:l6" met="4" type="-+">Poort des Hemels ons alle <rhyme label="c" type="m" corresp="#s12:l5">verblijt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">O waerste Parel! Goods verborge <rhyme label="a" type="m">schat</rhyme>!</l>
        <l id="s13:l2" met="3" type="-+">Bidt voor Klerck als <rhyme label="b" type="f">Leecken</rhyme>.</l>
        <l id="s13:l3" met="4" type="-+">Tempel des Heyligen Geest, Godes <rhyme label="a" type="m" corresp="#s13:l1">stadt</rhyme>!</l>
        <l id="s13:l4" met="3" type="-+">Ey! voor ons wilt <rhyme label="b" type="f" corresp="#s13:l2">spreecken</rhyme>.</l>
        <l id="s13:l5" met="4" type="-+">Blijft by onse Zondaers tot d' uur des <rhyme label="c" type="m">doots</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Versoent, ende toont ons 't Lamke <rhyme label="c" type="m" corresp="#s13:l5">Goods</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Koninginne van d' Eng'len <rhyme label="a" type="m">allegaar</rhyme>!</l>
        <l id="s14:l2" met="3" type="-+">Bidt voor ons <rhyme label="b" type="f">Sondaren</rhyme></l>
        <l id="s14:l3" met="4" type="-+">Keyserinne van al d' heyligen <rhyme label="a" type="m" corresp="#s14:l1">schaer</rhyme>!</l>
        <l id="s14:l4" met="3" type="-+">Bidt voor dese <rhyme label="b" type="f" corresp="#s14:l2">Scharen</rhyme></l>
        <l id="s14:l5" met="4" type="-+">U Ziel met u Lichaem tot 's Hemels <rhyme label="c" type="m">Throon</rhyme></l>
        <l id="s14:l6" met="4" type="-+">Is opgeklommen met Lof-sangh <rhyme label="c" type="m" corresp="#s14:l5">schoon</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">U Hooft is met twalif Starre <rhyme label="a" type="m">gekroont</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Bidt sonder <rhyme label="b" type="f">vertoeven</rhyme></l>
        <l id="s15:l3" met="4" type="-+">Boven al d' Heyligen zijt ghy <rhyme label="a" type="m" corresp="#s15:l1">geloont</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Laet ons u vreucht <rhyme label="b" type="f" corresp="#s15:l2">proeven</rhyme>.</l>
        <l id="s15:l5" met="4" type="-+">Dienstmaeght des Heere voor u 'k neder <rhyme label="c" type="m">kniel</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Ghy zijt, die 'k bemin met lijf en <rhyme label="c" type="m" corresp="#s15:l5">Ziel</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">O Lam Gods! die 's werelts Zonden neemt <rhyme label="a" type="m">af</rhyme>.</l>
        <l id="s16:l2" met="3" type="-+">Spaert ons doch o <rhyme label="b" type="f">Heere</rhyme>!</l>
        <l id="s16:l3" met="4" type="-+">Lam Godts die wegh neemt 's Werelts zondigh <rhyme label="a" type="m" corresp="#s16:l1">kaf</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Hoort onse <rhyme label="b" type="f" corresp="#s16:l2">begeere</rhyme>.</l>
        <l id="s16:l5" met="4" type="-+">Lam Goods, die wegh neemt al 's Werelts <rhyme label="c" type="m">fenijn</rhyme></l>
        <l id="s16:l6" met="4" type="-+">Wilt ons ontfermen, O <rhyme label="c" type="m" corresp="#s16:l5">Medecijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
