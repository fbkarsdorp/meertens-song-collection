  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zielen wilt gy eeuwig <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">En op Eng'len schachten <rhyme label="a" type="f" corresp="#s1:l1">zweeven</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Voedt U dan met <rhyme label="b" type="m">Hemelspys</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Uit het eeuwig <rhyme label="b" type="m" corresp="#s1:l3">Paradys</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Deze spys doet nimmer <rhyme label="a" type="f">sterren</rhyme>.</l>
        <l id="s2:l2" met="4" type="+-">Deze spys zal nooit <rhyme label="a" type="f" corresp="#s2:l1">bederven</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Dit is spys, die elk <rhyme label="b" type="m">bekoort</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Jezus schaft ze ons in zyn <rhyme label="b" type="m" corresp="#s2:l3">woordt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Komt dan, hongerige <rhyme label="a" type="f">zielen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Komt by uwen Heilland <rhyme label="a" type="f" corresp="#s3:l1">knielen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Want hy weigert <rhyme label="b" type="m">nimmermeer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Komt by dezen milden <rhyme label="b" type="m" corresp="#s3:l3">Heer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
