  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hey lustig braave <rhyme label="a" type="m">maats</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Maak nu voor een roemer <rhyme label="a" type="m" corresp="#s1:l1">plaats</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Ghy Amsterdammers <rhyme label="b" type="m">trouw</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">d' Haarlemmers tarten <rhyme label="b" type="m" corresp="#s1:l3">jouw</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Wie is dien <rhyme label="c" type="m">heldt</rhyme>?</l>
        <l id="s1:l6" met="2" type="-+">Hy komt te <rhyme label="c" type="m" corresp="#s1:l5">veld</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hy komt, hy <rhyme label="d" type="m">komt</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">Hy komt te <rhyme label="c" type="m" corresp="#s1:l5">veld</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">En laat hem vry <rhyme label="e" type="f">verschijne</rhyme>:</l>
        <l id="s1:l10" met="3" type="+-">Want hier is een <rhyme label="f" type="m">klerk</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Die en maakt geen <rhyme label="f" type="m" corresp="#s1:l10">werk</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Van borden vol <rhyme label="e" type="f" corresp="#s1:l9">douzijne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Kom nu dan voor den <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Stoffers van de natte <rhyme label="a" type="m" corresp="#s2:l1">vlag</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En toont wie datje <rhyme label="b" type="m">bent</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Eerje braave baazen <rhyme label="b" type="m" corresp="#s2:l3">schent</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Zie daar een <rhyme label="c" type="m">fluit</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Die driemaal <rhyme label="c" type="m" corresp="#s2:l5">uit</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Zie daar, zie <rhyme label="d" type="m">daar</rhyme>,</l>
        <l id="s2:l8" met="1" type="-+">Zie daar een <rhyme label="c" type="m" corresp="#s2:l5">fluit</rhyme>;</l>
        <l id="s2:l9" met="3" type="+-">Wel waar is nu dat <rhyme label="e" type="f">baasje</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Die mijn bescheit doen <rhyme label="f" type="m">zal</rhyme>?</l>
        <l id="s2:l11" met="3" type="+-">Noch dubbel ik 't <rhyme label="f" type="m" corresp="#s2:l10">getal</rhyme>:</l>
        <l id="s2:l12" met="3" type="-+">'t En steekt my op geen <rhyme label="e" type="f" corresp="#s2:l9">glaasje</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hey man je bent het <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Houw vry op, eer datje <rhyme label="a" type="m" corresp="#s3:l1">zwiet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Komt 'er geen aar te <rhyme label="b" type="m">baan</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Zo voert â€¦. noch de <rhyme label="b" type="m" corresp="#s3:l3">baan</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Slik in, slik <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dat druive <rhyme label="c" type="m" corresp="#s3:l5">zop</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Slik in, slik <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s3:l8" met="1" type="-+">Slik in, slik <rhyme label="c" type="m" corresp="#s3:l5">op</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Uyt roemers en uit <rhyme label="e" type="f">schaalen</rhyme>.</l>
        <l id="s3:l10" met="3" type="+-">Wy hebben nieuwe <rhyme label="f" type="m">stof</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">En moogen weer met <rhyme label="f" type="m" corresp="#s3:l10">lof</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Als Bacchus helden <rhyme label="e" type="f" corresp="#s3:l9">praalen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
