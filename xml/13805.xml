      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Boksvoetje</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WEest vrolijck mijn beesjes en toont u <rhyme label="a" type="m">verheught</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Lerinde die ‘k <rhyme label="b" type="f">minde</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">O wat een <rhyme label="a" type="m" corresp="#s1:l1">gheneucht</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Sal zijn mijn <rhyme label="c" type="m">Vooghdes</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En u <rhyme label="c" type="m" corresp="#s1:l4">Herderes</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Laet blijcken als ick u groote <rhyme label="a" type="m" corresp="#s1:l1">vreucht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ey Christalle stroompjes u golfjes laet <rhyme label="a" type="m">sien</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Tot tuygen, van ‘t <rhyme label="b" type="f">juygen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">‘t Geen ghy haer kondt <rhyme label="a" type="m" corresp="#s2:l1">bien</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Springht op, en dan <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Veel meer als wel <rhyme label="c" type="m" corresp="#s2:l4">eer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Waer aen ick u vreughde sal konnen <rhyme label="a" type="m" corresp="#s2:l1">sien</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En ghy Echo die mijn klachjes wel <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Vaeck hoorde, wiens <rhyme label="b" type="f">woorde</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Ghy naebootse <rhyme label="a" type="m" corresp="#s3:l1">weer</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ey schatert door ‘t <rhyme label="c" type="m">Wout</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Dat Silvia <rhyme label="c" type="m" corresp="#s3:l4">trouwt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Met haer Amintas op mijn <rhyme label="a" type="m" corresp="#s3:l1">begeer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ey boompjes! u blaetjes van vreugde eens <rhyme label="a" type="m">drilt</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Wilt swieren, en <rhyme label="b" type="f">gieren</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">V lovertjes <rhyme label="a" type="m" corresp="#s4:l1">spilt</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Op dat mijn <rhyme label="c" type="m">Lerind</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Een rust-plaets <rhyme label="c" type="m" corresp="#s4:l4">vindt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Waer noch mijn klachjes met onrust <rhyme label="a" type="m" corresp="#s4:l1">gilt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Verheught u Najade voegh u aen den <rhyme label="a" type="m">dans</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Wilt vlechten, en <rhyme label="b" type="f">hechten</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Lerinda een <rhyme label="a" type="m" corresp="#s5:l1">krans</rhyme>:</l>
        <l id="s5:l4" met="2" type="-+">Wiens bloempjes en <rhyme label="c" type="m">kruyt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Op veer munten <rhyme label="c" type="m" corresp="#s5:l4">uyt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Het groen van Pallas ghecierde <rhyme label="a" type="m" corresp="#s5:l1">lans</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Haest u Amintas de tijdt die <rhyme label="a" type="m">ghenaeckt</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Dat lusjes, door <rhyme label="b" type="f">kusjes</rhyme></l>
        <l id="s6:l3" met="2" type="-+">‘t Ghenieten <rhyme label="a" type="m" corresp="#s6:l1">smaeckt</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Lerind haer <rhyme label="c" type="m">bedroeft</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Om dat ghy <rhyme label="c" type="m" corresp="#s6:l4">vertoeft</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">Wiens hertje als d’ uw’ in liefde <rhyme label="a" type="m" corresp="#s6:l1">blaeckt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
