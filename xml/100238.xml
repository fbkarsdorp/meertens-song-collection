<song>
  <meta>
    <id>100238</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1760</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wees er gegroet schone herderin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IK mag wel zeggen dat onz' <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s1:l2" met="4" type="-+">De hardste Rots in hartheid <rhyme label="a" type="f" corresp="#s1:l1">terten</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Vermits myn uwe trotsheid tegen <rhyme label="b" type="m">staat</rhyme></l>
        <l id="s1:l4" met="5" type="+-">En het uwe zig niet meuken <rhyme label="b" type="m" corresp="#s1:l3">laat</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Door liefde, tranen of <rhyme label="a" type="f" corresp="#s1:l1">smerten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En neem ik dan myn oogmerk <rhyme label="a" type="f">meden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dat dit moet eeuwig zyn <rhyme label="a" type="f" corresp="#s2:l1">geleden</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Zoo noeme ik dan my, het gedient <rhyme label="b" type="m">gezeid</rhyme>,</l>
        <l id="s2:l4" met="5" type="+-">Eene Rotze van <rhyme label="b" type="m" corresp="#s2:l3">stantvastigheid</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Daar gy 't zyt van <rhyme label="a" type="f" corresp="#s2:l1">wredigheden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
