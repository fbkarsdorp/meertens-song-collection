  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Sie daar nou men <rhyme label="a" type="f">maijsje</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Nou staat et an <rhyme label="b" type="m">jou</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Bedenk jou ien <rhyme label="a" type="f" corresp="#s1:l1">raijsje</rhyme></l>
        <l id="s1:l4" met="3" type="+-">En aanvaard men <rhyme label="b" type="m" corresp="#s1:l2">trouw</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Ik selse jou <rhyme label="c" type="f">geven</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Mit hart in mit <rhyme label="d" type="m">sin</rhyme></l>
        <l id="s1:l7" met="3" type="+-">En steeds mit jou <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme></l>
        <l id="s1:l8" met="3" type="+-">In ien ware <rhyme label="d" type="m" corresp="#s1:l6">min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wel as dan jou <rhyme label="a" type="f">sinne</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Zijn tot men <rhyme label="b" type="m">gezind</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Sel ik jou <rhyme label="a" type="f" corresp="#s2:l1">beminnen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Als ien willig <rhyme label="b" type="m" corresp="#s2:l2">kind</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Om dat et <rhyme label="c" type="f">begeeren</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Van mortjen et <rhyme label="d" type="m">wou</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Sel ik et niet <rhyme label="c" type="f" corresp="#s2:l5">weren</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Maar ontfang jou <rhyme label="d" type="m" corresp="#s2:l6">trouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
