  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Och edel ziele <rhyme label="a" type="f">anscouwet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">wat v mynne heuet <rhyme label="b" type="m">gedaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">sich hoe ic stae <rhyme label="a" type="f" corresp="#s1:l1">bedouwe</rhyme></l>
        <l id="s1:l4" met="3" type="-+">mit wonden al <rhyme label="b" type="m" corresp="#s1:l2">ontdaen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">myn bloet hebbe ic gestortet <rhyme label="c" type="m">wt</rhyme></l>
        <l id="s1:l6" met="6" type="-+">op dat ic v mocht hebben o ziele in eenre <rhyme label="c" type="m" corresp="#s1:l5">bruut</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ic heb om v <rhyme label="a" type="f">gestreden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">wal dertich <rhyme label="b" type="m">iaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">ende heb den doot <rhyme label="a" type="f" corresp="#s2:l1">geleden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">anden cruce ynt <rhyme label="b" type="m" corresp="#s2:l2">openbaer</rhyme></l>
        <l id="s2:l5" met="4" type="-+">ic en kan v o ziele laten <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s2:l6" met="6" type="-+">wilt dit gedencken als gi hebt swaer <rhyme label="c" type="m" corresp="#s2:l5">verdriet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Â¶ Geuet my in <rhyme label="a" type="f">allen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">dat v liden ende <rhyme label="b" type="m">smerte</rhyme></l>
        <l id="s3:l3" met="3" type="-+">mit uwen roden <rhyme label="a" type="f" corresp="#s3:l1">wonden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">geprentet staen in myn <rhyme label="b" type="m" corresp="#s3:l2">harte</rhyme></l>
        <l id="s3:l5" met="4" type="-+">ende ic v den wech moet volgen <rhyme label="c" type="m">nae</rhyme></l>
        <l id="s3:l6" met="6" type="-+">den gi myn lief van mynnen hier syt <rhyme label="c" type="m" corresp="#s3:l5">voer|gegaen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In allen dinen <rhyme label="a" type="f">liden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">gedencket myn swaer <rhyme label="b" type="m">pyn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">in gaet doet mynre <rhyme label="a" type="f" corresp="#s4:l1">syden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">ende ruest in dat herte <rhyme label="b" type="m" corresp="#s4:l2">myn</rhyme></l>
        <l id="s4:l5" met="4" type="-+">ende wilt ene corte wyl liden om <rhyme label="c" type="m">my</rhyme></l>
        <l id="s4:l6" met="6" type="-+">du salste tot ewigen tijden o ziele verbliden  mit <rhyme label="c" type="m" corresp="#s4:l5">my</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O lief bouen al <rhyme label="a" type="f">vercaren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">o alre getrouweste <rhyme label="b" type="m">vrient</rhyme></l>
        <l id="s5:l3" met="3" type="-+">om uwer swaere <rhyme label="a" type="f" corresp="#s5:l1">pinen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">die gi al <rhyme label="b" type="m" corresp="#s5:l2">onuerdient</rhyme></l>
        <l id="s5:l5" met="4" type="-+">van mynnen woldeste liden om <rhyme label="c" type="m">my</rhyme></l>
        <l id="s5:l6" met="6" type="-+">geuet my in allen liden te verbliden o lief om <rhyme label="c" type="m" corresp="#s5:l5">di</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mijn lief dat vercaren <rhyme label="a" type="f">haen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">dat is soe scoen <rhyme label="b" type="m">gedaen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">dat sonne sternen ende <rhyme label="a" type="f" corresp="#s6:l1">maen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">des alle wonder <rhyme label="b" type="m" corresp="#s6:l2">haen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">wt dusenden vercaren blanck wit ende <rhyme label="c" type="m">roet</rhyme></l>
        <l id="s6:l6" met="6" type="-+">hi draget op syn hoeuet enen doernen rosen <rhyme label="c" type="m" corresp="#s6:l5">hoet</rhyme></l>
      </lg>
    </lg>
  </text>
