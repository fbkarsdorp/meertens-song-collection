  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ghelijck als die witte <rhyme label="a" type="m">Swaen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Sterft en singht een droevich <rhyme label="b" type="m">liet</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Soo moet ick eylaes <rhyme label="a" type="m" corresp="#s1:l1">voortaen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Singhen dit in myn <rhyme label="b" type="m" corresp="#s1:l2">verdriet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">O felle doot wilt my niet <rhyme label="c" type="f">sparen</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Deur straelt <rhyme label="d" type="m">myn</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">soo sal ick bevrijt <rhyme label="e" type="m">eenpaer</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Sijn van <rhyme label="d" type="m" corresp="#s1:l6">pijn</rhyme>.</l>
