    <melody>Branle de la reine</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Susanna siet toch <rhyme label="a" type="m">om</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En laat u eens <rhyme label="b" type="f">versachten</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Door clagten, vol <rhyme label="b" type="f" corresp="#s1:l2">cragten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van hun die soetelen <rhyme label="a" type="m" corresp="#s1:l1">om</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Te plucken uwe <rhyme label="a" type="m" corresp="#s1:l1">blom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een roos diemen niet <rhyme label="a" type="m">pluct</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Verwelckt end' valt by <rhyme label="b" type="f">syen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Laat dyen, het <rhyme label="b" type="f" corresp="#s2:l2">vryen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Terwyle men voor u <rhyme label="a" type="m" corresp="#s2:l1">buct</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Lanck vryen selden <rhyme label="a" type="m" corresp="#s2:l1">luct</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Den tijt sal u <rhyme label="a" type="m">ontgaen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Als zijt ghy ras te <rhyme label="b" type="f">voete</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Mijn soette, <rhyme label="b" type="f" corresp="#s3:l2">ghegroete</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Oeffent ter wyle ghy <rhyme label="a" type="m" corresp="#s3:l1">meugt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">t' Bloeysel van uwe <rhyme label="a" type="m" corresp="#s3:l1">jeugt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">V schoon,heyt u <rhyme label="a" type="m">beval</rhyme></l>
        <l id="s4:l2" met="3" type="-+">V minlijckheyt, u <rhyme label="b" type="f">leeden</rhyme></l>
        <l id="s4:l3" met="2" type="-+">V reeden, end' <rhyme label="b" type="f" corresp="#s4:l2">seeden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Oock tspreken <rhyme label="a" type="m" corresp="#s4:l1">liefghetal</rhyme></l>
        <l id="s4:l5" met="3" type="-+">V daar toe porren <rhyme label="a" type="m" corresp="#s4:l1">al</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">t' Waar jammer dat een <rhyme label="a" type="m">beelt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">d' Welck soo veel schoonheyts <rhyme label="b" type="f">deelen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Wt veelen, <rhyme label="b" type="f" corresp="#s5:l2">vereelen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Jn wiens ooch tliefdeken <rhyme label="a" type="m" corresp="#s5:l1">speelt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Soud' blyven <rhyme label="a" type="m" corresp="#s5:l1">onghestreelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Goet zijt ghy en hebt <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Kiest een naar uwen <rhyme label="b" type="f">sinne</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Goddinne, de <rhyme label="b" type="f" corresp="#s6:l2">minne</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Eyst (oft spel is <rhyme label="a" type="m" corresp="#s6:l1">onsoet</rhyme>)</l>
        <l id="s6:l5" met="3" type="-+">Dat lief by lief sijn <rhyme label="a" type="m" corresp="#s6:l1">moet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wat wilt ghy langh <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Sonder ws liefs <rhyme label="b" type="f">vermaken</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Jnt laken, <rhyme label="b" type="f" corresp="#s7:l2">ontwaken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">End' grypen oock naar het <rhyme label="a" type="m" corresp="#s7:l1">gheen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Daar den droom vliegt met <rhyme label="a" type="m" corresp="#s7:l1">heen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En vreest ghy niet ter <rhyme label="a" type="m">noot</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat Iupiter vol <rhyme label="b" type="f">looven</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Van booven, <rhyme label="b" type="f" corresp="#s8:l2">gheschooven</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Met een gout reghen <rhyme label="a" type="m" corresp="#s8:l1">bloot</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Mogt wentelen in u <rhyme label="a" type="m" corresp="#s8:l1">schoot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Want hy siet noy <rhyme label="a" type="m">tsaisoen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat maagdekens lanck <rhyme label="b" type="f">souwen</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Van trouwen, <rhyme label="b" type="f" corresp="#s9:l2">onthouwen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Terwyle de jeugt is <rhyme label="a" type="m" corresp="#s9:l1">groen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Wat soudmen anders <rhyme label="a" type="m" corresp="#s9:l1">doen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Denckt dat het blanck <rhyme label="a" type="m">coleur</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Vergeelt end' schiet sijn <rhyme label="b" type="f">verwe</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Conserve,, noch <rhyme label="b" type="f" corresp="#s10:l2">Tarwe</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Baten, des tijts <rhyme label="a" type="m" corresp="#s10:l1">betreur</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Gaat met de blomme <rhyme label="a" type="m" corresp="#s10:l1">deur</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De slang tgheluck <rhyme label="a" type="m">gheniet</rhyme></l>
        <l id="s11:l2" met="3" type="-+">d'Out vel, jaarlijcx voor <rhyme label="b" type="f">ooghen</rhyme></l>
        <l id="s11:l3" met="2" type="-+">Te moghen, <rhyme label="b" type="f" corresp="#s11:l2">wttooghen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Dan sulcx tgheen my <rhyme label="a" type="m" corresp="#s11:l1">verdriet</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Sal u gebeuren <rhyme label="a" type="m" corresp="#s11:l1">niet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Susanna comt dan <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s12:l2" met="3" type="-+">By d' amoureuse <rhyme label="b" type="f">scharen</rhyme></l>
        <l id="s12:l3" met="2" type="-+">Te paren, u <rhyme label="b" type="f" corresp="#s12:l2">jaaren</rhyme></l>
        <l id="s12:l4" met="3" type="-+">End' maackt u tijd'lijck <rhyme label="a" type="m" corresp="#s12:l1">ree</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Want schoonheyt hout geen <rhyme label="a" type="m" corresp="#s12:l1">stee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
