        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Grâce au bon petit Jésus</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">APollo roept ons tot <rhyme label="a" type="m">playsier</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Kom Dianier, Rust u wat <rhyme label="a" type="m" corresp="#s1:l1">hier</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Op dees klaver-rijcke <rhyme label="b" type="m">strant</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daer de boompjes Langs de <rhyme label="c" type="f">stroompjes</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Staen <rhyme label="b" type="m" corresp="#s1:l3">geplant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ziet hoe verflouwt de visjes <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Terwijl den Rhijn Door d'heldre <rhyme label="a" type="m" corresp="#s2:l1">schijn</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Wort gesoden van <rhyme label="b" type="m">Apoll</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die door 't dalen Van sijn <rhyme label="c" type="f">stralen</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Schijnt op <rhyme label="b" type="m" corresp="#s2:l3">holl</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Of zouw een tweede <rhyme label="a" type="m">Phaéton</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die met de Son niet om gaen <rhyme label="a" type="m" corresp="#s3:l1">kon</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Sitten op sijn Vaders <rhyme label="b" type="m">Throon</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En de wegen Niet te <rhyme label="c" type="f">degen</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Kent der <rhyme label="b" type="m" corresp="#s3:l3">Goôn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Soo raeckt gewis het heele <rhyme label="a" type="m">Landt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Aen alle kant Weer in den <rhyme label="a" type="m" corresp="#s4:l1">brandt</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Neen, het is een ander <rhyme label="b" type="m">vier</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'k Was bedroogen 't Zijn uw' <rhyme label="c" type="f">oogen</rhyme></l>
        <l id="s4:l5" met="2" type="+-"><rhyme label="b" type="m" corresp="#s4:l3">Dianier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
