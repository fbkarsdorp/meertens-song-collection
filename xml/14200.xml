    <melody>Jan wil gij u beteren niet</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WIl ende lust als Davidt <rhyme label="a" type="m">vroedt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Laet wesen Heer in mijn <rhyme label="a" type="m" corresp="#s1:l1">ghemoet</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">t' Sy vreucht of <rhyme label="b" type="m">cruys</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Om in u <rhyme label="b" type="m" corresp="#s1:l3">huys</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Sijn eenpaer, dat ick <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">V dienen <rhyme label="d" type="m">mach</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Als Anna in ghebeden, <rhyme label="e" type="m">snel</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Laet my den tijdt besteden, <rhyme label="e" type="m" corresp="#s1:l7">wel</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Nacht ende <rhyme label="d" type="m" corresp="#s1:l6">Dach</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Leert my doch uwen wille <rhyme label="a" type="m">altijt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En uwen naem <rhyme label="a" type="m" corresp="#s2:l1">ghebenedijt</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Aenhangen <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">In s'werelts <rhyme label="b" type="m" corresp="#s2:l3">pleyn</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Onbelet, <rhyme label="c" type="m">onbesmet</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Aen Lijf en <rhyme label="d" type="m">geest</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Op dat ick mach bequaeme <rhyme label="e" type="m">zijn</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Met alle die den name <rhyme label="e" type="m" corresp="#s2:l7">dijn</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Hebben <rhyme label="d" type="m" corresp="#s2:l6">ghevreest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn toeverlaet en sterckte <rhyme label="a" type="m">weest</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">O Heer en mijn swackheyt <rhyme label="a" type="m" corresp="#s3:l1">geneest</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Blijft my <rhyme label="b" type="m">ontrent</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Tot inden <rhyme label="b" type="m" corresp="#s3:l3">ent</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dat ick heel, sy als <rhyme label="c" type="m">Seel</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dryvaudich <rhyme label="d" type="m">sterck</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Dat ick gheknocht te gader <rhyme label="e" type="m">stijf</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Met u aen uwen Vader <rhyme label="e" type="m" corresp="#s3:l7">blijf</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">In wil en <rhyme label="d" type="m" corresp="#s3:l6">werck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Kennisse geeft my voort en <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En lust in u heylighe <rhyme label="a" type="m" corresp="#s4:l1">woort</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Mijn siel <rhyme label="b" type="m">altoost</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Daer mede <rhyme label="b" type="m" corresp="#s4:l3">troost</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dat mijn hert vreuchdich <rhyme label="c" type="m">wert</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Liefdich <rhyme label="d" type="m">bevaen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Op dat alle mijn verlangen <rhyme label="e" type="m">sy</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">V stadich aen te hangen <rhyme label="e" type="m" corresp="#s4:l7">vry</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Sonder <rhyme label="d" type="m" corresp="#s4:l6">afgaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Petrus seer vierich inden <rhyme label="a" type="m">gront</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Doe hy verstont uyt uwen <rhyme label="a" type="m" corresp="#s5:l1">mont</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">De woorden <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Des Levens <rhyme label="b" type="m" corresp="#s5:l3">goet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Gheren niet en <rhyme label="c" type="m">verliet</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Hy u och <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Gheeft, dat ick van ghelijcke, <rhyme label="e" type="m">u</rhyme></l>
        <l id="s5:l8" met="4" type="+-">By blijf en niet en wijcke, <rhyme label="e" type="m" corresp="#s5:l7">nu</rhyme></l>
        <l id="s5:l9" met="2" type="+-">Noch <rhyme label="d" type="m" corresp="#s5:l6">nemmermeer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dochter doet ghy uyt uwen <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">V volck en Vaders huys waer <rhyme label="a" type="m" corresp="#s6:l1">in</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Begrepen <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">s'Vleeschs wil en <rhyme label="b" type="m" corresp="#s6:l3">raet</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">En gherust uwen <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Aen my stelt <rhyme label="d" type="m">vast</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Aen u schoonheyt ick stellen, <rhyme label="e" type="m">sal</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Oock mijnen lust en vellen, <rhyme label="e" type="m" corresp="#s6:l7">al</rhyme></l>
        <l id="s6:l9" met="2" type="+-">Wat u <rhyme label="d" type="m" corresp="#s6:l6">belast</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Van achter volcht mijn paden <rhyme label="a" type="m">naer</rhyme></l>
        <l id="s7:l2" met="4" type="-+">V cruys neempt op al schijnet <rhyme label="a" type="m" corresp="#s7:l1">swaer</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Voor t'vleesch tis <rhyme label="b" type="m">licht</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">En cleyn <rhyme label="b" type="m" corresp="#s7:l3">ghewicht</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Teghen die <rhyme label="c" type="m">Heerschappie</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Die eeuwich <rhyme label="d" type="m">deurt</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Daer niemant druc noch sneven <rhyme label="e" type="m">deert</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Dat soet is in dit Leven <rhyme label="e" type="m" corresp="#s7:l7">weert</rhyme>,</l>
        <l id="s7:l9" met="2" type="+-">Te zijn <rhyme label="d" type="m" corresp="#s7:l6">beseurt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Geboren zijnde uyt den geest <rhyme label="a" type="m">nieu</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Der gantscher werelt seght <rhyme label="a" type="m" corresp="#s8:l1">Adieu</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Met haer <rhyme label="b" type="m">ghewoel</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">En haren <rhyme label="b" type="m" corresp="#s8:l3">boel</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Anthechrist en den <rhyme label="c" type="m">mist</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Door seckten <rhyme label="d" type="m">vuyl</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Die wonder te doen weten <rhyme label="e" type="m">meest</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Maer duyvel valsch propheet en <rhyme label="e" type="m" corresp="#s8:l7">beest</rhyme></l>
        <l id="s8:l9" met="2" type="+-">Moet inden <rhyme label="d" type="m" corresp="#s8:l6">kuyl</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Tot u wil ick my keeren <rhyme label="a" type="m">doch</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Soo ghy tot my gewent blyft <rhyme label="a" type="m" corresp="#s9:l1">noch</rhyme></l>
        <l id="s9:l3" met="2" type="-+">In liefden <rhyme label="b" type="m">milt</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Volherden <rhyme label="b" type="m" corresp="#s9:l3">wilt</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Bidt en waeckt, haestich <rhyme label="c" type="m">naeckt</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Mijn comste <rhyme label="d" type="m">saen</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Vreest niet voor tijts benauwen <rhyme label="e" type="m">hier</rhyme></l>
        <l id="s9:l8" met="4" type="+-">In vreucht sult ghy uyt rauwen <rhyme label="e" type="m" corresp="#s9:l7">schier</rhyme>,</l>
        <l id="s9:l9" met="2" type="+-">Verlieven <rhyme label="d" type="m" corresp="#s9:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">g'Eert sy altijt Heer uwen <rhyme label="a" type="m">naem</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Mijn lieve Bruydegom <rhyme label="a" type="m" corresp="#s10:l1">bequaem</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Die eeuwich <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Inde wacht, door u <rhyme label="b" type="m" corresp="#s10:l3">macht</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Volherdens <rhyme label="c" type="m">deucht</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Dat ic als vruchtbaer rancken <rhyme label="d" type="m">groen</rhyme></l>
        <l id="s10:l7" met="4" type="+-">V alles mach te dancke <rhyme label="e" type="m">doen</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Met s herten <rhyme label="e" type="m" corresp="#s10:l7">vreucht</rhyme>,</l>
        <l id="s10:l9" met="2" type="+-">Bergen sullen wel wijcken <rhyme label="d" type="m" corresp="#s10:l6">maer</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">V woort staet vast <rhyme label="a" type="m">onwanckelbaer</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Niet en <rhyme label="a" type="m" corresp="#s11:l1">versloft</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Ghy u <rhyme label="b" type="m">beloft</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Dat ick my mach daer <rhyme label="b" type="m" corresp="#s11:l3">by</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Wel troosten <rhyme label="c" type="m">wis</rhyme>,</l>
        <l id="s11:l6" met="2" type="-+">Sonder u gelt berommen, <rhyme label="d" type="m">geen</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Maer ghy zijt dat volcommen, <rhyme label="e" type="m">een</rhyme></l>
        <l id="s11:l8" met="4" type="+-">Welck noodich <rhyme label="e" type="m" corresp="#s11:l7">is</rhyme>.</l>
        <l id="s11:l9" met="2" type="+-">De Liefde moet <rhyme label="d" type="m" corresp="#s11:l6">blijcken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
