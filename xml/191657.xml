<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191657</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Bei mir mein Herz</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Cupido <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Gy steelt door <rhyme label="b" type="m">Min</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Myn hert en <rhyme label="b" type="m" corresp="#s1:l2">zin</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Moet dit nu zyn myn <rhyme label="a" type="m" corresp="#s1:l1">lot</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Cupido wilt my dog <rhyme label="c" type="f">helpen</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">En wilt myn lyden <rhyme label="c" type="f" corresp="#s1:l5">stelpen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">ô Zoete waarde <rhyme label="a" type="m" corresp="#s1:l1">Godt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Gy quelt myn <rhyme label="a" type="m">zin</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Met een <rhyme label="b" type="m">Jonkvrouw</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Og dat ik <rhyme label="b" type="m" corresp="#s2:l2">zouw</rhyme></l>
        <l id="s2:l4" met="3" type="-+">verwerven hare <rhyme label="a" type="m" corresp="#s2:l1">Min</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Mag zy my niet <rhyme label="c" type="f">gebeuren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Zo moet myn herte <rhyme label="c" type="f" corresp="#s2:l5">scheuren</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En ik verlies myn <rhyme label="a" type="m" corresp="#s2:l1">zin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Geen schoonder <rhyme label="a" type="m">Vrouw</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Zoude op der <rhyme label="b" type="m">Aard</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">My zyn zo <rhyme label="b" type="m" corresp="#s3:l2">waard</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als deze wezen <rhyme label="a" type="m" corresp="#s3:l1">zouw</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Mogt zy my dog <rhyme label="c" type="f">gebeuren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zo waar ik vry van <rhyme label="c" type="f" corresp="#s3:l5">treuren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">ô Zoet' en waarde <rhyme label="a" type="m" corresp="#s3:l1">Vrouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Uw aanschyn <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Uw Borsjes <rhyme label="b" type="m">rond</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Uw lachend' <rhyme label="b" type="m" corresp="#s4:l2">Mond</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Laat die dog zyn myn <rhyme label="a" type="m" corresp="#s4:l1">loon</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Laat het u dog <rhyme label="c" type="f">erbermen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Laat ik door klagt en <rhyme label="c" type="f" corresp="#s4:l5">kermen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">U bidden schoonste <rhyme label="a" type="m" corresp="#s4:l1">Schoon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">ô Liefste <rhyme label="a" type="m">Lief</rhyme>!</l>
        <l id="s5:l2" met="2" type="-+">Denkt dat ik <rhyme label="b" type="m">myn</rhyme></l>
        <l id="s5:l3" met="2" type="-+">In druk en <rhyme label="b" type="m" corresp="#s5:l2">pyn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Nu stel tot uw <rhyme label="a" type="m" corresp="#s5:l1">gerief</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Laat het uw hert <rhyme label="c" type="f">verzagten</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat ik door pynt en <rhyme label="c" type="f" corresp="#s5:l5">klachten</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">U bidden waarde <rhyme label="a" type="m" corresp="#s5:l1">Lief</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">ô Waarde <rhyme label="a" type="m">Bloem</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Aanziet myn <rhyme label="b" type="m">hert</rhyme>!</l>
        <l id="s6:l3" met="2" type="-+">Aanziet myn <rhyme label="b" type="m" corresp="#s6:l2">smert</rhyme>!</l>
        <l id="s6:l4" met="3" type="-+">Mind m' om u eygen <rhyme label="a" type="m" corresp="#s6:l1">roem</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Wilt dog myn klagt <rhyme label="c" type="f">verhoren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Laat zinken uwen <rhyme label="c" type="f" corresp="#s6:l5">tooren</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">ô Bloeyend schoone <rhyme label="a" type="m" corresp="#s6:l1">Bloem</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wilt uw <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Wilt dog uw <rhyme label="b" type="m">zin</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Door waare <rhyme label="b" type="m" corresp="#s7:l2">Min</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Keeren tot uw Lief <rhyme label="a" type="m" corresp="#s7:l1">zoet</rhyme>!</l>
        <l id="s7:l5" met="3" type="-+">Wilt hem uw Minne <rhyme label="c" type="f">schenken</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En zyn Liefd' <rhyme label="c" type="f" corresp="#s7:l5">gedenken</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En keert dog uw <rhyme label="a" type="m" corresp="#s7:l1">gemoedt</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Keerd dog uw <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Mind hem dog <rhyme label="b" type="m">weer</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">In trouw en <rhyme label="b" type="m" corresp="#s8:l2">eer</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Door een opregte <rhyme label="a" type="m" corresp="#s8:l1">Min</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wilt hem uw Liefd' <rhyme label="c" type="f">bewyzen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Zo zal hy u dan <rhyme label="c" type="f" corresp="#s8:l5">pryzen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Door een verwonnen <rhyme label="a" type="m" corresp="#s8:l1">zin</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Zo gy zyn <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Met waare <rhyme label="b" type="m">Liefd'</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Dog eens <rhyme label="b" type="m" corresp="#s9:l2">geriefd</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Zo waar hy zonder <rhyme label="a" type="m" corresp="#s9:l1">smert</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Zo zou hy uw slaaf <rhyme label="c" type="f">blyven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En zyn tot uw <rhyme label="c" type="f" corresp="#s9:l5">geryven</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Met Lyf, Ziel, Bloed en <rhyme label="a" type="m" corresp="#s9:l1">Hert</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Cupido <rhyme label="a" type="m">Wicht</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Zo gy myn <rhyme label="b" type="m">Lief</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Tot myn <rhyme label="b" type="m" corresp="#s10:l2">gerief</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Kond brengen door u <rhyme label="a" type="m" corresp="#s10:l1">schicht</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Zo zal ik u dan <rhyme label="c" type="f">pryzen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En eeuwig eer <rhyme label="c" type="f" corresp="#s10:l5">bewyzen</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">ô Zoet' en Lieve <rhyme label="a" type="m" corresp="#s10:l1">wicht</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
