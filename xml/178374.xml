<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178374</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Dragonder te paard</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Komt nu braave wel <rhyme label="a" type="f">gezinden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die altydt den Prins <rhyme label="a" type="f" corresp="#s1:l1">beminden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Laat ons nu same Vreugde <rhyme label="b" type="f">bedryven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Laat ons nu vrolyk <rhyme label="c" type="f">zingen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ons hart niet meer <rhyme label="c" type="f" corresp="#s1:l4">bedwingen</rhyme>:</l>
        <l id="s1:l6" met="4" type="-+">Want dees' dag is het dubbeld <rhyme label="d" type="m">waart</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Het is reeds den Agste <rhyme label="d" type="m" corresp="#s1:l6">Maart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vrolyk moeten wy nu <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ja al wierd het ook <rhyme label="a" type="f" corresp="#s2:l1">mispreezen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Van die hoop die 't Staatsgebouw <rhyme label="b" type="f">omvroeten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die onse Prins <rhyme label="c" type="f">benyden</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Zyn deugd niet kunnen <rhyme label="c" type="f" corresp="#s2:l4">lijden</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">En hem daarom stout <rhyme label="d" type="m">versmaen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Maar dat Volk gaat ons niet <rhyme label="d" type="m" corresp="#s2:l6">aan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wij willen niet aan U <rhyme label="a" type="f">denken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">'t Souw onze Vreugde maar <rhyme label="a" type="f" corresp="#s3:l1">krenken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En gij zult uw doel nooit <rhyme label="b" type="f">berijken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Want alle braave <rhyme label="c" type="f">menschen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Doen haar beste <rhyme label="c" type="f" corresp="#s3:l4">wenschen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Voor de Prins en voor zijn <rhyme label="d" type="m">Huijs</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Dat is uw wel het grootste <rhyme label="d" type="m" corresp="#s3:l6">Kruis</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vieren zullen wij <rhyme label="a" type="f">branden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Gij meugt vrij knerse <rhyme label="a" type="f" corresp="#s4:l1">tanden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En van spijt ons tragten te <rhyme label="b" type="f">vernielen</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Wij blijven de Prins <rhyme label="c" type="f">beminnen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Met onze Ziel en <rhyme label="c" type="f" corresp="#s4:l4">zinnen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">En wij vallen hem nooijt <rhyme label="d" type="m">af</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Maar getrouw tot aan het <rhyme label="d" type="m" corresp="#s4:l6">Graf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Waarom zou wij het <rhyme label="a" type="f">ontveinsen</rhyme>?</l>
        <l id="s5:l2" met="4" type="-+">En nu te rugge gaan <rhyme label="a" type="f" corresp="#s5:l1">deinsen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Neen ô neen! wij durven het wel <rhyme label="b" type="f">zeggen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Al het bloed in onse <rhyme label="c" type="f">Aderen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Is als dat van onse <rhyme label="c" type="f" corresp="#s5:l4">Vaderen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Het is het egte Neerdands <rhyme label="d" type="m">Bloet</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Het ontbreekt ons aan geen <rhyme label="d" type="m" corresp="#s5:l6">moedt</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Laat ons nu ook onze <rhyme label="a" type="f">beeden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die wij steeds heimelijk <rhyme label="a" type="f" corresp="#s6:l1">deeden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Opentlijk voor onze Prins <rhyme label="b" type="f">uijtspreeken</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wij wensche hem van <rhyme label="c" type="f">herte</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Een eijnd van al zijn <rhyme label="c" type="f" corresp="#s6:l4">smerte</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Dat hij spoedig al het <rhyme label="d" type="m">verdriet</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Vergeet het geen hem tans <rhyme label="d" type="m" corresp="#s6:l6">geschiedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">En dat al die hem <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Eijndelijk eens <rhyme label="a" type="f" corresp="#s7:l1">overwinnen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En de Lasters zelvers mag doen <rhyme label="b" type="f">zwijgen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dan zullen al zijn <rhyme label="c" type="f">haters</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Zijn trouwelooze <rhyme label="c" type="f" corresp="#s7:l4">Verlaaters</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Van hun booze weg <rhyme label="d" type="m">afgaan</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">En hem weer ten dienste <rhyme label="d" type="m" corresp="#s7:l6">staan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dan zal Neerland weer <rhyme label="a" type="f">floreren</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Den Nabuur zal ons dan weer <rhyme label="a" type="f" corresp="#s8:l1">Eeren</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Rust en Voorspoed zal ons weer <rhyme label="b" type="f">ontmoeten</rhyme>!</l>
        <l id="s8:l4" met="3" type="-+">Tweedragt ons land <rhyme label="c" type="f">verlaaten</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Dan zulle de Heere <rhyme label="c" type="f" corresp="#s8:l4">Staaten</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Eensgezint met onze <rhyme label="d" type="m">Vorst</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Die zijn last nu willig <rhyme label="d" type="m" corresp="#s8:l6">Torst</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Nu eens helder <rhyme label="a" type="f">ingeschonken</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Tot de grondt toe <rhyme label="a" type="f" corresp="#s9:l1">uitgedronken</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Want wij moeten dog regt Vrolijk <rhyme label="b" type="f">weezen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Roepen wij Viva <rhyme label="c" type="f">Hoezée</rhyme>!</l>
        <l id="s9:l5" met="3" type="-+">Alle menschen doen dog <rhyme label="c" type="f" corresp="#s9:l4">mee</rhyme>:</l>
        <l id="s9:l6" met="4" type="-+">Want het is den Agtste <rhyme label="d" type="m">Maart</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Onze Prins van daag <rhyme label="d" type="m" corresp="#s9:l6">Verjaart</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Nog eens Helder <rhyme label="a" type="f">ingeschonken</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">En nog weer eens <rhyme label="a" type="f" corresp="#s10:l1">uijtgedronken</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Ter Eer van onze braave <rhyme label="b" type="f">Vorstinne</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Roepen wy, Viva <rhyme label="c" type="f">Hoezée</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Alle braaven doen tog <rhyme label="c" type="f" corresp="#s10:l4">mee</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">'t Is van daag den Agste <rhyme label="d" type="m">Maart</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Onze Prins is 't die <rhyme label="d" type="m" corresp="#s10:l6">Verjaart</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Nu nog eens een Frisse <rhyme label="a" type="f">Beeker</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Want wij meenen het <rhyme label="a" type="f" corresp="#s11:l1">voorzeeker</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Tot Eere van de Jong Orangie <rhyme label="b" type="f">Spruiten</rhyme>.</l>
        <l id="s11:l4" met="3" type="-+">Roepen wij! Viva <rhyme label="c" type="f">Hoezée</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Patriotten doen niet <rhyme label="c" type="f" corresp="#s11:l4">mee</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Want dees dag al lang <rhyme label="d" type="m">Verjaart</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Is ons blydschap dubbelt <rhyme label="d" type="m" corresp="#s11:l6">waart</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Nu eens helder <rhyme label="a" type="f">opgesprongen</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">En ook uit den Borst <rhyme label="a" type="f" corresp="#s12:l1">gezongen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Tot vreugd van de Prins en de <rhyme label="b" type="f">Princesse</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En niet min voor 't Jonge <rhyme label="c" type="f">Kroost</rhyme>:</l>
        <l id="s12:l5" met="3" type="-+">Want dat is tot Neêrlands <rhyme label="c" type="f" corresp="#s12:l4">troost</rhyme>:</l>
        <l id="s12:l6" met="4" type="-+">Roepen zingen Wy, en <rhyme label="d" type="m">loven</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Duyzent maal Orangje...., Orangje <rhyme label="d" type="m" corresp="#s12:l6">boven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
