  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wes sall ich mich <rhyme label="a" type="f">ernerenn</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">ich werdtt gehaltenn so <rhyme label="b" type="m">kortt</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">die liebste m s ich <rhyme label="a" type="f" corresp="#s1:l1">meidenn</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">dar ich mich hab noch <rhyme label="b" type="m" corresp="#s1:l2">gestaltt</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">nach mag sei mir nitt geb <rhyme label="c" type="f">ren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">So weitt das ich vonn ir <rhyme label="d" type="m">bin</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">a ff ir so woltt ich tra <rhyme label="e" type="f">wenn</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">von wachholtz  woltt ich ba wen vnnd ba <rhyme label="e" type="f" corresp="#s1:l7">wenn</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Der allerliebste ein ha <rhyme label="f" type="m">ß</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Reyff schnehe vnnd kalter <rhyme label="a" type="f">wintt</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">kan mich bezwingenn <rhyme label="b" type="m">nitt</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">mein hertze vnnd alle mein <rhyme label="a" type="f" corresp="#s2:l1">Synne</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">hab ich z  der liebster <rhyme label="b" type="m" corresp="#s2:l2">gekertt</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Mag sei mir dan nitt geb <rhyme label="c" type="f">renn</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">so weitt das ich von ir <rhyme label="d" type="m">bin</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Ein krentzelein woltt ich <rhyme label="e" type="f">thragenn</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">vnnd dan mitt fre denn <rhyme label="e" type="f" corresp="#s2:l7">sagen</rhyme> /</l>
        <l id="s2:l9" met="4" type="-+">mein hertz gra wett jetz z  <rhyme label="f" type="m">ir</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vnnd offt ich kem <rhyme label="a" type="f">gerittenn</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">dae ich die liebste f <rhyme label="b" type="m">ndt</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">ich woltt mich z  jr <rhyme label="a" type="f" corresp="#s3:l1">kerenn</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">so ferne als jmmer kh <rhyme label="b" type="m" corresp="#s3:l2">ndt</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">hertz lieb d  moiß dich <rhyme label="c" type="f">leidenn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">vnnd haltenn einenn h ffschenn m <rhyme label="d" type="m">edt</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">vnnd schlagh hera ff deine <rhyme label="e" type="f">armenn</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">Laß dich meins leidtz <rhyme label="e" type="f" corresp="#s3:l7">erbarmenn</rhyme> /</l>
        <l id="s3:l9" met="4" type="-+">helff mir aus dieser <rhyme label="f" type="m">nott</rhyme>.</l>
      </lg>
    </lg>
