    <melody>Wat is de wereld toch</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NV ick de vverelt <rhyme label="a" type="m">ken</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Beclaegh' ick my te <rhyme label="b" type="m">recht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat ick gheboren <rhyme label="a" type="m" corresp="#s1:l1">ben</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van stam, en af-comst <rhyme label="b" type="m" corresp="#s1:l2">slecht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">,,Midts men meere,, om <rhyme label="c" type="m">t'goet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">,,Dan om d'eere,, nu <rhyme label="c" type="m" corresp="#s1:l5">doet</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Wee mijns ermen <rhyme label="b" type="m" corresp="#s1:l2">knecht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">,,Op deucht, oft <rhyme label="a" type="m">trouvvicheyt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">,,Wordt nu niet meer <rhyme label="b" type="m">gheacht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">,,Een maeght vvordt nu <rhyme label="a" type="m" corresp="#s2:l1">verleyt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">,,Gout-suchtich, door den <rhyme label="b" type="m" corresp="#s2:l2">pracht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">,,Die niet rijck'lijck,, en <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">,,Wordt, t'is blijck'lijck,, <rhyme label="c" type="m" corresp="#s2:l5">versmaet</rhyme></l>
        <l id="s2:l7" met="3" type="+-">,,Want naer staet elck <rhyme label="b" type="m" corresp="#s2:l2">tracht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">,,O eeuvv, Ã´ snooden <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">,,Die s'menschen boosheyt <rhyme label="b" type="m">deckt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">,,Met gout <rhyme label="a" type="m" corresp="#s3:l1">vermalendijt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">,,Dat eer, en deucht <rhyme label="b" type="m" corresp="#s3:l2">bevleckt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">,,Draeght dan rouvve,, t'is <rhyme label="c" type="m">noot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">,,Want de trouvve,, is <rhyme label="c" type="m" corresp="#s3:l5">doot</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">,,Endt' bedroch <rhyme label="b" type="m" corresp="#s3:l2">vervveckt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">,,Ist moordenaer, oft <rhyme label="a" type="m">dief</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">,,Heeft hy maer goet, en <rhyme label="b" type="m">gelt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">,,Elck eert, en heeft hem <rhyme label="a" type="m" corresp="#s4:l1">lief</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">D'vvelck my ont'rust, en <rhyme label="b" type="m" corresp="#s4:l2">quelt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ia besvvijcker,, om <rhyme label="c" type="m">dick</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van een rijcker,, als <rhyme label="c" type="m" corresp="#s4:l5">ick</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Lijt mijn lief <rhyme label="b" type="m" corresp="#s4:l2">ghevvelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Denckt lief, denckt dat een <rhyme label="a" type="m">man</rhyme></l>
        <l id="s5:l2" met="3" type="-+">,,Die t'sijn vvel gaede <rhyme label="b" type="m">slaet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">,,Aen goet licht raken <rhyme label="a" type="m" corresp="#s5:l1">can</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En vvat is rijckdom <rhyme label="b" type="m" corresp="#s5:l2">quaet</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">,,Een die t'sijne,, <rhyme label="c" type="m">verquist</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">,,Ghelijck schijne,, oft <rhyme label="c" type="m" corresp="#s5:l5">mist</rhyme></l>
        <l id="s5:l7" met="3" type="+-">,,Sijnen schadt <rhyme label="b" type="m" corresp="#s5:l2">vergaet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Siet Lief, siet dan mijn <rhyme label="a" type="m">trouvv'</rhyme></l>
        <l id="s6:l2" met="3" type="-+">,,Voor't goet, kiest deucht en <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo ghy vvilt sijn mijn <rhyme label="a" type="m" corresp="#s6:l1">Vrouvv'</rhyme>?</l>
        <l id="s6:l4" met="3" type="-+">Ick sal u achten <rhyme label="b" type="m" corresp="#s6:l2">meer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Dan een Coninck,, de <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Mijn soet honinck,, <rhyme label="c" type="m" corresp="#s6:l5">aenschijn</rhyme></l>
        <l id="s6:l7" met="3" type="+-">Neemt tot my den <rhyme label="b" type="m" corresp="#s6:l2">keer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
