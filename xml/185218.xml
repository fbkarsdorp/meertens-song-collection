<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185218</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatstmaal ging ik ter jacht</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Mijn schepper en <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">V moet ick altijt <rhyme label="b" type="f">loven</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Tot u neem ick mijn <rhyme label="a" type="m" corresp="#s1:l1">keer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verhoort my doch daer <rhyme label="b" type="f" corresp="#s1:l2">boven</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wijckt niet van my ‘t eenigher <rhyme label="c" type="m">tijt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Aen u doe ick mijn <rhyme label="c" type="m" corresp="#s1:l5">belydt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">ghy mijn trooster <rhyme label="c" type="m" corresp="#s1:l5">zijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghy weet o Heere <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoe oprecht dat ick ‘t <rhyme label="b" type="f">meene</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Aen u mijn wille <rhyme label="a" type="m" corresp="#s2:l1">doet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al sou die natuur <rhyme label="b" type="f" corresp="#s2:l2">weene</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ick geef my over in u <rhyme label="c" type="m">hant</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ziel en Geest set ick te <rhyme label="c" type="m" corresp="#s2:l5">pant</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Natuur moet aen een <rhyme label="c" type="m" corresp="#s2:l5">cant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe wel dat die <rhyme label="a" type="m">natuur</rhyme></l>
        <l id="s3:l2" met="3" type="-+">So garen wil <rhyme label="b" type="f">regieren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En maeckt den Geest wat <rhyme label="a" type="m" corresp="#s3:l1">suur</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met sijn oude <rhyme label="b" type="f" corresp="#s3:l2">manieren</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Als stercker comt moet swacker <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Natuur moet besijden <rhyme label="c" type="m" corresp="#s3:l5">staen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maeckt voor den Geest <rhyme label="c" type="m" corresp="#s3:l5">ruym-baen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat soudet schicken <rhyme label="a" type="m">seght</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat twee contrary <rhyme label="b" type="f">Heeren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">In een goet-meenent <rhyme label="a" type="m" corresp="#s4:l1">knecht</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">‘t Samen souden <rhyme label="b" type="f" corresp="#s4:l2">logeeren</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">Neen, neen natuur ‘t mach wesen <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ick geef my onder ‘s Geests <rhyme label="c" type="m" corresp="#s4:l5">gebiet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Al ist u groot <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Natuur is haest <rhyme label="a" type="m">versaet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Weest daer dan med’ te <rhyme label="b" type="f">vreden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En u vernoeghen <rhyme label="a" type="m" corresp="#s5:l1">laet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Buyght u onder ‘s Geests <rhyme label="b" type="f" corresp="#s5:l2">reden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Wilt ghy, of niet, ghy moeter <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Ick wil letten op ‘s Geest <rhyme label="c" type="m" corresp="#s5:l5">vermaen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Soo als Godt wil salt <rhyme label="c" type="m" corresp="#s5:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nu bid ick Heere <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Om uwen Geest van <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Sent die tot mijn <rhyme label="a" type="m" corresp="#s6:l1">behoet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat ick altijt mach <rhyme label="b" type="f" corresp="#s6:l2">loven</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Het gene dat ghy mijn <rhyme label="c" type="m">toesent</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Van nu tot aen mijns levens <rhyme label="c" type="m" corresp="#s6:l5">endt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Waerom is ‘t u <rhyme label="c" type="m" corresp="#s6:l5">bekent</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nu Oorelof hier <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Die dit singen of <rhyme label="b" type="f">lesen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick wensch u Godes <rhyme label="a" type="m" corresp="#s7:l1">vree</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Tot een leydtsman in <rhyme label="b" type="f" corresp="#s7:l2">desen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Laet die u leyden na sijn <rhyme label="c" type="m">wil</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Seght nature hout u <rhyme label="c" type="m" corresp="#s7:l5">stil</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Hebt met Godt geen <rhyme label="c" type="m" corresp="#s7:l5">geschil</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
