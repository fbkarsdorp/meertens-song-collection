  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEmelsche <rhyme label="a" type="f">Koninghinne</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Verblijt v nu: verblijt v <rhyme label="b" type="m">nu</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Want Iesus die ick <rhyme label="a" type="f" corresp="#s1:l1">minne</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Die komt tot <rhyme label="b" type="m" corresp="#s1:l2">v</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die ghy weert waert te <rhyme label="a" type="f">dragen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">In uwen schoot, in uwen <rhyme label="b" type="m">schoot</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Verrijst naer schrifts <rhyme label="a" type="f" corresp="#s2:l1">ghewaghen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Rasch vander <rhyme label="b" type="m" corresp="#s2:l2">doot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Bidt Godt voor ons <rhyme label="a" type="f">Marie</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Helpt onsen strijt, helpt onsen <rhyme label="b" type="m">strijt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Laet ons verweruen <rhyme label="a" type="f" corresp="#s3:l1">blie</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Dâ€™eeuwich <rhyme label="b" type="m" corresp="#s3:l2">iolijt</rhyme>.</l>
      </lg>
    </lg>
