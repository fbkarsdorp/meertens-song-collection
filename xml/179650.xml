<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179650</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody>Ha ha zie zo</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ZA Bruigom als een <rhyme label="a" type="m">Held</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Gy moet van Nagt in ’t <rhyme label="a" type="m" corresp="#s1:l1">veld</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Maer eer gy schieten zult de <rhyme label="b" type="m">bres</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Zoo drinkt met ons nog eenen <rhyme label="b" type="m" corresp="#s1:l3">Vles</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s1:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Laedstok tot hy <rhyme label="a" type="m">stuyt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Moet telkens in en <rhyme label="a" type="m" corresp="#s2:l1">uit</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En maekt dat gy daer wel op <rhyme label="b" type="m">let</rhyme>;</l>
        <l id="s2:l4" met="4" type="-+">Dat gy de prop daer wel op <rhyme label="b" type="m" corresp="#s2:l3">zet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s2:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Legt aen tot uw <rhyme label="a" type="m">Pleizier</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En geeft wel dapper <rhyme label="a" type="m" corresp="#s3:l1">Vier</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">En laed dan weer van voren of <rhyme label="b" type="m">an</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En schiet dan op de halve <rhyme label="b" type="m" corresp="#s3:l3">man</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s3:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Zyt gy van schieten <rhyme label="a" type="m">moe</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Sluyt dan de Pan wel <rhyme label="a" type="m" corresp="#s4:l1">toe</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">En zet de Haen wel op zyn <rhyme label="b" type="m">rust</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Tot uw weer de Exercietie <rhyme label="b" type="m" corresp="#s4:l3">lust</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s4:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ach Bruidje schreid dog <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Als hy wat schielyk <rhyme label="a" type="m" corresp="#s5:l1">schiet</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En schiet hy niet altyd <rhyme label="b" type="m">gewis</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">De beste Schieter, schiet wel <rhyme label="b" type="m" corresp="#s5:l3">mis</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s5:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ach Bruygom ’t <rhyme label="a" type="m">Manuael</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Leerd haer in stomme <rhyme label="a" type="m" corresp="#s6:l1">Tael</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Op dat zy op het <rhyme label="b" type="m">Trommelslag</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">De Snaphaen handeld als een <rhyme label="b" type="m" corresp="#s6:l3">Vlag</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s6:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Voor dat gy op zult <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Moet gy Revelje <rhyme label="a" type="m" corresp="#s7:l1">slaen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En twee Schild-wagten voor ’t <rhyme label="b" type="m">gebien</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Om d’Exercietie aen te <rhyme label="b" type="m" corresp="#s7:l3">zien</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s7:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Voor ’e laetste tot <rhyme label="a" type="m">besluyt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Wensch ik uw altyd <rhyme label="a" type="m" corresp="#s8:l1">kruyt</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">En dat uw Bruidjes <rhyme label="b" type="m">Magazyn</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Mag vrugt voort brengen zonder <rhyme label="b" type="m" corresp="#s8:l3">pyn</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s8:l5">Stroo</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dat zy een <rhyme label="a" type="m">Piekenier</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Haest ziet tot haer <rhyme label="a" type="m" corresp="#s9:l1">plyzier</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Voor ’t komen uit haer <rhyme label="b" type="m">Magazyn</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Kom drink daer op en geeft ons <rhyme label="b" type="m" corresp="#s9:l3">Wyn</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Ha, ha, zie <rhyme label="c" type="m">zoo</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">De liefde brand als <rhyme label="c" type="m" corresp="#s9:l5">Stroo</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
