<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155495</id>
    <date>
      <year-start>1669</year-start>
      <year-end>1669</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Echo wil ik tergen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">DIcht by Idaas <rhyme label="a" type="m">kruin</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">In 't <rhyme label="b" type="m">geboomt</rhyme>;</l>
        <l id="s1:l3" met="2" type="+-">Koel <rhyme label="b" type="m" corresp="#s1:l2">bestroomt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Leit een schoone <rhyme label="a" type="m" corresp="#s1:l1">Tuin</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Die met wit <rhyme label="a" type="m" corresp="#s1:l1">Arduin</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Is <rhyme label="c" type="f">beslooten</rhyme>;</l>
        <l id="s1:l7" met="2" type="+-"><rhyme label="c" type="f" corresp="#s1:l6">Roozelooten</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Tulp en <rhyme label="d" type="f">Lauwerieren</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Boorden yder <rhyme label="e" type="m">laan</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Die met brave <rhyme label="d" type="f" corresp="#s1:l8">zwieren</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Recht na 'et Tuinslot <rhyme label="e" type="m" corresp="#s1:l9">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Voor de <rhyme label="a" type="m">Valbrugpoort</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Van het <rhyme label="b" type="m">slot</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Waakt een <rhyme label="b" type="m" corresp="#s2:l2">rot</rhyme>;</l>
        <l id="s2:l4" met="3" type="+-">'t Geen nooit slaap <rhyme label="a" type="m" corresp="#s2:l1">verstoort</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Maar het al <rhyme label="a" type="m" corresp="#s2:l1">vermoort</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Die hun <rhyme label="c" type="f">eeden</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-"><rhyme label="c" type="f" corresp="#s2:l6">Overtreeden</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En beloften <rhyme label="d" type="f">breken</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Die zy aan de <rhyme label="e" type="m">min</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Doen, en slimme <rhyme label="d" type="f" corresp="#s2:l8">streken</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Hebben in haar <rhyme label="e" type="m" corresp="#s2:l9">zin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Almayde <rhyme label="a" type="m">quam</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Door de <rhyme label="b" type="m">laan</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-"><rhyme label="b" type="m" corresp="#s3:l2">Aangegaan</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Als een vredig <rhyme label="a" type="m" corresp="#s3:l1">lam</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">'t Geen nooit list <rhyme label="a" type="m" corresp="#s3:l1">vernam</rhyme>;</l>
        <l id="s3:l6" met="2" type="+-">Dies de <rhyme label="c" type="f">wachten</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Die haar <rhyme label="c" type="f" corresp="#s3:l6">achten</rhyme>;</l>
        <l id="s3:l8" met="3" type="+-">Venus zelf te <rhyme label="d" type="f">wezen</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Weken aan d'een <rhyme label="e" type="m">zy</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Yder was vol <rhyme label="d" type="f" corresp="#s3:l8">vrezen</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Zy alleenig <rhyme label="e" type="m" corresp="#s3:l9">bly</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Flux ontsloot de <rhyme label="a" type="m">deur</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Van 't <rhyme label="b" type="m">klinkket</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-"><rhyme label="b" type="m" corresp="#s4:l2">Opgezet</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Daar zy rustig <rhyme label="a" type="m" corresp="#s4:l1">deur</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Zonder droef <rhyme label="a" type="m" corresp="#s4:l1">getreur</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Heen gink <rhyme label="c" type="f">strijken</rhyme>.</l>
        <l id="s4:l7" met="2" type="+-">Ware <rhyme label="c" type="f" corresp="#s4:l6">blijken</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Dat haar trouwe <rhyme label="d" type="f">liefde</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Alles <rhyme label="e" type="m">overwint</rhyme>;</l>
        <l id="s4:l10" met="3" type="+-">En meer harten <rhyme label="d" type="f" corresp="#s4:l8">g'riefden</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Als vrouw Venus <rhyme label="e" type="m" corresp="#s4:l9">Kint</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Toenze op 't <rhyme label="a" type="m">middelplein</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Van den <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Rijk van <rhyme label="b" type="m" corresp="#s5:l2">stof</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Zag de <rhyme label="a" type="m" corresp="#s5:l1">minfontein</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">En de brave <rhyme label="a" type="m" corresp="#s5:l1">trein</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Van de <rhyme label="c" type="f">Goodtjes</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Die voor <rhyme label="c" type="f" corresp="#s5:l6">Bootjes</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Ericijn <rhyme label="d" type="f">verstrekken</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Riepze, vol van <rhyme label="e" type="m">gloet</rhyme>:</l>
        <l id="s5:l10" met="3" type="+-">Deze dan <rhyme label="d" type="f" corresp="#s5:l8">verwekken</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Min in ons <rhyme label="e" type="m" corresp="#s5:l9">gemoet</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Wijl zy dit noch <rhyme label="a" type="m">sprak</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Quam een <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">('t Was haar <rhyme label="b" type="m" corresp="#s6:l2">lot</rhyme>,)</l>
        <l id="s6:l4" met="3" type="+-">Die heur spoedig <rhyme label="a" type="m" corresp="#s6:l1">stak</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Dat zijn moortpriem <rhyme label="a" type="m" corresp="#s6:l1">brak</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">In haar <rhyme label="c" type="f">wonden</rhyme>;</l>
        <l id="s6:l7" met="2" type="+-"><rhyme label="c" type="f" corresp="#s6:l6">Ongeschonden</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Riep hy, zult gy <rhyme label="d" type="f">sterven</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Om uw stout <rhyme label="e" type="m">bestaan</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Op dees heilige <rhyme label="d" type="f" corresp="#s6:l8">erven</rhyme>;</l>
        <l id="s6:l11" met="3" type="+-">Mach maar Godtheit <rhyme label="e" type="m" corresp="#s6:l9">gaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
