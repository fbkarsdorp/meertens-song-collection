  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn hart, dat door uw oogen <rhyme label="a" type="f">stralen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En van de minschigt is <rhyme label="b" type="m">gewont</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dat wilt gy niet naar loon <rhyme label="a" type="f" corresp="#s1:l1">betalen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Gy spot met my, myn <rhyme label="b" type="m" corresp="#s1:l2">Rosemont</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Kan u noch meer <rhyme label="c" type="f">behagen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Al myn minne <rhyme label="d" type="m">smart</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Ai schoonste, hoort myn <rhyme label="c" type="f" corresp="#s1:l5">klagen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">En myn zugtent <rhyme label="d" type="m" corresp="#s1:l6">hart</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Dat stervens is <rhyme label="d" type="m" corresp="#s1:l6">benart</rhyme>.</l>
      </lg>
    </lg>
  </text>
