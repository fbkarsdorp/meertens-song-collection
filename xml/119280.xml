    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Ton humeur est Catherine</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HY zo moet men Bruiloft <rhyme label="a" type="f">houwen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Bruiloft Bruiloft zoete <rhyme label="b" type="m">pret</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Bruidje na ik kan <rhyme label="a" type="f" corresp="#s1:l1">beschouwen</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Zo verlangt gy om na <rhyme label="b" type="m" corresp="#s1:l2">bed</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Met uw Bruidegom te <rhyme label="c" type="f">treden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Ja ik zie gy <rhyme label="d" type="m">watertand</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Jonge Schoonheid uwe <rhyme label="c" type="f" corresp="#s1:l5">leden</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Voegen tot den Huwelyks <rhyme label="d" type="m" corresp="#s1:l6">Band</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vlugge Bruidegom uw loon <rhyme label="a" type="f">is</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Nu verzoet door de egten <rhyme label="b" type="m">staat</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Leeft als Venus en <rhyme label="a" type="f" corresp="#s2:l1">Adonis</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Nu gy na de Slaap-Koets <rhyme label="b" type="m" corresp="#s2:l2">gaat</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daar dat lieve leden <rhyme label="c" type="f">rekken</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Jonge Lieden roept ten <rhyme label="d" type="m">stryt</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ã´! Dat zoete <rhyme label="c" type="f" corresp="#s2:l5">trekkebekken</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Is gebalzemd met <rhyme label="d" type="m" corresp="#s2:l6">confyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Strand op zagt en malze <rhyme label="a" type="f">kaken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Daar het blozend <rhyme label="b" type="m">fermiljoen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Uw in heeten gloed kan <rhyme label="a" type="f" corresp="#s3:l1">blaken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Beurt om beurt met zoen op <rhyme label="b" type="m" corresp="#s3:l2">zoen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Lieve lonkjes zoete <rhyme label="c" type="f">lachjes</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Op de pluimpjes even <rhyme label="d" type="m">zagt</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En veel minnende <rhyme label="c" type="f" corresp="#s3:l5">gedachjes</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Gunne u een goede <rhyme label="d" type="m" corresp="#s3:l6">nacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
