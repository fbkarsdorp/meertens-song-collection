  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ick wil verhoechgen mijnen <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ende laten <rhyme label="b" type="f">overlijden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Daer werelts vroechde ende al haer <rhyme label="a" type="m" corresp="#s1:l1">mijn</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Tot Jhesum wil ic my <rhyme label="b" type="f" corresp="#s1:l2">tyden</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Daer werelts vroechde ende al haer <rhyme label="a" type="m" corresp="#s1:l1">mijn</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Tot Jhesum wil ic my <rhyme label="b" type="f" corresp="#s1:l2">tyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hy is myn troest, mijn <rhyme label="a" type="m">toeverlaat</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In alle mynen <rhyme label="b" type="f">liden</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Hi ist daer al myn sin toe <rhyme label="a" type="m" corresp="#s2:l1">staet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hi doet myn hert <rhyme label="b" type="f" corresp="#s2:l2">verbliden</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Hi ist daer al myn sin toe <rhyme label="a" type="m" corresp="#s2:l1">staet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hi doet myn hert <rhyme label="b" type="f" corresp="#s2:l2">verbliden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Eens sjaers in mijnre jongher <rhyme label="a" type="m">joecht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Doen dwaelde ic uuten <rhyme label="b" type="f">paden</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Maer Jhesus heeft met Sijnre <rhyme label="a" type="m" corresp="#s3:l1">doecht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">mijn sinnen nu <rhyme label="b" type="f" corresp="#s3:l2">beraden</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Maer Jhesus heeft met Sijnre <rhyme label="a" type="m" corresp="#s3:l1">doecht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">mijn sinnen nu <rhyme label="b" type="f" corresp="#s3:l2">beraden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sijn minne is soeter boven <rhyme label="a" type="m">al</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dengenen die see <rhyme label="b" type="f">smaken</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Soe wie wil gheven al om <rhyme label="a" type="m" corresp="#s4:l1">al</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die sel daertoe <rhyme label="b" type="f" corresp="#s4:l2">gheraken</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Soe wie wil gheven al om <rhyme label="a" type="m" corresp="#s4:l1">al</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die sel daertoe <rhyme label="b" type="f" corresp="#s4:l2">gheraken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Vriheit is een edel <rhyme label="a" type="m">schat</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Die dien wel can <rhyme label="b" type="f">bewaren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Sy leyt hem in den rechten <rhyme label="a" type="m" corresp="#s5:l1">pat</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hij en derf voer nyemant <rhyme label="b" type="f" corresp="#s5:l2">varen</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Sy leyt hem in den rechten <rhyme label="a" type="m" corresp="#s5:l1">pat</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hij en derf voer nyemant <rhyme label="b" type="f" corresp="#s5:l2">varen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sy sijn van herten <rhyme label="a" type="m">ongheschickt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die creaturen <rhyme label="b" type="f">minnen</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Maer diet al om Jhesus <rhyme label="a" type="m" corresp="#s6:l1">laet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Die sel veel vreeds <rhyme label="b" type="f" corresp="#s6:l2">ghewinnen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Maer diet al om Jhesus <rhyme label="a" type="m" corresp="#s6:l1">laet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die sel veel vreeds <rhyme label="b" type="f" corresp="#s6:l2">ghewinnen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Conste ic my selfve wel <rhyme label="a" type="m">uutgaen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ende alle dinck <rhyme label="b" type="f">vergheten</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Jhesus sel my soe wel <rhyme label="a" type="m" corresp="#s7:l1">ontfaen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Sijn min is <rhyme label="b" type="f" corresp="#s7:l2">onghemeten</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Jhesus sel my soe wel <rhyme label="a" type="m" corresp="#s7:l1">ontfaen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Sijn min is <rhyme label="b" type="f" corresp="#s7:l2">onghemeten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Recht minlick is Sijn fyer <rhyme label="a" type="m">ghelaet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ende Sijn ghenoechlick <rhyme label="b" type="f">wesen</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Hi is mijn troest, mijn <rhyme label="a" type="m" corresp="#s8:l1">toevaerlaet</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hi doet mijn hert <rhyme label="b" type="f" corresp="#s8:l2">ghenesen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Hi is mijn troest, mijn <rhyme label="a" type="m" corresp="#s8:l1">toevaerlaet</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Hi doet mijn hert <rhyme label="b" type="f" corresp="#s8:l2">ghenesen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">In Hem soe vind men al <rhyme label="a" type="m">gheheel</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Soe wat men mach <rhyme label="b" type="f">begheren</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Hy is certeyn mijns hertsen <rhyme label="a" type="m" corresp="#s9:l1">greyn</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Hi doet mijn hert <rhyme label="b" type="f" corresp="#s9:l2">verbliden</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Hy is certeyn mijns hertsen <rhyme label="a" type="m" corresp="#s9:l1">greyn</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Hi doet mijn hert <rhyme label="b" type="f" corresp="#s9:l2">verbliden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Suverlijck is Sijn claer <rhyme label="a" type="m">aenschijn</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Boven hondert dusent <rhyme label="b" type="f">sinnen</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Sijn minne verblijt dat herte <rhyme label="a" type="m" corresp="#s10:l1">mijn</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">This recht dat ic Hem <rhyme label="b" type="f" corresp="#s10:l2">minne</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Sijn minne verblijt dat herte <rhyme label="a" type="m" corresp="#s10:l1">mijn</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">This recht dat ic Hem <rhyme label="b" type="f" corresp="#s10:l2">minne</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Trou is Hi, in alre <rhyme label="a" type="m">noet</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Sijn minnaers tot allen <rhyme label="b" type="f">tijden</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Hij staet hem by in alre <rhyme label="a" type="m" corresp="#s11:l1">noet</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Ende doetse seer <rhyme label="b" type="f" corresp="#s11:l2">verbliden</rhyme>.</l>
        <l id="s11:l5" met="4" type="-+">Hij staet hem by in alre <rhyme label="a" type="m" corresp="#s11:l1">noet</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Ende doetse seer <rhyme label="b" type="f" corresp="#s11:l2">verbliden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Vrienden ende maghen gaen hem <rhyme label="a" type="m">oef</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Ende nemen die aertsche <rhyme label="b" type="f">haven</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Die werelt leent hem alleen een <rhyme label="a" type="m" corresp="#s12:l1">graft</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Daer wort hi in <rhyme label="b" type="f" corresp="#s12:l2">begraven</rhyme>.</l>
        <l id="s12:l5" met="4" type="-+">Die werelt leent hem alleen een <rhyme label="a" type="m" corresp="#s12:l1">graft</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Daer wort hi in <rhyme label="b" type="f" corresp="#s12:l2">begraven</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Sijn minres vergheten hem <rhyme label="a" type="m">haestelyck</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Ende kyesen schyer een <rhyme label="b" type="f">ander</rhyme>.</l>
        <l id="s13:l3" met="4" type="-+">Mer Jhesus min duert <rhyme label="a" type="m" corresp="#s13:l1">ewelijck</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Want Hi en doet gheen <rhyme label="b" type="f" corresp="#s13:l2">verander</rhyme>.</l>
        <l id="s13:l5" met="4" type="-+">Mer Jhesus min duert <rhyme label="a" type="m" corresp="#s13:l1">ewelijck</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Want Hi en doet gheen <rhyme label="b" type="f" corresp="#s13:l2">verander</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
