    <tags>
      <tag>christmas song</tag>
      <tag>new year song</tag>
    </tags>
    <melody>De lustelijke mei met zijn bladen vrij</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Voerblyt u met <rhyme label="a" type="m">eendracht</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Want ons is op kors <rhyme label="a" type="m" corresp="#s1:l1">nacht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">een soen geboeren <rhyme label="b" type="m">eet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">van een maecht doer goedt <rhyme label="a" type="m" corresp="#s1:l1">kracht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">al uit daeuits <rhyme label="a" type="m" corresp="#s1:l1">geslacht</rhyme></l>
        <l id="s1:l6" met="3" type="-+">te betteleeiem seer <rhyme label="b" type="m" corresp="#s1:l3">snel</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Deesen <rhyme label="b" type="m" corresp="#s1:l3">eemaenuewel</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Is onsen <rhyme label="c" type="m">middelaer</rhyme></l>
        <l id="s1:l9" met="3" type="-+">en tsaet twelck breeck seer <rhyme label="b" type="m" corresp="#s1:l3">snel</rhyme></l>
        <l id="s1:l10" met="3" type="-+">tsarpens doot en <rhyme label="b" type="m" corresp="#s1:l3">hel</rhyme></l>
        <l id="s1:l11" met="3" type="+-">al in het niewe <rhyme label="c" type="m" corresp="#s1:l8">Iaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De harderkens opt <rhyme label="a" type="m">velt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Werde dees maer <rhyme label="a" type="m" corresp="#s2:l1">voertelt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">van dengelen <rhyme label="b" type="m">serteyn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">sy loef sange dees <rhyme label="a" type="m" corresp="#s2:l1">helt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">synyoen heeft <rhyme label="a" type="m" corresp="#s2:l1">voermelt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">aensiende dit kint <rhyme label="b" type="m" corresp="#s2:l3">kleen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Heer laet u knecht In <rhyme label="b" type="m" corresp="#s2:l3">vreen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">drie Koeningen <rhyme label="c" type="m">eerbaer</rhyme></l>
        <l id="s2:l9" met="3" type="-+">quaemen hem oeck aen <rhyme label="b" type="m" corresp="#s2:l3">been</rhyme></l>
        <l id="s2:l10" met="3" type="-+">doet soe mee al <rhyme label="b" type="m" corresp="#s2:l3">geemeen</rhyme></l>
        <l id="s2:l11" met="3" type="+-">al In het niewe <rhyme label="c" type="m" corresp="#s2:l8">Iaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy laegen doet ter <rhyme label="a" type="m">neer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Oeck mach nu krygen <rhyme label="a" type="m" corresp="#s3:l1">weer</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Doer dit kint tsleuens <rhyme label="b" type="m">kroen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">nae sins vaeders <rhyme label="a" type="m" corresp="#s3:l1">begeer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">ontfinck hy prys en <rhyme label="a" type="m" corresp="#s3:l1">eer</rhyme></l>
        <l id="s3:l6" met="3" type="-+">een stem riep uit den <rhyme label="b" type="m" corresp="#s3:l3">troen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">dit Is myn lieue <rhyme label="b" type="m" corresp="#s3:l3">soen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In hem heb Ick <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s3:l9" met="3" type="-+">een wel behegen <rhyme label="b" type="m" corresp="#s3:l3">schoen</rhyme></l>
        <l id="s3:l10" met="3" type="-+">hoert dees hout syn <rhyme label="b" type="m" corresp="#s3:l3">geboen</rhyme></l>
        <l id="s3:l11" met="3" type="+-">al in het niewe <rhyme label="c" type="m" corresp="#s3:l8">Iaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doer deesen nieu <rhyme label="a" type="m">geboert</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Is aedams schult <rhyme label="a" type="m" corresp="#s4:l1">voorsmoert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">wy hebben vree met <rhyme label="b" type="m">godt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Looft nu toch heilich <rhyme label="a" type="m" corresp="#s4:l1">voert</rhyme></l>
        <l id="s4:l5" met="3" type="-+">op dit vast proefeets <rhyme label="a" type="m" corresp="#s4:l1">woert</rhyme></l>
        <l id="s4:l6" met="3" type="-+">hebt acht al nae <rhyme label="b" type="m" corresp="#s4:l3">tgebodt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">want tout moet syn <rhyme label="b" type="m" corresp="#s4:l3">voerrot</rhyme></l>
        <l id="s4:l8" met="3" type="-+">en tnieu weer aen seer <rhyme label="c" type="m">klaer</rhyme></l>
        <l id="s4:l9" met="3" type="-+">volstandich sonder <rhyme label="b" type="m" corresp="#s4:l3">spot</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Dan hebt gy tbeste <rhyme label="b" type="m" corresp="#s4:l3">lot</rhyme></l>
        <l id="s4:l11" met="3" type="+-">al in het niewe <rhyme label="c" type="m" corresp="#s4:l8">Iaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dees niewe prinse <rhyme label="a" type="m">pryst</rhyme></l>
        <l id="s5:l2" met="3" type="-+">hem eer lof danck <rhyme label="a" type="m" corresp="#s5:l1">betwist</rhyme></l>
        <l id="s5:l3" met="3" type="-+">met spel sanck hart en <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">maeckt dat gy nae <rhyme label="a" type="m" corresp="#s5:l1">voerryst</rhyme></l>
        <l id="s5:l5" met="3" type="-+">by hem die siel lyt <rhyme label="a" type="m" corresp="#s5:l1">spyst</rhyme></l>
        <l id="s5:l6" met="3" type="-+">al uit genaede <rhyme label="b" type="m" corresp="#s5:l3">goedt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dees Iesus Kristus <rhyme label="b" type="m" corresp="#s5:l3">doet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">wech al ons sonde <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Laet ons In liefde <rhyme label="b" type="m" corresp="#s5:l3">soet</rhyme></l>
        <l id="s5:l10" met="3" type="-+">nu doet oprechte <rhyme label="b" type="m" corresp="#s5:l3">boet</rhyme></l>
        <l id="s5:l11" met="3" type="+-">al In het niewe <rhyme label="c" type="m" corresp="#s5:l8">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
