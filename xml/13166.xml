<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13166</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Het is al gedaan mijn oostwaarts gaan</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">GElijck een <rhyme label="a" type="m">Blom</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Uyt niet ick <rhyme label="a" type="m" corresp="#s1:l1">kom</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Van G odt <rhyme label="b" type="f">almachtigh</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">De wereldt <rhyme label="c" type="m">meer</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Als mijnen <rhyme label="c" type="m" corresp="#s1:l4">Heer</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Ick ben <rhyme label="b" type="f" corresp="#s1:l3">gedachtigh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">ICk niet en <rhyme label="a" type="m">ken</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Waer toe ick <rhyme label="a" type="m" corresp="#s2:l1">ben</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Van hem <rhyme label="b" type="f">geschapen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Als ick den <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Alsoo <rhyme label="c" type="m" corresp="#s2:l4">verslijt</rhyme></l>
        <l id="s2:l6" met="2" type="-+">In droom, en <rhyme label="b" type="f" corresp="#s2:l3">slapen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">DEn will' is <rhyme label="a" type="m">blint</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Die niet <rhyme label="a" type="m" corresp="#s3:l1">bemint</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Het eeuwigh <rhyme label="b" type="f">leven</rhyme>;</l>
        <l id="s3:l4" met="2" type="-+">Maer meerder <rhyme label="c" type="m">hout</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Van 't aerdtsche <rhyme label="c" type="m" corresp="#s3:l4">gout</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Daer toe <rhyme label="b" type="f" corresp="#s3:l3">gegeven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">O Mijn <rhyme label="a" type="m">gesicht</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Na 't eeuwigh <rhyme label="a" type="m" corresp="#s4:l1">licht</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Van Godt <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Een <rhyme label="c" type="m">Berzabee</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Van dese <rhyme label="c" type="m" corresp="#s4:l4">ree</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Heeft u <rhyme label="b" type="f" corresp="#s4:l3">verdreven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">VOor mijnen <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s5:l2" met="2" type="-+">En sijn <rhyme label="a" type="m" corresp="#s5:l1">gebodt</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Ick sluyt mijn <rhyme label="b" type="f">ooren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Die open <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">En hooren <rhyme label="c" type="m" corresp="#s5:l4">aen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Die my <rhyme label="b" type="f" corresp="#s5:l3">bekooren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">SOo langh my <rhyme label="a" type="m">dwaes</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Een ydel <rhyme label="a" type="m" corresp="#s6:l1">aes</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Noch kan <rhyme label="b" type="f">vermaken</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Den geest soo <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Van Godt soo <rhyme label="c" type="m" corresp="#s6:l4">goet</rhyme></l>
        <l id="s6:l6" met="2" type="-+">En kan niet <rhyme label="b" type="f" corresp="#s6:l3">smaken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">DEn minsten <rhyme label="a" type="m">tocht</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Of quade <rhyme label="a" type="m" corresp="#s7:l1">locht</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Terstont doet <rhyme label="b" type="f">klagen</rhyme>.</l>
        <l id="s7:l4" met="2" type="-+">Hoe sal ick <rhyme label="c" type="m">kranck</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Der sonden <rhyme label="c" type="m" corresp="#s7:l4">stanck</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Hier na <rhyme label="b" type="f" corresp="#s7:l3">verdragen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">HEt is my <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Voor 't werelts <rhyme label="a" type="m" corresp="#s8:l1">goet</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">En eer te <rhyme label="b" type="f">strijden</rhyme>.</l>
        <l id="s8:l4" met="2" type="-+">Het doet my <rhyme label="c" type="m">seer</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Voor mijnen <rhyme label="c" type="m" corresp="#s8:l4">Heer</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Soo veel te <rhyme label="b" type="f" corresp="#s8:l3">lijden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
