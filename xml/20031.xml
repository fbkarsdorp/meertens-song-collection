    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">A Lentree de ce <rhyme label="a" type="m">bois</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Ou l’amour dort quelque <rhyme label="a" type="m" corresp="#s1:l1">fois</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Le tient de <rhyme label="b" type="m">Madelon</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vermeil comme une <rhyme label="c" type="f">Rose</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ma fait voir en <rhyme label="d" type="m">dormant</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Encore tout autre <rhyme label="c" type="f" corresp="#s1:l4">chose</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">I’ ay creu derriere un <rhyme label="a" type="m">buisson</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Tenir cette <rhyme label="a" type="m" corresp="#s2:l1">Madelon</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ma main sur son <rhyme label="b" type="m">teton</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Lors que je luy <rhyme label="c" type="f">propose</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Que le Jeu de l’ <rhyme label="d" type="m">amour</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Est encore autre <rhyme label="c" type="f" corresp="#s2:l4">chose</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dieux que de <rhyme label="a" type="m">ravissement</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Mais en songe <rhyme label="a" type="m" corresp="#s3:l1">seulement</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Je pensois la <rhyme label="b" type="m">baiser</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Mais presque hors d’ <rhyme label="c" type="f">Haleine</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Je me suis <rhyme label="d" type="m">eveille</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Bien loingh de <rhyme label="c" type="f" corresp="#s3:l4">Madeleine</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Je te jure <rhyme label="a" type="m">Madelon</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Que ce songhe estoit si <rhyme label="a" type="m" corresp="#s4:l1">bon</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Je voudrois de bon <rhyme label="b" type="m">Coeur</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Que tu eusse le <rhyme label="c" type="f">meme</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Tu dirois en <rhyme label="d" type="m">effet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ton Amour est <rhyme label="c" type="f" corresp="#s4:l4">extreme</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">He que vous semble mon <rhyme label="a" type="m">coeur</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Amour n’ est il pas <rhyme label="a" type="m" corresp="#s5:l1">vainqueur</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Il n’a point de <rhyme label="b" type="m">repos</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Sil n’a rendu <rhyme label="c" type="f">contente</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">L’ ame qui scait <rhyme label="d" type="m">aimer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Vne si belle <rhyme label="c" type="f" corresp="#s5:l4">Amante</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
