<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>175996</id>
    <date>
      <year-start>1640</year-start>
      <year-end>1640</year-end>
    </date>
    <location>
      <orginal-name>deventer</orginal-name>
      <standard-name>Deventer, The Netherlands</standard-name>
      <coordinates>
        <lon>52.2532272</lon>
        <lat>6.1584725</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>Psalm 038 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlt in overgrooten <rhyme label="a" type="f">tooren</rhyme></l>
        <l id="s1:l2" met="2" type="+-">V niet <rhyme label="a" type="f" corresp="#s1:l1">stooren</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Over mijne sware <rhyme label="b" type="m">sond'</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Vwe grimmicheyt doch <rhyme label="c" type="f">keere</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Van my, <rhyme label="c" type="f" corresp="#s1:l4">Heere</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">In dees jammerlijcke <rhyme label="b" type="m" corresp="#s1:l3">stond'</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Want u pijlen <rhyme label="a" type="f">voort-gedreven</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Vaste <rhyme label="a" type="f" corresp="#s2:l1">cleven</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Binnen in mijn <rhyme label="b" type="m">ingewant</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Vwe straffen op my <rhyme label="c" type="f">dalen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Harde <rhyme label="c" type="f" corresp="#s2:l4">qualen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Lijd' ick van u slaende <rhyme label="b" type="m" corresp="#s2:l3">hant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Mijne ledematen <rhyme label="a" type="f">steken</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Vol <rhyme label="a" type="f" corresp="#s3:l1">gebreken</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Door u toorn en <rhyme label="b" type="m">ongedult</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Gants ontrustet zijn mijn <rhyme label="c" type="f">beenen</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Tsaem met <rhyme label="c" type="f" corresp="#s3:l4">eenen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Om mijn doodelijcke <rhyme label="b" type="m" corresp="#s3:l3">schult</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Mijner sonden groote <rhyme label="a" type="f">sommen</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Zijn <rhyme label="a" type="f" corresp="#s4:l1">geclommen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Al veel hooger dan mijn <rhyme label="b" type="m">hooft</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Met het pack van mijn <rhyme label="c" type="f">misdaden</rhyme></l>
        <l id="s4:l5" met="2" type="+-"><rhyme label="c" type="f" corresp="#s4:l4">Overladen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Lig' ick mat en <rhyme label="b" type="m" corresp="#s4:l3">afgeslooft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Mijne sweyren my <rhyme label="a" type="f">vermoeyen</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Die daer <rhyme label="a" type="f" corresp="#s5:l1">vloeyen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Vuyl van etter ende <rhyme label="b" type="m">bloet</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Om mijn dwaesheyt moet ick <rhyme label="c" type="f">dragen</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Dese <rhyme label="c" type="f" corresp="#s5:l4">plagen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En al dese <rhyme label="b" type="m" corresp="#s5:l3">tegenspoet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ick ben soo ter neer <rhyme label="a" type="f">gedrucket</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Dat <rhyme label="a" type="f" corresp="#s6:l1">gebucket</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Ick my totter aerden <rhyme label="b" type="m">cromm'</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Ia ick moet geheele <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s6:l5" met="2" type="+-">Rouwe <rhyme label="c" type="f" corresp="#s6:l4">dragen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En int swarte cleet gaen <rhyme label="b" type="m" corresp="#s6:l3">om</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Want al mijne <rhyme label="a" type="f">ingewanden</rhyme></l>
        <l id="s7:l2" met="2" type="+-">In my <rhyme label="a" type="f" corresp="#s7:l1">branden</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Door de pijne die ick <rhyme label="b" type="m">ly</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Mijne leden <rhyme label="c" type="f">altemale</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Sijn vol <rhyme label="c" type="f" corresp="#s7:l4">quale</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Niet gesonts en is aen <rhyme label="b" type="m" corresp="#s7:l3">my</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ick die ras en cloeck voor <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Plach te <rhyme label="a" type="f" corresp="#s8:l1">wesen</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Ben gebroken gants <rhyme label="b" type="m">ontwee</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Dies ick door ʼtgeruys mijns <rhyme label="c" type="f">herten</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Vol van <rhyme label="c" type="f" corresp="#s8:l4">smerten</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Stedes roepe, ach en <rhyme label="b" type="m" corresp="#s8:l3">wee</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Al het geen dat ick <rhyme label="a" type="f">begeere</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">O mijn <rhyme label="a" type="f" corresp="#s9:l1">Heere</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Is voor u ontdeckt en <rhyme label="b" type="m">claer</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">Al mijn droevige <rhyme label="c" type="f">gedachten</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">En mijn <rhyme label="c" type="f" corresp="#s9:l4">clachten</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Zijn voor u gants <rhyme label="b" type="m" corresp="#s9:l3">openbaer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">ʼtHerte in my <rhyme label="a" type="f">omgedreven</rhyme></l>
        <l id="s10:l2" met="2" type="+-">Is aen <rhyme label="a" type="f" corresp="#s10:l1">ʼtbeven</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Mijne sterckte my <rhyme label="b" type="m">verlaet</rhyme>.</l>
        <l id="s10:l4" met="4" type="+-">Om te sien, mijn duyst're <rhyme label="c" type="f">oogen</rhyme></l>
        <l id="s10:l5" met="2" type="+-">Niet <rhyme label="c" type="f" corresp="#s10:l4">vermogen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Hare claerheyt heel <rhyme label="b" type="m" corresp="#s10:l3">vergaet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Mijne vrienden staen ter <rhyme label="a" type="f">syden</rhyme>,</l>
        <l id="s11:l2" met="2" type="+-">En mijn <rhyme label="a" type="f" corresp="#s11:l1">lijden</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Sien sy onbarmhertich <rhyme label="b" type="m">aen</rhyme>.</l>
        <l id="s11:l4" met="4" type="+-">Die my waren <rhyme label="c" type="f">d'aldernaeste</rhyme></l>
        <l id="s11:l5" met="2" type="+-">Syn met <rhyme label="c" type="f" corresp="#s11:l4">haeste</rhyme></l>
        <l id="s11:l6" met="4" type="+-">Verre van my <rhyme label="b" type="m" corresp="#s11:l3">afgegaen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Die mijn ziele netten <rhyme label="a" type="f">stellen</rhyme></l>
        <l id="s12:l2" met="2" type="+-">My te <rhyme label="a" type="f" corresp="#s12:l1">vellen</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">En my gonnen alle <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Leggen my bedeckte <rhyme label="c" type="f">lagen</rhyme></l>
        <l id="s12:l5" met="2" type="+-">Gantsche <rhyme label="c" type="f" corresp="#s12:l4">dagen</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Houdend' over my den <rhyme label="b" type="m" corresp="#s12:l3">raet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">En ick, als met doove <rhyme label="a" type="f">ooren</rhyme></l>
        <l id="s13:l2" met="2" type="+-">Sonder <rhyme label="a" type="f" corresp="#s13:l1">hooren</rhyme></l>
        <l id="s13:l3" met="4" type="+-">Onbeweget blijve <rhyme label="b" type="m">staen</rhyme>.</l>
        <l id="s13:l4" met="4" type="+-">Als of ick niet spreken <rhyme label="c" type="f">conde</rhyme></l>
        <l id="s13:l5" met="2" type="+-">Wt den <rhyme label="c" type="f" corresp="#s13:l4">monde</rhyme></l>
        <l id="s13:l6" met="4" type="+-">Laet ick niet een woort <rhyme label="b" type="m" corresp="#s13:l3">ontgaen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Ia, ick ben gelijck als <rhyme label="a" type="f">eene</rhyme></l>
        <l id="s14:l2" met="2" type="+-">Die gants <rhyme label="a" type="f" corresp="#s14:l1">gene</rhyme></l>
        <l id="s14:l3" met="4" type="+-">Tale noch gehoor en <rhyme label="b" type="m">heeft</rhyme>:</l>
        <l id="s14:l4" met="4" type="+-">Als een mensch van wien het <rhyme label="c" type="f">spreken</rhyme></l>
        <l id="s14:l5" met="2" type="+-">Is <rhyme label="c" type="f" corresp="#s14:l4">geweken</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Die geen tegen-reden <rhyme label="b" type="m" corresp="#s14:l3">geeft</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Maer ick wil volstandich <rhyme label="a" type="f">bouwen</rhyme></l>
        <l id="s15:l2" met="2" type="+-">Mijn <rhyme label="a" type="f" corresp="#s15:l1">vertrouwen</rhyme></l>
        <l id="s15:l3" met="4" type="+-">Heere God, alleen op <rhyme label="b" type="m">dy</rhyme>.</l>
        <l id="s15:l4" met="4" type="+-">Ghy sult met geneygde <rhyme label="c" type="f">ooren</rhyme></l>
        <l id="s15:l5" met="2" type="+-">My oock <rhyme label="c" type="f" corresp="#s15:l4">hooren</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">Dat mijn hoop niet ydel <rhyme label="b" type="m" corresp="#s15:l3">zy</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Laet doch nimmermeer <rhyme label="a" type="f">verblijden</rhyme></l>
        <l id="s16:l2" met="2" type="+-">In mijn <rhyme label="a" type="f" corresp="#s16:l1">lijden</rhyme></l>
        <l id="s16:l3" met="4" type="+-">Die my haten totter <rhyme label="b" type="m">doot</rhyme>.</l>
        <l id="s16:l4" met="4" type="+-">Want als mijne voeten <rhyme label="c" type="f">glijden</rhyme></l>
        <l id="s16:l5" met="2" type="+-">Wat <rhyme label="c" type="f" corresp="#s16:l4">besijden</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">Haren hoochmoet wert te <rhyme label="b" type="m" corresp="#s16:l3">groot</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Ah! van een soo swaren <rhyme label="a" type="f">plage</rhyme></l>
        <l id="s17:l2" met="2" type="+-">Die ick <rhyme label="a" type="f" corresp="#s17:l1">drage</rhyme></l>
        <l id="s17:l3" met="4" type="+-">Bynae hincket mijnen <rhyme label="b" type="m">voet</rhyme>.</l>
        <l id="s17:l4" met="4" type="+-">En mijn overwonnen <rhyme label="c" type="f">herte</rhyme></l>
        <l id="s17:l5" met="2" type="+-">Voelt de <rhyme label="c" type="f" corresp="#s17:l4">smerte</rhyme></l>
        <l id="s17:l6" met="4" type="+-">Die ghy my o Heer <rhyme label="b" type="m" corresp="#s17:l3">aendoet</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">V ick stedes <rhyme label="a" type="f">voren-stelle</rhyme></l>
        <l id="s18:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s18:l1">vertelle</rhyme></l>
        <l id="s18:l3" met="4" type="+-">Gants eenvoudich mijn <rhyme label="b" type="m">misdaet</rhyme>.</l>
        <l id="s18:l4" met="4" type="+-">Ick en doe niet dan <rhyme label="c" type="f">beclagen</rhyme></l>
        <l id="s18:l5" met="2" type="+-">Nachten, <rhyme label="c" type="f" corresp="#s18:l4">dagen</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">Alle mijn bedreven <rhyme label="b" type="m" corresp="#s18:l3">quaet</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Maer mijn wreede haters <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s19:l2" met="2" type="+-">Hooch <rhyme label="a" type="f" corresp="#s19:l1">verheven</rhyme></l>
        <l id="s19:l3" met="4" type="+-">En verstercken langs soo <rhyme label="b" type="m">meer</rhyme>.</l>
        <l id="s19:l4" met="4" type="+-">Sy die my onbillick <rhyme label="c" type="f">haten</rhyme></l>
        <l id="s19:l5" met="2" type="+-"><rhyme label="c" type="f" corresp="#s19:l4">Wter-maten</rhyme></l>
        <l id="s19:l6" met="4" type="+-">Nemen toe in cracht en <rhyme label="b" type="m" corresp="#s19:l3">eer</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Tegen my haer trotsich <rhyme label="a" type="f">stellen</rhyme></l>
        <l id="s20:l2" met="2" type="+-">De <rhyme label="a" type="f" corresp="#s20:l1">gesellen</rhyme></l>
        <l id="s20:l3" met="4" type="+-">Die ick heb veel goets <rhyme label="b" type="m">gedaen</rhyme>.</l>
        <l id="s20:l4" met="4" type="+-">Daerom ist dat sy my <rhyme label="c" type="f">schaden</rhyme></l>
        <l id="s20:l5" met="2" type="+-">En my <rhyme label="c" type="f" corresp="#s20:l4">smaden</rhyme></l>
        <l id="s20:l6" met="4" type="+-">Dat ick gae de rechte <rhyme label="b" type="m" corresp="#s20:l3">baen</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Wilt my niet verlaten, <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s21:l2" met="2" type="+-">Treedt niet <rhyme label="a" type="f" corresp="#s21:l1">veere</rhyme></l>
        <l id="s21:l3" met="4" type="+-">Van my die dus ben <rhyme label="b" type="m">veracht</rhyme>.</l>
        <l id="s21:l4" met="4" type="+-">Dat van my u goetheyt <rhyme label="c" type="f">rijcke</rhyme></l>
        <l id="s21:l5" met="2" type="+-">Niet en <rhyme label="c" type="f" corresp="#s21:l4">wijcke</rhyme>,</l>
        <l id="s21:l6" met="4" type="+-">Want mijn herte u <rhyme label="b" type="m" corresp="#s21:l3">verwacht</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Wilt u eenmael tot my <rhyme label="a" type="f">naken</rhyme>,</l>
        <l id="s22:l2" met="2" type="+-">Richt mijn <rhyme label="a" type="f" corresp="#s22:l1">saken</rhyme>,</l>
        <l id="s22:l3" met="4" type="+-">Wt dit jammer my <rhyme label="b" type="m">bevrijdt</rhyme>.</l>
        <l id="s22:l4" met="4" type="+-">Sonder uytstel u doch <rhyme label="c" type="f">spoede</rhyme>,</l>
        <l id="s22:l5" met="2" type="+-">My te <rhyme label="c" type="f" corresp="#s22:l4">goede</rhyme>,</l>
        <l id="s22:l6" met="4" type="+-">Ghy die mijn verlosser <rhyme label="b" type="m" corresp="#s22:l3">zijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
