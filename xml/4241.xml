<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4241</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag translation="lament">klaaglied</tag>
      <tag translation="love song">liefdeslied</tag>
      <tag translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Branle marquis</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">CRachtich // <rhyme label="a" type="m">noyt</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Geen gesont // hert <rhyme label="b" type="m">gewont</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Als het mijne <rhyme label="c" type="m">onverschoont</rhyme>:</l>
        <l id="s1:l4" met="2" type="+-">Clachtich // <rhyme label="a" type="m" corresp="#s1:l1">oyt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Mijnen mont // gans den <rhyme label="b" type="m" corresp="#s1:l2">gront</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Mijnder sielen heeft <rhyme label="c" type="m" corresp="#s1:l3">verthoont</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Aen een ionghe <rhyme label="d" type="m">maeght</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Die my iaeght // <rhyme label="d" type="m" corresp="#s1:l7">onversaeght</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">En gans desperaet // <rhyme label="e" type="m">laet</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Tis om niet <rhyme label="d" type="m" corresp="#s1:l7">geclaeght</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Haer <rhyme label="d" type="m" corresp="#s1:l7">behaeght</rhyme></l>
        <l id="s1:l12" met="4" type="+-">Desen mijnen droeven <rhyme label="e" type="m" corresp="#s1:l9">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Al dat <rhyme label="a" type="m">in</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Mijnen geest // <rhyme label="b" type="m">onbevreest</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Plach te leven sonder <rhyme label="c" type="m">noot</rhyme>:</l>
        <l id="s2:l4" met="2" type="+-">Heeft den <rhyme label="a" type="m" corresp="#s2:l1">sin</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Aldermeest // met <rhyme label="b" type="m" corresp="#s2:l2">tempeest</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Vast benout, tot inde <rhyme label="c" type="m" corresp="#s2:l3">doot</rhyme>:</l>
        <l id="s2:l7" met="3" type="+-">Eylaes! wie sal <rhyme label="d" type="m">my</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Maken vry // nu dat <rhyme label="d" type="m" corresp="#s2:l7">sy</rhyme></l>
        <l id="s2:l9" met="4" type="+-">My houdt inden last // <rhyme label="e" type="m">vast</rhyme>?</l>
        <l id="s2:l10" met="3" type="+-">die my gaen voor <rhyme label="d" type="m" corresp="#s2:l7">by</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Roupen <rhyme label="d" type="m" corresp="#s2:l7">sy</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Over my ellendich <rhyme label="e" type="m" corresp="#s2:l9">gast</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">T Herte <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Sucht en treurt // angstich <rhyme label="b" type="m">scheurt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">In benoutheyt schier <rhyme label="c" type="m">versmacht</rhyme>:</l>
        <l id="s3:l4" met="2" type="+-">Moet het <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Dus besuert // my <rhyme label="b" type="m" corresp="#s3:l2">gebuert</rhyme></l>
        <l id="s3:l6" met="4" type="+-">(Arme weese) meer dan <rhyme label="c" type="m" corresp="#s3:l3">cracht</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Heb ick metter <rhyme label="d" type="m">daet</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Haren raet // oyt <rhyme label="d" type="m" corresp="#s3:l7">versmaet</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Of geacht te cleen: // <rhyme label="e" type="m">neen</rhyme>?</l>
        <l id="s3:l10" met="3" type="+-">Maer als <rhyme label="d" type="m" corresp="#s3:l7">delicaet</rhyme></l>
        <l id="s3:l11" met="2" type="+-">
          <rhyme label="d" type="m" corresp="#s3:l7">Toeverlaet</rhyme>
        </l>
        <l id="s3:l12" met="4" type="+-">Haer ter noot bekent <rhyme label="e" type="m" corresp="#s3:l9">alleen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Anders <rhyme label="a" type="m">geen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Troost soo soet // die <rhyme label="b" type="m">t'gemoet</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Geeft de ruste <rhyme label="c" type="m">principael</rhyme>:</l>
        <l id="s4:l4" met="2" type="+-">Dan dat <rhyme label="a" type="m" corresp="#s4:l1">reen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Edel bloet // die my <rhyme label="b" type="m" corresp="#s4:l2">doet</rhyme></l>
        <l id="s4:l6" met="4" type="+-">s'Daegs suchten thien <rhyme label="c" type="m" corresp="#s4:l3">dusentmael</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Valt sy my wat <rhyme label="d" type="m">swaer</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Corts hier naer // wert <rhyme label="d" type="m" corresp="#s4:l7">troostbaer</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Al wat sy beleeft // <rhyme label="e" type="m">geeft</rhyme>:</l>
        <l id="s4:l10" met="3" type="+-">Sulcx betrouw' ick <rhyme label="d" type="m" corresp="#s4:l7">haer</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Die <rhyme label="d" type="m" corresp="#s4:l7">voorwaer</rhyme></l>
        <l id="s4:l12" met="4" type="+-">Een goet hert te my-waert <rhyme label="e" type="m" corresp="#s4:l9">heeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Rustich <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Ick met vlijt // zijn <rhyme label="b" type="m">altijt</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Dienstbaer vast in haer <rhyme label="c" type="m">verbont</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">T'sy of <rhyme label="a" type="m" corresp="#s5:l1">al</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Mijn profijt // storm, en <rhyme label="b" type="m" corresp="#s5:l2">strijt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Wesen sal, ter laester <rhyme label="c" type="m" corresp="#s5:l3">stont</rhyme>:</l>
        <l id="s5:l7" met="3" type="+-">Daer my anders <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Dan verdriet // en <rhyme label="d" type="m" corresp="#s5:l7">geschiet</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Wacht ick goet accoort // <rhyme label="e" type="m">voort</rhyme>:</l>
        <l id="s5:l10" met="3" type="+-">Soo sy my <rhyme label="d" type="m" corresp="#s5:l7">aensiet</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">Ende <rhyme label="d" type="m" corresp="#s5:l7">biet</rhyme></l>
        <l id="s5:l12" met="4" type="+-">Een gesontmakende <rhyme label="e" type="m" corresp="#s5:l9">woort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">In mijn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Schoonder kint // noyt <rhyme label="b" type="m">gesint</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Niet aensiende druck, of <rhyme label="c" type="m">noot</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Thisbe <rhyme label="a" type="m" corresp="#s6:l1">wert</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Vierich int // hert <rhyme label="b" type="m" corresp="#s6:l2">bemint</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Van Piramus totter <rhyme label="c" type="m" corresp="#s6:l3">doot</rhyme>:</l>
        <l id="s6:l7" met="3" type="+-">Maer dit <rhyme label="d" type="m">excelent</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Partinent // bloemken <rhyme label="d" type="m" corresp="#s6:l7">ient</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Climt in hooger graet // <rhyme label="e" type="m">iaet</rhyme>!</l>
        <l id="s6:l10" met="3" type="+-">Ben ick een <rhyme label="d" type="m" corresp="#s6:l7">moment</rhyme></l>
        <l id="s6:l11" met="2" type="+-">Haer <rhyme label="d" type="m" corresp="#s6:l7">absent</rhyme></l>
        <l id="s6:l12" met="4" type="+-">Tis een leven <rhyme label="e" type="m" corresp="#s6:l9">desolaet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">NAer dien <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Even vast // groijt en <rhyme label="b" type="m">wast</rhyme></l>
        <l id="s7:l3" met="4" type="+-">In mijn hert sonder af <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">Vind' ick <rhyme label="a" type="m" corresp="#s7:l1">my</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Aengetast // <rhyme label="b" type="m" corresp="#s7:l2">overlast</rhyme></l>
        <l id="s7:l6" met="4" type="+-">(Boven reden) <rhyme label="c" type="m" corresp="#s7:l3">aengedaen</rhyme>:</l>
        <l id="s7:l7" met="3" type="+-">Ick moet even <rhyme label="d" type="m">wel</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Mijn gequel // achten <rhyme label="d" type="m" corresp="#s7:l7">spel</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">Wat baet veel geclach // <rhyme label="e" type="m">ach</rhyme>!</l>
        <l id="s7:l10" met="3" type="+-">Al valt sy <rhyme label="d" type="m" corresp="#s7:l7">rebel</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">Niemant <rhyme label="d" type="m" corresp="#s7:l7">el</rhyme></l>
        <l id="s7:l12" met="4" type="+-">Mijn versuchten eynden <rhyme label="e" type="m" corresp="#s7:l9">mach</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
