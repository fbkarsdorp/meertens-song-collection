<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184244</id>
    <date>
      <year-start>1798</year-start>
      <year-end>1798</year-end>
    </date>
    <tags/>
    <melody>Vrienden luister naar mijn rede</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'t Heb reeds lang langs straat <rhyme label="a" type="f">geloopen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met myn Marsje vol <rhyme label="b" type="m">gelaan</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maar geen mensch wil van my <rhyme label="a" type="f" corresp="#s1:l1">koopen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">'k Zal eens deze straat <rhyme label="b" type="m" corresp="#s1:l2">inslaan</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Koop koop Burger, koop dog <rhyme label="c" type="m">wat</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zie wat Flipjes laars <rhyme label="c" type="m" corresp="#s1:l5">bevat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ik heb Kristalaine <rhyme label="a" type="f">Brillen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Van het alderbeste <rhyme label="b" type="m">slag</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Kom dan die nog kyken <rhyme label="a" type="f" corresp="#s2:l1">willen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">By den avond of den <rhyme label="b" type="m" corresp="#s2:l2">dag</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Ik heb Brillen <rhyme label="c" type="m">Comisso</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ha zie zoo zie zoo zie <rhyme label="c" type="m" corresp="#s2:l5">zoo</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Heb ook Kammen naar de <rhyme label="a" type="f">mode</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Voor de Juffers grof en <rhyme label="b" type="m">syn</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Heeft 'er niemand iets van <rhyme label="a" type="f" corresp="#s3:l1">noode</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Zie hoe schoon mys Kammen <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Krom en regt en nauw en <rhyme label="c" type="m">wyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Koop zoo gy verleegen <rhyme label="c" type="m" corresp="#s3:l5">zyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik heb hier ook beste <rhyme label="a" type="f">Spelde</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Om te steeken in ja <rhyme label="b" type="m">hair</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Meisjes zulken vind men <rhyme label="a" type="f" corresp="#s4:l1">zelden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Franse Flip heeft beste <rhyme label="b" type="m" corresp="#s4:l2">waar</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Staale Spelden groot en <rhyme label="c" type="m">klein</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En ook Naalden glad en <rhyme label="c" type="m" corresp="#s4:l5">rein</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Fransje heeft ook <rhyme label="a" type="f">Naaldekookers</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Van Yvoor en <rhyme label="b" type="m">Paarlemoer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Pypen vroeters Tanden <rhyme label="a" type="f" corresp="#s5:l1">stookers</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Tonteldoosjes voor de <rhyme label="b" type="m" corresp="#s5:l2">Boer</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Karketrekken naar 't <rhyme label="c" type="m">fatzoen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Om de Flessen op te <rhyme label="c" type="m" corresp="#s5:l5">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ha zie zoo ik heb ook <rhyme label="a" type="f">meden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Nieuwe dubbelde <rhyme label="b" type="m">Almanak</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Om den avond te <rhyme label="a" type="f" corresp="#s6:l1">besteeden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Als gy zit op uw <rhyme label="b" type="m" corresp="#s6:l2">gemak</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">In het hoekje van den <rhyme label="c" type="m">haart</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Is dit Boek een Daalder <rhyme label="c" type="m" corresp="#s6:l5">waard</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Koop Tabakdoos 'k heb <rhyme label="a" type="f">verlakten</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">En ook blanken dubbeld <rhyme label="b" type="m">blik</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">'k Heb gestreepten en <rhyme label="a" type="f" corresp="#s7:l1">gevlakten</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Met Medallies aan een <rhyme label="b" type="m" corresp="#s7:l2">strik</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Achtkant vierkant en ook <rhyme label="c" type="m">rond</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Kom by Frans en koop <rhyme label="c" type="m" corresp="#s7:l5">terstond</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ik heb puik van fyne <rhyme label="a" type="f">Schaaren</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Engels staal zoo glad als <rhyme label="b" type="m">glas</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">'k Heb ook heerlyk brabands <rhyme label="a" type="f" corresp="#s8:l1">garen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Van het allerbest <rhyme label="b" type="m" corresp="#s8:l2">vlas</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Rygsnoer feter <rhyme label="c" type="m">Kouseband</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En Pomade naar den <rhyme label="c" type="m" corresp="#s8:l5">trand</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">'k Heb ook Ringen lieve <rhyme label="a" type="f">Vrouwtjes</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Die Gy dragen kunt voor <rhyme label="b" type="m">Goud</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Steek maar toe jou blanke <rhyme label="a" type="f" corresp="#s9:l1">boutjes</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Heb gy fraajer ooit <rhyme label="b" type="m" corresp="#s9:l2">beschouwd</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Kom ik zal ze met <rhyme label="c" type="m">fatsoen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Wel eens aan je Vinger <rhyme label="c" type="m" corresp="#s9:l5">doen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Zoo heb ik van alle <rhyme label="a" type="f">dingen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Koop by Fransje Koop tog <rhyme label="b" type="m">wat</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Ik heb ook Liedjes om te <rhyme label="a" type="f" corresp="#s10:l1">zingen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Welk een zoet de min <rhyme label="b" type="m" corresp="#s10:l2">bevat</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Hoe het lieve Jonge <rhyme label="c" type="m">paar</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Moet verkeeren met <rhyme label="c" type="m" corresp="#s10:l5">malkaar</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Willer niemand van myn <rhyme label="a" type="f">koopen</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Ã´ Dat is een droevig <rhyme label="b" type="m">kruis</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">'k Lag wat met vergeefs te <rhyme label="a" type="f" corresp="#s11:l1">loopen</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Ik gaa regelregt naar <rhyme label="b" type="m" corresp="#s11:l2">huis</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">En hoop dat een andre <rhyme label="c" type="m">dag</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Voor my beter wezen <rhyme label="c" type="m" corresp="#s11:l5">mag</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
