  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WIe zou zoo'n Maagt niet <rhyme label="a" type="f">Minne</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zoo 'n overschoone <rhyme label="b" type="m">Meyd</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">'k Heb mijn Verstand en <rhyme label="a" type="f" corresp="#s1:l1">Zinne</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Alleen op haar <rhyme label="b" type="m" corresp="#s1:l2">geleyd</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Ik wil haar gaan <rhyme label="c" type="f">zoeken</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">En geven mijn <rhyme label="d" type="m">Trouw</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">In Hollen en <rhyme label="c" type="f" corresp="#s1:l5">Hoeken</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Dat zy word mijn <rhyme label="d" type="m" corresp="#s1:l6">Vrouw</rhyme>:</l>
        <l id="s1:l9" met="3" type="+-">Want ik Min <rhyme label="e" type="m">alleen</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Haar, om haar <rhyme label="f" type="m">Schoonheyd</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">En anders <rhyme label="e" type="m" corresp="#s1:l9">geen</rhyme>.</l>
      </lg>
    </lg>
