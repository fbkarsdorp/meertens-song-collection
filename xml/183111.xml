<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183111</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Juich Batavieren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Laet de Faem maer <rhyme label="a" type="f">blaezen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Over Bosch / Berg en <rhyme label="b" type="m">Dal</rhyme> /</l>
        <l id="s1:l3" met="2" type="-+">Laet de Trompetter <rhyme label="a" type="f" corresp="#s1:l1">raesen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Met een bly <rhyme label="b" type="m" corresp="#s1:l2">geschal</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Laet de Klokken fyn en <rhyme label="c" type="m">grof</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">Speelen met een bly <rhyme label="c" type="m" corresp="#s1:l5">gelof</rhyme> /</l>
        <l id="s1:l7" met="6" type="-+">Viva hoezee / Viva Prins Oranje <rhyme label="d" type="m">hoezee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Men hoord uit <rhyme label="a" type="f">Aken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die lang gewenschte <rhyme label="b" type="m">vreugd</rhyme> /</l>
        <l id="s2:l3" met="2" type="-+">Door het <rhyme label="a" type="f" corresp="#s2:l1">vermaken</rhyme> /</l>
        <l id="s2:l4" met="2" type="-+">Ons hert <rhyme label="b" type="m" corresp="#s2:l2">verheugd</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Wie had het ooit <rhyme label="c" type="m">gedagt</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">Die vreugd zo <rhyme label="c" type="m" corresp="#s2:l5">onverwagt</rhyme>:</l>
        <l id="s2:l7" met="6" type="-+">Viva hoezee / Viva Prins Oranje <rhyme label="d" type="m">hoezee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Nu zal <rhyme label="a" type="f">floreere</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Koopluy en <rhyme label="b" type="m">Ambagtsman</rhyme> /</l>
        <l id="s3:l3" met="2" type="-+">'t Welk God den <rhyme label="a" type="f" corresp="#s3:l1">Heere</rhyme> /</l>
        <l id="s3:l4" met="2" type="-+">Geeft in ons <rhyme label="b" type="m" corresp="#s3:l2">Land</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Vivat de vreugde <rhyme label="c" type="m">lof</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">Klinkt in het <rhyme label="c" type="m" corresp="#s3:l5">Hemelhof</rhyme> /</l>
        <l id="s3:l7" met="6" type="-+">Viva hoezee / Viva Prins Oranje <rhyme label="d" type="m">hoezee</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">God wil <rhyme label="a" type="f">bewaere</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ons Prins en <rhyme label="b" type="m">Princes</rhyme> /</l>
        <l id="s4:l3" met="2" type="-+">Haer teerheid <rhyme label="a" type="f" corresp="#s4:l1">spaere</rhyme> /</l>
        <l id="s4:l4" met="2" type="-+">Ik wensch haer 't <rhyme label="b" type="m" corresp="#s4:l2">best</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Dat Godes <rhyme label="c" type="m">vuurcolom</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">Beschermd haer <rhyme label="c" type="m" corresp="#s4:l5">rondom</rhyme>/</l>
        <l id="s4:l7" met="6" type="-+">Viva hoezee / Viva Prins Oranje <rhyme label="d" type="m">hoezee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">God die geeft de <rhyme label="a" type="f">zegen</rhyme> /</l>
        <l id="s5:l2" met="3" type="-+">Over 't heele Huis <rhyme label="b" type="m">Nassouw</rhyme> /</l>
        <l id="s5:l3" met="2" type="-+">Word haer <rhyme label="a" type="f" corresp="#s5:l1">genegen</rhyme> /</l>
        <l id="s5:l4" met="2" type="-+">En daer toe <rhyme label="b" type="m" corresp="#s5:l2">getrouw</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Verspreid doch uwen <rhyme label="c" type="m">Naem</rhyme> /</l>
        <l id="s5:l6" met="3" type="-+">Maekt haer daer toe <rhyme label="c" type="m" corresp="#s5:l5">bekwaem</rhyme> /</l>
        <l id="s5:l7" met="6" type="-+">Viva hoezee / Viva Prins Oranje <rhyme label="d" type="m">hoezee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">God die wil <rhyme label="a" type="f">geeven</rhyme> /</l>
        <l id="s6:l2" met="3" type="-+">Dat hy zyn Vaders <rhyme label="b" type="m">gang</rhyme> /</l>
        <l id="s6:l3" met="2" type="-+">Gelyk mag <rhyme label="a" type="f" corresp="#s6:l1">leeven</rhyme> /</l>
        <l id="s6:l4" met="2" type="-+">Nog jaeren <rhyme label="b" type="m" corresp="#s6:l2">lang</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Den Heer geeft hem <rhyme label="c" type="m">verstand</rhyme> /</l>
        <l id="s6:l6" met="3" type="-+">'t Is 't hoofd al van ons <rhyme label="c" type="m" corresp="#s6:l5">Land</rhyme> /</l>
        <l id="s6:l7" met="6" type="-+">Viva hoezee / Viva Prins Oranje <rhyme label="d" type="m">hoezee</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Speeld nu op <rhyme label="a" type="f">fluiten</rhyme> /</l>
        <l id="s7:l2" met="3" type="-+">Op hobo en <rhyme label="b" type="m">schalmey</rhyme> /</l>
        <l id="s7:l3" met="2" type="-+">Laet u ooren <rhyme label="a" type="f" corresp="#s7:l1">tuiten</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Meest vrolyk en <rhyme label="b" type="m" corresp="#s7:l2">bly</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Om die Oranje <rhyme label="c" type="m">Stam</rhyme> /</l>
        <l id="s7:l6" met="3" type="-+">Die den agtsten Maert <rhyme label="c" type="m" corresp="#s7:l5">aenkwam</rhyme> /</l>
        <l id="s7:l7" met="6" type="-+">Viva hoezee / Viva Prins Oranje <rhyme label="d" type="m">hoezee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
