    <melody>Laura zat laatst bij de beek</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ROzemond zag op, en <rhyme label="a" type="m">riep</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met de doodt op hare <rhyme label="b" type="f">lippen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Toen de wang haar verf <rhyme label="a" type="m" corresp="#s1:l1">verschiep</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Boksvoet, wat bediet dit <rhyme label="b" type="f" corresp="#s1:l2">knippen</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Zoek in wouden en op <rhyme label="b" type="f" corresp="#s1:l2">klippen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Andre jacht en <rhyme label="c" type="m">Minnestof</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Zonder onzen slaap te <rhyme label="d" type="f">steuren</rhyme>.</l>
        <l id="s1:l8" met="4" type="+-">Mach ons dan geen rust <rhyme label="d" type="f" corresp="#s1:l7">gebeuren</rhyme>?</l>
        <l id="s1:l9" met="4" type="+-">Och, och, och! dit is te <rhyme label="c" type="m" corresp="#s1:l6">grof</rhyme>.</l>
      </lg>
    </lg>
  </text>
