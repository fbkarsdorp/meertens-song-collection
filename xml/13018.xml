    <melody>In Den Haag in het Voorhout</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HA bonjour <rhyme label="a" type="f">Margriete</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">'k Komze jou wat <rhyme label="b" type="f">vrye</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Spijte Bruin <rhyme label="a" type="f" corresp="#s1:l1">Angniete</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Bette, ou <rhyme label="b" type="f" corresp="#s1:l2">Marie</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'k Hebze jou zoo lief <rhyme label="c" type="m">Madaam</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">As Iane doete <rhyme label="c" type="m" corresp="#s1:l5">Guiljaam</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Par ma foy bonne <rhyme label="a" type="f">femme</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Mette blame <rhyme label="b" type="f">borsse</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Meugze jou wel <rhyme label="a" type="f" corresp="#s2:l1">bremme</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Ou de zukke <rhyme label="b" type="f" corresp="#s2:l2">worsse</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">'k Zels jou geef de zuik're <rhyme label="c" type="m">zoet</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Ha, pour vous zoo noble <rhyme label="c" type="m" corresp="#s2:l5">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">O! 'k zels jou zoo <rhyme label="a" type="f">kusze</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Alle nact, ou <rhyme label="b" type="f">daakke</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Watte jou zels <rhyme label="a" type="f" corresp="#s3:l1">lusze</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Es pour vous <rhyme label="b" type="f" corresp="#s3:l2">emaakke</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Lekke wijn est il <rhyme label="c" type="m">pavreé</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Lanke slaap, ne pas <rhyme label="c" type="m" corresp="#s3:l5">fileé</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Ha! ma bon <rhyme label="a" type="f">Matresze</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Wilze jou <rhyme label="b" type="f">brageeren</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">As de Haags <rhyme label="a" type="f" corresp="#s4:l1">Princesze</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Mette zyze <rhyme label="b" type="f" corresp="#s4:l2">kleeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Za allon, en haas jou <rhyme label="c" type="m">wat</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Jou zels prouf van dit, van <rhyme label="c" type="m" corresp="#s4:l5">dat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Kom, maar wilze <rhyme label="a" type="f">trouwe</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Viteman <rhyme label="b" type="f">Koriaze</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Moy die zelze <rhyme label="a" type="f" corresp="#s5:l1">houwe</rhyme></l>
        <l id="s5:l4" met="3" type="+-">A bien bon <rhyme label="b" type="f" corresp="#s5:l2">mariaze</rhyme></l>
        <l id="s5:l5" met="4" type="+-">'k Zels jou vingt cinq foy 's naks <rhyme label="c" type="m">draay</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Kom slaap in mijn armken <rhyme label="c" type="m" corresp="#s5:l5">gaay</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Serviteur mon <rhyme label="a" type="f">Engle</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Tot le jour van <rhyme label="b" type="f">morgen</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">'k Zels jou en de <rhyme label="a" type="f" corresp="#s6:l1">bengle</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Van de zoete <rhyme label="b" type="f" corresp="#s6:l2">borgen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">En dan geef de lekke <rhyme label="c" type="m">dink</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Mette gouwe trouwe <rhyme label="c" type="m" corresp="#s6:l5">Rink</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">'k Zels zek aan mon <rhyme label="a" type="f">pere</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Mammeer, ou ma <rhyme label="b" type="f">filjes</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Grampeer, ou <rhyme label="a" type="f" corresp="#s7:l1">grammere</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Tout le rest an <rhyme label="b" type="f" corresp="#s7:l2">viljes</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Par ma foy bents jy niet <rhyme label="c" type="m">moe</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">'k Slaats de koop pour trouwe <rhyme label="c" type="m" corresp="#s7:l5">toe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
