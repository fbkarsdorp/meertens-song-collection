<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11417</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="praise song">loflied</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL wat aen Phoebus <rhyme label="a" type="m">hoort</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Is goud, en goude <rhyme label="a" type="m" corresp="#s1:l1">koort</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Goud is zijn rock <rhyme label="b" type="f">geweven</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Daer mee hy die oock <rhyme label="c" type="m">haeckt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Is van die stof <rhyme label="c" type="m" corresp="#s1:l4">ghemaeckt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met kunstigh werck <rhyme label="b" type="f" corresp="#s1:l3">gedreven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sijn Lier daer hy op <rhyme label="a" type="m">speelt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zijn Veel-boogh die 't spel <rhyme label="a" type="m" corresp="#s2:l1">teelt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sijn snaeren, 't is al <rhyme label="b" type="f">guldich</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">Sijn pijl en koocker <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Sijn boogh en al 't <rhyme label="c" type="m" corresp="#s2:l4">geree</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daer meed' hy <rhyme label="b" type="f" corresp="#s2:l3">menighfuldich</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo menigh Monster <rhyme label="a" type="m">schoot</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En wreede dieren <rhyme label="a" type="m" corresp="#s3:l1">dood</rhyme></l>
        <l id="s3:l3" met="3" type="-+">In 't woud en op de <rhyme label="b" type="f">Velden</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Zijn schoenen zijn van <rhyme label="c" type="m">goud</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zijn Leersjens soo <rhyme label="c" type="m" corresp="#s3:l4">getouwt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hoe kan men 't rijcker <rhyme label="b" type="f" corresp="#s3:l3">melden</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Apollo die is <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wie isser zijns <rhyme label="a" type="m" corresp="#s4:l1">ghelijck</rhyme>?</l>
        <l id="s4:l3" met="3" type="-+">In goud en <rhyme label="b" type="f">kostelheden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En die hem aerdigh <rhyme label="c" type="m">kleet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Soo cierlijck met een <rhyme label="c" type="m" corresp="#s4:l4">weet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Vol Majesteyt oock <rhyme label="b" type="f" corresp="#s4:l3">mede</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daer is hy krachtigh <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ghetuyghen dat zijt <rhyme label="a" type="m" corresp="#s5:l1">ghy</rhyme></l>
        <l id="s5:l3" met="3" type="-+">O Python! vol <rhyme label="b" type="f">verschricken</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Zijn jeughd is als de <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En nimmer aen zijn <rhyme label="c" type="m" corresp="#s5:l4">kin</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Komt hayr, of stoppels <rhyme label="b" type="f" corresp="#s5:l3">blicken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Soo jeughdigh als een <rhyme label="a" type="m">Maeghd</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die noch haer Maeghdom <rhyme label="a" type="m" corresp="#s6:l1">draeght</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Is hy en blijft soo <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Zijn schitterighe <rhyme label="c" type="m">glans</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Zijn groene <rhyme label="c" type="m" corresp="#s6:l4">Lauwer-krans</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Wort over al <rhyme label="b" type="f" corresp="#s6:l3">ghepresen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
