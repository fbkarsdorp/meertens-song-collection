    <melody>Ei wat hoort men vieze grillen</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EY wat hoortmen viese <rhyme label="a" type="f">grillen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Onder ’t Cortisanen <rhyme label="b" type="m">rot</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Hoe komt dat die geen nu <rhyme label="a" type="f" corresp="#s1:l1">willen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die elck hielden voor een <rhyme label="b" type="m" corresp="#s1:l2">Sot</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">‘k Loof het moet de veelheydt <rhyme label="c" type="f">maecken</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Want daer isser soo een <rhyme label="d" type="m">hoop</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dat een yder aen kan <rhyme label="c" type="f" corresp="#s1:l5">raecken</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Tot sijn lust heel goede <rhyme label="d" type="m" corresp="#s1:l6">koop</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maer, se hebben vreemde <rhyme label="a" type="f">grepen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Om een jonge <rhyme label="b" type="m">Melck-muyl</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dickwils in haer nest te <rhyme label="a" type="f" corresp="#s2:l1">slepen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die daer nae siet als een <rhyme label="b" type="m" corresp="#s2:l2">Vyl</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Aerlant seyt, sy soud niet <rhyme label="c" type="f">weygeren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Hadse maer gien <rhyme label="d" type="m">Serviteurs</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Die haer dan ten toon wilt <rhyme label="c" type="f" corresp="#s2:l5">steyg’ren</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Kost het licht een volle <rhyme label="d" type="m" corresp="#s2:l6">beurs</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">’t Is een Vercken argh in ’t <rhyme label="a" type="f">kak’len</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Schoon van wesen en <rhyme label="b" type="m">ghestalt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En haer backhuys doet <rhyme label="a" type="f" corresp="#s3:l1">mirak’len</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Soo dats’ yder een <rhyme label="b" type="m" corresp="#s3:l2">gevalt</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Nae dees Capiteyn mach <rhyme label="c" type="f">sitten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Damoiselle Grand <rhyme label="d" type="m">Monseur</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Langh van lijf doch uyt ten <rhyme label="c" type="f" corresp="#s3:l5">witten</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Geel van hayr, soo staet me <rhyme label="d" type="m" corresp="#s3:l6">veur</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die haer korts niet kost <rhyme label="a" type="f">belyen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Als juyst met een <rhyme label="b" type="m">Kamenier</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om het gelt, van alle <rhyme label="a" type="f" corresp="#s4:l1">zyen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Op te helpen, ’t is een <rhyme label="b" type="m" corresp="#s4:l2">dier</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Die ’t niet als om gelt te doen <rhyme label="c" type="f">is</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Ia al komter dagh’licks <rhyme label="d" type="m">thien</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">(Al hoe wel ’t noch voor den noen <rhyme label="c" type="f" corresp="#s4:l5">is</rhyme>,)</l>
        <l id="s4:l8" met="4" type="+-">‘k Wed dat sy die af sou <rhyme label="d" type="m" corresp="#s4:l6">sien</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Daerom moets’ in ’t smeerbed <rhyme label="a" type="f">leggen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Vier of vijfmael  daer die <rhyme label="b" type="m">Hoer</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Aerlant, achtmael so se <rhyme label="a" type="f" corresp="#s5:l1">seggen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Op de selve wagen <rhyme label="b" type="m" corresp="#s5:l2">voer</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">‘k Meen het is een luck voor <rhyme label="c" type="f">Keesje</rhyme>.</l>
        <l id="s5:l6" met="4" type="+-">Datse uyt de selve <rhyme label="d" type="m">kop</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Niet gelijck dat <rhyme label="c" type="f" corresp="#s5:l5">Pimpelmeesje</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">(‘k Segh haer Suster) dronck sulck <rhyme label="d" type="m" corresp="#s5:l6">sop</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maer ‘k beklaegh dat soete <rhyme label="a" type="f">Meysje</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">’t Schoon Ieudinnetje <rhyme label="b" type="m">Isabel</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat haer soet en teere <rhyme label="a" type="f" corresp="#s6:l1">vleysje</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Nu moet lijden sulck <rhyme label="b" type="m" corresp="#s6:l2">gequel</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">’t Isser sulck een <rhyme label="c" type="f">onghewoonte</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Eerst ge-eert, en nu <rhyme label="d" type="m">bespot</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Eerst heel spijtigh op haer <rhyme label="c" type="f" corresp="#s6:l5">schoonte</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Nu demoedigh op dit <rhyme label="d" type="m" corresp="#s6:l6">Lot</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Agnes pokkigh, lebbigh <rhyme label="a" type="f">vercken</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Die ’t geluck noch gaf een <rhyme label="b" type="m">man</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Ist niet beter eerlijck <rhyme label="a" type="f" corresp="#s7:l1">wercken</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Als men daer van leven <rhyme label="b" type="m" corresp="#s7:l2">kan</rhyme>?</l>
        <l id="s7:l5" met="4" type="+-">Dan voor hoer, langhs ’t Landt te <rhyme label="c" type="f">loopen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Welcken Keerel is so <rhyme label="d" type="m">geck</rhyme>;</l>
        <l id="s7:l7" met="4" type="+-">Dat hy u mach kleeren <rhyme label="c" type="f" corresp="#s7:l5">koopen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Langh-neus, Geelvel, <rhyme label="d" type="m" corresp="#s7:l6">Kevelbeck</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Leelicke Belle <rhyme label="a" type="f">Allemande</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Wie gaf u doch sulcken <rhyme label="b" type="m">naem</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Foey! ’t is immers sond en <rhyme label="a" type="f" corresp="#s8:l1">schande</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Aller leelijckheden <rhyme label="b" type="m" corresp="#s8:l2">kraem</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">‘k Machje grijns by niet <rhyme label="c" type="f">stellen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">‘k Hou noch meer van Mistris <rhyme label="d" type="m">Blom</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Die de Schout met jou quam <rhyme label="c" type="f" corresp="#s8:l5">quellen</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Beyder schoonheydt niemandt <rhyme label="d" type="m" corresp="#s8:l6">wondt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Spilleboutjes leck’re <rhyme label="a" type="f">kinders</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Engheltjes ick gecker <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Want u backhuys doet meer <rhyme label="a" type="f" corresp="#s9:l1">hinders</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Als ’t hooft van Medusa <rhyme label="b" type="m" corresp="#s9:l2">dee</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">’t Ionghste mach noch door de <rhyme label="c" type="f">beugel</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Of het scheelt altijt niet <rhyme label="d" type="m">veel</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">D’Outst’ is buyten allen <rhyme label="c" type="f" corresp="#s9:l5">teugel</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Loopt van dit in dat <rhyme label="d" type="m" corresp="#s9:l6">bordeel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">’k Weet niet hoe dat rijcke <rhyme label="a" type="f">Fockers</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Noch verlieven op je <rhyme label="b" type="m">muyl</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Had ick Anne <rhyme label="a" type="f" corresp="#s10:l1">Bolleschockers</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">‘k Dee voor alle bey gheen <rhyme label="b" type="m" corresp="#s10:l2">ruyl</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Hoewel d’uytghescheere <rhyme label="c" type="f">Besti</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Reets wel thien jaer liep voor <rhyme label="d" type="m">hoer</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Doense dickwils maeckten <rhyme label="c" type="f" corresp="#s10:l5">questi</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Door haer schoonheyt, <rhyme label="d" type="m" corresp="#s10:l6">overschoer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Foey van jou steeckt my de <rhyme label="a" type="f">wallegh</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Als ick aen mijn Heyntje <rhyme label="b" type="m">denck</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Heyntje, Boutje, die mijn <rhyme label="a" type="f" corresp="#s11:l1">ballegh</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Teugelt deur jou ooghen <rhyme label="b" type="m" corresp="#s11:l2">wenck</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Och eer jy waert out <rhyme label="c" type="f">gheworden</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Waer je schoon, en had een <rhyme label="d" type="m">keel</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Die door alle stemmen <rhyme label="c" type="f" corresp="#s11:l5">snorden</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Trots een Sachpijp of een <rhyme label="d" type="m" corresp="#s11:l6">Veel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">I’ eene Suster  was wat <rhyme label="a" type="f">wilder</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">En kreegh Wiltsanck tot <rhyme label="b" type="m">partuyr</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">d’Ander die is vry wat <rhyme label="a" type="f" corresp="#s12:l1">stilder</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Doch het valter ruym soo <rhyme label="b" type="m" corresp="#s12:l2">suyr</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Och die weeu (doch <rhyme label="c" type="f">onbesturven</rhyme>)</l>
        <l id="s12:l6" met="4" type="+-">Suckelt met een kint of <rhyme label="d" type="m">twee</rhyme>;</l>
        <l id="s12:l7" met="4" type="+-">En had sy ’t noch seggen <rhyme label="c" type="f" corresp="#s12:l5">durven</rhyme>,</l>
        <l id="s12:l8" met="4" type="+-">’t Was verlichtingh in haer <rhyme label="d" type="m" corresp="#s12:l6">wee</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Machtelt treckbeck, wat <rhyme label="a" type="f">beletsel</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Maeckt dat niemant u en <rhyme label="b" type="m">roemt</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">‘k Loof niet dat u het <rhyme label="a" type="f" corresp="#s13:l1">blancketsel</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Van dit schoon geselschap <rhyme label="b" type="m" corresp="#s13:l2">doemt</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">‘k Weet wel ghy had noyt de <rhyme label="c" type="f">pocken</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Maer loopt altijdt met een <rhyme label="d" type="m">kraem</rhyme>,</l>
        <l id="s13:l7" met="4" type="+-">Onder ’t vuyle hemt en <rhyme label="c" type="f" corresp="#s13:l5">rocken</rhyme>,</l>
        <l id="s13:l8" met="4" type="+-">Daer sulcks wel uyt kan <rhyme label="d" type="m" corresp="#s13:l6">ontstaen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Truytje met je kleyne <rhyme label="a" type="f">montje</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Was nu onlanghs met het <rhyme label="b" type="m">Kint</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">En ‘t vergingh weer, doch het <rhyme label="a" type="f" corresp="#s14:l1">Bontje</rhyme></l>
        <l id="s14:l4" met="4" type="+-">Is noch even dol <rhyme label="b" type="m" corresp="#s14:l2">ghesint</rhyme>:</l>
        <l id="s14:l5" met="4" type="+-">Jenne van den Bosch, ‘k <rhyme label="c" type="f">vergeetje</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Mostje na de boeijen <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s14:l7" met="4" type="+-">Met jou Serviteur het <rhyme label="c" type="f" corresp="#s14:l5">speetje</rhyme></l>
        <l id="s14:l8" met="4" type="+-">Seer, na dat ick heb <rhyme label="d" type="m" corresp="#s14:l6">verstaen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">O Me Juffrou vander <rhyme label="a" type="f">Dussen</rhyme>,</l>
        <l id="s15:l2" met="4" type="+-">Wat ick soeckje over <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Laetg’ u noch van Ioden <rhyme label="a" type="f" corresp="#s15:l1">kussen</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Helptge Koopluy noch ten <rhyme label="b" type="m" corresp="#s15:l2">val</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Door ’t geschenck van silv’re <rhyme label="c" type="f">vaten</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">En ’t Servis soo dier <rhyme label="d" type="m">betaelt</rhyme>,</l>
        <l id="s15:l7" met="4" type="+-">Daerg’ u dickwils hebt <rhyme label="c" type="f" corresp="#s15:l5">verlaten</rhyme>,</l>
        <l id="s15:l8" met="4" type="+-">‘k Weet niet waer van daen jy ’t <rhyme label="d" type="m" corresp="#s15:l6">haelt</rhyme>?</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Want men schrikt u Muyl <rhyme label="a" type="f">t’aenschouwen</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Och so haest ghy ’t open <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Achtmen u het puyck der <rhyme label="a" type="f" corresp="#s16:l1">Vrouwen</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Gruw’lijck Backhuys, honigh <rhyme label="b" type="m" corresp="#s16:l2">soet</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Leelicker kondt ghy niet <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">Want An Carels ’t swarte <rhyme label="d" type="m">beest</rhyme>,</l>
        <l id="s16:l7" met="4" type="+-">Overtreft u verd in <rhyme label="c" type="f" corresp="#s16:l5">desen</rhyme>,</l>
        <l id="s16:l8" met="4" type="+-">Sy is oock niet schoon <rhyme label="d" type="m" corresp="#s16:l6">geweest</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Juffrou Verstraet, die de <rhyme label="a" type="f">Winckel</rhyme>.</l>
        <l id="s17:l2" met="4" type="+-">Van de Hoerdery <rhyme label="b" type="m">opset</rhyme>,</l>
        <l id="s17:l3" met="4" type="+-">Kiesje soms geen <rhyme label="a" type="f" corresp="#s17:l1">Boere-kinckel</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Voor een Ioncker in het <rhyme label="b" type="m" corresp="#s17:l2">bedt</rhyme>?</l>
        <l id="s17:l5" met="4" type="+-">Als hy maer heeft van de <rhyme label="c" type="f">Schijven</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">En jou rijckelick <rhyme label="d" type="m">betaelt</rhyme>,</l>
        <l id="s17:l7" met="4" type="+-">‘k Weet ghy sult hem wel <rhyme label="c" type="f" corresp="#s17:l5">gherijven</rhyme>,</l>
        <l id="s17:l8" met="4" type="+-">Al was ’t achter om <rhyme label="d" type="m" corresp="#s17:l6">ghehaelt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Och! jy wilter niet van <rhyme label="a" type="f">weten</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">Datje quansuys zijt een <rhyme label="b" type="m">Hoer</rhyme>,</l>
        <l id="s18:l3" met="4" type="+-">Want by daegh bent ghy <rhyme label="a" type="f" corresp="#s18:l1">gheseten</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Als een Ioffer op de <rhyme label="b" type="m" corresp="#s18:l2">vloer</rhyme>,</l>
        <l id="s18:l5" met="4" type="+-">Maer des nachts dan vintmer <rhyme label="c" type="f">Pollen</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">Vrijers, Mannen, yeder <rhyme label="d" type="m">een</rhyme>,</l>
        <l id="s18:l7" met="4" type="+-">In u huys, jae Hoeren, <rhyme label="c" type="f" corresp="#s18:l5">snollen</rhyme>,</l>
        <l id="s18:l8" met="4" type="+-">Is daer even seer <rhyme label="d" type="m" corresp="#s18:l6">ghemeen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Moffinnetje, ick docht niet <rhyme label="a" type="f">omje</rhyme>,</l>
        <l id="s19:l2" met="4" type="+-">‘k Hadje hier vergeten <rhyme label="b" type="m">haest</rhyme>,</l>
        <l id="s19:l3" met="4" type="+-">Doch vergeefme, ziet ick <rhyme label="a" type="f" corresp="#s19:l1">komje</rhyme></l>
        <l id="s19:l4" met="4" type="+-">Noch begroeten op het <rhyme label="b" type="m" corresp="#s19:l2">laetst</rhyme>.</l>
        <l id="s19:l5" met="4" type="+-">Benje noch soo seer <rhyme label="c" type="f">gheneghen</rhyme>,</l>
        <l id="s19:l6" met="4" type="+-">Tot een braven jonghen <rhyme label="d" type="m">quant</rhyme>?</l>
        <l id="s19:l7" met="4" type="+-">Gaeje noch jou <rhyme label="c" type="f" corresp="#s19:l5">hoere-weghen</rhyme>?</l>
        <l id="s19:l8" met="4" type="+-">Doch men eertje als een <rhyme label="d" type="m" corresp="#s19:l6">Sant</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Yder prijst jou schoone <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s20:l2" met="4" type="+-">Doch waer in dat die <rhyme label="b" type="m">bestaen</rhyme>,</l>
        <l id="s20:l3" met="4" type="+-">Daer van hoord ick nimmer <rhyme label="a" type="f" corresp="#s20:l1">reden</rhyme>,</l>
        <l id="s20:l4" met="4" type="+-">‘k Mach ‘er oock niet eens nae <rhyme label="b" type="m" corresp="#s20:l2">raen</rhyme>.</l>
        <l id="s20:l5" met="4" type="+-">Benje wel soo soet van <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s20:l6" met="4" type="+-">Als de Ridderstraetsche <rhyme label="d" type="m">haed</rhyme>,</l>
        <l id="s20:l7" met="4" type="+-">Die in hoerdom sat voor <rhyme label="c" type="f" corresp="#s20:l5">desen</rhyme>,</l>
        <l id="s20:l8" met="4" type="+-">En nu niet veel beter <rhyme label="d" type="m" corresp="#s20:l6">gaet</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Doch sy heeft een man <rhyme label="a" type="f">ghenomen</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Tot heur scherm en tot haer <rhyme label="b" type="m">schut</rhyme>,</l>
        <l id="s21:l3" met="4" type="+-">Als Susan en al de <rhyme label="a" type="f" corresp="#s21:l1">vromen</rhyme>,</l>
        <l id="s21:l4" met="4" type="+-">Oock Meniste An de <rhyme label="b" type="m" corresp="#s21:l2">Slut</rhyme>.</l>
        <l id="s21:l5" met="4" type="+-">Dorst ick, get wat sou ‘ker <rhyme label="c" type="f">noemen</rhyme>,</l>
        <l id="s21:l6" met="4" type="+-">Die zijn zieck van sulcken <rhyme label="d" type="m">quaet</rhyme>,</l>
        <l id="s21:l7" met="4" type="+-">Doch men soud my haest <rhyme label="c" type="f" corresp="#s21:l5">verdoemen</rhyme>,</l>
        <l id="s21:l8" met="4" type="+-">Dus ist best dat ick het <rhyme label="d" type="m" corresp="#s21:l6">laet</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Doch Me Juffrou uyt het <rhyme label="a" type="f">Haeghje</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Die nu woont op ’t <rhyme label="b" type="m">Wale-pleyn</rhyme>,</l>
        <l id="s22:l3" met="4" type="+-">Met je scheele oogh, ick <rhyme label="a" type="f" corresp="#s22:l1">vraeghje</rhyme>,</l>
        <l id="s22:l4" met="4" type="+-">Of je bent noch maget <rhyme label="b" type="m" corresp="#s22:l2">reyn</rhyme>?</l>
        <l id="s22:l5" met="4" type="+-">‘k Loof, jy soud het gaeren <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s22:l6" met="4" type="+-">En je wenscht wel datjet <rhyme label="d" type="m">waert</rhyme>,</l>
        <l id="s22:l7" met="4" type="+-">Doch den Haegh heeft jou voor <rhyme label="c" type="f" corresp="#s22:l5">desen</rhyme>,</l>
        <l id="s22:l8" met="4" type="+-">Van jou Maeghdom niet <rhyme label="d" type="m" corresp="#s22:l6">gespaert</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Nu bevind ick u te <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">In den avont vroegh en <rhyme label="b" type="m">laet</rhyme>.</l>
        <l id="s23:l3" met="4" type="+-">Daer de Hoeren gaen met <rhyme label="a" type="f" corresp="#s23:l1">hopen</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Om en om de <rhyme label="b" type="m" corresp="#s23:l2">Kalver-straet</rhyme>,</l>
        <l id="s23:l5" met="4" type="+-">Scheele Lijsbet, en dat <rhyme label="c" type="f">Lijsje</rhyme>,</l>
        <l id="s23:l6" met="4" type="+-">Dat van Gorcum is van <rhyme label="d" type="m">daen</rhyme>,</l>
        <l id="s23:l7" met="4" type="+-">Dat zijn Diertjes, yder <rhyme label="c" type="f" corresp="#s23:l5">Meysje</rhyme>,</l>
        <l id="s23:l8" met="4" type="+-">Roemt vier vijf man af te <rhyme label="d" type="m" corresp="#s23:l6">staen</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Get indien ick voort dorst <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">‘k Noemd’ er hondert op een <rhyme label="b" type="m">ry</rhyme>,</l>
        <l id="s24:l3" met="4" type="+-">Doch ick sal die noch wat <rhyme label="a" type="f" corresp="#s24:l1">sparen</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">‘k Kom die wel hier na eens <rhyme label="b" type="m" corresp="#s24:l2">by</rhyme>,</l>
        <l id="s24:l5" met="4" type="+-">Nu vaert wel ick machje <rhyme label="c" type="f">vlijen</rhyme>,</l>
        <l id="s24:l6" met="4" type="+-">Met je eerelickste <rhyme label="d" type="m">naem</rhyme>,</l>
        <l id="s24:l7" met="4" type="+-">Hoeren, Varckens, Teven, <rhyme label="c" type="f" corresp="#s24:l5">Prijen</rhyme>,</l>
        <l id="s24:l8" met="4" type="+-">‘k Blijf u dienaer <rhyme label="d" type="m" corresp="#s24:l6">al-te-saem</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
