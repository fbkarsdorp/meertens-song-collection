  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">De May // Seer <rhyme label="a" type="m">fray</rhyme></l>
        <l id="s1:l2" met="6" type="-+">Gaet hem soet openbaren, Gaet hem soet <rhyme label="b" type="f">openbaren</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">De Jeucht // Haer <rhyme label="c" type="m">veucht</rhyme></l>
        <l id="s1:l4" met="6" type="-+">Om int groen wt te varen, Om int groen wt te <rhyme label="b" type="f" corresp="#s1:l2">varen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Vergaren, By paren // Dies <rhyme label="d" type="m">zijt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Verblijt // Tis tijt // En mijt // <rhyme label="b" type="f" corresp="#s1:l2">Beswaren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Met v // Lieff <rhyme label="a" type="m">nv</rhyme>,</l>
        <l id="s2:l2" met="6" type="-+">Comt Jongmans gaet spanceeren, Comt Jongmans gaet <rhyme label="b" type="f">spanceeren</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Jonst biet // Const <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s2:l4" met="6" type="-+">Zal v Venus vereeren, Zal v Venus <rhyme label="b" type="f" corresp="#s2:l2">vereeren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Vermeren // Tbegeren // Dies zijt // <rhyme label="d" type="m">Verblijt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tis tijt // Een mijt // <rhyme label="b" type="f" corresp="#s2:l2">Verteeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Met lust // V <rhyme label="a" type="m">rust</rhyme>,</l>
        <l id="s3:l2" met="6" type="-+">By v liefste ghepresen, By v liefste <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Int groen // Zeer <rhyme label="c" type="m">coen</rhyme></l>
        <l id="s3:l4" met="6" type="-+">Moet ghy haer leytsman wesen, Moet ghy haer leytsman <rhyme label="b" type="f" corresp="#s3:l2">wesen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">De besen // Te verlesen // Dies zijt // <rhyme label="d" type="m">verblijt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Tis tijt // En mijt // Het <rhyme label="b" type="f" corresp="#s3:l2">vresen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">U stelt // Int <rhyme label="a" type="m">velt</rhyme></l>
        <l id="s4:l2" met="6" type="-+">Om den derden te iaghen, Om den derden te <rhyme label="b" type="f">iaghen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">En vliet // <rhyme label="c" type="m">Verdriet</rhyme>,</l>
        <l id="s4:l4" met="6" type="-+">Zy sal v ionst toedraghen, Zy sal v ionst <rhyme label="b" type="f" corresp="#s4:l2">toedraghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">V plagen // Vertragen // Dies zijt // <rhyme label="d" type="m">Verblijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En mijt // Tis tijt // Drucx <rhyme label="b" type="f" corresp="#s4:l2">lagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">V last // So <rhyme label="a" type="m">t'past</rhyme>,</l>
        <l id="s5:l2" met="6" type="-+">Sult ghy hier van verbreden, Sult ghy hier van <rhyme label="b" type="f">verbreden</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">So wert // V <rhyme label="c" type="m">smert</rhyme>,</l>
        <l id="s5:l4" met="6" type="-+">Door v om haer gheleden, Door v om haer <rhyme label="b" type="f" corresp="#s5:l2">gheleden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Met reden // Vertreden // Dies zijt, <rhyme label="d" type="m">Verblijt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Tis tijt // En mijt // <rhyme label="b" type="f" corresp="#s5:l2">Droefheden</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Als ghy // Dus <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s6:l2" met="6" type="-+">En vrolijck zijt by bijen, En vrolijck zijt by <rhyme label="b" type="f">bijen</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Sal haer // <rhyme label="c" type="m">Voorwaer</rhyme>,</l>
        <l id="s6:l4" met="6" type="-+">Venus int midden vlijen, Venus int midden <rhyme label="b" type="f" corresp="#s6:l2">vlijen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Gedijen // Salt vrijen // Diet zijt, <rhyme label="d" type="m">verblijt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Tis tijt // En mijt // Het <rhyme label="b" type="f" corresp="#s6:l2">lijen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
