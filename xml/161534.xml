  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Lief, ach! soete <rhyme label="a" type="f">Harderinne</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Afgodesje van mijn <rhyme label="b" type="m">Ziel</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die ik als mijn selfs <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Soetert die mijn wel <rhyme label="b" type="m" corresp="#s1:l2">beviel</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Moet ick steets <rhyme label="c" type="f">klagen</rhyme>?</l>
        <l id="s1:l6" met="2" type="+-">Moet ick steets <rhyme label="c" type="f" corresp="#s1:l5">draghen</rhyme></l>
        <l id="s1:l7" met="4" type="+-">U ongemeene <rhyme label="d" type="f">wrevelheden</rhyme>;</l>
        <l id="s1:l8" met="2" type="+-">Schoon mijn <rhyme label="d" type="f" corresp="#s1:l7">beden</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Tot u <rhyme label="d" type="f" corresp="#s1:l7">treden</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Met ootmoet, Vaeck alle <rhyme label="c" type="f" corresp="#s1:l5">dagen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Seght mijn, schoonste van de <rhyme label="a" type="f">reye</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Waerom ghy my soo <rhyme label="b" type="m">veracht</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Laet mijn bidden, laet mijn <rhyme label="a" type="f" corresp="#s2:l1">schreyen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Op u hebben soo veel <rhyme label="b" type="m" corresp="#s2:l2">macht</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Dat ick magh <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Dat ick magh <rhyme label="c" type="f" corresp="#s2:l5">spooren</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Wat mijn mis-staet, en niet na u zin en <rhyme label="d" type="f">is</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Waer dat 't in <rhyme label="d" type="f" corresp="#s2:l7">is</rhyme>.</l>
        <l id="s2:l9" met="2" type="+-">Ach Goddin <rhyme label="d" type="f" corresp="#s2:l7">is</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Geen ick doe, ick sal my <rhyme label="c" type="f" corresp="#s2:l5">spooren</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Stadigh, naer u soet <rhyme label="a" type="f">gebieden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Al mijn fouten af te <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dus wilt niet meer voor mijn <rhyme label="a" type="f" corresp="#s3:l1">vlieden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Hartje blijft een weynigh <rhyme label="b" type="m" corresp="#s3:l2">staen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Laet dees u <rhyme label="c" type="f">Harder</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Laet dees u <rhyme label="c" type="f" corresp="#s3:l5">Harder</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Niet meer door Bosch, door bergh en <rhyme label="d" type="f">Dalen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Treurigh <rhyme label="d" type="f" corresp="#s3:l7">dwalen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Maer onthael hem liever <rhyme label="d" type="f" corresp="#s3:l7">sacht</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Soo raeckt hy <rhyme label="c" type="f" corresp="#s3:l5">varder</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
