  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOe wonder is t'gestrael van <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En hoe crachtigh sijn haer <rhyme label="b" type="f">vlercken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die ons t' beroert ghemoet daer <rhyme label="a" type="m" corresp="#s1:l1">in</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Soo ghedurighlijck doen <rhyme label="b" type="f" corresp="#s1:l2">wercken</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Den minne-schicht vlieght te <rhyme label="c" type="m">snel</rhyme></l>
        <l id="s1:l6" met="6" type="+-">Door suere pijn,, Tot in mijn,, Grage <rhyme label="d" type="m">borst</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Want t'is mijn <rhyme label="c" type="m" corresp="#s1:l5">Isabel</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Daer ick soo seer door liefd' naer <rhyme label="d" type="m" corresp="#s1:l6">dorst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick heb soo vast mijn hert <rhyme label="a" type="m">gheset</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Om haer eeuwighlijck te <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want dese overschoon <rhyme label="a" type="m" corresp="#s2:l1">Bruynet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Hout ghekerckert my mijn <rhyme label="b" type="f" corresp="#s2:l2">sinnen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ick voel haere stricken <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s2:l6" met="6" type="+-">Door minne-ly,, Daer sy my,, t'hert m√© <rhyme label="d" type="m">bint</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Het is mijn <rhyme label="c" type="m" corresp="#s2:l5">Isabel</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Daer ick ben eeuwigh toe <rhyme label="d" type="m" corresp="#s2:l6">ghesint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer ooghskens zijn my <rhyme label="a" type="m">t'genster-licht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Daer my t'hert door wert <rhyme label="b" type="f">ontvoencken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dies voel ick my noch meer <rhyme label="a" type="m" corresp="#s3:l1">doorschicht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Alsoo haest sy my <rhyme label="b" type="f" corresp="#s3:l2">beloencken</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Want t'sorg'lijck liefdens <rhyme label="c" type="m">spel</rhyme></l>
        <l id="s3:l6" met="6" type="+-">Vol min-ghekriel,, Heeft de ziel,, My <rhyme label="d" type="m">ghevoet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Van mijn lief <rhyme label="c" type="m" corresp="#s3:l5">Isabel</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Die ick in t' hert beminnen <rhyme label="d" type="m" corresp="#s3:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Oft dat ick van haer wesen <rhyme label="a" type="m">ben</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Noch op haer staen mijn <rhyme label="b" type="f">ghedachten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die ick noch niet int minst en <rhyme label="a" type="m" corresp="#s4:l1">ken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Doet my naer haer wangsken <rhyme label="b" type="f" corresp="#s4:l2">trachten</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dees minne-pijn is te <rhyme label="c" type="m">fel</rhyme>,</l>
        <l id="s4:l6" met="6" type="+-">Doch al de smert,, ick int hert,, geren <rhyme label="d" type="m">ly</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Om mijn schoon <rhyme label="c" type="m" corresp="#s4:l5">Isabel</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Die ick voor eeuwigh heden <rhyme label="d" type="m" corresp="#s4:l6">vry</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick wil gewis mijn hert en <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Nu seer geren aen haer <rhyme label="b" type="f">gheven</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En waert dat t' ievers soo <rhyme label="a" type="m" corresp="#s5:l1">gheviel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">k' Sou haer helpen met mijn <rhyme label="b" type="f" corresp="#s5:l2">leven</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Al t'geen dat ick hier <rhyme label="c" type="m">vertel</rhyme></l>
        <l id="s5:l6" met="6" type="+-">En is noch al,, Int getal,, maer het <rhyme label="d" type="m">minst</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Want voor mijn <rhyme label="c" type="m" corresp="#s5:l5">Isabel</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Stel ick mijn lijf en bloet ten <rhyme label="d" type="m" corresp="#s5:l6">dinst</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">k' En wil dan anders niemant <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Als mijn Isabel gaen <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Die my met liefde t' hert soo <rhyme label="a" type="m" corresp="#s6:l1">seer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Vast doorschoten heeft van <rhyme label="b" type="f" corresp="#s6:l2">binnen</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Want t'poeselachtigh <rhyme label="c" type="m">vel</rhyme>,</l>
        <l id="s6:l6" met="6" type="+-">Het bruyn gevoenc,, oog-geloenc,, soeten <rhyme label="d" type="m">mont</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Van mijn schoon <rhyme label="c" type="m" corresp="#s6:l5">Isabel</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Die heeft my al te seer <rhyme label="d" type="m" corresp="#s6:l6">gewont</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nu wech dan overschoon <rhyme label="a" type="m">Ienno</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Marianne gaet vry <rhyme label="b" type="f">henen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Verhuyst van my Mari, <rhyme label="a" type="m" corresp="#s7:l1">Cato</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Want in roock sijt ghy <rhyme label="b" type="f" corresp="#s7:l2">verdwenen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">De oogh van het min <rhyme label="c" type="m">ghequel</rhyme></l>
        <l id="s7:l6" met="6" type="+-">Die staet op haer,, Die ick maer,, Min <rhyme label="d" type="m">alleen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Want t' is mijn <rhyme label="c" type="m" corresp="#s7:l5">Isabel</rhyme></l>
        <l id="s7:l8" met="4" type="-+">En ick en wil oit anders <rhyme label="d" type="m" corresp="#s7:l6">geen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
