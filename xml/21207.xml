<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21207</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
    </tags>
    <melody>'s-Hertogenbosch gij stad verheven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Sparendam playsant van <rhyme label="a" type="f">Huysen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Vol van Schepen in de <rhyme label="b" type="m">Kolck</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Water-rijck, versien met <rhyme label="a" type="f" corresp="#s1:l1">Sluysen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Veel Schippers, en varend' <rhyme label="b" type="m" corresp="#s1:l2">Volck</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Gherijft ghy <rhyme label="c" type="f">goedertieren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met dienstbaer <rhyme label="c" type="f" corresp="#s1:l5">manieren</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Ons Borghers <rhyme label="d" type="m">mee</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Die uyt de <rhyme label="d" type="m" corresp="#s1:l7">Stee</rhyme></l>
        <l id="s1:l9" met="3" type="-+">In u komen <rhyme label="c" type="f" corresp="#s1:l5">playsieren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die uyt Harelemsch <rhyme label="a" type="f">besluyten</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Sondaeghs nemen haren <rhyme label="b" type="m">ganck</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Tot u Vracht-halende <rhyme label="a" type="f" corresp="#s2:l1">Schuyten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'t Is daer loop in voor een <rhyme label="b" type="m" corresp="#s2:l2">Blanck</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Om onvermoeyt te <rhyme label="c" type="f">koomen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ghereedt over de <rhyme label="c" type="f" corresp="#s2:l5">Stroomen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">In u <rhyme label="d" type="m">Ghebouw</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Daer sorgh en <rhyme label="d" type="m" corresp="#s2:l7">rouw</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Niet veel en wordt <rhyme label="c" type="f" corresp="#s2:l5">vernomen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die te voet wil gaen <rhyme label="a" type="f">vermeyen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Kan bequaem met lusten <rhyme label="b" type="m">treen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Deur de groene <rhyme label="a" type="f" corresp="#s3:l1">Klaver-weyen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Neffens 't voedsaem Kooren <rhyme label="b" type="m" corresp="#s3:l2">heen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En wie den wegh mocht <rhyme label="c" type="f">falen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Om gantsch niet te <rhyme label="c" type="f" corresp="#s3:l5">verdwalen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Vindt ghy een <rhyme label="d" type="m">Handt</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Voor elck <rhyme label="d" type="m" corresp="#s3:l7">gheplant</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Die wijst recht nae u <rhyme label="c" type="f" corresp="#s3:l5">Palen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Al die binnen u <rhyme label="a" type="f">vergaren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Toeft ghy met een soet <rhyme label="b" type="m">vermaeck</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Sou men Broodt voor Vrienden <rhyme label="a" type="f" corresp="#s4:l1">sparen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'t Waer een onghewoone <rhyme label="b" type="m" corresp="#s4:l2">saeck</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy zijt altijdt <rhyme label="c" type="f">gheneghen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Den Spaer pot uyt te <rhyme label="c" type="f" corresp="#s4:l5">veghen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Om speelen <rhyme label="d" type="m">net</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Panneken <rhyme label="d" type="m" corresp="#s4:l7">vet</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Soo milde Heeren <rhyme label="c" type="f" corresp="#s4:l5">pleghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Die by u, door vreckigh <rhyme label="a" type="f">schroomen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Den Spaerwouwschen Hondt <rhyme label="b" type="m">uytstack</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">'t Was of Iacob quam te <rhyme label="a" type="f" corresp="#s5:l1">Roomen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dat hy Pieter niet en <rhyme label="b" type="m" corresp="#s5:l2">sprack</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">'t Waer onbeleeft, jae <rhyme label="c" type="f">schande</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat yemandt uyt den <rhyme label="c" type="f" corresp="#s5:l5">Lande</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Een Sant <rhyme label="d" type="m">gheacht</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Besocht, en <rhyme label="d" type="m" corresp="#s5:l7">bracht</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Hem niet een <rhyme label="c" type="f" corresp="#s5:l5">Offerhande</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">'s Winters als de dorre <rhyme label="a" type="f">paden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Glat op 't Swater legghen <rhyme label="b" type="m">sterck</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Is menigh met lust <rhyme label="a" type="f" corresp="#s6:l1">beladen</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Om te reysen nae u <rhyme label="b" type="m" corresp="#s6:l2">perck</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Met veel ghespoock ten <rhyme label="c" type="f">Yse</rhyme>:</l>
        <l id="s6:l6" met="3" type="-+">En ghy (nae d'oude <rhyme label="c" type="f" corresp="#s6:l5">wijse</rhyme>)</l>
        <l id="s6:l7" met="2" type="-+">Schaft Wijn en <rhyme label="d" type="m">Bier</rhyme></l>
        <l id="s6:l8" met="2" type="-+">daer by goet <rhyme label="d" type="m" corresp="#s6:l7">Vyer</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Oock veelderhande <rhyme label="c" type="f" corresp="#s6:l5">Spijse</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
