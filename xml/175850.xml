  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoort toe alle <rhyme label="a" type="f">gelijcke</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Jonck, out, arm en <rhyme label="a" type="f" corresp="#s1:l1">rijcke</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Wat men u singen <rhyme label="b" type="m">sal</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al met een bly <rhyme label="b" type="m" corresp="#s1:l3">geschal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe dat de Vyant <rhyme label="a" type="f">spoedigh</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met Sloepen <rhyme label="a" type="f" corresp="#s2:l1">overvloedigh</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Daer sy wel negen <rhyme label="b" type="m">jaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Over timmerden <rhyme label="b" type="m" corresp="#s2:l3">klaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Sloepen sach men <rhyme label="a" type="f">komen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">By Bergen op den <rhyme label="a" type="f" corresp="#s3:l1">Zoome</rhyme></l>
        <l id="s3:l3" met="3" type="-+">En voeren na <rhyme label="b" type="m">Rommer-wael</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daer vergaerdense <rhyme label="b" type="m" corresp="#s3:l3">al-te-mael</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Van Antwerpen <rhyme label="a" type="f">gevaren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Om Wilm-stad te <rhyme label="a" type="f" corresp="#s4:l1">beswaren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En het Land te nemen <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Grootelijx tot haer <rhyme label="b" type="m" corresp="#s4:l3">gewin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ons Oorlog-schepen <rhyme label="a" type="f">moedigh</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Sagen haer komen <rhyme label="a" type="f" corresp="#s5:l1">spoedigh</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En volghden haer oock <rhyme label="b" type="m">naer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Gaven oock vyer op <rhyme label="b" type="m" corresp="#s5:l3">haer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Men saghse haer na <rhyme label="a" type="f">seylen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En dat al sonder <rhyme label="a" type="f" corresp="#s6:l1">feylen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Tot aen Sint Anna <rhyme label="b" type="m">Lant</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Daer moesten sy van <rhyme label="b" type="m" corresp="#s6:l3">kant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daer hebben wy <rhyme label="a" type="f">geschoten</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat heeft Paep-Jan <rhyme label="a" type="f" corresp="#s7:l1">verdroten</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Al waren se <rhyme label="b" type="m">verstoort</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Sy sprongen over <rhyme label="b" type="m" corresp="#s7:l3">boort</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Doen sy te Lande <rhyme label="a" type="f">quamen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Vonden sy daer te <rhyme label="a" type="f" corresp="#s8:l1">samen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ruyters Soldaten <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die sy vielen te <rhyme label="b" type="m" corresp="#s8:l3">voet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Al om quartier sy <rhyme label="a" type="f">baden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Wy deden haer <rhyme label="a" type="f" corresp="#s9:l1">genaden</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Leyden af haer <rhyme label="b" type="m">geweer</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Alles na ons <rhyme label="b" type="m" corresp="#s9:l3">begeer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De Paep sprack sonder <rhyme label="a" type="f">vreesen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Datse al in slaep soude <rhyme label="a" type="f" corresp="#s10:l1">wesen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Ter eeren ons <rhyme label="b" type="m">Lieve-vrou</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Die 't oock niet sien en <rhyme label="b" type="m" corresp="#s10:l3">sou</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Sy hebben anders <rhyme label="a" type="f">vernomen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Het sijn maer blaeuwe <rhyme label="a" type="f" corresp="#s11:l1">blomen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Wy quamen haer kort <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Sy konnen 't niet <rhyme label="b" type="m" corresp="#s11:l3">ontgaen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Graef Jan is het <rhyme label="a" type="f">ontkomen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Hy hoeft hem niet te <rhyme label="a" type="f" corresp="#s12:l1">roemen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">De eer die hy nu <rhyme label="b" type="m">heyt</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Die is seer haest <rhyme label="b" type="m" corresp="#s12:l3">geseyt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Capiteyns en <rhyme label="a" type="f">Luytenande</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Vaendragers ende <rhyme label="a" type="f" corresp="#s13:l1">Sergeande</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">En veel Geestelijckheyt <rhyme label="b" type="m">mee</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Quamen ter selver <rhyme label="b" type="m" corresp="#s13:l3">stee</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Als men 't getal sou <rhyme label="a" type="f">grooten</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Verdroncken of <rhyme label="a" type="f" corresp="#s14:l1">geschoten</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Ruym sestien-hondert <rhyme label="b" type="m">Man</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Wat raet gaet ons nu <rhyme label="b" type="m" corresp="#s14:l3">an</rhyme>?</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Ons macht is nu <rhyme label="a" type="f">ghebroken</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Geusen haer tanden <rhyme label="a" type="f" corresp="#s15:l1">stoken</rhyme></l>
        <l id="s15:l3" met="3" type="-+">'t Gelt sijn wy nu oock <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Jst niet een groote <rhyme label="b" type="m" corresp="#s15:l3">spijt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Ons schoone <rhyme label="a" type="f">Pattekonnen</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Hebben sy al <rhyme label="a" type="f" corresp="#s16:l1">gewonnen</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Die uyt-leggers <rhyme label="b" type="m">eerbaer</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Vielen ons veel te <rhyme label="b" type="m" corresp="#s16:l3">swaer</rhyme>[.]</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Driellingen ende <rhyme label="a" type="f">Slangen</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Halve Kartouwen <rhyme label="a" type="f" corresp="#s17:l1">langen</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Ruym hondert soo ick <rhyme label="b" type="m">meen</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Waren daer groot en <rhyme label="b" type="m" corresp="#s17:l3">kleen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Oock ontrent hondert <rhyme label="a" type="f">seylen</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Waerom wy sonder <rhyme label="a" type="f" corresp="#s18:l1">feylen</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Gheladen in <rhyme label="b" type="m">overvloet</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Met mannen, Gelt en <rhyme label="b" type="m" corresp="#s18:l3">goet</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Victory sagh men <rhyme label="a" type="f">branden</rhyme>:</l>
        <l id="s19:l2" met="3" type="-+">Grootelijcks tot onsere <rhyme label="a" type="f" corresp="#s19:l1">schanden</rhyme></l>
        <l id="s19:l3" met="3" type="-+">Schoten of ons <rhyme label="b" type="m">Kanons</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Over onse <rhyme label="b" type="m" corresp="#s19:l3">Pattekons</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Loopt nu Pater en <rhyme label="a" type="f">koster</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Met Snoer en <rhyme label="a" type="f" corresp="#s20:l1">Pater-noster</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Naer u lieve Vrou van <rhyme label="b" type="m">Hal</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Of sy u helpen <rhyme label="b" type="m" corresp="#s20:l3">sal</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Sint Jacob met sijn <rhyme label="a" type="f">Schelpen</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">En kan ons niet meer <rhyme label="a" type="f" corresp="#s21:l1">helpen</rhyme></l>
        <l id="s21:l3" met="3" type="-+">Noch oock ons Roomschen <rhyme label="b" type="m">vaer</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Maer het gelt wachten wy <rhyme label="b" type="m" corresp="#s21:l3">naer</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Komt vry ghy Spaense <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Wilt ghy mee Hollants <rhyme label="a" type="f" corresp="#s22:l1">leeren</rhyme></l>
        <l id="s22:l3" met="3" type="-+">Ghelooft den Paep doch <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Want hy brenght u int <rhyme label="b" type="m" corresp="#s22:l3">verdriet</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">O Heer bewaert <rhyme label="a" type="f">Orangien</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Dat hy tot spijt van <rhyme label="a" type="f" corresp="#s23:l1">Spaengien</rhyme>,</l>
        <l id="s23:l3" met="3" type="-+">Magh leven als een <rhyme label="b" type="m">helt</rhyme></l>
        <l id="s23:l4" met="3" type="-+">En breken 't Spaens <rhyme label="b" type="m" corresp="#s23:l3">ghewelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
