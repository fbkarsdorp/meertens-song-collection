  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GEt! hoe zoet ken Brankje <rhyme label="a" type="f">kallen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Nouw hij mit Peet Sijtje <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Kuijeren langs <rhyme label="a" type="f" corresp="#s1:l1">d'Amstel-wallen</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Mijn konfijte <rhyme label="b" type="m" corresp="#s1:l2">dageraet</rhyme>!</l>
        <l id="s1:l5" met="4" type="+-">Sunt ik uit je suik're <rhyme label="c" type="f">murref</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">De konfijte Nekter <rhyme label="d" type="m">zoog</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Zonk men hart (as klinkker <rhyme label="c" type="f" corresp="#s1:l5">turref</rhyme>)</l>
        <l id="s1:l8" met="4" type="+-">In men slinkker <rhyme label="d" type="m" corresp="#s1:l6">elle-boog</rhyme>.</l>
      </lg>
    </lg>
  </text>
