  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'T zy wy klagen, 't zy wy <rhyme label="a" type="f">treuren</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Of verblijt zijn af, en <rhyme label="b" type="m">aan</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Wat beneden zon en <rhyme label="b" type="m" corresp="#s1:l2">maan</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Om mag <rhyme label="b" type="m" corresp="#s1:l2">gaan</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Tot ons lief of leed <rhyme label="a" type="f" corresp="#s1:l1">gebeuren</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Hoe het lot dray heen, of <rhyme label="c" type="m">weer</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">'T zy wy zingen, 't zy wy <rhyme label="a" type="f" corresp="#s1:l1">treuren</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">'T zy wy staan, of vallen <rhyme label="c" type="m" corresp="#s1:l6">neer</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Gode zy alleenig de <rhyme label="c" type="m" corresp="#s1:l6">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
