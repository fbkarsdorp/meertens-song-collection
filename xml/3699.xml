    <melody>Psalm 096 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SINGHT een nieuw liet den Heer <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Singht ghy volcken, wilt vroylick <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Loeft end pryst sijnen naem <rhyme label="b" type="m">altydt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">End verkondight breedt ende <rhyme label="b" type="m" corresp="#s1:l3">wydt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Godts verlossinghen groot mits <rhyme label="a" type="f" corresp="#s1:l1">desen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laet die heydenen sijn eer' <rhyme label="a" type="f">mercken</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Maeckt ghewagh van sijn <rhyme label="a" type="f" corresp="#s2:l1">wonderwercken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want hoogh' verheuen is de <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die ghevreest moet wesen veel <rhyme label="b" type="m" corresp="#s2:l3">meer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dan d'afgoden in allen <rhyme label="a" type="f" corresp="#s2:l1">percken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want die goden die t'volck doen <rhyme label="a" type="f">beuen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Sijn gantschlick niet, want se niet <rhyme label="a" type="f" corresp="#s3:l1">leuen</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Maer Godt schiep den hemel seer <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Sijn heerlicheit end kracht niet <rhyme label="b" type="m" corresp="#s3:l3">kleyn</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Gaen voor hem: hoogh' is hy <rhyme label="a" type="f" corresp="#s3:l1">verheuen</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maiesteit end kracht t'allen <rhyme label="a" type="f">stonden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Sijn in sijn heyligh huys <rhyme label="a" type="f" corresp="#s4:l1">beuonden</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Daerom ghy volcken komt doch <rhyme label="b" type="m">hier</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Gheeft Godt den Heere <rhyme label="b" type="m" corresp="#s4:l3">goedertier</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Eer' end vreese met hert' end <rhyme label="a" type="f" corresp="#s4:l1">monden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Loeft ende pryst den Heer' <rhyme label="a" type="f">eendrachtigh</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Maeckt groot den naeme Godts <rhyme label="a" type="f" corresp="#s5:l1">almachtigh</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ghy volcken bringht hem met <rhyme label="b" type="m">demoet</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Gheschencken end veel gaeuen <rhyme label="b" type="m" corresp="#s5:l3">goet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Komt in sijn huys, sijt daer <rhyme label="a" type="f" corresp="#s5:l1">aendachtigh</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Komt t'saemen, doet in sijn <rhyme label="a" type="f">woensteden</rhyme></l>
        <l id="s6:l2" met="4" type="-+">In heylicheit uwe <rhyme label="a" type="f" corresp="#s6:l1">ghebeden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">In sijn woninghe tot hem <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dat alle menschen met der <rhyme label="b" type="m" corresp="#s6:l3">daet</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Hem vreesen end dienen met <rhyme label="a" type="f" corresp="#s6:l1">vreden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ieghelick moet nu <rhyme label="a" type="f">openbaeren</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Dat ons Godt herscht, end sulcks <rhyme label="a" type="f" corresp="#s7:l1">verklaeren</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Dies sal t'ęerdtryck seer vastlick <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Als doer sijn handt werden voort <rhyme label="b" type="m" corresp="#s7:l3">aen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Rechtlick gherichtet alle <rhyme label="a" type="f" corresp="#s7:l1">schaeren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dat de hemelen hen <rhyme label="a" type="f">verblyden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">T'ęerdtryck dat lach' in desen <rhyme label="a" type="f" corresp="#s8:l1">tyden</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">T'Męer verheffe met vrueghdt den <rhyme label="b" type="m">kop</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">T'veldt end oock al wat groeyt daer <rhyme label="b" type="m" corresp="#s8:l3">op</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">End bosschen moeten Godt <rhyme label="a" type="f" corresp="#s8:l1">belyden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">De Heer komt, de Heer komt seer <rhyme label="a" type="f">spoedigh</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Om t'ęerdtryck met herten <rhyme label="a" type="f" corresp="#s9:l1">sachtmoedigh</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Te richten in <rhyme label="b" type="m">gherechticheit</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">End die volcken in <rhyme label="b" type="m" corresp="#s9:l3">billicheit</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Te leyden nae sijn waerheit <rhyme label="a" type="f" corresp="#s9:l1">goedigh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
