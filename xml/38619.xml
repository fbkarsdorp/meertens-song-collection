  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoe is mijn teere <rhyme label="a" type="m">jeucht</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Soo verblijdt door Venus <rhyme label="a" type="m" corresp="#s1:l1">vreucht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die met haer loose <rhyme label="b" type="f">loncken</rhyme>,</l>
        <l id="s1:l4" met="1" type="-+">Doet <rhyme label="b" type="f" corresp="#s1:l3">voncken</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Het hert, dat sich <rhyme label="a" type="m" corresp="#s1:l1">verheucht</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In't jock van haer <rhyme label="c" type="f">ghenuchten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat dickmaels baert veel <rhyme label="c" type="f" corresp="#s1:l6">suchten</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Elaes,, ick <rhyme label="d" type="m">daes</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Hoe ligh ick hier en <rhyme label="d" type="m" corresp="#s1:l8">raes</rhyme></l>
        <l id="s1:l10" met="5" type="-+">Nu de min,, mijnen sin,, <rhyme label="e" type="m">verkeert</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">In't spel dat ons Cupido <rhyme label="e" type="m" corresp="#s1:l10">leert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoort minnaers wie ghy <rhyme label="a" type="m">sijt</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Ick die eertijdts heb <rhyme label="a" type="m" corresp="#s2:l1">ghevrijt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En jouffers met veel <rhyme label="b" type="f">treken</rhyme></l>
        <l id="s2:l4" met="1" type="-+">Gonck <rhyme label="b" type="f" corresp="#s2:l3">spreken</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Waer is den schoonen <rhyme label="a" type="m" corresp="#s2:l1">tijt</rhyme>?</l>
        <l id="s2:l6" met="3" type="-+">De uren en de <rhyme label="c" type="f">daghen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Die moet ick nu <rhyme label="c" type="f" corresp="#s2:l6">beclaghen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Want siet,, om <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Ist all' wat Venus <rhyme label="d" type="m" corresp="#s2:l8">riet</rhyme>:</l>
        <l id="s2:l10" met="5" type="-+">Haeren ganck,, haren sanck,, <rhyme label="e" type="m">vergaet</rhyme></l>
        <l id="s2:l11" met="4" type="-+">En al wat haer bevallijck <rhyme label="e" type="m" corresp="#s2:l10">staet</rhyme>.</l>
      </lg>
    </lg>
  </text>
