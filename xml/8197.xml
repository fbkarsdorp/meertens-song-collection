  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MOeder ick heb <rhyme label="a" type="f">geswooren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Soo hy myn niet en <rhyme label="b" type="m">trout</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat 't al sal zijn <rhyme label="a" type="f" corresp="#s1:l1">verlooren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Myn Hofste, met sijn <rhyme label="b" type="m" corresp="#s1:l2">hout</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">In 't midden van sijn <rhyme label="c" type="m">pleyn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn vyver en <rhyme label="c" type="m" corresp="#s1:l5">Fonteyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick word' staegh <rhyme label="a" type="f">aengebeden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Een valt daer my te <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">(Hy laet my niet met <rhyme label="a" type="f" corresp="#s2:l1">vreden</rhyme>)</l>
        <l id="s2:l4" met="3" type="-+">Met sijn ontsteken <rhyme label="b" type="m" corresp="#s2:l2">moet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En sijn geswollen <rhyme label="c" type="m">neck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">In 't binnenst mijns <rhyme label="c" type="m" corresp="#s2:l5">vertreck</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat staet my nu te <rhyme label="a" type="f">dencken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wat staet my nu te <rhyme label="b" type="m">doen</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Dit sou mijn sinnen <rhyme label="a" type="f" corresp="#s3:l1">krencken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Mijn smarte meerder <rhyme label="b" type="m" corresp="#s3:l2">voen</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">'k Sal d'ongeremde <rhyme label="c" type="m">vlam</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Smoren, daer 't oorspronck <rhyme label="c" type="m" corresp="#s3:l5">nam</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
