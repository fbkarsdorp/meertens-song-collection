  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat roemen we onze <rhyme label="a" type="m">deugd</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Als men met wederzinn' iets <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wyl 't alles maar geveinstheid <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Als men met wederzinn' iets <rhyme label="b" type="m" corresp="#s1:l2">geeft</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wat roemen we onze <rhyme label="a" type="m" corresp="#s1:l1">deugd</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wyl 't alles maar geveinstheid <rhyme label="c" type="m" corresp="#s1:l3">is</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Wyl Jesus zig <rhyme label="a" type="m" corresp="#s1:l1">verheugt</rhyme></l>
        <l id="s1:l8" met="4" type="-+">(Die hier in zyne leden <rhyme label="b" type="m" corresp="#s1:l2">leeft</rhyme>)</l>
        <l id="s1:l9" met="4" type="-+">Als hy ons, vry van <rhyme label="c" type="m" corresp="#s1:l3">schynvernis</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Iet van het zyne schenken <rhyme label="d" type="m">ziet</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Aan die Hy na zig achter <rhyme label="d" type="m" corresp="#s1:l10">liet</rhyme>.</l>
      </lg>
    </lg>
