    <melody>Ik minde eens konings zoon</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAria maghet <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghi sijt een bloeme <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Verheuen inden <rhyme label="a" type="f" corresp="#s1:l1">throone</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die ons verbliden <rhyme label="b" type="m" corresp="#s1:l2">doet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghi sijt die ghene <rhyme label="a" type="f">alleyne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Daer alle minen troost aen <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van sonden sijt ghi <rhyme label="a" type="f" corresp="#s2:l1">reyne</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Maria suyuer <rhyme label="b" type="m" corresp="#s2:l2">vat</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Der sondaren <rhyme label="a" type="f">medicine</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Neemt mi in v <rhyme label="b" type="m">behoet</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Hoe schoon is v <rhyme label="a" type="f" corresp="#s3:l1">aenschine</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Schoon suyuer maghet <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Bidt uwen sone <rhyme label="a" type="f">fonteyne</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die aenden cruyce <rhyme label="b" type="m">hinck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hi sal ons wel <rhyme label="a" type="f" corresp="#s4:l1">ontfermen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die ons sijn minne <rhyme label="b" type="m" corresp="#s4:l2">schinct</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Toocht ons v claer <rhyme label="a" type="f">aenschijn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Och hemelsche <rhyme label="b" type="m">eewicheyt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Verlost ons wter <rhyme label="a" type="f" corresp="#s5:l1">pine</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Daer wi toe sijn <rhyme label="b" type="m" corresp="#s5:l2">gheneycht</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wij willen altoos <rhyme label="a" type="f">louen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die weerdighe <rhyme label="b" type="m">triniteyt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Die regneert daer <rhyme label="a" type="f" corresp="#s6:l1">bouen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Al inder <rhyme label="b" type="m" corresp="#s6:l2">eewicheyt</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Haer willen wi altijt <rhyme label="a" type="f">dienen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Met eenen soeten <rhyme label="b" type="m">sanck</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dat si ons helpe te <rhyme label="a" type="f" corresp="#s7:l1">siene</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Den schoonen enghelen <rhyme label="b" type="m" corresp="#s7:l2">sanck</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu segghet alle <rhyme label="a" type="f">Amen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Beyde cleyn ende <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s8:l3" met="3" type="-+">So moghen wij <rhyme label="a" type="f" corresp="#s8:l1">versamen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Al in der enghelen <rhyme label="b" type="m" corresp="#s8:l2">hoop</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
