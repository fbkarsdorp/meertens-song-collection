  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MY mach den dach wel <rhyme label="a" type="f">rouwen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">d'Vr en den <rhyme label="b" type="m">ooghenblic</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ic my begaf tot <rhyme label="a" type="f" corresp="#s1:l1">trouwen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En brocht in soo swaeren <rhyme label="b" type="m" corresp="#s1:l2">strick</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">My hechten aen dat <rhyme label="c" type="m">wijf</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Verpanden soo mijn <rhyme label="c" type="m" corresp="#s1:l5">lijf</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Die soo boos is, en soo <rhyme label="d" type="m">fel</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Als den Duyvel vande <rhyme label="d" type="m" corresp="#s1:l7">hel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ia fiel t'mach my wel <rhyme label="a" type="f">spijten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat ick't soo hebt <rhyme label="b" type="m">mistast</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">K'mochter wel een jaer om <rhyme label="a" type="f" corresp="#s2:l1">crijten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat ic quamp aen sulc een <rhyme label="b" type="m" corresp="#s2:l2">gast</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Een Hannen in sijn <rhyme label="c" type="m">broeck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Een Teeuvven inden <rhyme label="c" type="m" corresp="#s2:l5">hoeck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Oft ick soo gheluckich <rhyme label="d" type="m">vvaer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat den guyt eens stont in <rhyme label="d" type="m" corresp="#s2:l7">baer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sus Modde-bruyt vvilt <rhyme label="a" type="f">svvijghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghy stinckt als <rhyme label="b" type="m">vuyle-vis</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">K'souvver af de corts noch <rhyme label="a" type="f" corresp="#s3:l1">crijghen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">VVant ghy richt soo stel naer <rhyme label="b" type="m" corresp="#s3:l2">pis</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">t'Is stront vvat by u <rhyme label="c" type="m">blinckt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">V handen sijn <rhyme label="c" type="m" corresp="#s3:l5">verminckt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Siet eens vvat een nette <rhyme label="d" type="m">daey</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Daer me gaetse te <rhyme label="d" type="m" corresp="#s3:l7">schappraey</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Luysbos ghy meught vvat <rhyme label="a" type="f">snappen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die u aen taefel <rhyme label="b" type="m">kemt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met die handen loopt hy <rhyme label="a" type="f" corresp="#s4:l1">tappen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer een luys tvvee dry in <rhyme label="b" type="m" corresp="#s4:l2">svvemt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ia lest noch by het <rhyme label="c" type="m">vier</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Daer svvomper tvve int <rhyme label="c" type="m" corresp="#s4:l5">bier</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Op de boter satter <rhyme label="d" type="m">dry</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die hy craeckten noch daer <rhyme label="d" type="m" corresp="#s4:l7">by</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Neen Ioffrou <rhyme label="a" type="f">vuylen-vlieger</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Speekt  op u schorsten <rhyme label="b" type="m">bol</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ick en ben u gheen <rhyme label="a" type="f" corresp="#s5:l1">beliegher</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">VVant sy houden daer hun <rhyme label="b" type="m" corresp="#s5:l2">hol</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Daer sitter in een <rhyme label="c" type="m">puyst</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Niet minder als een <rhyme label="c" type="m" corresp="#s5:l5">duyst</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Sonder al dat cleyn <rhyme label="d" type="m">geswem</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die noch cruypen in u <rhyme label="d" type="m" corresp="#s5:l7">hem</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ian gat die cunt <rhyme label="a" type="f">beseycken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">V stinc-broeck sonder <rhyme label="b" type="m">nop</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Op u vvambas soumen <rhyme label="a" type="f" corresp="#s6:l1">vveycken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Een goey verse vette <rhyme label="b" type="m" corresp="#s6:l2">sop</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">V tessen <rhyme label="c" type="m">af-geleurt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">V vvambas is <rhyme label="c" type="m" corresp="#s6:l5">ghescheurt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Siet eens vvat een vuylen <rhyme label="d" type="m">boec</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">t'Hem dat hanght hem door de <rhyme label="d" type="m" corresp="#s6:l7">broeck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Neen sekers puyck der <rhyme label="a" type="f">vrouvven</rhyme></l>
        <l id="s7:l2" met="3" type="-+">V cleet moet sijn <rhyme label="b" type="m">aenveert</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Vvven besten rock en <rhyme label="a" type="f" corresp="#s7:l1">bouvven</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En is gheen twee blancken <rhyme label="b" type="m" corresp="#s7:l2">weert</rhyme></l>
        <l id="s7:l5" met="3" type="-+">V handen sijn soo <rhyme label="c" type="m">vvit</rhyme>?</l>
        <l id="s7:l6" met="3" type="-+">Gelijck mijn heer den <rhyme label="c" type="m" corresp="#s7:l5">smit</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Op u troni schoonste <rhyme label="d" type="m">vrou</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Peeterceli vvassen <rhyme label="d" type="m" corresp="#s7:l7">sou</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghy roem en bloem der <rhyme label="a" type="f">mannen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Comt nu eens uyt den <rhyme label="b" type="m">hoeck</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sydy niet een rechten <rhyme label="a" type="f" corresp="#s8:l1">hannen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Stinckaert en een <rhyme label="b" type="m" corresp="#s8:l2">schyte-broec</rhyme>!</l>
        <l id="s8:l5" met="3" type="-+">VVaer ben ick toe <rhyme label="c" type="m">gheraeckt</rhyme>:</l>
        <l id="s8:l6" met="3" type="-+">Mijn hert van hem nu <rhyme label="c" type="m" corresp="#s8:l5">braeckt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">K'gaffer noch een somme <rhyme label="d" type="m">gelt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Hongh hy maer op <rhyme label="d" type="m" corresp="#s8:l7">t'galge-velt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ioffrou ick sou vvel <rhyme label="a" type="f">spouvven</rhyme></l>
        <l id="s9:l2" met="3" type="-+">t'Snot hanght u uyt den <rhyme label="b" type="m">neus</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Siet eens vvat een netten <rhyme label="a" type="f" corresp="#s9:l1">bouwen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Lieve Godt had ick eens <rhyme label="b" type="m" corresp="#s9:l2">keus</rhyme>!</l>
        <l id="s9:l5" met="3" type="-+">Och waer dit svvijn <rhyme label="c" type="m">ghekelt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">K'en trouden noit om <rhyme label="c" type="m" corresp="#s9:l5">gelt</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">VVant het is te groote <rhyme label="d" type="m">pijn</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Met soo een getrout te <rhyme label="d" type="m" corresp="#s9:l7">sijn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ionghmans die gaet uyt <rhyme label="a" type="f">vrijen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Nemt hier exempel <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Brenght u noit om t'gelt in <rhyme label="a" type="f" corresp="#s10:l1">lijen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Als ick u heb voor <rhyme label="b" type="m" corresp="#s10:l2">ghedaen</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">K'heb haer om t'ghelt <rhyme label="c" type="m">ghetrout</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Maer t'is my vvel <rhyme label="c" type="m" corresp="#s10:l5">berout</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Dit in u gedachten <rhyme label="d" type="m">stelt</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Trout toch niemandt om sijn <rhyme label="d" type="m" corresp="#s10:l7">ghelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
