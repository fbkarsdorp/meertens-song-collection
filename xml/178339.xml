<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178339</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Juich Batavieren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Wy zullen <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Uw Jaardag Groote <rhyme label="b" type="m">Vorst</rhyme>:</l>
        <l id="s1:l3" met="2" type="-+">De Snaaren <rhyme label="a" type="f" corresp="#s1:l1">springen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Uit volle <rhyme label="b" type="m" corresp="#s1:l2">borst</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Ik zing nu bly te <rhyme label="c" type="m">moê</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Breng u myn danktoon <rhyme label="c" type="m" corresp="#s1:l5">toe</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s1:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s1:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Weg vuige <rhyme label="a" type="f">Baazen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Uw laster is <rhyme label="b" type="m">geknot</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Weg met u <rhyme label="a" type="f" corresp="#s2:l1">raazen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Vol hoon en <rhyme label="b" type="m" corresp="#s2:l2">spot</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Myn Prins die <rhyme label="c" type="m">triumpheert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Daar hy den nek u <rhyme label="c" type="m" corresp="#s2:l5">keert</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s2:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s2:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Weg <rhyme label="a" type="f">Lasteraaren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die 't Volk hebt <rhyme label="b" type="m">opgeruit</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Thans aan 't <rhyme label="a" type="f" corresp="#s3:l1">bedaaren</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">In 't woên <rhyme label="b" type="m" corresp="#s3:l2">gestuit</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Gy zogt mijn Prins zijn <rhyme label="c" type="m">dood</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Thans is hij buiten <rhyme label="c" type="m" corresp="#s3:l5">nood</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s3:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s3:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">ô <rhyme label="a" type="f">Landbedervers</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">Die Neêrlands magt <rhyme label="b" type="m">vergruisd</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Met Fransche <rhyme label="a" type="f" corresp="#s4:l1">Zwervers</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Hier tegen <rhyme label="b" type="m" corresp="#s4:l2">druist</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Dit was uw toeleg <rhyme label="c" type="m">tog</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Uw doen is volks <rhyme label="c" type="m" corresp="#s4:l5">bedrog</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s4:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s4:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Gy zogt den <rhyme label="a" type="f">luister</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van onze <rhyme label="b" type="m">Oranje-Vorst</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Geheel in 't <rhyme label="a" type="f" corresp="#s5:l1">duister</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Te dooven.... <rhyme label="b" type="m" corresp="#s5:l2">dorst</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Gy niet vol snood <rhyme label="c" type="m">bestaan</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Den Braaven Prins <rhyme label="c" type="m" corresp="#s5:l5">verraên</rhyme>?</l>
        <l id="s5:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s5:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s5:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Lees de <rhyme label="a" type="f">Memorie</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van WILLEM Willems <rhyme label="b" type="m">Zoon</rhyme>:</l>
        <l id="s6:l3" met="2" type="-+">Lees 's Lands <rhyme label="a" type="f" corresp="#s6:l1">Historie</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Vlecht hem een <rhyme label="b" type="m" corresp="#s6:l2">Kroon</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Zy zyn 't die voor ons <rhyme label="c" type="m">Land</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hun goed en bloed <rhyme label="c" type="m" corresp="#s6:l5">verpand</rhyme>!</l>
        <l id="s6:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s6:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s6:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Zy, <rhyme label="a" type="f">Vryheid-Bouwers</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Zy redden ons <rhyme label="b" type="m">altoos</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Als onheil <rhyme label="a" type="f" corresp="#s7:l1">Brouwers</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Ons <rhyme label="b" type="m" corresp="#s7:l2">Goddeloos</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Vergruisden door hun <rhyme label="c" type="m">magt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Door 't Fransche Goud <rhyme label="c" type="m" corresp="#s7:l5">verkracht</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s7:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s7:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">O <rhyme label="a" type="f">Nederlander</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">O Prins! O <rhyme label="b" type="m">Batavier</rhyme>!</l>
        <l id="s8:l3" met="2" type="-+">d'Oranje <rhyme label="a" type="f" corresp="#s8:l1">Stander</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Draagt nu <rhyme label="b" type="m" corresp="#s8:l2">Laurier</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Het Loevesteins <rhyme label="c" type="m">gebroed</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Verliest nu al zijn <rhyme label="c" type="m" corresp="#s8:l5">moed</rhyme>.</l>
        <l id="s8:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s8:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s8:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Zij schrikken, <rhyme label="a" type="f">beven</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Voor Grooten Fredriks <rhyme label="b" type="m">stem</rhyme>:</l>
        <l id="s9:l3" met="2" type="-+">Die ons zal <rhyme label="a" type="f" corresp="#s9:l1">geeven</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Dat gij met <rhyme label="b" type="m" corresp="#s9:l2">klem</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Der Burg'ren regten <rhyme label="c" type="m">schraagt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Als Palinuur U <rhyme label="c" type="m" corresp="#s9:l5">draagt</rhyme>.</l>
        <l id="s9:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s9:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s9:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Zoo blijft <rhyme label="a" type="f">Oranje</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Altoos ons <rhyme label="b" type="m">toeverlaat</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Dat groot <rhyme label="a" type="f" corresp="#s10:l1">Brittanje</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Met onzen <rhyme label="b" type="m" corresp="#s10:l2">Staat</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Weer spoedig Vrienden <rhyme label="c" type="m">zij</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Dan zijn wij vrank en <rhyme label="c" type="m" corresp="#s10:l5">vrij</rhyme>.</l>
        <l id="s10:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s10:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s10:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">God kroon uw <rhyme label="a" type="f">wegen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">O braave Vorst, Hij <rhyme label="b" type="m">geev'</rhyme></l>
        <l id="s11:l3" met="2" type="-+">Uw Huis zijn <rhyme label="a" type="f" corresp="#s11:l1">Zegen</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Dat gij lang <rhyme label="b" type="m" corresp="#s11:l2">leev'</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Ja leev den Staat ten <rhyme label="c" type="m">nut</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Der Burgren Recht ten <rhyme label="c" type="m" corresp="#s11:l5">stut</rhyme>.</l>
        <l id="s11:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s11:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s11:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Zie uw <rhyme label="a" type="f">Vyanden</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Versmelten voor uw <rhyme label="b" type="m">oog</rhyme>,</l>
        <l id="s12:l3" met="2" type="-+">Bedekt met <rhyme label="a" type="f" corresp="#s12:l1">schanden</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Van 's Hemels <rhyme label="b" type="m" corresp="#s12:l2">boog</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Triomf, o Vorst Uw <rhyme label="c" type="m">Huis</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Maak vrolyk <rhyme label="c" type="m" corresp="#s12:l5">Feestgedruis</rhyme>.</l>
        <l id="s12:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s12:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s12:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="2" type="-+">God zal u <rhyme label="a" type="f">schragen</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">o Vorst! in al uw <rhyme label="b" type="m">doen</rhyme>:</l>
        <l id="s13:l3" met="2" type="-+">Doe zyn <rhyme label="a" type="f" corresp="#s13:l1">behaagen</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">En keer het <rhyme label="b" type="m" corresp="#s13:l2">woên</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Van al 't Socyns <rhyme label="c" type="m">gespuis</rhyme>;</l>
        <l id="s13:l6" met="3" type="-+">Bouw Gy des Heeren <rhyme label="c" type="m" corresp="#s13:l5">Huis</rhyme>.</l>
        <l id="s13:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s13:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s13:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="2" type="-+">Geen list of <rhyme label="a" type="f">vleijen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Verandert uw <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s14:l3" met="2" type="-+">God zal u <rhyme label="a" type="f" corresp="#s14:l1">leijen</rhyme>,</l>
        <l id="s14:l4" met="2" type="-+">Die u <rhyme label="b" type="m" corresp="#s14:l2">behoed</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">Hy was uw Vad'ren <rhyme label="c" type="m">God</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Gy deelt in 't zelve <rhyme label="c" type="m" corresp="#s14:l5">Lot</rhyme>.</l>
        <l id="s14:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s14:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s14:l7">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="2" type="-+">Uw <rhyme label="a" type="f">Gemalinne</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Dat Cieraad van ons <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s15:l3" met="2" type="-+">Die Land <rhyme label="a" type="f" corresp="#s15:l1">Vorstinne</rhyme>,</l>
        <l id="s15:l4" met="2" type="-+">Dat dierbaar <rhyme label="b" type="m" corresp="#s15:l2">Pand</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Blyv' met uw Vorstelyk <rhyme label="c" type="m">Zaat</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">De vreugd van onzen <rhyme label="c" type="m" corresp="#s15:l5">Staat</rhyme>.</l>
        <l id="s15:l7" met="2" type="-+">Viva, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
        <l id="s15:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s15:l7">Hoezée</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
