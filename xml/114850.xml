  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O Coleta, rijck van <rhyme label="a" type="f">deughden</rhyme></l>
        <l id="s1:l2" met="4" type="+-">In 'nen al te hooghen <rhyme label="b" type="m">graet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die u cierden boven <rhyme label="b" type="m" corresp="#s1:l2">maet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hemel, aerde seer <rhyme label="a" type="f" corresp="#s1:l1">verheughden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wie isser in dit tranen <rhyme label="c" type="m">dal</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die u ghenoechsaem loven <rhyme label="c" type="m" corresp="#s1:l5">sal</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Gh' ebt, met iever Godts <rhyme label="a" type="f">onsteken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Claras Orden <rhyme label="b" type="m">opgherecht</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die ghy saeght doen seer <rhyme label="b" type="m" corresp="#s2:l2">verslecht</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">In haer strengicheydt <rhyme label="a" type="f" corresp="#s2:l1">ontbreken</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die gh' ebt ghebrocht tot d'eerste <rhyme label="c" type="m">jeught</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Self haer ghesticht tot alle <rhyme label="c" type="m" corresp="#s2:l5">deught</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Met Heer Iesvm in dit <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s3:l2" met="4" type="+-">U ghetrouwt Joannes <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Daer ghy eeuwich nu me <rhyme label="b" type="m" corresp="#s3:l2">leeft</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">U heeft hy 'nen rinck <rhyme label="a" type="f" corresp="#s3:l1">ghegheven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Godt ghetrouw die ghebleven <rhyme label="c" type="m">zijt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Met hem vereenicht in <rhyme label="c" type="m" corresp="#s3:l5">jolijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Om sijn liefde t' alle <rhyme label="a" type="f">tijden</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Hebdy op sijn Cruys <rhyme label="b" type="m">geacht</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En naer pijnen staegh <rhyme label="b" type="m" corresp="#s4:l2">ghetracht</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Aller Martelaren <rhyme label="a" type="f" corresp="#s4:l1">lijden</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Hebdy, nae uwen wensch <rhyme label="c" type="m">ghesmaect</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">In alle cruycen seer <rhyme label="c" type="m" corresp="#s4:l5">vermaect</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Waer toe Christus hier <rhyme label="a" type="f">beneden</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Oock onsteken heeft u <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Die vertelt heeft u de <rhyme label="b" type="m" corresp="#s5:l2">smert</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Van all' sijn ghequetste <rhyme label="a" type="f" corresp="#s5:l1">leden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En heeft d'een voor en d'ander <rhyme label="c" type="m">naer</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Aen u ghemaect hier <rhyme label="c" type="m" corresp="#s5:l5">openbaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Oock sijn Moeder hem vol <rhyme label="a" type="f">wonden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">U ghethoont heeft, kleyn en <rhyme label="b" type="m">teer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Nat oock van sijn traentjens <rhyme label="b" type="m" corresp="#s6:l2">seer</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Die u sey, dat hem de <rhyme label="a" type="f" corresp="#s6:l1">sonden</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Der kind'ren oock deden die <rhyme label="c" type="m">smert</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Dat hy daer van ghecruyst oock <rhyme label="c" type="m" corresp="#s6:l5">wert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hoe seer oock den Heer der <rhyme label="a" type="f">heeren</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Is gheweest van u <rhyme label="b" type="m">ghekent</rhyme></l>
        <l id="s7:l3" met="4" type="+-">In het heylich <rhyme label="b" type="m" corresp="#s7:l2">Sacrament</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat m'u wonderlijck sagh <rhyme label="a" type="f" corresp="#s7:l1">eeren</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Hongherich daer naer al te <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Om versaeit te zijn van den <rhyme label="c" type="m" corresp="#s7:l5">Heer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Die oock self u comen <rhyme label="a" type="f">gheven</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Heeft sijn lichaem en sijn <rhyme label="b" type="m">bloedt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Die u was soo <rhyme label="b" type="m" corresp="#s8:l2">oversoet</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En soo jonstich in dit <rhyme label="a" type="f" corresp="#s8:l1">leven</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Dat gh'er het eeuwich in <rhyme label="c" type="m">begost</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Droncken van den hemelschen <rhyme label="c" type="m" corresp="#s8:l5">most</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Waer van ghy versaeit daer <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Aen de Beke van <rhyme label="b" type="m">vvellust</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Uwen dorst nu eeuwich <rhyme label="b" type="m" corresp="#s9:l2">blust</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Godt daer eeuwich van sult <rhyme label="a" type="f" corresp="#s9:l1">loven</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Maer tot die Bek' ons oock <rhyme label="c" type="m">beleyt</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Door u ghebedt daer toe <rhyme label="c" type="m" corresp="#s9:l5">bereyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
