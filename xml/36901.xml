<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>36901</id>
    <date>
      <year-start>1677</year-start>
      <year-end>1677</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Si c'est pour mon pucelage</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WAt magh viesen dief <rhyme label="a" type="f">beweghen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">My het trouwen afte <rhyme label="b" type="m">raên</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ick en kan dat niet <rhyme label="b" type="m" corresp="#s1:l2">verstaen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Want 't en is my niet <rhyme label="a" type="f" corresp="#s1:l1">gheleghen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Af te slyten myne <rhyme label="c" type="m">jeught</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Sonder vrucht oft sõder <rhyme label="c" type="m" corresp="#s1:l5">vreugt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hoe sou liefde my <rhyme label="a" type="f">vervelen</rhyme>?</l>
        <l id="s2:l2" met="4" type="+-">Liefden is een lieve <rhyme label="b" type="m">saeck</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Liefden is vol soet <rhyme label="b" type="m" corresp="#s2:l2">vermaeck</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Met syn eygen maecksel <rhyme label="a" type="f" corresp="#s2:l1">spelen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En te groeyen tot een <rhyme label="c" type="m">stadt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">bloemen-deegh wat vreught is <rhyme label="c" type="m" corresp="#s2:l5">dat</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ick wil houwen, ick wil <rhyme label="a" type="f">trouwen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ick ben noch een jongh <rhyme label="b" type="m">gesel</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ick en vrees voor geen <rhyme label="b" type="m" corresp="#s3:l2">gequel</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Daer en is geẽ soort van <rhyme label="a" type="f" corresp="#s3:l1">vrouwẽ</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Die haer blyschap niet en <rhyme label="c" type="m">heeft</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Als- men reden plaetse <rhyme label="c" type="m" corresp="#s3:l5">gheeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Soo misschien een goet <rhyme label="a" type="f">geselle</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Die 't niet al te breedt en <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Trout met een van synen <rhyme label="b" type="m" corresp="#s4:l2">staet</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Hy kan sigh te vreden <rhyme label="a" type="f" corresp="#s4:l1">stellen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Wat geeft een kloeck Man om <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Samen winnen is soo <rhyme label="c" type="m" corresp="#s4:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Soo een Calistrouwt een <rhyme label="a" type="f">Rycke</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Och die isser dan wel <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Want hy magh dan spelen <rhyme label="b" type="m" corresp="#s5:l2">gaen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Vry wat meer als syns <rhyme label="a" type="f" corresp="#s5:l1">ghelycke</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Goet ghewonnen met de <rhyme label="c" type="m">min</rhyme>:</l>
        <l id="s5:l6" met="4" type="+-">Is dat niet een goet <rhyme label="c" type="m" corresp="#s5:l5">ghewin</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Soo daer een uyt suyver <rhyme label="a" type="f">minne</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Nemt syn minder metter <rhyme label="b" type="m">hant</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat is oock een geestigh <rhyme label="b" type="m" corresp="#s6:l2">quant</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Want hy trouwt met een <rhyme label="a" type="f" corresp="#s6:l1">vriendinne</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Die in liefden over <rhyme label="c" type="m">stort</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat s'in ryckdom schiet te <rhyme label="c" type="m" corresp="#s6:l5">kort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Gaeter iemandt sigh <rhyme label="a" type="f">verbinden</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Met een schoon en geestig <rhyme label="b" type="m">dier</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Is dat niet een Hemel <rhyme label="b" type="m" corresp="#s7:l2">schier</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Is sy lee lyck uw' <rhyme label="a" type="f" corresp="#s7:l1">Beminde</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">S'is misschien schoon in de <rhyme label="c" type="m">deught</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En dat is't dat't <rhyme label="c" type="m" corresp="#s7:l5">hert'verheught</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Soo g'aen een Lief kont <rhyme label="a" type="f">gheraken</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Die haer' saken wel <rhyme label="b" type="m">verstaet</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">G'hebt dan uwen <rhyme label="b" type="m" corresp="#s8:l2">toe-verlaet</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Maer verstaet sy haer geen' <rhyme label="a" type="f" corresp="#s8:l1">saken</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">G'hebt een duyf ken sonder <rhyme label="c" type="m">gal</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Ghy zyt meester <rhyme label="c" type="m" corresp="#s8:l5">over-al</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Is uw'vrouw soo out van <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Dat sy niet en kindert <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En bedroeft u niet te <rhyme label="b" type="m" corresp="#s9:l2">seer</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Want ghy meught dan spelen <rhyme label="a" type="f" corresp="#s9:l1">varen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">'t Huys en laet ghy gheenen <rhyme label="c" type="m">last</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Sonder Kinders, liever <rhyme label="c" type="m" corresp="#s9:l5">Gast</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Wordt u menigh Kindt <rhyme label="a" type="f">gheboren</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Daerom is het dat men <rhyme label="b" type="m">trouwt</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">En den echten Acker <rhyme label="b" type="m" corresp="#s10:l2">bouwt</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Sonder dat is't Landt <rhyme label="a" type="f" corresp="#s10:l1">verloren</rhyme></l>
        <l id="s10:l5" met="4" type="+-">'t Huys, de stadt, en heel het <rhyme label="c" type="m">ryck</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Steunen op het <rhyme label="c" type="m" corresp="#s10:l5">Houwelyck</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Heeft uw'Huys-vrouw slimme <rhyme label="a" type="f">ganghen</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Hinckt en springht sy op een <rhyme label="b" type="m">kruck</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">En maeckt daer - om gheenen <rhyme label="b" type="m" corresp="#s11:l2">druck</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Sy en moet geen Hasen <rhyme label="a" type="f" corresp="#s11:l1">vangen</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Ghy en hebt haer niet <rhyme label="c" type="m">getrouwt</rhyme></l>
        <l id="s11:l6" met="4" type="+-">Dat ghy met haer jaghen <rhyme label="c" type="m" corresp="#s11:l5">sout</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Is uw' vrouw vol quaey <rhyme label="a" type="f">manieren</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Spreckt s'u aen met <rhyme label="b" type="m">noordtsghedruys</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Siet nu leerdy binnens <rhyme label="b" type="m" corresp="#s12:l2">huys</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Ander-mans ghebreken <rhyme label="a" type="f" corresp="#s12:l1">vieren</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Is't niet goet dat ghy de <rhyme label="c" type="m">deught</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Sonder kost soo leeren <rhyme label="c" type="m" corresp="#s12:l5">meught</rhyme>?</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Liefde weet het al te <rhyme label="a" type="f">voeghen</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Liefde wint het quaedt in 't <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Liefde maeckt het bitter <rhyme label="b" type="m" corresp="#s13:l2">soet</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Liefde doet het hert' <rhyme label="a" type="f" corresp="#s13:l1">vernoegen</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Liefde vindt in als <rhyme label="c" type="m">gherief</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Noyt en vont-men leelyck <rhyme label="c" type="m" corresp="#s13:l5">Lief</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
