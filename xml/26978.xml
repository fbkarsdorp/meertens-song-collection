  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Schonn vnnd <rhyme label="a" type="m">zartt</rhyme> /</l>
        <l id="s1:l2" met="2" type="-+">vonn edler <rhyme label="a" type="m" corresp="#s1:l1">artt</rhyme> /</l>
        <l id="s1:l3" met="2" type="-+">erzeigett hast <rhyme label="b" type="m">dich</rhyme></l>
        <l id="s1:l4" met="2" type="-+">genn mir fre <rhyme label="b" type="m" corresp="#s1:l3">ndtlich</rhyme> /</l>
        <l id="s1:l5" met="2" type="-+">was das bed <rhyme label="c" type="m">tt</rhyme></l>
        <l id="s1:l6" met="2" type="-+">pleib mir der <rhyme label="c" type="m" corresp="#s1:l5">streitt</rhyme> /</l>
        <l id="s1:l7" met="2" type="-+">noch meinem <rhyme label="d" type="m">gir</rhyme> /</l>
        <l id="s1:l8" met="4" type="-+">all leib vnnd trew will ich mitt <rhyme label="d" type="m" corresp="#s1:l7">dir</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Sich <rhyme label="a" type="m">ich</rhyme></l>
        <l id="s2:l2" met="2" type="-+">daß gl ck v r <rhyme label="a" type="m" corresp="#s2:l1">sich</rhyme>  /</l>
        <l id="s2:l3" met="2" type="-+">kertt a ff mein <rhyme label="b" type="m">fartt</rhyme></l>
        <l id="s2:l4" met="2" type="-+">obs mich schonn <rhyme label="b" type="m" corresp="#s2:l3">hartt</rhyme> /</l>
        <l id="s2:l5" met="2" type="-+">woltt kommen <rhyme label="c" type="m">ahn</rhyme> /</l>
        <l id="s2:l6" met="2" type="-+">leitt mir nicht / <rhyme label="c" type="m" corresp="#s2:l5">dran</rhyme></l>
        <l id="s2:l7" met="2" type="-+">in fre denn <rhyme label="d" type="m">reich</rhyme> /</l>
        <l id="s2:l8" met="4" type="-+">wen d ß n r wiltt gillt mir <rhyme label="d" type="m" corresp="#s2:l7">gleich</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Lieb hatt kein <rhyme label="a" type="m">maß</rhyme> /</l>
        <l id="s3:l2" met="2" type="-+">bedenck f <rhyme label="a" type="m" corresp="#s3:l1">rbaß</rhyme></l>
        <l id="s3:l3" met="2" type="-+">der sagh ich <rhyme label="b" type="m">mein</rhyme> /</l>
        <l id="s3:l4" met="2" type="-+">n r dir <rhyme label="b" type="m" corresp="#s3:l3">allein</rhyme> /</l>
        <l id="s3:l5" met="2" type="-+">a s gantzer <rhyme label="c" type="m">gnad</rhyme> /</l>
        <l id="s3:l6" met="2" type="-+">so weiß  doch <rhyme label="c" type="m" corresp="#s3:l5">scheitt</rhyme> /</l>
        <l id="s3:l7" met="2" type="-+">das ich dich <rhyme label="d" type="m">laß</rhyme></l>
        <l id="s3:l8" met="4" type="-+">feins meglein fein / hab kein <rhyme label="d" type="m" corresp="#s3:l7">verdreiß</rhyme>.</l>
      </lg>
    </lg>
  </text>
