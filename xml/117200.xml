  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk kreeg laest in mijn <rhyme label="a" type="m">gemoet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Om mijn wat te <rhyme label="b" type="f">vertreden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een aerdig Meysje <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Was hups en fraey van <rhyme label="b" type="f" corresp="#s1:l2">Leden</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ick sprack haer toe, En sy sag mijn so veranderlijck <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s1:l6" met="5" type="+-">En ick vraegde of sy met mijn wou <rhyme label="c" type="m" corresp="#s1:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sy scheen terstont <rhyme label="a" type="m">bereyt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En was daer mee te <rhyme label="b" type="f">vreede</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sy heeft niet <rhyme label="a" type="m" corresp="#s2:l1">lang</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En sprak met soete <rhyme label="b" type="f" corresp="#s2:l2">reden</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Uyt wandelen te gaen, Hier buyten in ’t groene <rhyme label="c" type="m">Velt</rhyme>,</l>
        <l id="s2:l6" met="5" type="+-">Al waer hy zijn sinnen op haer had <rhyme label="c" type="m" corresp="#s2:l5">gestelt</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy gingen hant aen <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Al langs de groene <rhyme label="b" type="f">wegen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Digt aen een <rhyme label="a" type="m" corresp="#s3:l1">waterkant</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Is ’t Meysje neer <rhyme label="b" type="f" corresp="#s3:l2">gesegen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Want siet hier leyt, Ontbonden onsen <rhyme label="c" type="m">stryt</rhyme>,</l>
        <l id="s3:l6" met="5" type="+-">Wagt toe dat gy u wel dapper <rhyme label="c" type="m" corresp="#s3:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy trock sijn kort <rhyme label="a" type="m">geweer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hy heeft haer schilt <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Sy heeft na sijn <rhyme label="a" type="m" corresp="#s4:l1">begeer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Toen al sijn ledetjens <rhyme label="b" type="f" corresp="#s4:l2">beven</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Want siet terstont, So verloor hy sijn kragten by <rhyme label="c" type="m">haer</rhyme>,</l>
        <l id="s4:l6" met="5" type="+-">Waer door sy hoe langer hoe sterker <rhyme label="c" type="m" corresp="#s4:l5">waer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nog meenden desen <rhyme label="a" type="m">Knegt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat hy het al hadde <rhyme label="b" type="f">gewonnen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Maer door haer sterck <rhyme label="a" type="m" corresp="#s5:l1">gevegt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Was ’t werck maer eerst <rhyme label="b" type="f" corresp="#s5:l2">begonnen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Want siet desen Helt, Hoe dapper dat hy hem <rhyme label="c" type="m">stelt</rhyme>,</l>
        <l id="s5:l6" met="5" type="+-">En nogtans so moest hy verlaten het <rhyme label="c" type="m" corresp="#s5:l5">velt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy heeft terstont <rhyme label="a" type="m">ontdeckt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Haer suyvere naekte <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Haer Armpjes uyt <rhyme label="a" type="m" corresp="#s6:l1">gestreckt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En sprack met soete <rhyme label="b" type="f" corresp="#s6:l2">reden</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Treckt u der geweer, Komt worstelt nu eensjes met <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s6:l6" met="5" type="+-">En waerom dat wy hier gekomen <rhyme label="c" type="m" corresp="#s6:l5">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
