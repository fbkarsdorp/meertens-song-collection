<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188114</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>alkmaar</orginal-name>
      <standard-name>Alkmaar, The Netherlands</standard-name>
      <coordinates>
        <lon>52.6312592</lon>
        <lat>4.7503798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 023 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">LAat ons <rhyme label="a" type="m">eenpaar</rhyme></l>
        <l id="s1:l2" met="3" type="-+">De eenigheid der <rhyme label="b" type="f">zielen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Wel neemen <rhyme label="a" type="m" corresp="#s1:l1">waar</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En al in ons <rhyme label="b" type="f" corresp="#s1:l2">vernielen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Wat liefd en <rhyme label="c" type="m">vreed</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In ons zou moogen <rhyme label="d" type="f">krenken</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">'s Herten <rhyme label="c" type="m" corresp="#s1:l5">gebeed'</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Den Heer geduurig <rhyme label="d" type="f" corresp="#s1:l6">schenken</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Met een <rhyme label="e" type="m">ootmoed</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Wandelen voor Gods <rhyme label="f" type="f">oogen</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">In liefde <rhyme label="e" type="m" corresp="#s1:l9">zoet</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Tot malkander <rhyme label="f" type="f" corresp="#s1:l10">geboogen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Naa dat 't <rhyme label="a" type="m">gemoed</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Des menschen is <rhyme label="b" type="f">geleegen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Gods weezen <rhyme label="a" type="m" corresp="#s2:l1">zoet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Tot hem-waards is <rhyme label="b" type="f" corresp="#s2:l2">geneegen</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Is hy vol <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Liefd zal hem <rhyme label="d" type="f">weeder-baren</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Heeft hy strijd <rhyme label="c" type="m" corresp="#s2:l5">in</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Strijd zal hem ook <rhyme label="d" type="f" corresp="#s2:l6">beswaaren</rhyme>;</l>
        <l id="s2:l9" met="2" type="-+">Naa den <rhyme label="e" type="m">magneet</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Daar naa vloeien de <rhyme label="f" type="f">krachten</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Zoo wie dit <rhyme label="e" type="m" corresp="#s2:l9">weet</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Roep uit zijn qua'e <rhyme label="f" type="f" corresp="#s2:l10">gedachten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Verlaat u <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Op uw verstand of <rhyme label="b" type="f">weeten</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Al schijnt het <rhyme label="a" type="m" corresp="#s3:l1">iet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">'t Is maar stuk-werk <rhyme label="b" type="f" corresp="#s3:l2">geheeten</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Buig uwen <rhyme label="c" type="m">zin</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Onder gemeine <rhyme label="d" type="f">stichting</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Van booven <rhyme label="c" type="m" corresp="#s3:l5">in</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Vloeid wijsheids <rhyme label="d" type="f" corresp="#s3:l6">onder-richting</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Niet woord, maar <rhyme label="e" type="m">geest</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Kan 't hert alleen <rhyme label="f" type="f">geneezen</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Niet in <rhyme label="e" type="m" corresp="#s3:l9">tempeest</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Maar stilheid wil God <rhyme label="f" type="f" corresp="#s3:l10">weezen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Den <rhyme label="a" type="m">Noorden-wind</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Verderfd veel zoete <rhyme label="b" type="f">vruchten</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Dit elk <rhyme label="a" type="m" corresp="#s4:l1">bevind</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Als hy liefde voeld <rhyme label="b" type="f" corresp="#s4:l2">vluchten</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Van Hermon <rhyme label="c" type="m">komt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Den dau <rhyme label="d" type="f">needer-gesteegen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En maakt <rhyme label="c" type="m" corresp="#s4:l5">gezond</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Den vreede geefd den <rhyme label="d" type="f" corresp="#s4:l6">zeegen</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Als met <rhyme label="e" type="m">eendracht</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Gebroeders herten t' <rhyme label="f" type="f">zaamen</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Zijn in <rhyme label="e" type="m" corresp="#s4:l9">aandacht</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Haar woort is ja en <rhyme label="f" type="f" corresp="#s4:l10">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
