  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Sich wunder wol gemachit <rhyme label="a" type="m">wip</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">an dine reyne <rhyme label="b" type="f">guoitte</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">sich an dinen kuschen <rhyme label="a" type="m" corresp="#s1:l1">lip</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">sich an din ho <rhyme label="b" type="f" corresp="#s1:l2">gemuoitte</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">sich an dine ganze <rhyme label="c" type="m">dugent</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">sich an din lop. sich an <rhyme label="d" type="f">leben</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">sich an din vrauweliche <rhyme label="c" type="m" corresp="#s1:l5">jugent</rhyme>.</l>
        <l id="s1:l8" met="6" type="-+">sijs du dar an du macht dir selber vreude <rhyme label="d" type="f" corresp="#s1:l6">geben</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Eyn spegil diner augin <rhyme label="a" type="m">si</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">die kusze zallin <rhyme label="b" type="f">ziden</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">al sus bis du wandils <rhyme label="a" type="m" corresp="#s2:l1">vri</rhyme></l>
        <l id="s2:l4" met="3" type="-+">ob du dat macht ir <rhyme label="b" type="f" corresp="#s2:l2">striden</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">din hoe gemutte wnne <rhyme label="c" type="m">git</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">din vreude cleidit sich da <rhyme label="d" type="f">mide</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">da von wirt din siel de <rhyme label="c" type="m" corresp="#s2:l5">wit</rhyme>.</l>
        <l id="s2:l8" met="6" type="-+">din ere breit. din liep ziere in reynen <rhyme label="d" type="f" corresp="#s2:l6">siden</rhyme>.</l>
      </lg>
    </lg>
  </text>
