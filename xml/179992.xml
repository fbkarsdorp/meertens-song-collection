<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179992</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Alhier auf diesem Platz</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Lief, wilt gy by my <rhyme label="a" type="m">zyn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Neem dan deez' <rhyme label="a" type="m" corresp="#s1:l1">Colombyn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Gy krygt, wanneer gy <rhyme label="b" type="m">vlied</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dit Colombyntje <rhyme label="b" type="m" corresp="#s1:l3">niet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Indien gy stuursheid <rhyme label="c" type="m">haat</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Neem 't spritsje dat daar <rhyme label="c" type="m" corresp="#s1:l5">staat</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Maar als gy norsheid <rhyme label="d" type="m">mind</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Zoo laat my 't Spritsje <rhyme label="d" type="m" corresp="#s1:l7">kind</rhyme>;</l>
        <l id="s1:l9" met="3" type="-+">Want Sprits noch <rhyme label="e" type="m">Colombyn</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Mag voor geen fierheid <rhyme label="e" type="m" corresp="#s1:l9">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Indien ge uw hand my <rhyme label="a" type="m">gaaft</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gy wierd met Wyn <rhyme label="a" type="m" corresp="#s2:l1">gelaafd</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Maar weigerd gy me u <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">'k Maak dan myn fles niet <rhyme label="b" type="m" corresp="#s2:l3">kant</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Als gy me een lonkje <rhyme label="c" type="m">bood</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">'k Gaf u confyte <rhyme label="c" type="m" corresp="#s2:l5">Noot</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Al was het zelf een <rhyme label="d" type="m">paar</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Zo niet, ik geef ze een <rhyme label="d" type="m" corresp="#s2:l7">aÃªr</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Des, hebt gy smullens <rhyme label="e" type="m">lust</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Waar toe dan tyd <rhyme label="e" type="m" corresp="#s2:l9">verbrust</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ben ik een kus u <rhyme label="a" type="m">waard</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dan krygt gy deze <rhyme label="a" type="m" corresp="#s3:l1">Taart</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zo gy geen kus my <rhyme label="b" type="m">bied</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Krygt gy dit Taartje <rhyme label="b" type="m" corresp="#s3:l3">niet</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Gy lagt myn <rhyme label="c" type="m">Engelin</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ha! dat is naar myn <rhyme label="c" type="m" corresp="#s3:l5">zin</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Neem Noten, <rhyme label="d" type="m">Colombyn</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Sprits, Taart en lekkren <rhyme label="d" type="m" corresp="#s3:l7">Wyn</rhyme></l>
        <l id="s3:l9" met="3" type="-+">En eet en drinkt met <rhyme label="e" type="m">lust</rhyme>;</l>
        <l id="s3:l10" met="3" type="-+">Maar eerst gelonkt, <rhyme label="e" type="m" corresp="#s3:l9">gekust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
