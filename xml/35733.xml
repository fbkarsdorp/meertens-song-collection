    <melody>Nu laat ons allegaar</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy zijt, o Heer! soo milt en <rhyme label="a" type="m">goedt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat ghy ons maeckt <rhyme label="b" type="f">deelachtigh</rhyme></l>
        <l id="s1:l3" met="4" type="-+">U Hemels broot, u heyligh <rhyme label="a" type="m" corresp="#s1:l1">bloedt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daer by u vleesch <rhyme label="b" type="f" corresp="#s1:l2">waerachtigh</rhyme></l>
        <l id="s1:l5" met="4" type="-+">In 't Heyligh Autaers <rhyme label="c" type="m">Sacrament</rhyme>;</l>
        <l id="s1:l6" met="4" type="-+">Dat ghy ons voor een <rhyme label="c" type="m" corresp="#s1:l5">Testament</rhyme></l>
        <l id="s1:l7" met="3" type="-+">In 't eynde van u <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ons alle hebt <rhyme label="d" type="f" corresp="#s1:l7">gegeven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Met een dronck Wijn, en stickje <rhyme label="a" type="m">broot</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Komt ghy ons niet <rhyme label="b" type="f">betalen</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Ghy Godt en Mensch, ja Coningh <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In onse ziel komt <rhyme label="b" type="f" corresp="#s2:l2">dalen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">U lichaem gebaert in een <rhyme label="c" type="m">stal</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Komt in ons met uw' gaven <rhyme label="c" type="m" corresp="#s2:l5">al</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Van de Wijze <rhyme label="d" type="f">aengebeden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat aen 't Kruys heeft <rhyme label="d" type="f" corresp="#s2:l7">geleden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">U Testament, uw' woorde <rhyme label="a" type="m">zoet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hebben <rhyme label="b" type="f">d'Euangelisten</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Al, met Paulus gekeurt voor <rhyme label="a" type="m" corresp="#s3:l1">goedt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sonder tegens te <rhyme label="b" type="f" corresp="#s3:l2">twisten</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">d'Oudtvaders mede al hier <rhyme label="c" type="m">in</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Zijnen van een tongh, pen, en <rhyme label="c" type="m" corresp="#s3:l5">zin</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Met al de Wijze <rhyme label="d" type="f">scharen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van sesthien-hondert <rhyme label="d" type="f" corresp="#s3:l7">Iaren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Geen Moeder met haer vleesch en <rhyme label="a" type="m">bloet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sal haer waerdtse Kindt <rhyme label="b" type="f">spijzen</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Nochtans goe Harder ghy 't ons <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Niemant kan 't genoegh <rhyme label="b" type="f" corresp="#s4:l2">prijzen</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Als 't hout door lijm, yser door 't <rhyme label="c" type="m">sme'en</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Soo maeckt ghy ons Heer! met u <rhyme label="c" type="m" corresp="#s4:l5">een</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">In ons soo wilt ghy <rhyme label="d" type="f">weeze</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En wy in u met <rhyme label="d" type="f" corresp="#s4:l7">dese</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mijn Gast, mijn Al, mijn Spijs, mijn <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wilt altijdt by mijn <rhyme label="b" type="f">blijven</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Als Ionathas Hoonigh-raet, <rhyme label="a" type="m" corresp="#s5:l1">meer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Mijn flauwheyt wilt <rhyme label="b" type="f" corresp="#s5:l2">verstijven</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">O Pellicaen! opent u <rhyme label="c" type="m">borst</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Geeft my 't leven, verzaedt mijn <rhyme label="c" type="m" corresp="#s5:l5">dorst</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Fonteyn van alle <rhyme label="d" type="f">deughden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Geeft wasdom my, en <rhyme label="d" type="f" corresp="#s5:l7">vreughden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Schoonste parel, verborge <rhyme label="a" type="m">schat</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Weest mijn als Davids <rhyme label="b" type="f">steenen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Tegens den helsche <rhyme label="a" type="m" corresp="#s6:l1">Goliath</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Wilt Sampsons kracht <rhyme label="b" type="f" corresp="#s6:l2">verleenen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Manna vol smaeck; Gedeons <rhyme label="c" type="m">Swaert</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Maeckt all' mijn vyanden <rhyme label="c" type="m" corresp="#s6:l5">vervaert</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Een hoorns Soon wilt <rhyme label="d" type="f">verjagen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ziels-fenijn, Satans <rhyme label="d" type="f" corresp="#s6:l7">lagen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Alderwaerst' banquet, tarw, en <rhyme label="a" type="m">wijn</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Waer van de Maeghde <rhyme label="b" type="f">groeyen</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Maeghde Colom, en <rhyme label="a" type="m" corresp="#s7:l1">Medecijn</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Laet Venus lust niet <rhyme label="b" type="f" corresp="#s7:l2">bloeyen</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Zuyver Lam, en Gedeons <rhyme label="c" type="m">schilt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Geeft, Opper-hooft van 't Maeghde <rhyme label="c" type="m" corresp="#s7:l5">gilt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dat wy met reyne <rhyme label="d" type="f">leden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Met nieuw zangh u na <rhyme label="d" type="f" corresp="#s7:l7">treden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O Esaus kleedt vol soete <rhyme label="a" type="m">geur</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">Met Iacobs groote <rhyme label="b" type="f">zegen</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">O zoete balsem! 's hemels <rhyme label="a" type="m" corresp="#s8:l1">deur</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Door 's hemels douw en <rhyme label="b" type="f" corresp="#s8:l2">regen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Geeft dat mijn ziel, leden noch <rhyme label="c" type="m">hart</rhyme></l>
        <l id="s8:l6" met="4" type="-+">In 's werelts stanck verrot, <rhyme label="c" type="m" corresp="#s8:l5">verwart</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Bewaert mijn ziel van 't <rhyme label="d" type="f">sterven</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Verrotten en <rhyme label="d" type="f" corresp="#s8:l7">bederven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O vrucht van 's hemels <rhyme label="a" type="m">Paradijs</rhyme>!</l>
        <l id="s9:l2" met="3" type="-+">Vast pant van 's hemels <rhyme label="b" type="f">hoven</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Oly van de vijf Maeghden <rhyme label="a" type="m" corresp="#s9:l1">wijs</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Toet-steen van 't waer <rhyme label="b" type="f" corresp="#s9:l2">geloven</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Bruylofts-kleedt onder schijn van <rhyme label="c" type="m">broodt</rhyme>:</l>
        <l id="s9:l6" met="4" type="-+">Leydts-man, en teer-gelt in de <rhyme label="c" type="m" corresp="#s9:l5">doodt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Leydt ons alle te <rhyme label="d" type="f">zamen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">In 't eeuwigh leven, <rhyme label="d" type="f" corresp="#s9:l7">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
