<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14655</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Een Venus' diertje heb ik uitverkoren</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">STaet op Ierusalem, en laet u <rhyme label="a" type="f">Sonen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Vertoonen // doen een blijd' <rhyme label="b" type="m">aenschijn</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">En wilt u Dochteren met gulden <rhyme label="a" type="f" corresp="#s1:l1">Kroonen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Verschoonen // om bereyt te <rhyme label="b" type="m" corresp="#s1:l2">sijn</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Want Godes Sone komt haer met <rhyme label="b" type="m" corresp="#s1:l2">pijn</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Versoecken, om als Bruyt <rhyme label="c" type="m">eerbaer</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Voortaen te <rhyme label="d" type="f">trouwen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Dus wilt u <rhyme label="d" type="f" corresp="#s1:l7">rouwe</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Verlaten, in dit nieuwe <rhyme label="c" type="m" corresp="#s1:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Ey siet hoe suyverlijck komt een <rhyme label="a" type="f">Maghet</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En draget // hem aen haren <rhyme label="b" type="m">Borst</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Waer aen hy bitterlijck leyt en <rhyme label="a" type="f" corresp="#s2:l1">klaget</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Doch vraget // niet nae 's lichaem <rhyme label="b" type="m" corresp="#s2:l2">dorst</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Maer na u siele seer vuyl <rhyme label="b" type="m" corresp="#s2:l2">bemorst</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die hy komt suyveren met sijn bloet <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Waer door hy <rhyme label="d" type="f">heden</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Soeckt om <rhyme label="d" type="f" corresp="#s2:l7">besneden</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Te wesen, in dit nieuwe <rhyme label="c" type="m" corresp="#s2:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Dus wilt u Tempel doch op 't hoogst <rhyme label="a" type="f">vercieren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Steeckt vieren // tot u toorns <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">En singht Emanuel seer <rhyme label="a" type="f" corresp="#s3:l1">goedertiere</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Komt hiere // soecken sijn waerste <rhyme label="b" type="m" corresp="#s3:l2">Bruyt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hoe wel wy sitten onder <rhyme label="b" type="m" corresp="#s3:l2">tribuyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Van Satans wille boos' en <rhyme label="c" type="m">swaer</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Nochtans sijn <rhyme label="d" type="f">sinne</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Doen ons door <rhyme label="d" type="f" corresp="#s3:l7">minnen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Begeeren // in dit nieuwe <rhyme label="c" type="m" corresp="#s3:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Het geen dat Abraham seer lang' <rhyme label="a" type="f">begeerden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Vereerden // Godt hem inden <rhyme label="b" type="m">Gheest</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">En 'tgheen dat Salomon op dese <rhyme label="a" type="f" corresp="#s4:l1">eerden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Verveerden // was dit wonder <rhyme label="b" type="m" corresp="#s4:l2">meest</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Hoe sal, sprack hy, die 't al <rhyme label="b" type="m" corresp="#s4:l2">gheneest</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Hier woonen met den mensch <rhyme label="c" type="m">eenpaer</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Gheluckich <rhyme label="d" type="f">ooghen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Sijn 't, die dat <rhyme label="d" type="f" corresp="#s4:l7">mogen</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Aenschouwen, in dat Nieuwe <rhyme label="c" type="m" corresp="#s4:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Dus mogen wy nu dan met recht wel <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En springen // als de Herders <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Want wy van Bethelem veel wonder <rhyme label="a" type="f" corresp="#s5:l1">dinghen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Wt kringhen // sien tot ons <rhyme label="b" type="m" corresp="#s5:l2">behoet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">De vrou die Satan vertreden <rhyme label="b" type="m" corresp="#s5:l2">moet</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Heeft daer Godts Soon gebaert <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Dus sy Godt <rhyme label="d" type="f">Glorie</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">En ons <rhyme label="d" type="f" corresp="#s5:l7">victorie</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Tot vrede, in dit nieuwe <rhyme label="c" type="m" corresp="#s5:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Lof Prince Godt ghy die onse <rhyme label="a" type="f">klachte</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">By nachte // hebt te recht <rhyme label="b" type="m">verhoort</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Waer door ghy d'Hemelen met al haer <rhyme label="a" type="f" corresp="#s6:l1">machte</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met krachte // hebt gheheel <rhyme label="b" type="m" corresp="#s6:l2">door-boordt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Waer deur u soone neer daelde <rhyme label="b" type="m" corresp="#s6:l2">voort</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Tot Beth'lem in een krebbe <rhyme label="c" type="m">naer</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Wiens waerde <rhyme label="d" type="f">name</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Wy heden <rhyme label="d" type="f" corresp="#s6:l7">t'samen</rhyme></l>
        <l id="s6:l9" met="4" type="+-">Aen-roepen, in dit nieuwe <rhyme label="c" type="m" corresp="#s6:l6">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
