<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185847</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pange lingua</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Vvilt ontspringhen/lof sanck <rhyme label="a" type="f">singhen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Onsen Heer <rhyme label="b" type="m">ghebenedijt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Want hy doet ons groote <rhyme label="a" type="f" corresp="#s1:l1">dinghen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Hier of looft hem met <rhyme label="b" type="m" corresp="#s1:l2">iolijt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">En wilt v van quaet <rhyme label="a" type="f" corresp="#s1:l1">bedwinghen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Zijn weldaet ghedachtich <rhyme label="b" type="m" corresp="#s1:l2">zijt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ghy ingheseten wildy <rhyme label="a" type="f">weten</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Wat den heer ons heeft <rhyme label="b" type="m">ghedaen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Door sijn goetheyt <rhyme label="a" type="f" corresp="#s2:l1">ongemeten</rhyme>/</l>
        <l id="s2:l4" met="4" type="+-">Gheeft hy ons zijn lichaem <rhyme label="b" type="m" corresp="#s2:l2">zaen</rhyme>/</l>
        <l id="s2:l5" met="4" type="+-">Voor een spyse om te <rhyme label="a" type="f" corresp="#s2:l1">eeten</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Wilt hem waerdelijck <rhyme label="b" type="m" corresp="#s2:l2">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Proeft v sinnen wel van <rhyme label="a" type="f">binnen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Eer dat ghy ontfanghen <rhyme label="b" type="m">sult</rhyme>/</l>
        <l id="s3:l3" met="4" type="+-">Desen groten schadt der <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme>/</l>
        <l id="s3:l4" met="4" type="+-">Die hemel en aert <rhyme label="b" type="m" corresp="#s3:l2">vervult</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Eer dat ghy dit sult <rhyme label="a" type="f" corresp="#s3:l1">beghinnen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Spreeckt ootmoedich v <rhyme label="b" type="m" corresp="#s3:l2">schult</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Groot en cleyne// maeckt v <rhyme label="a" type="f">reyne</rhyme>/</l>
        <l id="s4:l2" met="4" type="+-">En treckt aen het bruylofts <rhyme label="b" type="m">cleyt</rhyme>/</l>
        <l id="s4:l3" met="4" type="+-">Comt ter tafelen Gods <rhyme label="a" type="f" corresp="#s4:l1">ghemeyne</rhyme>/</l>
        <l id="s4:l4" met="4" type="+-">Aensiet wat daer is <rhyme label="b" type="m" corresp="#s4:l2">bereyt</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Gods lichaem en bloet <rhyme label="a" type="f" corresp="#s4:l1">certeyne</rhyme>/</l>
        <l id="s4:l6" met="4" type="+-">Ghelijck hy ons selve <rhyme label="b" type="m" corresp="#s4:l2">seyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wat cost hy ons meerder <rhyme label="a" type="f">geven</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Dan dit heylich <rhyme label="b" type="m">Testament</rhyme>/</l>
        <l id="s5:l3" met="4" type="+-">Dit levende broot <rhyme label="a" type="f" corresp="#s5:l1">verheven</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Vanden Hemel neer <rhyme label="b" type="m" corresp="#s5:l2">ghesent</rhyme>/</l>
        <l id="s5:l5" met="4" type="+-">Dat wy eeuwich souden <rhyme label="a" type="f" corresp="#s5:l1">leven</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Gheeft ons dese spijse <rhyme label="b" type="m" corresp="#s5:l2">ient</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dese spijse, groot van <rhyme label="a" type="f">prijse</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Liefde in ons Siele <rhyme label="b" type="m">straelt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wt des Hemels <rhyme label="a" type="f" corresp="#s6:l1">Paradijse</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Is voor ons hier neer <rhyme label="b" type="m" corresp="#s6:l2">ghedaelt</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Looft hem metter Engelen <rhyme label="a" type="f" corresp="#s6:l1">wijse</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Van dese spijse <rhyme label="b" type="m" corresp="#s6:l2">ongefaelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Glorie lof hoocheyt en <rhyme label="a" type="f">eere</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Sy die heylige <rhyme label="b" type="m">triniteyt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die ons arme kinderen <rhyme label="a" type="f" corresp="#s7:l1">teere</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Dees spijse heeft <rhyme label="b" type="m" corresp="#s7:l2">bereyt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">V loven wy onsen <rhyme label="a" type="f" corresp="#s7:l1">heere</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Nu en inder <rhyme label="b" type="m" corresp="#s7:l2">eewicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
