  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Stilt uwe <rhyme label="a" type="f">ganghen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Lifste laet my <rhyme label="a" type="f" corresp="#s1:l1">langhen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Van u witte <rhyme label="a" type="f" corresp="#s1:l1">wanghen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Een Provensche <rhyme label="b" type="m">roos</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die noyt van kau <rhyme label="b" type="m" corresp="#s1:l4">bevroos</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Laet my <rhyme label="c" type="f">ghenieten</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Die twee honinck <rhyme label="c" type="f" corresp="#s1:l6">vlieten</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Die als <rhyme label="d" type="m">portael</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Staen voor d'abaste <rhyme label="d" type="m" corresp="#s1:l8">Sael</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">U loddelycke <rhyme label="e" type="f">loncken</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Doen my t'hertjen <rhyme label="e" type="f" corresp="#s1:l10">voncken</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Clorinde helpt myn <rhyme label="d" type="m" corresp="#s1:l8">quael</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Uyt wat een <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Tyter Komt die <rhyme label="a" type="f" corresp="#s2:l1">beden</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Dat ghy nu op <rhyme label="a" type="f" corresp="#s2:l1">hede</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Bidt om eenen <rhyme label="b" type="m">soen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hebt ghyer twee van <rhyme label="b" type="m" corresp="#s2:l4">doen</rhyme>?</l>
        <l id="s2:l6" met="2" type="-+">Wilt u vry <rhyme label="c" type="f">spoeden</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">'kMoet myn schaepkens <rhyme label="c" type="f" corresp="#s2:l6">hoeden</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">O Soetste <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Ontfanght dat tot een <rhyme label="d" type="m" corresp="#s2:l8">groet</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Ick sal u schaepkens <rhyme label="e" type="f">leyden</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Naer de klavere <rhyme label="e" type="f" corresp="#s2:l10">weyden</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">My sulcken eer nu <rhyme label="d" type="m" corresp="#s2:l8">doet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">3.My soo te <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Sou myn gunst <rhyme label="a" type="f" corresp="#s3:l1">vermeeren</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Laet ons <rhyme label="a" type="f" corresp="#s3:l1">weder-keeren</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">'kSien als dat de <rhyme label="b" type="m">Son</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Schier is daer sy <rhyme label="b" type="m" corresp="#s3:l4">begon</rhyme>.</l>
        <l id="s3:l6" met="2" type="-+">De <rhyme label="c" type="f">avont-straelen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Die het voetsel <rhyme label="c" type="f" corresp="#s3:l6">haelen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Van Phebus <rhyme label="d" type="m">licht</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Zyn self u soet <rhyme label="d" type="m" corresp="#s3:l8">ghesicht</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Gheen duyster kan ons <rhyme label="e" type="f">krincken</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Soo langh u ooghskens <rhyme label="e" type="f" corresp="#s3:l10">blincken</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Verselt met <rhyme label="d" type="m" corresp="#s3:l8">'tminne-wicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">U lodder <rhyme label="a" type="f">ooghen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Herder my <rhyme label="a" type="f" corresp="#s4:l1">vertooghen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Dat ghy van <rhyme label="a" type="f" corresp="#s4:l1">vermooghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Soet, en krachtigh <rhyme label="b" type="m">zyt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Vertreckt, vertreckt 'tis <rhyme label="b" type="m" corresp="#s4:l4">tydt</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Clorind wilt <rhyme label="c" type="f">beyden</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">'kSal u vee <rhyme label="c" type="f" corresp="#s4:l6">gheleyden</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Neen, Herder <rhyme label="d" type="m">neen</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Laet my den last <rhyme label="d" type="m" corresp="#s4:l8">alleen</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Vaert wel dan <rhyme label="e" type="f">Herderinne</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">'kBlyf u liefste <rhyme label="e" type="f" corresp="#s4:l10">minne</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Ick u, en anders <rhyme label="d" type="m" corresp="#s4:l8">gheen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
