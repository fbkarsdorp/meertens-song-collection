<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4047</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
      <tag>spiritual song</tag>
      <tag>Maria song</tag>
    </tags>
    <melody>Och lig dij nu en slaapt</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TOt groter <rhyme label="a" type="m">weerdicheyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Der hemelscher <rhyme label="b" type="f">coninghinnen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Soe wick sonder <rhyme label="a" type="m" corresp="#s1:l1">verbeyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Een lof nu gaen <rhyme label="b" type="f" corresp="#s1:l2">beghinnen</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Sy heeft ons allen <rhyme label="c" type="m">verblijt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Lof si haer inden <rhyme label="d" type="f">throone</rhyme></l>
        <l id="s1:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s1:l5">wijt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En was noyt gheen so <rhyme label="d" type="f" corresp="#s1:l6">schoone</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Om die sonden van <rhyme label="a" type="m">Adam</rhyme></l>
        <l id="s2:l2" met="3" type="-+">So waren wij al in <rhyme label="b" type="f">liden</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Tot dat dese maghet <rhyme label="a" type="m" corresp="#s2:l1">quam</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Lof si haer tot allen <rhyme label="b" type="f" corresp="#s2:l2">tiden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Want si heeft ons <rhyme label="c" type="m">beurijdt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Al vander helscher <rhyme label="d" type="f">woone</rhyme></l>
        <l id="s2:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s2:l5">wijt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En was noyt gheen so <rhyme label="d" type="f" corresp="#s2:l6">schoone</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe mach eenich mensche <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Alsoe ontsint van <rhyme label="b" type="f">binnen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die niet en dient dit <rhyme label="a" type="m" corresp="#s3:l1">maechdekijn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat daer is vol van <rhyme label="b" type="f" corresp="#s3:l2">minnen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">wie haer begeert tot eenige <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Si helpt hem sonder <rhyme label="d" type="f">hoone</rhyme></l>
        <l id="s3:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s3:l5">wijt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En was noyt gheen so <rhyme label="d" type="f" corresp="#s3:l6">schoone</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Si is edel ende <rhyme label="a" type="m">rijcke</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ende schoon bouen alle <rhyme label="b" type="f">vrouwen</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Men vint niet haers <rhyme label="a" type="m" corresp="#s4:l1">ghelijcke</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Aen haer staet mijn <rhyme label="b" type="f" corresp="#s4:l2">betrouwen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Alle schoonheyt deser <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En is hier niet een <rhyme label="d" type="f">boone</rhyme></l>
        <l id="s4:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s4:l5">wijt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En was noyt gheen so <rhyme label="d" type="f" corresp="#s4:l6">schoone</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Minen troost al aen haer <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Si heeft een minnelijc <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Een ioncfroulijck <rhyme label="a" type="m" corresp="#s5:l1">ghelaet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Alle druck can si <rhyme label="b" type="f" corresp="#s5:l2">ghenesen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Al in des hemels <rhyme label="c" type="m">iolijt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Draecht si een gulden <rhyme label="d" type="f">croone</rhyme></l>
        <l id="s5:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s5:l5">wijt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En was noyt gheen so <rhyme label="d" type="f" corresp="#s5:l6">schoone</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Van vrouwen men wel <rhyme label="a" type="m">leest</rhyme>.</l>
        <l id="s6:l2" met="3" type="-+">Die schoon waren <rhyme label="b" type="f">wtghenomen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">mer voer haer is gheen <rhyme label="a" type="m" corresp="#s6:l1">geweest</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Oft na haer en sal geen <rhyme label="b" type="f" corresp="#s6:l2">comen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Haers ghelijcke / dyes seker <rhyme label="c" type="m">sijt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Alsoe ick wel <rhyme label="d" type="f">bethoone</rhyme></l>
        <l id="s6:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s6:l5">wijt</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En was noyt gheen so <rhyme label="d" type="f" corresp="#s6:l6">schoone</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laet ons dan <rhyme label="a" type="m">hooghelijck</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dienen dese maghet <rhyme label="b" type="f">puere</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Inden hemel oft in <rhyme label="a" type="m" corresp="#s7:l1">aertrijcke</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En was nye sulcke <rhyme label="b" type="f" corresp="#s7:l2">creatuere</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Op dat wi na dit <rhyme label="c" type="m">ouerlijt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Haer moghen hebben te <rhyme label="d" type="f">loone</rhyme></l>
        <l id="s7:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s7:l5">wijt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">En was noyt gheen so <rhyme label="d" type="f" corresp="#s7:l6">schoone</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O minnelike <rhyme label="a" type="m">maecht</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Beminde Gods <rhyme label="b" type="f">vriendinne</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Mijn hert al tot v <rhyme label="a" type="m" corresp="#s8:l1">draecht</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ghi sijt mijn liefste <rhyme label="b" type="f" corresp="#s8:l2">minne</rhyme></l>
        <l id="s8:l5" met="3" type="-+">O Maria <rhyme label="c" type="m">ghebenedijt</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">Bidt voer mi uwen <rhyme label="d" type="f">sone</rhyme></l>
        <l id="s8:l7" met="3" type="-+">In alle die werelt <rhyme label="c" type="m" corresp="#s8:l5">wijt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">En weet ick gheen so <rhyme label="d" type="f" corresp="#s8:l6">schone</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
