<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9190</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O welig Amsterdam</melody>
  <author>E. Pels de Jonge</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VVel Duysje wat ick <rhyme label="a" type="m">hoor</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Binje de Bruyt met <rhyme label="a" type="m" corresp="#s1:l1">Floor</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Pietje Brancken <rhyme label="b" type="m">Broer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ay secht ist waer of <rhyme label="c" type="m">jock</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ick mit die <rhyme label="c" type="m" corresp="#s1:l4">gallichbrock</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Neen, deynckt dat niet <rhyme label="b" type="m" corresp="#s1:l3">Moer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Secht wie het dat <rhyme label="a" type="m">eseyt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is tien Knecht of ien <rhyme label="a" type="m" corresp="#s2:l1">Meyt</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">'tIs jock, 'tis niet <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ick heb him niet <rhyme label="c" type="m">esien</rhyme></l>
        <l id="s2:l5" met="3" type="-+">(wat het het te <rhyme label="c" type="m" corresp="#s2:l4">bedien</rhyme>)</l>
        <l id="s2:l6" met="3" type="+-">In gien hallef <rhyme label="b" type="m" corresp="#s2:l3">Jaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Heer Duysje hoe jock <rhyme label="a" type="m">jy</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ick sach hem aenje <rhyme label="a" type="m" corresp="#s3:l1">sy</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Guster avont <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Je waert doe wel <rhyme label="c" type="m">emoet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hy het mee noch <rhyme label="c" type="m" corresp="#s3:l4">egroet</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Doen hy my sach <rhyme label="b" type="m" corresp="#s3:l3">gaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O neen hy was het <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">'tWas Syme Neef en <rhyme label="a" type="m" corresp="#s4:l1">Griet</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Die hadd'we <rhyme label="b" type="m">enoot</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Se stonden aen de <rhyme label="c" type="m">deur</rhyme></l>
        <l id="s4:l5" met="3" type="-+">j'Hebt ien verkierde <rhyme label="c" type="m" corresp="#s4:l4">veur</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Maeckje niet soo <rhyme label="b" type="m" corresp="#s4:l3">groot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ja Duysje praetje <rhyme label="a" type="m">wat</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">'tIs door de hiele <rhyme label="a" type="m" corresp="#s5:l1">Stadt</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Datje bint de <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">'tIs van jou datmen <rhyme label="c" type="m">praet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Waer datmee gaet of <rhyme label="c" type="m" corresp="#s5:l4">staet</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Gaet maer iensjes <rhyme label="b" type="m" corresp="#s5:l3">uyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer Haesje watje <rhyme label="a" type="m">secht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sou ick die jonge <rhyme label="a" type="m" corresp="#s6:l1">Knecht</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Die kaelbeck <rhyme label="b" type="m">aenslaen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Och Haesje het waer <rhyme label="c" type="m">schant</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ick mocht dan after <rhyme label="c" type="m" corresp="#s6:l4">Landt</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Men Broot bidden <rhyme label="b" type="m" corresp="#s6:l3">gaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy hetmee wel <rhyme label="a" type="m">evrijt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Maer hy het niet en <rhyme label="a" type="m" corresp="#s7:l1">mijt</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">`tIs ien kael <rhyme label="b" type="m">neet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">´tIs waer ick ben de <rhyme label="c" type="m">Bruyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">´tHoochste woort moeter <rhyme label="c" type="m" corresp="#s7:l4">uyt</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Omdat jijt niet <rhyme label="b" type="m" corresp="#s7:l3">weet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Duysje wat ist voor <rhyme label="a" type="m">ien</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ick moetje eluck <rhyme label="a" type="m" corresp="#s8:l1">bien</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Ist ien Knecht die'k <rhyme label="b" type="m">ken</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Maer'tis met Swaentjes <rhyme label="c" type="m">Broer</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Jutje Jans dat's sen <rhyme label="c" type="m" corresp="#s8:l4">Moer</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">En hy hiet <rhyme label="b" type="m" corresp="#s8:l3">Harmen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick weet nou wie je <rhyme label="a" type="m">mient</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Ons Keesjes beste <rhyme label="a" type="m" corresp="#s9:l1">vrient</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">'tIs ien brave <rhyme label="b" type="m">langhst</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ja wel ist met die <rhyme label="c" type="m">giest</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Tot Quinten is hy <rhyme label="c" type="m" corresp="#s9:l4">miest</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Want hy aerdich <rhyme label="b" type="m" corresp="#s9:l3">danghst</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
