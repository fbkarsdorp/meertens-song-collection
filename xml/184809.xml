  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">SInght <rhyme label="a" type="f">victori'</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">In de <rhyme label="a" type="f" corresp="#s1:l1">glori'</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daer Godt m√© is <rhyme label="b" type="m">op-gestaen</rhyme>:</l>
        <l id="s1:l4" met="2" type="-+">Naer pyn <rhyme label="c" type="f">verdraegen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Licht doodt <rhyme label="c" type="f" corresp="#s1:l4">verslaegen</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Moet nu <rhyme label="d" type="f">swichten</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Met haer <rhyme label="d" type="f" corresp="#s1:l6">schichten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">En haer heerschappy' <rhyme label="b" type="m" corresp="#s1:l3">af-gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Hem <rhyme label="a" type="f">vercieren</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Voor <rhyme label="a" type="f" corresp="#s2:l1">Lauwrieren</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Wonder gaeven, klaer <rhyme label="b" type="m">gesicht</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">In Godt, <rhyme label="c" type="f">verresen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Syn <rhyme label="c" type="f" corresp="#s2:l4">ut-gelesen</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Schoonder <rhyme label="d" type="f">straelen</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Alser <rhyme label="d" type="f" corresp="#s2:l6">daelen</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Van de Sonn', en <rhyme label="b" type="m" corresp="#s2:l3">t'Maene-licht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Siet <rhyme label="a" type="f">verschynen</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Naer veel <rhyme label="a" type="f" corresp="#s3:l1">pynen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Weerde Moeder, uwen <rhyme label="b" type="m">pandt</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Uw' Soon naer <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Komt u <rhyme label="c" type="f" corresp="#s3:l4">verblyden</rhyme>;</l>
        <l id="s3:l6" met="2" type="+-">Sich <rhyme label="d" type="f">verthoonen</rhyme></l>
        <l id="s3:l7" met="2" type="+-">Met de <rhyme label="d" type="f" corresp="#s3:l6">Kroonen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Die hy draeght uyt vyandts <rhyme label="b" type="m" corresp="#s3:l3">handt</rhyme>.</l>
      </lg>
    </lg>
  </text>
