  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE zinnekeur doet vaak <rhyme label="a" type="f">betreuren</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Het gunme <rhyme label="b" type="m">kiest</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Byzonder, alsmen in ‘et <rhyme label="a" type="f" corresp="#s1:l1">keuren</rhyme></l>
        <l id="s1:l4" met="2" type="-+">De keur <rhyme label="b" type="m" corresp="#s1:l2">verliest</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Met my ist eevezoo <rhyme label="c" type="f">geloopen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Dat tuigd den <rhyme label="d" type="m">rouw</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">En ‘t vainzig, knijpend trekkend <rhyme label="c" type="f" corresp="#s1:l5">noopen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Nu voormen <rhyme label="d" type="m" corresp="#s1:l6">Trouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
