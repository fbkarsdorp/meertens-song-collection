<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24556</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Puer nobis nascitur</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O   Ons is gebaren een <rhyme label="a" type="m">kyndekyn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">noch claere dan die <rhyme label="b" type="f">sonne</rhyme></l>
        <l id="s1:l3" met="4" type="-+">dat sal ons alle vroude <rhyme label="a" type="m" corresp="#s1:l1">syn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">al totter enghelen <rhyme label="b" type="f" corresp="#s1:l2">wonne</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die steernen geuen ons lichten <rhyme label="a" type="m">schyn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">al doer den hemel <rhyme label="b" type="f">gedrongen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Maria die heeft hoer lieue <rhyme label="a" type="m" corresp="#s2:l1">kynt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">mit ganser mynnen <rhyme label="b" type="f" corresp="#s2:l2">gewonnen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy legden in een <rhyme label="a" type="m">crybbekyn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">mit oren snee witsen <rhyme label="b" type="f">handen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">sy cusseden voer syn <rhyme label="a" type="m" corresp="#s3:l1">mondelyn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">des had oer seer <rhyme label="b" type="f" corresp="#s3:l2">verlangen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat kyndekyn spoelden der moeder <rhyme label="a" type="m">toe</rhyme></l>
        <l id="s4:l2" met="3" type="-+">mit synen cleynen <rhyme label="b" type="f">armkens</rhyme></l>
        <l id="s4:l3" met="4" type="-+">het lachgede oer alsoe suetelicken <rhyme label="a" type="m" corresp="#s4:l1">toe</rhyme></l>
        <l id="s4:l4" met="3" type="-+">mit blyden blenckenden <rhyme label="b" type="f" corresp="#s4:l2">ogekyns</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Waldy waldy <rhyme label="a" type="m">Joncfrouwelyn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">der sueter weerder <rhyme label="b" type="f">stonden</rhyme></l>
        <l id="s5:l3" met="4" type="-+">dattu dat suete <rhyme label="a" type="m" corresp="#s5:l1">kyndekyn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">mit ganser mynnen hebt <rhyme label="b" type="f" corresp="#s5:l2">gewonnen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Nv swyget nv swyget myn <rhyme label="a" type="m">kyndekyn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">myn god myn troest myn <rhyme label="b" type="f">here</rhyme></l>
        <l id="s6:l3" met="4" type="-+">van di soe sal ic moeder <rhyme label="a" type="m" corresp="#s6:l1">syn</rhyme></l>
        <l id="s6:l4" met="3" type="-+">beholden mynre <rhyme label="b" type="f" corresp="#s6:l2">eren</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Een kynt is ons <rhyme label="a" type="m">gebaren</rhyme></l>
        <l id="s7:l2" met="3" type="-+">een soen is ons <rhyme label="b" type="f">gegeuen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">hi heeft versoent syns vaders <rhyme label="a" type="m" corresp="#s7:l1">toern</rhyme></l>
        <l id="s7:l4" met="3" type="-+">hi brenget ons int ewighe <rhyme label="b" type="f" corresp="#s7:l2">leuen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Doe Jhesus christus gebaren <rhyme label="a" type="m">was</rhyme></l>
        <l id="s8:l2" met="3" type="-+">gewonden in armen <rhyme label="b" type="f">doeken</rhyme></l>
        <l id="s8:l3" met="4" type="-+">daer en was bont daer en was <rhyme label="a" type="m" corresp="#s8:l1">graeu</rhyme></l>
        <l id="s8:l4" met="3" type="-+">hi lach mit bloeten <rhyme label="b" type="f" corresp="#s8:l2">voeten</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die osse ende oec dat <rhyme label="a" type="m">eselkyn</rhyme></l>
        <l id="s9:l2" met="3" type="-+">en konden niet <rhyme label="b" type="f">gespreken</rhyme></l>
        <l id="s9:l3" met="4" type="-+">doe Jhesus inder crybben <rhyme label="a" type="m" corresp="#s9:l1">lach</rhyme></l>
        <l id="s9:l4" met="3" type="-+">doe lieten sy oer <rhyme label="b" type="f" corresp="#s9:l2">eten</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Die osse ende oec dat <rhyme label="a" type="m">eselkyn</rhyme></l>
        <l id="s10:l2" met="3" type="-+">die dreuen daer grote <rhyme label="b" type="f">feeste</rhyme></l>
        <l id="s10:l3" met="4" type="-+">doe Jhesus inder crybben <rhyme label="a" type="m" corresp="#s10:l1">lach</rhyme></l>
        <l id="s10:l4" met="3" type="-+">tusschen tween stommen <rhyme label="b" type="f" corresp="#s10:l2">besten</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Die osse ende oec dat <rhyme label="a" type="m">eselkyn</rhyme></l>
        <l id="s11:l2" met="3" type="-+">die dreuen daer groten <rhyme label="b" type="f">wonder</rhyme></l>
        <l id="s11:l3" met="4" type="-+">doe Jhesus inder crybben <rhyme label="a" type="m" corresp="#s11:l1">lach</rhyme></l>
        <l id="s11:l4" met="3" type="-+">in crancken doeken <rhyme label="b" type="f" corresp="#s11:l2">gewonden</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">O et o et <rhyme label="a" type="m">gloria</rhyme></l>
        <l id="s12:l2" met="3" type="-+">dat suetste vander <rhyme label="b" type="f">mynnen</rhyme></l>
        <l id="s12:l3" met="4" type="-+">dat is Jhesus marien <rhyme label="a" type="m" corresp="#s12:l1">soen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">god laten ons <rhyme label="b" type="f" corresp="#s12:l2">gewynnen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
