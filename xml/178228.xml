<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178228</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Hoe zoet is het daar de vriendschap woont</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wy armen Schoppen zyn in <rhyme label="a" type="m">druk</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wy worden steeds <rhyme label="b" type="m">gekweld</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die Antipothers met haar <rhyme label="a" type="m" corresp="#s1:l1">kruk</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verschynen tans in 't <rhyme label="b" type="m" corresp="#s1:l2">veld</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Die Draaken slaan een groote <rhyme label="c" type="m">front</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">De Duivel schoptse voor haar <rhyme label="c" type="m" corresp="#s1:l5">kont</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Men schendt en vloekt den <rhyme label="a" type="m">Admiraal</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die nooyt iets heeft <rhyme label="b" type="m">misdaan</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Elk hunner is vervloekt <rhyme label="a" type="m" corresp="#s2:l1">brutaal</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Waar komt dit dog van <rhyme label="b" type="m" corresp="#s2:l2">daan</rhyme>?</l>
        <l id="s2:l5" met="4" type="-+">'t Is meest al Eigenbaat en <rhyme label="c" type="m">trots</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Maar wy staan pal gelyk een <rhyme label="c" type="m" corresp="#s2:l5">rots</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Vryheid die men 't meest <rhyme label="a" type="m">bemind</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">In onze <rhyme label="b" type="m">Magestraat</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Oranje, Neêrlands beste <rhyme label="a" type="m" corresp="#s3:l1">vrind</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Het Hooft van onzen <rhyme label="b" type="m" corresp="#s3:l2">Staat</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Daar leeven wy mee wel <rhyme label="c" type="m">gemoed</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Geen Teering die ons kwynen <rhyme label="c" type="m" corresp="#s3:l5">doed</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Vryheid is reeds lang <rhyme label="a" type="m">verkragt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Door dat Moordzugtig <rhyme label="b" type="m">rot</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat rukeloos eed en pligt <rhyme label="a" type="m" corresp="#s4:l1">veragt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Wat heilig is <rhyme label="b" type="m" corresp="#s4:l2">bespot</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Maar wy, wy zyn de zelfde <rhyme label="c" type="m">nog</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">In weerwil van hun snood <rhyme label="c" type="m" corresp="#s4:l5">bedrog</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Pylen, die d'Oranje <rhyme label="a" type="m">band</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sints lang gebonden <rhyme label="b" type="m">houd</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Zyn hun, die Pesten van ons <rhyme label="a" type="m" corresp="#s5:l1">Land</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Waaragtig niet <rhyme label="b" type="m" corresp="#s5:l2">betroud</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Zoo lang de Hemel ons <rhyme label="c" type="m">behoed</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Zingen wy met vereende <rhyme label="c" type="m" corresp="#s5:l5">moed</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Constitutie van ons <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Is door den Prins <rhyme label="b" type="m">vereerd</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Men vind in Kruijer, Post en <rhyme label="a" type="m" corresp="#s6:l1">Krant</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Boosaardig dit <rhyme label="b" type="m" corresp="#s6:l2">verkeerd</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Men wil den Prins in boeijen <rhyme label="c" type="m">slaan</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Dat krygt men immers nooyt <rhyme label="c" type="m" corresp="#s6:l5">gedaan</rhyme>?</l>
        <l id="s6:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Vrybuyters, die u steeds <rhyme label="a" type="m">beroemt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Als waart gy <rhyme label="b" type="m">Patriot</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">De Prinsgezinden Schoppen <rhyme label="a" type="m" corresp="#s7:l1">noemt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Denkt aan u droevig <rhyme label="b" type="m" corresp="#s7:l2">Lot</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Wy Schoppen u nog eens tot <rhyme label="c" type="m">stront</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">En roepen dan uit eenen <rhyme label="c" type="m" corresp="#s7:l5">Mont</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Des Hemels gunste daal' op <rhyme label="a" type="m">Hem</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Voor wien ons herte <rhyme label="b" type="m">blaakt</rhyme>!</l>
        <l id="s8:l3" met="4" type="-+">Wy Zingen met vereende <rhyme label="a" type="m" corresp="#s8:l1">stem</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die tot de Wolken <rhyme label="b" type="m" corresp="#s8:l2">raakt</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Leeff lang, ô Prins! in rust en <rhyme label="c" type="m">vree</rhyme>!</l>
        <l id="s8:l6" met="4" type="-+">Kom, Draaken! Zingt met Schoppen <rhyme label="c" type="m" corresp="#s8:l5">mee</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wy heeten by dat Vee een <rhyme label="a" type="m">Schop</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Maar zy zyn <rhyme label="b" type="m">Barnevels</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">Wy Schoppen hangen ons niet <rhyme label="a" type="m" corresp="#s9:l1">op</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Als die van de <rhyme label="b" type="m" corresp="#s9:l2">Capp...s</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Zy zullen altemaal <rhyme label="c" type="m">vergaan</rhyme>;</l>
        <l id="s9:l6" met="4" type="-+">Wy Schoppen zullen blyven <rhyme label="c" type="m" corresp="#s9:l5">staan</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Nu Schoppen troef, en dat is <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Oranje zy <rhyme label="b" type="m">verhoog</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">Gebeeden van een vroom <rhyme label="a" type="m" corresp="#s10:l1">Gemoed</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Klimmen tot 's Hemels <rhyme label="b" type="m" corresp="#s10:l2">boog</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Wy Zingen ongestoord en <rhyme label="c" type="m">bly</rhyme>:</l>
        <l id="s10:l6" met="4" type="-+">Ach, dat het weer Oranje <rhyme label="c" type="m" corresp="#s10:l5">zy</rhyme>!</l>
        <l id="s10:l7" met="3" type="-+">Hoezée, Hoezée, <rhyme label="d" type="m">Hoezée</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
