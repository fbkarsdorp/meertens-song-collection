    <melody>Helaas Alcander helaas</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">WAt ist, o! mensch wat ist! of ghy veel <rhyme label="a" type="f">landen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hebt onder u <rhyme label="b" type="m">ghebracht</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">En machtighe <rhyme label="c" type="f">vyanden</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Hebt <rhyme label="d" type="m">ne'erghevelt</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Door u <rhyme label="d" type="m" corresp="#s1:l4">ghewelt</rhyme>:</l>
        <l id="s1:l6" met="3" type="+-">Soo <rhyme label="b" type="m" corresp="#s1:l2">datmen</rhyme></l>
        <l id="s1:l7" met="2" type="+-">krachtigh 't Vuur doet <rhyme label="c" type="f" corresp="#s1:l3">branden</rhyme></l>
        <l id="s1:l8" met="6" type="+-">Tot eer van u, en blijck van u kloecke <rhyme label="a" type="f" corresp="#s1:l1">handen</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Wat is het of 't gheluyt van u vroome <rhyme label="a" type="f">daden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Tot aen de wolcke <rhyme label="b" type="m">dringht</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En dat 't gheluyt der <rhyme label="c" type="f">menschen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">U lof met <rhyme label="d" type="m">vreucht</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">En groot <rhyme label="d" type="m" corresp="#s2:l4">gheneucht</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Uytschreyt ende <rhyme label="b" type="m" corresp="#s2:l2">singht</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Boven <rhyme label="c" type="f" corresp="#s2:l3">wenschen</rhyme>,</l>
        <l id="s2:l8" met="6" type="+-">Om dat ghy haer verlost van die haer <rhyme label="a" type="f" corresp="#s2:l1">vertraden</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">'t Is al maer ydelheyt, en een droom der <rhyme label="a" type="f">droomen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een roock, die haest <rhyme label="b" type="m">verswint</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Een kittelingh der <rhyme label="c" type="f">ooren</rhyme>;</l>
        <l id="s3:l4" met="2" type="-+">Soo ghy niet 't <rhyme label="d" type="m">quaet</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Dat zielen <rhyme label="d" type="m" corresp="#s3:l4">schaet</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Met kracht <rhyme label="b" type="m" corresp="#s3:l2">onderwint</rhyme></l>
        <l id="s3:l7" met="2" type="+-">Te <rhyme label="c" type="f" corresp="#s3:l3">versmooren</rhyme>,</l>
        <l id="s3:l8" met="6" type="+-">En quade driften en sonden te <rhyme label="a" type="f" corresp="#s3:l1">betoomen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Wat ist? of gy een stadt door u macht kond <rhyme label="a" type="f">winnen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En vryheyt door het <rhyme label="b" type="m">swaert</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Hebt kloeckelijck <rhyme label="c" type="f">verkreghen</rhyme>:</l>
        <l id="s4:l4" met="2" type="-+">Ghy zijt niet <rhyme label="d" type="m">vry</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Indien dat <rhyme label="d" type="m" corresp="#s4:l4">ghy</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Schrickt, of sijt <rhyme label="b" type="m" corresp="#s4:l2">vervaert</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Of <rhyme label="c" type="f" corresp="#s4:l3">verleghen</rhyme>,</l>
        <l id="s4:l8" met="6" type="+-">Dat quaet te roeyen uyt, dat schae doet van <rhyme label="a" type="f" corresp="#s4:l1">binnen</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Mach oock dat volck den naem van een vry volck <rhyme label="a" type="f">draghen</rhyme>?</l>
        <l id="s5:l2" met="3" type="-+">Die rust heeft buyten 't <rhyme label="b" type="m">lant</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Maer midden in de <rhyme label="c" type="f">steden</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Leven in <rhyme label="d" type="m">veel</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Twist en <rhyme label="d" type="m" corresp="#s5:l4">krackeel</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Soo dat selfs <rhyme label="b" type="m" corresp="#s5:l2">niemant</rhyme></l>
        <l id="s5:l7" met="2" type="+-">Haeckt nae <rhyme label="c" type="f" corresp="#s5:l3">vreden</rhyme>,</l>
        <l id="s5:l8" met="6" type="+-">En om de onrust uyt het lant te <rhyme label="a" type="f" corresp="#s5:l1">verjaghen</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="+-">Hy die hem selfs verwint, is veel meer te <rhyme label="a" type="f">achten</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dan die sijn vyant <rhyme label="b" type="m">slaet</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Dit konnen oock de <rhyme label="c" type="f">beeren</rhyme>.</l>
        <l id="s6:l4" met="2" type="-+">Twist u <rhyme label="d" type="m">ghemoet</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">'t Sy wat ghy <rhyme label="d" type="m" corresp="#s6:l4">doet</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Godt prijst niet u <rhyme label="b" type="m" corresp="#s6:l2">daet</rhyme>.</l>
        <l id="s6:l7" met="2" type="+-">Kondt ghy u <rhyme label="c" type="f" corresp="#s6:l3">Heeren</rhyme>,</l>
        <l id="s6:l8" met="6" type="+-">En 't quaet verwinnen, ghy hebt heyls-kroon te <rhyme label="a" type="f" corresp="#s6:l1">wachten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="+-">Een machtich legher-vorst pooght sigh vooght te <rhyme label="a" type="f">maecken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van 't gheen dat sterckste <rhyme label="b" type="m">is</rhyme>.</l>
        <l id="s7:l3" met="3" type="+-">Wilt ghy u meester <rhyme label="c" type="f">werden</rhyme>?</l>
        <l id="s7:l4" met="2" type="-+">Van daer <rhyme label="d" type="m">begint</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Dat ghy 't quaetst <rhyme label="d" type="m" corresp="#s7:l4">vint</rhyme>.</l>
        <l id="s7:l6" met="3" type="+-">Weert ziels <rhyme label="b" type="m" corresp="#s7:l2">duysternis</rhyme>.</l>
        <l id="s7:l7" met="2" type="+-">Wilt <rhyme label="c" type="f" corresp="#s7:l3">volherden</rhyme>;</l>
        <l id="s7:l8" met="6" type="+-">Waeckt staegh in 't goet, soo sal u gheen vyant <rhyme label="a" type="f" corresp="#s7:l1">naecken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
