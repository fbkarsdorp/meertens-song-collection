<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25114</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="Marian song">Marialied</tag>
    </tags>
    <melody>Ten baat mij geen kastijden</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Met herten ende met <rhyme label="a" type="f">sinne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sinden wy v ewige <rhyme label="b" type="m">lof</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Vri edel <rhyme label="a" type="f" corresp="#s1:l1">coninginne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Daer bouen in v <rhyme label="b" type="m" corresp="#s1:l2">hof</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hier toe sijn wy te <rhyme label="a" type="f">cleyne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als wy bedincken <rhyme label="b" type="m">dat</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat ghy sijt vrouwe <rhyme label="a" type="f" corresp="#s2:l1">alleyne</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van al soe hoger <rhyme label="b" type="m" corresp="#s2:l2">stat</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die coninc es v <rhyme label="a" type="f">sone</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat vaderlike <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Naest hem dragic de <rhyme label="a" type="f" corresp="#s3:l1">crone</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Want v dat rijc <rhyme label="b" type="m" corresp="#s3:l2">behoort</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die alle dinc heeft <rhyme label="a" type="f">gescepen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat es v enich <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s4:l3" met="3" type="-+">V meechden ende v <rhyme label="a" type="f" corresp="#s4:l1">knapen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Sijnt al datmen daer <rhyme label="b" type="m" corresp="#s4:l2">vint</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy hebt alleene die <rhyme label="a" type="f">eere</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En sijt daer met <rhyme label="b" type="m">verhoeght</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Uwen stoel steet biden <rhyme label="a" type="f" corresp="#s5:l1">heere</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Daer ghy gebieden <rhyme label="b" type="m" corresp="#s5:l2">moeght</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy droechten scone <rhyme label="a" type="f">reyne</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En waert sijn voesterkijn <rhyme label="b" type="m">met</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Want god hadde v <rhyme label="a" type="f" corresp="#s6:l1">alleyne</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Te selken dienst <rhyme label="b" type="m" corresp="#s6:l2">geset</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daer om moet v van <rhyme label="a" type="f">rechte</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Al te dienste <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Die ingelen sijn v <rhyme label="a" type="f" corresp="#s7:l1">knechten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Ende altoes <rhyme label="b" type="m" corresp="#s7:l2">onderdaen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghy sijt des hemels <rhyme label="a" type="f">chierheit</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En ons een <rhyme label="b" type="m">aduocaet</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Ende een vloet der <rhyme label="a" type="f" corresp="#s8:l1">mildicheit</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Geeft ons tot duechden <rhyme label="b" type="m" corresp="#s8:l2">raet</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Nv hulpt ons scone <rhyme label="a" type="f">vrouwe</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Daer bouen by v lief <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Daer men sonder <rhyme label="a" type="f" corresp="#s9:l1">rouwe</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Gebruict dies men daer <rhyme label="b" type="m" corresp="#s9:l2">vint</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
