  <author>Gerrit Manheer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">God Jupyn, die bliksem - <rhyme label="a" type="f">voerder</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Geeft de wet aan 't <rhyme label="b" type="m">Godendom</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En Neptun, dien zee - <rhyme label="a" type="f" corresp="#s1:l1">beroerder</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hutseld al de golven <rhyme label="b" type="m" corresp="#s1:l2">om</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Vulkaan smeet de <rhyme label="c" type="f">harnasplaten</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Giet vast kogels en <rhyme label="c" type="f" corresp="#s1:l5">granaten</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Voor den stouten <rhyme label="b" type="m" corresp="#s1:l2">heldendrom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Mavors schaart zyn <rhyme label="a" type="f">krygs-kolommen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Vat zyn schild en <rhyme label="b" type="m">benkelaar</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Op 't geluid van pauk en <rhyme label="a" type="f" corresp="#s2:l1">trommen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Woeld de moedige <rhyme label="b" type="m" corresp="#s2:l2">Oorlogsschaar</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Febus, op parnas <rhyme label="c" type="f">gezeten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Spand, als hoofdman der <rhyme label="c" type="f" corresp="#s2:l5">Poëten</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Op zyn gouden lier, de <rhyme label="b" type="m" corresp="#s2:l2">snaar</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Bolle Bachus perst zyn <rhyme label="a" type="f">druiven</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">In den beker van <rhyme label="b" type="m">kristal</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Moeder Venus lieve <rhyme label="a" type="f" corresp="#s3:l1">duiven</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Trekkebekken, in het <rhyme label="b" type="m" corresp="#s3:l2">dal</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Cupido, dat olyk <rhyme label="c" type="f">wichtje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Slypt een puntje aan 't dunne <rhyme label="c" type="f" corresp="#s3:l5">schichtje</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Daar hy straks meê treffen <rhyme label="b" type="m" corresp="#s3:l2">zal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hymen sluit de <rhyme label="a" type="f">huwlyksbanden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Voor het outer van de <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ziet den Bruigom <rhyme label="a" type="f" corresp="#s4:l1">watertanden</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Leid de Bruid ter slaapzaale <rhyme label="b" type="m" corresp="#s4:l2">in</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">En wat doe ik, <rhyme label="c" type="f">ondertusschen</rhyme>?</l>
        <l id="s4:l6" met="4" type="+-">Niets, dan myn Clotilde <rhyme label="c" type="f" corresp="#s4:l5">kusschen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Teeder kusschen, naar myn <rhyme label="b" type="m" corresp="#s4:l2">zin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
