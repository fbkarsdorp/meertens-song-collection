        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>J'aimerai en ce village</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Ave Vrou der <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Godes waerdste <rhyme label="b" type="m">Bruyd</rhyme>!</l>
        <l id="s1:l3" met="3" type="+-">Door wien <rhyme label="a" type="f" corresp="#s1:l1">onderg'houwen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Wordt den Helschen <rhyme label="b" type="m" corresp="#s1:l2">Guyt</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Die eertijts bekooren <rhyme label="c" type="f">Evam</rhyme></l>
        <l id="s1:l6" met="4" type="+-">quam in 't <rhyme label="d" type="m">Pa—ra—dijs</rhyme>:</l>
        <l id="s1:l7" met="3" type="+-">De-se uytverkoore Zijt <rhyme label="c" type="f" corresp="#s1:l5">ghy</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Maget vroet en <rhyme label="d" type="m" corresp="#s1:l6">wijs</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Gratiâ Tu <rhyme label="a" type="f">plena</rhyme></l>
        <l id="s2:l2" met="3" type="+-">In den hoochsten <rhyme label="b" type="m">graed</rhyme>;</l>
        <l id="s2:l3" met="3" type="+-">Tu salutis <rhyme label="a" type="f" corresp="#s2:l1">vena</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En ons' <rhyme label="b" type="m" corresp="#s2:l2">Toeverlaet</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Ghy zijt <rhyme label="c" type="f">Coninginne</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van het schoone <rhyme label="d" type="m">Hemelrijck</rhyme>:</l>
        <l id="s2:l7" met="3" type="+-">'s Hemels <rhyme label="c" type="f" corresp="#s2:l5">Keyserinne</rhyme>!</l>
        <l id="s2:l8" met="4" type="+-">Ghy hebt niemandt uws <rhyme label="d" type="m" corresp="#s2:l6">gelijck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Dominus est <rhyme label="a" type="f">tecum</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Alder-reynste <rhyme label="b" type="m">Maeght</rhyme>!</l>
        <l id="s3:l3" met="3" type="+-">Vtinam &amp; <rhyme label="a" type="f" corresp="#s3:l1">mecum</rhyme>!</l>
        <l id="s3:l4" met="3" type="+-">Die uw' bystandt <rhyme label="b" type="m" corresp="#s3:l2">vraeght</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Alderliefste <rhyme label="c" type="f">Moeder</rhyme>!</l>
        <l id="s3:l6" met="4" type="+-">Aldertrouwste <rhyme label="d" type="m">Voorsprakers</rhyme>!</l>
        <l id="s3:l7" met="3" type="+-">Weest by ons' <rhyme label="c" type="f" corresp="#s3:l5">Behoeder</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Mijnder saecken <rhyme label="d" type="m" corresp="#s3:l6">Middelers</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Tu es <rhyme label="a" type="f">benedicta</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Onder 't Vrouw <rhyme label="b" type="m">geslacht</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">Nobis Tu <rhyme label="a" type="f" corresp="#s4:l1">Relicta</rhyme></l>
        <l id="s4:l4" met="3" type="+-">In dees duyst're <rhyme label="b" type="m" corresp="#s4:l2">nacht</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Bidt des Hemels <rhyme label="c" type="f">Coningh</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat hy mijne sond' <rhyme label="d" type="m">vergeeft</rhyme>;</l>
        <l id="s4:l7" met="3" type="+-">Brengt m' in 's Hemels <rhyme label="c" type="f" corresp="#s4:l5">wooningh</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Waer met Godt ghy eeuwich <rhyme label="d" type="m" corresp="#s4:l6">leeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
