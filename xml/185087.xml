  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De Prins leeft in ons Land thans <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Tot vreugd der goeden, in zyn <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">De Landverdervers zyn <rhyme label="b" type="m">ontsteld</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Om dat die Braave Oranje – <rhyme label="b" type="m" corresp="#s1:l3">Held</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Gered is door den <rhyme label="a" type="m" corresp="#s1:l1">Heer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Prins, die eertyds wierd <rhyme label="a" type="m">versmaad</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Leeft nu weer in verheven <rhyme label="a" type="m" corresp="#s2:l1">staat</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Zy, die hem smaadden, zyn <rhyme label="b" type="m">verneêrd</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Door Hem, die ’t gansch Heelal <rhyme label="b" type="m" corresp="#s2:l3">regeerd</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Door Neêrlands <rhyme label="a" type="m" corresp="#s2:l1">Toeverlaat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Prins, die eertyds wierd <rhyme label="a" type="m">veracht</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Leeft nu in eer, met zyn <rhyme label="a" type="m" corresp="#s3:l1">Geslacht</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die hem verachten zyn <rhyme label="b" type="m">vergaan</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Om dat zy konden niet <rhyme label="b" type="m" corresp="#s3:l3">bestaan</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Voor Pruissen’s <rhyme label="a" type="m" corresp="#s3:l1">Legermacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Prins was eertyds in <rhyme label="a" type="m">gevaar</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Maar, op ’t gebed der Vroome <rhyme label="a" type="m" corresp="#s4:l1">Schaar</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Heeft God hem uit gevaar en <rhyme label="b" type="m">druk</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Gered, hy gaf aan hem <rhyme label="b" type="m" corresp="#s4:l3">geluk</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Door eenen <rhyme label="a" type="m" corresp="#s4:l1">Adelaar</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Des Prinsen Gemaalin en <rhyme label="a" type="m">Zaad</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Die wierden eertyds ook <rhyme label="a" type="m" corresp="#s5:l1">versmaad</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Maar God, Die heeft, in gunst, <rhyme label="b" type="m">gered</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Haar voeten uit des Boozen <rhyme label="b" type="m" corresp="#s5:l3">net</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hy was haar <rhyme label="a" type="m" corresp="#s5:l1">Toeverlaat</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Den Prins, die Gy, in <rhyme label="a" type="m">Nederland</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Hersteld heeft in verheven <rhyme label="a" type="m" corresp="#s6:l1">stand</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Wil die, ô Heer! steeds in zyn <rhyme label="b" type="m">werk</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Tot heil van Vaderland en <rhyme label="b" type="m" corresp="#s6:l3">Kerk</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Geleiden door Uw <rhyme label="a" type="m" corresp="#s6:l1">Hand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Den Prins laat die, door U, ô <rhyme label="a" type="m">Heer</rhyme>!</l>
        <l id="s7:l2" met="4" type="-+">Bestierd, steeds leeven tot Uw <rhyme label="a" type="m" corresp="#s7:l1">Eer</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Behoed hem toch voor <rhyme label="b" type="m">ongeval</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En zend altoos in ’t <rhyme label="b" type="m" corresp="#s7:l3">traanendal</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Op hem Uw Zegen <rhyme label="a" type="m" corresp="#s7:l1">neêr</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Des Prinsen schrandre <rhyme label="a" type="m">Gemaalin</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Die Gy, ô Heer! uit tedre <rhyme label="a" type="m" corresp="#s8:l1">min</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Verloste, laat die, ’t allen <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Uw heil ontvangen, maak <rhyme label="b" type="m" corresp="#s8:l3">verblyd</rhyme></l>
        <l id="s8:l5" met="3" type="-+">In U steeds die <rhyme label="a" type="m" corresp="#s8:l1">Vorstin</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Des Prinsen dierbre Kindren, <rhyme label="a" type="m">Heer</rhyme>!</l>
        <l id="s9:l2" met="4" type="-+">Zend die geduurig zegen <rhyme label="a" type="m" corresp="#s9:l1">neêr</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Doe hen altoos, in u <rhyme label="b" type="m">verheugd</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Betreeden ’t pad der waare <rhyme label="b" type="m" corresp="#s9:l3">deugd</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Dat uwen Geest hen <rhyme label="a" type="m" corresp="#s9:l1">leer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De Prins, zyn Gemaalin en <rhyme label="a" type="m">Zaad</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">En Neêrlands Kerk- en <rhyme label="a" type="m" corresp="#s10:l1">Burgerstaat</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Elk Braaven, die dit Land <rhyme label="b" type="m">bewoond</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Zy steeds door U met heil <rhyme label="b" type="m" corresp="#s10:l3">bekroond</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">O, Neêrlands <rhyme label="a" type="m" corresp="#s10:l1">Toeverlaat</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
