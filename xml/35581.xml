<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35581</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="love song">liefdeslied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody>Wie zal hier zo komen tieren</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Comt doch ziel wilt my <rhyme label="a" type="f">inlaeten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Die soo minnelijck staen en <rhyme label="b" type="m">wacht</rhyme></l>
        <l id="s1:l3" met="4" type="+-">My u Godt, die t'uwer <rhyme label="a" type="f" corresp="#s1:l1">baeten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Uwe liefde seer <rhyme label="b" type="m" corresp="#s1:l2">beacht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Mijne haer vlichten zijn vol <rhyme label="c" type="m">daeuw</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">V grendelen wilt ontsluyten <rhyme label="c" type="m" corresp="#s1:l5">gaeuw</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">O mijn lieve wel <rhyme label="a" type="f">beminde</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Siet den winter is nu <rhyme label="b" type="m">voorby</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Maeckt dat ick u ghesnoeyt <rhyme label="a" type="f" corresp="#s2:l1">vinde</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Vander sonden <rhyme label="b" type="m" corresp="#s2:l2">slaverny</rhyme></l>
        <l id="s2:l5" met="4" type="-+">En als een duyfken met my <rhyme label="c" type="m">paert</rhyme></l>
        <l id="s2:l6" met="4" type="-+">V hert en siele voor my <rhyme label="c" type="m" corresp="#s2:l5">bewaert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ick sal u soo soet <rhyme label="a" type="f">vermaecken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">In mijn minnelijcke <rhyme label="b" type="m">rust</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Laet ghy maer de aertsche <rhyme label="a" type="f" corresp="#s3:l1">saecken</rhyme></l>
        <l id="s3:l4" met="4" type="+-">In u wesen <rhyme label="b" type="m" corresp="#s3:l2">wtgheblust</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Al de wellust van s'wereldts <rhyme label="c" type="m">dal</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Vindt ghy in my de oorspronck van <rhyme label="c" type="m" corresp="#s3:l5">al</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Staet Schoonheyt in u <rhyme label="a" type="f">behaghen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Ghy vintse in my recht t'uwen <rhyme label="b" type="m">wensch</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Luystert naer Schriftuers <rhyme label="a" type="f" corresp="#s4:l1">ghewaghen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Waer sy seyt den Soon des <rhyme label="b" type="m" corresp="#s4:l2">mensch</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Op aerde noch in <rhyme label="c" type="m">Hemelrijck</rhyme></l>
        <l id="s4:l6" met="4" type="-+">En was in schoonheyt noyt sijns <rhyme label="c" type="m" corresp="#s4:l5">gelijck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hebt ghy vermaeck in wijse <rhyme label="a" type="f">taele</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Oft wel ghestemden soeten <rhyme label="b" type="m">sanck</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Door mijn woordt vergaet de <rhyme label="a" type="f" corresp="#s5:l1">quaele</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Van die daer sieck zijn oft <rhyme label="b" type="m" corresp="#s5:l2">cranck</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Melodieus singht m'in mijn <rhyme label="c" type="m">hoff</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Met Enghelsche stem tot mijnen <rhyme label="c" type="m" corresp="#s5:l5">loff</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Soo ghy mint <rhyme label="a" type="f">groot-daedichede</rhyme></l>
        <l id="s6:l2" met="4" type="+-">s'Weereldts cieraet, rijckdom, en <rhyme label="b" type="m">pracht</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Den heelen hemel is mijn <rhyme label="a" type="f" corresp="#s6:l1">stede</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En wat leeft, onder mijn <rhyme label="b" type="m" corresp="#s6:l2">macht</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Mijn cleetsel schoon boven al <rhyme label="c" type="m">claer</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Dat is voor altijdt <rhyme label="c" type="m" corresp="#s6:l5">onwandelbaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Soeckt ghy wellust in u <rhyme label="a" type="f">spijse</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Ghecockte saucen, goet wilt <rhyme label="b" type="m">braet</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Een gherecht van grooter <rhyme label="a" type="f" corresp="#s7:l1">prijse</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Sal 'k u dienen vroech en <rhyme label="b" type="m" corresp="#s7:l2">laet</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">t'Welck sal zijn mijn vlees en <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Waer bracht oyt minnaer dat was soo  <rhyme label="c" type="m" corresp="#s7:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">'K sal u in overvloet <rhyme label="a" type="f">graven</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Ist dat ghy my maer trouw en <rhyme label="b" type="m">dient</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En verlaet Cupidoos <rhyme label="a" type="f" corresp="#s8:l1">slaven</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">My u Heer en Godt <rhyme label="b" type="m" corresp="#s8:l2">bemint</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Gheeft my t'ja-woordt, wil en <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Die om u leden heeft soo veel <rhyme label="c" type="m" corresp="#s8:l5">smert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
