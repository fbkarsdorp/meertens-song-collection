  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Cornelyae wilt toch eens <rhyme label="a" type="m">drae</rhyme></l>
        <l id="s1:l2" met="3" type="-+">mijn u dienaer troest <rhyme label="b" type="f">geven</rhyme></l>
        <l id="s1:l3" met="4" type="-+">waer Ick lech off stae vroech ende <rhyme label="a" type="m" corresp="#s1:l1">spae</rhyme></l>
        <l id="s1:l4" met="3" type="-+">es mijn hert u <rhyme label="b" type="f" corresp="#s1:l2">beneeven</rhyme></l>
        <l id="s1:l5" met="4" type="-+">met liefde ben ick <rhyme label="c" type="m">duerwont</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Dat heeft gedaen u rooder <rhyme label="c" type="m" corresp="#s1:l5">mont</rhyme></l>
        <l id="s1:l7" met="4" type="-+">dies Ick nacht noch dach en <rhyme label="d" type="m">rust</rhyme></l>
        <l id="s1:l8" met="4" type="-+">om dat nae mijn <rhyme label="d" type="m" corresp="#s1:l7">lust</rhyme></l>
        <l id="s1:l9" met="4" type="+-">u schoen maeget mach <rhyme label="e" type="f">genaken</rhyme></l>
        <l id="s1:l10" met="4" type="+-">dat gij mijne liefde <rhyme label="d" type="m" corresp="#s1:l7">blust</rhyme></l>
        <l id="s1:l11" met="4" type="+-">die gij doet soe vierich <rhyme label="e" type="f" corresp="#s1:l9">blaeken</rhyme></l>
        <l id="s1:l12" met="3" type="-+">waer Ick, doch van u <rhyme label="d" type="m" corresp="#s1:l7">gekust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Kessels geslacht gij hebt die <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">wilt u tminwaerts <rhyme label="b" type="f">voervveeren</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Op mijn woert acht doer liefien <rhyme label="a" type="m" corresp="#s2:l1">kracht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Zal Ick u altoes <rhyme label="b" type="f" corresp="#s2:l2">eeren</rhyme></l>
        <l id="s2:l5" met="4" type="-+">en constant getrouwe <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s2:l6" met="4" type="-+">ontfangh mijn In u armkens <rhyme label="c" type="m" corresp="#s2:l5">fin</rhyme></l>
        <l id="s2:l7" met="4" type="-+">die als snee wit sijn en <rhyme label="d" type="m">schoender</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Gelick Is u eedel <rhyme label="d" type="m" corresp="#s2:l7">persoene</rhyme></l>
        <l id="s2:l9" met="4" type="+-">volmaeckt In gemeins <rhyme label="e" type="f">soekondich</rhyme></l>
        <l id="s2:l10" met="4" type="+-">die gij verre spant de <rhyme label="d" type="m" corresp="#s2:l7">kroen</rhyme></l>
        <l id="s2:l11" met="4" type="+-">van die Ick oeit sout <rhyme label="e" type="f" corresp="#s2:l9">hertgrondich</rhyme></l>
        <l id="s2:l12" met="3" type="-+">willen kiesen mijnen <rhyme label="d" type="m" corresp="#s2:l7">loen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Lenaerten sal hier In dit <rhyme label="a" type="m">dal</rhyme></l>
        <l id="s3:l2" met="3" type="-+">doch anders geen <rhyme label="b" type="f">beminnen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">off doer misval bitter als <rhyme label="a" type="m" corresp="#s3:l1">gal</rhyme></l>
        <l id="s3:l4" met="3" type="-+">sal hem die doet <rhyme label="b" type="f" corresp="#s3:l2">voerslinnen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Vant u persoens Is sin <rhyme label="c" type="m">begeert</rhyme></l>
        <l id="s3:l6" met="4" type="-+">anders niet als op duecht en <rhyme label="c" type="m" corresp="#s3:l5">eer</rhyme></l>
        <l id="s3:l7" met="4" type="-+">hij heeft sin hert op u <rhyme label="d" type="m">gestelt</rhyme></l>
        <l id="s3:l8" met="4" type="-+">dus hem troest geeft niet meer en <rhyme label="d" type="m" corresp="#s3:l7">quelt</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Soe mach hij doer lieft <rhyme label="e" type="f">eendrachtich</rhyme></l>
        <l id="s3:l10" met="4" type="+-">eens met u werden <rhyme label="d" type="m" corresp="#s3:l7">voerstelt</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Daer toe hij aen roupt <rhyme label="e" type="f" corresp="#s3:l9">aendachtich</rhyme></l>
        <l id="s3:l12" met="3" type="-+">die sulcks heeft in sijn <rhyme label="d" type="m" corresp="#s3:l7">gewelt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Stoeters prinses sal sijn <rhyme label="a" type="m">maetres</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Geduerich liefde <rhyme label="b" type="f">dragen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Tsy waer sij Is die schoene <rhyme label="a" type="m" corresp="#s4:l1">daer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Sal altoes nae haer <rhyme label="b" type="f" corresp="#s4:l2">Jaegen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">om tgeen sij voer hem heeft <rhyme label="c" type="m">voerklaert</rhyme></l>
        <l id="s4:l6" met="4" type="-+">en dat sij Is meer prijsen <rhyme label="c" type="m" corresp="#s4:l5">waeert</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Zal hij haer dienen met <rhyme label="d" type="m">oetmoet</rhyme></l>
        <l id="s4:l8" met="4" type="-+">en presenteren haer tot lljf en <rhyme label="d" type="m" corresp="#s4:l7">goedt</rhyme></l>
        <l id="s4:l9" met="4" type="+-">alle dat In sijn voermoegen <rhyme label="e" type="f">Is</rhyme></l>
        <l id="s4:l10" met="4" type="+-">comen mach in lijf en <rhyme label="d" type="m" corresp="#s4:l7">bloet</rhyme></l>
        <l id="s4:l11" met="4" type="+-">Daer mee sij haer sal <rhyme label="e" type="f" corresp="#s4:l9">genougen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">hoop Ick die nimphe <rhyme label="d" type="m" corresp="#s4:l7">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
