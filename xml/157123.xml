<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157123</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach Amaryl sta doch wat stil</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Haes op <rhyme label="a" type="m">Diaen</rhyme>.</l>
        <l id="s1:l2" met="2" type="-+">En maeckt <rhyme label="a" type="m" corresp="#s1:l1">ruymbaen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Met al u <rhyme label="b" type="f">sterren</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">En wilt niet <rhyme label="b" type="f" corresp="#s1:l3">marren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Tot den dach komt dringen <rhyme label="a" type="m" corresp="#s1:l1">aen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Want, <rhyme label="a" type="m">Amaril</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Bekruypt een <rhyme label="a" type="m" corresp="#s2:l1">gril</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Van kille <rhyme label="b" type="f">schroomen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Als ick een <rhyme label="b" type="f" corresp="#s2:l3">koomen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">(By haer in heldre maenschijn) <rhyme label="a" type="m" corresp="#s2:l1">wil</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Vyt vreese, <rhyme label="a" type="m">dat</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Sy mocht <rhyme label="a" type="m" corresp="#s3:l1">beklad</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Werden met <rhyme label="b" type="f">schennis</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Van buur of <rhyme label="b" type="f" corresp="#s3:l3">kennis</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Want maegden eer licht op spraeck <rhyme label="a" type="m" corresp="#s3:l1">vat</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ick gae, wel <rhyme label="a" type="m">hoe</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Doen ick 't? ick <rhyme label="a" type="m" corresp="#s4:l1">doe</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Waer toe 't <rhyme label="b" type="f">getrantel</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+">Ick slae mijn <rhyme label="b" type="f" corresp="#s4:l3">mantel</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Om mijn hooft en treed nae haer <rhyme label="a" type="m" corresp="#s4:l1">toe</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Dan sacht wie <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Ick loop <rhyme label="a" type="m" corresp="#s5:l1">gevaer</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Of droom ick <rhyme label="b" type="f">wacker</rhyme>?</l>
        <l id="s5:l4" met="2" type="-+">Neen 't is mijn <rhyme label="b" type="f" corresp="#s5:l3">macker</rhyme></l>
        <l id="s5:l5" met="4" type="-+">En minnemaet ick volgh' hem <rhyme label="a" type="m" corresp="#s5:l1">naer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Om te <rhyme label="a" type="m">bespien</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Of ick kan <rhyme label="a" type="m" corresp="#s6:l1">sien</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Of sy den <rhyme label="b" type="f">desen</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Sal gunstich <rhyme label="b" type="f" corresp="#s6:l3">wesen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Of dat s'hem sal haer huys <rhyme label="a" type="m" corresp="#s6:l1">verbien</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
