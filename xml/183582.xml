  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IK kwam laest by <rhyme label="a" type="m">Climeen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Sy kon myn Hert <rhyme label="b" type="f">bekooren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sy bragt myn in 't <rhyme label="a" type="m" corresp="#s1:l1">geween</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">Sy had haer Poes <rhyme label="b" type="f" corresp="#s1:l2">verlooren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Wat ik haer <rhyme label="c" type="m">bad</rhyme></l>
        <l id="s1:l6" met="2" type="-+">En sy myn <rhyme label="c" type="m" corresp="#s1:l5">schad</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Zy riep myn Poes, poes, <rhyme label="d" type="m">poes</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Zy riep myn poes, poes, <rhyme label="d" type="m" corresp="#s1:l7">poes</rhyme>,</l>
        <l id="s1:l9" met="1" type="+-">Ach! ach! myn <rhyme label="d" type="m" corresp="#s1:l7">Kat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Was een zoo'n aerdig <rhyme label="a" type="m">Beest</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik hielt 't veel in <rhyme label="b" type="f">waerde</rhyme>!</l>
        <l id="s2:l3" met="3" type="-+">Zyn witte, swarte <rhyme label="a" type="m" corresp="#s2:l1">Leest</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En lange <rhyme label="b" type="f" corresp="#s2:l2">Knevelbaerde</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Stonden hem zoo <rhyme label="c" type="m">eel</rhyme>;</l>
        <l id="s2:l6" met="2" type="-+">Als een <rhyme label="c" type="m" corresp="#s2:l5">Iuweel</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">Al van een Poes, poes, <rhyme label="d" type="m">poes</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Al van een Poes, poes, <rhyme label="d" type="m" corresp="#s2:l7">poes</rhyme>,</l>
        <l id="s2:l9" met="1" type="+-">Hou ik zoo <rhyme label="d" type="m" corresp="#s2:l7">veel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik zey wel schoone <rhyme label="a" type="m">Maegt</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Mogt ik uw gunst <rhyme label="b" type="f">verwerven</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Anders zal uw <rhyme label="a" type="m" corresp="#s3:l1">Minnaer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Uit reyne liefde <rhyme label="b" type="f" corresp="#s3:l2">sterven</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Gunt my uw <rhyme label="c" type="m">schoot</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Uit liefde <rhyme label="c" type="m" corresp="#s3:l5">groot</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En 'k zal zyn uw Poes, poes, <rhyme label="d" type="m">poes</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">En 'k zal zyn uw Poes, poes, <rhyme label="d" type="m" corresp="#s3:l7">poes</rhyme>,</l>
        <l id="s3:l9" met="1" type="+-">Tot in der <rhyme label="d" type="m" corresp="#s3:l7">dood</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doen kreeg Climeen een <rhyme label="a" type="m">bloos</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Op haer sneeuw witte <rhyme label="b" type="f">kaeken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En sprak gy zult <rhyme label="a" type="m" corresp="#s4:l1">altoos</rhyme></l>
        <l id="s4:l4" met="3" type="-+">My in uw Min zien <rhyme label="b" type="f" corresp="#s4:l2">blaeken</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">Maer komt wat <rhyme label="c" type="m">gaeuw</rhyme></l>
        <l id="s4:l6" met="2" type="-+">En weest niet <rhyme label="c" type="m" corresp="#s4:l5">flaeuw</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Doe riep zy Poes, poes, <rhyme label="d" type="m">poes</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Doe riep zy Poes, poes, <rhyme label="d" type="m" corresp="#s4:l7">poes</rhyme>,</l>
        <l id="s4:l9" met="1" type="+-">En 'k zy <rhyme label="d" type="m" corresp="#s4:l7">Maeuw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
