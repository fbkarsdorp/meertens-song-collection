    </location>
    <tags>
      <tag>erotic song</tag>
      <tag>narrative song</tag>
      <tag>secular song</tag>
    </tags>
    <melody>Daar ik laatst leed</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAer ick lest <rhyme label="a" type="m">leedt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Daer vant ick een schoon <rhyme label="b" type="f">dierken</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick sachse ligghen in <rhyme label="a" type="m" corresp="#s1:l1">secreet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al by een <rhyme label="b" type="f" corresp="#s1:l2">parmentierken</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Het is int <rhyme label="c" type="m">openbaer</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s1:l5">daer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Aldaer mijn liefken <rhyme label="c" type="m" corresp="#s1:l5">daer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoort wat dat vrouken <rhyme label="a" type="m">sprack</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Al tot dat <rhyme label="b" type="f">parmentierken</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Den lesten dye my <rhyme label="a" type="m" corresp="#s2:l1">stack</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die gaf my een <rhyme label="b" type="f" corresp="#s2:l2">colierken</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Het was so vryen <rhyme label="c" type="m">haer</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s2:l5">daer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Aldaer mijn liefken <rhyme label="c" type="m" corresp="#s2:l5">daer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Den parmentier die <rhyme label="a" type="m">sprack</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wildijt mi niet <rhyme label="b" type="f">ontsegghen</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Ick sal op uwen <rhyme label="a" type="m" corresp="#s3:l1">koers</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Een trypen boort gaen <rhyme label="b" type="f" corresp="#s3:l2">legghen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Hy en wort v niet te <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s3:l5">daer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Aldaer mijn liefken <rhyme label="c" type="m" corresp="#s3:l5">daer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Met eenen trypen <rhyme label="a" type="m">boort</rhyme> |</l>
        <l id="s4:l2" met="3" type="-+">En ben ben ick niet om <rhyme label="b" type="f">payen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Maer ick ben gheschuert al <rhyme label="a" type="m" corresp="#s4:l1">voort</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Soudijt niet willen <rhyme label="b" type="f" corresp="#s4:l2">naeyen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dan volghe ick v al <rhyme label="c" type="m">naer</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s4:l5">daer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Aldaer mijn liefken <rhyme label="c" type="m" corresp="#s4:l5">daer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoort lieuen <rhyme label="a" type="m">parmentier</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ick sal v een eyken <rhyme label="b" type="f">doppen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Ick heb een gaetken <rhyme label="a" type="m" corresp="#s5:l1">fier</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Willet mi doch eens <rhyme label="b" type="f" corresp="#s5:l2">stoppen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ick sal v tooghen <rhyme label="c" type="m">waer</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s5:l5">daer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Aldaer mijn liefken <rhyme label="c" type="m" corresp="#s5:l5">daer</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Den ruyter sprack seer <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Wilt wat ontrent my <rhyme label="b" type="f">troppen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ick sal op dit <rhyme label="a" type="m" corresp="#s6:l1">termijn</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Daer aen stellen twee <rhyme label="b" type="f" corresp="#s6:l2">knoppen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Daer me salt wesen <rhyme label="c" type="m">claer</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s6:l5">daer</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Aldaer mijn lief <rhyme label="c" type="m" corresp="#s6:l5">aldaer</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wat cnoppen soudent <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Segt aen wilt v <rhyme label="b" type="f">verbonden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Al waren si noch so <rhyme label="a" type="m" corresp="#s7:l1">fijn</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Sy en sullender niet aen <rhyme label="b" type="f" corresp="#s7:l2">houden</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Daer heb ick voren <rhyme label="c" type="m">vaer</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s7:l5">daer</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Aldaer mijn liefken <rhyme label="c" type="m" corresp="#s7:l5">daer</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof ghesellen <rhyme label="a" type="m">alle</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En wilt daer niet op <rhyme label="b" type="f">letten</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Maer isser yemant <rhyme label="a" type="m" corresp="#s8:l1">malle</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Die daer twee cnoppen wil aen <rhyme label="b" type="f" corresp="#s8:l2">setten</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Die come noch bat <rhyme label="c" type="m">naer</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Daer hoerekint <rhyme label="c" type="m" corresp="#s8:l5">daer</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Alder mijn liefken <rhyme label="c" type="m" corresp="#s8:l5">daer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
