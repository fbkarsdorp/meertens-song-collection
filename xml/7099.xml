    </location>
    <tags>
      <tag>christmas song</tag>
      <tag>narrative song</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Het was een maagd uitverkoren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Het was een maghet <rhyme label="a" type="m">wtuercoren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Daer Iesus aff sijn wilde <rhyme label="a" type="m" corresp="#s1:l1">gheboren</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s1:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al totter stadt van <rhyme label="a" type="m">Nazareth</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Daer is een maghet <rhyme label="a" type="m" corresp="#s2:l1">ombesmet</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s2:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Groetse mij metten naeme <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Segt haer dat ick haer kint wil <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s3:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die Enghel was een bode <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Hij voer derwaerts metter <rhyme label="a" type="m" corresp="#s4:l1">spoet</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s4:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hij seyde Godt gruet v suyuer <rhyme label="a" type="m">maecht</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Ghij zijt die Gode wel <rhyme label="a" type="m" corresp="#s5:l1">behaecht</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s5:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hij wil van v gheboren <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Iesus die lieue meester <rhyme label="a" type="m" corresp="#s6:l1">mijn</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s6:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maria sprac hoe mocht ic hem <rhyme label="a" type="m">ghewinnen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Want ic noyt man en begeerden om <rhyme label="a" type="m" corresp="#s7:l1">minnen</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s7:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die heylighe gheest sal in v <rhyme label="a" type="m">comen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Soo den dau valt op die <rhyme label="a" type="m" corresp="#s8:l1">blommen</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s8:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maria weest <rhyme label="a" type="m">onuersaecht</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Tis den Godts sone die ghij <rhyme label="a" type="m" corresp="#s9:l1">draecht</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Des ben ick vro. O. o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s9:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hij heeft v daer toe <rhyme label="a" type="m">wtuercoren</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Hij wil verlossen dat was <rhyme label="a" type="m" corresp="#s10:l1">verloren</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s10:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Uan allen sorghen ben ick <rhyme label="a" type="m">ontsleghen</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Die deerne Godts wil ick <rhyme label="a" type="m" corresp="#s11:l1">wesen</rhyme></l>
        <l id="s11:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s11:l3">domino</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Maria viel neder op haer <rhyme label="a" type="m">knien</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Den wille Godts moet <rhyme label="a" type="m" corresp="#s12:l1">gheschien</rhyme></l>
        <l id="s12:l3" met="2" type="-+">Des ben ick vro. O, o, o, <rhyme label="b" type="m">o</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">benedicamus <rhyme label="b" type="m" corresp="#s12:l3">domino</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
