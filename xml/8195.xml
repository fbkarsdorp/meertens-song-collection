      <category translation="formal genres">formele genres</category>
      <category translation="amusement">vermaak</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIe wil nu met Ioncker <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Leeren 't vrolick <rhyme label="a" type="m" corresp="#s1:l1">A.B.C</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">D.E.F. moet het <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Wil hy G.H.I. <rhyme label="b" type="f" corresp="#s1:l3">lesen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s1:l6" met="4" type="+-">'t A.B.C. dat kenje <rhyme label="c" type="m" corresp="#s1:l5">dra</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-"><rhyme label="a" type="m">K.L.M.N.O</rhyme>.</l>
        <l id="s2:l2" met="4" type="+-">Trecken hem van Bachus <rhyme label="a" type="m" corresp="#s2:l1">noo</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Met P. en <rhyme label="b" type="f">Q.R.S</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Kan hy noyt sijn dorst schier <rhyme label="b" type="f" corresp="#s2:l3">lesse</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Ioncker volght Bachus <rhyme label="c" type="m" corresp="#s2:l5">na</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">T. met <rhyme label="a" type="m">V.W</rhyme>.</l>
        <l id="s3:l2" met="4" type="+-">Wie is voor den wyn oock <rhyme label="a" type="m" corresp="#s3:l1">schuw</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">X.Y.Z. koncke <rhyme label="b" type="f">tuttel</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ioncker drinckt noch al te <rhyme label="b" type="f" corresp="#s3:l3">luttel</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Al sijn sinnen staander <rhyme label="c" type="m" corresp="#s3:l5">na</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als hy't A.B.C. nu <rhyme label="a" type="m">leert</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Is als hy na Loosdrecht <rhyme label="a" type="m" corresp="#s4:l1">keert</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">D.E.F. heele <rhyme label="b" type="f">flappe</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Drinckt hy uyt tot Gerrit <rhyme label="b" type="f" corresp="#s4:l3">slappe</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Soo doet hy sijn boodschap <rhyme label="c" type="m" corresp="#s4:l5">dra</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Aan den Dam en <rhyme label="a" type="m">Nichtevecht</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Is G.H.I. <rhyme label="a" type="m" corresp="#s5:l1">onderrecht</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Soo drinckt hy tot Willem <rhyme label="b" type="f">Ponne</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Tot den ondergangh der <rhyme label="b" type="f" corresp="#s5:l3">sonnen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Ioncker volght Bachus <rhyme label="c" type="m" corresp="#s5:l5">na</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Gaat hy smorgens door den <rhyme label="a" type="m">dou</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Met twee kannen na <rhyme label="a" type="m" corresp="#s6:l1">Abcou</rhyme></l>
        <l id="s6:l3" met="4" type="+-">K.L.M. om de <rhyme label="b" type="f">boete</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">N.O.P. komt hem <rhyme label="b" type="f" corresp="#s6:l3">groete</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Altijd volght Bachus <rhyme label="c" type="m" corresp="#s6:l5">na</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Moet hy na Bylmer, <rhyme label="a" type="m">Diemerbrug</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Syn voeten vliegen als een <rhyme label="a" type="m" corresp="#s7:l1">mugh</rhyme>;</l>
        <l id="s7:l3" met="4" type="+-">Q.R.S.T. en <rhyme label="b" type="f">Uwe</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Hy en sal geen hulck <rhyme label="b" type="f" corresp="#s7:l3">schuuwe</rhyme></l>
        <l id="s7:l5" met="2" type="-+">, Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s7:l6" met="4" type="+-">'t A.B.C. dat leert hy <rhyme label="c" type="m" corresp="#s7:l5">dra</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">W.X.Y.Z. <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Soo ken hy sijn <rhyme label="a" type="m" corresp="#s8:l1">A.B.C</rhyme>.</l>
        <l id="s8:l3" met="4" type="+-">Die men doet hem noyt <rhyme label="b" type="f">verdrieten</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Als hy moog met Bachus <rhyme label="b" type="f" corresp="#s8:l3">gieten</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Viva, <rhyme label="c" type="m">viva</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Soo komt hy tot Muyden <rhyme label="c" type="m" corresp="#s8:l5">dra</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Ioncker gaet ghy na de <rhyme label="a" type="m">Zee</rhyme>:</l>
        <l id="s9:l2" met="4" type="+-">Onthout wel u <rhyme label="a" type="m" corresp="#s9:l1">A.B.C</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Maeckt vry vrolick u <rhyme label="b" type="f">geschater</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Maer voor al valt niet in 't <rhyme label="b" type="f" corresp="#s9:l3">water</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Denckt soo, denckt <rhyme label="c" type="m">soo</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Niemandt die verdrenckt <rhyme label="c" type="m" corresp="#s9:l5">noo</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Waer ghy komt tot elcken <rhyme label="a" type="m">stee</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Altyt brenght ghy Kermis <rhyme label="a" type="m" corresp="#s10:l1">mee</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Kermis-Feest is u <rhyme label="b" type="f">behagen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Vrolick zyn de <rhyme label="b" type="f" corresp="#s10:l3">Kermis-dagen</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Gaet me, gaet <rhyme label="c" type="m">me</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">En leert voort u <rhyme label="c" type="m" corresp="#s10:l5">A.B.C</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
