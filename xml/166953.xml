    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Hoe zoet is het daar de vriendschap woont</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Toen ik onlangs de Schoonheyt <rhyme label="a" type="m">zag</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Der poesele <rhyme label="b" type="m">Cesimeen</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Trof myn de Min niet slag op <rhyme label="a" type="m" corresp="#s1:l1">slag</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En quetste my met <rhyme label="b" type="m" corresp="#s1:l2">een</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Mijn geest verrukt door haar <rhyme label="c" type="m">gezigt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Eyst dat ik tot haar <rhyme label="c" type="m" corresp="#s1:l5">Lofgedigt</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Op zing, op zing, op <rhyme label="d" type="m">zing</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Volmaakte schoone <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Proefstuk der <rhyme label="b" type="m">Natuur</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Myn Hert dat brant om uwe <rhyme label="a" type="m" corresp="#s2:l1">Min</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">En is zo heet als <rhyme label="b" type="m" corresp="#s2:l2">Vuur</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Stort in u schoot myn minne <rhyme label="c" type="m">Brand</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En sterkt myn tot een <rhyme label="c" type="m" corresp="#s2:l5">Onderpant</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">U mond, u mond, u <rhyme label="d" type="m">mond</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">U ooge slaa met tintelend <rhyme label="a" type="m">Ligt</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">Zo ras gy maar een <rhyme label="b" type="m">lonk</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wilt schieten op myn <rhyme label="a" type="m" corresp="#s3:l1">Aangezigt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Gestaag een nieuwe <rhyme label="b" type="m" corresp="#s3:l2">vonk</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Zy schittert met haar bruine <rhyme label="c" type="m">glans</rhyme>:</l>
        <l id="s3:l6" met="4" type="-+">En maakt dat myne Ziel <rhyme label="c" type="m" corresp="#s3:l5">althans</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Staag kwynt, staag kwynt, staag <rhyme label="d" type="m">kwynt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">u Mond op ‘t allermeest <rhyme label="a" type="m">besneen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">u Lipjes Mals en <rhyme label="b" type="m">rood</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Vertone meer <rhyme label="a" type="m" corresp="#s4:l1">aanminnigheen</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Als Venus naakte <rhyme label="b" type="m" corresp="#s4:l2">schood</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Die lipjes fluite als <rhyme label="c" type="m">coraal</rhyme>;</l>
        <l id="s4:l6" met="4" type="-+">Ach mogt ik maar myn wil een <rhyme label="c" type="m" corresp="#s4:l5">maal</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Daar in, daar in, daar <rhyme label="d" type="m">in</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Liefwaardig halsje met blank <rhyme label="a" type="m">haar</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Van agtere schoon <rhyme label="b" type="m">gekrult</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Met minne lokjes door <rhyme label="a" type="m" corresp="#s5:l1">elkaar</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Van Vrouw Nature <rhyme label="b" type="m" corresp="#s5:l2">gehult</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Myn Zieltje broeid met <rhyme label="c" type="m">minnelust</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Zo ras myn Mond het halsje <rhyme label="c" type="m" corresp="#s5:l5">kust</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Met vreugt, met vreugt, met <rhyme label="d" type="m">vreugt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">u Boesem poesel Mals en <rhyme label="a" type="m">teer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Puylt door de linde <rhyme label="b" type="m">heen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Met golfjes op een walm pees <rhyme label="a" type="m" corresp="#s6:l1">neer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vol van <rhyme label="b" type="m" corresp="#s6:l2">aanlokkelykheen</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">Daar schuilt niet als een eerbaar <rhyme label="c" type="m">vogt</rhyme>;</l>
        <l id="s6:l6" met="4" type="-+">Ach dat ik daar mee lesse <rhyme label="c" type="m" corresp="#s6:l5">mogt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Miyn dorst, myn dorst, myn <rhyme label="d" type="m">dorst</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">u Handjes zagter als <rhyme label="a" type="m">Fluweel</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Met Adertjes <rhyme label="b" type="m">doorsneen</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Die ik op ‘t allerbest <rhyme label="a" type="m" corresp="#s7:l1">verbeel</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">By levend <rhyme label="b" type="m" corresp="#s7:l2">marmersteen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Doen myn staag wensche om daar <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Te leggen dromen van de <rhyme label="c" type="m" corresp="#s7:l5">Min</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Op bed, op bed, op <rhyme label="d" type="m">bed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ik zwyg nog stil van al het <rhyme label="a" type="m">geen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Van ‘t schoon voor ‘t oog <rhyme label="b" type="m">bevryd</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Wel dubbeld waart is ‘t <rhyme label="a" type="m" corresp="#s8:l1">aangebeen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En tienmaal word <rhyme label="b" type="m" corresp="#s8:l2">gevryd</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Ter plaats lief daar ik sterf van <rhyme label="c" type="m">min</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Van Venus met haar <rhyme label="c" type="m" corresp="#s8:l5">huisgezin</rhyme></l>
        <l id="s8:l7" met="3" type="-+">In woont, in woont, in <rhyme label="d" type="m">woont</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dat plaesje lief dat ruig <rhyme label="a" type="m">begroeid</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Beplant aan alle <rhyme label="b" type="m">zy</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Dat om de maand eens werd <rhyme label="a" type="m" corresp="#s9:l1">besproeid</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Daar hoeft geen regen <rhyme label="b" type="m" corresp="#s9:l2">by</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Dat plaasje lief dat niet <rhyme label="c" type="m">ontbreekt</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Dan dat men daar een kraan <rhyme label="c" type="m" corresp="#s9:l5">ontsteekt</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Of boord, of boord, of <rhyme label="d" type="m">boord</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ach lief waar ik zoo <rhyme label="a" type="m">Wellekom</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Dat ik met het hooft voor <rhyme label="b" type="m">uit</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Eens in dat soete <rhyme label="a" type="m" corresp="#s10:l1">Heiligdom</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Mogt doen gelyk een <rhyme label="b" type="m" corresp="#s10:l2">spuit</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Gewis gevoelde in uw <rhyme label="c" type="m">ziel</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Geen vreugde die u zoo <rhyme label="c" type="m" corresp="#s10:l5">beviel</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Als dat, als dat, als <rhyme label="d" type="m">dat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
