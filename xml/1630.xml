    <melody>Dat ruitertje in de schuur lag</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DAt ruyterken inder schueren <rhyme label="a" type="m">lach</rhyme></l>
        <l id="s1:l2" met="4" type="-+">DAt ruyterken inder schueren <rhyme label="a" type="m" corresp="#s1:l1">lach</rhyme></l>
        <l id="s1:l3" met="4" type="-+">die schuer was cout  den ruyter was <rhyme label="a" type="m" corresp="#s1:l1">nat</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Och lieue heer waert decket <rhyme label="a" type="m">mi</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Och lieue heer waert decket <rhyme label="a" type="m" corresp="#s2:l1">mi</rhyme></l>
        <l id="s2:l3" met="4" type="-+">die schuer is cout seer vrieset <rhyme label="a" type="m" corresp="#s2:l1">mi</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die waert sprac zijnder dienstmaecht <rhyme label="a" type="m">toe</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Die waert sprac zijnder dienstmaecht <rhyme label="a" type="m" corresp="#s3:l1">toe</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Gaet decket den ruyter met hauer <rhyme label="a" type="m" corresp="#s3:l1">stroe</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat meysken en dorstet laten <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Dat meysken en dorstet laten <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Si dede dat haer haer meester <rhyme label="a" type="m" corresp="#s4:l1">hiet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Si nam den ruyter in haren <rhyme label="a" type="m">arm</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Si nam den ruyter in haren <rhyme label="a" type="m" corresp="#s5:l1">arm</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Al was hi cout  si maecte hem <rhyme label="a" type="m" corresp="#s5:l1">warm</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doen die ruyter zijn willeken had <rhyme label="a" type="m">gedaen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Doen die ruyter zijn willeken had <rhyme label="a" type="m" corresp="#s6:l1">gedaen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Sprac hi scoon meisken ghi moecht wel <rhyme label="a" type="m" corresp="#s6:l1">gaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Waer soude ic rijden waer soude ic <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Waer soude ic rijden waer soude ic <rhyme label="a" type="m" corresp="#s7:l1">gaen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Ic ben met uwen kinde <rhyme label="a" type="m" corresp="#s7:l1">beuaen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Sidy met mijnen kinde <rhyme label="a" type="m">beuaen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Sidy met mijnen kinde <rhyme label="a" type="m" corresp="#s8:l1">beuaen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">So sult ghi rijden ende ick sal <rhyme label="a" type="m" corresp="#s8:l1">gaen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hi settese voor hem op zijn <rhyme label="a" type="m">paert</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Hi settese voor hem op zijn <rhyme label="a" type="m" corresp="#s9:l1">paert</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Hi voerdese tzijnder moeder <rhyme label="a" type="m" corresp="#s9:l1">waert</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Och lieue moeder hier is mijn <rhyme label="a" type="m">wijf</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Och lieue moeder hier is mijn <rhyme label="a" type="m" corresp="#s10:l1">wijf</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Ick hebse lieuer dan alle v <rhyme label="a" type="m" corresp="#s10:l1">lijf</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Die moeder sette haer eenen <rhyme label="a" type="m">stoel</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Die moeder sette haer eenen <rhyme label="a" type="m" corresp="#s11:l1">stoel</rhyme></l>
        <l id="s11:l3" met="4" type="-+">die dochter worp daer een cussen <rhyme label="a" type="m" corresp="#s11:l1">toe</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Die moeder leyde een ey int <rhyme label="a" type="m">vier</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Die moeder leyde een ey int <rhyme label="a" type="m" corresp="#s12:l1">vier</rhyme></l>
        <l id="s12:l3" met="4" type="-+">die dochter twee dat waren <rhyme label="a" type="m" corresp="#s12:l1">drie</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
