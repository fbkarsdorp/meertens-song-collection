    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik was Jong en <rhyme label="a" type="f">onvervaren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik minde zonder <rhyme label="b" type="m">list</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">De liefde in deze <rhyme label="a" type="f" corresp="#s1:l1">Jaaren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Trof men eer ik het <rhyme label="b" type="m" corresp="#s1:l2">wist</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik mag haar naam niet <rhyme label="a" type="f">weeten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Haar afgunst baart myn <rhyme label="b" type="m">pyn</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Zeg wat moet liefde <rhyme label="a" type="f" corresp="#s2:l1">heeten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Om niet verlieft te <rhyme label="b" type="m" corresp="#s2:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Haar stem kan myn <rhyme label="a" type="f">bekooren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En maakt myn hert <rhyme label="b" type="m">verheugt</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Zoo draa zy maar laat <rhyme label="a" type="f" corresp="#s3:l1">hooren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Verdryft zy my <rhyme label="b" type="m" corresp="#s3:l2">geneugt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Zy komt my 's morgens <rhyme label="a" type="f">siere</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Met Bloempjes mals en <rhyme label="b" type="m">vris</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met Roozen en <rhyme label="a" type="f" corresp="#s4:l1">Angelieren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gekleet geheel in <rhyme label="b" type="m" corresp="#s4:l2">Sis</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Malkander te <rhyme label="a" type="f">verlichten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Is Annetten en <rhyme label="b" type="m">Lubyn</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Hier kan geen liefde <rhyme label="a" type="f" corresp="#s5:l1">swigten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">'t moet al verlieft hier <rhyme label="b" type="m" corresp="#s5:l2">zyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
