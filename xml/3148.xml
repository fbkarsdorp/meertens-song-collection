    <melody>Ne vous offensez madame</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIe wil nu een kansje <rhyme label="a" type="f">wagen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Song den Hollansche <rhyme label="b" type="m">nachtegael</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Al de geen die willen <rhyme label="a" type="f" corresp="#s1:l1">wagen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Moeten mee na <rhyme label="b" type="m" corresp="#s1:l2">Portugael</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Doen lagh ick op een goe <rhyme label="c" type="m">Ree</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Ick kreegh een moet en ick wou <rhyme label="c" type="m" corresp="#s1:l5">mee</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Vyva vyva, <rhyme label="d" type="m">Portugael</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Pour eun belle fille bon <rhyme label="d" type="m" corresp="#s1:l7">Chevael</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wij sijn t' samen scheep <rhyme label="a" type="f">gekomen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Voeren op de Spaensche <rhyme label="b" type="m">zee</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Door de baren door de <rhyme label="a" type="f" corresp="#s2:l1">stroomen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Tot de Lissabonse <rhyme label="b" type="m" corresp="#s2:l2">Ree</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daer wy traden bey te <rhyme label="c" type="m">lant</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Songen lustigh handt aen <rhyme label="c" type="m" corresp="#s2:l5">handt</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Vyva, vyva, <rhyme label="d" type="m">Poortugael</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Pour eun belle fille bon <rhyme label="d" type="m" corresp="#s2:l7">Chevael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Gincker op een knevel <rhyme label="a" type="f">strijcken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Yder woude vrolick <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Yder wou de vlagge <rhyme label="a" type="f" corresp="#s3:l1">strijcken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Droncken Portugaelsche <rhyme label="b" type="m" corresp="#s3:l2">wijn</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En daer saghmen duytsen en <rhyme label="c" type="m">fransen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Singen al aen de negeren <rhyme label="c" type="m" corresp="#s3:l5">dans</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Vyva, vyva <rhyme label="d" type="m">Portugael</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Pour eun belle fille bon <rhyme label="d" type="m" corresp="#s3:l7">Chevael</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'t Wasser al Donsjan de <rhyme label="a" type="f">vierde</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Vyva, wat men hoort of <rhyme label="b" type="m">sagh</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Watmen taesd' of watmen <rhyme label="a" type="f" corresp="#s4:l1">tierde</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Daer was noyt soo blijden <rhyme label="b" type="m" corresp="#s4:l2">dagh</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Siet dat is broot en dat is <rhyme label="c" type="m">wijn</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Waer kan sulck fruytasie <rhyme label="c" type="m" corresp="#s4:l5">zijn</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Vyva, vyva, <rhyme label="d" type="m">Poortugael</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Pour eun belle fille bon <rhyme label="d" type="m" corresp="#s4:l7">Chevael</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer en wilt u niet <rhyme label="a" type="f">verschricken</rhyme></l>
        <l id="s5:l2" met="4" type="+-">'t Is geen gout al watter <rhyme label="b" type="m">blinckt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ay gansch lyden watte <rhyme label="a" type="f" corresp="#s5:l1">sticke</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Dese schoone Bruydt die <rhyme label="b" type="m" corresp="#s5:l2">hinckt</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Want ick hoor een groot <rhyme label="c" type="m">geluydt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Komen de Frontuyren <rhyme label="c" type="m" corresp="#s5:l5">uyt</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Pover, pover, <rhyme label="d" type="m">Poortugael</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Povre fille mauvijs <rhyme label="d" type="m" corresp="#s5:l7">Chevael</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Want de braefste <rhyme label="a" type="f">Kavailiere</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Sitten met de hant in 't <rhyme label="b" type="m">hayr</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Niet ie hebben om van te <rhyme label="a" type="f" corresp="#s6:l1">teere</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">'t Valt een nobel hert te <rhyme label="b" type="m" corresp="#s6:l2">zwaer</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">En daer by men leven <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Dat de Ruyters gaen te <rhyme label="c" type="m" corresp="#s6:l5">voet</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Pover, pover, <rhyme label="d" type="m">Poortugael</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Povre fille mauvijs <rhyme label="d" type="m" corresp="#s6:l7">Chevael</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wat doet onse stercke <rhyme label="a" type="f">Wijne</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Wat doet ons dit noble <rhyme label="b" type="m">Fruyt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Groote sieckte, groote <rhyme label="a" type="f" corresp="#s7:l1">pijne</rhyme></l>
        <l id="s7:l4" met="4" type="+-">O neen o neen, ick scheyer <rhyme label="b" type="m" corresp="#s7:l2">uyt</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Want het Rotterdamsche <rhyme label="c" type="m">nat</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Maeckt de keeltjens vet en <rhyme label="c" type="m" corresp="#s7:l5">glat</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Pover pover, <rhyme label="d" type="m">Poortugael</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Povre fille mauvais <rhyme label="d" type="m" corresp="#s7:l7">Chevael</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Soete Nederlandtsche <rhyme label="a" type="f">hartjes</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Met u blancke poesel <rhyme label="b" type="m">vleys</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Staeckt u suchjes, heelt u <rhyme label="a" type="f" corresp="#s8:l1">smartjes</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Want ick koom haest op de <rhyme label="b" type="m" corresp="#s8:l2">reys</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Op dat ick weer als <rhyme label="c" type="m">plagh</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">U presentie hebben <rhyme label="c" type="m" corresp="#s8:l5">magh</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Pover, pover, <rhyme label="d" type="m">Poortugael</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Povre fille mauvijs <rhyme label="d" type="m" corresp="#s8:l7">Chevael</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Och hoe sal ick voor u <rhyme label="a" type="f">knielen</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Bidde u om <rhyme label="b" type="m">vergiffenis</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">En om helsen u lieve <rhyme label="a" type="f" corresp="#s9:l1">zielen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Soo daer noch een vonckjen <rhyme label="b" type="m" corresp="#s9:l2">is</rhyme>:</l>
        <l id="s9:l5" met="4" type="+-">En ick sal met eenen <rhyme label="c" type="m">soen</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Voor u altaer Offer <rhyme label="c" type="m" corresp="#s9:l5">doen</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Dan verfoey ick <rhyme label="d" type="m">Poortugael</rhyme></l>
        <l id="s9:l8" met="4" type="-+">Met haer fille mauwijs <rhyme label="d" type="m" corresp="#s9:l7">Chevael</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Vyva Prince van <rhyme label="a" type="f">Oraengie</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">'k Heb ons doortocht al <rhyme label="b" type="m">gesien</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Dat komt door het vruchtbaer <rhyme label="a" type="f" corresp="#s10:l1">spaengie</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Dat den Hemel heeft <rhyme label="b" type="m" corresp="#s10:l2">versien</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Want den Koningh geeft <rhyme label="c" type="m">geley</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Vrye passe en gelt van <rhyme label="c" type="m" corresp="#s10:l5">bey</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Pover, pover, <rhyme label="d" type="m">Poortugael</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">Povre fille mauvijs <rhyme label="d" type="m" corresp="#s10:l7">Chevael</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
