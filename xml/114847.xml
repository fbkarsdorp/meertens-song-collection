      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Iris tu te plains</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">GVdula, reyn <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Heylich is u <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Heylich uwen <rhyme label="b" type="f" corresp="#s1:l2">Broeder</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Twee Susters <rhyme label="a" type="m" corresp="#s1:l1">behaeght</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Hebben Godt in <rhyme label="c" type="m">heyligheydt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Van u Ouders wel <rhyme label="c" type="m" corresp="#s1:l5">beleydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Geertruyt, van u <rhyme label="a" type="m">bloedt</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Heeft u oock <rhyme label="b" type="f">gheheven</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">U den naem <rhyme label="b" type="f" corresp="#s2:l2">ghegheven</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En ghestiert tot 't <rhyme label="a" type="m" corresp="#s2:l1">goedt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En u tot de <rhyme label="c" type="m">suyverheydt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Door haer goedt vermaen <rhyme label="c" type="m" corresp="#s2:l5">bereydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">In volmaeckte <rhyme label="a" type="m">deught</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Heylich was u <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">'t Hert altijdt <rhyme label="b" type="f" corresp="#s3:l2">verheven</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Was tot Godt in <rhyme label="a" type="m" corresp="#s3:l1">vreught</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Strengh ghy oock in 't <rhyme label="c" type="m">heymelijck</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Leefde, in verdiensten <rhyme label="c" type="m" corresp="#s3:l5">rijck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Boven wel <rhyme label="a" type="m">gheschoeit</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Ginghdy over <rhyme label="b" type="f">straten</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Barvoets, op dat <rhyme label="b" type="f" corresp="#s4:l2">laten</rhyme></l>
        <l id="s4:l4" met="3" type="+-">'t Sou dus zijn <rhyme label="a" type="m" corresp="#s4:l1">bevroeit</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Maer dit, by gheval <rhyme label="c" type="m">verclaert</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Door Godts teecken wert <rhyme label="c" type="m" corresp="#s4:l5">vermaert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Een melaetse <rhyme label="a" type="m">vrouw</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Tot u is <rhyme label="b" type="f">ghecomen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Op dat wegh <rhyme label="b" type="f" corresp="#s5:l2">ghenomen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Door u worden <rhyme label="a" type="m" corresp="#s5:l1">sou</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Haer melaetsheydt, die <rhyme label="c" type="m">terstondt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Wert door u ghebedt <rhyme label="c" type="m" corresp="#s5:l5">ghesont</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Wilt oock van ons <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s6:l2" met="3" type="+-">De melaetsheydt <rhyme label="b" type="f">weiren</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Die ons meer sou <rhyme label="b" type="f" corresp="#s6:l2">deiren</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Die verhoort hier <rhyme label="a" type="m" corresp="#s6:l1">wert</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Maer by Godt nu meer <rhyme label="c" type="m">vermeught</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Die gh' aenschout met alle <rhyme label="c" type="m" corresp="#s6:l5">vreught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
