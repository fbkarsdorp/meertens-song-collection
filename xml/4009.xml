    <melody>Maagdje wilt gij geestelijk wezen</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">OCh maechdeken wildy gheestelijc <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">So moet ghi v onder eenen anderen <rhyme label="a" type="f" corresp="#s1:l1">gheuen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Wat si v ghebieden tot alle <rhyme label="b" type="m">stonden</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Wat si v ghebieden tot alle <rhyme label="b" type="m" corresp="#s1:l3">stonden</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Daer in so moety sijn te <rhyme label="b" type="m" corresp="#s1:l3">vreden</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O heere dat can ic seer qualijck <rhyme label="a" type="f">gheliden</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Dat mi een ander sal <rhyme label="a" type="f" corresp="#s2:l1">onderwisen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Ic wil mijns selfs regeerder <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Ic wil mijns selfs regeerder <rhyme label="b" type="m" corresp="#s2:l3">sijn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Dats mijnder herten so grote <rhyme label="b" type="m" corresp="#s2:l3">pijn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Lief kint ghi en moecht der pauwen niet <rhyme label="a" type="f">slachten</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ghi en moecht v veerkens niet so hooch op <rhyme label="a" type="f" corresp="#s3:l1">rechten</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Ghi moet v oogen beneden <rhyme label="b" type="m">slaen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Ghi moet v oogen beneden <rhyme label="b" type="m" corresp="#s3:l3">slaen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Suldy die valsce stricken <rhyme label="b" type="m" corresp="#s3:l3">ontgane</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O here tis mi een salige <rhyme label="a" type="f">leer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Want dat striden valt mi soe <rhyme label="a" type="f" corresp="#s4:l1">suer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Het sal my costen mijn vleesch mijn <rhyme label="b" type="m">bloet</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Het sal my costen mijn vleesch mijn <rhyme label="b" type="m" corresp="#s4:l3">bloet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Want die stormen sijn also <rhyme label="b" type="m" corresp="#s4:l3">groot</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Lief kint ghi en dorst so seer niet <rhyme label="a" type="f">droeuen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Heer Ihesus sal sijn vrienden so <rhyme label="a" type="f" corresp="#s5:l1">proeuen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Hi salse proeuen ghelijc dat <rhyme label="b" type="m">gout</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Hi salse proeuen ghelijc dat <rhyme label="b" type="m" corresp="#s5:l3">gout</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Want ghi noch sijt van minnen <rhyme label="b" type="m" corresp="#s5:l3">cout</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O here sult ghi v vrienden so <rhyme label="a" type="f">proeuen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Des mach ic mi wel seer <rhyme label="a" type="f" corresp="#s6:l1">bedroeuen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Want seyt mi yemant een toornich <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Want seyt mi yemant een toornich <rhyme label="b" type="m" corresp="#s6:l3">woort</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Ick ben so haest op hem <rhyme label="b" type="m" corresp="#s6:l3">ghestoort</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Lief kint ghi en dorst so seere niet <rhyme label="a" type="f">clagen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Ick sal v doer alle v stormen <rhyme label="a" type="f" corresp="#s7:l1">draghen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Ende ick en sals v niet af <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Ende ick en sals v niet af <rhyme label="b" type="m" corresp="#s7:l3">gaen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Ende dan sal ic v ridder <rhyme label="b" type="m" corresp="#s7:l3">slaen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O here sult ghi mi ridder <rhyme label="a" type="f">slaen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">So is mijn trueren al <rhyme label="a" type="f" corresp="#s8:l1">ghedaen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">So en wil ic toch nu niet meer <rhyme label="b" type="m">sorghen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">So en wil ic toch nu niet meer <rhyme label="b" type="m" corresp="#s8:l3">sorghen</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Als ick v heb tot eenen <rhyme label="b" type="m" corresp="#s8:l3">borghe</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ick wil metter tortelduyue gaan <rhyme label="a" type="f">rusten</rhyme></l>
        <l id="s9:l2" met="4" type="-+">In Iesus wonden mi mach <rhyme label="a" type="f" corresp="#s9:l1">ghelusten</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Ende bliuen daer desen winter <rhyme label="b" type="m">wt</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Ende bliuen daer desen winter <rhyme label="b" type="m" corresp="#s9:l3">wt</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Want Iesus mijn brudegom is so <rhyme label="b" type="m" corresp="#s9:l3">rijc</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Te somer wil ick weder opwaerts <rhyme label="a" type="f">vlieghen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Tot Iesum minen soeten <rhyme label="a" type="f" corresp="#s10:l1">lieue</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Ende sien wat hi mi heeft <rhyme label="b" type="m">bereet</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Ende sien wat hi mi heeft <rhyme label="b" type="m" corresp="#s10:l3">bereet</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Hi heeft mi dic so seer <rhyme label="b" type="m" corresp="#s10:l3">ghenoot</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hi salse nemen al metter <rhyme label="a" type="f">hant</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Hi salse leyden aen den <rhyme label="a" type="f" corresp="#s11:l1">dans</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Daer salse hooren die enghelen <rhyme label="b" type="m">singhen</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Daer salse hooren die enghelen <rhyme label="b" type="m" corresp="#s11:l3">singhen</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Ende bliuen daer als een <rhyme label="b" type="m" corresp="#s11:l3">keyserinne</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Men sal segghen mijn lieue <rhyme label="a" type="f">vriendinne</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Die ick met goeder herten <rhyme label="a" type="f" corresp="#s12:l1">beminne</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Nu siet die bruyloft is v <rhyme label="b" type="m">bereet</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Nu siet die bruyloft is v <rhyme label="b" type="m" corresp="#s12:l3">bereet</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Nu slaet om een hemels <rhyme label="b" type="m" corresp="#s12:l3">cleet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
