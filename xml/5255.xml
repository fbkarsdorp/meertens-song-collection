    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Nu looft den Heer al <rhyme label="a" type="f">t'same</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Zijnen heylighen <rhyme label="a" type="f" corresp="#s1:l1">name</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Prijst oock nae den <rhyme label="a" type="f" corresp="#s1:l1">betame</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Eeuwich voor alle <rhyme label="b" type="f">saken</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wijt en breet gaet de <rhyme label="a" type="f" corresp="#s1:l1">fame</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van zijn goetheyt <rhyme label="a" type="f" corresp="#s1:l1">bequame</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Niemant hem dan en <rhyme label="a" type="f" corresp="#s1:l1">schame</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Zijnen roem groot te <rhyme label="b" type="f" corresp="#s1:l4">maken</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Want hy is den <rhyme label="c" type="m">crijchs-man</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Die voor ons strijden <rhyme label="c" type="m" corresp="#s1:l9">can</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Also wy mercken <rhyme label="c" type="m" corresp="#s1:l9">dan</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Dat hy recht voren <rhyme label="c" type="m" corresp="#s1:l9">an</rhyme>:</l>
        <l id="s1:l13" met="3" type="-+">Dus wilt den lof <rhyme label="d" type="f">verclaren</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Vanden Heer der <rhyme label="d" type="f" corresp="#s1:l13">heyrscharen</rhyme></l>
        <l id="s1:l15" met="3" type="-+">Ghy zijn trouwe <rhyme label="d" type="f" corresp="#s1:l13">dienaren</rhyme>,</l>
        <l id="s1:l16" met="2" type="-+">Want hy wil <rhyme label="e" type="m">zijn</rhyme></l>
        <l id="s1:l17" met="3" type="-+">Onsen bescherm-heer <rhyme label="e" type="m" corresp="#s1:l16">fijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den vyant na <rhyme label="a" type="f">d'usance</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die quam met zweert en <rhyme label="a" type="f" corresp="#s2:l1">lance</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Voor Bommel fray ten <rhyme label="a" type="f" corresp="#s2:l1">dance</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Maer t'spel ginck hem <rhyme label="b" type="f">bedroeven</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Hy en sach daer gheen <rhyme label="a" type="f" corresp="#s2:l1">kanse</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daerom maeckten een <rhyme label="a" type="f" corresp="#s2:l1">Schance</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En stelde zijn <rhyme label="a" type="f" corresp="#s2:l1">finance</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Tot den winter te <rhyme label="b" type="f" corresp="#s2:l4">toeven</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Om also over <rhyme label="c" type="m">d'ijs</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Te comen seer <rhyme label="c" type="m" corresp="#s2:l9">propijs</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Maer God den Heere <rhyme label="c" type="m" corresp="#s2:l9">wijs</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Sy eeuwich lof en <rhyme label="c" type="m" corresp="#s2:l9">prijs</rhyme></l>
        <l id="s2:l13" met="3" type="-+">Ginck hem den wech <rhyme label="d" type="f">toepalen</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Met veel sneeus sonder <rhyme label="d" type="f" corresp="#s2:l13">falen</rhyme></l>
        <l id="s2:l15" met="3" type="-+">Muyten ginghen die <rhyme label="d" type="f" corresp="#s2:l13">Walen</rhyme></l>
        <l id="s2:l16" met="2" type="-+">In Sint <rhyme label="e" type="m">Andries</rhyme></l>
        <l id="s2:l17" met="3" type="-+">Elck zijn betalingh <rhyme label="e" type="m" corresp="#s2:l16">hies</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als het vriesen <rhyme label="a" type="f">cesseerden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En dat sneeuwen <rhyme label="a" type="f" corresp="#s3:l1">mineerden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Seer haestelick <rhyme label="a" type="f" corresp="#s3:l1">marcheerden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Onsen Vorst van <rhyme label="b" type="f">Nassouwen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">De Schansse hy <rhyme label="a" type="f" corresp="#s3:l1">sommeerden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die Hollandt <rhyme label="a" type="f" corresp="#s3:l1">perturbeerden</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Maer sy lien <rhyme label="a" type="f" corresp="#s3:l1">persisteerden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Het Fort seer wel te <rhyme label="b" type="f" corresp="#s3:l4">houwen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Doch ten duerden niet <rhyme label="c" type="m">lanck</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Al haer voornemen <rhyme label="c" type="m" corresp="#s3:l9">stranck</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Want den noot haer <rhyme label="c" type="m" corresp="#s3:l9">bedwanck</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Dat sy teghens haer <rhyme label="c" type="m" corresp="#s3:l9">danck</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Zochten om <rhyme label="d" type="f">t'appoincteren</rhyme></l>
        <l id="s3:l14" met="3" type="-+">D'welckmen ginck <rhyme label="d" type="f" corresp="#s3:l13">accorderen</rhyme>,</l>
        <l id="s3:l15" met="3" type="-+">Doen sachmen <rhyme label="d" type="f" corresp="#s3:l13">triumpheren</rhyme>,</l>
        <l id="s3:l16" met="2" type="-+">Het Landt <rhyme label="e" type="m">ydoon</rhyme></l>
        <l id="s3:l17" met="3" type="-+">Om de victory <rhyme label="e" type="m" corresp="#s3:l16">schoon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daer nae zijn <rhyme label="a" type="f">Excellenty</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In aller <rhyme label="a" type="f" corresp="#s4:l1">diligenty</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Door des Landts <rhyme label="a" type="f" corresp="#s4:l1">assistenty</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Vergaerden duyst <rhyme label="b" type="f">schepen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Niemant wist zijn <rhyme label="a" type="f" corresp="#s4:l1">intenty</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Waer toe al dees <rhyme label="a" type="f" corresp="#s4:l1">inventy</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Bereyt wert <rhyme label="a" type="f" corresp="#s4:l1">imprudenty</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Oft het wert niet <rhyme label="b" type="f" corresp="#s4:l4">begrepen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Maer Juny twintich <rhyme label="c" type="m">een</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Zeylden sy groot en <rhyme label="c" type="m" corresp="#s4:l9">cleen</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Nae Philippyn soo 't <rhyme label="c" type="m" corresp="#s4:l9">scheen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Daer sy alle <rhyme label="c" type="m" corresp="#s4:l9">ghemeen</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Door Vlaend'ren zijn <rhyme label="d" type="f">ghetoghen</rhyme></l>
        <l id="s4:l14" met="3" type="-+">Stont voor des Vyants <rhyme label="d" type="f" corresp="#s4:l13">ooghen</rhyme></l>
        <l id="s4:l15" met="3" type="-+">Brugghe moest oock <rhyme label="d" type="f" corresp="#s4:l13">ghedooghen</rhyme></l>
        <l id="s4:l16" met="2" type="-+">Voor by de <rhyme label="e" type="m">stadt</rhyme></l>
        <l id="s4:l17" met="3" type="-+">Te nemen haren <rhyme label="e" type="m" corresp="#s4:l16">padt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Oudenborch stont eerst <rhyme label="a" type="f">ane</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Want dat lach op de <rhyme label="a" type="f" corresp="#s5:l1">bane</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Om na Nieupoort te <rhyme label="a" type="f" corresp="#s5:l1">gane</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Daer ons volck wilde <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Albertus Schans <rhyme label="a" type="f" corresp="#s5:l1">voortane</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Conde niet <rhyme label="a" type="f" corresp="#s5:l1">wederstane</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Ons ghewelt soo ick <rhyme label="a" type="f" corresp="#s5:l1">wane</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Maer gavent op tot <rhyme label="b" type="f" corresp="#s5:l4">desen</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">Doen trock het Legher <rhyme label="c" type="m">voort</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Na de stadt van <rhyme label="c" type="m" corresp="#s5:l9">Nieupoort</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Zijn Excellenty <rhyme label="c" type="m" corresp="#s5:l9">hoort</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Aenboot hen goet <rhyme label="c" type="m" corresp="#s5:l9">accoort</rhyme>,</l>
        <l id="s5:l13" met="3" type="-+">Doch soo zijt niet op <rhyme label="d" type="f">gaven</rhyme></l>
        <l id="s5:l14" met="3" type="-+">Soud' hyse haest om <rhyme label="d" type="f" corresp="#s5:l13">graven</rhyme></l>
        <l id="s5:l15" met="3" type="-+">Aen weer zijts vande <rhyme label="d" type="f" corresp="#s5:l13">haven</rhyme></l>
        <l id="s5:l16" met="2" type="-+">Zijn gheschut <rhyme label="e" type="m">koen</rhyme></l>
        <l id="s5:l17" met="3" type="-+">Soud' de Poorten op <rhyme label="e" type="m" corresp="#s5:l16">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Albertus met zijn <rhyme label="a" type="f">Papen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Heeft hier niet op <rhyme label="a" type="f" corresp="#s6:l1">gheslapen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Maer al zijn Spaensche <rhyme label="a" type="f" corresp="#s6:l1">knapen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van wijt en zijts <rhyme label="b" type="f">ontboden</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Om ons volck te <rhyme label="a" type="f" corresp="#s6:l1">betrapen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Stelden sy hen in <rhyme label="a" type="f" corresp="#s6:l1">wapen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Het was doen al claer <rhyme label="a" type="f" corresp="#s6:l1">schapen</rhyme>:</l>
        <l id="s6:l8" met="3" type="-+">T'quam hem al <rhyme label="b" type="f" corresp="#s6:l4">toeghevloden</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Ja tot van by <rhyme label="c" type="m">Maestricht</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Wert het volck <rhyme label="c" type="m" corresp="#s6:l9">opghelicht</rhyme></l>
        <l id="s6:l11" met="3" type="+-">Nacht en dach <rhyme label="c" type="m" corresp="#s6:l9">onghezwicht</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Marceerden sy seer <rhyme label="c" type="m" corresp="#s6:l9">dicht</rhyme>,</l>
        <l id="s6:l13" met="3" type="-+">Te Peerd' en oock te <rhyme label="d" type="f">waghen</rhyme>,</l>
        <l id="s6:l14" met="3" type="-+">Wert het voet-volck <rhyme label="d" type="f" corresp="#s6:l13">gedraghen</rhyme></l>
        <l id="s6:l15" met="3" type="-+">Soo datse in vier <rhyme label="d" type="f" corresp="#s6:l13">daghen</rhyme></l>
        <l id="s6:l16" met="2" type="-+">Meest al haer <rhyme label="e" type="m">macht</rhyme></l>
        <l id="s6:l17" met="3" type="-+">Hebben by een <rhyme label="e" type="m" corresp="#s6:l16">ghebracht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Oudenborgh quam in <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Weder van ons <rhyme label="a" type="f" corresp="#s7:l1">vyanden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Want sijt vooreerst <rhyme label="a" type="f" corresp="#s7:l1">aenranden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Met haren gantschen <rhyme label="b" type="f">Legher</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Accoort voor den <rhyme label="a" type="f" corresp="#s7:l1">vailjanden</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Wert ghemaeckt met <rhyme label="a" type="f" corresp="#s7:l1">verstanden</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Maer braken t'harer <rhyme label="a" type="f" corresp="#s7:l1">schanden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Daer bleef menich stout <rhyme label="b" type="f" corresp="#s7:l4">vegher</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Twee Schanssen daer <rhyme label="c" type="m">ontrent</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Werden oock <rhyme label="c" type="m" corresp="#s7:l9">aengherent</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">En hebben daer <rhyme label="c" type="m" corresp="#s7:l9">gheschent</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Boven <rhyme label="c" type="m" corresp="#s7:l9">t'appoinctement</rhyme></l>
        <l id="s7:l13" met="3" type="-+">Al ons vrome <rhyme label="d" type="f">Soldaten</rhyme></l>
        <l id="s7:l14" met="3" type="-+">Wreedelijck boven <rhyme label="d" type="f" corresp="#s7:l13">maten</rhyme></l>
        <l id="s7:l15" met="3" type="-+">Maer 'sanderendaechs <rhyme label="d" type="f" corresp="#s7:l13">saten</rhyme></l>
        <l id="s7:l16" met="2" type="-+">Sy vol van <rhyme label="e" type="m">schrick</rhyme></l>
        <l id="s7:l17" met="3" type="-+">Meest inden selven <rhyme label="e" type="m" corresp="#s7:l16">strick</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Onsen Prince <rhyme label="a" type="f">cloeckmoedich</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die creech die wete <rhyme label="a" type="f" corresp="#s8:l1">spoedich</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dat den vyandt <rhyme label="a" type="f" corresp="#s8:l1">verwoedich</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Met zijn macht quam <rhyme label="b" type="f">aensetten</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Hy sant hen teghen <rhyme label="a" type="f" corresp="#s8:l1">vroedich</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Twee regimenten <rhyme label="a" type="f" corresp="#s8:l1">goedich</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Om des Vyandt seer <rhyme label="a" type="f" corresp="#s8:l1">bloedich</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Voornemen te <rhyme label="b" type="f" corresp="#s8:l4">beletten</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Maer werden inde <rhyme label="c" type="m">vlucht</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Gheslaghen met <rhyme label="c" type="m" corresp="#s8:l9">gherucht</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Daer bleeffer int <rhyme label="c" type="m" corresp="#s8:l9">ghesucht</rhyme>,</l>
        <l id="s8:l12" met="3" type="-+">Ten is voorwaer gheen <rhyme label="c" type="m" corresp="#s8:l9">clucht</rhyme></l>
        <l id="s8:l13" met="3" type="-+">Veel cloecke <rhyme label="d" type="f">Capiteynen</rhyme></l>
        <l id="s8:l14" met="3" type="-+">Vochten daer als <rhyme label="d" type="f" corresp="#s8:l13">Romeynen</rhyme></l>
        <l id="s8:l15" met="3" type="-+">Om alsoo te <rhyme label="d" type="f" corresp="#s8:l13">vercleynen</rhyme></l>
        <l id="s8:l16" met="2" type="-+">S'vyandts macht <rhyme label="e" type="m">groot</rhyme>:</l>
        <l id="s8:l17" met="3" type="-+">Maer bleven meest al <rhyme label="e" type="m" corresp="#s8:l16">doot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Seer haest al sonder <rhyme label="a" type="f">beyden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Ginck elck zijn volck <rhyme label="a" type="f" corresp="#s9:l1">bereyden</rhyme></l>
        <l id="s9:l3" met="3" type="-+">En in slachorden <rhyme label="a" type="f" corresp="#s9:l1">leyden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Daer den prijs was te <rhyme label="b" type="f">geven</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Sijn Excellenty <rhyme label="a" type="f" corresp="#s9:l1">seyden</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Tot zijn volck op der <rhyme label="a" type="f" corresp="#s9:l1">heyden</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Ick sal u niet <rhyme label="a" type="f" corresp="#s9:l1">afscheyden</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Soo langh' als ick sal <rhyme label="b" type="f" corresp="#s9:l4">leven</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Strijt vroom voor <rhyme label="c" type="m">t'Vaderlant</rhyme></l>
        <l id="s9:l10" met="3" type="-+">En voor Gods woort <rhyme label="c" type="m" corresp="#s9:l9">playsant</rhyme></l>
        <l id="s9:l11" met="3" type="+-">Met dat zweert inde <rhyme label="c" type="m" corresp="#s9:l9">hant</rhyme></l>
        <l id="s9:l12" met="3" type="-+">Trat hy voor aen <rhyme label="c" type="m" corresp="#s9:l9">constant</rhyme>,</l>
        <l id="s9:l13" met="3" type="-+">T'gheschut seer dapper <rhyme label="d" type="f">speelden</rhyme></l>
        <l id="s9:l14" met="3" type="-+">T'welck den vyant <rhyme label="d" type="f" corresp="#s9:l13">verveelden</rhyme></l>
        <l id="s9:l15" met="3" type="-+">Om dat het veel volcx <rhyme label="d" type="f" corresp="#s9:l13">heelden</rhyme></l>
        <l id="s9:l16" met="2" type="-+">Die daer <rhyme label="e" type="m">subijt</rhyme></l>
        <l id="s9:l17" met="3" type="-+">Doot bleven inden <rhyme label="e" type="m" corresp="#s9:l16">strijt</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ons volck doen manlick <rhyme label="a" type="f">vielen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">En maeckten seer veel <rhyme label="a" type="f" corresp="#s10:l1">zielen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Onder de Spaensche <rhyme label="a" type="f" corresp="#s10:l1">fielen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Door het houwen en <rhyme label="b" type="f">kerven</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Men sach den vyandt <rhyme label="a" type="f" corresp="#s10:l1">crielen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Die ons wilden <rhyme label="a" type="f" corresp="#s10:l1">vernielen</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Maer toonden haest de <rhyme label="a" type="f" corresp="#s10:l1">hielen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Doch moesten leeren <rhyme label="b" type="f" corresp="#s10:l4">sterven</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Menighen Spaenschen <rhyme label="c" type="m">Helt</rhyme></l>
        <l id="s10:l10" met="3" type="-+">Bleef daer ligghen int <rhyme label="c" type="m" corresp="#s10:l9">velt</rhyme></l>
        <l id="s10:l11" met="3" type="+-">Sonder die met <rhyme label="c" type="m" corresp="#s10:l9">ghewelt</rhyme></l>
        <l id="s10:l12" met="3" type="-+">Gevangen wiert <rhyme label="c" type="m" corresp="#s10:l9">gestelt</rhyme>,</l>
        <l id="s10:l13" met="3" type="-+">L'Admirant de quade <rhyme label="d" type="f">reste</rhyme></l>
        <l id="s10:l14" met="3" type="-+">Gheensins een vande <rhyme label="d" type="f" corresp="#s10:l13">beste</rhyme></l>
        <l id="s10:l15" met="3" type="-+">Die wert daer oock int <rhyme label="d" type="f" corresp="#s10:l13">leste</rhyme></l>
        <l id="s10:l16" met="2" type="-+">Teghen zijn <rhyme label="e" type="m">wil</rhyme></l>
        <l id="s10:l17" met="3" type="-+">Gevangen int <rhyme label="e" type="m" corresp="#s10:l16">gheschil</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Veel vande <rhyme label="a" type="f">principale</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Heeren int <rhyme label="a" type="f" corresp="#s11:l1">generale</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Ginghen nae Plutos <rhyme label="a" type="f" corresp="#s11:l1">sale</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Sonder eenich <rhyme label="b" type="f">vertrecken</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Laberlote den <rhyme label="a" type="f" corresp="#s11:l1">Wale</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Quam daer ooc in s'doots <rhyme label="a" type="f" corresp="#s11:l1">quale</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Recht na zijns Meesters <rhyme label="a" type="f" corresp="#s11:l1">tale</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Met noch meer groote <rhyme label="b" type="f" corresp="#s11:l4">gecken</rhyme></l>
        <l id="s11:l9" met="3" type="-+">Veel Spaensche Heeren <rhyme label="c" type="m">snoy</rhyme>,</l>
        <l id="s11:l10" met="3" type="-+">Hoefden daer geen <rhyme label="c" type="m" corresp="#s11:l9">convoy</rhyme></l>
        <l id="s11:l11" met="3" type="+-">Don Ambrose <rhyme label="c" type="m" corresp="#s11:l9">Loondroy</rhyme></l>
        <l id="s11:l12" met="3" type="-+">En Don Sappena <rhyme label="c" type="m" corresp="#s11:l9">moy</rhyme>,</l>
        <l id="s11:l13" met="3" type="-+">Met noch meer Spaensche <rhyme label="d" type="f">dwergen</rhyme></l>
        <l id="s11:l14" met="3" type="-+">Sullen ons niet meer <rhyme label="d" type="f" corresp="#s11:l13">terghen</rhyme>,</l>
        <l id="s11:l15" met="3" type="-+">Noch oock geen moordaet <rhyme label="d" type="f" corresp="#s11:l13">vergen</rhyme></l>
        <l id="s11:l16" met="2" type="-+">Fresne is <rhyme label="e" type="m">daer</rhyme></l>
        <l id="s11:l17" met="3" type="-+">Met Standly den <rhyme label="e" type="m" corresp="#s11:l16">verraer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Al dees Spaensche <rhyme label="a" type="f">Seignoren</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Hadden met eedt <rhyme label="a" type="f" corresp="#s12:l1">ghezworen</rhyme>.</l>
        <l id="s12:l3" met="3" type="-+">Ons al t'samen te <rhyme label="a" type="f" corresp="#s12:l1">versmoren</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Sonder eenich <rhyme label="b" type="f">ontfermen</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Maer God heeft zijnen <rhyme label="a" type="f" corresp="#s12:l1">thoren</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Op hen gestort te <rhyme label="a" type="f" corresp="#s12:l1">vooren</rhyme></l>
        <l id="s12:l7" met="3" type="-+">En ons zijn <rhyme label="a" type="f" corresp="#s12:l1">uytvercoren</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Voor hen willen <rhyme label="b" type="f" corresp="#s12:l4">beschermen</rhyme>:</l>
        <l id="s12:l9" met="3" type="-+">Hierom laet ons <rhyme label="c" type="m">certeyn</rhyme>,</l>
        <l id="s12:l10" met="3" type="-+">Godt loven <rhyme label="c" type="m" corresp="#s12:l9">alghemeyn</rhyme></l>
        <l id="s12:l11" met="3" type="+-">Dat hy dees Landen <rhyme label="c" type="m" corresp="#s12:l9">cleyn</rhyme></l>
        <l id="s12:l12" met="3" type="-+">Voor dat ghespuys <rhyme label="c" type="m" corresp="#s12:l9">villeyn</rhyme></l>
        <l id="s12:l13" met="3" type="-+">Bewaert in allen <rhyme label="d" type="f">percken</rhyme></l>
        <l id="s12:l14" met="3" type="-+">Dus laet ons recht <rhyme label="d" type="f" corresp="#s12:l13">bemercken</rhyme></l>
        <l id="s12:l15" met="3" type="-+">Gods wonderlijcke <rhyme label="d" type="f" corresp="#s12:l13">wercken</rhyme></l>
        <l id="s12:l16" met="2" type="-+">Want hy ten <rhyme label="e" type="m">val</rhyme></l>
        <l id="s12:l17" met="3" type="-+">Brenght ons vyanden <rhyme label="e" type="m" corresp="#s12:l16">al</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Lof Prince Godt <rhyme label="a" type="f">almachtich</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Lof onsen Heylant <rhyme label="a" type="f" corresp="#s13:l1">crachtich</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Eeuwighen loff <rhyme label="a" type="f" corresp="#s13:l1">eendrachtich</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Zy u aen allen <rhyme label="b" type="f">Enden</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Lof noot-helper <rhyme label="a" type="f" corresp="#s13:l1">waerachtich</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Die u volck zijt <rhyme label="a" type="f" corresp="#s13:l1">ghedachtich</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Als ons den vyandt <rhyme label="a" type="f" corresp="#s13:l1">prachtich</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Geheelijck wilde <rhyme label="b" type="f" corresp="#s13:l4">schenden</rhyme>:</l>
        <l id="s13:l9" met="3" type="-+">Looft hem al <rhyme label="c" type="m">onghetreurt</rhyme></l>
        <l id="s13:l10" met="3" type="-+">Wiens goetheyt eeuwich <rhyme label="c" type="m" corresp="#s13:l9">deurt</rhyme></l>
        <l id="s13:l11" met="3" type="+-">En den vyandt <rhyme label="c" type="m" corresp="#s13:l9">verscheurt</rhyme></l>
        <l id="s13:l12" met="3" type="-+">Soomen voor ooghen <rhyme label="c" type="m" corresp="#s13:l9">speurt</rhyme></l>
        <l id="s13:l13" met="3" type="-+">Hy leert tot allen <rhyme label="d" type="f">tijden</rhyme></l>
        <l id="s13:l14" met="3" type="-+">Onsen Vorst kloecklijck <rhyme label="d" type="f" corresp="#s13:l13">strijden</rhyme></l>
        <l id="s13:l15" met="3" type="-+">Daerom wil ick <rhyme label="d" type="f" corresp="#s13:l13">belijden</rhyme></l>
        <l id="s13:l16" met="2" type="-+">Godt is mijn <rhyme label="e" type="m">Borght</rhyme>:</l>
        <l id="s13:l17" met="3" type="-+">Die voor ons allen <rhyme label="e" type="m" corresp="#s13:l16">sorght</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
