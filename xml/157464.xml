<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157464</id>
    <date>
      <year-start>1700</year-start>
      <year-end>1700</year-end>
    </date>
    <tags/>
    <melody>Van Marritje van Oostzanen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Goeden avond Maeritje van <rhyme label="a" type="f">Oost-zaene</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Jk heb 't jou moertje laest <rhyme label="b" type="m">esayt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Jk moet het jou ierst gaen <rhyme label="a" type="f" corresp="#s1:l1">verhalen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Wilt gy myn Liefje wesen dikke vette bolle <rhyme label="b" type="m" corresp="#s1:l2">mayt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Jk sal jou schenken Bachus <rhyme label="a" type="f" corresp="#s1:l1">traenen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Doet myn maer in 't kort <rhyme label="b" type="m" corresp="#s1:l2">bescheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wel Krelis bent ghy buyten <rhyme label="a" type="f">zinnen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En wat slaet gy uyt voor malle <rhyme label="b" type="m">praet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En wilt soo haestig niet <rhyme label="a" type="f" corresp="#s2:l1">beginnen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Voorsiet mijn alderliefste Jonge <rhyme label="b" type="m" corresp="#s2:l2">maet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">En wilt u ierst ter deeg <rhyme label="a" type="f" corresp="#s2:l1">beraden</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Eer dat gy met myn <rhyme label="b" type="m" corresp="#s2:l2">angaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wel Marritje het zijnder <rhyme label="a" type="f">grillen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Want gy zijt soo een braven <rhyme label="b" type="m">Mayt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Met een paer dikke vette bolle <rhyme label="a" type="f" corresp="#s3:l1">billen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die van heur Mortje nog wat <rhyme label="b" type="m" corresp="#s3:l2">hayt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Jk moet u ook eens <rhyme label="a" type="f" corresp="#s3:l1">verhaelen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Waer mijn vaertjes goed al <rhyme label="b" type="m" corresp="#s3:l2">layt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Jk heb mijn vartjes beste <rhyme label="a" type="f">klieren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En drie stuyvers en een <rhyme label="b" type="m">doyt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Daer toe heb ik mantel met <rhyme label="a" type="f" corresp="#s4:l1">eeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Om te komen by mijn soete lieve <rhyme label="b" type="m" corresp="#s4:l2">bruyt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Jk sweer ik sal u soo <rhyme label="a" type="f" corresp="#s4:l1">stoffieren</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Jk heb mijn vaertjes goet voor <rhyme label="b" type="m" corresp="#s4:l2">uyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wel Krelis en gy sou myn <rhyme label="a" type="f">bekooren</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En dat al deur u groote <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Jk moet het u ook eens laten <rhyme label="a" type="f" corresp="#s5:l1">hooren</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Jk heb ook voor uyt mijn Moers <rhyme label="b" type="m" corresp="#s5:l2">goet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Jk heb een Esel mey lange <rhyme label="a" type="f" corresp="#s5:l1">ooren</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Die ons altijt dienen <rhyme label="b" type="m" corresp="#s5:l2">moet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Jk heb mijn kalvers te <rhyme label="a" type="f">wayden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">En een Sog die werpen <rhyme label="b" type="m">zal</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Jk heb mijn Schaepen aen der <rhyme label="a" type="f" corresp="#s6:l1">hayden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En nog een mooie hont op <rhyme label="b" type="m" corresp="#s6:l2">stal</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Die als wy zijn met ongs <rhyme label="a" type="f" corresp="#s6:l1">bayde</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Die ons wel beschutten <rhyme label="b" type="m" corresp="#s6:l2">zal</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Jk heb mijn potten en mijn <rhyme label="a" type="f">pannen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En een zay spek al inde <rhyme label="b" type="m">schouw</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Jk heb mijn hennen en mijn <rhyme label="a" type="f" corresp="#s7:l1">haennen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En mijn twee hamme zijn alleen voor <rhyme label="b" type="m" corresp="#s7:l2">Jou</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Jk heb mijn Enden en mijn <rhyme label="a" type="f" corresp="#s7:l1">Swaenen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">En een nieuwe melck <rhyme label="b" type="m" corresp="#s7:l2">mout</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wel Maeritje wy zijn Rijke <rhyme label="a" type="f">Luyden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Leggen wy ons goet dan by <rhyme label="b" type="m">malkaer</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Jk sweer ik sal u van avond nog eens <rhyme label="a" type="f" corresp="#s8:l1">soene</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Want wy sijn dat soete lieve Jonge <rhyme label="b" type="m" corresp="#s8:l2">paer</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Wy sullen vanavond Mosselen <rhyme label="a" type="f" corresp="#s8:l1">kruyen</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Met de Arremen door <rhyme label="b" type="m" corresp="#s8:l2">malkaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
