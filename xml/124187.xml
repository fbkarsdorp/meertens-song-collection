    <tags>
      <tag translation="seasonal song">seizoenslied</tag>
    </tags>
    <melody>Het rozenknopje</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WIe schept geen <rhyme label="a" type="f">behagen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">By Zomerze <rhyme label="a" type="f" corresp="#s1:l1">Dagen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In 't zoet vermakelijk <rhyme label="b" type="m">Groen</rhyme>?</l>
        <l id="s1:l4" met="2" type="-+">Wie gaat niet <rhyme label="c" type="f">aanschouwen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Die schoone <rhyme label="c" type="f" corresp="#s1:l4">Lands-douwen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die cierlijk haar op <rhyme label="b" type="m" corresp="#s1:l3">doen</rhyme>?</l>
        <l id="s1:l7" met="2" type="-+">Wie kan het al <rhyme label="d" type="f">noemen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Wat Kruyden en <rhyme label="d" type="f" corresp="#s1:l7">Bloemen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Van zoort, Couleur en <rhyme label="b" type="m" corresp="#s1:l3">Fatzoe...n</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Die zig daar alle op <rhyme label="b" type="m" corresp="#s1:l3">doen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Den landman niet <rhyme label="a" type="f">ledig</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Is al stadig <rhyme label="a" type="f" corresp="#s2:l1">bezig</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van 's Morgens vroeg tot <rhyme label="b" type="m">laat</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Met Spitten en <rhyme label="c" type="f">Spayen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Met Zayen en <rhyme label="c" type="f" corresp="#s2:l4">Mayen</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Een ander uyt Melke <rhyme label="b" type="m" corresp="#s2:l3">gaat</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Zijn Koetjes die <rhyme label="d" type="f">loeyen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Het Zuyvel komt <rhyme label="d" type="f" corresp="#s2:l7">vloeyen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Waar van dat ons land in de <rhyme label="b" type="m" corresp="#s2:l3">daa...d</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Geprezen word boven <rhyme label="b" type="m" corresp="#s2:l3">maat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">De Vogelen <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">De Beesjes die <rhyme label="a" type="f" corresp="#s3:l1">springen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van vreugde in 't Grazige <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ia waar dat wy <rhyme label="c" type="f">mogen</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Ook wende ons <rhyme label="c" type="f" corresp="#s3:l4">Oogen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">'t Is lustig aan alle <rhyme label="b" type="m" corresp="#s3:l3">kant</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">De Ieugd schept <rhyme label="d" type="f">behagen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">En gaat alle <rhyme label="d" type="f" corresp="#s3:l7">Dagen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Te zamen Hand aan <rhyme label="b" type="m" corresp="#s3:l3">Hand...d</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Daar 't schaduwrijk is <rhyme label="b" type="m" corresp="#s3:l3">beplant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Op Wagens en <rhyme label="a" type="f">Schuyten</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Trekt men na <rhyme label="a" type="f" corresp="#s4:l1">Buyten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Het is een lustige <rhyme label="b" type="m">zaak</rhyme>;</l>
        <l id="s4:l4" met="2" type="-+">Dus komt men zijn <rhyme label="c" type="f">Vrinden</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Op zoeken en <rhyme label="c" type="f" corresp="#s4:l4">vinden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Men heeft zijn grootste <rhyme label="b" type="m" corresp="#s4:l3">vermaak</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">Op Duynen en <rhyme label="d" type="f">Heyden</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">In Grazige <rhyme label="d" type="f" corresp="#s4:l7">Weyden</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Daar rold men, En zolt men, zo <rhyme label="b" type="m" corresp="#s4:l3">vaa...k</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Daar in schept de Ieugd zijn <rhyme label="b" type="m" corresp="#s4:l3">vermaak</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">De Ionkmans zijn <rhyme label="a" type="f">vierig</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">De Meysjes niet <rhyme label="a" type="f" corresp="#s5:l1">gierig</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Op een kusje onder het <rhyme label="b" type="m">Groen</rhyme>;</l>
        <l id="s5:l4" met="2" type="-+">Zy zingen en <rhyme label="c" type="f">kweelen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Met heldere <rhyme label="c" type="f" corresp="#s5:l4">keelen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En wat ze daar boven nog <rhyme label="b" type="m" corresp="#s5:l3">doen</rhyme>;</l>
        <l id="s5:l7" met="2" type="-+">Dat mag ik niet <rhyme label="d" type="f">noemen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Ik wil 't liever <rhyme label="d" type="f" corresp="#s5:l7">verbloemen</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Of zeggen het met <rhyme label="b" type="m" corresp="#s5:l3">fatz...n</rhyme>;</l>
        <l id="s5:l10" met="3" type="-+">Het Meysje dat laat 't haar <rhyme label="b" type="m" corresp="#s5:l3">doen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">In Christalijne <rhyme label="a" type="f">Stroomen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Onder 't schaduw der <rhyme label="a" type="f" corresp="#s6:l1">Boomen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Of anders op Strand in <rhyme label="b" type="m">Zee</rhyme>;</l>
        <l id="s6:l4" met="2" type="-+">Daar ziet men ze <rhyme label="c" type="f">plassen</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Haar Voete gaan <rhyme label="c" type="f" corresp="#s6:l4">wassen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Als eerteyds Diana wel <rhyme label="b" type="m" corresp="#s6:l3">dee</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Op Ionge en <rhyme label="d" type="f">Ouwen</rhyme>;</l>
        <l id="s6:l8" met="2" type="-+">'t Is lust te <rhyme label="d" type="f" corresp="#s6:l7">aanschouwen</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">De Zomer deeld 't ons <rhyme label="b" type="m" corresp="#s6:l3">mee</rhyme>...</l>
        <l id="s6:l10" met="3" type="-+">Vermaak voor Mensch en <rhyme label="b" type="m" corresp="#s6:l3">Vee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
