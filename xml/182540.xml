  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Geleid my door de woeste <rhyme label="a" type="m">zee</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Waer door myn ziel verdryft waer <rhyme label="a" type="m" corresp="#s1:l1">heen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Waer ik ga of waer ik <rhyme label="b" type="m">sta</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">Want u lieffelyk <rhyme label="c" type="m">gelaet</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dat zal my <rhyme label="d" type="f">brengen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Dat zal my <rhyme label="d" type="f" corresp="#s1:l5">brengen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">In een droeve <rhyme label="c" type="m" corresp="#s1:l4">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zeg myn tog myn lieve <rhyme label="a" type="m">Meid</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Wat is het dat my <rhyme label="a" type="m" corresp="#s2:l1">verleid</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Kan ik anders vertrouwen op <rhyme label="b" type="m">uw</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Anders dan moet ik gaen dalen in 't <rhyme label="c" type="m">graf</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Daer ons <rhyme label="d" type="f">niemant</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Daer ons <rhyme label="d" type="f" corresp="#s2:l5">niemant</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Scheiden van <rhyme label="c" type="m" corresp="#s2:l4">al</rhyme>.</l>
      </lg>
    </lg>
