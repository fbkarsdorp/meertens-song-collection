<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185178</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O schepper fier</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">V Heerlick <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O reyne Liefd’ <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Wilt my nu <rhyme label="b" type="f" corresp="#s1:l2">ingeven</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat ick mach singhen <rhyme label="c" type="m">luydt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">In ‘t Hemels <rhyme label="a" type="m" corresp="#s1:l1">hof</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Is Godt u lust en <rhyme label="b" type="f" corresp="#s1:l2">leven</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Ghy zijt hier <rhyme label="b" type="f" corresp="#s1:l2">beneven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Gods alderliefste <rhyme label="c" type="m" corresp="#s1:l4">Bruyt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Hy schenckt u de croone // der <rhyme label="d" type="f">eeren</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En sal u lof schoone // <rhyme label="d" type="f" corresp="#s1:l9">vermeeren</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">In ‘t aertsche <rhyme label="e" type="m">dal</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Reyne Liefd’ prijst boven <rhyme label="e" type="m" corresp="#s1:l11">al</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Godd’lijcke <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die uyt Godt zijt <rhyme label="b" type="f">gheboren</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Langhe van te <rhyme label="b" type="f" corresp="#s2:l2">voren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Voor ‘s werelts bros <rhyme label="c" type="m">begin</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Wel prijsens <rhyme label="a" type="m" corresp="#s2:l1">waert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">V stemme soet laet <rhyme label="b" type="f" corresp="#s2:l2">hooren</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Al ghy <rhyme label="b" type="f" corresp="#s2:l2">uytvercoren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Looft dese schoon <rhyme label="c" type="m" corresp="#s2:l4">Goddin</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Laet reyne Liefd’ ‘t ghemoet <rhyme label="d" type="f">bevangen</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Dichter van haer soete <rhyme label="d" type="f" corresp="#s2:l9">gesangen</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Met bly <rhyme label="e" type="m">gheschal</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Reyn liefd’ prijst boven <rhyme label="e" type="m" corresp="#s2:l11">al</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Godt is de <rhyme label="a" type="m">Liefd</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En liefde Godt in ‘t <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Liefde hooch <rhyme label="b" type="f" corresp="#s3:l2">gheresen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Is ‘t Godd’lick klaer <rhyme label="c" type="m">aenschijn</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Of ghy <rhyme label="a" type="m" corresp="#s3:l1">verhieft</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Godts liefde hooch mits <rhyme label="b" type="f" corresp="#s3:l2">desen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Noch soud sy <rhyme label="b" type="f" corresp="#s3:l2">verresen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">‘t Verstant te boven <rhyme label="c" type="m" corresp="#s3:l4">zijn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Dit kont ghy bysonder <rhyme label="d" type="f">mercken</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Als men siet Liefd’s wonder <rhyme label="d" type="f" corresp="#s3:l9">wercken</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">In groot <rhyme label="e" type="m">getal</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Reyn liefd’ prijst boven <rhyme label="e" type="m" corresp="#s3:l11">al</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Der liefden <rhyme label="a" type="m">Gheest</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Barmhertich en <rhyme label="b" type="f">ghenadigh</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Is altijt <rhyme label="b" type="f" corresp="#s4:l2">weldadich</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Den menschen in ‘t <rhyme label="c" type="m">ghemeyn</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Ia minst en <rhyme label="a" type="m" corresp="#s4:l1">meest</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ten goeden seer <rhyme label="b" type="f" corresp="#s4:l2">beradich</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Reyn liefde <rhyme label="b" type="f" corresp="#s4:l2">ghestadich</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Is ‘s levens klaer <rhyme label="c" type="m" corresp="#s4:l4">fonteyn</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Wilt na liefde ten goeden <rhyme label="d" type="f">haken</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Van haer soete vloeden <rhyme label="d" type="f" corresp="#s4:l9">smaken</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Claer als <rhyme label="e" type="m">cristal</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Reyn Liefd’ prijst boven <rhyme label="e" type="m" corresp="#s4:l11">al</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Reyn liefde <rhyme label="a" type="m">is</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Sachtmoedich en <rhyme label="b" type="f">lanckmoedich</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Wilt ghy zijn <rhyme label="b" type="f" corresp="#s5:l2">voorspoedich</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Soo volcht haer na met <rhyme label="c" type="m">vlyt</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Sy toont haer <rhyme label="a" type="m" corresp="#s5:l1">wis</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">In deuchden <rhyme label="b" type="f" corresp="#s5:l2">overvloedich</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Vriendelick en <rhyme label="b" type="f" corresp="#s5:l2">goedich</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Geen mensche sy <rhyme label="c" type="m" corresp="#s5:l4">benyt</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Toont geen opgeblasen <rhyme label="d" type="f">wesen</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Soo wy van veel dwasen <rhyme label="d" type="f" corresp="#s5:l9">lesen</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Schout sot <rhyme label="e" type="m">ghekal</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">Reyn Liefd’ prijst boven <rhyme label="e" type="m" corresp="#s5:l11">al</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Liefd’ laet haer <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Tot felle gramschap <rhyme label="b" type="f">trecken</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Noch tot toorn <rhyme label="b" type="f" corresp="#s6:l2">verwecken</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Daer door sy yemant <rhyme label="c" type="m">krenckt</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Tot schalckheyt <rhyme label="a" type="m" corresp="#s6:l1">siet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Haer sinnen niet en <rhyme label="b" type="f" corresp="#s6:l2">strecken</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Cry is sy van <rhyme label="b" type="f" corresp="#s6:l2">vlecken</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">De liefde niet erchs <rhyme label="c" type="m" corresp="#s6:l4">denckt</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Salt al hopen, lyden, <rhyme label="d" type="f">dragen</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Onrecht sal sy myden <rhyme label="d" type="f" corresp="#s6:l9">t’klagen</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Gaet den wech <rhyme label="e" type="m">smal</rhyme>,</l>
        <l id="s6:l12" met="4" type="+-">Reyn Liefd’ prijst boven <rhyme label="e" type="m" corresp="#s6:l11">al</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">De liefd’ <rhyme label="a" type="m">eerbaer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Sal nimmermeer <rhyme label="b" type="f">verkouden</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Noch oock niet <rhyme label="b" type="f" corresp="#s7:l2">verouden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Soo vreemde tonghen <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Soeckt niet het <rhyme label="a" type="m" corresp="#s7:l1">haer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Maer ‘t voordeel der <rhyme label="b" type="f" corresp="#s7:l2">benouden</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Goet doen haer niet <rhyme label="b" type="f" corresp="#s7:l2">rouden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Sy gaet het alles <rhyme label="c" type="m" corresp="#s7:l4">voen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Met haer alderbeste <rhyme label="d" type="f">gaven</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Sal sy ons int leste <rhyme label="d" type="f" corresp="#s7:l9">laven</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Alst eynden <rhyme label="e" type="m">sal</rhyme>,</l>
        <l id="s7:l12" met="4" type="+-">Reyn Liefd’ prijst boven <rhyme label="e" type="m" corresp="#s7:l11">al</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Reyn liefd blijft <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Als alle menschen <rhyme label="b" type="f">weten</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Lichtelik wort <rhyme label="b" type="f" corresp="#s8:l2">vergeten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hoe hoogh men daer van <rhyme label="c" type="m">roemt</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Kennis, <rhyme label="a" type="m" corresp="#s8:l1">verstaen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Proiphecy der <rhyme label="b" type="f" corresp="#s8:l2">Propheten</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Stuck-werck wort <rhyme label="b" type="f" corresp="#s8:l2">gheheeten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Daer dit volcomen <rhyme label="c" type="m" corresp="#s8:l4">comt</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Die liefde niet in ‘t herte <rhyme label="d" type="f">schrijven</rhyme>,</l>
        <l id="s8:l10" met="3" type="-+">Moeten al in smarte <rhyme label="d" type="f" corresp="#s8:l9">blyven</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">In <rhyme label="e" type="m">ongheval</rhyme>,</l>
        <l id="s8:l12" met="4" type="+-">Reyn Liefd’ prijst boven <rhyme label="e" type="m" corresp="#s8:l11">al</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Met desen <rhyme label="a" type="m">Prins</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Ghy Princen wilt <rhyme label="b" type="f">beginnen</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">In u hert en <rhyme label="b" type="f" corresp="#s9:l2">sinnen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ontsteeckt der liefden <rhyme label="c" type="m">brant</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Sijn gunst men <rhyme label="a" type="m" corresp="#s9:l1">vint</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Om hartelicke <rhyme label="b" type="f" corresp="#s9:l2">minne</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">Met deucht te <rhyme label="b" type="f" corresp="#s9:l2">verwinnen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Al syn sy u <rhyme label="c" type="m" corresp="#s9:l4">vyant</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Dan sal liefde verholen <rhyme label="d" type="f">baren</rhyme>,</l>
        <l id="s9:l10" met="3" type="-+">Op ‘t hooft vierighe kolen <rhyme label="d" type="f" corresp="#s9:l9">garen</rhyme>,</l>
        <l id="s9:l11" met="3" type="+-">Ist sonder <rhyme label="e" type="m">gal</rhyme>,</l>
        <l id="s9:l12" met="4" type="+-">Reyn Liefd’ prijst boven <rhyme label="e" type="m" corresp="#s9:l11">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
