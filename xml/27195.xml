  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Bedroefde <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Moet ghy nu <rhyme label="b" type="f">desoleren</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Door cranck <rhyme label="a" type="f" corresp="#s1:l1">gewinnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van troost geheel <rhyme label="b" type="f" corresp="#s1:l2">mineren</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Alle myn <rhyme label="c" type="m">melodye</rhyme></l>
        <l id="s1:l6" met="2" type="-+">En <rhyme label="d" type="f">jubileren</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Moet ick door <rhyme label="c" type="m" corresp="#s1:l5">fantazye</rhyme></l>
        <l id="s1:l8" met="2" type="-+"><rhyme label="d" type="f" corresp="#s1:l6">Abandoneren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Door <rhyme label="a" type="f">lamentaty</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Verdwynen al myn <rhyme label="b" type="f">leden</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Ten zy v <rhyme label="a" type="f" corresp="#s2:l1">graty</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Vuyt <rhyme label="b" type="f" corresp="#s2:l2">barmherticheden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Al mynen noet <rhyme label="c" type="m">aeniet</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Dies ick ter <rhyme label="d" type="f">steden</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Sonder occazy <rhyme label="c" type="m" corresp="#s2:l5">niet</rhyme></l>
        <l id="s2:l8" met="2" type="-+">En claech met <rhyme label="d" type="f" corresp="#s2:l6">reden</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Want v <rhyme label="a" type="f">absenty</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Brengt my lief in <rhyme label="b" type="f">dangiere</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Maer v <rhyme label="a" type="f" corresp="#s3:l1">presenty</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Holpt my seer <rhyme label="b" type="f" corresp="#s3:l2">goedertieren</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Dickmaels vuyt alle <rhyme label="c" type="m">noot</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Reyn <rhyme label="d" type="f">balsomiere</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Aensiet myn liefde <rhyme label="c" type="m" corresp="#s3:l5">groot</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Edel <rhyme label="d" type="f" corresp="#s3:l6">rosiere</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Door liefd'ick <rhyme label="a" type="f">vragen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Moet droeuich van <rhyme label="b" type="f">aenschyne</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Of ick al myn <rhyme label="a" type="f" corresp="#s4:l1">dagen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Sal verslyten In <rhyme label="b" type="f" corresp="#s4:l2">pyne</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Alderliefste ick <rhyme label="c" type="m">kendt</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Onweert te <rhyme label="d" type="f">zyne</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Al zulck een <rhyme label="c" type="m" corresp="#s4:l5">exelent</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Edel <rhyme label="d" type="f" corresp="#s4:l6">robyne</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Niet myn <rhyme label="a" type="f">onweerdt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Hop'ick sult ghy aen <rhyme label="b" type="f">schouwen</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Maer de <rhyme label="a" type="f" corresp="#s5:l1">expeerde</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Liefde zonder <rhyme label="b" type="f" corresp="#s5:l2">verflouwen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Alleen nemen <rhyme label="c" type="m">regaert</rhyme></l>
        <l id="s5:l6" met="2" type="-+">O fleur der <rhyme label="d" type="f">vrouwen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Helpt my och <rhyme label="c" type="m" corresp="#s5:l5">beswaert</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Hert wt <rhyme label="d" type="f" corresp="#s5:l6">benouwen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Draecht reyne <rhyme label="a" type="f">liefde</rhyme></l>
        <l id="s6:l2" met="3" type="-+">V dienaer sonder <rhyme label="b" type="f">verlaten</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Als de <rhyme label="a" type="f" corresp="#s6:l1">geriefde</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Compt my schoon lief te <rhyme label="b" type="f" corresp="#s6:l2">baten</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Biedt toch een mael <rhyme label="c" type="m">dehandt</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Hooge in <rhyme label="d" type="f">staten</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En blust der liefden <rhyme label="c" type="m" corresp="#s6:l5">brant</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Wt <rhyme label="d" type="f" corresp="#s6:l6">caritaten</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wyse <rhyme label="a" type="f">notabel</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Deuchsame maecht <rhyme label="b" type="f">verheuen</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Tot een <rhyme label="a" type="f" corresp="#s7:l1">morabel</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Gedachtenis wilt <rhyme label="b" type="f" corresp="#s7:l2">geuen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">V trouwe en <rhyme label="c" type="m">faueur</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Helpt hier <rhyme label="d" type="f">beneuen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">V dienaer vuyt <rhyme label="c" type="m" corresp="#s7:l5">getruer</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Schenckt hem het <rhyme label="d" type="f" corresp="#s7:l6">leuen</rhyme></l>
      </lg>
    </lg>
