  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">REpt mijn soete <rhyme label="a" type="f">Cameraetje</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Repje wat dese Kamer, <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Al de nacht lust die <rhyme label="a" type="f" corresp="#s1:l1">vergaetje</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die ghy niet in het bedd' <rhyme label="b" type="m" corresp="#s1:l2">besluyt</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Schenckt, voor 't laest', de Bruydtjes <rhyme label="c" type="f">tranen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Tapt die uyt het Paters <rhyme label="d" type="m">vat</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Maer niet uyt uw' <rhyme label="c" type="f" corresp="#s1:l5">ooge-kranen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ick deed 't niet om de <rhyme label="d" type="m" corresp="#s1:l6">schat</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Van de geheele <rhyme label="d" type="m" corresp="#s1:l6">Stadt</rhyme>.</l>
      </lg>
    </lg>
  </text>
