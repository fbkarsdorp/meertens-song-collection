<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35095</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1700</year-end>
    </date>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody>Allons tous à la crèche</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Rien, Iris, vous <rhyme label="a" type="f">surpasse</rhyme></l>
        <l id="s1:l2" met="3" type="-+">en beaute j'en <rhyme label="b" type="m">convien</rhyme></l>
        <l id="s1:l3" met="3" type="-+">mais a un coeur de <rhyme label="a" type="f" corresp="#s1:l1">glâce</rhyme></l>
        <l id="s1:l4" met="3" type="-+">la beaute sert de <rhyme label="b" type="m" corresp="#s1:l2">rien</rhyme></l>
        <l id="s1:l5" met="3" type="-+">un fleur dans le <rhyme label="c" type="m">printems</rhyme></l>
        <l id="s1:l6" met="3" type="-+">nous promet pour <rhyme label="d" type="f">l'autome</rhyme></l>
        <l id="s1:l7" met="4" type="-+">de fruit, ce qui son <rhyme label="c" type="m" corresp="#s1:l5">agrement</rhyme></l>
        <l id="s1:l8" met="4" type="-+">augmente, et devint plus <rhyme label="c" type="m" corresp="#s1:l5">charmant</rhyme></l>
        <l id="s1:l9" met="3" type="-+">par l'espoir qu'il nous <rhyme label="d" type="f" corresp="#s1:l6">donne</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Tout que la terre et <rhyme label="a" type="f">l'onde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">de rare peut <rhyme label="b" type="m">donner</rhyme></l>
        <l id="s2:l3" met="3" type="-+">touts les tresors du <rhyme label="a" type="f" corresp="#s2:l1">monde</rhyme></l>
        <l id="s2:l4" met="3" type="-+">si seul, vous <rhyme label="b" type="m" corresp="#s2:l2">possedez</rhyme></l>
        <l id="s2:l5" met="3" type="-+">tout sert autant que <rhyme label="c" type="m">rien</rhyme></l>
        <l id="s2:l6" met="3" type="-+">en nouant bas <rhyme label="d" type="f">l'usage</rhyme></l>
        <l id="s2:l7" met="4" type="-+">lors que l'on ne peut pas son <rhyme label="c" type="m" corresp="#s2:l5">bien</rhyme></l>
        <l id="s2:l8" met="4" type="-+">communiquer avec le <rhyme label="c" type="m" corresp="#s2:l5">siens</rhyme></l>
        <l id="s2:l9" met="3" type="-+">cest un <rhyme label="d" type="f" corresp="#s2:l6">desavantage</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">si les dieux sur la <rhyme label="a" type="f">terre</rhyme></l>
        <l id="s3:l2" met="3" type="-+">ne versoient pas leurs <rhyme label="b" type="m">dons</rhyme></l>
        <l id="s3:l3" met="3" type="-+">on s'empresseroit <rhyme label="a" type="f" corresp="#s3:l1">guerre</rhyme></l>
        <l id="s3:l4" met="3" type="-+">de celebrer leurs <rhyme label="b" type="m" corresp="#s3:l2">noms</rhyme></l>
        <l id="s3:l5" met="3" type="-+">si leurs <rhyme label="c" type="m">divinitez</rhyme></l>
        <l id="s3:l6" met="3" type="-+">n' addoucoient nos <rhyme label="d" type="f">souffrances</rhyme></l>
        <l id="s3:l7" met="4" type="-+">ils seront guerre <rhyme label="c" type="m" corresp="#s3:l5">reveres</rhyme></l>
        <l id="s3:l8" met="4" type="-+">si lon n'attend des leurs <rhyme label="c" type="m" corresp="#s3:l5">bonte</rhyme></l>
        <l id="s3:l9" met="3" type="-+">les bonnes <rhyme label="d" type="f" corresp="#s3:l6">Influences</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Iris qui par vos <rhyme label="a" type="f">charmes</rhyme></l>
        <l id="s4:l2" met="3" type="-+">de pres les <rhyme label="b" type="m">approcher</rhyme></l>
        <l id="s4:l3" met="3" type="-+">vins calmer les <rhyme label="a" type="f" corresp="#s4:l1">allarmes</rhyme></l>
        <l id="s4:l4" met="3" type="-+">que tu as <rhyme label="b" type="m" corresp="#s4:l2">excité</rhyme></l>
        <l id="s4:l5" met="3" type="-+">vous avez leurs <rhyme label="c" type="m">beauté</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Iris qui vous <rhyme label="d" type="f">empeche</rhyme></l>
        <l id="s4:l7" met="4" type="-+">pour ce coeur que vous <rhyme label="c" type="m" corresp="#s4:l5">possedez</rhyme></l>
        <l id="s4:l8" met="4" type="-+">de vous servir de la <rhyme label="c" type="m" corresp="#s4:l5">bonté</rhyme></l>
        <l id="s4:l9" met="3" type="-+">des Dieux et des <rhyme label="d" type="f" corresp="#s4:l6">Déesses</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
