<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3102</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kluchtlied" translation="humorous song">kluchtlied</tag>
    </tags>
    <melody>Vrouwtje is uw man niet thuis</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HEy! wat ister al te <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Met de <rhyme label="b" type="f">Vastel-avondt</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Yder die wil Cluchjens <rhyme label="a" type="m" corresp="#s1:l1">broen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Just van desen <rhyme label="b" type="f" corresp="#s1:l2">Naevont</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">d' Eenen lacht, d' ander <rhyme label="c" type="m">singht</rhyme>:</l>
        <l id="s1:l6" met="3" type="+-">Dees wil Turfjes <rhyme label="d" type="f">rapen'</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Al 't Gepeupel danst en <rhyme label="c" type="m" corresp="#s1:l5">springht</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Met geschreuw en <rhyme label="d" type="f" corresp="#s1:l6">gapen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Teeuwis Tijssen onse <rhyme label="a" type="m">Reus</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Sal de Bosseu <rhyme label="b" type="f">spelen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want hy op sijn kromme <rhyme label="a" type="m" corresp="#s2:l1">Neus</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Kan soo aerdigh <rhyme label="b" type="f" corresp="#s2:l2">quelen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Trots een Orgel of een <rhyme label="c" type="m">Fluyt</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">En ons Jochem <rhyme label="d" type="f">Janssen</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Sal de soete vuyle <rhyme label="c" type="m" corresp="#s2:l5">Bruydt</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Onder 't spelen <rhyme label="d" type="f" corresp="#s2:l6">danssen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Goris doet de Grijns niet <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Om u te <rhyme label="b" type="f">vermommen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Want u Trony (als een <rhyme label="a" type="m" corresp="#s3:l1">Haen</rhyme>)</l>
        <l id="s3:l4" met="3" type="+-">Sal soo vremt niet <rhyme label="b" type="f" corresp="#s3:l2">kommen</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Blauwe ooghen die wat <rhyme label="c" type="m">scheel</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">'t Aensicht vol <rhyme label="d" type="f">Karboncken</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">'t Montjen van een <rhyme label="c" type="m" corresp="#s3:l5">vierendeel</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Sal wel geestigh <rhyme label="d" type="f" corresp="#s3:l6">proncken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Haeghse Nimphjes komt te <rhyme label="a" type="m">baen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Wilt u lust <rhyme label="b" type="f">versaden</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Treckt malkanders Kleederen <rhyme label="a" type="m" corresp="#s4:l1">aen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Gaet uyt <rhyme label="b" type="f" corresp="#s4:l2">Mascuraden</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Na de Moden op sen <rhyme label="c" type="m">Franss</rhyme>/</l>
        <l id="s4:l6" met="3" type="+-">En ghy lichte <rhyme label="d" type="f">Doeckjes</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Danst den soeten <rhyme label="c" type="m" corresp="#s4:l5">Wolven-dans</rhyme></l>
        <l id="s4:l8" met="3" type="+-">In de duyster <rhyme label="d" type="f" corresp="#s4:l6">hoeckjes</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Meysjes, tsa de Pan te <rhyme label="a" type="m">vyer</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Leeght u <rhyme label="b" type="f">Bloeme-sacken</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Haelt een kruyckje <rhyme label="a" type="m" corresp="#s5:l1">smockel-bier</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">'t Wil u Koeckjes <rhyme label="b" type="f" corresp="#s5:l2">backen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Nooter u beminde <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Set hem op een <rhyme label="d" type="f">Stoeltje</rhyme>:</l>
        <l id="s5:l7" met="4" type="+-">Of gaet schong'len op een <rhyme label="c" type="m" corresp="#s5:l5">Schop</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Met u liefste <rhyme label="d" type="f" corresp="#s5:l6">Boeltje</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maeckt de noorden wint <rhyme label="a" type="m">gedruys</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Gaet by 't Veene <rhyme label="b" type="f">Sonnetje</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Isser niet een stoel in <rhyme label="a" type="m" corresp="#s6:l1">Huys</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Sit soo langh op 't <rhyme label="b" type="f" corresp="#s6:l2">Tonnetje</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Koopter Ocker-neutjes <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Of lust jou een <rhyme label="d" type="f">praetie</rhyme>?</l>
        <l id="s6:l7" met="4" type="+-">Neemt je soetert aen je <rhyme label="c" type="m" corresp="#s6:l5">sy</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Soo wert hy u <rhyme label="d" type="f" corresp="#s6:l6">kaertje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Klapper-luyden of je <rhyme label="a" type="m">sagh</rhyme></l>
        <l id="s7:l2" met="3" type="+-">In de Stoep een <rhyme label="b" type="f">wonder</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dat den eenen boven <rhyme label="a" type="m" corresp="#s7:l1">lagh</rhyme></l>
        <l id="s7:l4" met="3" type="+-">En den ander <rhyme label="b" type="f" corresp="#s7:l2">onder</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Denckt dat het soo wesen <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Gaet maer uwer <rhyme label="d" type="f">straten</rhyme></l>
        <l id="s7:l7" met="4" type="+-">Want men moet het Meysje <rhyme label="c" type="m" corresp="#s7:l5">soet</rhyme></l>
        <l id="s7:l8" met="3" type="+-">In het huysje <rhyme label="d" type="f" corresp="#s7:l6">laten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
