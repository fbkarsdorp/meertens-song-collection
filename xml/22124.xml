    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>O Herr wer wird Wohnungen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SAlich is hy die dArmen <rhyme label="a" type="m">altijt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Bewijst goede <rhyme label="b" type="f">weldaden</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Die sal verlost worden in quaden <rhyme label="a" type="m" corresp="#s1:l1">tijt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Door Godt van alle <rhyme label="b" type="f" corresp="#s1:l2">quaden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sijn leuen sal dan behouden <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Op daerd salt hem wel gaen: noch in <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Vyandts handt en salt hy hem <rhyme label="d" type="f">gheuen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Heer sal hem bequicken <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Als hy sieck leyt in <rhyme label="b" type="f">smerten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En zijne rustplaets omkeerende <rhyme label="a" type="m" corresp="#s2:l1">zijn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als hy cranck is van <rhyme label="b" type="f" corresp="#s2:l2">herten</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Ick seyde: Heer ontfermt u <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Gheneest mijn Siele, want ick heb <rhyme label="c" type="m" corresp="#s2:l5">vry</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Teghen u swaerlijcken <rhyme label="d" type="f">ghesondicht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn vyanden veel quaets alle <rhyme label="a" type="m">glijck</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Segghen: wanneer sal hy <rhyme label="b" type="f">steruen</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">En synen name <rhyme label="a" type="m" corresp="#s3:l1">eewelijck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met hem gheheel <rhyme label="b" type="f" corresp="#s3:l2">verderuen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Sy commen wel om te besien <rhyme label="c" type="m">yet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">(Maer hun hert dat en meynes <rhyme label="c" type="m" corresp="#s3:l5">niet</rhyme>)</l>
        <l id="s3:l7" met="3" type="-+">Ten is maer om wat te <rhyme label="d" type="f">berispen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy ginghen wt en verbreyden <rhyme label="a" type="m">dat</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">En alle die my <rhyme label="b" type="f">haten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Soecken teghen my een oorsaeck <rhyme label="a" type="m" corresp="#s4:l1">pladt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om my eens te moghen <rhyme label="b" type="f" corresp="#s4:l2">vaten</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Sy raetslaghen veel quaets teghen <rhyme label="c" type="m">my</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Ja hebben alree een stucxken <rhyme label="c" type="m" corresp="#s4:l5">vry</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ouer my schoon <rhyme label="d" type="f">besloten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Als hy leyt, en sal hy niet wedrom <rhyme label="a" type="m">opstaen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Mijn vrindt dien ick <rhyme label="b" type="f">betroude</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die mijn broodt adt, my nu (wel <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme>)</l>
        <l id="s5:l4" met="3" type="-+">Gheren met voeten treden <rhyme label="b" type="f" corresp="#s5:l2">soude</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Maer O Heer Godt onfermt u <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Helpt my doch op, en staet my <rhyme label="c" type="m" corresp="#s5:l5">by</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Soo wil ickse <rhyme label="d" type="f">betalen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Daer aen merckten ick dat ghy <rhyme label="a" type="m">haet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Aen my een goet <rhyme label="b" type="f">behaghen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Midts dat ghy ouer my niet en <rhyme label="a" type="m" corresp="#s6:l1">laet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Mijnen vyandt blijschap <rhyme label="b" type="f" corresp="#s6:l2">draghen</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Maer ghy hebt my O Heer mijn <rhyme label="c" type="m">Godt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Om mijn onschult oock inden <rhyme label="c" type="m" corresp="#s6:l5">noodt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Verlost, ende <rhyme label="d" type="f">onderhouden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ghy hebt oock voor u aensicht <rhyme label="a" type="m">recht</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">My ghestelt eewelijck <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Gheloeft sy Godt die my wedrom <rhyme label="a" type="m" corresp="#s7:l1">oprecht</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En verkiest mijn leuen <rhyme label="b" type="f" corresp="#s7:l2">teere</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Om te prijsen zijne <rhyme label="c" type="m">bermherticheyt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Van nu af tot inder <rhyme label="c" type="m" corresp="#s7:l5">eewicheyt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Door Jhesum Christum, <rhyme label="d" type="f">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
