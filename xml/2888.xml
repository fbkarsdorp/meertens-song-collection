  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een eerbaer schoon <rhyme label="a" type="f">Jonckvrouwen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Durwont het hardt <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">haer minnelijck <rhyme label="a" type="f" corresp="#s1:l1">aenschouwen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Lycht als de Sonne <rhyme label="b" type="m" corresp="#s1:l2">claer</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Dor haer gesycht, min hardt <rhyme label="c" type="m">verlycht</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ghelijck Den <rhyme label="d" type="m">Dachraet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Tys De Schonste <rhyme label="a" type="f" corresp="#s1:l1">cercouwe</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dye Inden hove <rhyme label="d" type="m" corresp="#s1:l6">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghy hebt mijn Ghest van <rhyme label="a" type="f">binnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">met venis stralen <rhyme label="b" type="m">Dorwont</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Goch wartiat ghy te <rhyme label="a" type="f" corresp="#s2:l1">winnen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">ghelick apolo <rhyme label="b" type="m" corresp="#s2:l2">stont</rhyme></l>
        <l id="s2:l5" met="4" type="-+">met clock bedrijf, Soudt Ick <rhyme label="c" type="m">mijn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Stellen In avon <rhyme label="d" type="m">tuer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">want Ick moet u <rhyme label="a" type="f" corresp="#s2:l1">beminnen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">wt reinder lyefden <rhyme label="d" type="m" corresp="#s2:l6">puer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">vleys en bloedt Soudyck <rhyme label="a" type="f">waghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">om u rein rosken <rhyme label="b" type="m">rooet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Al blef Ick dan <rhyme label="a" type="f" corresp="#s3:l1">verslaghen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ghelijck trozyblus <rhyme label="b" type="m" corresp="#s3:l2">Dooedt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Ick waer bevrijt al om Dat <rhyme label="c" type="m">fayt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ghewyllijch te <rhyme label="d" type="m">bestaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">een wylt tot ghenen <rhyme label="a" type="f" corresp="#s3:l1">Daghen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">uwen Dyenaer <rhyme label="d" type="m" corresp="#s3:l6">versmaen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Eedel prinses <rhyme label="a" type="f">Ghepresen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dye met minnen Sijt <rhyme label="b" type="m">versamt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Laet mijn u Dyenaer <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Want het Sooe wel <rhyme label="b" type="m" corresp="#s4:l2">betamt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Dyckmael Ghemen, gae Ick <rhyme label="c" type="m">allen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">met Sinnen seer <rhyme label="d" type="m">bedrouft</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wylt mijn Schoen lyef <rhyme label="a" type="f" corresp="#s4:l1">ghenesen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Want mijn hardt trost <rhyme label="d" type="m" corresp="#s4:l6">behoft</rhyme></l>
      </lg>
    </lg>
  </text>
