<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190481</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Het kuipertje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Een troepje Hondjes uit de <rhyme label="a" type="m">buurt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Meest al waaren 't zwart en <rhyme label="b" type="f">witten</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Met een Apenrok <rhyme label="a" type="m" corresp="#s1:l1">gehuurt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zouden eens op Schildwagt <rhyme label="b" type="f" corresp="#s1:l2">zitten</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Met een stokje in heur <rhyme label="c" type="m">poot</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">By een vuile <rhyme label="c" type="m" corresp="#s1:l5">Kikkersloot</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Zy basten en keften gelyk een <rhyme label="d" type="m">Kees</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Vol vuur en moed en zonder <rhyme label="d" type="m" corresp="#s1:l7">vrees</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wel te vrede in Govaards <rhyme label="a" type="m">wout</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Weiden veel Oranje <rhyme label="b" type="f">Harten</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dog de Keezen fier en <rhyme label="a" type="m" corresp="#s2:l1">stout</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Kwaamen hun geduurig <rhyme label="b" type="f" corresp="#s2:l2">tarten</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dreigden zelf hun dood te <rhyme label="c" type="m">slaan</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Met drie Witte <rhyme label="c" type="m" corresp="#s2:l5">Lelyblaan</rhyme>;</l>
        <l id="s2:l7" met="4" type="-+">Zy baften en keften gelyk een <rhyme label="d" type="m">Kees</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">En toonden schier voor niemand <rhyme label="d" type="m" corresp="#s2:l7">vrees</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een oude gryze tamme <rhyme label="a" type="m">Leeuw</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sperd zyn muil en schud zyn <rhyme label="b" type="f">maanen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Op dat wild en woest <rhyme label="a" type="m" corresp="#s3:l1">geschreeuw</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Doch zyn kragt was reeds aan 't <rhyme label="b" type="f" corresp="#s3:l2">taanen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Want hy was een ruimen <rhyme label="c" type="m">tyd</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Zyn getrouwe Leidsman <rhyme label="c" type="m" corresp="#s3:l5">kwyd</rhyme>!</l>
        <l id="s3:l7" met="4" type="-+">Dus tarten zy Harten en nog daar <rhyme label="d" type="m">by</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Bragten den ouden Leeuw in <rhyme label="d" type="m" corresp="#s3:l7">ly</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een dubb'len Arend snel van <rhyme label="a" type="m">vlucht</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Spits van snavel, fors van <rhyme label="b" type="f">pooten</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Kliefde onverwagts de <rhyme label="a" type="m" corresp="#s4:l1">Lucht</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Kwam die Keezen weg te <rhyme label="b" type="f" corresp="#s4:l2">stooten</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Die toen byna dood van <rhyme label="c" type="m">schrik</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Vlooden in een <rhyme label="c" type="m" corresp="#s4:l5">oogenblik</rhyme>!</l>
        <l id="s4:l7" met="4" type="-+">Zy liepen al piepende een voor <rhyme label="d" type="m">een</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Met heur staaretjes druipend <rhyme label="d" type="m" corresp="#s4:l7">heen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Toen was 't vreugd in <rhyme label="a" type="m">Leeuwendaal</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Vreugd by all' Oranje <rhyme label="b" type="f">Harten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Vreugd in Vryheids <rhyme label="a" type="m" corresp="#s5:l1">Tempelzaal</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Vreugd verdreef toen alle <rhyme label="b" type="f" corresp="#s5:l2">smarten</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'t Leeuwtje kreeg zyn Leidsman <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Den Adelaar al√∂m de <rhyme label="c" type="m" corresp="#s5:l5">Eer</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En zugtend en vlugtende zonder <rhyme label="d" type="m">stok</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Kroopen de Keezen in heur <rhyme label="d" type="m" corresp="#s5:l7">hok</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
