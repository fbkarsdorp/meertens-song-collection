<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8925</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>O Herr Gott begnade mich</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEER van den boosen Menschen <rhyme label="a" type="m">my</rhyme></l>
        <l id="s1:l2" met="4" type="-+">End van den Wreeff'ligen <rhyme label="a" type="m" corresp="#s1:l1">bevry</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die Boosheyt in haer <rhyme label="b" type="f">Herte</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghedencken t'mijner <rhyme label="b" type="f" corresp="#s1:l3">smerte</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Daeg'lijcx sy krijch Aenvaerden <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Haer Tong' en Lippen syn so <rhyme label="c" type="m" corresp="#s1:l5">daen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Als Slanghen vol <rhyme label="d" type="f">Fenijne</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">End Adren fel ter <rhyme label="d" type="f" corresp="#s1:l7">pijne</rhyme>.</l>
        <l id="s1:l9" met="4" type="-+">Bewaert my (Heer) voor s'Boosen <rhyme label="e" type="m">handt</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Voor Wreeff'ligen doet my <rhyme label="e" type="m" corresp="#s1:l9">bystandt</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Die my (als ick nae dijne <rhyme label="f" type="m">Leer</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Wandle) omstooten willen <rhyme label="f" type="m" corresp="#s1:l11">Neer</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">End legghen my haer <rhyme label="g" type="f">stricke</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die Hoovaerdig' van sinn' en <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Breyden my Listich Netten <rhyme label="a" type="m" corresp="#s2:l1">quaet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Stellen my al <rhyme label="b" type="f">Enteeghen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Haer vallen aen mijn <rhyme label="b" type="f" corresp="#s2:l3">Weghen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Maer ick seg' tot den Heer met <rhyme label="c" type="m">Claeg</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Ghy zijt mijn Godt, op dien ickt <rhyme label="c" type="m" corresp="#s2:l5">waeg'</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Aenmerckt mijn stemme <rhyme label="d" type="f">Clachtich</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Heer ghy mijn hulpe <rhyme label="d" type="f" corresp="#s2:l7">machtich</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">Mijn Hooft beschermt ghy in den <rhyme label="e" type="m">strijt</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Heer laet den Boos' syn Wille <rhyme label="e" type="m" corresp="#s2:l9">niet</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Sterct syn Moetwil doch geensins <rhyme label="f" type="m">Heer</rhyme>,</l>
        <l id="s2:l12" met="4" type="-+">Hy mocht hem dies verheffen <rhyme label="f" type="m" corresp="#s2:l11">meer</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">End dijn Goetheydt <rhyme label="g" type="f">misbruycken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">D'ongluck dat mijn Vyanden <rhyme label="a" type="m">fel</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Raedtslaghen, t'selve moet al <rhyme label="a" type="m" corresp="#s3:l1">snel</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Op haer Kop neder <rhyme label="b" type="f">dalen</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">De Heer sal syne <rhyme label="b" type="f" corresp="#s3:l3">Stralen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Over haer schudden met <rhyme label="c" type="m">Blixem</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">End laten gaen syns Donders <rhyme label="c" type="m" corresp="#s3:l5">stem</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Slaense sy diep in die <rhyme label="d" type="f">Aerde</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Met vuer schricklijck <rhyme label="d" type="f" corresp="#s3:l7">vervaerde</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">So diep dat sy t'Eenmael <rhyme label="e" type="m">vergaen</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">En nimmermeer moghen op <rhyme label="e" type="m" corresp="#s3:l9">staen</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Tot haren Staet end <rhyme label="f" type="m">Hovaerdy</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">End maecken boosachtich <rhyme label="f" type="m" corresp="#s3:l11">Party</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">Maer blijven heel <rhyme label="g" type="f">verloren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Een boosachtich Mont geen <rhyme label="a" type="m">geluck</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Op Aerd' sal hebben in syn <rhyme label="a" type="m" corresp="#s4:l1">stuck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">End een Moetwillich <rhyme label="b" type="f">Mensche</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Boosachtich in syn <rhyme label="b" type="f" corresp="#s4:l3">Wensche</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Verjaecht sal worden, end <rhyme label="c" type="m">verstoort</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Want ick weet, dat de HEEr wt <rhyme label="c" type="m" corresp="#s4:l5">voert</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Des Armen saeck nae <rhyme label="d" type="f">Rechte</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Helpt die Elenden <rhyme label="d" type="f" corresp="#s4:l7">slechte</rhyme>.</l>
        <l id="s4:l9" met="4" type="-+">Het sullen (Heer) die Vromen <rhyme label="e" type="m">fijn</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Dancken vrolijck den Naemen <rhyme label="e" type="m" corresp="#s4:l9">dijn</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Die Vromen voor dijn Aensicht <rhyme label="f" type="m">bly</rhyme>,</l>
        <l id="s4:l12" met="4" type="-+">Sullen van Sond' en Doode <rhyme label="f" type="m" corresp="#s4:l11">vry</rhyme>,</l>
        <l id="s4:l13" met="3" type="-+">(HEER) Eewich blijven, <rhyme label="g" type="f">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
