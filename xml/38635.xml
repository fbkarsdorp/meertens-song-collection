    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Savons was ick t'huys <rhyme label="a" type="f">ghekomen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">En heb wat ter handt <rhyme label="a" type="f" corresp="#s1:l1">ghenomen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">'K schreef, dat seer in mijnen <rhyme label="b" type="m">sin</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Had gheleghen van de <rhyme label="b" type="m" corresp="#s1:l3">min</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Ick en kon niet langher <rhyme label="c" type="f">swijghen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Waer sal ick een vrijster <rhyme label="c" type="f" corresp="#s1:l5">krijghen</rhyme>?</l>
        <l id="s1:l7" met="4" type="+-">Riep ick tot het Venus <rhyme label="d" type="m">kindt</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Daer de liefde is soo <rhyme label="d" type="m" corresp="#s1:l7">blindt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Seker Iouffrouw, die voor <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">My veel jonsten had <rhyme label="a" type="f" corresp="#s2:l1">bewesen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Soo het scheen (tot mijn <rhyme label="b" type="m">faveur</rhyme>)</l>
        <l id="s2:l4" met="4" type="+-">Was ick haren <rhyme label="b" type="m" corresp="#s2:l3">serviteur</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Hoe kan liefde soo <rhyme label="c" type="f">verand'ren</rhyme>!</l>
        <l id="s2:l6" met="4" type="+-">Want soo haest daer eenen <rhyme label="c" type="f" corresp="#s2:l5">and'ren</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Was ghekomen (tot mijn <rhyme label="d" type="m">leet</rhyme>)</l>
        <l id="s2:l8" met="4" type="+-">Gaf sy my een slecht <rhyme label="d" type="m" corresp="#s2:l7">bescheet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wel ick moet my eens <rhyme label="a" type="f">bedencken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Waer toe sy my placht te <rhyme label="a" type="f" corresp="#s3:l1">wencken</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En stets aen haer door te <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Tot dat ick quam <rhyme label="b" type="m" corresp="#s3:l3">aenghegaen</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Loncken, luymen door de <rhyme label="c" type="f">tralie</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Nu te kercken met een <rhyme label="c" type="f" corresp="#s3:l5">falie</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">'Tscheen sy was tot my <rhyme label="d" type="m">ghesint</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Maer vervloghen als den <rhyme label="d" type="m" corresp="#s3:l7">wint</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
