    <tags/>
    <melody>Psalm 061 Datheen</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wil, o God, myn schreijen <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Heb doch <rhyme label="a" type="f" corresp="#s1:l1">ooren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Merk eens op myn droeve <rhyme label="b" type="m">bee</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">'k Roep tot u, in myn <rhyme label="c" type="f">ellende</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Van 's lands <rhyme label="c" type="f" corresp="#s1:l4">ende</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wyl myn hart bestelpt van <rhyme label="b" type="m" corresp="#s1:l3">wee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Gy leidt my, door veele <rhyme label="a" type="f">droogten</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Tot de <rhyme label="a" type="f" corresp="#s2:l1">hoogten</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van een rotssteen, my te <rhyme label="b" type="m">steil</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Gy, myn toevluchts sterke <rhyme label="c" type="f">tooren</rhyme>.</l>
        <l id="s2:l5" met="2" type="+-">Waart te <rhyme label="c" type="f" corresp="#s2:l4">vooren</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Wel, voor 's vyands oog, myn <rhyme label="b" type="m" corresp="#s2:l3">heil</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dat uw heil my weer <rhyme label="a" type="f">beschutte</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">En uw <rhyme label="a" type="f" corresp="#s3:l1">hutte</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Eeuwig my een wooning <rhyme label="b" type="m">strek</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Brey uw vleugels, my ten <rhyme label="c" type="f">goede</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Tot myn <rhyme label="c" type="f" corresp="#s3:l4">hoede</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Toevlucht, en geheim <rhyme label="b" type="m" corresp="#s3:l3">vertrek</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Want, o God, gy hebt <rhyme label="a" type="f">alreede</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Myne <rhyme label="a" type="f" corresp="#s4:l1">beede</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En beloftenis <rhyme label="b" type="m">gehoort</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">'t Erf, voor die uw eernaam <rhyme label="c" type="f">preesen</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">En u <rhyme label="c" type="f" corresp="#s4:l4">vreesen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Gaaftge my al naa uw <rhyme label="b" type="m" corresp="#s4:l3">woord</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gy doet by des koonings <rhyme label="a" type="f">daagen</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Met <rhyme label="a" type="f" corresp="#s5:l1">behaagen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Nieuwe daagen, vol van <rhyme label="b" type="m">kracht</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Uwe goedheit zal syn <rhyme label="c" type="f">jaaren</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Op doen <rhyme label="c" type="f" corresp="#s5:l4">klaaren</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Van geslacht, tot in <rhyme label="b" type="m" corresp="#s5:l3">geslacht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hy zal eeuwig, voor Gods <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Met <rhyme label="a" type="f" corresp="#s6:l1">vermoogen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Sitten heerschen, en <rhyme label="b" type="m">bestaan</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Schik slechs waarheit, en <rhyme label="c" type="f">genaade</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Hem te <rhyme label="c" type="f" corresp="#s6:l4">staade</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Laat die steeds hem gaade <rhyme label="b" type="m" corresp="#s6:l3">slaan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dan zo sing ik eeuwig, <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">U ter <rhyme label="a" type="f" corresp="#s7:l1">eere</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Psalmen, met uw naams <rhyme label="b" type="m">verhaal</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Op dat ik, na uw <rhyme label="c" type="f">behaagen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Alle <rhyme label="c" type="f" corresp="#s7:l4">daagen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Myn geloften bly <rhyme label="b" type="m" corresp="#s7:l3">betaal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
