<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3876</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Ik weet een vrouwtje wel bereid</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O God lof moet v altijt <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Al bouen der engelen <rhyme label="b" type="m">choor</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Al met die lieue moeder <rhyme label="a" type="m" corresp="#s1:l1">dijn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ende dinen heyligen <rhyme label="b" type="m" corresp="#s1:l2">confessoor</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Die daer is waerlijck <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s1:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronyme</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s1:l5">noot</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hi is een sterre gheleken <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die doer den neuel <rhyme label="b" type="m">schijnt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Want hi verwan die ketters <rhyme label="a" type="m" corresp="#s2:l1">fel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die daer seer waren <rhyme label="b" type="m" corresp="#s2:l2">ghepijnt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Om sijnder leeringen <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s2:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s2:l5">noot</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hi heeft verlicht heel <rhyme label="a" type="m">kerstenheyt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Alsmen daer wel op <rhyme label="b" type="m">let</rhyme></l>
        <l id="s3:l3" met="4" type="-+">En de scrifture daert al aen <rhyme label="a" type="m" corresp="#s3:l1">leit</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Heeft hi claer ouer <rhyme label="b" type="m" corresp="#s3:l2">gheset</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Met swaren arbeyt <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s3:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s3:l5">noot</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sijnder duecht hadde dien leeu te <rhyme label="a" type="m">bat</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die hem in den doerne hadde <rhyme label="b" type="m">ghewont</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ende quam gheloopen daer hi <rhyme label="a" type="m" corresp="#s4:l1">sat</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Op dyen seluen <rhyme label="b" type="m" corresp="#s4:l2">stont</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ende toonde hem sinen <rhyme label="c" type="m">poot</rhyme>.</l>
        <l id="s4:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s4:l5">noot</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat hi so heylich <rhyme label="a" type="m">was</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat suldy wel <rhyme label="b" type="m">verstaen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Die leeu dyen hi den voet <rhyme label="a" type="m" corresp="#s5:l1">genas</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die was hem <rhyme label="b" type="m" corresp="#s5:l2">onderdaen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Met sinen arbeyt <rhyme label="c" type="m">groot</rhyme>.</l>
        <l id="s5:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s5:l5">noot</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De reynicheyt heeft hi <rhyme label="a" type="m">bemint</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dies hadden die sommige <rhyme label="b" type="m">nijt</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Die in haer sonden waren <rhyme label="a" type="m" corresp="#s6:l1">verblint</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ende bespotten hem met <rhyme label="b" type="m" corresp="#s6:l2">spijt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ter woestinen dat hi doen <rhyme label="c" type="m">vloot</rhyme></l>
        <l id="s6:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s6:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s6:l5">noot</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sijn heylicheit was daer <rhyme label="a" type="m">vermaert</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ende sijn penitencie <rhyme label="b" type="m">swaer</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Van daer track hy te Bethleem <rhyme label="a" type="m" corresp="#s7:l1">waert</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Een clooster stichte hi <rhyme label="b" type="m" corresp="#s7:l2">daer</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Daer woonde hi tot synder <rhyme label="c" type="m">doot</rhyme>.</l>
        <l id="s7:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s7:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s7:l5">noot</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Doen hi was out bi hondert <rhyme label="a" type="m">iaer</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Quam hem een siecte <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Hi sprac tot sinen broeders <rhyme label="a" type="m" corresp="#s8:l1">claer</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Wilt mi ontcleeden <rhyme label="b" type="m" corresp="#s8:l2">gaen</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Ende legt mi op die aerde <rhyme label="c" type="m">bloot</rhyme>.</l>
        <l id="s8:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s8:l5">noot</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Aldaer hi op die aerde <rhyme label="a" type="m">lach</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Eer hi schiet van <rhyme label="b" type="m">aertrijck</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Ten hemel dat hi opwaert <rhyme label="a" type="m" corresp="#s9:l1">sach</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ende hi sprac <rhyme label="b" type="m" corresp="#s9:l2">minnelijck</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Och willecoem bitter <rhyme label="c" type="m">doot</rhyme></l>
        <l id="s9:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s9:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s9:l5">noot</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Als hi aensach de broeders <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Seer weenende so sprack <rhyme label="b" type="m">hi</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Mijn alder liefste <rhyme label="a" type="m" corresp="#s10:l1">kinderkijn</rhyme></l>
        <l id="s10:l4" met="3" type="-+">En weent doch niet op <rhyme label="b" type="m" corresp="#s10:l2">mi</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Bemint malcander totter <rhyme label="c" type="m">doot</rhyme>.</l>
        <l id="s10:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s10:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s10:l5">noot</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Laet ons hem dienen tot <rhyme label="a" type="m">aldertijt</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Ende bidden <rhyme label="b" type="m">ootmoedelijc</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Dat hi ons na dit <rhyme label="a" type="m" corresp="#s11:l1">ouerlijt</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Wil brenghen in <rhyme label="b" type="m" corresp="#s11:l2">hemelrijck</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Daer hi is waerlijck <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s11:l6" met="4" type="-+">O heylige vader <rhyme label="d" type="f">Hieronime</rhyme></l>
        <l id="s11:l7" met="4" type="+-">Staet ons bi in onser <rhyme label="c" type="m" corresp="#s11:l5">noot</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
