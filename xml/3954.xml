<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3954</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>christmas song</tag>
      <tag>narrative song</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Dat is wil dij van de waarheid horen zingen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ons naket eenen soeten <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Wij mogen al wel sijn <rhyme label="a" type="m" corresp="#s1:l1">verblijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Met herten ende met <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Ons is gheboren des seker <rhyme label="a" type="m" corresp="#s1:l1">sijt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Soe soeten <rhyme label="b" type="f" corresp="#s1:l3">ionghelinghe</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ons is geboren so cleynen <rhyme label="a" type="m">kint</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Van eender maecht een <rhyme label="a" type="m" corresp="#s2:l1">iongelinc</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Een sone is ons <rhyme label="b" type="f">ghegeuen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat is ons allen een salich <rhyme label="a" type="m" corresp="#s2:l1">dinc</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ende hem is eewelijck <rhyme label="b" type="f" corresp="#s2:l3">leuen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">So scier als hi geboren <rhyme label="a" type="m">was</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Wert hi gheleyt / alsoe ick <rhyme label="a" type="m" corresp="#s3:l1">las</rhyme></l>
        <l id="s3:l3" met="3" type="-+">In eender couder <rhyme label="b" type="f">cribben</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Want daer gheen ander wiege en <rhyme label="a" type="m" corresp="#s3:l1">was</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Daer hi in mocht <rhyme label="b" type="f" corresp="#s3:l3">liggen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Op wat hoys wert hi <rhyme label="a" type="m">geleet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Iosephs coussen sijn eerste <rhyme label="a" type="m" corresp="#s4:l1">cleet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Daer wert hi in <rhyme label="b" type="f">ghewonden</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Noyt en was meerder <rhyme label="a" type="m" corresp="#s4:l1">ootmoedicheyt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">In deser werelt <rhyme label="b" type="f" corresp="#s4:l3">vonden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die beesten vielen op haer <rhyme label="a" type="m">knyen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Waer was dat wonder oyt meer <rhyme label="a" type="m" corresp="#s5:l1">ghesien</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Sij bekenden haren <rhyme label="b" type="f">heere</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Dat en sachmen noyt <rhyme label="a" type="m" corresp="#s5:l1">gheschien</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Het is ons een groote <rhyme label="b" type="f" corresp="#s5:l3">leere</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Het wert den herderkens <rhyme label="a" type="m">gheopenbaert</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Al van den enghel metter <rhyme label="a" type="m" corresp="#s6:l1">vaert</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Die hen die boetscap <rhyme label="b" type="f">brochte</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Sij liepen alle te Bethleem <rhyme label="a" type="m" corresp="#s6:l1">waert</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Daer sij dat kindeken <rhyme label="b" type="f" corresp="#s6:l3">sochten</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sij vonden alle die <rhyme label="a" type="m">waerheit</rhyme></l>
        <l id="s7:l2" met="4" type="-+">So hen die engel hadde <rhyme label="a" type="m" corresp="#s7:l1">geseyt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Vanden kinde / en vander <rhyme label="b" type="f">moeder</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Het is ons een grote <rhyme label="a" type="m" corresp="#s7:l1">weerdicheyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Dat hi is onse <rhyme label="b" type="f" corresp="#s7:l3">broeder</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die herden liepen alte <rhyme label="a" type="m">hand</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Si maecten meyre al om int <rhyme label="a" type="m" corresp="#s8:l1">lant</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dat Iesus waer <rhyme label="b" type="f">gheboren</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Waer ons kindeken niet <rhyme label="a" type="m" corresp="#s8:l1">ghesant</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Wij waren alle <rhyme label="b" type="f" corresp="#s8:l3">verloren</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Daer na al opten achsten <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Also ons Lucas doet <rhyme label="a" type="m" corresp="#s9:l1">ghewach</rhyme></l>
        <l id="s9:l3" met="3" type="-+">So wert dat kint <rhyme label="b" type="f">besneden</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Sijn suuer bloet men storten <rhyme label="a" type="m" corresp="#s9:l1">sach</rhyme></l>
        <l id="s9:l5" met="3" type="-+">In sine ionghe <rhyme label="b" type="f" corresp="#s9:l3">leden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Iesus wert dat kindeken <rhyme label="a" type="m">ionc</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Gheheeten op die selue <rhyme label="a" type="m" corresp="#s10:l1">stont</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Dies wij ons alle <rhyme label="b" type="f">verbliden</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Ende dragen in onser sielen <rhyme label="a" type="m" corresp="#s10:l1">gront</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Hem eewelijck tallen <rhyme label="b" type="f" corresp="#s10:l3">tiden</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Drie coningen quamen alsoe <rhyme label="a" type="m">verre</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Wt haren landen bi eender <rhyme label="a" type="m" corresp="#s11:l1">sterre</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Tot Bethleem inder <rhyme label="b" type="f">steden</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Sij offerden wieroock / gout / en <rhyme label="a" type="m" corresp="#s11:l1">myrrhe</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Tot sijnder <rhyme label="b" type="f" corresp="#s11:l3">weerdicheden</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Doen Herodes dat <rhyme label="a" type="m">vernam</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Wert hi toornich ende <rhyme label="a" type="m" corresp="#s12:l1">gram</rhyme></l>
        <l id="s12:l3" met="3" type="-+">In alle sine <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Hi dacht hoe hi dat ionge <rhyme label="a" type="m" corresp="#s12:l1">lam</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Al totter doot soude <rhyme label="b" type="f" corresp="#s12:l3">bringhen</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Herodes geboot den ridders <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Te doden al die <rhyme label="a" type="m" corresp="#s13:l1">kindekijn</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Die in dyen lande <rhyme label="b" type="f">waren</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Sijn herte was in groter <rhyme label="a" type="m" corresp="#s13:l1">pijn</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Hi en woude niemant <rhyme label="b" type="f" corresp="#s13:l3">sparen</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Ioseph doen met haesten <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Metten kinde ende metter moeder <rhyme label="a" type="m" corresp="#s14:l1">vloot</rhyme></l>
        <l id="s14:l3" met="3" type="-+">So hem die engel <rhyme label="b" type="f">syde</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Vliet in Egipten om den <rhyme label="a" type="m" corresp="#s14:l1">noot</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Al doer die langhe <rhyme label="b" type="f" corresp="#s14:l3">heyde</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Nu bidden wij dat kindeken <rhyme label="a" type="m">cleyn</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Ende Maria die suyuer maghet <rhyme label="a" type="m" corresp="#s15:l1">reyn</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Dat sij ons willen <rhyme label="b" type="f">beschermen</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Ende bi hem brengen ons al <rhyme label="a" type="m" corresp="#s15:l1">gemeyn</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Ende verlossen ons van alle <rhyme label="b" type="f" corresp="#s15:l3">kermen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
