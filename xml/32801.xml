<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>32801</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>De mei die komt ons bij zeer blij</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Weerde Coningin // die <rhyme label="a" type="m">in</rhyme></l>
        <l id="s1:l2" met="5" type="-+">V lichaem hebt omvadt den grooten <rhyme label="b" type="m">Al</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Den Vader van t'begin // vol <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Die was, die is, die eeuwich wesen <rhyme label="b" type="m" corresp="#s1:l2">sal</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Die al wat dat hier <rhyme label="c" type="m">leeft</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van u iet gheschaepen <rhyme label="c" type="m" corresp="#s1:l5">heeft</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Mensch, Vee, en dieren, het cruyt, op <rhyme label="d" type="m">t'velt</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Son, Sterren en Maen, die wonderlijck <rhyme label="e" type="m">gaen</rhyme>?</l>
        <l id="s1:l9" met="2" type="+-">Him paelen <rhyme label="d" type="m" corresp="#s1:l7">ghestelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Schoon suyver Danae // vol <rhyme label="a" type="m">vre</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Waer in den gulden regen is <rhyme label="b" type="m">ghedaelt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Die ons den schadt ontde // waer <rhyme label="a" type="m" corresp="#s2:l1">me</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Dat Adams schuldt volkomen is <rhyme label="b" type="m" corresp="#s2:l2">betaelt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Den Schadt het hooghste <rhyme label="c" type="m">goet</rhyme> //</l>
        <l id="s2:l6" met="3" type="-+">Dat onse sonden <rhyme label="c" type="m" corresp="#s2:l5">voet</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Door veel Propheten, belooft, <rhyme label="d" type="m">verwacht</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Die inne den noodt, door lijden en <rhyme label="e" type="m">doodt</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Ons salicheyt <rhyme label="d" type="m" corresp="#s2:l7">vraeght</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O wonderlijcke vrou // seer <rhyme label="a" type="m">trou</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Die aen Ioannem blinckend' is <rhyme label="b" type="m">vertoont</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Troosters in onsen rou // seer <rhyme label="a" type="m" corresp="#s3:l1">nou</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Wiens hooft met twelff Sterren was <rhyme label="b" type="m" corresp="#s3:l2">ghecroont</rhyme></l>
        <l id="s3:l5" met="3" type="-+">De sonne ongesalft // u lichaem had <rhyme label="c" type="m">omstraelt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">De Maen recht onder u voeten <rhyme label="c" type="m" corresp="#s3:l5">lach</rhyme></l>
        <l id="s3:l7" met="4" type="-+">O dagheraet claer // die brenghet <rhyme label="d" type="m">voorwaer</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Den eeuwighen <rhyme label="e" type="m">dagh</rhyme>.</l>
        <l id="s3:l9" met="2" type="+-">Deughtrijcke Abigael // die <rhyme label="d" type="m" corresp="#s3:l7">snel</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Versoent hebt Davids soon op ons <rhyme label="a" type="m">verstoort</rhyme>.</l>
        <l id="s4:l2" met="5" type="-+">Een Hester die 't bevel // seer <rhyme label="b" type="m">fel</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Te niet de van Assuerus aen <rhyme label="a" type="m" corresp="#s4:l1">hoort</rhyme>:</l>
        <l id="s4:l4" met="5" type="-+">Een Iudich die de <rhyme label="b" type="m" corresp="#s4:l2">macht</rhyme> //</l>
        <l id="s4:l5" met="3" type="-+">Des vyandts t' onder <rhyme label="c" type="m">bracht</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En met haeren voet den cop <rhyme label="c" type="m" corresp="#s4:l5">verpledt</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Die nijdich onvroet // ons salicheydts <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Langh hadde <rhyme label="e" type="m">beledt</rhyme>.</l>
        <l id="s4:l9" met="2" type="+-">Pincers die inden troon // seer <rhyme label="d" type="m" corresp="#s4:l7">schoon</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Van uwen Bruydegom sijt <rhyme label="a" type="m">in-gheleydt</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Daer ghy gheniet de croon // tot <rhyme label="b" type="m">loon</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Die van 't begin voor u was <rhyme label="a" type="m" corresp="#s5:l1">toe-bereydt</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Daer hem den Al met <rhyme label="b" type="m" corresp="#s5:l2">vreught</rhyme> //</l>
        <l id="s5:l5" met="3" type="-+">Ghestadich nu <rhyme label="c" type="m">verheught</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Den Enghelen singhen nu <rhyme label="c" type="m" corresp="#s5:l5">overluydt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Weest willecom <rhyme label="d" type="m">maeght</rhyme> //</l>
        <l id="s5:l8" met="4" type="+-">De Croone ghy <rhyme label="e" type="m">draeght</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Van Moeder, en <rhyme label="d" type="m" corresp="#s5:l7">Bruydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
