  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Die godheit vant der gracien <rhyme label="a" type="m">vont</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Van enen behoudere es ons nv <rhyme label="a" type="m" corresp="#s1:l1">cont</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Des hebbe god <rhyme label="b" type="m">danc</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Een maget sal ons een kindeken <rhyme label="c" type="f">bringen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Eer iet <rhyme label="b" type="m" corresp="#s1:l3">lanc</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ons boetscap es wel <rhyme label="a" type="m">gedaen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Her gabriel is wel <rhyme label="a" type="m" corresp="#s2:l1">ontfaen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Des hebbe god <rhyme label="b" type="m">danc</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Een maget sal ons een kindeken <rhyme label="c" type="f">bringen</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Eer iet <rhyme label="b" type="m" corresp="#s2:l3">lanc</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Des selen wy alle sijn <rhyme label="a" type="m">verblijt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Die coninc comt ons in corten <rhyme label="a" type="m" corresp="#s3:l1">tijt</rhyme></l>
        <l id="s3:l3" met="2" type="-+">geloeft sy <rhyme label="b" type="m">hy</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s3:l3">by</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Des sijn verblijt der ingelen <rhyme label="a" type="m">scaren</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Dat een maget sal cort <rhyme label="a" type="m" corresp="#s4:l1">gebaren</rhyme></l>
        <l id="s4:l3" met="2" type="-+">gelooft sy <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s4:l3">by</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Al gheet dese maget met kinde <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Sy es ombesorgt van alre <rhyme label="a" type="m" corresp="#s5:l1">noot</rhyme></l>
        <l id="s5:l3" met="2" type="-+">gelooft sy <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s5:l3">by</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dit kinderbedde moet hebben <rhyme label="a" type="m">lof</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Daer en es voirsien huys noch <rhyme label="a" type="m" corresp="#s6:l1">hof</rhyme></l>
        <l id="s6:l3" met="2" type="-+">gelooft sy <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s6:l3">by</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">In armoeden wil hy sijn <rhyme label="a" type="m">geboren</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Die alle rijcheit soude <rhyme label="a" type="m" corresp="#s7:l1">behooren</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Gelooft sy <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s7:l3">by</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Her iosep heeft een goet <rhyme label="a" type="m">geual</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Dat hy dit kindeken voesteren <rhyme label="a" type="m" corresp="#s8:l1">sal</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Ghelooft sy <rhyme label="b" type="m">hy</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s8:l3">by</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Her ihosop is ten dienst <rhyme label="a" type="m">bereet</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Sijn couse moet sijn des conincx <rhyme label="a" type="m" corresp="#s9:l1">cleet</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Gelooft sy <rhyme label="b" type="m">si</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s9:l3">by</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dese edele vrucht die moeten <rhyme label="a" type="m">louen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Metter moeder int rijcke <rhyme label="a" type="m" corresp="#s10:l1">daerbouen</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Gelooft sy <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Wy dancken v ihesus soete <rhyme label="c" type="f">vruecht</rhyme></l>
        <l id="s10:l5" met="2" type="-+">Die ons es <rhyme label="b" type="m" corresp="#s10:l3">by</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Die godheit vant der gracien <rhyme label="a" type="m">vont</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Van eenen behoudere  es ons nv <rhyme label="a" type="m" corresp="#s11:l1">cont</rhyme></l>
        <l id="s11:l3" met="2" type="-+">Des hebbe god <rhyme label="b" type="m">danc</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Een maget sal ons een kindeken <rhyme label="c" type="f">bringen</rhyme></l>
        <l id="s11:l5" met="2" type="-+">Eer iet <rhyme label="b" type="m" corresp="#s11:l3">lanc</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
