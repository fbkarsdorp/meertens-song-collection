  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Die doer gades <rhyme label="a" type="f">wyllen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">des mergens wyl meyen <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">syne ziele wort hem <rhyme label="c" type="f">gespyset</rhyme></l>
        <l id="s1:l4" met="3" type="-+">doer des hiligen gheestes <rhyme label="b" type="m" corresp="#s1:l2">raet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s1:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s1:l5">gasten</rhyme></l>
        <l id="s1:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s1:l6">syn</rhyme></l>
        <l id="s1:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s1:l5">gasten</rhyme></l>
        <l id="s1:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s1:l6">syn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die meye dien ic <rhyme label="a" type="f">meyne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">dat is die suete <rhyme label="b" type="m">god</rhyme></l>
        <l id="s2:l3" met="3" type="-+">hi gynck al op <rhyme label="c" type="f">eertryke</rhyme></l>
        <l id="s2:l4" met="3" type="-+">hi leet daer mennygen <rhyme label="b" type="m" corresp="#s2:l2">spot</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s2:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s2:l5">gasten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s2:l6">syn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s2:l5">gasten</rhyme></l>
        <l id="s2:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s2:l6">syn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die meye wyl die <rhyme label="a" type="f">meye</rhyme></l>
        <l id="s3:l2" met="3" type="-+">die gae an christus <rhyme label="b" type="m">hant</rhyme></l>
        <l id="s3:l3" met="3" type="-+">hi henck daer aen den <rhyme label="c" type="f">cruce</rhyme></l>
        <l id="s3:l4" met="3" type="-+">daer hem die mynne toe <rhyme label="b" type="m" corresp="#s3:l2">dwanck</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s3:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s3:l5">gasten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s3:l6">syn</rhyme></l>
        <l id="s3:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s3:l5">gasten</rhyme></l>
        <l id="s3:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s3:l6">syn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sie wy aen dat <rhyme label="a" type="f">cruce</rhyme></l>
        <l id="s4:l2" met="3" type="-+">hi henc daer naket ende <rhyme label="b" type="m">bloet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">mit bloede al auer <rhyme label="c" type="f">beronnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">dat hi doer ons <rhyme label="b" type="m" corresp="#s4:l2">wtgoet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s4:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s4:l5">gasten</rhyme></l>
        <l id="s4:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s4:l6">syn</rhyme></l>
        <l id="s4:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s4:l5">gasten</rhyme></l>
        <l id="s4:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s4:l6">syn</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sie wy aen syn <rhyme label="a" type="f">voeten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">sy syn mit nagelen <rhyme label="b" type="m">doerwont</rhyme></l>
        <l id="s5:l3" met="3" type="-+">an alle syne <rhyme label="c" type="f">liue</rhyme></l>
        <l id="s5:l4" met="3" type="-+">daer en was oec niet <rhyme label="b" type="m" corresp="#s5:l2">gesonts</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s5:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s5:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s5:l5">gasten</rhyme></l>
        <l id="s5:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s5:l6">syn</rhyme></l>
        <l id="s5:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s5:l5">gasten</rhyme></l>
        <l id="s5:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s5:l6">syn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sie wi aen syn <rhyme label="a" type="f">armen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">sy syn wyde <rhyme label="b" type="m">opgedaen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">hi wil ons arme <rhyme label="c" type="f">sundaren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">tot  sinre genaeden <rhyme label="b" type="m" corresp="#s6:l2">ontfaen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s6:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s6:l5">gasten</rhyme></l>
        <l id="s6:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s6:l6">syn</rhyme></l>
        <l id="s6:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s6:l5">gasten</rhyme></l>
        <l id="s6:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s6:l6">syn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Sie wi aen syn <rhyme label="a" type="f">hande</rhyme></l>
        <l id="s7:l2" met="3" type="-+">sy syn mit nagelen <rhyme label="b" type="m">doerslagen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">wi sullen christus <rhyme label="c" type="f">liden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">in onsen herten <rhyme label="b" type="m" corresp="#s7:l2">dragen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s7:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s7:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s7:l5">gasten</rhyme></l>
        <l id="s7:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s7:l6">syn</rhyme></l>
        <l id="s7:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s7:l5">gasten</rhyme></l>
        <l id="s7:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s7:l6">syn</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sie wy aen syn <rhyme label="a" type="f">houet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">dat is mit doernen <rhyme label="b" type="m">doerwondet</rhyme></l>
        <l id="s8:l3" met="3" type="-+">die god van herten <rhyme label="c" type="f">mynnet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">syne ziele wort hem <rhyme label="b" type="m" corresp="#s8:l2">gesont</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s8:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s8:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s8:l5">gasten</rhyme></l>
        <l id="s8:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s8:l6">syn</rhyme></l>
        <l id="s8:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s8:l5">gasten</rhyme></l>
        <l id="s8:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s8:l6">syn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Sie wi aen syne <rhyme label="a" type="f">syde</rhyme></l>
        <l id="s9:l2" met="3" type="-+">syn herte is wyde <rhyme label="b" type="m">opgedaen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">daer sullen die edelle <rhyme label="c" type="f">zielen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">des smorgens in vermeyen <rhyme label="b" type="m" corresp="#s9:l2">gaen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s9:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s9:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s9:l5">gasten</rhyme></l>
        <l id="s9:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s9:l6">syn</rhyme></l>
        <l id="s9:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s9:l5">gasten</rhyme></l>
        <l id="s9:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s9:l6">syn</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Die meye wil die <rhyme label="a" type="f">meye</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die  neme des meyes <rhyme label="b" type="m">waer</rhyme></l>
        <l id="s10:l3" met="3" type="-+">hi henck in volre <rhyme label="c" type="f">brusen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">die ons die maget <rhyme label="b" type="m" corresp="#s10:l2">gebaerden</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s10:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s10:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s10:l5">gasten</rhyme></l>
        <l id="s10:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s10:l6">syn</rhyme></l>
        <l id="s10:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s10:l5">gasten</rhyme></l>
        <l id="s10:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s10:l6">syn</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die joncfrouwen daer ter tafelen <rhyme label="a" type="f">gaen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">die engelen schone <rhyme label="b" type="m">syngen</rhyme></l>
        <l id="s11:l3" met="3" type="-+">maria is <rhyme label="c" type="f">keyserynne</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Jhesus sal schencker <rhyme label="b" type="m" corresp="#s11:l2">syn</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s11:l6" met="3" type="-+">daer bloeyet  die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s11:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s11:l5">gasten</rhyme></l>
        <l id="s11:l8" met="3" type="-+">sy moeten lutter <rhyme label="e" type="m" corresp="#s11:l6">syn</rhyme></l>
        <l id="s11:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s11:l5">gasten</rhyme></l>
        <l id="s11:l10" met="3" type="-+">sy moeten vrolic <rhyme label="e" type="m" corresp="#s11:l6">syn</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Daer schencket men den <rhyme label="a" type="f">vader</rhyme></l>
        <l id="s12:l2" met="3" type="-+">den soen den hiligen <rhyme label="b" type="m">geest</rhyme></l>
        <l id="s12:l3" met="3" type="-+">in godtelicker <rhyme label="c" type="f">mynnen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">mit vrouden alre <rhyme label="b" type="m" corresp="#s12:l2">meest</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Op des cruces <rhyme label="d" type="f">aste</rhyme></l>
        <l id="s12:l6" met="3" type="-+">daer bloeyet die rode <rhyme label="e" type="m">wyn</rhyme></l>
        <l id="s12:l7" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s12:l5">gasten</rhyme></l>
        <l id="s12:l8" met="3" type="-+">sy moten lutter <rhyme label="e" type="m" corresp="#s12:l6">syn</rhyme></l>
        <l id="s12:l9" met="3" type="-+">men schencket den lieuen <rhyme label="d" type="f" corresp="#s12:l5">gasten</rhyme></l>
        <l id="s12:l10" met="3" type="-+">sy moeten vroelic <rhyme label="e" type="m" corresp="#s12:l6">syn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
