    </tags>
    <melody>Met een droeve zang zo klaag ik mijn nood</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="groups">groepen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilt doer den mey <rhyme label="a" type="f">ontluiken</rhyme></l>
        <l id="s1:l2" met="3" type="-+">gy minnaers al <rhyme label="b" type="m">gemein</rhyme></l>
        <l id="s1:l3" met="3" type="-+">u liefste wilt <rhyme label="a" type="f" corresp="#s1:l1">gebruiken</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ionstich uit liefde <rhyme label="b" type="m" corresp="#s1:l2">reyn</rhyme></l>
        <l id="s1:l5" met="4" type="+-">laet u vruecht syn <rhyme label="c" type="m">voermeert</rhyme></l>
        <l id="s1:l6" met="4" type="+-">en genuecht toch <rhyme label="c" type="m" corresp="#s1:l5">useert</rhyme></l>
        <l id="s1:l7" met="3" type="+-">u In vroelick heit <rhyme label="d" type="m">formeert</rhyme></l>
        <l id="s1:l8" met="4" type="+-">alle druck toch van u <rhyme label="c" type="m" corresp="#s1:l5">weert</rhyme></l>
        <l id="s1:l9" met="4" type="+-">van geueinstheit <rhyme label="c" type="m" corresp="#s1:l5">deklineert</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Duer den mey diet al <rhyme label="d" type="m" corresp="#s1:l7">reetIeert</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Siet al de kruiden <rhyme label="a" type="f">spruiten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">het wout hem schoen <rhyme label="b" type="m">voertoent</rhyme></l>
        <l id="s2:l3" met="3" type="-+">al doer dees meys <rhyme label="a" type="f" corresp="#s2:l1">viertuiten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">al het lof hem schoen <rhyme label="b" type="m" corresp="#s2:l2">voertoent</rhyme></l>
        <l id="s2:l5" met="4" type="+-">dat daer was bedwongen <rhyme label="c" type="m">seer</rhyme></l>
        <l id="s2:l6" met="4" type="+-">van den winter duer den <rhyme label="c" type="m" corresp="#s2:l5">Heer</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Die voegelen <rhyme label="d" type="m">tgediert</rhyme></l>
        <l id="s2:l8" met="4" type="+-">loeuen daer om te <rhyme label="c" type="m" corresp="#s2:l5">meer</rhyme></l>
        <l id="s2:l9" met="4" type="+-">haeren godt snellen <rhyme label="c" type="m" corresp="#s2:l5">keer</rhyme></l>
        <l id="s2:l10" met="3" type="+-">duer den mey diet al <rhyme label="d" type="m" corresp="#s2:l7">reetgeert</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Tgouts blommeken schoen <rhyme label="a" type="f">voerheeuen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">leert u dit wel <rhyme label="b" type="m">onthout</rhyme></l>
        <l id="s3:l3" met="3" type="-+">twelck nu seer schoen beseeuen <rhyme label="a" type="f" corresp="#s3:l1">staet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Int groene <rhyme label="b" type="m" corresp="#s3:l2">wout</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Gekoeluert als goude <rhyme label="c" type="m">schoen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Duer den dach hem stelt ten <rhyme label="c" type="m" corresp="#s3:l5">toen</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Soe is het daer <rhyme label="d" type="m">voersiert</rhyme></l>
        <l id="s3:l8" met="4" type="+-">tot boeuen In des tscheemels <rhyme label="c" type="m" corresp="#s3:l5">troen</rhyme></l>
        <l id="s3:l9" met="4" type="+-">spannen die blommekens de <rhyme label="c" type="m" corresp="#s3:l5">kroen</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Duer den mey diet al <rhyme label="d" type="m" corresp="#s3:l7">reetgeert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gy prinse alte <rhyme label="a" type="f">saemen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dit Is uit Ionste <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">u geschonken sonder <rhyme label="a" type="f" corresp="#s4:l1">vroemen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">al doer retoerykae <rhyme label="b" type="m" corresp="#s4:l2">soet</rhyme></l>
        <l id="s4:l5" met="4" type="+-">dus al die de konst aen <rhyme label="c" type="m">kleeft</rhyme></l>
        <l id="s4:l6" met="4" type="+-">u dienaers ruitheit hem <rhyme label="c" type="m" corresp="#s4:l5">vergeeft</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Ten besten hem toch <rhyme label="d" type="m">stiert</rhyme></l>
        <l id="s4:l8" met="4" type="+-">of Ick hadt <rhyme label="c" type="m" corresp="#s4:l5">gesneeft</rhyme></l>
        <l id="s4:l9" met="4" type="+-">twelck den geest begreepen <rhyme label="c" type="m" corresp="#s4:l5">heeft</rhyme></l>
        <l id="s4:l10" met="3" type="+-">Duer den mey diet al <rhyme label="d" type="m" corresp="#s4:l7">reetieert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
