<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11438</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>A lieta vita</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">DIt aertsche <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Dat word <rhyme label="a" type="f" corresp="#s1:l1">gedreven</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Door de doot weer tot een <rhyme label="b" type="m">eynd'</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Geen menschen <rhyme label="c" type="f">dinghen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Hier <rhyme label="c" type="f" corresp="#s1:l4">sonderlinghen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Yets vast mach <rhyme label="d" type="f">bouwen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Het moet <rhyme label="d" type="f" corresp="#s1:l6">verouwen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Ja veel eerder dan het <rhyme label="b" type="m" corresp="#s1:l3">schijnt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Daerom myn <rhyme label="a" type="f">Iaeren</rhyme></l>
        <l id="s2:l2" met="2" type="-+">En wil ick <rhyme label="a" type="f" corresp="#s2:l1">spaeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Onderwyl sy jeuchdigh <rhyme label="b" type="m">syn</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Ick weet myn <rhyme label="c" type="f">daeghen</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Die 'k heb <rhyme label="c" type="f" corresp="#s2:l4">ghedraeghen</rhyme>:</l>
        <l id="s2:l6" met="2" type="-+">Maer gheen die <rhyme label="d" type="f">kommen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Hoe groot van <rhyme label="d" type="f" corresp="#s2:l6">somme</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Dat te weten is my <rhyme label="b" type="m" corresp="#s2:l3">pijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dus wilt my <rhyme label="a" type="f">schuwen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Ick sal u <rhyme label="a" type="f" corresp="#s3:l1">gruwen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">O droefheyt! ghy slyte <rhyme label="b" type="m">vleesch</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Wech bracke <rhyme label="c" type="f">suchten</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Wilt van my <rhyme label="c" type="f" corresp="#s3:l4">vluchten</rhyme>:</l>
        <l id="s3:l6" met="2" type="-+">Wech staetigh <rhyme label="d" type="f">wesen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Ick wil u <rhyme label="d" type="f" corresp="#s3:l6">vreesen</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Ofmen my de duyvel <rhyme label="b" type="m" corresp="#s3:l3">wees</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Komt soete <rhyme label="a" type="f">kusjens</rhyme>!</l>
        <l id="s4:l2" met="2" type="-+">Kom soete <rhyme label="a" type="f" corresp="#s4:l1">lusjens</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Kom myn Nimphje, kom myn <rhyme label="b" type="m">min</rhyme>!</l>
        <l id="s4:l4" met="2" type="-+">Kom laet ons <rhyme label="c" type="f">soenen</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">En eensjens <rhyme label="c" type="f" corresp="#s4:l4">boenen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Ach! uwe <rhyme label="d" type="f">tantjens</rhyme></l>
        <l id="s4:l7" met="2" type="-+">En uwe <rhyme label="d" type="f" corresp="#s4:l6">handjens</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Dat eens vreughdigh na myn <rhyme label="b" type="m" corresp="#s4:l3">sin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Wilt u niet <rhyme label="a" type="f">stooren</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Noch haer klap <rhyme label="a" type="f" corresp="#s5:l1">hooren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Die u praeten vande <rhyme label="b" type="m">tucht</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">'t Sijn <rhyme label="c" type="f">visevaesen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Ouw-wijfjes <rhyme label="c" type="f" corresp="#s5:l4">raesen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Die Maeghdom <rhyme label="d" type="f">roemen</rhyme>:</l>
        <l id="s5:l7" met="2" type="-+">Verwelckte <rhyme label="d" type="f" corresp="#s5:l6">bloemen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Sy haer kiesen voor de <rhyme label="b" type="m" corresp="#s5:l3">vrucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
