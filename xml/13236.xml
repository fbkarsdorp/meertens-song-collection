    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MAria Moeder weest <rhyme label="a" type="m">gegroet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Uyt wiens aldereelste <rhyme label="a" type="m" corresp="#s1:l1">bloet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Gebooren is dit soete <rhyme label="b" type="m">Kindt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat onse saligheyt <rhyme label="b" type="m" corresp="#s1:l3">bemindt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">GHy zijt den hooghverheven <rhyme label="a" type="m">throon</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Van Godt geworden uwen <rhyme label="a" type="m" corresp="#s2:l1">Soon</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Van 't Woord' in u geworden <rhyme label="b" type="m">vlees</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat uyt u suyver herte <rhyme label="b" type="m" corresp="#s2:l3">rees</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">GHy zijt den schoonsten <rhyme label="a" type="m">dageraet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Waer uyt ons nieuwe Son <rhyme label="a" type="m" corresp="#s3:l1">op-gaet</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Waer uyt ons vloeyt den soeten <rhyme label="b" type="m">douw</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Den oorspronck van een goet <rhyme label="b" type="m" corresp="#s3:l3">berouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">GHy brenght den hemel op der <rhyme label="a" type="m">aerdt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En uwen Schepper hebt <rhyme label="a" type="m" corresp="#s4:l1">gebaert</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Tot u de Sterren dalen <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En bidden aen den lieven <rhyme label="b" type="m" corresp="#s4:l3">Heer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">NOyt sagh ick schoonder' <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Geen Moeder ick alsoo <rhyme label="a" type="m" corresp="#s5:l1">bemin</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Ghy mijnen Herder hebt <rhyme label="b" type="m">gebracht</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Soo langen tijt van my <rhyme label="b" type="m" corresp="#s5:l3">verwacht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O Soeten Herder van mijn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Voor my geleyt in desen <rhyme label="a" type="m" corresp="#s6:l1">smert</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ick geef u weer het mijn <rhyme label="b" type="m">geheel</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En al mijn sinnen u <rhyme label="b" type="m" corresp="#s6:l3">beveel</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">WY blijven by u kribbe <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En hooren uwe wijsheyt <rhyme label="a" type="m" corresp="#s7:l1">aen</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Hier worden wy alleen <rhyme label="b" type="m">versaet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">O Heer u schaepjens niet <rhyme label="b" type="m" corresp="#s7:l3">verlaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O Soetste kindt <rhyme label="a" type="m">Emmanuël</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Geboren Vorst van <rhyme label="a" type="m" corresp="#s8:l1">Israël</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Vergadert weer in uwen <rhyme label="b" type="m">stal</rhyme></l>
        <l id="s8:l4" met="4" type="-+">U afgedwaelde Schaepjens <rhyme label="b" type="m" corresp="#s8:l3">al</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">VErlicht haer met u ooghjens <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En weder tot u keeren <rhyme label="a" type="m" corresp="#s9:l1">doet</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Geluckigh is hy, die <rhyme label="b" type="m">aenhoort</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Alleen dit eenigh eeuwigh <rhyme label="b" type="m" corresp="#s9:l3">Woordt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">GEluckigh lidt van sulcken <rhyme label="a" type="m">hooft</rhyme>!</l>
        <l id="s10:l2" met="4" type="-+">Geluckigh hert, dat hem <rhyme label="a" type="m" corresp="#s10:l1">gelooft</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">Geluckigh is hy boven <rhyme label="b" type="m">al</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Die in sijn liefde blijven <rhyme label="b" type="m" corresp="#s10:l3">sal</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">U Soete kindt, ô Maget <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Alleen is onsen <rhyme label="a" type="m" corresp="#s11:l1">Capiteyn</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Die weer ons uyt AEgypten <rhyme label="b" type="m">leyt</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">En het beloofde landt <rhyme label="b" type="m" corresp="#s11:l3">bereyt</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">U Suyv're borsten, suyv're <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Den Schepper van de selve, <rhyme label="a" type="m" corresp="#s12:l1">vraeght</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Laet suygen 't Kindt, en worden <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">En ons verlossen van de <rhyme label="b" type="m" corresp="#s12:l3">doot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
