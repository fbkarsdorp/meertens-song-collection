  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Voegt u by myn gy viese <rhyme label="a" type="f">gezelle</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Knegte en Meide aanhoord myn <rhyme label="b" type="m">propost</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ik zal u een aardig klugje <rhyme label="a" type="f" corresp="#s1:l1">vertelle</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">'t is van de onbezorgden <rhyme label="b" type="m" corresp="#s1:l2">kost</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Zy neemen <rhyme label="c" type="f">vermaaken</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">In aardige <rhyme label="c" type="f" corresp="#s1:l5">zaaken</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Goede spyse verwekt de Mensch tot <rhyme label="d" type="m">vreugt</rhyme>;</l>
        <l id="s1:l8" met="2" type="-+">'t Zyn Cupidoris <rhyme label="e" type="f">lonken</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Die schoot met zyn <rhyme label="e" type="f" corresp="#s1:l8">vonken</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Tot dat hy zyn <rhyme label="f" type="m">lust</rhyme></l>
        <l id="s1:l11" met="2" type="+-">En vlam had <rhyme label="f" type="m" corresp="#s1:l10">gebluscht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daar is onlangs een klugje <rhyme label="a" type="f">gebleeken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Met onze Jan en Anna <rhyme label="b" type="m">Catryn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">De Boer zy wel meid 't Brood zal <rhyme label="a" type="f" corresp="#s2:l1">ontbreeken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Gy moet eens na de Mulder gaan <rhyme label="b" type="m" corresp="#s2:l2">zien</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Za wild zonder <rhyme label="c" type="f">faalen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">'t meel eens gaan <rhyme label="c" type="f" corresp="#s2:l5">haalen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">De meid was vlugs en rad te <rhyme label="d" type="m">been</rhyme></l>
        <l id="s2:l8" met="2" type="-+">zy mulder wat <rhyme label="e" type="f">d</rhyme>â€¦.</l>
        <l id="s2:l9" met="2" type="-+">Gy kuld 'er wat <rhyme label="e" type="f" corresp="#s2:l8">onder</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Zwyg stil zoete <rhyme label="f" type="m">Kind</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Daar is weinig <rhyme label="f" type="m" corresp="#s2:l10">Wind</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
