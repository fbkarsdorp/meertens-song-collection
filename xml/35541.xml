<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35541</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Sarabande Pinel</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'S Nachts als een yeghelijck was in <rhyme label="a" type="f">rusten</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In d'alderstilste vanden <rhyme label="b" type="m">nacht</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Soo baerde een Maghet met <rhyme label="a" type="f" corresp="#s1:l1">wellusten</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Dat cleyne kindeken langh <rhyme label="b" type="m" corresp="#s1:l2">verwacht</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Seght eens, o <rhyme label="c" type="m">kint</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Waerom ghy <rhyme label="c" type="m" corresp="#s1:l5">mint</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Den mensch, die 't niet en heeft <rhyme label="c" type="m" corresp="#s1:l5">verdint</rhyme>?</l>
        <l id="s1:l8" met="2" type="-+">Waerom ghy <rhyme label="d" type="m">nouw</rhyme></l>
        <l id="s1:l9" met="2" type="-+">In desen <rhyme label="d" type="m" corresp="#s1:l8">kouw</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Gheboren zijt wilt van een <rhyme label="d" type="m" corresp="#s1:l8">vrouw</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Voghelkens, die met soeter <rhyme label="a" type="f">kelen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Aen den hemel <rhyme label="b" type="m">tiereliert</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Viuskens, die op't water <rhyme label="a" type="f" corresp="#s2:l1">spelen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vee, en kruypenden <rhyme label="b" type="m" corresp="#s2:l2">ghediert</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Comt, vraeght dit <rhyme label="c" type="m">kint</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Waerom het <rhyme label="c" type="m" corresp="#s2:l5">mint</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Den mensch, die 't niet en heeft <rhyme label="c" type="m" corresp="#s2:l5">verdint</rhyme>.</l>
        <l id="s2:l8" met="2" type="-+">Waerom het <rhyme label="d" type="m">nouw</rhyme></l>
        <l id="s2:l9" met="2" type="-+">In dese <rhyme label="d" type="m" corresp="#s2:l8">kouw</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Gheboren zijn wilt van een <rhyme label="d" type="m" corresp="#s2:l8">vrouw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Herderkens, die recht zijt van <rhyme label="a" type="f">sinnen</rhyme>;</l>
        <l id="s3:l2" met="4" type="-+">Herten, die meynt dat ghy <rhyme label="b" type="m">seght</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Herderkens, die wel weet van <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme>;</l>
        <l id="s3:l4" met="4" type="-+">Al gaet uwe taele plomp en <rhyme label="b" type="m" corresp="#s3:l2">slecht</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Comt, vraeght dit <rhyme label="c" type="m">kint</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Waerom het <rhyme label="c" type="m" corresp="#s3:l5">mint</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Den mensch, die 't niet en heeft <rhyme label="c" type="m" corresp="#s3:l5">verdint</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Waerom het <rhyme label="d" type="m">nouw</rhyme></l>
        <l id="s3:l9" met="2" type="-+">In dese <rhyme label="d" type="m" corresp="#s3:l8">kouw</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Gheboren zijn wilt van een <rhyme label="d" type="m" corresp="#s3:l8">vrouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Enghelkens, die van Godt den <rhyme label="a" type="f">Vader</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Zijt tot dienst van 't kindt <rhyme label="b" type="m">besteet</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Isser van u-lien <rhyme label="a" type="f" corresp="#s4:l1">allegader</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Iemandt, die de reden <rhyme label="b" type="m" corresp="#s4:l2">weet</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Waerom dit <rhyme label="c" type="m">kint</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Soo seer <rhyme label="c" type="m" corresp="#s4:l5">bemint</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Den mensch, die 't niet en heeft <rhyme label="c" type="m" corresp="#s4:l5">verdint</rhyme>:</l>
        <l id="s4:l8" met="2" type="-+">Waerom hy <rhyme label="d" type="m">nouw</rhyme></l>
        <l id="s4:l9" met="2" type="-+">In dese <rhyme label="d" type="m" corresp="#s4:l8">kouw</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Gheboren zijn wilt van een <rhyme label="d" type="m" corresp="#s4:l8">vrouw</rhyme>.?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Kindeken, dat ghy't woud' <rhyme label="a" type="f">uyt-legghen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Waerom ghy den mensch soo <rhyme label="b" type="m">vrijt</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Wat soudy toch anders <rhyme label="a" type="f" corresp="#s5:l1">segghen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dan om dat ghy Liefde <rhyme label="b" type="m" corresp="#s5:l2">zijt</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Daerom, o <rhyme label="c" type="m">kint</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Ist dat ghy <rhyme label="c" type="m" corresp="#s5:l5">mint</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Den mensch, die 't niet en heeft <rhyme label="c" type="m" corresp="#s5:l5">verdint</rhyme>:</l>
        <l id="s5:l8" met="2" type="-+">En dat ghy <rhyme label="d" type="m">nouw</rhyme></l>
        <l id="s5:l9" met="2" type="-+">In dese <rhyme label="d" type="m" corresp="#s5:l8">kouw</rhyme></l>
        <l id="s5:l10" met="4" type="-+">Gheboren zijn wilt van een <rhyme label="d" type="m" corresp="#s5:l8">vrouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
