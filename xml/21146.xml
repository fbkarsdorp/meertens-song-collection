    <melody>The hunt is up</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">KLuchtige <rhyme label="a" type="f">Maeghden</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">dat men u <rhyme label="a" type="f" corresp="#s1:l1">vraeghden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wat dit droevigh treuren <rhyme label="b" type="m">beduyt</rhyme>?</l>
        <l id="s1:l4" met="2" type="+-">Ghy kondt wel <rhyme label="c" type="f">veynsen</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">maer u <rhyme label="c" type="f" corresp="#s1:l4">gepeynsen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Zijn, wy waren soo garen de <rhyme label="b" type="m" corresp="#s1:l3">Bruydt</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">'t Sal noch wel <rhyme label="d" type="f">komen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">sit niet te <rhyme label="d" type="f" corresp="#s1:l7">droomen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">'t Sal noch wel <rhyme label="d" type="f" corresp="#s1:l7">komen</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">in 't leste <rhyme label="b" type="m" corresp="#s1:l3">besluyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Dit is wat <rhyme label="a" type="f">bijsters</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">scheldt ghy de <rhyme label="a" type="f" corresp="#s2:l1">Vrijsters</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die doch de meeste vreughde <rhyme label="b" type="m">broen</rhyme>:</l>
        <l id="s2:l4" met="2" type="+-">Twee van ons, <rhyme label="c" type="f">spelen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">singen en <rhyme label="c" type="f" corresp="#s2:l4">quelen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Soeter als twintigh Jonghmans <rhyme label="b" type="m" corresp="#s2:l3">doen</rhyme>:</l>
        <l id="s2:l7" met="2" type="+-">Wat dese <rhyme label="d" type="f">Vryers</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">sitten als <rhyme label="d" type="f" corresp="#s2:l7">Suyers</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Wat dese <rhyme label="d" type="f" corresp="#s2:l7">Vryers</rhyme></l>
        <l id="s2:l10" met="2" type="-+">die sijn niet <rhyme label="b" type="m" corresp="#s2:l3">groen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Dit doet de <rhyme label="a" type="f">donder</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">is dit geen <rhyme label="a" type="f" corresp="#s3:l1">wonder</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat dese Meysjes quellen <rhyme label="b" type="m">soo</rhyme>?</l>
        <l id="s3:l4" met="2" type="+-">Laet ons eens <rhyme label="c" type="f">singen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">danssen en <rhyme label="c" type="f" corresp="#s3:l4">springen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Hippelen rontom als een <rhyme label="b" type="m" corresp="#s3:l3">vloo</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Laet u <rhyme label="d" type="f">geleyen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Meysjes aen <rhyme label="d" type="f" corresp="#s3:l7">reyen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Laet u <rhyme label="d" type="f" corresp="#s3:l7">geleyen</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">of doet ghy 't <rhyme label="b" type="m" corresp="#s3:l3">noo</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Weest niet <rhyme label="a" type="f">verlegen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">van onsent <rhyme label="a" type="f" corresp="#s4:l1">wegen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Maer de Bruydt most met ons <rhyme label="b" type="m">gaen</rhyme>.</l>
        <l id="s4:l4" met="2" type="+-">Bruydt weest wat <rhyme label="c" type="f">soetjes</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">rept uwe <rhyme label="c" type="f" corresp="#s4:l4">voetjes</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Stelt u mee vroolijck op de <rhyme label="b" type="m" corresp="#s4:l3">baen</rhyme>:</l>
        <l id="s4:l7" met="2" type="+-">Nu jongelingen <rhyme label="d" type="f">dan</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">wiltger wat singen <rhyme label="d" type="f" corresp="#s4:l7">van</rhyme>?</l>
        <l id="s4:l9" met="2" type="+-">Nu jongelingen <rhyme label="d" type="f" corresp="#s4:l7">dan</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">vanght eensjes <rhyme label="b" type="m" corresp="#s4:l3">an</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Nu dan eens <rhyme label="a" type="f">rustigh</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">toont u wat <rhyme label="a" type="f" corresp="#s5:l1">lustigh</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Vroolijcke Bruydt, springht lustigh <rhyme label="b" type="m">om</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">'t Veynsen wilt <rhyme label="c" type="f">staecken</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">en u <rhyme label="c" type="f" corresp="#s5:l4">vermaecken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Met ons en uwen <rhyme label="b" type="m" corresp="#s5:l3">Bruydegom</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Die u met <rhyme label="d" type="f">kusjes</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">die u met <rhyme label="d" type="f" corresp="#s5:l7">lusjes</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Die u met <rhyme label="d" type="f" corresp="#s5:l7">kusjes</rhyme></l>
        <l id="s5:l10" met="2" type="-+">heet <rhyme label="b" type="m" corresp="#s5:l3">wellekom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Nu waerom <rhyme label="a" type="f">lachtje</rhyme>?</l>
        <l id="s6:l2" met="2" type="+-">is 't wijl het <rhyme label="a" type="f" corresp="#s6:l1">nachtje</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Van uwe vreught voor handen <rhyme label="b" type="m">staet</rhyme>?</l>
        <l id="s6:l4" met="2" type="+-">'k Acht dat men waer <rhyme label="c" type="f">seyt</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">dus maeckt geen <rhyme label="c" type="f" corresp="#s6:l4">swaerheydt</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Maer u met blijdtschappen voeren <rhyme label="b" type="m" corresp="#s6:l3">laet</rhyme>,</l>
        <l id="s6:l7" met="2" type="+-">Daer ghy met <rhyme label="d" type="f">lusten</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">vroolijck sult <rhyme label="d" type="f" corresp="#s6:l7">rusten</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-">Daer ghy met <rhyme label="d" type="f" corresp="#s6:l7">lusten</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">en vreughd' <rhyme label="b" type="m" corresp="#s6:l3">om-gaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
