  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'t LEvertjen Ã´ zoete <rhyme label="a" type="f">Susje</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Is geweldig droog van <rhyme label="b" type="m">smaak</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dus gunt my een lekker <rhyme label="a" type="f" corresp="#s1:l1">kusje</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat ik het wat vochtig <rhyme label="b" type="m" corresp="#s1:l2">maak</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Want door 't vocht van uwe <rhyme label="c" type="m">mont</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Gy mijn daat'lijk helpen <rhyme label="c" type="m" corresp="#s1:l5">kont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Leg u lipjes op mijn <rhyme label="a" type="f">lippen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Leg u montjen op het <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Laat de nectar <rhyme label="a" type="f" corresp="#s2:l1">overglippen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En het lekker <rhyme label="b" type="m" corresp="#s2:l2">Ambrozijn</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Want door 't vocht van uwen <rhyme label="c" type="m">mont</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Gy mijn daat'lijk helpen <rhyme label="c" type="m" corresp="#s2:l5">kont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat mijn tong dan op de <rhyme label="a" type="f">kantjes</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Dwalen, daar het wit <rhyme label="b" type="m">albast</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Zig vertoont van uwe <rhyme label="a" type="f" corresp="#s3:l1">tantjes</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En in Suyker-stroompjes <rhyme label="b" type="m" corresp="#s3:l2">wast</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Want door 't vocht van uwen <rhyme label="c" type="m">mont</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Ghy mijn daetlijk helpen <rhyme label="c" type="m" corresp="#s3:l5">kont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laet den Honig soete <rhyme label="a" type="f">waesem</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Pruyssen door u tantjes <rhyme label="b" type="m">heen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Mengt u lieffelijken <rhyme label="a" type="f" corresp="#s4:l1">asem</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En de mijne dan door <rhyme label="b" type="m" corresp="#s4:l2">een</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Want door 't vocht van uwen <rhyme label="c" type="m">mont</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Ghy mijn daetlijk helpen <rhyme label="c" type="m" corresp="#s4:l5">kont</rhyme>:</l>
      </lg>
    </lg>
  </text>
</song>
