    </location>
    <tags>
      <tag standard-name="paaslied" translation="Easter song">paaslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Christ ist erstanden</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Christus is op <rhyme label="a" type="f">ghestaen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En naer Galileen <rhyme label="a" type="f" corresp="#s1:l1">voorghegaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dus willen wy alle gader vrolijck <rhyme label="b" type="f">sijn</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Christus sal onse trooster <rhyme label="b" type="f" corresp="#s1:l3">sijn</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Kyrie <rhyme label="c" type="m">eleyson</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al sijn wy Gods <rhyme label="a" type="f">gheuanghen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Na Christus is ons <rhyme label="a" type="f" corresp="#s2:l1">verlanghen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat Cruys dat moeten wy <rhyme label="b" type="f">draghen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sullen wy Christo <rhyme label="b" type="f" corresp="#s2:l3">behaghen</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Kyrie <rhyme label="c" type="m">eleyson</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Christus heeft <rhyme label="a" type="f">gheleden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hy heeft voor ons <rhyme label="a" type="f" corresp="#s3:l1">ghestreden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die Vyandt is <rhyme label="b" type="f">verwonnen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die doot heeft hy <rhyme label="b" type="f" corresp="#s3:l3">verstonnen</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Kyrie <rhyme label="c" type="m">eleyson</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Christus is doot <rhyme label="a" type="f">ghesleghen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hy heeft victorie <rhyme label="a" type="f" corresp="#s4:l1">vercreghen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hy is ons allen een <rhyme label="b" type="f">Medecijn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Christus is onse verlosser <rhyme label="b" type="f" corresp="#s4:l3">fijn</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Kyrie <rhyme label="c" type="m">eleyson</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Christus is nv <rhyme label="a" type="f">verresen</rhyme>.</l>
        <l id="s5:l2" met="3" type="-+">Aldus willen wy vrolijck <rhyme label="a" type="f" corresp="#s5:l1">wesen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Die doodt heeft haren naem <rhyme label="b" type="f">verloren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Christus is ons verlosser <rhyme label="b" type="f" corresp="#s5:l3">vercoren</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Kyrie <rhyme label="c" type="m">eleyson</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
