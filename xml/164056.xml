<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164056</id>
    <date>
      <year-start>1730</year-start>
      <year-end>1730</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie hoe gelukkig zijn ze dan</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Werelt vol van <rhyme label="a" type="m">overdaet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wat baent gy open <rhyme label="b" type="f">wegen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Tot alderhande <rhyme label="a" type="m" corresp="#s1:l1">quaedt</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Tot wraeck bedrijf en <rhyme label="c" type="m">moort</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Een saeck noyt meer <rhyme label="c" type="m" corresp="#s1:l4">gehoort</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sal ick u brengen <rhyme label="c" type="m" corresp="#s1:l4">voort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In Zeelant is dit feyt <rhyme label="a" type="m">geschiedt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ter tijd doen Hertog <rhyme label="b" type="f">Carel</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Hier voerde 't groot <rhyme label="a" type="m" corresp="#s2:l1">gebiedt</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Zijn Gouverneur <rhyme label="c" type="m">bekent</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Heeft hem tot quaet <rhyme label="c" type="m" corresp="#s2:l4">gewendt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En kreeg een droevig <rhyme label="c" type="m" corresp="#s2:l4">endt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy sloeg een geyl en dertel <rhyme label="a" type="m">oogh</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Op eenen edel <rhyme label="b" type="f">vrouwe</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">'t Geen hem op 't lest <rhyme label="a" type="m" corresp="#s3:l1">bedroogh</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">En sprack mijn herte <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Sal nimmer zijn <rhyme label="c" type="m" corresp="#s3:l4">gerust</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Voor gy mijn vlamme <rhyme label="c" type="m" corresp="#s3:l4">blust</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy als een eerbaar kuysche <rhyme label="a" type="m">vrouw</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Sprak Heer zou ik <rhyme label="b" type="f">verbreeken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met u de egte <rhyme label="a" type="m" corresp="#s4:l1">trouw</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Soud' ick mijn lieve <rhyme label="c" type="m">Man</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">Doen zulken gruwel <rhyme label="c" type="m" corresp="#s4:l4">an</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Och spreekt daar nimmer <rhyme label="c" type="m" corresp="#s4:l4">van</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy met een op,stinaert <rhyme label="a" type="m">gemoet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ginck hem na huys toe <rhyme label="b" type="f">keeren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Heel toornigh en <rhyme label="a" type="m" corresp="#s5:l1">verwoet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hy liet van stonden <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zijn dienaars heenen <rhyme label="c" type="m" corresp="#s5:l4">gaen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En haelt haer Man <rhyme label="c" type="m" corresp="#s5:l4">gevaan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Beticht hem met Landt <rhyme label="a" type="m">verraedt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zijn vrouw geheel <rhyme label="b" type="f">verslagen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die vraegt op heter <rhyme label="a" type="m" corresp="#s6:l1">daet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wat magh de oorsaek <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat dus mijn Man vol <rhyme label="c" type="m" corresp="#s6:l4">pijn</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Daer moet gevangen <rhyme label="c" type="m" corresp="#s6:l4">zijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">U Man sprack hy wou 'tLant <rhyme label="a" type="m">verraen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ik sal hem door Beuls <rhyme label="b" type="f">handen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Doen 't hooft van 't Lichaem <rhyme label="a" type="m" corresp="#s7:l1">slaen</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Maer by mijn macht ick <rhyme label="c" type="m">zweer</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Voldoet gy mijn <rhyme label="c" type="m" corresp="#s7:l4">begeer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Gy krijgt u Man dan <rhyme label="c" type="m" corresp="#s7:l4">weer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De sondt wert hem vergeven <rhyme label="a" type="m">dra</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Voldoet gy mijne <rhyme label="b" type="f">lusten</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">So kreegh u Man <rhyme label="a" type="m" corresp="#s8:l1">gena</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Dies kiest in dit <rhyme label="c" type="m">geval</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Mijn wil te doen voor <rhyme label="c" type="m" corresp="#s8:l4">al</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Of ick hem straffen <rhyme label="c" type="m" corresp="#s8:l4">sal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">De kuysche vrouwe toog <rhyme label="a" type="m">mitsdien</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Seer bitterlijk aen 't <rhyme label="b" type="f">schrieijen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Sal ik mijn Man dan <rhyme label="a" type="m" corresp="#s9:l1">sien</rhyme>;</l>
        <l id="s9:l4" met="3" type="-+">'t Hooft vallen voor hem <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Of af staan van mijn <rhyme label="c" type="m" corresp="#s9:l4">eer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En daerom doen zijn <rhyme label="c" type="m" corresp="#s9:l4">begeer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Mijn lieve man is mijn so <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Als eenig schat op <rhyme label="b" type="f">aerden</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Of als mijn eer my <rhyme label="a" type="m" corresp="#s10:l1">doet</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Mijn Man mijn <rhyme label="c" type="m">rechterhant</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Moet een van bey aen <rhyme label="c" type="m" corresp="#s10:l4">kant</rhyme>:</l>
        <l id="s10:l6" met="3" type="-+">Ã´ Godt doet my <rhyme label="c" type="m" corresp="#s10:l4">bystant</rhyme>!</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Den booswicht greep haer by de <rhyme label="a" type="m">hant</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">In 't midden van haer <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Wierp haar op 't <rhyme label="a" type="m" corresp="#s11:l1">Ledikant</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En heeft seer <rhyme label="c" type="m">ongerust</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Zijn geyl en dertele <rhyme label="c" type="m" corresp="#s11:l4">lust</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Tot walgens toe <rhyme label="c" type="m" corresp="#s11:l4">geblust</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Toen sprack hy vrouwe gaet nu <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Op morgen moogt gy <rhyme label="b" type="f">halen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">U Man uyt het <rhyme label="a" type="m" corresp="#s12:l1">geween</rhyme>:</l>
        <l id="s12:l4" met="3" type="-+">Bedroeft is sy <rhyme label="c" type="m">gegaen</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Hy liet van stonden <rhyme label="c" type="m" corresp="#s12:l4">aen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Haar Man het hooft <rhyme label="c" type="m" corresp="#s12:l4">af-slaan</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Des morgens opent sy haer <rhyme label="a" type="m">deur</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">En ging vrymoedig <rhyme label="b" type="f">treden</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Voort na den <rhyme label="a" type="m" corresp="#s13:l1">Gouverneur</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Sy sprack groot mogent <rhyme label="c" type="m">heer</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Geeft my mijn Man nu <rhyme label="c" type="m" corresp="#s13:l4">weer</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Voor mijn geschonden <rhyme label="c" type="m" corresp="#s13:l4">eer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Hy riep twee dienaars voor den <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Die bragten haer ter <rhyme label="b" type="f">plaetse</rhyme>:</l>
        <l id="s14:l3" met="3" type="-+">Daar sy haer Man aen <rhyme label="a" type="m" corresp="#s14:l1">sagh</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Daer lagh hy  sonder <rhyme label="c" type="m">hooft</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Van 't bloedig zwaert <rhyme label="c" type="m" corresp="#s14:l4">verdooft</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">En 't leven wreet <rhyme label="c" type="m" corresp="#s14:l4">ontrooft</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Sy viel op 't doode Lichaem <rhyme label="a" type="m">neer</rhyme> //</l>
        <l id="s15:l2" met="3" type="-+">En kusten zijne <rhyme label="b" type="f">wangen</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">En schreyden om haar <rhyme label="a" type="m" corresp="#s15:l1">eer</rhyme>:</l>
        <l id="s15:l4" met="3" type="-+">Trock 't hayr uyt d' <rhyme label="c" type="m">harssepan</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">En riep Ã´ snoo <rhyme label="c" type="m" corresp="#s15:l4">Tyran</rhyme>!</l>
        <l id="s15:l6" met="3" type="-+">Vermoort gy soo mijn <rhyme label="c" type="m" corresp="#s15:l4">man</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Sy ging al sinloos en <rhyme label="a" type="m">verwoet</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">En klaegden aen haer <rhyme label="b" type="f">vrienden</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Haer aldernaeste <rhyme label="a" type="m" corresp="#s16:l1">bloet</rhyme>:</l>
        <l id="s16:l4" met="3" type="-+">Den Gouverneur Ã´ <rhyme label="c" type="m">schant</rhyme>!</l>
        <l id="s16:l5" met="3" type="-+">Heeft my mijn eer <rhyme label="c" type="m" corresp="#s16:l4">ontmant</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Mijn man geleyt in 't <rhyme label="c" type="m" corresp="#s16:l4">sant</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Haer vrienden spraken, Nicht wel <rhyme label="a" type="m">hoe</rhyme>!</l>
        <l id="s17:l2" met="3" type="-+">Laet ons van Zeeland <rhyme label="b" type="f">reysen</rhyme>:</l>
        <l id="s17:l3" met="3" type="-+">Terstont na Hollant <rhyme label="a" type="m" corresp="#s17:l1">toe</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Beklagen daer dit <rhyme label="c" type="m">quaet</rhyme>;</l>
        <l id="s17:l5" met="3" type="-+">En grouwelijcke <rhyme label="c" type="m" corresp="#s17:l4">daedt</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Aen d'Hertog groot van <rhyme label="c" type="m" corresp="#s17:l4">staet</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Men quam seer haest en <rhyme label="a" type="m">ongemeen</rhyme>;</l>
        <l id="s18:l2" met="3" type="-+">Tot Delft binnen <rhyme label="b" type="f">ryden</rhyme>:</l>
        <l id="s18:l3" met="3" type="-+">Men ging ten Hoove <rhyme label="a" type="m" corresp="#s18:l1">treen</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Sy sprack den Hertog <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Deed' hem de saeck <rhyme label="c" type="m" corresp="#s18:l4">verstaen</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Hoe 't al was <rhyme label="c" type="m" corresp="#s18:l4">toegegaen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Ick wreeckt de dood van mijnen <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">Wreeckt mijn geschonden <rhyme label="b" type="f">eere</rhyme>:</l>
        <l id="s19:l3" met="3" type="-+">So yemant wreken <rhyme label="a" type="m" corresp="#s19:l1">kan</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">U valschen <rhyme label="c" type="m">Gouverneur</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Die brocht my in't <rhyme label="c" type="m" corresp="#s19:l4">getreur</rhyme>:</l>
        <l id="s19:l6" met="3" type="-+">Bestel hem straf daer <rhyme label="c" type="m" corresp="#s19:l4">veur</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Den Hertog sprack u wel <rhyme label="a" type="m">versint</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">En let wel op u <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Hy is my een goet <rhyme label="a" type="m" corresp="#s20:l1">vriendt</rhyme>?</l>
        <l id="s20:l4" met="3" type="-+">Dus segt in dit <rhyme label="c" type="m">geval</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">De suyvere waerheyt <rhyme label="c" type="m" corresp="#s20:l4">al</rhyme>:</l>
        <l id="s20:l6" met="3" type="-+">'k Zweer ick hem straffen <rhyme label="c" type="m" corresp="#s20:l4">sal</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Maer so 'k dees saeck onwaer <rhyme label="a" type="m">bevint</rhyme>?</l>
        <l id="s21:l2" met="3" type="-+">So sal ick u doen <rhyme label="b" type="f">smaken</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">De dood die u <rhyme label="a" type="m" corresp="#s21:l1">verslint</rhyme>:</l>
        <l id="s21:l4" met="3" type="-+">Ick daegh hem voor het <rhyme label="c" type="m">recht</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Dus hier in <rhyme label="c" type="m" corresp="#s21:l4">overlegt</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">Bedenckt wel wat gy <rhyme label="c" type="m" corresp="#s21:l4">segt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Sy sprack, mijn Heer is dit <rhyme label="a" type="m">onwaer</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">So laet mijn lichaem <rhyme label="b" type="f">saegen</rhyme></l>
        <l id="s22:l3" met="3" type="-+">Aen stucken van <rhyme label="a" type="m" corresp="#s22:l1">malkaer</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">Of helpt my onder d' <rhyme label="c" type="m">aerd</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Door een Scherp-rechters <rhyme label="c" type="m" corresp="#s22:l4">zwaert</rhyme>,</l>
        <l id="s22:l6" met="3" type="-+">De dood my niet <rhyme label="c" type="m" corresp="#s22:l4">vervaert</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">De Hertog liet den breeden <rhyme label="a" type="m">Raet</rhyme>,</l>
        <l id="s23:l2" met="3" type="-+">Terstont ten Hoof <rhyme label="b" type="f">vergaren</rhyme>,</l>
        <l id="s23:l3" met="3" type="-+">Om dit vervloeckte <rhyme label="a" type="m" corresp="#s23:l1">quaet</rhyme></l>
        <l id="s23:l4" met="3" type="-+">Te straffen <rhyme label="c" type="m">ongemeen</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Men deed de vrouw <rhyme label="c" type="m" corresp="#s23:l4">alleen</rhyme></l>
        <l id="s23:l6" met="3" type="-+">Al in een Kamer <rhyme label="c" type="m" corresp="#s23:l4">treen</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Toen quam den Booswicht voor den <rhyme label="a" type="m">dagh</rhyme>,</l>
        <l id="s24:l2" met="3" type="-+">Voor d'hoogen Raet <rhyme label="b" type="f">verschenen</rhyme>,</l>
        <l id="s24:l3" met="3" type="-+">En sprack met bly <rhyme label="a" type="m" corresp="#s24:l1">gelacht</rhyme>:</l>
        <l id="s24:l4" met="3" type="-+">Wat is d'oorsaeck doch mijn <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Dat ick om u <rhyme label="c" type="m" corresp="#s24:l4">begeer</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">Kom reysen hier dus <rhyme label="c" type="m" corresp="#s24:l4">veer</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">De Hertog sprack hem vreeslijck <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s25:l2" met="3" type="-+">Gy eer vergeten <rhyme label="b" type="f">schelm</rhyme>,</l>
        <l id="s25:l3" met="3" type="-+">Wat quaet hebt gy <rhyme label="a" type="m" corresp="#s25:l1">gedaen</rhyme>?</l>
        <l id="s25:l4" met="3" type="-+">Een eerb're vrouw <rhyme label="c" type="m">verkragt</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">Haer man ter doodt <rhyme label="c" type="m" corresp="#s25:l4">gebracht</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">Denckt dat gy straf <rhyme label="c" type="m" corresp="#s25:l4">verwacht</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Hy sprack wie leydt my dit te <rhyme label="a" type="m">last</rhyme>?</l>
        <l id="s26:l2" met="3" type="-+">Het zijn verdichte <rhyme label="b" type="f">logens</rhyme>,</l>
        <l id="s26:l3" met="3" type="-+">Ã´ Vorst! daer 's niet aen <rhyme label="a" type="m" corresp="#s26:l1">vast</rhyme>.</l>
        <l id="s26:l4" met="3" type="-+">Stel mijn party ten <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s26:l5" met="3" type="-+">Ick zweer u by de <rhyme label="c" type="m" corresp="#s26:l4">Goon</rhyme>,</l>
        <l id="s26:l6" met="3" type="-+">Hy krijgt verdienden <rhyme label="c" type="m" corresp="#s26:l4">loon</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Den Hertog sprack: Ã´ <rhyme label="a" type="m">Gouverneur</rhyme>,</l>
        <l id="s27:l2" met="3" type="-+">'k Sal oprecht blijck u <rhyme label="b" type="f">toonen</rhyme>:</l>
        <l id="s27:l3" met="3" type="-+">Men opende de <rhyme label="a" type="m" corresp="#s27:l1">deur</rhyme>,</l>
        <l id="s27:l4" met="3" type="-+">Doen wiert zijn hert <rhyme label="c" type="m">belaen</rhyme>,</l>
        <l id="s27:l5" met="3" type="-+">Hy sag de vrouwe <rhyme label="c" type="m" corresp="#s27:l4">aen</rhyme>,</l>
        <l id="s27:l6" met="3" type="-+">Bedroeft daer voor hem <rhyme label="c" type="m" corresp="#s27:l4">staen</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Don Carel wiert als <rhyme label="a" type="m">opstinaet</rhyme>,</l>
        <l id="s28:l2" met="3" type="-+">Sprack kent gy wel dees <rhyme label="b" type="f">vrouwe</rhyme>,</l>
        <l id="s28:l3" met="3" type="-+">Die hier nu voor u <rhyme label="a" type="m" corresp="#s28:l1">staet</rhyme>,</l>
        <l id="s28:l4" met="3" type="-+">Dit is u <rhyme label="c" type="m">vyandin</rhyme>,</l>
        <l id="s28:l5" met="3" type="-+">Die gy door geyle <rhyme label="c" type="m" corresp="#s28:l4">min</rhyme>,</l>
        <l id="s28:l6" met="3" type="-+">Dwong naer u lust en <rhyme label="c" type="m" corresp="#s28:l4">sin</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Hy viel van schaemt ter aerden <rhyme label="a" type="m">neer</rhyme>,</l>
        <l id="s29:l2" met="3" type="-+">Den Hertogh sprack met <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s29:l3" met="3" type="-+">Herstelt dees vrouw haer <rhyme label="a" type="m" corresp="#s29:l1">eer</rhyme>,</l>
        <l id="s29:l4" met="3" type="-+">Ick wil tot straffens <rhyme label="c" type="m">schult</rhyme>,</l>
        <l id="s29:l5" met="3" type="-+">Dat ghy haer met <rhyme label="c" type="m" corresp="#s29:l4">gedult</rhyme>,</l>
        <l id="s29:l6" met="3" type="-+">Terstont hier Echten <rhyme label="c" type="m" corresp="#s29:l4">sult</rhyme>.</l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="4" type="-+">Don Karel vraegde aen dees <rhyme label="a" type="m">vrouw</rhyme></l>
        <l id="s30:l2" met="3" type="-+">Of sy haer wou <rhyme label="b" type="f">begeven</rhyme>,</l>
        <l id="s30:l3" met="3" type="-+">Met hem in d'Echte <rhyme label="a" type="m" corresp="#s30:l1">Trouw</rhyme>?</l>
        <l id="s30:l4" met="3" type="-+">Sy sprack Ã´ Vorst ick <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s30:l5" met="3" type="-+">In d' Ecgt niet nemen <rhyme label="c" type="m" corresp="#s30:l4">an</rhyme>,</l>
        <l id="s30:l6" met="3" type="-+">Den Moorder van mijn <rhyme label="c" type="m" corresp="#s30:l4">man</rhyme>.</l>
      </lg>
      <lg id="s31" type="stanza">
        <l id="s31:l1" met="4" type="-+">Hy sprack 't sal u niet schadelijck <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s31:l2" met="3" type="-+">Gy sult oock wraeck <rhyme label="b" type="f">genieten</rhyme>,</l>
        <l id="s31:l3" met="3" type="-+">So doet de raet van <rhyme label="a" type="m" corresp="#s31:l1">mijn</rhyme>,</l>
        <l id="s31:l4" met="3" type="-+">Dus voort door vrinden <rhyme label="c" type="m">raet</rhyme>,</l>
        <l id="s31:l5" met="3" type="-+">En d'Hertogs soete <rhyme label="c" type="m" corresp="#s31:l4">praet</rhyme></l>
        <l id="s31:l6" met="3" type="-+">Sy 't jawoort geven <rhyme label="c" type="m" corresp="#s31:l4">gaet</rhyme>.</l>
      </lg>
      <lg id="s32" type="stanza">
        <l id="s32:l1" met="4" type="-+">Dus wiert de Weduw weer de <rhyme label="a" type="m">Bruyt</rhyme>,</l>
        <l id="s32:l2" met="3" type="-+">En d' houwelijcks voorwaerden <rhyme label="b" type="f">uyt</rhyme>,</l>
        <l id="s32:l3" met="3" type="-+">Wiert daert geschreven <rhyme label="a" type="m" corresp="#s32:l1">uyt</rhyme>,</l>
        <l id="s32:l4" met="3" type="-+">Al leyt sy noyt geen <rhyme label="c" type="m">kraem</rhyme>,</l>
        <l id="s32:l5" met="3" type="-+">En hy te sterven <rhyme label="c" type="m" corresp="#s32:l4">quaem</rhyme>,</l>
        <l id="s32:l6" met="3" type="-+">Soo bleef sy <rhyme label="c" type="m" corresp="#s32:l4">Erfgenaem</rhyme>.</l>
      </lg>
      <lg id="s33" type="stanza">
        <l id="s33:l1" met="4" type="-+">Dus wierdt den Prister voort <rhyme label="a" type="m">gehaelt</rhyme>,</l>
        <l id="s33:l2" met="3" type="-+">Het houw'lijck wierdt <rhyme label="b" type="f">bevestigt</rhyme>,</l>
        <l id="s33:l3" met="3" type="-+">En voor den Raet <rhyme label="a" type="m" corresp="#s33:l1">bepaelt</rhyme>:</l>
        <l id="s33:l4" met="3" type="-+">Den Gouverneur sey <rhyme label="c" type="m">heer</rhyme>,</l>
        <l id="s33:l5" met="3" type="-+">Wy dancken d' Hertogh <rhyme label="c" type="m" corresp="#s33:l4">seer</rhyme>,</l>
        <l id="s33:l6" met="3" type="-+">Voor des genoten <rhyme label="c" type="m" corresp="#s33:l4">eer</rhyme>.</l>
      </lg>
      <lg id="s34" type="stanza">
        <l id="s34:l1" met="4" type="-+">Don Karel sprack zijt gy te <rhyme label="a" type="m">vreen</rhyme>?</l>
        <l id="s34:l2" met="3" type="-+">Ia heer en wy <rhyme label="b" type="f">vertrecken</rhyme>,</l>
        <l id="s34:l3" met="3" type="-+">Met alle <rhyme label="a" type="m" corresp="#s34:l1">danckbaerheen</rhyme>,</l>
        <l id="s34:l4" met="3" type="-+">Sa! sprack hy seer <rhyme label="c" type="m">lieftal</rhyme>,</l>
        <l id="s34:l5" met="3" type="-+">Mijn Bruylofts-geest die <rhyme label="c" type="m" corresp="#s34:l4">sal</rhyme>,</l>
        <l id="s34:l6" met="3" type="-+">Oock volgen na <rhyme label="c" type="m" corresp="#s34:l4">geval</rhyme>.</l>
      </lg>
      <lg id="s35" type="stanza">
        <l id="s35:l1" met="4" type="-+">Neen sprack de Vorst na mijn <rhyme label="a" type="m">vermoen</rhyme>,</l>
        <l id="s35:l2" met="3" type="-+">So moet gy de <rhyme label="b" type="f">Iustici</rhyme>,</l>
        <l id="s35:l3" met="3" type="-+">En mijn oock <rhyme label="a" type="m" corresp="#s35:l1">voldoen</rhyme>,</l>
        <l id="s35:l4" met="3" type="-+">Sa knielt neer voor het <rhyme label="c" type="m">zant</rhyme>,</l>
        <l id="s35:l5" met="3" type="-+">Op dat gy door <rhyme label="c" type="m" corresp="#s35:l4">beulshant</rhyme>:</l>
        <l id="s35:l6" met="3" type="-+">Gestraft wert weer met <rhyme label="c" type="m" corresp="#s35:l4">schant</rhyme>.</l>
      </lg>
      <lg id="s36" type="stanza">
        <l id="s36:l1" met="4" type="-+">Hy badt den Vorst om lijfs <rhyme label="a" type="m">gena</rhyme>,</l>
        <l id="s36:l2" met="3" type="-+">Maer smeken was <rhyme label="b" type="f">verlooren</rhyme>:</l>
        <l id="s36:l3" met="3" type="-+">Den Hertogh sprack nu <rhyme label="a" type="m" corresp="#s36:l1">dra</rhyme>,</l>
        <l id="s36:l4" met="3" type="-+">Scherp-rechter volgh mijn <rhyme label="c" type="m">last</rhyme>:</l>
        <l id="s36:l5" met="3" type="-+">En geeft den snooden <rhyme label="c" type="m" corresp="#s36:l4">gast</rhyme>,</l>
        <l id="s36:l6" met="3" type="-+">Het loon hem <rhyme label="c" type="m" corresp="#s36:l4">toegepast</rhyme>.</l>
      </lg>
      <lg id="s37" type="stanza">
        <l id="s37:l1" met="4" type="-+">Men sloegh hem 't hooft al met een <rhyme label="a" type="m">slag</rhyme></l>
        <l id="s37:l2" met="3" type="-+">Het welck de jonge <rhyme label="b" type="f">Weduw</rhyme>:</l>
        <l id="s37:l3" met="3" type="-+">Met blijdschap al <rhyme label="a" type="m" corresp="#s37:l1">aensag</rhyme>,</l>
        <l id="s37:l4" met="3" type="-+">Sy kreeg na zijne <rhyme label="c" type="m">doodt</rhyme>,</l>
        <l id="s37:l5" met="3" type="-+">Alle de goederen <rhyme label="c" type="m" corresp="#s37:l4">groot</rhyme>,</l>
        <l id="s37:l6" met="3" type="-+">Dat holp haer uyt den <rhyme label="c" type="m" corresp="#s37:l4">noot</rhyme>.</l>
      </lg>
      <lg id="s38" type="stanza">
        <l id="s38:l1" met="4" type="-+">Dit is waerachtig so <rhyme label="a" type="m">geschiet</rhyme>,</l>
        <l id="s38:l2" met="3" type="-+">By Hartog Carel de <rhyme label="b" type="f">stoute</rhyme>,</l>
        <l id="s38:l3" met="3" type="-+">Als de Chronijck <rhyme label="a" type="m" corresp="#s38:l1">bediet</rhyme>:</l>
        <l id="s38:l4" met="3" type="-+">Hier mee wort ons <rhyme label="c" type="m">betoont</rhyme>,</l>
        <l id="s38:l5" met="3" type="-+">Dat straf geen vrint <rhyme label="c" type="m" corresp="#s38:l4">verschoont</rhyme>:</l>
        <l id="s38:l6" met="3" type="-+">Maer 't quaet zijn Meester <rhyme label="c" type="m" corresp="#s38:l4">loont</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
