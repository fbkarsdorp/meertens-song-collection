<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5189</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag standard-name="geuzenlied" translation="Geuzen song">geuzenlied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Wilhelmus</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Tfy u ghy <rhyme label="a" type="f">Afgodisten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die Gods Raet <rhyme label="b" type="m">wederstaen</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Ja meer met den <rhyme label="a" type="f" corresp="#s1:l1">Papisten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dan met tPrincelick <rhyme label="b" type="m" corresp="#s1:l2">graen</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Dunckt u niet Godt salt <rhyme label="c" type="f">wreken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En brenghen aen den <rhyme label="d" type="m">dach</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">U valsche loose <rhyme label="c" type="f" corresp="#s1:l5">treken</rhyme>?</l>
        <l id="s1:l8" met="3" type="-+">U naect eewich <rhyme label="d" type="m" corresp="#s1:l6">gheclach</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schaemt u het boos <rhyme label="a" type="f">hantieren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ghy Hypocrijten <rhyme label="b" type="m">quaet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">U loghenen <rhyme label="a" type="f" corresp="#s2:l1">versieren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En sit in des Landts <rhyme label="b" type="m" corresp="#s2:l2">Raedt</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Gheveynst zijn u <rhyme label="c" type="f">Raedtslaghen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ghy Achitopels <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ghy sult als Aman <rhyme label="c" type="f" corresp="#s2:l5">beclaghen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">De boosheyt die ghy <rhyme label="d" type="m" corresp="#s2:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy soect des Graven <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Op dat ghy sout te <rhyme label="b" type="m">bet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die Landen bringhen in <rhyme label="a" type="f" corresp="#s3:l1">sneven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Door u trouloos <rhyme label="b" type="m" corresp="#s3:l2">opset</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Ende hebt den Prins <rhyme label="c" type="f">ghesworen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Maer uwen Eedt is <rhyme label="d" type="m">vals</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ghy soect den Edelen <rhyme label="c" type="f" corresp="#s3:l5">gheboren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Te bringhen om den <rhyme label="d" type="m" corresp="#s3:l6">hals</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy sitt als <rhyme label="a" type="f">Potentaten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In des Lants <rhyme label="b" type="m">Regiment</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Maer Monicken, Papen, <rhyme label="a" type="f" corresp="#s4:l1">Prelaten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Maken u sienden <rhyme label="b" type="m" corresp="#s4:l2">blent</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Daerom zijt ghy <rhyme label="c" type="f">Partije</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Den cloecken Grave, <rhyme label="d" type="m">maer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Al u <rhyme label="c" type="f" corresp="#s4:l5">Verraderije</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Sal comen noch <rhyme label="d" type="m" corresp="#s4:l6">openbaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy hebt het Landt tot <rhyme label="a" type="f">schande</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Den Alff <rhyme label="b" type="m">ghepresenteert</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Te leveren in zijn <rhyme label="a" type="f" corresp="#s5:l1">handen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Sulcks was <rhyme label="b" type="m" corresp="#s5:l2">ghepractiseert</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Daerom quam hy <rhyme label="c" type="f">ghevloghen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Naer Haerlem, soot wel <rhyme label="d" type="m">scheen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Daer was de Prins <rhyme label="c" type="f" corresp="#s5:l5">bedrogen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Van boven tot <rhyme label="d" type="m" corresp="#s5:l6">beneen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy zijt het Lant seer <rhyme label="a" type="f">schadich</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Tis jammer dat ghy <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De Prince end Grave <rhyme label="a" type="f" corresp="#s6:l1">ghenadich</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met al die hem <rhyme label="b" type="m" corresp="#s6:l2">aencleeft</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Dacht ghy toch al <rhyme label="c" type="f">waerachtich</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ter Vleeschbanck hebben <rhyme label="d" type="m">bracht</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Maer Godt die Heer <rhyme label="c" type="f" corresp="#s6:l5">almachtich</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Heeft voor ons wel <rhyme label="d" type="m" corresp="#s6:l6">ghewacht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Waer uwen dienst <rhyme label="a" type="f">ghetrouwe</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Somen van sulcken <rhyme label="b" type="m">leest</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Graef Lodewijck van <rhyme label="a" type="f" corresp="#s7:l1">Nassouwe</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Waer langh int Landt <rhyme label="b" type="m" corresp="#s7:l2">gheweest</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Den Duck met zijn <rhyme label="c" type="f">Bloethonden</rhyme></l>
        <l id="s7:l6" met="3" type="-+">En hadde dy niet <rhyme label="d" type="m">gheschilt</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Waerdy ghetrou <rhyme label="c" type="f" corresp="#s7:l5">ghevonden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Het Lant waer al <rhyme label="d" type="m" corresp="#s7:l6">ghestilt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghy bindt u selven <rhyme label="a" type="f">strecken</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Daer ghy in vallen <rhyme label="b" type="m">sult</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Hy sal u quaet <rhyme label="a" type="f" corresp="#s8:l1">ontdecken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Wanneer den tijt <rhyme label="b" type="m" corresp="#s8:l2">vervult</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Ghy sult u noch voor hem <rhyme label="c" type="f">schamen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Laet hem comen int <rhyme label="d" type="m">parck</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Den Edelen Heer van <rhyme label="c" type="f" corresp="#s8:l5">Namen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Den Grave vander <rhyme label="d" type="m" corresp="#s8:l6">Marck</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Daerom Edel Heer <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Schaft eenen goeden <rhyme label="b" type="m">moet</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Godt sal sparen u <rhyme label="a" type="f" corresp="#s9:l1">leven</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Spijt u Vyanden <rhyme label="b" type="m" corresp="#s9:l2">verwoet</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">En sal u sterckheyt <rhyme label="c" type="f">gheven</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Door zijn ghenade <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Op dat door u <rhyme label="c" type="f" corresp="#s9:l5">beseven</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Wy comen uut der <rhyme label="d" type="m" corresp="#s9:l6">noot</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Al was David <rhyme label="a" type="f">verdreven</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Door Sauls hooghen <rhyme label="b" type="m">moet</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Nochtans sachmen tlot <rhyme label="a" type="f" corresp="#s10:l1">keeren</rhyme></l>
        <l id="s10:l4" met="3" type="-+">De welcke lach onder <rhyme label="b" type="m" corresp="#s10:l2">voet</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">So sullen sy oock <rhyme label="c" type="f">beclaghen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Aenmerckt wel mijn <rhyme label="d" type="m">vermaen</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Dat zijt dorsten <rhyme label="c" type="f" corresp="#s10:l5">ghewaghen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">U Edelheyt te <rhyme label="d" type="m" corresp="#s10:l6">vaen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Sy hebben veel quaets <rhyme label="a" type="f">ghebrouwen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Door haer gheveynstheyt <rhyme label="b" type="m">groot</rhyme>:</l>
        <l id="s11:l3" met="3" type="-+">Oock Mannen ende <rhyme label="a" type="f" corresp="#s11:l1">Vrouwen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Ghebracht in lijden <rhyme label="b" type="m" corresp="#s11:l2">groot</rhyme>:</l>
        <l id="s11:l5" met="3" type="-+">Eer tLant sal sijn in <rhyme label="c" type="f">vreden</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Tsal costen menighen <rhyme label="d" type="m">Man</rhyme>:</l>
        <l id="s11:l7" met="3" type="-+">Daerom met ons <rhyme label="c" type="f" corresp="#s11:l5">ghebeden</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Laet ons Godt roepen <rhyme label="d" type="m" corresp="#s11:l6">aen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Bidt voor den wel <rhyme label="a" type="f">ghesenden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Prince seer <rhyme label="b" type="m">vermaert</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En Heeren in allen <rhyme label="a" type="f" corresp="#s12:l1">enden</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Daer Gods Woort wort <rhyme label="b" type="m" corresp="#s12:l2">verclaert</rhyme>:</l>
        <l id="s12:l5" met="3" type="-+">Den Graef vander Marck <rhyme label="c" type="f">ydoone</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Den Palsgraef op den <rhyme label="d" type="m">Rhijn</rhyme>:</l>
        <l id="s12:l7" met="3" type="-+">Voor Enghelant die <rhyme label="c" type="f" corresp="#s12:l5">Croone</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Mach wel ghebeden <rhyme label="d" type="m" corresp="#s12:l6">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
