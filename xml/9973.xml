<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9973</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zoete lieve reine maagd</melody>
  <author>Isaak A. van Vaerlen en Jan Jansz. van Asten</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">OP een <rhyme label="a" type="m">Pincxter-manendagh</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Sou Kees knol te Kermis <rhyme label="b" type="f">varen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">By de Boeren in 't <rhyme label="a" type="m" corresp="#s1:l1">gelagh</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daer de Huys-luy alle <rhyme label="b" type="f" corresp="#s1:l2">jaren</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Lustigh danssen op de <rhyme label="c" type="m">Veel</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En met haer verroeste <rhyme label="c" type="m" corresp="#s1:l5">Keel</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Singhen op de <rhyme label="b" type="f" corresp="#s1:l2">Snaren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Eerst een soopje <rhyme label="a" type="m">Brandewijn</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Heeft hy 's morghens <rhyme label="b" type="f">in-ghenomen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Van ontrent een half <rhyme label="a" type="m" corresp="#s2:l1">douzijn</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Mutsjes, wel magh 't jou <rhyme label="b" type="f" corresp="#s2:l2">bekomen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Docht ick, lieve <rhyme label="c" type="m">Keesje-maet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Als jy flus te Bedde <rhyme label="c" type="m" corresp="#s2:l5">gaet</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Wonder selje <rhyme label="b" type="f" corresp="#s2:l2">dromen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Daer mee teegh hy op het <rhyme label="a" type="m">padt</rhyme>.</l>
        <l id="s3:l2" met="4" type="+-">Met de aldereerste <rhyme label="b" type="f">Waghen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Daer hy daetelijck op <rhyme label="a" type="m" corresp="#s3:l1">sat</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Singhend' lustigh <rhyme label="b" type="f" corresp="#s3:l2">angheslagen</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Elck riep, siet hoe Keesje <rhyme label="c" type="m">bromt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Welkom Keesje, wel hae <rhyme label="c" type="m" corresp="#s3:l5">komt</rhyme></l>
        <l id="s3:l7" met="3" type="+-">'t Varcken by sijn <rhyme label="b" type="f" corresp="#s3:l2">Maghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Keesje teegh de <rhyme label="a" type="m">Wentel-trap</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Reghel-reckt om  hoogh nae <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Daetelijck sat Keesje <rhyme label="a" type="m" corresp="#s4:l1">schrap</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En hy leeck wel half <rhyme label="b" type="f" corresp="#s4:l2">bestoven</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Wacker doen een slapje <rhyme label="c" type="m">rondt</rhyme></l>
        <l id="s4:l6" met="4" type="+-">'t Minste teughje was een <rhyme label="c" type="m" corresp="#s4:l5">pondt</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Datse t'samen <rhyme label="b" type="f" corresp="#s4:l2">kloven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Keesje naer een uer drie <rhyme label="a" type="m">vier</rhyme></l>
        <l id="s5:l2" met="4" type="+-">(Deur het gieten, en het <rhyme label="b" type="f">gapen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Van het lecker <rhyme label="a" type="m" corresp="#s5:l1">Kermis-bier</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Uyt de Croon, by 't Haerlems <rhyme label="b" type="f" corresp="#s5:l2">Wapen</rhyme>)</l>
        <l id="s5:l5" met="4" type="+-">Kon niet langher doen <rhyme label="c" type="m">bescheydt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Daerom heeft hy hem <rhyme label="c" type="m" corresp="#s5:l5">gheleydt</rhyme></l>
        <l id="s5:l7" met="3" type="+-">In het Gras te <rhyme label="b" type="f" corresp="#s5:l2">slapen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Keesjen in sijn droomen <rhyme label="a" type="m">docht</rhyme></l>
        <l id="s6:l2" met="4" type="+-">(Soo hy niet en was <rhyme label="b" type="f">bedroghen</rhyme>)</l>
        <l id="s6:l3" met="4" type="+-">Dat hy teghen Dirrick <rhyme label="a" type="m" corresp="#s6:l1">vocht</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En sie daer 't en was gheen <rhyme label="b" type="f" corresp="#s6:l2">loghen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Want hy soo bespoghen <rhyme label="c" type="m">lagh</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat een yeder die hem <rhyme label="c" type="m" corresp="#s6:l5">sagh</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Stondt als <rhyme label="b" type="f" corresp="#s6:l2">opghetoghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Keesje deden sy <rhyme label="a" type="m">weerom</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Op een Waghen t'huyswaert <rhyme label="b" type="f">voeren</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dese schoone <rhyme label="a" type="m" corresp="#s7:l1">Pincxter-blom</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Kon noch Handt, noch Voet <rhyme label="b" type="f" corresp="#s7:l2">verroeren</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Was dit niet wel mooy ter <rhyme label="c" type="m">Feest</rhyme></l>
        <l id="s7:l6" met="4" type="+-">En te Kerremis <rhyme label="c" type="m" corresp="#s7:l5">gheweest</rhyme></l>
        <l id="s7:l7" met="3" type="+-">By de droncke <rhyme label="b" type="f" corresp="#s7:l2">Boeren</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">O ghy droncke Boeren <rhyme label="a" type="m">leert</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Uwe sinnen bet <rhyme label="b" type="f">bedaren</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">En wanneer ghy teert en <rhyme label="a" type="m" corresp="#s8:l1">smeert</rhyme></l>
        <l id="s8:l4" met="4" type="+-">In u Kermis alle <rhyme label="b" type="f" corresp="#s8:l2">jaeren</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Drinckt gheen heel noch halve <rhyme label="c" type="m">slap</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Dat ghy sonder <rhyme label="c" type="m" corresp="#s8:l5">dronckenschap</rhyme></l>
        <l id="s8:l7" met="3" type="+-">Vrolijck meught <rhyme label="b" type="f" corresp="#s8:l2">vergaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
