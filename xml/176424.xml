  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Liefde, ja <rhyme label="a" type="f">liefde</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wat is de liefde <rhyme label="b" type="m">blind</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wat baat al myn <rhyme label="a" type="f" corresp="#s1:l1">klagten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als gy myn tog niet <rhyme label="b" type="m" corresp="#s1:l2">bemind</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Wat baat al myn <rhyme label="c" type="f">klagten</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Van my aan u <rhyme label="d" type="m">gedaan</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Waar door ik moet <rhyme label="c" type="f" corresp="#s1:l5">versmagten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Waar door ik moet <rhyme label="d" type="m" corresp="#s1:l6">vergaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Weg Jongman met al uw <rhyme label="a" type="f">liefde</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Want uw liefde die is <rhyme label="b" type="m">blind</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Zonder uw kan ik nog <rhyme label="a" type="f" corresp="#s2:l1">leeven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Want ik ben nog maar een <rhyme label="b" type="m" corresp="#s2:l2">kind</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Ziet ik ben nog jong van <rhyme label="c" type="f">jaaren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Steld uw min vry uit uw <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Wilt de moeite voor my dog <rhyme label="c" type="f" corresp="#s2:l5">spaaren</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Zoek een ander die uw <rhyme label="d" type="m" corresp="#s2:l6">bemind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ach ach! wat droevige <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ach wat verdriet voor myn jong <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Want door uw <rhyme label="a" type="f" corresp="#s3:l1">liefdekoorden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Zyn myn zinnen gantsch <rhyme label="b" type="m" corresp="#s3:l2">verwerd</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Daar ik ging geheele <rhyme label="c" type="f">nagten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Door hagel, storm, sneeuw en <rhyme label="d" type="m">wind</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Op myn zoet lief stond te <rhyme label="c" type="f" corresp="#s3:l5">wagten</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En nog blyf ik <rhyme label="d" type="m" corresp="#s3:l6">onbemind</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik zal uw altyd <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ja zo lang ik Jongman <rhyme label="b" type="m">leef</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En uw altyd eer <rhyme label="a" type="f" corresp="#s4:l1">bewyzen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die my zulk een liefde <rhyme label="b" type="m" corresp="#s4:l2">geeft</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Maar â€™t zal uw tog nooit <rhyme label="c" type="f">gebeuren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Lief dat gy myn liefde <rhyme label="d" type="m">wint</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Daarom houd maar op van <rhyme label="c" type="f" corresp="#s4:l5">treuren</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Zoek een ander die uw <rhyme label="d" type="m" corresp="#s4:l6">bemind</rhyme>.</l>
      </lg>
    </lg>
  </text>
