    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="gezongen onze vader" translation="sung Lord's prayer">gezongen onze vader</tag>
    </tags>
    <melody>Het waren negen soldaten</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHeluck welvaert en <rhyme label="a" type="f">Eere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Moet u van Godt <rhyme label="b" type="m">gheschien</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Door Christum onsen <rhyme label="a" type="f" corresp="#s1:l1">Heere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Laet ons zijn lof <rhyme label="a" type="f" corresp="#s1:l1">vermeeren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Nu en ten allen <rhyme label="b" type="m" corresp="#s1:l2">tien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al die hier zijn <rhyme label="a" type="f">gheseten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Deur Christum verheven <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om drincken en om <rhyme label="a" type="f" corresp="#s2:l1">eeten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En laet ons niet <rhyme label="a" type="f" corresp="#s2:l1">vergeten</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Diet ons al ghegheven <rhyme label="b" type="m" corresp="#s2:l2">heeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy is ons Godt <rhyme label="a" type="f">almachtich</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wy staen al in zijnder <rhyme label="b" type="m">hant</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Laet ons hem loven <rhyme label="a" type="f" corresp="#s3:l1">eendrachtich</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hy sal ons wt liefden <rhyme label="a" type="f" corresp="#s3:l1">crachtich</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Schencken zijn rijcke <rhyme label="b" type="m" corresp="#s3:l2">plaisant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy is der sonden <rhyme label="a" type="f">ontlader</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dus smaeckt zijnder vreuchden <rhyme label="b" type="m">keest</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Eet en drinckt alle <rhyme label="a" type="f" corresp="#s4:l1">gader</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Het wort ons ghesont vanden <rhyme label="a" type="f" corresp="#s4:l1">vader</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">den soon en den heyligen <rhyme label="b" type="m" corresp="#s4:l2">geest</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laet ons te samen <rhyme label="a" type="f">spreecken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">den heere lof prijs en <rhyme label="b" type="m">danck</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Sijn gratie is hier <rhyme label="a" type="f" corresp="#s5:l1">ghebleecken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">An ons menschen vol van <rhyme label="a" type="f" corresp="#s5:l1">ghebreecken</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">die hier versaedt zijn met spijse en <rhyme label="b" type="m" corresp="#s5:l2">dranck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Laet ons gehoorsaem <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En altijt zijnen lof <rhyme label="b" type="m">vermeert</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wy zijn, zijn kinders <rhyme label="a" type="f" corresp="#s6:l1">ghepresen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Alsoo wy in Matheeus <rhyme label="a" type="f" corresp="#s6:l1">leesen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">die ons aldus Bidden, <rhyme label="b" type="m" corresp="#s6:l2">leert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Onsen vader vol alder <rhyme label="a" type="f">same</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">die daer inden hemel <rhyme label="b" type="m">zijdt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Gheheylicht sy uwen <rhyme label="a" type="f" corresp="#s7:l1">name</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">V Rijcke toecome <rhyme label="a" type="f" corresp="#s7:l1">bequame</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Vwen wille schiede <rhyme label="b" type="m" corresp="#s7:l2">altijt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Inden Hemel als opder <rhyme label="a" type="f">aerden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gheeft ons heden ons daghelijckx <rhyme label="b" type="m">broot</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En vergheeft ons Heere <rhyme label="a" type="f" corresp="#s8:l1">volwaerden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Onsen schulder die ons oyt <rhyme label="a" type="f" corresp="#s8:l1">deerden</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wy vergevense al cleen en <rhyme label="b" type="m" corresp="#s8:l2">groot</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En laet ons tot gheender <rhyme label="a" type="f">spatie</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">O Heere deur onse <rhyme label="b" type="m">misdaet</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Niet vallen in gheen <rhyme label="a" type="f" corresp="#s9:l1">temtatie</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Maer deur u groote <rhyme label="a" type="f" corresp="#s9:l1">gratie</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Maer verlost ons heere van alle <rhyme label="b" type="m" corresp="#s9:l2">quaet</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Want u is dat rijcke vol <rhyme label="a" type="f">vreeden</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Verhoort ons o Hemelschen <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Die cracht en Heerlick <rhyme label="a" type="f" corresp="#s10:l1">heeden</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Van nu tot inder eeuwicheyt <rhyme label="a" type="f" corresp="#s10:l1">mede</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Amen seggen wy voor een <rhyme label="b" type="m" corresp="#s10:l2">slodt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
