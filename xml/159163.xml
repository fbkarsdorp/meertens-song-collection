  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Kund gy dus de liefde <rhyme label="a" type="f">haten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dwase Damon, die   elk <rhyme label="b" type="m">eerd</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En haar om de Jagt <rhyme label="a" type="f" corresp="#s1:l1">verlaten</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">Dat is Venus <rhyme label="b" type="m" corresp="#s1:l2">gebraveerd</rhyme>!</l>
        <l id="s1:l5" met="4" type="+-">Spot vry met het soete <rhyme label="c" type="f">minnen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'k Hoop dat gy door hare <rhyme label="d" type="m">kragt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Haast   sult sien uw hert <rhyme label="c" type="f" corresp="#s1:l5">verwinnen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Heeft de Minne-god nog <rhyme label="d" type="m" corresp="#s1:l6">magt</rhyme>.</l>
      </lg>
    </lg>
  </text>
