  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mijn boel daer al mijn vruecht an <rhyme label="a" type="m">steit</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Doet mir onstede al eist mi <rhyme label="a" type="m" corresp="#s1:l1">leit</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Daer up so acht hi <rhyme label="b" type="f">cleine</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Riet dat metten winde <rhyme label="a" type="m" corresp="#s1:l1">gheit</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Heift vele met hem <rhyme label="b" type="f" corresp="#s1:l3">ghemeine</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Wat baedt ghezonghen of <rhyme label="a" type="m" corresp="#s1:l1">gheseit</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Trauwe scaemte ende <rhyme label="a" type="m" corresp="#s1:l1">stedicheit</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ontgaet hem scone ende <rhyme label="b" type="f" corresp="#s1:l3">reine</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mijn boel daer al mijn vruecht an <rhyme label="a" type="m">steit</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Doet mir onstede al eist mi <rhyme label="a" type="m" corresp="#s2:l1">leit</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Daer up so acht hi <rhyme label="b" type="f">cleine</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Wankel herte onstedich <rhyme label="a" type="m" corresp="#s2:l1">pleit</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Hets al verloren <rhyme label="b" type="f" corresp="#s2:l3">aerbeit</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Dat men om di <rhyme label="a" type="m" corresp="#s2:l1">beweine</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Du begheers der trauwen <rhyme label="a" type="m" corresp="#s2:l1">heit</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Maer dune does selve <rhyme label="b" type="f" corresp="#s2:l3">gheine</rhyme></l>
      </lg>
    </lg>
