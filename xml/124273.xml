<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>124273</id>
    <date>
      <year-start>1860</year-start>
      <year-end>1860</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>In het duister der bossen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">In het duistere der <rhyme label="a" type="f">bosschen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In een hol met loof <rhyme label="b" type="m">bedekt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Rust de dapperste der <rhyme label="c" type="f">Roovers</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Tot dat hem zijn Rosa <rhyme label="d" type="m">wekt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Rinaldini! wilt <rhyme label="a" type="f">ontwaken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Roept zij minzaam, <rhyme label="b" type="m">welgemoed</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Uwe gezellen zijn reeds <rhyme label="c" type="f">wakker</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En de zon verspreidt haar <rhyme label="d" type="m">gloed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hij, hij opent flaauw zijn <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Lonkt haar vriendlijk in 't <rhyme label="b" type="m">gezigt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zij zinkt zachtjes in zijn <rhyme label="c" type="f">armen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Sluit zijn mond met kusjes <rhyme label="d" type="m">digt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Thans hoort men de honden <rhyme label="a" type="f">keffen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ieder haast zich even <rhyme label="b" type="m">zeer</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En maakt zich gereed ten <rhyme label="c" type="f">strijde</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En laadt dubbel zijn <rhyme label="d" type="m">geweer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Rinaldini, wel <rhyme label="a" type="f">gewapend</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Treedt in 't midden van den <rhyme label="b" type="m">hoop</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Goede morgen, <rhyme label="c" type="f">kameraden</rhyme>!</l>
        <l id="s5:l4" met="4" type="+-">Wat is hier zoo vroeg te <rhyme label="d" type="m">koop</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Onze vijand is <rhyme label="a" type="f">gewapend</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Nadert ons met alle <rhyme label="b" type="m">man</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Maar zij zullen <rhyme label="c" type="f">ondervinden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Hoe een boschman strijden <rhyme label="d" type="m">kan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wij, wij moeten <rhyme label="a" type="f">zegenpralen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Roepen allen, dit zij 't <rhyme label="b" type="m">woord</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Zoodat bosch en berg en <rhyme label="c" type="f">dalen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Er van druischen door dit <rhyme label="d" type="m">oord</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ziet hen vechten, hoort hen <rhyme label="a" type="f">strijden</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Elk verdubbeld hier zijn <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Maar vergeefs - zij moeten <rhyme label="c" type="f">wijken</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Spillen nutteloos hun <rhyme label="d" type="m">bloed</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Rinaldini! <rhyme label="a" type="f">ingesloten</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Slaat zich dapper door het <rhyme label="b" type="m">rot</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En vindt achter steile <rhyme label="c" type="f">rotsen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Schuilplaats op een ad'lijk <rhyme label="d" type="m">slot</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Achter oude, donkre <rhyme label="a" type="f">muren</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Smaakt hij weÃªr het <rhyme label="b" type="m">mingeluk</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Dianora komt hem <rhyme label="c" type="f">troosten</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Met een zachte <rhyme label="d" type="m">handendruk</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Rinaldini! lieve <rhyme label="a" type="f">Roover</rhyme>!</l>
        <l id="s11:l2" met="4" type="+-">Gij rooft vrouw- en <rhyme label="b" type="m">kinderhart</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Gij zijt vurig in het <rhyme label="c" type="f">strijden</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Minzaam in de liefde en <rhyme label="d" type="m">smart</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Rinaldini, laat u <rhyme label="a" type="f">raden</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Sprak zij minzaam, <rhyme label="b" type="m">welgemoed</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Hier zijn passen en ook <rhyme label="c" type="f">paden</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Nu vertrek met alle <rhyme label="d" type="m">spoed</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Ach, daar vlugt hij nu, die <rhyme label="a" type="f">dapp're</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Van zijn volkje gansch <rhyme label="b" type="m">ontbloot</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Maar vindt toch zijn Rosa <rhyme label="c" type="f">weder</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Die hij mint tot in den <rhyme label="d" type="m">dood</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
