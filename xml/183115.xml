  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'k Heb in Herfstmaand noit <rhyme label="a" type="f">vernomen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Eene frisse <rhyme label="b" type="m">Lely-blom</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want haar kragt is dan <rhyme label="a" type="f" corresp="#s1:l1">benomen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En legd tot der aarde <rhyme label="b" type="m" corresp="#s1:l2">krom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">In October wil 't niet <rhyme label="a" type="f">zeggen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dan heeft zy gansch <rhyme label="b" type="m">afgedaan</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dan gaat zy ter aarde <rhyme label="a" type="f" corresp="#s2:l1">leggen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dan blyft 'er geen Lely <rhyme label="b" type="m" corresp="#s2:l2">staan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dan is zy ter <rhyme label="a" type="f">neêrgezegen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dan legt ze ter aarde <rhyme label="b" type="m">plat</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En kan tegen koude of <rhyme label="a" type="f" corresp="#s3:l1">regen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Maar men vind nog wel een <rhyme label="b" type="m" corresp="#s3:l2">blad</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dat hem dan komt uit te <rhyme label="a" type="f">strekken</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Plat ter neder op de <rhyme label="b" type="m">grond</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'t Kan nog kwaade zweeren <rhyme label="a" type="f" corresp="#s4:l1">dekken</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Zegt men met een volle <rhyme label="b" type="m" corresp="#s4:l2">mond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maar plant gy <rhyme label="a" type="f">Oranje-pitten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">'t Geeft een vaste en frisse <rhyme label="b" type="m">kleur</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En weet van geen koude of <rhyme label="a" type="f" corresp="#s5:l1">hitte</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Het blyft altoos in zyn <rhyme label="b" type="m" corresp="#s5:l2">fleur</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Gy kunt my geen tyd <rhyme label="a" type="f">opnoemen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Zelf in 't felste van de <rhyme label="b" type="m">kou</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Of men vind <rhyme label="a" type="f" corresp="#s6:l1">Oranje-bloemen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Wier kleur ik voor de beste <rhyme label="b" type="m" corresp="#s6:l2">hou</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die zal nimmermeer <rhyme label="a" type="f">verstikken</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Die zal nimmermeer <rhyme label="b" type="m">vergaan</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En die kleur kan ons <rhyme label="a" type="f" corresp="#s7:l1">verkwikken</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Viva de <rhyme label="b" type="m" corresp="#s7:l2">Oranje-blaân</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Viva de <rhyme label="a" type="f">Oranje-klanten</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Viva wat Oranje <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Wil Oranje in uw hart <rhyme label="a" type="f" corresp="#s8:l1">planten</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Het de bloem der wereld <rhyme label="b" type="m" corresp="#s8:l2">is</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
