    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SAl ick spreecken ofte <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van t'leet dat my nu is <rhyme label="a" type="m" corresp="#s1:l1">geschiet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Sal ick schaemen dit te <rhyme label="b" type="f">claeghen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Daer ick met t'gemoet niet <rhyme label="c" type="m">van</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Swijghen <rhyme label="c" type="m" corresp="#s1:l4">can</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Het en is niet te <rhyme label="b" type="f" corresp="#s1:l3">verdraeghen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Ick seght' haer <rhyme label="c" type="m" corresp="#s1:l4">dan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als ick peys maer op dees <rhyme label="a" type="m">wee</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Berst my het hert by naer in <rhyme label="a" type="m" corresp="#s2:l1">twee</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Door de Clappers die uyt <rhyme label="b" type="f">strooden</rhyme></l>
        <l id="s2:l4" met="4" type="+-">By mijn liefste quaet <rhyme label="c" type="m">fenijn</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Om mijn <rhyme label="c" type="m" corresp="#s2:l4">pijn</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van mijn liefde doen in <rhyme label="b" type="f" corresp="#s2:l3">t'dooden</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Veracht te <rhyme label="c" type="m" corresp="#s2:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Lief ist moghelijck dat <rhyme label="a" type="m">wert</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Door een quaey tongh verdraeyt u <rhyme label="a" type="m" corresp="#s3:l1">hert</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat met mijn soo was <rhyme label="b" type="f">ghebonden</rhyme></l>
        <l id="s3:l4" met="4" type="+-">En gevest in trouwe <rhyme label="c" type="m">min</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Is dat <rhyme label="c" type="m" corresp="#s3:l4">in</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Haet en nijt soo haest <rhyme label="b" type="f" corresp="#s3:l3">versonden</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Op my <rhyme label="c" type="m" corresp="#s3:l4">Goddin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Liefste waer om ghy <rhyme label="a" type="m">ghelooft</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Die my by u mijn eer <rhyme label="a" type="m" corresp="#s4:l1">berooft</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Leugens connen noyt <rhyme label="b" type="f">beclijven</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Oft sy stelen nu mijn <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">k'Weet o <rhyme label="c" type="m" corresp="#s4:l4">Heer</rhyme>!</l>
        <l id="s4:l6" met="4" type="+-">d'Oli sal noch boven <rhyme label="b" type="f" corresp="#s4:l3">drijven</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Op sijnen <rhyme label="c" type="m" corresp="#s4:l4">keer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Schoon Princers die ick <rhyme label="a" type="m">bemin</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ke laet dit vaeren uyt den <rhyme label="a" type="m" corresp="#s5:l1">sin</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Op Dat ghy door quade <rhyme label="b" type="f">tongen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Maeckt gheen Cephale van <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Die in <rhyme label="c" type="m" corresp="#s5:l4">ly</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Quamp door Procris haet <rhyme label="b" type="f" corresp="#s5:l3">ghesonghen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Van <rhyme label="c" type="m" corresp="#s5:l4">Clapperny</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
