<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>174427</id>
    <date>
      <year-start>1620</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="New Year's song">nieuwjaarslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">T'is nu den rechten <rhyme label="a" type="m">tyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">met vlijt wilt <rhyme label="b" type="f">visenteren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">arm Bethlehem die <rhyme label="a" type="m" corresp="#s1:l1">zyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">veracht deur t'cleyn <rhyme label="b" type="f" corresp="#s1:l2">generen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">u licht Int <rhyme label="b" type="f" corresp="#s1:l2">ommekeeren</rhyme></l>
        <l id="s1:l6" met="2" type="-+">laet schynen <rhyme label="c" type="f">clare</rhyme></l>
        <l id="s1:l7" met="3" type="-+">volgt zo t' bevel des <rhyme label="b" type="f" corresp="#s1:l2">heeren</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s1:l6">Iare</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De meeste vreucht <rhyme label="a" type="m">geacht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">kersnacht, hoorden <rhyme label="b" type="f">geschieden</rhyme></l>
        <l id="s2:l3" met="3" type="-+">de harders, op de <rhyme label="a" type="m" corresp="#s2:l1">wacht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die meenden wech te <rhyme label="b" type="f" corresp="#s2:l2">vlieden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">deur gods Engels <rhyme label="b" type="f" corresp="#s2:l2">gebieden</rhyme></l>
        <l id="s2:l6" met="2" type="-+">en hemels <rhyme label="c" type="f">schare</rhyme></l>
        <l id="s2:l7" met="3" type="-+">verlicht veel slechte <rhyme label="b" type="f" corresp="#s2:l2">lieden</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s2:l6">Iare</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als harders opten <rhyme label="a" type="m">velt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">vertelt gods wonder <rhyme label="b" type="f">daden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">vanden vader, <rhyme label="a" type="m" corresp="#s3:l1">bestelt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">al uijt louter <rhyme label="b" type="f" corresp="#s3:l2">genaden</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Belast ende <rhyme label="b" type="f" corresp="#s3:l2">beladen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">vertroost te <rhyme label="c" type="f">garen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">die hongert wilt <rhyme label="b" type="f" corresp="#s3:l2">versaden</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s3:l6">Iare</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Blijschap van <rhyme label="a" type="m">geboort</rhyme></l>
        <l id="s4:l2" met="3" type="-+">wilt voort prysen en <rhyme label="b" type="f">loven</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Blijff In lieffden <rhyme label="a" type="m" corresp="#s4:l1">accoort</rhyme></l>
        <l id="s4:l4" met="3" type="-+">die vrede sent van <rhyme label="b" type="f" corresp="#s4:l2">Boven</rhyme></l>
        <l id="s4:l5" met="3" type="-+">gods hulpe wij <rhyme label="b" type="f" corresp="#s4:l2">behoven</rhyme></l>
        <l id="s4:l6" met="2" type="-+">als <rhyme label="c" type="f">middelare</rhyme></l>
        <l id="s4:l7" met="3" type="-+">zyn woort laet nijet <rhyme label="b" type="f" corresp="#s4:l2">verdoven</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s4:l6">Iare</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Met Simeon maeckt <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s5:l2" met="3" type="-+">die t' broot des hemels <rhyme label="b" type="f">smaeckte</rhyme></l>
        <l id="s5:l3" met="3" type="-+">en nijet zou sien de <rhyme label="a" type="m" corresp="#s5:l1">Doot</rhyme></l>
        <l id="s5:l4" met="3" type="-+">eer hij Christum an <rhyme label="b" type="f" corresp="#s5:l2">raeckte</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Gods naem hij oock groot <rhyme label="b" type="f" corresp="#s5:l2">maeckte</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Int <rhyme label="c" type="f">openbare</rhyme></l>
        <l id="s5:l7" met="3" type="-+">veel vrede hem <rhyme label="b" type="f" corresp="#s5:l2">genaeckte</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s5:l6">Iare</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De ster gaet noch voor <rhyme label="a" type="m">heen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In treen wilt met <rhyme label="b" type="f">bewegen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Om zo de naeckte <rhyme label="a" type="m" corresp="#s6:l1">leen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Te cleen van godes <rhyme label="b" type="f" corresp="#s6:l2">zegen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">als wijzen zyt <rhyme label="b" type="f" corresp="#s6:l2">genegen</rhyme></l>
        <l id="s6:l6" met="2" type="-+">tot vreucht <rhyme label="c" type="f">eenpare</rhyme></l>
        <l id="s6:l7" met="3" type="-+">den vyant wort <rhyme label="b" type="f" corresp="#s6:l2">verslegen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s6:l6">Iare</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Prophetij <rhyme label="a" type="m">vervult</rhyme></l>
        <l id="s7:l2" met="3" type="-+">ons schult voldaen voor <rhyme label="b" type="f">sonden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">gelovich hebt <rhyme label="a" type="m" corresp="#s7:l1">gedult</rhyme></l>
        <l id="s7:l4" met="3" type="-+">die lijt verscheyden <rhyme label="b" type="f" corresp="#s7:l2">stonden</rhyme></l>
        <l id="s7:l5" met="3" type="-+">waer achtich wort <rhyme label="b" type="f" corresp="#s7:l2">bevonden</rhyme></l>
        <l id="s7:l6" met="2" type="-+">geen druve <rhyme label="c" type="f">mare</rhyme></l>
        <l id="s7:l7" met="3" type="-+">schriftuyr wilt wel <rhyme label="b" type="f" corresp="#s7:l2">deurgronden</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s7:l6">Iare</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Een Prins, en vorst <rhyme label="a" type="m">bekent</rhyme></l>
        <l id="s8:l2" met="3" type="-+">gesent tot ons <rhyme label="b" type="f">Bereyen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">want lieffd'es t' <rhyme label="a" type="m" corresp="#s8:l1">fondament</rhyme></l>
        <l id="s8:l4" met="3" type="-+">voor witte <rhyme label="b" type="f" corresp="#s8:l2">acoleyen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Goet nijt den quaden <rhyme label="b" type="f" corresp="#s8:l2">scheijen</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Meester <rhyme label="c" type="f">Dienare</rhyme></l>
        <l id="s8:l7" met="3" type="-+">recht oordeel wilt <rhyme label="b" type="f" corresp="#s8:l2">verbeyen</rhyme></l>
        <l id="s8:l8" met="2" type="-+">Int nieuwe <rhyme label="c" type="f" corresp="#s8:l6">Iare</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
