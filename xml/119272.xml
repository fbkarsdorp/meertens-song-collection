  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">PYn en smert en droeve <rhyme label="a" type="f">plagen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Lei ik steeds by nacht en <rhyme label="a" type="f" corresp="#s1:l1">dagen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Om myn <rhyme label="b" type="m">Godin</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Myne lieve <rhyme label="b" type="m" corresp="#s1:l3">Engelin</rhyme>,</l>
        <l id="s1:l5" met="1" type="-+">Ik <rhyme label="c" type="m">voel</rhyme>,</l>
        <l id="s1:l6" met="1" type="-+"><rhyme label="c" type="m" corresp="#s1:l5">Gewoel</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En iets wonders door myn <rhyme label="d" type="f">leden</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-"><rhyme label="e" type="m">Carileen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Ach! ik <rhyme label="e" type="m" corresp="#s1:l8">ween</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Wilt dog hoore na myn <rhyme label="d" type="f" corresp="#s1:l7">reden</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Of ik <rhyme label="f" type="m">stik</rhyme>,</l>
        <l id="s1:l12" met="2" type="+-">Door de <rhyme label="f" type="m" corresp="#s1:l11">schrik</rhyme>,</l>
        <l id="s1:l13" met="4" type="+-">Van een droeve <rhyme label="f" type="m" corresp="#s1:l11">minnesnik</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Laat ik in uw boezem <rhyme label="a" type="f">wonen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Laat ik uwe roode <rhyme label="a" type="f" corresp="#s2:l1">konen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Met kus, op <rhyme label="b" type="m">kus</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Drukken dat ik dog eens <rhyme label="b" type="m" corresp="#s2:l3">blus</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">Myn <rhyme label="c" type="m">gloed</rhyme>,</l>
        <l id="s2:l6" met="1" type="-+">Zo <rhyme label="c" type="m" corresp="#s2:l5">zoet</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ziet wie dat uw houd in <rhyme label="d" type="f">waarde</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Ei laat <rhyme label="e" type="m">af</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Zyt niet <rhyme label="e" type="m" corresp="#s2:l8">straf</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Schoonste dien ik vond op <rhyme label="d" type="f" corresp="#s2:l7">aarde</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-"><rhyme label="f" type="m">Carileen</rhyme>,</l>
        <l id="s2:l12" met="2" type="+-">Ach! ik <rhyme label="f" type="m" corresp="#s2:l11">ween</rhyme>,</l>
        <l id="s2:l13" met="4" type="+-">Blyft gy stuurs en zegt steeds <rhyme label="f" type="m" corresp="#s2:l11">neen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gund dat ik u in myn <rhyme label="a" type="f">armen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Mag op 't Bruilofts Bed <rhyme label="a" type="f" corresp="#s3:l1">verwarmen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Dat wy als <rhyme label="b" type="m">Twee</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Lief jes leven in veel <rhyme label="b" type="m" corresp="#s3:l3">vreê</rhyme>,</l>
        <l id="s3:l5" met="1" type="-+">Ach! <rhyme label="c" type="m">ach</rhyme>!</l>
        <l id="s3:l6" met="1" type="-+">Ik <rhyme label="c" type="m" corresp="#s3:l5">mag</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Of ik kan geen ander <rhyme label="d" type="f">minnen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Gy <rhyme label="e" type="m">alleen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Anders <rhyme label="e" type="m" corresp="#s3:l8">geen</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Zal ooit komen in myn <rhyme label="d" type="f" corresp="#s3:l7">zinnen</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Gund <rhyme label="f" type="m">gena</rhyme>,</l>
        <l id="s3:l12" met="2" type="+-">Zegt dan <rhyme label="f" type="m" corresp="#s3:l11">Ja</rhyme>,</l>
        <l id="s3:l13" met="4" type="+-">Eer ik door de min <rhyme label="f" type="m" corresp="#s3:l11">verga</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laat myn liefde uw <rhyme label="a" type="f">verwekke</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Laat ons t'zame <rhyme label="a" type="f" corresp="#s4:l1">trekkebekke</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Daar <rhyme label="b" type="m">minnevrucht</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Steeds verdryft de <rhyme label="b" type="m" corresp="#s4:l3">ongenucht</rhyme>,</l>
        <l id="s4:l5" met="1" type="-+">Gund <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s4:l6" met="1" type="-+">Om <rhyme label="c" type="m" corresp="#s4:l5">vry</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">U myn Deelgenood te <rhyme label="d" type="f">noemen</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Dat ik <rhyme label="e" type="m">mag</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Dag aan <rhyme label="e" type="m" corresp="#s4:l8">dag</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">U volmaakte leden <rhyme label="d" type="f" corresp="#s4:l7">roemen</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Lieve <rhyme label="f" type="m">Pand</rhyme>,</l>
        <l id="s4:l12" met="2" type="+-">'k Blyf <rhyme label="f" type="m" corresp="#s4:l11">constant</rhyme>,</l>
        <l id="s4:l13" met="4" type="+-">Ryk my dog uw regter <rhyme label="f" type="m" corresp="#s4:l11">hand</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
