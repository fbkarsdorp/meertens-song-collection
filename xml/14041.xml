<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14041</id>
    <date>
      <year-start>1621</year-start>
      <year-end>1621</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Come shepherds deck your heads</melody>
  <author>Jan Jansz. Starter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als u 't geluck toe <rhyme label="a" type="m">lacht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En ghy niet hoeft te <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dan word ghy hoogh <rhyme label="a" type="m" corresp="#s1:l1">geacht</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van Vrienden, en van <rhyme label="b" type="f" corresp="#s1:l2">Maghen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Maer so dat rad eens <rhyme label="c" type="m">wend</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En d'Hemel u <rhyme label="d" type="f">verlienden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Het alderminst <rhyme label="c" type="m" corresp="#s1:l5">elend</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Adieu dan hulp van <rhyme label="d" type="f" corresp="#s1:l6">Vrienden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">S'aensien u met de <rhyme label="a" type="m">neck</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En om-gekeerde <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wie heeft met u <rhyme label="a" type="m" corresp="#s2:l1">gebreck</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dan eenigh <rhyme label="b" type="f" corresp="#s2:l2">mededoghen</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Niemand, elck vriend is <rhyme label="c" type="m">vreemd</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">So haest men maer word <rhyme label="d" type="f">siende</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat rou u vreughd <rhyme label="c" type="m" corresp="#s2:l5">beneemd</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Adieu dan hulp van <rhyme label="d" type="f" corresp="#s2:l6">Vrienden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Van die u roem, u <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">U deughd, u <rhyme label="b" type="f">wetenschappen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Verhieven eerst so <rhyme label="a" type="m" corresp="#s3:l1">seer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Tot lofs verheven <rhyme label="b" type="f" corresp="#s3:l2">trappen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Word ghy so af <rhyme label="c" type="m">gemaeld</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat het nau slimmer <rhyme label="d" type="f">diende</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ach! als u voorspoed <rhyme label="c" type="m" corresp="#s3:l5">faeld</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Adieu dan hulp van <rhyme label="d" type="f" corresp="#s3:l6">Vrienden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men vind dan naulijcks <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dus is des noods <rhyme label="b" type="f">bedroeven</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Alleenigh de <rhyme label="a" type="m" corresp="#s4:l1">toetsteen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om Vrienden op te <rhyme label="b" type="f" corresp="#s4:l2">proeven</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dan sietmen door die <rhyme label="c" type="m">bril</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Hoe sy haer vriendschap <rhyme label="d" type="f">mienden</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Ach! als 't geluck niet <rhyme label="c" type="m" corresp="#s4:l5">wil</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Adieu dan hulp van <rhyme label="d" type="f" corresp="#s4:l6">Vrienden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dus yeder stel sijn <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sijn handel en sijn <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat hy na Vrienden <rhyme label="a" type="m" corresp="#s5:l1">baet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Geensins behoeft te <rhyme label="b" type="f" corresp="#s5:l2">geven</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Elck help sich so hy <rhyme label="c" type="m">mach</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat 's beter op het <rhyme label="d" type="f">thiende</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Want komt ghy met <rhyme label="c" type="m" corresp="#s5:l5">beklagh</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Adieu dan hulp van <rhyme label="d" type="f" corresp="#s5:l6">Vrienden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
