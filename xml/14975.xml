    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Laet sang en spel, tambour en <rhyme label="a" type="m">fluyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Nu klincken tot Gods <rhyme label="b" type="m">eer</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Dat Orgel, Chiter, Harp en <rhyme label="a" type="m" corresp="#s1:l1">Luyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Oock opgae voor den <rhyme label="b" type="m" corresp="#s1:l2">Heer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Die haest wel van// Ons keeren <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Duc d'Alve den <rhyme label="c" type="m" corresp="#s1:l5">Tyran</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gods goetheyt wesen moet <rhyme label="a" type="m">vertelt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die noch so voor ons <rhyme label="b" type="m">sorgt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En ons den Briel en Mase <rhyme label="a" type="m" corresp="#s2:l1">stelt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Als tot een vaste <rhyme label="b" type="m" corresp="#s2:l2">borgt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die haest wel van// Ons keeren <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Duc d'Alve den <rhyme label="c" type="m" corresp="#s2:l5">Tyran</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Spanjaert wert nu een <rhyme label="a" type="m">gebit</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In synen muyl <rhyme label="b" type="m">geleyt</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">God sy, die daer om hooge <rhyme label="a" type="m" corresp="#s3:l1">sit</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gedanckt in <rhyme label="b" type="m" corresp="#s3:l2">eeuwicheyt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Die haest wel van// Ons keeren <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Duc d'Alve den <rhyme label="c" type="m" corresp="#s3:l5">Tyran</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy Princen/ Heeren van ons <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Maeckt ons de Spanjaert <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Malcand'ren trouw'lyck biet de <rhyme label="a" type="m" corresp="#s4:l1">hand</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In Godes vrees' <rhyme label="b" type="m" corresp="#s4:l2">altyt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Die haest wel van// Ons keeren <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Duc d'Alve den <rhyme label="c" type="m" corresp="#s4:l5">Tyran</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
