<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35081</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1700</year-end>
    </date>
    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Pour quoi pourquoi o cher <rhyme label="a" type="m">iris</rhyme></l>
        <l id="s1:l2" met="3" type="-+">d'un coeur a vous tot <rhyme label="a" type="m" corresp="#s1:l1">sousmis</rhyme></l>
        <l id="s1:l3" met="2" type="-+">mourir la <rhyme label="b" type="f">souffrance</rhyme></l>
        <l id="s1:l4" met="2" type="-+">vostre <rhyme label="b" type="f" corresp="#s1:l3">contenance</rhyme></l>
        <l id="s1:l5" met="2" type="-+">si froide <rhyme label="c" type="f">m'accable</rhyme></l>
        <l id="s1:l6" met="3" type="-+">vous mavez seu <rhyme label="d" type="m">charmer</rhyme></l>
        <l id="s1:l7" met="2" type="-+">de mon sort <rhyme label="c" type="f" corresp="#s1:l5">lamentable</rhyme></l>
        <l id="s1:l8" met="3" type="-+">arrestez le <rhyme label="d" type="m" corresp="#s1:l6">progrÃªs</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dois je estre tousjours <rhyme label="a" type="m">malheureux</rhyme></l>
        <l id="s2:l2" met="3" type="-+">en adorant vos beaux <rhyme label="a" type="m" corresp="#s2:l1">ijeux</rhyme>?</l>
        <l id="s2:l3" met="2" type="-+">ai je fait un <rhyme label="b" type="f">crime</rhyme></l>
        <l id="s2:l4" met="2" type="-+">digne dune <rhyme label="b" type="f" corresp="#s2:l3">abime</rhyme></l>
        <l id="s2:l5" met="2" type="-+">des douleurs <rhyme label="c" type="f">penibles</rhyme></l>
        <l id="s2:l6" met="3" type="-+">ah que vos charmes <rhyme label="d" type="m">sont</rhyme></l>
        <l id="s2:l7" met="2" type="-+">restez vous <rhyme label="c" type="f" corresp="#s2:l5">insensible</rhyme></l>
        <l id="s2:l8" met="3" type="-+">aux doulereux <rhyme label="d" type="m" corresp="#s2:l6">eprons</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">je ne crains point des durs <rhyme label="a" type="m">climats</rhyme></l>
        <l id="s3:l2" met="3" type="-+">l'air ne m'epouvante <rhyme label="a" type="m" corresp="#s3:l1">pas</rhyme></l>
        <l id="s3:l3" met="2" type="-+">je membrasse <rhyme label="b" type="f">guerre</rhyme></l>
        <l id="s3:l4" met="2" type="-+">des maux de la <rhyme label="b" type="f" corresp="#s3:l3">terre</rhyme></l>
        <l id="s3:l5" met="2" type="-+">mais iris je <rhyme label="c" type="f">jure</rhyme></l>
        <l id="s3:l6" met="3" type="-+">que les tristes <rhyme label="d" type="m">eceuils</rhyme></l>
        <l id="s3:l7" met="2" type="-+">que pour vous <rhyme label="c" type="f" corresp="#s3:l5">jendure</rhyme></l>
        <l id="s3:l8" met="3" type="-+">me menent au <rhyme label="d" type="m" corresp="#s3:l6">cerceuil</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">toujours souffrir, et sans <rhyme label="a" type="m">espoir</rhyme></l>
        <l id="s4:l2" met="3" type="-+">iris est ce ton <rhyme label="a" type="m" corresp="#s4:l1">vouloir</rhyme></l>
        <l id="s4:l3" met="2" type="-+">faire moi la <rhyme label="b" type="f">grace</rhyme></l>
        <l id="s4:l4" met="2" type="-+">que ton coeur de <rhyme label="b" type="f" corresp="#s4:l3">glace</rhyme></l>
        <l id="s4:l5" met="2" type="-+">qui reduit en <rhyme label="c" type="f">cendres</rhyme></l>
        <l id="s4:l6" met="3" type="-+">par sa <rhyme label="d" type="m">severite</rhyme></l>
        <l id="s4:l7" met="2" type="-+">en coeur fidel et <rhyme label="c" type="f" corresp="#s4:l5">tendre</rhyme></l>
        <l id="s4:l8" met="3" type="-+">schasche que je <rhyme label="d" type="m" corresp="#s4:l6">l'aimois</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">si cela ne le peut <rhyme label="a" type="m">fleschir</rhyme></l>
        <l id="s5:l2" met="3" type="-+">je fais gloire de <rhyme label="a" type="m" corresp="#s5:l1">morir</rhyme></l>
        <l id="s5:l3" met="2" type="-+">pour vous <rhyme label="b" type="f">adorable</rhyme></l>
        <l id="s5:l4" met="2" type="-+">et suis je <rhyme label="b" type="f" corresp="#s5:l3">capable</rhyme></l>
        <l id="s5:l5" met="2" type="-+">de mourir ma <rhyme label="c" type="f">flame</rhyme></l>
        <l id="s5:l6" met="3" type="-+">iris apres ma <rhyme label="d" type="m">mort</rhyme></l>
        <l id="s5:l7" met="2" type="-+">encor serat mon <rhyme label="c" type="f" corresp="#s5:l5">ame</rhyme></l>
        <l id="s5:l8" met="3" type="-+">contente de son <rhyme label="d" type="m" corresp="#s5:l6">sort</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
