  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAria maghet <rhyme label="a" type="f">reene</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Des sondaers <rhyme label="b" type="m">toeuerlaet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ghi sijt schoon / suuer / <rhyme label="a" type="f" corresp="#s1:l1">reene</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Daer minen troost aen <rhyme label="b" type="m" corresp="#s1:l2">staet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al ben ick vol <rhyme label="a" type="f">misdaden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">O hoochste vrouwe <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Fonteyne van <rhyme label="a" type="f" corresp="#s2:l1">ghenaden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Toont doch v duecht aen <rhyme label="b" type="m" corresp="#s2:l2">mi</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vrijdt ons van allen <rhyme label="a" type="f">sonden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">O schoonste ooghen <rhyme label="b" type="m">opslach</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Op dat ick tot allen <rhyme label="a" type="f" corresp="#s3:l1">stonden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">V lief kint dienen <rhyme label="b" type="m" corresp="#s3:l2">mach</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In v is sulck <rhyme label="a" type="f">vermoghen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wat ghi aen God <rhyme label="b" type="m">begheert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Elck mach bi v <rhyme label="a" type="f" corresp="#s4:l1">verhoghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Troost mi scoon bloeme <rhyme label="b" type="m" corresp="#s4:l2">weert</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Aen v stelle ick al <rhyme label="a" type="f">tsamen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Mijn siele / lijf / ende <rhyme label="b" type="m">goet</rhyme> /</l>
        <l id="s5:l3" met="3" type="-+">O bloeme vry van <rhyme label="a" type="f" corresp="#s5:l1">blamen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Neemt mi in v <rhyme label="b" type="m" corresp="#s5:l2">behoet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Princesse hoochste <rhyme label="a" type="f">verheuen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Voer al dat God <rhyme label="b" type="m">gheboot</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Bewaert ons in dat <rhyme label="a" type="f" corresp="#s6:l1">leuen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Beschermt ons van alder <rhyme label="b" type="m" corresp="#s6:l2">noot</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
