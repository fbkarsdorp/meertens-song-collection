    <tags>
      <tag>christmas song</tag>
    </tags>
    <melody>O herders al zoetjes al zonder getier</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">O Herderkens al soetjens en sonder <rhyme label="a" type="m">getier</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Messias rust <rhyme label="a" type="m" corresp="#s1:l1">alhier</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m" corresp="#s1:l3">reyn</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Slaept bloem van Iesse <rhyme label="c" type="m">stam</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Nu slaept onnoosel <rhyme label="c" type="m" corresp="#s1:l5">Lam</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Slaept nieuw-gheboren Vorst van <rhyme label="d" type="m">Israel</rhyme>,</l>
        <l id="s1:l8" met="5" type="-+">Slaept cleynen Koningh, slaept <rhyme label="d" type="m" corresp="#s1:l7">Emanuel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Goe liekens van buyten al stillekens <rhyme label="a" type="m">al</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die komt in desen <rhyme label="a" type="m" corresp="#s2:l1">stel</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m" corresp="#s2:l3">reyn</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Slaept bloem van Iesse <rhyme label="c" type="m">stam</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Nu slaept onnoosel <rhyme label="c" type="m" corresp="#s2:l5">Lam</rhyme>,</l>
        <l id="s2:l7" met="5" type="-+">Slaept nieuw-gheboren Vorst van <rhyme label="d" type="m">Israel</rhyme>,</l>
        <l id="s2:l8" met="5" type="-+">Slaept cleynen Koningh, slaept <rhyme label="d" type="m" corresp="#s2:l7">Emanuel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Hout op u van stormen ghy noorden <rhyme label="a" type="m">wint</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">T'is uwen Heer dit <rhyme label="a" type="m" corresp="#s3:l1">Kindt</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m" corresp="#s3:l3">reyn</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Slaept bloem van Iesse <rhyme label="c" type="m">stam</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Nu slaept onnoosel <rhyme label="c" type="m" corresp="#s3:l5">Lam</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">Slaept nieuw-gheboren Vorst van <rhyme label="d" type="m">Israel</rhyme>,</l>
        <l id="s3:l8" met="5" type="-+">Slaept cleynen Koningh, slaept <rhyme label="d" type="m" corresp="#s3:l7">Emanuel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Nu Iesu, nu slaept nu isset al <rhyme label="a" type="m">ghestilt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Nu slaept alsoo ghy <rhyme label="a" type="m" corresp="#s4:l1">wilt</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Na, na, na Kindtje <rhyme label="b" type="m" corresp="#s4:l3">reyn</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Slaept bloem van Iesse <rhyme label="c" type="m">stam</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Nu slaept onnoosel <rhyme label="c" type="m" corresp="#s4:l5">Lam</rhyme>,</l>
        <l id="s4:l7" met="5" type="-+">Slaept nieuw-gheboren Vorst van <rhyme label="d" type="m">Israel</rhyme>,</l>
        <l id="s4:l8" met="5" type="-+">Slaept cleynen Koningh, slaept <rhyme label="d" type="m" corresp="#s4:l7">Emanuel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
