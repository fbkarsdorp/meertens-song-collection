    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody>Een kindje is ons geboren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s1:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s1:l1">thooren</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s1:l2">hem</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Drie Koningen <rhyme label="c" type="f">eendrachtigh</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Tot Jerusalem <rhyme label="d" type="m">zijn</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Komen soecken <rhyme label="c" type="f" corresp="#s1:l5">aendachtigh</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Den nieuwen Koningh <rhyme label="d" type="m" corresp="#s1:l6">fijn</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">In Orienten <rhyme label="e" type="f">lande</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Hadden sy <rhyme label="f" type="m">klaer</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Sijn sterre met <rhyme label="e" type="f" corresp="#s1:l9">verstande</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Ghesien <rhyme label="f" type="m" corresp="#s1:l10">voorwaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s2:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s2:l1">thooren</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s2:l2">hem</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Te Jerusalem <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Zijnde, hebben <rhyme label="d" type="m">gheseydt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Met kloecken moet, en <rhyme label="c" type="f" corresp="#s2:l5">sinnen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wy zijn hier wel <rhyme label="d" type="m" corresp="#s2:l6">bereydt</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Ghekomen van soo <rhyme label="e" type="f">verre</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Om 't kindt vol <rhyme label="f" type="m">deughdt</rhyme></l>
        <l id="s2:l11" met="3" type="+-">T' aenbidden, door sijn <rhyme label="e" type="f" corresp="#s2:l9">sterre</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Beweeght met <rhyme label="f" type="m" corresp="#s2:l10">vreughdt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s3:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s3:l1">thooren</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s3:l2">hem</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Maer doen Herodes <rhyme label="c" type="f">hoorde</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En die stadt dit <rhyme label="d" type="m">vernam</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Sy haer gheheel <rhyme label="c" type="f" corresp="#s3:l5">verstoorde</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hy vreesd' en wierdt seer <rhyme label="d" type="m" corresp="#s3:l6">gram</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Dat hy verliesen <rhyme label="e" type="f">soude</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Sijn Rijcke <rhyme label="f" type="m">groot</rhyme>:</l>
        <l id="s3:l11" met="3" type="+-">Daerom hy brenghen <rhyme label="e" type="f" corresp="#s3:l9">woude</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Het kindt ter <rhyme label="f" type="m" corresp="#s3:l10">doodt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s4:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s4:l1">thooren</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s4:l2">hem</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hy vraeghde vol <rhyme label="c" type="f">onvrede</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Waer 't kindt ghebooren <rhyme label="d" type="m">was</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">In Bethlehem die <rhyme label="c" type="f" corresp="#s4:l5">stede</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Soo was dat men daer <rhyme label="d" type="m" corresp="#s4:l6">las</rhyme>;</l>
        <l id="s4:l9" met="3" type="-+">Dus hebben 't die <rhyme label="e" type="f">Propheten</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Beschreven <rhyme label="f" type="m">net</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">En klaerlijck laten <rhyme label="e" type="f" corresp="#s4:l9">weten</rhyme></l>
        <l id="s4:l12" met="2" type="-+">In d' oude <rhyme label="f" type="m" corresp="#s4:l10">wet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s5:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s5:l1">thooren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s5:l2">hem</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Hy seyde tot de <rhyme label="c" type="f">Wijse</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met een gheveynst <rhyme label="d" type="m">ghelaet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Gaet met goeden <rhyme label="c" type="f" corresp="#s5:l5">advijse</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Soeckt neerstich met der <rhyme label="d" type="m" corresp="#s5:l6">daet</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Dat kindt, den Heer der <rhyme label="e" type="f">Heeren</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Soo groot <rhyme label="f" type="m">gheacht</rhyme>:</l>
        <l id="s5:l11" met="3" type="+-">Men seght hy sal <rhyme label="e" type="f" corresp="#s5:l9">vermeeren</rhyme></l>
        <l id="s5:l12" met="2" type="-+">Sijn rijck met <rhyme label="f" type="m" corresp="#s5:l10">kracht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s6:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s6:l1">thooren</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s6:l2">hem</rhyme></l>
        <l id="s6:l5" met="3" type="-+">En als ghy 't kindt <rhyme label="c" type="f">ghepresen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Hebt vonden, keert tot <rhyme label="d" type="m">my</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Met goed bericht van <rhyme label="c" type="f" corresp="#s6:l5">desen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Op dat ick 't selve <rhyme label="d" type="m" corresp="#s6:l6">vry</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Aenbidden mach, na <rhyme label="e" type="f">reden</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">In korter <rhyme label="f" type="m">stondt</rhyme>:</l>
        <l id="s6:l11" met="3" type="+-">Het heeft mijn hert <rhyme label="e" type="f" corresp="#s6:l9">door-sneden</rhyme>,</l>
        <l id="s6:l12" met="2" type="-+">En seer <rhyme label="f" type="m" corresp="#s6:l10">ghewondt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s7:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s7:l1">thooren</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s7:l2">hem</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Maer als de Wijse <rhyme label="c" type="f">quamen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Buyten die stadt <rhyme label="d" type="m">ghegaen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Met vreughde sy <rhyme label="c" type="f" corresp="#s7:l5">vernamen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">De sterre voor hen <rhyme label="d" type="m" corresp="#s7:l6">staen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Soo langh tot dat sy <rhyme label="e" type="f">vonden</rhyme></l>
        <l id="s7:l10" met="2" type="-+">Het kindt soo <rhyme label="f" type="m">kleen</rhyme></l>
        <l id="s7:l11" met="3" type="+-">By sijn' Moeder, <rhyme label="e" type="f" corresp="#s7:l9">ghewonden</rhyme></l>
        <l id="s7:l12" met="2" type="-+">In doeckskens <rhyme label="f" type="m" corresp="#s7:l10">reen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s8:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s8:l1">thooren</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s8:l2">hem</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Die Koninghen <rhyme label="c" type="f">aenbaden</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Het kindt soo <rhyme label="d" type="m">menighfout</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Offerden, wel <rhyme label="c" type="f" corresp="#s8:l5">beraden</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Myrrhe, Wieroock en <rhyme label="d" type="m" corresp="#s8:l6">Goudt</rhyme>:</l>
        <l id="s8:l9" met="3" type="-+">Sy eerden oock sijn' <rhyme label="e" type="f">Moeder</rhyme></l>
        <l id="s8:l10" met="2" type="-+">Die suyver <rhyme label="f" type="m">Maeght</rhyme>:</l>
        <l id="s8:l11" met="3" type="+-">Dit heeft onsen <rhyme label="e" type="f" corresp="#s8:l9">Behoeder</rhyme></l>
        <l id="s8:l12" met="2" type="-+">Al wel <rhyme label="f" type="m" corresp="#s8:l10">behaeght</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s9:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s9:l1">thooren</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s9:l2">hem</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Den Enghel heeft by <rhyme label="c" type="f">nachte</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Hen ghedaen sulck <rhyme label="d" type="m">bevel</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Te keeren, elck hem <rhyme label="c" type="f" corresp="#s9:l5">wachte</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Meer tot Herodes <rhyme label="d" type="m" corresp="#s9:l6">fel</rhyme>.</l>
        <l id="s9:l9" met="3" type="-+">Dies door verscheyden <rhyme label="e" type="f">weghen</rhyme></l>
        <l id="s9:l10" met="2" type="-+">Zijn sy <rhyme label="f" type="m">ghekeert</rhyme></l>
        <l id="s9:l11" met="3" type="+-">Naer huys, al met Godts <rhyme label="e" type="f" corresp="#s9:l9">seghen</rhyme>,</l>
        <l id="s9:l12" met="2" type="-+">Als men ons <rhyme label="f" type="m" corresp="#s9:l10">leert</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Een kindeken is ons <rhyme label="a" type="f">gebooren</rhyme></l>
        <l id="s10:l2" met="2" type="-+">in <rhyme label="b" type="m">Bethlehem</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Des hadd' Herodes <rhyme label="a" type="f" corresp="#s10:l1">thooren</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">dat scheen aen <rhyme label="b" type="m" corresp="#s10:l2">hem</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Wij bidden nu, en <rhyme label="c" type="f">singhen</rhyme>;</l>
        <l id="s10:l6" met="3" type="-+">Wy bidden met <rhyme label="d" type="m">ootmoet</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Dat Jesus ons wil <rhyme label="c" type="f" corresp="#s10:l5">bringhen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Al in sijn rijcke <rhyme label="d" type="m" corresp="#s10:l6">soet</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Daer hem sijn' Eng'len <rhyme label="e" type="f">loven</rhyme>,</l>
        <l id="s10:l10" met="2" type="-+">Tot aller <rhyme label="f" type="m">tijdt</rhyme></l>
        <l id="s10:l11" met="3" type="+-">Dat gun ons Godt van <rhyme label="e" type="f" corresp="#s10:l9">boven</rhyme>,</l>
        <l id="s10:l12" met="2" type="-+"><rhyme label="f" type="m" corresp="#s10:l10">Ghebenedijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
