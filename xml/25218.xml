  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Eer dat het licht ten onder <rhyme label="a" type="m">gae</rhyme></l>
        <l id="s1:l2" met="4" type="-+">aenbidden wij v o heere <rhyme label="b" type="m">vroem</rhyme></l>
        <l id="s1:l3" met="4" type="-+">op dat nae v gewoente <rhyme label="a" type="m" corresp="#s1:l1">drae</rhyme></l>
        <l id="s1:l4" met="4" type="-+">ghij ons behoede vande <rhyme label="b" type="m" corresp="#s1:l2">scroem</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ver van ons moeten droomen <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s2:l2" met="4" type="-+">des nachts veruaertheijt oeck sij <rhyme label="b" type="m">cleijn</rhyme></l>
        <l id="s2:l3" met="4" type="-+">bedwinck ons viand inde <rhyme label="a" type="m" corresp="#s2:l1">lijn</rhyme></l>
        <l id="s2:l4" met="4" type="-+">dat ons lichamen blijuen <rhyme label="b" type="m" corresp="#s2:l2">reijn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gunt ons vader almachtich <rhyme label="a" type="m">godt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">door Jesvm Christum <rhyme label="b" type="m">ombeureest</rhyme></l>
        <l id="s3:l3" met="4" type="-+">die met v leeft bouen <rhyme label="a" type="m" corresp="#s3:l1">gebot</rhyme></l>
        <l id="s3:l4" met="4" type="-+">ewich regneert met heijlich <rhyme label="b" type="m" corresp="#s3:l2">geest</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
