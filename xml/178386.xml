<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178386</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Juich Batavieren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Juicht <rhyme label="a" type="f">Batavieren</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">Verheugt in hart en <rhyme label="b" type="m">geest</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Vlegt weer <rhyme label="a" type="f" corresp="#s1:l1">Lauwrieren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Op het <rhyme label="b" type="m" corresp="#s1:l2">Geboorte-Feest</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Van Nassauws oudste <rhyme label="c" type="m">Zoon</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Verhef weer dankbaar <rhyme label="c" type="m" corresp="#s1:l5">toon</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Viva de jonge Fredrik, <rhyme label="d" type="m" corresp="#s1:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ziet eens Gods <rhyme label="a" type="f">wegen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">ô Brave <rhyme label="b" type="m">Burgerschaar</rhyme>!</l>
        <l id="s2:l3" met="2" type="-+">Hoe hy zyn <rhyme label="a" type="f" corresp="#s2:l1">zegen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Zo voor als <rhyme label="b" type="m" corresp="#s2:l2">naar</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Aan den Oranje <rhyme label="c" type="m">Boom</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Zo helder steld ten <rhyme label="c" type="m" corresp="#s2:l5">toon</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Viva de Vorst Oranje, <rhyme label="d" type="m" corresp="#s2:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dees Boom <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Scheen wel eens zond'r <rhyme label="b" type="m">Loot</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Den geest te <rhyme label="a" type="f" corresp="#s3:l1">geeven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Doch in den <rhyme label="b" type="m" corresp="#s3:l2">noot</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Schonk 't God'lyk <rhyme label="c" type="m">raadsbesluit</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Weer een Oranje <rhyme label="c" type="m" corresp="#s3:l5">Spruit</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Viva 't Oranje Stamhuis, <rhyme label="d" type="m" corresp="#s3:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">'t Is ons <rhyme label="a" type="f">geschonken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat aan dat Vorst'lyk <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Drie Paarlen <rhyme label="a" type="f" corresp="#s4:l1">pronken</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Vol eer en <rhyme label="b" type="m" corresp="#s4:l2">lof</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ons Erf-Prins zeer <rhyme label="c" type="m">vermaart</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Op dezen dag <rhyme label="c" type="m" corresp="#s4:l5">verjaart</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>:</l>
        <l id="s4:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s4:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Uw jonge <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">ô Vorst! ô Neêrlands <rhyme label="b" type="m">troost</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Doet al <rhyme label="a" type="f" corresp="#s5:l1">herleeven</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Voor Batoos <rhyme label="b" type="m" corresp="#s5:l2">Kroost</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Een Zuil, een <rhyme label="c" type="m">Palinuur</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Een Held in 't <rhyme label="c" type="m" corresp="#s5:l5">Staatsbestuur</rhyme>.</l>
        <l id="s5:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Viva de jonge Erf Prins, <rhyme label="d" type="m" corresp="#s5:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">'t Is nu <rhyme label="a" type="f">geleden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Een reeks van elf <rhyme label="b" type="m">Jaar</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Dat Neêrlands <rhyme label="a" type="f" corresp="#s6:l1">Steeden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dees blyde <rhyme label="b" type="m" corresp="#s6:l2">maar</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Van uw Geboorten <rhyme label="c" type="m">dag</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Met blydschap voor haar <rhyme label="c" type="m" corresp="#s6:l5">zag</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s6:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Op al uw <rhyme label="a" type="f">wegen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">O Willem, Willems <rhyme label="b" type="m">Zoon</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Schenkt Godt zyn <rhyme label="a" type="f" corresp="#s7:l1">zegen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Hy zy uw <rhyme label="b" type="m" corresp="#s7:l2">loon</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Uw Schilt en <rhyme label="c" type="m">Beukelaar</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Verjaar nog Jaar op <rhyme label="c" type="m" corresp="#s7:l5">Jaar</rhyme>!</l>
        <l id="s7:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Viva de Jonge Erf Prins, <rhyme label="d" type="m" corresp="#s7:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Uw Jeugt en <rhyme label="a" type="f">Leeven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Zy reeds aan Godt <rhyme label="b" type="m">gewyd</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Al uwe <rhyme label="a" type="f" corresp="#s8:l1">weegen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Alleen <rhyme label="b" type="m" corresp="#s8:l2">bereid</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Tot heil van Kerk en <rhyme label="c" type="m">Staat</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Der Vroomen <rhyme label="c" type="m" corresp="#s8:l5">toeverlaat</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Viva Oranjes Telg, <rhyme label="d" type="m" corresp="#s8:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Wat helpt uw <rhyme label="a" type="f">haaten</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">O lasterziek <rhyme label="b" type="m">gespuis</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Zou Godt <rhyme label="a" type="f" corresp="#s9:l1">verlaaten</rhyme></l>
        <l id="s9:l4" met="3" type="-+">'t Oranje <rhyme label="b" type="m" corresp="#s9:l2">Huis</rhyme>?</l>
        <l id="s9:l5" met="3" type="-+">Beloftens in haar <rhyme label="c" type="m">kragt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Schenkt hy aan 't <rhyme label="c" type="m" corresp="#s9:l5">Nageslagt</rhyme>.</l>
        <l id="s9:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Viva de Prins Oranje, <rhyme label="d" type="m" corresp="#s9:l7">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">O Heer der <rhyme label="a" type="f">Heeren</rhyme>!</l>
        <l id="s10:l2" met="3" type="-+">Uw zegen en uw <rhyme label="b" type="m">troost</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Wil die <rhyme label="a" type="f" corresp="#s10:l1">vermeeren</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Op 't Oranje <rhyme label="b" type="m" corresp="#s10:l2">Kroost</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Het Hoog Doorlugtig <rhyme label="c" type="m">Paar</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Bewaar nog Jaar op <rhyme label="c" type="m" corresp="#s10:l5">Jaar</rhyme>.</l>
        <l id="s10:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezée</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">Viva het Huis van Oranje, <rhyme label="d" type="m" corresp="#s10:l7">Hoezée</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
