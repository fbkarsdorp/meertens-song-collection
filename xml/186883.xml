<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186883</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>O Holland schoon gij leeft in vrede</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ontluikt ô lang verborgen <rhyme label="a" type="m">Zon</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Vertoon uw gulde <rhyme label="b" type="f">straalen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Helpt Dogters van den <rhyme label="a" type="m" corresp="#s1:l1">Helikon</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Ons Heldenmoed <rhyme label="b" type="f" corresp="#s1:l2">verhaalen</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Herleeft O Batavieren <rhyme label="c" type="m">moed</rhyme>!</l>
        <l id="s1:l6" met="4" type="-+">'s Lands-heil ontsteek elks Liefden <rhyme label="c" type="m" corresp="#s1:l5">gloed</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Men zie weer Zeegen <rhyme label="b" type="f" corresp="#s1:l2">praalen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Verknogte Zeeven nog geen <rhyme label="a" type="m">nood</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Ducht niets O Hollands <rhyme label="b" type="f">Leeuwen</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">Door d'Eendracht word het kleyne <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Uw macht dringt door de <rhyme label="b" type="f" corresp="#s2:l2">Eeuwen</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Verdeedigers van 't <rhyme label="c" type="m">Vaderland</rhyme>!</l>
        <l id="s2:l6" met="4" type="-+">Door kragt met Oordeel en <rhyme label="c" type="m" corresp="#s2:l5">Verstand</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Doet men den Vyand <rhyme label="b" type="f" corresp="#s2:l2">schreeuwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Beminnaers van den Vryen <rhyme label="a" type="m">staet</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Trouw Bond en <rhyme label="b" type="f">Landgenooten</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Nooit duurgekogte Vryheid <rhyme label="a" type="m" corresp="#s3:l1">smaet</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Wilt slaefsche dwang <rhyme label="b" type="f" corresp="#s3:l2">verstooten</rhyme>!</l>
        <l id="s3:l5" met="4" type="-+">Werpt 't lang gesleepte Jock aen <rhyme label="c" type="m">zy</rhyme>;</l>
        <l id="s3:l6" met="4" type="-+">Van Kromwels wrok en <rhyme label="c" type="m" corresp="#s3:l5">dwing'landy</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Uyt 't Engels' bloed <rhyme label="b" type="f" corresp="#s3:l2">gesprooten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'t Gaat wel, Held Zoutman <rhyme label="a" type="m">onvermoeyt</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">Doet d'overmacht zelfs <rhyme label="b" type="f">weyken</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Hy had den Engels'man <rhyme label="a" type="m" corresp="#s4:l1">ontschoeid</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En in ons Wal doen <rhyme label="b" type="f" corresp="#s4:l2">streyken</rhyme>!</l>
        <l id="s4:l5" met="4" type="-+">Had maer de Bruyn ons Vlood <rhyme label="c" type="m">verzeld</rhyme>;</l>
        <l id="s4:l6" met="4" type="-+">Wy duchten voor geen kleyn <rhyme label="c" type="m" corresp="#s4:l5">geweld</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Men had ze zien <rhyme label="b" type="f" corresp="#s4:l2">bezweyken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Amerika nu vry <rhyme label="a" type="m">verklaard</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Door de verëende <rhyme label="b" type="f">Staaten</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Met Vrankryk Spanjen wyd <rhyme label="a" type="m" corresp="#s5:l1">vermaerd</rhyme>!</l>
        <l id="s5:l4" met="3" type="-+">Zal ons Conventie <rhyme label="b" type="f" corresp="#s5:l2">Baeten</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Tot opbouw van een Vrêe <rhyme label="c" type="m">Verdrag</rhyme>!</l>
        <l id="s5:l6" met="4" type="-+">Maer eerst moet een besluytend <rhyme label="c" type="m" corresp="#s5:l5">slag</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Ons vryheid open <rhyme label="b" type="f" corresp="#s5:l2">laaten</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Herleeft ô braeve <rhyme label="a" type="m">Admiraels</rhyme></l>
        <l id="s6:l2" met="3" type="-+">De Ruyter, Tromp ô <rhyme label="b" type="f">Helden</rhyme>!</l>
        <l id="s6:l3" met="4" type="-+">Onsterfelyke <rhyme label="a" type="m" corresp="#s6:l1">Generaals</rhyme>!</l>
        <l id="s6:l4" met="3" type="-+">Die u ter Bretse <rhyme label="b" type="f" corresp="#s6:l2">stelden</rhyme>!</l>
        <l id="s6:l5" met="4" type="-+">Triumph ô Ed'le Nassouws <rhyme label="c" type="m">Bloed</rhyme>!</l>
        <l id="s6:l6" met="4" type="-+">'s Lands welzyn is uw <rhyme label="c" type="m" corresp="#s6:l5">Erfgoed</rhyme>!</l>
        <l id="s6:l7" met="3" type="-+">De Faam zal 't weer <rhyme label="b" type="f" corresp="#s6:l2">vermelden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wel aen nu Ed'le Batoos <rhyme label="a" type="m">kroost</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">Heldhafte <rhyme label="b" type="f">Vaderlanders</rhyme>!</l>
        <l id="s7:l3" met="4" type="-+">Verschaft ons weeder vreede <rhyme label="a" type="m" corresp="#s7:l1">Troost</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">ô Vryheids heil <rhyme label="b" type="f" corresp="#s7:l2">Voorstanders</rhyme>!</l>
        <l id="s7:l5" met="4" type="-+">Spaerd goed nog bloed ontziet geen <rhyme label="c" type="m">lyf</rhyme>!</l>
        <l id="s7:l6" met="4" type="-+">Begeeft vry Ouders kroost of <rhyme label="c" type="m" corresp="#s7:l5">Wyf</rhyme>!</l>
        <l id="s7:l7" met="3" type="-+">Als braev' oude <rhyme label="b" type="f" corresp="#s7:l2">Hollanders</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Kom Helden uit ons Gysbrechts <rhyme label="a" type="m">vest</rhyme>;</l>
        <l id="s8:l2" met="3" type="-+">Kloekmoedig op de <rhyme label="b" type="f">Lappen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Helpt mannen, doet uw trouwe <rhyme label="a" type="m" corresp="#s8:l1">best</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Tot bloey der <rhyme label="b" type="f" corresp="#s8:l2">Koopmanschappen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Amerikaense Rook <rhyme label="c" type="m">Tabak</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Ook Koffy Suyker wagt men <rhyme label="c" type="m" corresp="#s8:l5">strak</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Dan kan men nog eens <rhyme label="b" type="f" corresp="#s8:l2">grappen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wel haast herleeft ons <rhyme label="a" type="m">Visserey</rhyme>;</l>
        <l id="s9:l2" met="3" type="-+">Aan Hollands kust en <rhyme label="b" type="f">Wallen</rhyme>!</l>
        <l id="s9:l3" met="4" type="-+">Als 's Vyands vrees is aen een <rhyme label="a" type="m" corresp="#s9:l1">zey</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Zal ook zyn macht <rhyme label="b" type="f" corresp="#s9:l2">vervallen</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Want 't vry gevogten <rhyme label="c" type="m">Neederland</rhyme>!</l>
        <l id="s9:l6" met="4" type="-+">Getrouw aan Bond en Liefdens <rhyme label="c" type="m" corresp="#s9:l5">band</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Hout zig niet op met <rhyme label="b" type="f" corresp="#s9:l2">mallen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Het oog van Gods <rhyme label="a" type="m">voorzienigheid</rhyme>!</l>
        <l id="s10:l2" met="3" type="-+">Zal onze Vloot <rhyme label="b" type="f">verzellen</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">Bestierd met magt en wys <rhyme label="a" type="m" corresp="#s10:l1">beleyd</rhyme>!</l>
        <l id="s10:l4" met="3" type="-+">Ons vrees te leure <rhyme label="b" type="f" corresp="#s10:l2">stellen</rhyme>!</l>
        <l id="s10:l5" met="4" type="-+">Geen vuyl Tractaet ô Holland <rhyme label="c" type="m">schoon</rhyme>!</l>
        <l id="s10:l6" met="4" type="-+">Hegt u meer aan den Britten <rhyme label="c" type="m" corresp="#s10:l5">Kroon</rhyme>!</l>
        <l id="s10:l7" met="3" type="-+">Niets kan ons Vryheid <rhyme label="b" type="f" corresp="#s10:l2">kwellen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">O gy die 't al met Wysheid <rhyme label="a" type="m">stiert</rhyme>!</l>
        <l id="s11:l2" met="3" type="-+">En d'onschuld dog zult <rhyme label="b" type="f">wreeken</rhyme>!</l>
        <l id="s11:l3" met="4" type="-+">Ons waapenen in gunst <rhyme label="a" type="m" corresp="#s11:l1">bestierd</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Hoord hoe ons Helden <rhyme label="b" type="f" corresp="#s11:l2">smeeken</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Verleen ons kragten Heere <rhyme label="c" type="m">Godt</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Bestierder van ons tyd'lyk <rhyme label="c" type="m" corresp="#s11:l5">Lot</rhyme>.</l>
        <l id="s11:l7" met="3" type="-+">Van ons dog nooit <rhyme label="b" type="f" corresp="#s11:l2">geweeken</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Geen uytkomst Heer aen Volken <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Door u van alle <rhyme label="b" type="f">Tyden</rhyme>!</l>
        <l id="s12:l3" met="4" type="-+">Gestigt beschermt en vast <rhyme label="a" type="m" corresp="#s12:l1">geplant</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Laat 't zig in u <rhyme label="b" type="f" corresp="#s12:l2">verblyden</rhyme>!</l>
        <l id="s12:l5" met="4" type="-+">Wie Trouw en Bond verbrooken <rhyme label="c" type="m">heeft</rhyme>!</l>
        <l id="s12:l6" met="4" type="-+">Zelfs voor zyn eygen schaduw <rhyme label="c" type="m" corresp="#s12:l5">beeft</rhyme>!</l>
        <l id="s12:l7" met="3" type="-+">En moet de wraak <rhyme label="b" type="f" corresp="#s12:l2">vermyden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Verhaast een wel toeruste <rhyme label="a" type="m">Vloot</rhyme>!</l>
        <l id="s13:l2" met="3" type="-+">Om 's Vyands magt te <rhyme label="b" type="f">keeren</rhyme>!</l>
        <l id="s13:l3" met="4" type="-+">Spaer geen geschut; meyd kruid nog <rhyme label="a" type="m" corresp="#s13:l1">Lood</rhyme>!</l>
        <l id="s13:l4" met="3" type="-+">Manschappen wilt <rhyme label="b" type="f" corresp="#s13:l2">vermeeren</rhyme>!</l>
        <l id="s13:l5" met="4" type="-+">Uit Liefde voor het <rhyme label="c" type="m">Vaderland</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Komt Zeegenpraalend weer aan 't <rhyme label="c" type="m" corresp="#s13:l5">Strand</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Laat Holland <rhyme label="b" type="f" corresp="#s13:l2">Triumpheeren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Ontwaekt gy Noord! komt <rhyme label="a" type="m">Zuydewind</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Doorwaait nu Hollands <rhyme label="b" type="f">Tuynen</rhyme>!</l>
        <l id="s14:l3" met="4" type="-+">'s Lands Vadren waard! geëerd <rhyme label="a" type="m" corresp="#s14:l1">bemind</rhyme>!</l>
        <l id="s14:l4" met="3" type="-+">Gods gunst Rust op uw <rhyme label="b" type="f" corresp="#s14:l2">Kruynen</rhyme>!</l>
        <l id="s14:l5" met="4" type="-+">Den voorspoed in u woon <rhyme label="c" type="m">Paleys</rhyme>!</l>
        <l id="s14:l6" met="4" type="-+">Den Faam doe steeds toe dank <rhyme label="c" type="m" corresp="#s14:l5">bewys</rhyme>!</l>
        <l id="s14:l7" met="3" type="-+">Uw roem, alöm <rhyme label="b" type="f" corresp="#s14:l2">Bazuynen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
