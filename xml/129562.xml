    <melody>Ik ging nog gisteravond zo heimelijk een gang</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
