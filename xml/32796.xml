      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VVAt sal ick gaen <rhyme label="a" type="f">beginnen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Maria Maghet <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Den brandt van uwer <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">My tot u blaecken <rhyme label="b" type="m" corresp="#s1:l2">doet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ghy zijt die ick <rhyme label="c" type="m">bemin</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">O weerde <rhyme label="c" type="m" corresp="#s1:l5">Coninghin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghy zijt mijn Bruyt, mijn <rhyme label="a" type="f">Moeder</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Mijn troost, mijn <rhyme label="b" type="m">toeverlaet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">V Soon is mijn <rhyme label="a" type="f" corresp="#s2:l1">behoeder</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op u mijn hope <rhyme label="b" type="m" corresp="#s2:l2">staet</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Ghy zijt die ick <rhyme label="c" type="m">bemin</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">O weerde <rhyme label="c" type="m" corresp="#s2:l5">Coninghin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Och had ick soo veel <rhyme label="a" type="f">monden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Als sterren in't <rhyme label="b" type="m">Firmament</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om den lof te <rhyme label="a" type="f" corresp="#s3:l1">verkonden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die ghy hebt sonder <rhyme label="b" type="m" corresp="#s3:l2">ent</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Ghy zijt die ick <rhyme label="c" type="m">bemin</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">O weerde <rhyme label="c" type="m" corresp="#s3:l5">Coninghin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Och had ick soo veel <rhyme label="a" type="f">zielen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als in locht, water, <rhyme label="b" type="m">landt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Leven d'ghedierten <rhyme label="a" type="f" corresp="#s4:l1">krielen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ick schonckx u t'eender <rhyme label="b" type="m" corresp="#s4:l2">handt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Ghy zijt die ick <rhyme label="c" type="m">bemin</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">O weerde <rhyme label="c" type="m" corresp="#s4:l5">Coninghin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Eylaes! O bitter <rhyme label="a" type="f">klaghen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat ick katijvich <rhyme label="b" type="m">mensch</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">V niet en kan <rhyme label="a" type="f" corresp="#s5:l1">behaghen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Soo ick begheer en <rhyme label="b" type="m" corresp="#s5:l2">wensch</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Ghy zijt die ick <rhyme label="c" type="m">bemin</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">O weerde <rhyme label="c" type="m" corresp="#s5:l5">Coninghin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick sal my alle <rhyme label="a" type="f">daghen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En dat tot uwer <rhyme label="b" type="m">eere</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">In als gaen beter <rhyme label="a" type="f" corresp="#s6:l1">draghen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En dienen onsen <rhyme label="b" type="m" corresp="#s6:l2">Heer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ghy sijt die ick <rhyme label="c" type="m">bemin</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">O weerde <rhyme label="c" type="m" corresp="#s6:l5">Coninghin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
