<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8988</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Nun freut euch liebe Christen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEEr Christ wy dancken dijner <rhyme label="a" type="m">goed'</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En dijner Liefd' <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die ghy aen ons bewijsen <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In dien ghy hebt <rhyme label="b" type="f" corresp="#s1:l2">ghegeven</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">V selven gantschlijck over <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Op dat wy wt der Sonden <rhyme label="c" type="m" corresp="#s1:l5">Tal</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Verlosset moghen <rhyme label="b" type="f" corresp="#s1:l2">Leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy hebt u Jae verschoonet <rhyme label="a" type="m">nicht</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En syt voor ons <rhyme label="b" type="f">ghestorven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer door het al is <rhyme label="a" type="m" corresp="#s2:l1">wtghericht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat wy Ghenaed' <rhyme label="b" type="f" corresp="#s2:l2">verworven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Vergevingh' onser Sonden <rhyme label="c" type="m">gaer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">End GOdes Kintschap Eewich <rhyme label="c" type="m" corresp="#s2:l5">daer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Nu int Gheloof <rhyme label="b" type="f" corresp="#s2:l2">verborghen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dijn Lichaem ons nu spijse <rhyme label="a" type="m">meest</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dijn heylich Bloet ons <rhyme label="b" type="f">drencke</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat wy ghestercket in den <rhyme label="a" type="m" corresp="#s3:l1">Gheest</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Den Ouden Mensche <rhyme label="b" type="f" corresp="#s3:l2">krencken</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">End wassen in der Nieu <rhyme label="c" type="m">gheboort</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Op dat het Broot der kinder <rhyme label="c" type="m" corresp="#s3:l5">voort</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Den honden niet <rhyme label="b" type="f" corresp="#s3:l2">ghereycket</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Help dat wy oock der Liefde <rhyme label="a" type="m">werck</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Een yeg'lijck so <rhyme label="b" type="f">bewijsen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Als ghy ons dat bevelet <rhyme label="a" type="m" corresp="#s4:l1">sterck</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat wy tot dijnen <rhyme label="b" type="f" corresp="#s4:l2">Prijse</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dy Inghelijvet leven <rhyme label="c" type="m">vroe</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dijn Avontmael ontfanghen <rhyme label="c" type="m" corresp="#s4:l5">soe</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat wy volstandich <rhyme label="b" type="f" corresp="#s4:l2">blijven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghedencken dijner Weldaet <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">In tijdts dat wy hier <rhyme label="b" type="f">Leven</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Verconden dijnen bitter'n <rhyme label="a" type="m" corresp="#s5:l1">Doot</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tot ghy ons sult <rhyme label="b" type="f" corresp="#s5:l2">Verheven</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Te nemen in dat Rijcke <rhyme label="c" type="m">dijn</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Daer nimmer sal gheen Eynde <rhyme label="c" type="m" corresp="#s5:l5">zijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Des wilt Godt prijsen, <rhyme label="b" type="f" corresp="#s5:l2">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
