    <location>
      <orginal-name>heidelberg</orginal-name>
      <standard-name>Heidelberg, Germany</standard-name>
      <coordinates>
        <lon>49.4093582</lon>
        <lat>8.694724</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>Psalm 077 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">O HEER opent my v' <rhyme label="a" type="f">ooren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wilt my goedighlick <rhyme label="a" type="f" corresp="#s1:l1">verhooren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want ick vermagh gantsch niet <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Benauwt sijnde nu so <rhyme label="b" type="m" corresp="#s1:l3">seer</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Wilt doch myn leuen <rhyme label="c" type="f">bevryden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Ick wil weldoen met <rhyme label="c" type="f" corresp="#s1:l5">verblyden</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Bewaert doch uwen knecht <rhyme label="d" type="m">vroet</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Die v vertrawt o Heer <rhyme label="d" type="m" corresp="#s1:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt my Heer ghenaedigh <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Nae uwe goetheit <rhyme label="a" type="f" corresp="#s2:l1">ghepresen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die v bidde met <rhyme label="b" type="m">ootmoet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">End daeghlicks valle te <rhyme label="b" type="m" corresp="#s2:l3">voet</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Verhueght nu uwen kecht <rhyme label="c" type="f">klachtigh</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Die v Heer aenroept <rhyme label="c" type="f" corresp="#s2:l5">aendachtigh</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Want ick heffe t'hert' <rhyme label="d" type="m">alleyn</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Tot v in den noot niet <rhyme label="d" type="m" corresp="#s2:l7">kleyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ghy sijt Heer vriendtlick end <rhyme label="a" type="f">goedigh</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Barmhertigh end seer <rhyme label="a" type="f" corresp="#s3:l1">langhmoedigh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Den ghenen die tot v <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">End in den noot roepen <rhyme label="b" type="m" corresp="#s3:l3">aen</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Verhoort my Heer <rhyme label="c" type="f">wtverkoren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Laet myn stem v kommen <rhyme label="c" type="f" corresp="#s3:l5">vooren</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Verstaet myn klachten seer <rhyme label="d" type="m">swaer</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Want t'is meer dan tyt <rhyme label="d" type="m" corresp="#s3:l7">voorwaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als ick ben in noot end <rhyme label="a" type="f">schaeden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Tot v schrey ick seer <rhyme label="a" type="f" corresp="#s4:l1">belaeden</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om dat ghy verhoort <rhyme label="b" type="m">altydt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Myn stemm', end maeckt my <rhyme label="b" type="m" corresp="#s4:l3">verblydt</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">By v O Heer des <rhyme label="c" type="f">Ä™erdtrycken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Is gheen Godt te <rhyme label="c" type="f" corresp="#s4:l5">verghelycken</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Daer is oock niemant so <rhyme label="d" type="m">koen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die v' wercken kan nae <rhyme label="d" type="m" corresp="#s4:l7">doen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Alle volcken die ghy <rhyme label="a" type="f">krachtigh</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Hebt gheschaepen Heer <rhyme label="a" type="f" corresp="#s5:l1">almachtigh</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Sullen vereeren te <rhyme label="b" type="m">saem</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">End groot maeken uwen <rhyme label="b" type="m" corresp="#s5:l3">naem</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Ghy sijt groot niet om <rhyme label="c" type="f">verstercken</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">End doet wonderlicke <rhyme label="c" type="f" corresp="#s5:l5">wercken</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Ghy hebt den naem breedt end <rhyme label="d" type="m">wydt</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">dat ghy alleyn een Godt <rhyme label="d" type="m" corresp="#s5:l7">sijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">V' weghen wilt my doch <rhyme label="a" type="f">leeren</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat ick recht gae t'uwer <rhyme label="a" type="f" corresp="#s6:l1">eeren</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Op dat ghy van my oock <rhyme label="b" type="m">meest</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Ten rechten moeght sijn <rhyme label="b" type="m" corresp="#s6:l3">ghevreest</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Ick wil v Heer hoogh' <rhyme label="c" type="f">verheuen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Altydt prys end' eere <rhyme label="c" type="f" corresp="#s6:l5">gheuen</rhyme>:</l>
        <l id="s6:l7" met="4" type="+-">Ick wil uwen naem seer <rhyme label="d" type="m">klaer</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Groot maeken in't <rhyme label="d" type="m" corresp="#s6:l7">openbaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ghy hebt wt louter <rhyme label="a" type="f">ghenaeden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">My bewesen veel <rhyme label="a" type="f" corresp="#s7:l1">weldaeden</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">End my verlost wt den <rhyme label="b" type="m">noot</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Der hellen, end van der <rhyme label="b" type="m" corresp="#s7:l3">doodt</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Heer die wreed' onder hen <rhyme label="c" type="f">allen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Met kracht my gantsch <rhyme label="c" type="f" corresp="#s7:l5">ouervallen</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Nae myn leuen sy al <rhyme label="d" type="m">staen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Sonder v te mercken <rhyme label="d" type="m" corresp="#s7:l7">aen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Maer ghy sijt goet end <rhyme label="a" type="f">ghenaedigh</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Barmhertigh ende <rhyme label="a" type="f" corresp="#s8:l1">weldaedigh</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Langhmoedigh tot <rhyme label="b" type="m">toornicheit</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Bestendigh in der <rhyme label="b" type="m" corresp="#s8:l3">waerheit</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Wilt my goedighlick <rhyme label="c" type="f">aenschauwen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Sterckt my oock in dit <rhyme label="c" type="f" corresp="#s8:l5">benauwen</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Helpt my end maeckt <rhyme label="d" type="m">onuertzaeght</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Den knecht van uwer <rhyme label="d" type="m" corresp="#s8:l7">dienstmaeghdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
