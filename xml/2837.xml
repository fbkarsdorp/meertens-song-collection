      <year-start>1590</year-start>
      <year-end>1600</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag>Chamber of rhetoric's song</tag>
    </tags>
    <melody>De tijd is hier</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Geluck voerwaer wens Ick alle <rhyme label="a" type="f">geesten</rhyme></l>
        <l id="s1:l2" met="5" type="-+">In dees feeste die hier sin <rhyme label="b" type="m">voergaert</rhyme></l>
        <l id="s1:l3" met="3" type="-+">de minste niet de <rhyme label="a" type="f" corresp="#s1:l1">meeste</rhyme></l>
        <l id="s1:l4" met="3" type="-+">tont u als klouke <rhyme label="a" type="f" corresp="#s1:l1">geesten</rhyme></l>
        <l id="s1:l5" met="6" type="-+">In dees feeste daer gij nu In <rhyme label="c" type="m">bent</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Ende toent dat liefdt Is <rhyme label="c" type="m" corresp="#s1:l5">tfondaement</rhyme></l>
        <l id="s1:l7" met="4" type="+-">met u lieften <rhyme label="d" type="m">amoerues</rhyme></l>
        <l id="s1:l8" met="4" type="-+">en hout u altoes <rhyme label="d" type="m" corresp="#s1:l7">koeraetiues</rhyme></l>
        <l id="s1:l9" met="2" type="-+">al hier <rhyme label="c" type="m" corresp="#s1:l5">ontrent</rhyme></l>
        <l id="s1:l10" met="3" type="-+">daer liefde Ist <rhyme label="c" type="m" corresp="#s1:l5">tfondaement</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Vrou bruit eerbaer Ick wens u <rhyme label="a" type="f">teenegaeder</rhyme></l>
        <l id="s2:l2" met="5" type="-+">geluck voerwaer en u bruidegom <rhyme label="b" type="m">sertein</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Dat gij hier <rhyme label="a" type="f" corresp="#s2:l1">naer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Den heemelschen <rhyme label="a" type="f" corresp="#s2:l1">vaeder</rhyme></l>
        <l id="s2:l5" met="6" type="-+">Wilt hem loeven <rhyme label="c" type="m">eervaer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">van sin weldaden <rhyme label="c" type="m" corresp="#s2:l5">reyn</rhyme></l>
        <l id="s2:l7" met="4" type="+-">van sin giften groet of <rhyme label="d" type="m">klein</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Dus wilt met liefden <rhyme label="d" type="m" corresp="#s2:l7">ackoert</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Dit onthouden <rhyme label="c" type="m" corresp="#s2:l5">rechtevoert</rhyme></l>
        <l id="s2:l10" met="3" type="-+">die hier voergaert sin want vruecht en is geen <rhyme label="c" type="m" corresp="#s2:l5">pin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">In dees feest wilt vroelickheit <rhyme label="a" type="f">hanteeren</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Al nae dees geest  en maeken soet <rhyme label="b" type="m">Iolit</rhyme></l>
        <l id="s3:l3" met="3" type="-+">zijt onbedeest en wilt liefelick <rhyme label="a" type="f" corresp="#s3:l1">questen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">en sit vroelick van <rhyme label="a" type="f" corresp="#s3:l1">geesten</rhyme></l>
        <l id="s3:l5" met="6" type="-+">die hier voergaert <rhyme label="c" type="m">sijt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">en gij Jonckmans past dat gij wat <rhyme label="c" type="m" corresp="#s3:l5">vrijt</rhyme></l>
        <l id="s3:l7" met="4" type="+-">en praet van geen <rhyme label="d" type="m">oneer</rhyme></l>
        <l id="s3:l8" met="4" type="-+">brenckt u lief weer nae haer <rhyme label="d" type="m" corresp="#s3:l7">begeer</rhyme></l>
        <l id="s3:l9" met="2" type="-+">ter plaetse <rhyme label="c" type="m" corresp="#s3:l5">ontrent</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Daer liefde Is <rhyme label="c" type="m" corresp="#s3:l5">tfondaement</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Prinse eerbaer daer In liefie kan <rhyme label="a" type="f">bouwen</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Wilt u daer houwen maer draecht een huesen <rhyme label="b" type="m">mont</rhyme></l>
        <l id="s4:l3" met="3" type="-+">van de fluer der vrouwen liefelick Int <rhyme label="a" type="f" corresp="#s4:l1">aenschouwen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">ende praet van <rhyme label="a" type="f" corresp="#s4:l1">trouwen</rhyme></l>
        <l id="s4:l5" met="6" type="-+">en dan maeckt met haer <rhyme label="c" type="m">voerbont</rhyme></l>
        <l id="s4:l6" met="4" type="-+">soe sult gij voerwaer van haer weeten de <rhyme label="c" type="m" corresp="#s4:l5">gront</rhyme></l>
        <l id="s4:l7" met="4" type="+-">dan sult gij van haer <rhyme label="d" type="m">voerstaen</rhyme></l>
        <l id="s4:l8" met="4" type="-+">of gij weer nae huis sult <rhyme label="d" type="m" corresp="#s4:l7">gaen</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Krijcht gij dan het Jae <rhyme label="c" type="m" corresp="#s4:l5">woert</rhyme></l>
        <l id="s4:l10" met="3" type="-+">draecht liefde soet <rhyme label="c" type="m" corresp="#s4:l5">behoert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
