  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ONder dees witte Marmer <rhyme label="a" type="m">leyt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Onder dees witte Marmer <rhyme label="a" type="m" corresp="#s1:l1">leyt</rhyme></l>
        <l id="s1:l3" met="4" type="-+">De moeder van de <rhyme label="b" type="m">sorghloosheyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">De moeder vande <rhyme label="b" type="m" corresp="#s1:l3">sorghloosheyt</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">De welck haer lieve <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s1:l6" met="3" type="-+">An 't geselschap heeft <rhyme label="c" type="f" corresp="#s1:l5">gegeven</rhyme>,</l>
        <l id="s1:l7" met="6" type="+-">Brilin din dan bom, brilin din dan <rhyme label="d" type="m">bom</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De welck haer lieve <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme></l>
        <l id="s1:l9" met="3" type="-+">An 't geselschap heeft <rhyme label="c" type="f" corresp="#s1:l5">gegeven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In d' anval haer de geest <rhyme label="a" type="m">ontvloogh</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">In d' anval haer de geest <rhyme label="a" type="m" corresp="#s2:l1">ontvloogh</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ghy sietse hier met de naers om <rhyme label="b" type="m">hoogh</rhyme>!</l>
        <l id="s2:l4" met="4" type="-+">Ghy sietse hier met de naers om <rhyme label="b" type="m" corresp="#s2:l3">hoogh</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Sy storf in d' oorloghs <rhyme label="c" type="f">rasen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Omdrayen wys' eens met <rhyme label="c" type="f" corresp="#s2:l5">glasen</rhyme>,</l>
        <l id="s2:l7" met="6" type="+-">Brilin din dan bom, brilin din dan <rhyme label="d" type="m">bom</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Sy storf on d' orloghs <rhyme label="c" type="f" corresp="#s2:l5">rasen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Omdrayen wys' eens met <rhyme label="c" type="f" corresp="#s2:l5">glasen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Blydelijck is sy verresen <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Blydelijck is sy verresen <rhyme label="a" type="m" corresp="#s3:l1">weer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dus laet ons drincken t' haerder <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dus laet ons drincken t' haerder <rhyme label="b" type="m" corresp="#s3:l3">eer</rhyme>!</l>
        <l id="s3:l5" met="3" type="-+">En met gesange <rhyme label="c" type="f">trachten</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Haer t'houden in <rhyme label="c" type="f" corresp="#s3:l5">gedachten</rhyme>:</l>
        <l id="s3:l7" met="6" type="+-">Brilin din dan bom, brilin din dan <rhyme label="d" type="m">bom</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En met gesange <rhyme label="c" type="f" corresp="#s3:l5">trachten</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Haer t' houden in <rhyme label="c" type="f" corresp="#s3:l5">gedachten</rhyme>.</l>
      </lg>
    </lg>
  </text>
