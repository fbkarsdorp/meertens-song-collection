  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Minnent lonck <rhyme label="a" type="m">gesigt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O soet en <rhyme label="a" type="m" corresp="#s1:l1">lodder-ligt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dat in mijn hertje <rhyme label="a" type="m" corresp="#s1:l1">stigt</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Een <rhyme label="b" type="m">Minne-vier</rhyme></l>
        <l id="s1:l5" met="3" type="-+">O Neusje wel <rhyme label="c" type="m">gemaeckt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Als ick u reuck maer <rhyme label="c" type="m" corresp="#s1:l5">smaeck</rhyme></l>
        <l id="s1:l7" met="3" type="-+">De geur mijn Zieltje <rhyme label="c" type="m" corresp="#s1:l5">raeckt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En voet <rhyme label="b" type="m" corresp="#s1:l4">plaisier</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Ha lipjes, die de mont <rhyme label="d" type="m">bewalt</rhyme>.</l>
        <l id="s1:l10" met="2" type="-+">Met Tongh en <rhyme label="e" type="f">tanden</rhyme>;</l>
        <l id="s1:l11" met="2" type="+-">Als ick kom <rhyme label="e" type="f" corresp="#s1:l10">Stranden</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Mijn Geesje <rhyme label="d" type="m" corresp="#s1:l9">bralt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Halsje lijf <rhyme label="a" type="m">Colom</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Bespant met Kralen <rhyme label="a" type="m" corresp="#s2:l1">om</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Beweeght mijn dat ick <rhyme label="a" type="m" corresp="#s2:l1">kom</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">En u om <rhyme label="b" type="m">vat</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">O Borsjes root <rhyme label="c" type="m">betipt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Mijn leven schier <rhyme label="c" type="m" corresp="#s2:l5">ontglipt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als ghy u straeltjes <rhyme label="c" type="m" corresp="#s2:l5">ript</rhyme>.</l>
        <l id="s2:l8" met="2" type="-+">En mijn <rhyme label="b" type="m" corresp="#s2:l4">bespat</rhyme>:</l>
        <l id="s2:l9" met="4" type="-+">O witte Vogt ghy voet mijn <rhyme label="d" type="m">Borst</rhyme>:</l>
        <l id="s2:l10" met="2" type="-+">Mijn leven vol <rhyme label="e" type="f">lachjes</rhyme></l>
        <l id="s2:l11" met="2" type="+-">En jeuchdige <rhyme label="e" type="f" corresp="#s2:l10">nachjes</rhyme>.</l>
        <l id="s2:l12" met="2" type="-+">O soeten <rhyme label="d" type="m" corresp="#s2:l9">dorst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O Buyckje welig <rhyme label="a" type="m">Vat</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">O, Overrijcke <rhyme label="a" type="m" corresp="#s3:l1">schat</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">O, wonderlijcke <rhyme label="a" type="m" corresp="#s3:l1">Stadt</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Seer schoon <rhyme label="b" type="m">behaeght</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">O Haven <rhyme label="c" type="m">onbewust</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ick krijgh tot 't zeylen <rhyme label="c" type="m" corresp="#s3:l5">lust</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Na d'onbezelyde <rhyme label="c" type="m" corresp="#s3:l5">Cust</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Van't Eylandt <rhyme label="b" type="m" corresp="#s3:l4">Maeght</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Ick waegh al 't gene mijn lijf <rhyme label="d" type="m">om-vat</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">In sulcke <rhyme label="e" type="f">Baren</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Daer vaer ick <rhyme label="e" type="f" corresp="#s3:l10">garen</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">'t Is Minsaem <rhyme label="d" type="m" corresp="#s3:l9">Nat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
