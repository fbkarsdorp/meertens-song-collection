  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wel aan <rhyme label="a" type="f">Oranje-Steeden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Houd nu maar goede <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En exerceerd met <rhyme label="a" type="f" corresp="#s1:l1">vreeden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Stryd voor Oranje <rhyme label="b" type="m" corresp="#s1:l2">zoet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En laat u niet <rhyme label="c" type="f">verleiden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door Burgers, valsch van <rhyme label="d" type="m">moed</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wilt voor Oranje <rhyme label="c" type="f" corresp="#s1:l5">stryden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Al kost 't vleesch en <rhyme label="d" type="m" corresp="#s1:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gy Deedelaars te <rhyme label="a" type="f">zamen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ziet onze trouwe <rhyme label="b" type="m">daad</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Gy hoeft u niet te <rhyme label="a" type="f" corresp="#s2:l1">schaamen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ofschoon gy word <rhyme label="b" type="m" corresp="#s2:l2">versmaad</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wy zingen tog met <rhyme label="c" type="f">vreugde</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Marseeren regts en <rhyme label="d" type="m">lings</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Waar op wy ons <rhyme label="c" type="f" corresp="#s2:l5">verheugde</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Viva lang leeft de <rhyme label="d" type="m" corresp="#s2:l6">Prins</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Onz' Wapenen zyn <rhyme label="a" type="f">vaardig</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Getrouwe <rhyme label="b" type="m">Burgery</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Het staat ons ook zeer <rhyme label="a" type="f" corresp="#s3:l1">vaardig</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Uw yver toont gy <rhyme label="b" type="m" corresp="#s3:l2">bly</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Vreest tog voor geene <rhyme label="c" type="f">vlye</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Viva Oranje <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wy doenen <rhyme label="c" type="f" corresp="#s3:l5">offerhande</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Al kost 't ons vleesch en <rhyme label="d" type="m" corresp="#s3:l6">bloed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Onz' Wapenen tot <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">O vreugd van ons <rhyme label="b" type="m">Nederland</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wy zullen tot u <rhyme label="a" type="f" corresp="#s4:l1">keeren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Want nu zyn zy van <rhyme label="b" type="m" corresp="#s4:l2">kant</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Der Dortsche flaauwe <rhyme label="c" type="f">dienste</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Het springen is <rhyme label="d" type="m">gedaan</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wy zullen nu met <rhyme label="c" type="f" corresp="#s4:l5">vreugde</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Hoez√©e ook roepen <rhyme label="d" type="m" corresp="#s4:l6">gaan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Oudelaarse <rhyme label="a" type="f">Burgery</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Komt deftig in het <rhyme label="b" type="m">geweer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Of marseeren in de <rhyme label="a" type="f" corresp="#s5:l1">ry</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Gaat deftig in de <rhyme label="b" type="m" corresp="#s5:l2">weer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En wilt Oranje <rhyme label="c" type="f">bewaaren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Vreest voor geen <rhyme label="d" type="m">hagelbuy</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wilt zamen Eendragt <rhyme label="c" type="f" corresp="#s5:l5">paaren</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Toont u als <rhyme label="d" type="m" corresp="#s5:l6">Heldenluy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Men ziet de <rhyme label="a" type="f">vaardigheden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van Deedels <rhyme label="b" type="m">Burgery</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met veel <rhyme label="a" type="f" corresp="#s6:l1">vermaaklykheden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">De Snaphaan draagen <rhyme label="b" type="m" corresp="#s6:l2">vry</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Met Trommels en met <rhyme label="c" type="f">Fluiten</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Tamboer pas op uw <rhyme label="d" type="m">stuk</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Voor de <rhyme label="c" type="f" corresp="#s6:l5">Oranje-Spruiten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Men wenscht hen veel <rhyme label="d" type="m" corresp="#s6:l6">geluk</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De eedle <rhyme label="a" type="f">Burgermeesters</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Verdienen eer en <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zy toonen ons haar <rhyme label="a" type="f" corresp="#s7:l1">geesten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En geeven ons veel <rhyme label="b" type="m" corresp="#s7:l2">lof</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Om tot haar eer te <rhyme label="c" type="f">zingen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Met Lauweren <rhyme label="d" type="m">gekroond</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ach ziet wat wondere <rhyme label="c" type="f" corresp="#s7:l5">dingen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Haar deugd moet zyn <rhyme label="d" type="m" corresp="#s7:l6">geloond</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
