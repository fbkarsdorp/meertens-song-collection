    <melody>Baise-moi ma Jeanneton</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GAlathea gheestigh <rhyme label="a" type="m">Dier</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Oorsaeck van mijn <rhyme label="b" type="f">lusjes</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Oorsaeck van mijn eerste <rhyme label="a" type="m" corresp="#s1:l1">vier</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Oorsaeck van mijn <rhyme label="b" type="f" corresp="#s1:l2">kusjes</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Oorsaeck van mijn wreede <rhyme label="c" type="m">pijn</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Kussen doet mijn vrolijck <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Laet ons t' samen <rhyme label="d" type="f">kussen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Om ons vyer te <rhyme label="d" type="f" corresp="#s1:l7">blussen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Kussen is een soet <rhyme label="a" type="m">vreught</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Want het doet mijn <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'t Kussen maeckt mijn Hart <rhyme label="a" type="m" corresp="#s2:l1">verheugt</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Wilt mijn kusjes <rhyme label="b" type="f" corresp="#s2:l2">gheven</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Kusjes soeter als het <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Dat den Hemel leven <rhyme label="c" type="m" corresp="#s2:l5">doet</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Soeter als het <rhyme label="d" type="f">minnen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Van ons <rhyme label="d" type="f" corresp="#s2:l7">Velt-Goddinnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Het zijn kusjes suycker <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Die ick krijgh van <rhyme label="b" type="f">Phillis</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Met kusjes wordt ick <rhyme label="a" type="m" corresp="#s3:l1">ghevoedt</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Van mijn <rhyme label="b" type="f" corresp="#s3:l2">Amarillis</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Maer als Galathea <rhyme label="c" type="m">kust</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Hemel wist ghy wat een <rhyme label="c" type="m" corresp="#s3:l5">lust</rhyme>!</l>
        <l id="s3:l7" met="3" type="+-">Kusjes om de <rhyme label="d" type="f">Booden</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Op te gast te <rhyme label="d" type="f" corresp="#s3:l7">nooden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'t Hooghst dat ick van u <rhyme label="a" type="m">begeer</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Is mijn pijn te <rhyme label="b" type="f">blussen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Kusje mijn, ick kusje <rhyme label="a" type="m" corresp="#s4:l1">weer</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Laet ons 't samen <rhyme label="b" type="f" corresp="#s4:l2">kussen</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Duysent kusjes, <rhyme label="c" type="m">duysentmael</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Gheef ick 't bloeyende <rhyme label="c" type="m" corresp="#s4:l5">Corael</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Aen u teere <rhyme label="d" type="f">tipjes</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Van u soete <rhyme label="d" type="f" corresp="#s4:l7">Lipjes</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Nu mijn soet <rhyme label="a" type="m">Galathee</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Wilt niet langher <rhyme label="b" type="f">beyden</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Neemt my met een kusje <rhyme label="a" type="m" corresp="#s5:l1">mee</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Niemandt sal ons <rhyme label="b" type="f" corresp="#s5:l2">scheyden</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Hierom wie 't behaeght of <rhyme label="c" type="m">spijt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Ick ben doch mijn vryheyt <rhyme label="c" type="m" corresp="#s5:l5">quijt</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">En ick heb <rhyme label="d" type="f">ghegheven</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Om een kus mijn <rhyme label="d" type="f" corresp="#s5:l7">leve</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
