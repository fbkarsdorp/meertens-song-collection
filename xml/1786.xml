<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>1786</id>
    <date>
      <year-start>1544</year-start>
      <year-end>1544</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
      <tag>secular song</tag>
      <tag>history song</tag>
    </tags>
    <melody>Van Jonas</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOort menschen hier <rhyme label="a" type="f">gheseten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Int borgoensche <rhyme label="b" type="m">crijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">die vromelijc drincken en en <rhyme label="a" type="f" corresp="#s1:l1">eeten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">en bedreuen <rhyme label="b" type="m" corresp="#s1:l2">iolijt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Om dat ons Keyser nv wort <rhyme label="b" type="m" corresp="#s1:l2">benijt</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Ende so vromelic <rhyme label="b" type="m" corresp="#s1:l2">strijt</rhyme></l>
        <l id="s1:l7" met="2" type="-+">dies zijn wy <rhyme label="b" type="m" corresp="#s1:l2">verblijt</rhyme></l>
        <l id="s1:l8" met="5" type="-+">Wi dancken al tsamen god <rhyme label="b" type="m" corresp="#s1:l2">gebenedijt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Menich herteken was int <rhyme label="a" type="f">benauwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Int borgoensche <rhyme label="b" type="m">pleyn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Om dat Gelder en Cleefsche <rhyme label="a" type="f" corresp="#s2:l1">landouwen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En die Gulkenaers <rhyme label="b" type="m" corresp="#s2:l2">cleyn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Spraken van onsen Keyser <rhyme label="b" type="m" corresp="#s2:l2">vileyn</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Ende seyden <rhyme label="b" type="m" corresp="#s2:l2">certeyn</rhyme></l>
        <l id="s2:l7" met="2" type="-+">al tsamen <rhyme label="b" type="m" corresp="#s2:l2">ghemeyn</rhyme></l>
        <l id="s2:l8" met="5" type="-+">dat hem die cabbeliauwen hadden <rhyme label="b" type="m" corresp="#s2:l2">in</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een herte vernedert van <rhyme label="a" type="f">gheeste</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In gods toe <rhyme label="b" type="m">verlaet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Niewers hem en maect de <rhyme label="a" type="f" corresp="#s3:l1">meeste</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wert verheuen <rhyme label="b" type="m" corresp="#s3:l2">iaet</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Maer die hem verheft  hoort menschelijc <rhyme label="b" type="m" corresp="#s3:l2">saet</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Wert vernedert <rhyme label="b" type="m" corresp="#s3:l2">quaet</rhyme></l>
        <l id="s3:l7" met="2" type="-+">het blijct aent <rhyme label="b" type="m" corresp="#s3:l2">misdaet</rhyme></l>
        <l id="s3:l8" met="5" type="-+">Vraget Willem van Guilic die geuangen <rhyme label="b" type="m" corresp="#s3:l2">gaet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die coeruoorsters spraken te <rhyme label="a" type="f">rade</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Aen ons Keyserlijck <rhyme label="b" type="m">graen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Oft hy Willem niet in <rhyme label="a" type="f" corresp="#s4:l1">genade</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Soude willen <rhyme label="b" type="m" corresp="#s4:l2">ontfaen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ons Keyser en wilde aen sulc <rhyme label="b" type="m" corresp="#s4:l2">vermaen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">gheen hant aen <rhyme label="b" type="m" corresp="#s4:l2">slaen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">hi wilde weten <rhyme label="b" type="m" corresp="#s4:l2">saen</rhyme></l>
        <l id="s4:l8" met="5" type="-+">Waer om dat hy den Arent was af <rhyme label="b" type="m" corresp="#s4:l2">gegaen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die coeruoorsters elck ander in <rhyme label="a" type="f">stade</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Gingen Willem <rhyme label="b" type="m">beuroen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Ende spraken eert is te <rhyme label="a" type="f" corresp="#s5:l1">spade</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hoort hier Cleuenaer <rhyme label="b" type="m" corresp="#s5:l2">coen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">trec in Keysers handen op dit <rhyme label="b" type="m" corresp="#s5:l2">saisoen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">In der velden <rhyme label="b" type="m" corresp="#s5:l2">groen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">als geuangen <rhyme label="b" type="m" corresp="#s5:l2">baroen</rhyme></l>
        <l id="s5:l8" met="5" type="-+">En op diet v rieden  wilt v ontschult <rhyme label="b" type="m" corresp="#s5:l2">doen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Nv en sal prophecie niet <rhyme label="a" type="f">liegen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hoort al hier <rhyme label="b" type="m">naer</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Alle loosheyt en <rhyme label="a" type="f" corresp="#s6:l1">bedrieghen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Wert nv <rhyme label="b" type="m" corresp="#s6:l2">openbaer</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Tis so misselic wiet berocket <rhyme label="b" type="m" corresp="#s6:l2">heeft</rhyme></l>
        <l id="s6:l6" met="2" type="-+">En dus heeft <rhyme label="b" type="m" corresp="#s6:l2">gesneeft</rhyme></l>
        <l id="s6:l7" met="2" type="-+">vry lelie nv <rhyme label="b" type="m" corresp="#s6:l2">beeft</rhyme></l>
        <l id="s6:l8" met="5" type="-+">Wi dancken god dat onsen Keyser noch <rhyme label="b" type="m" corresp="#s6:l2">leeft</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Gelder Cleef en Guylic die <rhyme label="a" type="f">landen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Recht vry <rhyme label="b" type="m">ongespaert</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Begeren onder ons Keysers <rhyme label="a" type="f" corresp="#s7:l1">handen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Te sijne <rhyme label="b" type="m" corresp="#s7:l2">bewaert</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Wilde hyse ontfangen als Keyser <rhyme label="b" type="m" corresp="#s7:l2">vermaert</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Si zijn vol <rhyme label="b" type="m" corresp="#s7:l2">vaert</rhyme></l>
        <l id="s7:l7" met="2" type="-+">nae den vroemen <rhyme label="b" type="m" corresp="#s7:l2">libaert</rhyme></l>
        <l id="s7:l8" met="5" type="-+">Hi wilse met brabant hollant hebben <rhyme label="b" type="m" corresp="#s7:l2">gepaert</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O princelijc prince <rhyme label="a" type="f">verheuen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Karolus <rhyme label="b" type="m">coragieus</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Van ws gelijcken en was noit <rhyme label="a" type="f" corresp="#s8:l1">ghescreuen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Noch soe <rhyme label="b" type="m" corresp="#s8:l2">victorieus</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Het buycht al voor ons keyser <rhyme label="b" type="m" corresp="#s8:l2">valiant</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Doer gods crachtige <rhyme label="b" type="m" corresp="#s8:l2">hant</rhyme></l>
        <l id="s8:l7" met="2" type="-+">den Torcxen <rhyme label="b" type="m" corresp="#s8:l2">tirant</rhyme></l>
        <l id="s8:l8" met="5" type="-+">wilt god ons keiser sal besitten sijn <rhyme label="b" type="m" corresp="#s8:l2">lant</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
