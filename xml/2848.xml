  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Voersichtich en <rhyme label="a" type="m">voertuinyues</rhyme></l>
        <l id="s1:l2" met="2" type="-+">mach hij wel <rhyme label="b" type="f">heten</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Diet hem wel wachten <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s1:l4" met="4" type="-+">van het vier der minnen <rhyme label="a" type="m" corresp="#s1:l1">aemoerues</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daer men veel meer <rhyme label="d" type="m">droetheit</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Siet koemen <rhyme label="c" type="m" corresp="#s1:l3">van</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Dan meeloedye, quae <rhyme label="e" type="f">fantoesye</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Die hem bestrijen, tot allen <rhyme label="e" type="f" corresp="#s1:l7">tijen</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Wee hem die hem laet <rhyme label="f" type="f">voerblinnen</rhyme></l>
        <l id="s1:l10" met="3" type="-+">van veenes Ick <rhyme label="d" type="m" corresp="#s1:l5">belijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick heb eylaes soemeenigen <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Gewandelt <rhyme label="b" type="f">droevich</rhyme></l>
        <l id="s2:l3" met="3" type="-+">doer den minnen <rhyme label="c" type="m">brant</rhyme></l>
        <l id="s2:l4" met="4" type="-+">heb troest al van mijn lief <rhyme label="a" type="m" corresp="#s2:l1">voorwacht</rhyme></l>
        <l id="s2:l5" met="3" type="-+">heb gestaen voer de <rhyme label="d" type="m">deur</rhyme></l>
        <l id="s2:l6" met="2" type="-+">met den rinck In mijn <rhyme label="c" type="m" corresp="#s2:l3">hant</rhyme></l>
        <l id="s2:l7" met="4" type="-+">In haegel en snee reegen en <rhyme label="e" type="f">wint</rhyme></l>
        <l id="s2:l8" met="4" type="-+">het is pijn ende wee diet wel <rhyme label="e" type="f" corresp="#s2:l7">voersint</rhyme></l>
        <l id="s2:l9" met="3" type="+-">sachmender mijn oeit toe <rhyme label="f" type="f">brengen</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Kuybydoe veenes <rhyme label="d" type="m" corresp="#s2:l5">kint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Alt ongemak en swaer <rhyme label="a" type="m">voerdriet</rhyme></l>
        <l id="s3:l2" met="2" type="-+">therseer gequel dat <rhyme label="b" type="f">merck</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ick wel aen <rhyme label="c" type="m">u</rhyme></l>
        <l id="s3:l4" met="4" type="-+">en kan mijn gans voerveelen <rhyme label="a" type="m" corresp="#s3:l1">niet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">maer het gedoegen dat valt mij soe <rhyme label="d" type="m">swaer</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Dat sijder een ander bemint dan <rhyme label="c" type="m" corresp="#s3:l3">mijn</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Daer wijder <rhyme label="e" type="f">malkander</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Wel eertijts <rhyme label="e" type="f" corresp="#s3:l7">vrij</rhyme></l>
        <l id="s3:l9" met="3" type="+-">veel trouwicheit <rhyme label="f" type="f">beloefJe</rhyme></l>
        <l id="s3:l10" met="3" type="-+">maer eilaes tfaelieert <rhyme label="d" type="m" corresp="#s3:l5">mijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Doets bitter heit, en swaer <rhyme label="a" type="m">voerdriet</rhyme></l>
        <l id="s4:l2" met="2" type="-+">tharseer <rhyme label="b" type="f">gequel</rhyme></l>
        <l id="s4:l3" met="3" type="-+">eedel prinsesse <rhyme label="c" type="m">Ient</rhyme></l>
        <l id="s4:l4" met="4" type="-+">brengen mijn bij al doer een <rhyme label="a" type="m" corresp="#s4:l1">vij</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die quade tongen tot veel meerder <rhyme label="d" type="m">torment</rhyme></l>
        <l id="s4:l6" met="2" type="-+">al deese saeke vol van <rhyme label="c" type="m" corresp="#s4:l3">geween</rhyme></l>
        <l id="s4:l7" met="4" type="-+">die moet Ick <rhyme label="e" type="f">smaeken</rhyme></l>
        <l id="s4:l8" met="4" type="-+">tis sonder <rhyme label="e" type="f" corresp="#s4:l7">een</rhyme></l>
        <l id="s4:l9" met="3" type="+-">maer weinich troest en <rhyme label="f" type="f">hoepen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Wacht Ick van mijn lief <rhyme label="d" type="m" corresp="#s4:l5">alleen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
