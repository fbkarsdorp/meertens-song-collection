  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mochtt ich hertz lieb bei dir <rhyme label="a" type="m">gesein</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">nitt mehr woltt ich <rhyme label="b" type="f">begerenn</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">Du brengest groß fre de dem hertzen <rhyme label="a" type="m" corresp="#s1:l1">mein</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">Du th st mich baldtt des <rhyme label="b" type="f" corresp="#s1:l2">gewerenn</rhyme> /</l>
        <l id="s1:l5" met="2" type="-+">Heimlich vnnd <rhyme label="c" type="m">still</rhyme> /</l>
        <l id="s1:l6" met="2" type="-+">das wer mein <rhyme label="c" type="m" corresp="#s1:l5">will</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">Wolst  mir das z <rhyme label="d" type="f">sagenn</rhyme> /</l>
        <l id="s1:l8" met="2" type="-+">Mein leidt iß <rhyme label="e" type="m">groß</rhyme> /</l>
        <l id="s1:l9" met="2" type="-+">So gar ohn <rhyme label="e" type="m" corresp="#s1:l8">maß</rhyme> /</l>
        <l id="s1:l10" met="3" type="-+">Daß sei dir hertzlieb <rhyme label="d" type="f" corresp="#s1:l7">geclagett</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mein liebes lieb / wie mag das <rhyme label="a" type="m">gesein</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">So gar all jn dem hertzenn <rhyme label="b" type="f">dein</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">das d  mir thrages so grossen <rhyme label="a" type="m" corresp="#s2:l1">haß</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">Laß ab vonn s lchem <rhyme label="b" type="f" corresp="#s2:l2">schertzenn</rhyme> /</l>
        <l id="s2:l5" met="2" type="-+">Du th st groß <rhyme label="c" type="m">pein</rhyme></l>
        <l id="s2:l6" met="2" type="-+">dem hertzen <rhyme label="c" type="m" corresp="#s2:l5">mein</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Du hast mein hertz schier z <rhyme label="d" type="f">brochenn</rhyme> /</l>
        <l id="s2:l8" met="2" type="-+">Mannich zeitt vnd st <rhyme label="e" type="m">ndtt</rhyme> /</l>
        <l id="s2:l9" met="2" type="-+">Du hast d rchw <rhyme label="e" type="m" corresp="#s2:l8">ndtt</rhyme> /</l>
        <l id="s2:l10" met="3" type="-+">mein hertz mitt einer strall d <rhyme label="d" type="f" corresp="#s2:l7">rchschossenn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Behaltt dich woll mein schones <rhyme label="a" type="m">lieb</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">Laß d  den h ndtt nitt <rhyme label="b" type="f">hinckenn</rhyme> /</l>
        <l id="s3:l3" met="4" type="-+">Vnd schl ß mich jn das hertze <rhyme label="a" type="m" corresp="#s3:l1">dein</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Laß mich darauß nitt <rhyme label="b" type="f" corresp="#s3:l2">sinckenn</rhyme> /</l>
        <l id="s3:l5" met="2" type="-+">Haltt hartt ahn <rhyme label="c" type="m">mir</rhyme> /</l>
        <l id="s3:l6" met="2" type="-+">alß ich an <rhyme label="c" type="m" corresp="#s3:l5">dir</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">nitt mehr will ich <rhyme label="d" type="f">begerenn</rhyme> /</l>
        <l id="s3:l8" met="2" type="-+">Breches d  ahn <rhyme label="e" type="m">mir</rhyme> /</l>
        <l id="s3:l9" met="2" type="-+">Gott reches an <rhyme label="e" type="m" corresp="#s3:l8">dir</rhyme> /</l>
        <l id="s3:l10" met="3" type="-+">Vnsere liebde soll ewig <rhyme label="d" type="f" corresp="#s3:l7">weren</rhyme>.</l>
      </lg>
    </lg>
  </text>
