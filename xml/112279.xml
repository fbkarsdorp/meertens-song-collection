        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="pastoral song">herderslied</tag>
    </tags>
    <melody>Zoel zacht windje</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">'t HElder <rhyme label="a" type="f">maantje</rhyme></l>
        <l id="s1:l2" met="4" type="+-">'t Helder maantje met zyn <rhyme label="b" type="m">glans</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Flonkerden aan 's Hemels <rhyme label="b" type="m" corresp="#s1:l2">trans</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">En kwam door de wolken <rhyme label="c" type="f">booren</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Als <rhyme label="d" type="f">zigreynhart</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als zigreynhart dus liet <rhyme label="c" type="f" corresp="#s1:l4">hooren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Sprey uw <rhyme label="a" type="f">glanssen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Sprey uw glanssen zilv're <rhyme label="b" type="m">Maan</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Tuig, hoe ik myn <rhyme label="b" type="m" corresp="#s2:l2">leliaan</rhyme></l>
        <l id="s2:l4" met="4" type="+-">In het lommer van dees <rhyme label="c" type="f">Elzen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Met <rhyme label="d" type="f">vernoeging</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Met vernoeging zal <rhyme label="c" type="f" corresp="#s2:l4">omhelzen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Lieve <rhyme label="a" type="f">Schaapjes</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Lieve Schaapjes wit van <rhyme label="b" type="m">vagt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die wel eertyts op myn <rhyme label="b" type="m" corresp="#s3:l2">klagt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Scheen in stomme taal te <rhyme label="c" type="f">zuchten</rhyme>;</l>
        <l id="s3:l5" met="2" type="+-">Deel nu in <rhyme label="d" type="f">myn</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Deel nu in myn <rhyme label="c" type="f" corresp="#s3:l4">Zielgenuchten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Weste <rhyme label="a" type="f">wintje</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Weste wintje dat zagt <rhyme label="b" type="m">ruist</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Levend beekje dat hier <rhyme label="b" type="m" corresp="#s4:l2">bruist</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En het gras 't gebloemt en <rhyme label="c" type="f">boomen</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Kust door 't <rhyme label="d" type="f">Kabb'len</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Kust door 't Kabb'len van uw <rhyme label="c" type="f" corresp="#s4:l4">stroomen</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">'t Heugd u, hoe <rhyme label="a" type="f">wy</rhyme></l>
        <l id="s5:l2" met="4" type="+-">'t Heugd u, hoe wy by dees <rhyme label="b" type="m">beek</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Onze geitjes aan dees <rhyme label="b" type="m" corresp="#s5:l2">streek</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Weidden, die met zagte <rhyme label="c" type="f">trekjes</rhyme></l>
        <l id="s5:l5" met="2" type="+-">'t Kruit <rhyme label="d" type="f">afplukten</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">'t Kruit afplukten met hun <rhyme label="c" type="f" corresp="#s5:l4">bekjes</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Ach wat valt <rhyme label="a" type="f">het</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Ach wat valt het wagten <rhyme label="b" type="m">bang</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wyl'k met ongedult <rhyme label="b" type="m" corresp="#s6:l2">verlang</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Naar het meysje swart van <rhyme label="c" type="f">hairen</rhyme>!</l>
        <l id="s6:l5" met="2" type="+-">
          <rhyme label="d" type="f">Oogenblikjes</rhyme>
        </l>
        <l id="s6:l6" met="4" type="+-">Oogenblikjes schynen <rhyme label="c" type="f" corresp="#s6:l4">jaren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">
          <rhyme label="a" type="f">leliane</rhyme>
        </l>
        <l id="s7:l2" met="4" type="+-">leliane uw lieve <rhyme label="b" type="m">taal</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Rooft myn hert, wyl straal op <rhyme label="b" type="m" corresp="#s7:l2">straal</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Van uw lieve duiven <rhyme label="c" type="f">oogen</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Houd myn <rhyme label="d" type="f">zinnen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Houd myn zinnen <rhyme label="c" type="f" corresp="#s7:l4">opgetoogen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">Al zogt <rhyme label="a" type="f">venus</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Al zogt venus my tot <rhyme label="b" type="m">vrind</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Zy wierd nooit van my <rhyme label="b" type="m" corresp="#s8:l2">bemind</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Al wierd juno my <rhyme label="c" type="f">geschonken</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Gy alleen <rhyme label="d" type="f">zoud</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Gy alleen zoud my <rhyme label="c" type="f" corresp="#s8:l4">ontfonken</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="+-">Dus zong hy <rhyme label="a" type="f">om</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Dus zong hy om <rhyme label="b" type="m">leliaan</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">En ging, om niet leeg te <rhyme label="b" type="m" corresp="#s9:l2">staan</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Dit gezang de Nimph ter <rhyme label="c" type="f">eeren</rhyme></l>
        <l id="s9:l5" met="2" type="+-">In een <rhyme label="d" type="f">Linden</rhyme></l>
        <l id="s9:l6" met="4" type="+-">In een Linden schors <rhyme label="c" type="f" corresp="#s9:l4">graveeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
