<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>112663</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Polyphemus aan de stranden</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IS u schoonheid dan <rhyme label="a" type="f">gegeven</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">O! Myn <rhyme label="a" type="f" corresp="#s1:l1">Leeven</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Om myn moorderes te <rhyme label="b" type="m">zyn</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">'t Schynt zo: Rozelyn verheugt <rhyme label="c" type="f">zig</rhyme></l>
        <l id="s1:l5" met="4" type="+-">En vervreugt <rhyme label="c" type="f" corresp="#s1:l4">zig</rhyme></l>
        <l id="s1:l6" met="4" type="+-">In myn ongeneesb're <rhyme label="b" type="m" corresp="#s1:l3">pyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ongeneesbaar, niet te <rhyme label="a" type="f">blussen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Als met <rhyme label="a" type="f" corresp="#s2:l1">kussen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van uw lang benyde <rhyme label="b" type="m">mond</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Niet te blussen nog <rhyme label="c" type="f">geneezen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Als voor <rhyme label="c" type="f" corresp="#s2:l4">deezen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Met een Lonkje t'aller <rhyme label="b" type="m" corresp="#s2:l3">stond</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Nu myn Liefde word wat <rhyme label="a" type="f">ouder</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Word gy <rhyme label="a" type="f" corresp="#s3:l1">kouder</rhyme></l>
        <l id="s3:l3" met="4" type="+-">En gy weigert zelf uw <rhyme label="b" type="m">oog</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Hebt gy haar door uw <rhyme label="c" type="f">bekooren</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Eerst <rhyme label="c" type="f" corresp="#s3:l4">gebooren</rhyme>?</l>
        <l id="s3:l6" met="4" type="+-">Dat zy leeve, nu <rhyme label="b" type="m" corresp="#s3:l3">gedoog</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hoe versmacht en buiten <rhyme label="a" type="f">weezen</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Te <rhyme label="a" type="f" corresp="#s4:l1">geneezen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Is nochtans myn <rhyme label="b" type="m">Minnesmart</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Wilt gy, Zoetert, maar <rhyme label="c" type="f">gedoogen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dat uw <rhyme label="c" type="f" corresp="#s4:l4">oogen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Straalen door uw mond in't <rhyme label="b" type="m" corresp="#s4:l3">hart</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wat geneezen! adem <rhyme label="a" type="f">haalen</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Is dat <rhyme label="a" type="f" corresp="#s5:l1">draalen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Slechts, voor myn gewisse <rhyme label="b" type="m">dood</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Echter laat ze my <rhyme label="c" type="f">verwerven</rhyme>!</l>
        <l id="s5:l5" met="4" type="+-">'k Wil wel <rhyme label="c" type="f" corresp="#s5:l4">sterven</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Mag ik sterven in uw <rhyme label="b" type="m" corresp="#s5:l3">schoot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ach! die kleine <rhyme label="a" type="f">uitstel-poosjes</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Die de <rhyme label="a" type="f" corresp="#s6:l1">Roosjes</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Geven van uw lip en <rhyme label="b" type="m">kaak</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">Schoon zy maar een wyltje <rhyme label="c" type="f">duuren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">'t Aller <rhyme label="c" type="f" corresp="#s6:l4">uuren</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Zyn my een volmaakt <rhyme label="b" type="m" corresp="#s6:l3">vermaak</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'k Voel myn Zieltjen, <rhyme label="a" type="f">ondertussen</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Dat wy <rhyme label="a" type="f" corresp="#s7:l1">kussen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Op den oever van uw <rhyme label="b" type="m">Lip</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Wyl ik van u niet kan <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Wil my <rhyme label="c" type="f" corresp="#s7:l4">geven</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Vryheid, dat het binnen <rhyme label="b" type="m" corresp="#s7:l3">slip</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Of vergun aan 't <rhyme label="a" type="f">Minnesieltje</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Dat uw <rhyme label="a" type="f" corresp="#s8:l1">Zieltje</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Zy gesmolten in myn <rhyme label="b" type="m">Ziel</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Goden Minnen niet zo <rhyme label="c" type="f">kragtig</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Schoon <rhyme label="c" type="f" corresp="#s8:l4">vrymachtig</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Als daar zo de Liefde <rhyme label="b" type="m" corresp="#s8:l3">viel</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
