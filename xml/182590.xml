<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182590</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">'k Ging lest myn <rhyme label="a" type="f">verblyden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Langs een groene <rhyme label="a" type="f" corresp="#s1:l1">Hyden</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">'k Ging est myn <rhyme label="a" type="f" corresp="#s1:l1">verblyden</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Al door het <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Ik nam myn Vogels <rhyme label="c" type="f">meden</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Om die wel te <rhyme label="c" type="f" corresp="#s1:l5">besteeden</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Om te vangen naar <rhyme label="d" type="f">verlangen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Daar myn jonk hert na <rhyme label="e" type="m">tragt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ik kwam wat <rhyme label="a" type="f">nader</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Langs de groene <rhyme label="a" type="f" corresp="#s2:l1">bladers</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Ik kwam wat <rhyme label="a" type="f" corresp="#s2:l1">nader</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Al door het <rhyme label="b" type="m">groen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Ik zag een <rhyme label="c" type="f">Jagt-Godinne</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Zy was zeer zoet in 't <rhyme label="c" type="f" corresp="#s2:l5">minne</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Schep vreugde door <rhyme label="d" type="f">deugden</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Wy speelden in dat <rhyme label="e" type="m">groen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Schoone <rhyme label="a" type="f">Jagt-Godinne</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Laat ons met vreugde <rhyme label="a" type="f" corresp="#s3:l1">beginnen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Schoone <rhyme label="a" type="f" corresp="#s3:l1">Jagt-Godinnen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Met groot <rhyme label="b" type="m">ootmoed</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">En wild voor my niet <rhyme label="c" type="f">beven</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Schoonste Maagd <rhyme label="c" type="f" corresp="#s3:l5">verheven</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">k Zal u geven myn jonk <rhyme label="d" type="f">leeven</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Voor u met groot <rhyme label="e" type="m">ootmoed</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Jonkman <rhyme label="a" type="f">gepreezen</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Laat my alleenig <rhyme label="a" type="f" corresp="#s4:l1">Weezen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Jonkman <rhyme label="a" type="f" corresp="#s4:l1">geprcezen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Dat bidde ik <rhyme label="b" type="m">U</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Want daar zyn zo veel <rhyme label="c" type="f">Vryers</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Schoonsprekers en <rhyme label="c" type="f" corresp="#s4:l5">verlyers</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Het zyn liegers en <rhyme label="d" type="f">bedriegers</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Gelyk men <rhyme label="e" type="m">ondervind</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Schoone <rhyme label="a" type="f">Jagt-Godinne</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik koom u te <rhyme label="a" type="f" corresp="#s5:l1">minnen</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Schoone <rhyme label="a" type="f" corresp="#s5:l1">Jagt-Godinnen</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Myn liefste <rhyme label="b" type="m">beeld</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Ik zal u niet <rhyme label="c" type="f">bedriegen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Of ook niet <rhyme label="c" type="f" corresp="#s5:l5">beliegen</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Ik wil sterven ik wil <rhyme label="d" type="f">sterven</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Voor u de bleeken <rhyme label="e" type="m">dood</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Jonkman u <rhyme label="a" type="f">spreeken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Zyn die niet valsche <rhyme label="a" type="f" corresp="#s6:l1">streeken</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Jonkman u <rhyme label="a" type="f" corresp="#s6:l1">spreeken</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Zyn die niet vals van <rhyme label="b" type="m">grond</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">Als gy myn heb <rhyme label="c" type="f">gevreden</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">En door de min <rhyme label="c" type="f" corresp="#s6:l5">bestreeden</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Gaat hy vlugten laat my <rhyme label="d" type="f">zugten</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Dan is 't bedrukte <rhyme label="e" type="m">maagd</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Wilt myn <rhyme label="a" type="f">geloven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">'k Zal u niet <rhyme label="a" type="f" corresp="#s7:l1">verdoven</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Wilt my <rhyme label="a" type="f" corresp="#s7:l1">geloven</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Dat zweer ik <rhyme label="b" type="m">U</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Nu gy zo komt te <rhyme label="c" type="f">praten</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Wil 'k op u <rhyme label="c" type="f" corresp="#s7:l5">verlaaten</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Myn jong leeven aan u <rhyme label="d" type="f">geeven</rhyme>.</l>
        <l id="s7:l8" met="3" type="+-">Tot trouw en <rhyme label="e" type="m">onderpand</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">'k dankte u <rhyme label="a" type="f">eerwaarde</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ik wil u trouw <rhyme label="a" type="f" corresp="#s8:l1">aanvaarde</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Ik dank u <rhyme label="a" type="f" corresp="#s8:l1">eerwaarde</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Met veel <rhyme label="b" type="m">eerbaarheid</rhyme>.</l>
        <l id="s8:l5" met="3" type="+-">Speeld op Fiool en <rhyme label="c" type="f">Basie</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Clarinet en <rhyme label="c" type="f" corresp="#s8:l5">Cortasse</rhyme>;</l>
        <l id="s8:l7" met="4" type="-+">Ter eere doet <rhyme label="d" type="f">vermeere</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">het leven van ons <rhyme label="e" type="m">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
