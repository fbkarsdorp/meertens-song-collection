<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185088</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom, laat ons vrolyk <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Nu de Erfstadhouder <rhyme label="b" type="m">weer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In Neêrland, als voor <rhyme label="a" type="f" corresp="#s1:l1">deezen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hersteld is in zyn <rhyme label="b" type="m" corresp="#s1:l2">Eer</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Kom, laat ons eere <rhyme label="c" type="f">geeven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Aan God, die, door Zyn <rhyme label="d" type="m">Hand</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Den Prins thans weer doet <rhyme label="c" type="f" corresp="#s1:l5">leeven</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In zyn verheven <rhyme label="d" type="m" corresp="#s1:l6">stand</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O, dat nu al de <rhyme label="a" type="f">Leden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van ons <rhyme label="b" type="m">Gemeenebest</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met hem, in zoete <rhyme label="a" type="f" corresp="#s2:l1">vreeden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Bestieren dit <rhyme label="b" type="m" corresp="#s2:l2">Gewest</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Dat al die ons <rhyme label="c" type="f">regeeren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met hem, vereend van <rhyme label="d" type="m">hart</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ons door Gods kragt <rhyme label="c" type="f" corresp="#s2:l5">beheeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Zo leefd men vry van <rhyme label="d" type="m" corresp="#s2:l6">smart</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mogt zo de liefde <rhyme label="a" type="f">bloeijen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dan zou men, als <rhyme label="b" type="m">voorheen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ons Land zien weelig <rhyme label="a" type="f" corresp="#s3:l1">groeijen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dan zou men, in ’t <rhyme label="b" type="m" corresp="#s3:l2">gemeen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">De rampen die ons <rhyme label="c" type="f">drukten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Vergeeten, dan zou <rhyme label="d" type="m">dra</rhyme></l>
        <l id="s3:l7" met="3" type="-+">’t Onheild waar onder ’t <rhyme label="c" type="f" corresp="#s3:l5">bukten</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Vergaan; ô dat ’t zo <rhyme label="d" type="m" corresp="#s3:l6">gaa</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gelei, ô Heer! de <rhyme label="a" type="f">Staaten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Geduurig by de <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wil hen toch nooit <rhyme label="a" type="f" corresp="#s4:l1">verlaaten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Doe hen, voor Kerk en <rhyme label="b" type="m" corresp="#s4:l2">Land</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Met de Erfstadhouder <rhyme label="c" type="f">waaken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Geef hen Uw hulp en <rhyme label="d" type="m">raad</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Richt zelve hunne <rhyme label="c" type="f" corresp="#s4:l5">zaaken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En wees hun <rhyme label="d" type="m" corresp="#s4:l6">Toeverlaat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laat Vyfde willem <rhyme label="a" type="f">leeven</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Tot heil van Land en <rhyme label="b" type="m">Kerk</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wil hem Uw bystand <rhyme label="a" type="f" corresp="#s5:l1">geeven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">In zyn zwaarwichtig <rhyme label="b" type="m" corresp="#s5:l2">werk</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Wil hen goedgunstig <rhyme label="c" type="f">leiden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Door Uwen Geest, ô <rhyme label="d" type="m">Heer</rhyme>!</l>
        <l id="s5:l7" met="3" type="-+">Op dat hy, t’allen <rhyme label="c" type="f" corresp="#s5:l5">tyden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Vermelden mag Uw <rhyme label="d" type="m" corresp="#s5:l6">Eer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Laat nimmer onheil <rhyme label="a" type="f">schade</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Zyn Gemaalin en <rhyme label="b" type="m">Zaad</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wil die toch, uit <rhyme label="a" type="f" corresp="#s6:l1">genade</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Tot nut van Kerk en <rhyme label="b" type="m" corresp="#s6:l2">Staat</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Lang in Uw gunst doen <rhyme label="c" type="f">leeven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">In ’t dierbaar <rhyme label="d" type="m">Vaderland</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Wil hen steeds zegen <rhyme label="c" type="f" corresp="#s6:l5">geeven</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En leiden by de <rhyme label="d" type="m" corresp="#s6:l6">hand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">En wil ook de <rhyme label="a" type="f">Overheden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Der Stad, ô Groote <rhyme label="b" type="m">God</rhyme>!</l>
        <l id="s7:l3" met="3" type="-+">Steeds met Uw heil <rhyme label="a" type="f" corresp="#s7:l1">bekleeden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Schenk hen dit zalig <rhyme label="b" type="m" corresp="#s7:l2">lot</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dat zy altoos <rhyme label="c" type="f">ervaaren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Uw trouwe Hulp en <rhyme label="d" type="m">Raad</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wil hen tot nut steeds <rhyme label="c" type="f" corresp="#s7:l5">spaaren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Van Kerk, van Stad van <rhyme label="d" type="m" corresp="#s7:l6">Staat</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wil ons, die ’t Land <rhyme label="a" type="f">bewoonen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met Uwe zegen, <rhyme label="b" type="m">Heer</rhyme>!</l>
        <l id="s8:l3" met="3" type="-+">Uit vrye gunst <rhyme label="a" type="f" corresp="#s8:l1">bekroonen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En leer ons, Lof en <rhyme label="b" type="m" corresp="#s8:l2">Eer</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Geduurig U te <rhyme label="c" type="f">geeven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat gy thans in ons <rhyme label="d" type="m">Land</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Doet de Erfstadhouder <rhyme label="c" type="f" corresp="#s8:l5">leeven</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Weer in verheven <rhyme label="d" type="m" corresp="#s8:l6">stand</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
