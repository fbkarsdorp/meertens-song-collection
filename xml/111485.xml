<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>111485</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="children's song">kinderlied</tag>
    </tags>
    <melody>Kon ik de min ontvluchten</melody>
    <categories>
      <category translation="children">kinderen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOn ik de min <rhyme label="a" type="f">ontvlugten</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik sou niet langer <rhyme label="a" type="f" corresp="#s1:l1">sugten</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Nog slyten vol <rhyme label="b" type="m">druk</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Dus myn tyd in <rhyme label="b" type="m" corresp="#s1:l3">ongeluk</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Maar wyl Ik ben <rhyme label="c" type="f">gebooren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Om in de min te <rhyme label="c" type="f" corresp="#s1:l5">smooren</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Zoo blyf ik die ik <rhyme label="d" type="m">ben</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wyl ik dog niet anders <rhyme label="d" type="m" corresp="#s1:l7">ken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat kan ik dog <rhyme label="a" type="f">verrigten</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Daar selver moeten <rhyme label="a" type="f" corresp="#s2:l1">swigten</rhyme></l>
        <l id="s2:l3" met="2" type="-+">De Goon in haar <rhyme label="b" type="m">Throon</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Voor de magt van <rhyme label="b" type="m" corresp="#s2:l3">Venus-Soon</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">'k Wil dan met <rhyme label="c" type="f">Offerhanden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Verliefde vuuren <rhyme label="c" type="f" corresp="#s2:l5">branden</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Voor 't Altaar van de <rhyme label="d" type="m">min</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En blonde <rhyme label="d" type="m" corresp="#s2:l7">Schuym-Godin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ik leef dan in u <rhyme label="a" type="f">banden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gy hebt myn Hert in <rhyme label="a" type="f" corresp="#s3:l1">handen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Myn waarde <rhyme label="b" type="m">Engelin</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Die ik alleen <rhyme label="b" type="m" corresp="#s3:l3">bemin</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Het zyn uw lieve <rhyme label="c" type="f">Lonken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die my in min <rhyme label="c" type="f" corresp="#s3:l5">ontvonken</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">De straalen van uw <rhyme label="d" type="m">oog</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Myn ziel tot min <rhyme label="d" type="m" corresp="#s3:l7">bewoog</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die net gespanne <rhyme label="a" type="f">boogjes</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van uw verliefde <rhyme label="a" type="f" corresp="#s4:l1">oogjes</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Verleenen my een <rhyme label="b" type="m">zoet</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Dat myn Hert versmelten <rhyme label="b" type="m" corresp="#s4:l3">doet</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Wanneer het Lief <rhyme label="c" type="f">gewemel</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van dien Azuuren <rhyme label="c" type="f" corresp="#s4:l5">Hemel</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Myn Zieltje tot den <rhyme label="d" type="m">grond</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Treft met een diepe <rhyme label="d" type="m" corresp="#s4:l7">wond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De roo' Corale <rhyme label="a" type="f">tipjes</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van uwe lieve <rhyme label="a" type="f" corresp="#s5:l1">Lipjes</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Doen my vaak met een <rhyme label="b" type="m">sugt</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Beswymen, door 't <rhyme label="b" type="m" corresp="#s5:l3">genugt</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Wanneer ik op de <rhyme label="c" type="f">randjes</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Van uw yvore <rhyme label="c" type="f" corresp="#s5:l5">tandjes</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Een vogtig Kusje <rhyme label="d" type="m">druk</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En minnaars vrugten <rhyme label="d" type="m" corresp="#s5:l7">pluk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maar als ik streel uw <rhyme label="a" type="f">Boesem</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Zoo geurig als de <rhyme label="a" type="f" corresp="#s6:l1">Bloessem</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Van Peer, of <rhyme label="b" type="m">Apricoos</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Zo bloost gy als een <rhyme label="b" type="m" corresp="#s6:l3">Roos</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">En toond my hoe uw <rhyme label="c" type="f">Kaakjes</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Nog door <rhyme label="c" type="f" corresp="#s6:l5">Min-vermaakjes</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Eens sullen zyn <rhyme label="d" type="m">gekleurd</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Als ons dat Luk <rhyme label="d" type="m" corresp="#s6:l7">gebeurd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laat ons dan, <rhyme label="a" type="f">afgescheyden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van al 't gewoel, ons <rhyme label="a" type="f" corresp="#s7:l1">beyden</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Vermaken hier <rhyme label="b" type="m">alleen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">In onse Min te <rhyme label="b" type="m" corresp="#s7:l3">vreen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Die kend geen <rhyme label="c" type="f">swarigheden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Als 't Onheil is <rhyme label="c" type="f" corresp="#s7:l5">geleden</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Zo nadert weer 't <rhyme label="d" type="m">geluk</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Min groeid in ramp en <rhyme label="d" type="m" corresp="#s7:l7">druk</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
