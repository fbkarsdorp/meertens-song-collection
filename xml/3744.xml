    <melody>Psalm 141 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">ICK roep' v Heer aen in noot niet <rhyme label="a" type="m">kleyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Haest v ghenaedelick tot <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Opent my nu v' ooren <rhyme label="b" type="m" corresp="#s1:l2">vry</rhyme>,</l>
        <l id="s1:l4" met="5" type="+-">Dewyl' ick roepe tot v <rhyme label="a" type="m" corresp="#s1:l1">alleyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Tot v klimme myn ghebedt <rhyme label="a" type="m">ghemeyn</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Euen als een rueckwerck seer <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Myn handen ghestreckt in <rhyme label="b" type="m" corresp="#s2:l2">ootmoet</rhyme>,</l>
        <l id="s2:l4" met="5" type="+-">Ontfanght als een spaedtoffer seer <rhyme label="a" type="m" corresp="#s2:l1">reyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Houdt altydt O Heer toe mynen <rhyme label="a" type="m">mondt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Regiert oock myn lippen nu <rhyme label="b" type="m">meer</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Dat niet quaets tot uwer <rhyme label="b" type="m" corresp="#s3:l2">oneer</rhyme>,</l>
        <l id="s3:l4" met="5" type="+-">Daer wt komme tot eenigher <rhyme label="a" type="m" corresp="#s3:l1">stondt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Neyght myn herte niet tot stucken <rhyme label="a" type="m">loos</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat ick niet en hebbe te <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met den menschen in boosheyt <rhyme label="b" type="m" corresp="#s4:l2">koen</rhyme>,</l>
        <l id="s4:l4" met="5" type="+-">End niet en smaeke haeren roof <rhyme label="a" type="m" corresp="#s4:l1">boos</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="+-">Dat my de vrome vermaen' <rhyme label="a" type="m">eenpaer</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ick wilt vriendelick dulden <rhyme label="b" type="m">fyn</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Sulcks sal my op myn hooft oock <rhyme label="b" type="m" corresp="#s5:l2">sijn</rhyme>,</l>
        <l id="s5:l4" met="5" type="+-">Als een balsem lieuelick end <rhyme label="a" type="m" corresp="#s5:l1">klaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="+-">Maer seer haest sal ick sien <rhyme label="a" type="m">ouerwacht</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die godtloosen in sulck <rhyme label="b" type="m">elendt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dat ick voor hen noch in den <rhyme label="b" type="m" corresp="#s6:l2">eyndt</rhyme>,</l>
        <l id="s6:l4" met="5" type="+-">Sal Godt moeten bidden met <rhyme label="a" type="m" corresp="#s6:l1">aendacht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="+-">Als haer Ouerheeren boos end <rhyme label="a" type="m">fel</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Van bouen af werden <rhyme label="b" type="m">ghestoort</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dan werden myn woorden <rhyme label="b" type="m" corresp="#s7:l2">ghehoort</rhyme>,</l>
        <l id="s7:l4" met="5" type="+-">Als die goet sijn end ghesproken <rhyme label="a" type="m" corresp="#s7:l1">wel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="+-">Gelyck t'houdt end steen sijn <rhyme label="a" type="m">wtghespreyt</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Als men se klieft end breeckt seer <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Also sijn ons beenen <rhyme label="b" type="m" corresp="#s8:l2">ghemeyn</rhyme>,</l>
        <l id="s8:l4" met="5" type="+-">Ontrent onsen graeuen <rhyme label="a" type="m" corresp="#s8:l1">wtghebreydt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="+-">O Heer' in myn lyden end <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Tot v heff' ick myn ooghen <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Myn troost end myn hope sijt <rhyme label="b" type="m" corresp="#s9:l2">ghy</rhyme>,</l>
        <l id="s9:l4" met="5" type="+-">End laett myn herte vertzaeghen <rhyme label="a" type="m" corresp="#s9:l1">niet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="+-">Hoedt my dat ick niet werde <rhyme label="a" type="m">gheuaen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">In den stricken my <rhyme label="b" type="m">voorghestelt</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">End met banden niet sy <rhyme label="b" type="m" corresp="#s10:l2">ghequelt</rhyme>,</l>
        <l id="s10:l4" met="5" type="+-">Die my die boose steedts voor <rhyme label="a" type="m" corresp="#s10:l1">slaen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="+-">Sy moeten selue wesen <rhyme label="a" type="m">verstrickt</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">In haer eyghen netten <rhyme label="b" type="m">alt'saem</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Op dat ick ghesondt end <rhyme label="b" type="m" corresp="#s11:l2">bequaem</rhyme>,</l>
        <l id="s11:l4" met="5" type="+-">Daer van vry sy, end werde <rhyme label="a" type="m" corresp="#s11:l1">verquickt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
