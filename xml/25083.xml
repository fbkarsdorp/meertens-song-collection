      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Geloeft soe si die maget <rhyme label="a" type="m">maria</rhyme></l>
        <l id="s1:l2" met="2" type="-+">van <rhyme label="b" type="m">hemelrike</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Her ihesus was van haer <rhyme label="c" type="f">geboren</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Op <rhyme label="b" type="m" corresp="#s1:l2">erterike</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Maria maget <rhyme label="c" type="f" corresp="#s1:l3">vutuercoren</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Ghi hebt verlost dat was <rhyme label="c" type="f" corresp="#s1:l3">verloren</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">By uwer <rhyme label="d" type="m">dracht</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">R Ons es her ihesus kerst <rhyme label="c" type="f" corresp="#s1:l3">geboren</rhyme>.</l>
        <l id="s1:l9" met="2" type="-+">In deser <rhyme label="d" type="m" corresp="#s1:l7">nacht</rhyme>.</l>
        <l id="s1:l10" met="4" type="-+">Ons es her ihesus kerst <rhyme label="c" type="f" corresp="#s1:l3">geboren</rhyme>.</l>
        <l id="s1:l11" met="2" type="+-">Een edel <rhyme label="d" type="m" corresp="#s1:l7">dracht</rhyme>.</l>
