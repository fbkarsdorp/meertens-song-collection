<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>175860</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>beggar song</tag>
    </tags>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Graef Hendrick van den <rhyme label="a" type="f">Bergh</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ben ick van duytschen <rhyme label="b" type="m">bloedt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Jck wil als een <rhyme label="a" type="f" corresp="#s1:l1">Monarch</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gaen trecken metter <rhyme label="b" type="m" corresp="#s1:l2">spoet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">My dunckt den Bos wil <rhyme label="c" type="f">falen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">'t Welck ich geern anders <rhyme label="d" type="m">sagh</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Jck sal mijn schade <rhyme label="c" type="f" corresp="#s1:l5">verhalen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Waer dat ick kan of <rhyme label="d" type="m" corresp="#s1:l6">magh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ravesteyn sou ick wel <rhyme label="a" type="f">krijgen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De Graef dat waer mijn <rhyme label="b" type="m">sin</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Maer Heusden moeter <rhyme label="a" type="f" corresp="#s2:l1">blijven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Om Moes-wael te vallen <rhyme label="b" type="m" corresp="#s2:l2">in</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dan wil ick voort <rhyme label="c" type="f">marcheeren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met alsoo grooten <rhyme label="d" type="m">ghedruys</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Kan men den Geus niet <rhyme label="c" type="f" corresp="#s2:l5">keeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Soo win ick 't Genn'per <rhyme label="d" type="m" corresp="#s2:l6">huys</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die schoone Stadt <rhyme label="a" type="f">Nimweghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met de Schans <rhyme label="b" type="m">Knodsenborgh</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Arnhem Huyssen daer <rhyme label="a" type="f" corresp="#s3:l1">beneven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zutphen en oock <rhyme label="b" type="m" corresp="#s3:l2">Doesborg</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Reenen-wijck kan niet <rhyme label="c" type="f">ontkomen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wageningen dat is <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Amersfoort mach wel <rhyme label="c" type="f" corresp="#s3:l5">schromen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Jck sou geern t'Uytrecht <rhyme label="d" type="m" corresp="#s3:l6">sijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Meynt my den Geus te <rhyme label="a" type="f">dwingen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat hy komt in <rhyme label="b" type="m">Brabant</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Jck sal hem leeren <rhyme label="a" type="f" corresp="#s4:l1">singen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Jn 't over Yssels <rhyme label="b" type="m" corresp="#s4:l2">Landt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En veel verscheyde <rhyme label="c" type="f">Plecken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Eerst Campen, Deventer, <rhyme label="d" type="m">Swol</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Daer na wil ick gaen <rhyme label="c" type="f" corresp="#s4:l5">trecken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Weer na de Stadt van <rhyme label="d" type="m" corresp="#s4:l6">Grol</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Om rucken ende <rhyme label="a" type="f">rooven</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Js mijn Volck kloeck ter <rhyme label="b" type="m">handt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Die haer geen gelt wil <rhyme label="a" type="f" corresp="#s5:l1">loven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Sal werden <rhyme label="b" type="m" corresp="#s5:l2">af-ghebrant</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Haer kaf sal ick doen <rhyme label="c" type="f">swerven</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Verdrijven met <rhyme label="d" type="m">ghewelt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Haer Koorn al t'saem <rhyme label="c" type="f" corresp="#s5:l5">verderven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat daer staet op het <rhyme label="d" type="m" corresp="#s5:l6">velt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Jck kan niet wel <rhyme label="a" type="f">vergete</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die Ed'le Bossche <rhyme label="b" type="m">Maeght</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Dan sal ick in de <rhyme label="a" type="f" corresp="#s6:l1">Betuwe</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Oock trecken <rhyme label="b" type="m" corresp="#s6:l2">ongevraeght</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">'t Moet vluchten Man en <rhyme label="c" type="f">Vrouwe</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Soo ick toone mijn <rhyme label="d" type="m">gewelt</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Het sal den Geus wel <rhyme label="c" type="f" corresp="#s6:l5">rouwen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Dat hy den Bos dus <rhyme label="d" type="m" corresp="#s6:l6">quelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Had hy den Bos <rhyme label="a" type="f">ghelaten</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Jn haren eersten <rhyme label="b" type="m">stant</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Jck waer met mijn <rhyme label="a" type="f" corresp="#s7:l1">Soldaten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Ghebleven uyt syn <rhyme label="b" type="m" corresp="#s7:l2">Landt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wat kan ik daerom <rhyme label="c" type="f">treuren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Elck een die doet sijn <rhyme label="d" type="m">best</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Hy sal 't noch wel <rhyme label="c" type="f" corresp="#s7:l5">besuren</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Eer hy my krijgt uyt 't <rhyme label="d" type="m" corresp="#s7:l6">nest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof mijn vroom <rhyme label="a" type="f">Soldaten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Elck weert hem als een <rhyme label="b" type="m">heldt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wilt de boeren <rhyme label="a" type="f" corresp="#s8:l1">aen-praten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Waer dat ghy komt om <rhyme label="b" type="m" corresp="#s8:l2">gelt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Jck geeft u al ten <rhyme label="c" type="f">beste</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Steden en Dorpen <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Hoe 't noch sal gaen op 't <rhyme label="c" type="f" corresp="#s8:l5">leste</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Den tijdt wel leeren <rhyme label="d" type="m" corresp="#s8:l6">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
