      <tag>holy song</tag>
      <tag>biblical history song</tag>
    </tags>
    <melody>Edele kersouw</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Sint Ian, die <rhyme label="a" type="m">hoort</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Herodes <rhyme label="b" type="f">onkuysheden</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Dat tegens d' <rhyme label="c" type="m">Echten-staet</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Tegen 't <rhyme label="a" type="m" corresp="#s1:l1">accoort</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Tegen Godt ende <rhyme label="b" type="f" corresp="#s1:l2">reden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Herodias <rhyme label="c" type="m" corresp="#s1:l3">verlaet</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Haer Man, en die <rhyme label="c" type="m" corresp="#s1:l3">haet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Om by een te <rhyme label="d" type="f">boeleeren</rhyme>.</l>
        <l id="s1:l9" met="4" type="+-">Sint Jan Herodem <rhyme label="e" type="m">bestraft</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Niet vreest sijn staet nog <rhyme label="d" type="f" corresp="#s1:l8">eeren</rhyme>;</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">'t Kan niet <rhyme label="a" type="m">bestaen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Of ghy moet heel <rhyme label="b" type="f">verlaten</rhyme></l>
        <l id="s2:l3" met="3" type="-+">De hoer <rhyme label="c" type="m">Herodias</rhyme>;</l>
        <l id="s2:l4" met="2" type="-+">Laet haer dan <rhyme label="a" type="m" corresp="#s2:l1">gaen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Toont u zonden te <rhyme label="b" type="f" corresp="#s2:l2">haten</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Sijn Boeltjes hart als <rhyme label="c" type="m" corresp="#s2:l3">vlas</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Van haet ontsteeckt <rhyme label="c" type="m" corresp="#s2:l3">ras</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Haer hart dat borst en <rhyme label="d" type="f">scheurde</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Van onkuysheydt was sy <rhyme label="e" type="m">vol</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Hierom 't scheyden niet <rhyme label="d" type="f" corresp="#s2:l8">beurde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Sint Ian <rhyme label="a" type="m">terstont</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Als een misdadigh <rhyme label="b" type="f">leyden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Men na een <rhyme label="c" type="m">Kerckers-kot</rhyme>;</l>
        <l id="s3:l4" met="2" type="-+">Om dat sijn <rhyme label="a" type="m" corresp="#s3:l1">mondt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Aen haer de waerheydt <rhyme label="b" type="f" corresp="#s3:l2">seyde</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Dit gare lijdt om <rhyme label="c" type="m" corresp="#s3:l3">Godt</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">En om s' Hemels <rhyme label="c" type="m" corresp="#s3:l3">slot</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Herodes in die <rhyme label="d" type="f">tijde</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Noot tot sijn geboorte <rhyme label="e" type="m">dagh</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Princen, om te <rhyme label="d" type="f" corresp="#s3:l8">verblijde</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">De Dochter <rhyme label="a" type="m">van</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Herodes <rhyme label="b" type="f">concubijne</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Met dansen zijn <rhyme label="c" type="m">gemoet</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Bekollen <rhyme label="a" type="m" corresp="#s4:l1">kan</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat hy haer voor al <rhyme label="b" type="f" corresp="#s4:l2">sijnen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Princen edel van <rhyme label="c" type="m" corresp="#s4:l3">bloedt</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Sweert met woorden <rhyme label="c" type="m" corresp="#s4:l3">soet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Al dat u hart kan <rhyme label="d" type="f">dencken</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Al was 't half van al mijn <rhyme label="e" type="m">goet</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Of rijck, ick sal 't u <rhyme label="d" type="f" corresp="#s4:l8">schencken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">'t Venus-dier <rhyme label="a" type="m">boos</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Haer Moeder vraeght om <rhyme label="b" type="f">raden</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Wat acht ghy 't meest te <rhyme label="c" type="m">zijn</rhyme>!</l>
        <l id="s5:l4" met="2" type="-+">De moeder <rhyme label="a" type="m" corresp="#s5:l1">loos</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Met haet en nijdt <rhyme label="b" type="f" corresp="#s5:l2">beladen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Seydt: waerdste Dochter van <rhyme label="c" type="m" corresp="#s5:l3">mijn</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Acht alles maer <rhyme label="c" type="m" corresp="#s5:l3">schijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ia selver Konincx <rhyme label="d" type="f">Kroonen</rhyme>:</l>
        <l id="s5:l9" met="4" type="+-">Ioannes hooft eyst <rhyme label="e" type="m">terstont</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">My vrintschap sult <rhyme label="d" type="f" corresp="#s5:l8">betoonen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Sy <rhyme label="a" type="m">onbevreest</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Seyt: Koningh wilt my <rhyme label="b" type="f">geve</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Haestigh Ioannes <rhyme label="c" type="m">hooft</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Hy wat <rhyme label="a" type="m" corresp="#s6:l1">bedeest</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">De Beul Sinte Ians <rhyme label="b" type="f" corresp="#s6:l2">leven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Beneemt, end' hals <rhyme label="c" type="m" corresp="#s6:l3">af-klooft</rhyme>;</l>
        <l id="s6:l7" met="3" type="+-">In een schotel 't <rhyme label="c" type="m" corresp="#s6:l3">hooft</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Sy toont aen al de <rhyme label="d" type="f">Heeren</rhyme>.</l>
        <l id="s6:l9" met="4" type="+-">Wat quaet komter niet al <rhyme label="e" type="m">voort</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Uyt dansen, drincken, <rhyme label="d" type="f" corresp="#s6:l8">sweeren</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">De <rhyme label="a" type="m">zuyverheydt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van Sint Ian heeft <rhyme label="b" type="f">gekregen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">De schoonste <rhyme label="c" type="m">Maeghde-kroon</rhyme>:</l>
        <l id="s7:l4" met="2" type="-+">Om dat hy <rhyme label="a" type="m" corresp="#s7:l1">scheyt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Om Goods woordt door de <rhyme label="b" type="f" corresp="#s7:l2">degen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ontfanght in 's Hemels <rhyme label="c" type="m" corresp="#s7:l3">throon</rhyme></l>
        <l id="s7:l7" met="3" type="+-">De Maertelaers <rhyme label="c" type="m" corresp="#s7:l3">kroon</rhyme>.</l>
        <l id="s7:l8" met="3" type="-+">Herodes met sijn <rhyme label="d" type="f">boele</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">En haer dochter in de <rhyme label="e" type="m">brandt</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Goods gramschap eeuwigh <rhyme label="d" type="f" corresp="#s7:l8">voele</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Sint Ian, <rhyme label="a" type="m">gekroont</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van Godt in 's Hemels <rhyme label="b" type="f">zalen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wy smeken met <rhyme label="c" type="m">ootmoet</rhyme>;</l>
        <l id="s8:l4" met="2" type="-+">Liefde <rhyme label="a" type="m" corresp="#s8:l1">betoont</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Aen die u lof <rhyme label="b" type="f" corresp="#s8:l2">verhalen</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">Krijght door u zuyver <rhyme label="c" type="m" corresp="#s8:l3">bloedt</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Dat wy metter <rhyme label="c" type="m" corresp="#s8:l3">spoet</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Treden u <rhyme label="d" type="f">deughde-spooren</rhyme>;</l>
        <l id="s8:l9" met="4" type="+-">Dat wy 't quaedt bestraffen <rhyme label="e" type="m">vroom</rhyme>;</l>
        <l id="s8:l10" met="3" type="-+">En ons quaedt gaere <rhyme label="d" type="f" corresp="#s8:l8">hooren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
