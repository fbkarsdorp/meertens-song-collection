<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2812</id>
    <date>
      <year-start>1590</year-start>
      <year-end>1600</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag>May song</tag>
    </tags>
    <melody>Willen onze koeien dan niet stilstaan</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Den mey die nu lustich <rhyme label="a" type="m">ontspruit</rhyme></l>
        <l id="s1:l2" met="4" type="-+">met haeren blaeten op allen <rhyme label="b" type="f">paeden</rhyme></l>
        <l id="s1:l3" met="2" type="-+">met haeren <rhyme label="b" type="f" corresp="#s1:l2">blaede</rhyme></l>
        <l id="s1:l4" met="2" type="-+">soet van <rhyme label="a" type="m" corresp="#s1:l1">viertuit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De boemen brengen voert haer <rhyme label="a" type="m">loef</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Diemen siet bloeyen tsmenschen te <rhyme label="b" type="f">voeyen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Die men siet <rhyme label="b" type="f" corresp="#s2:l2">bloeyen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">In de mey <rhyme label="a" type="m" corresp="#s2:l1">hof</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ock vint gy daer veel blomkens <rhyme label="a" type="m">schoen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">de kruiden krachten wilt niet <rhyme label="b" type="f">voerachten</rhyme></l>
        <l id="s3:l3" met="2" type="-+">de kruiden <rhyme label="b" type="f" corresp="#s3:l2">krachte</rhyme></l>
        <l id="s3:l4" met="2" type="-+">krychdy te <rhyme label="a" type="m" corresp="#s3:l1">loen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De leeuerick hoermen oeck sin <rhyme label="a" type="m">tael</rhyme></l>
        <l id="s4:l2" met="4" type="-+">met liefelick singen vruechde <rhyme label="b" type="f">voertbrengen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">met liefelick <rhyme label="b" type="f" corresp="#s4:l2">singen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Den <rhyme label="a" type="m" corresp="#s4:l1">nachtegael</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Feebues die Deese al <rhyme label="a" type="m">beschint</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Juepyter opt thoechste als hy Is <rhyme label="b" type="f">droechste</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Juepiter Int <rhyme label="b" type="f" corresp="#s5:l2">thoechste</rhyme></l>
        <l id="s5:l4" met="2" type="-+">met een sui <rhyme label="a" type="m" corresp="#s5:l1">wint</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Gy allickmaerse vryers <rhyme label="a" type="m">bly</rhyme></l>
        <l id="s6:l2" met="4" type="-+">wilt gaen seer vaerdich met u lief <rhyme label="b" type="f">aerdich</rhyme></l>
        <l id="s6:l3" met="2" type="-+">wilt gaen seer <rhyme label="b" type="f" corresp="#s6:l2">vaerdich</rhyme></l>
        <l id="s6:l4" met="2" type="-+">In den mey <rhyme label="a" type="m" corresp="#s6:l1">tyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wilt eens ront gaen duer die lange <rhyme label="a" type="m">laen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">nae die luit slaetster ten Is geen <rhyme label="b" type="f">voerraetster</rhyme></l>
        <l id="s7:l3" met="2" type="-+">nae die luit <rhyme label="b" type="f" corresp="#s7:l2">slaester</rhyme></l>
        <l id="s7:l4" met="2" type="-+">wilt niet <rhyme label="a" type="m" corresp="#s7:l1">voersmaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Appoeloe soet <rhyme label="a" type="m">Isterement</rhyme></l>
        <l id="s8:l2" met="4" type="-+">gy hoerse speelen singen en <rhyme label="b" type="f">queelen</rhyme></l>
        <l id="s8:l3" met="2" type="-+">gy hoerse <rhyme label="b" type="f" corresp="#s8:l2">speelen</rhyme></l>
        <l id="s8:l4" met="2" type="-+">muisykae <rhyme label="a" type="m" corresp="#s8:l1">Ient</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">maeck met u liefie goede <rhyme label="a" type="m">sier</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Wilt banketeere danst vry als <rhyme label="b" type="f">heere</rhyme></l>
        <l id="s9:l3" met="2" type="-+">wilt <rhyme label="b" type="f" corresp="#s9:l2">banketeere</rhyme></l>
        <l id="s9:l4" met="2" type="-+">Drinck win en <rhyme label="a" type="m" corresp="#s9:l1">bier</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Als gy dan weer nae huit toe <rhyme label="a" type="m">gaet</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Kuibydoe straele wil op u <rhyme label="b" type="f">daele</rhyme></l>
        <l id="s10:l3" met="2" type="-+">Kuibydoe <rhyme label="b" type="f" corresp="#s10:l2">straele</rhyme></l>
        <l id="s10:l4" met="2" type="-+">bruick veenes <rhyme label="a" type="m" corresp="#s10:l1">raet</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dus neemt In danck nu deesen <rhyme label="a" type="m">mey</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Looft godt almachtich die tuoet <rhyme label="b" type="f">krachtich</rhyme></l>
        <l id="s11:l3" met="2" type="-+">loeft godt <rhyme label="b" type="f" corresp="#s11:l2">almachtich</rhyme></l>
        <l id="s11:l4" met="2" type="-+">In dees <rhyme label="a" type="m" corresp="#s11:l1">vaeley</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Orlof hier mee gy prinsen <rhyme label="a" type="m">toch</rhyme></l>
        <l id="s12:l2" met="4" type="-+">myn slecht gedichte  laet thart <rhyme label="b" type="f">voerlichten</rhyme></l>
        <l id="s12:l3" met="2" type="-+">myn slech <rhyme label="b" type="f" corresp="#s12:l2">gedichte</rhyme></l>
        <l id="s12:l4" met="2" type="-+">Ick korst soeck <rhyme label="a" type="m" corresp="#s12:l1">noch</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
