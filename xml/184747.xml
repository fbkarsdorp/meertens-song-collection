<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184747</id>
    <date>
      <year-start>1775</year-start>
      <year-end>1775</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Vrolijke uurtjes</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">VRolijk <rhyme label="a" type="f">uurties</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die k’ soo <rhyme label="a" type="f" corresp="#s1:l1">duurties</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">U mijn engelin <rhyme label="b" type="m">betaal</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Roosemondt stook aan de <rhyme label="a" type="f" corresp="#s1:l1">vuurties</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ik bidt volvoert mijn <rhyme label="b" type="m" corresp="#s1:l3">segenpraal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Voet myn <rhyme label="a" type="f">Lusies</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Doet u <rhyme label="a" type="f" corresp="#s2:l1">kusies</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Ag myn engelin hoort <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">’K bid Ey wagt dog niet tot <rhyme label="a" type="f" corresp="#s2:l1">flusies</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">‘K heys terstondt te zijn <rhyme label="b" type="m" corresp="#s2:l3">voldaan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">’t Geen der <rhyme label="a" type="f">boomen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s3:l1">Waterstroomen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">’K sie dat niemant ons <rhyme label="b" type="m">verspiet</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Wilt dog haastig herwaarts <rhyme label="a" type="f" corresp="#s3:l1">komen</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">’K diet weerhout mijn voetjes <rhyme label="b" type="m" corresp="#s3:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Laat ’k de <rhyme label="a" type="f">tippies</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Van u <rhyme label="a" type="f" corresp="#s4:l1">Lippen</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Sagjes drukken dog <rhyme label="b" type="m">wanneer</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Rosemondt dat syn <rhyme label="a" type="f" corresp="#s4:l1">klippies</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Daar ik Schipbreuk heb <rhyme label="b" type="m" corresp="#s4:l3">gelee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Voet mijn <rhyme label="a" type="f">Lussie</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">By u ’t <rhyme label="a" type="f" corresp="#s5:l1">ruste</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Og mijn lieve <rhyme label="b" type="m">Rosemond</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Om mijn minnebrand te <rhyme label="a" type="f" corresp="#s5:l1">blussen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Gy maakt mijn jong hert <rhyme label="b" type="m" corresp="#s5:l3">gesont</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Vrolyk <rhyme label="a" type="f">meyssie</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Poezel <rhyme label="a" type="f" corresp="#s6:l1">vleysie</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">’K bid u hoort mijn reden <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Laate wy zamen voor een <rhyme label="a" type="f" corresp="#s6:l1">reysie</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Met ons tweetjes wandelen <rhyme label="b" type="m" corresp="#s6:l3">gaan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">Vroolyk <rhyme label="a" type="f">meysje</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Ieugdig <rhyme label="a" type="f" corresp="#s7:l1">vleysie</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Hoort myn smeeken en <rhyme label="b" type="m">gebeen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Laat mijn dog maar een <rhyme label="a" type="f" corresp="#s7:l1">reysje</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Ey waarom vlugt gy nu <rhyme label="b" type="m" corresp="#s7:l3">heen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">Keurig <rhyme label="a" type="f">roosje</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Wat een <rhyme label="a" type="f" corresp="#s8:l1">bloosje</rhyme>.</l>
        <l id="s8:l3" met="4" type="+-">Daalt ’er op u aangezigt <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Gun myn dat ik maar een <rhyme label="a" type="f" corresp="#s8:l1">poosje</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">My mag buygen voor u <rhyme label="b" type="m" corresp="#s8:l3">neer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
