    <melody>Psalm 092 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HET sijn heerlicke <rhyme label="a" type="f">dinghen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Als men v loeft O <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Als men des hooghsten <rhyme label="b" type="m" corresp="#s1:l2">eer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met herten goet magh <rhyme label="a" type="f" corresp="#s1:l1">singen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Als men s'morgens <rhyme label="c" type="f">verkondet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Des Heeren <rhyme label="d" type="m">goedicheit</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">End sijn <rhyme label="d" type="m" corresp="#s1:l6">ghetrauwicheit</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Des nachts oock steedts <rhyme label="c" type="f" corresp="#s1:l5">vermondet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Op dat spel van tien <rhyme label="a" type="f">snaeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">End op den Psalter <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Iae oock op harpen <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wil ick sijn lof <rhyme label="a" type="f" corresp="#s2:l1">verklaeren</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Want v' heylighe <rhyme label="c" type="f">wercken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Verhueghen mijn hert' <rhyme label="d" type="m">seer</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">End uwe daeden <rhyme label="d" type="m" corresp="#s2:l6">Heer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Roem' ick, so men kan <rhyme label="c" type="f" corresp="#s2:l5">mercken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe heerlick sijn mits <rhyme label="a" type="f">desen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vwe wercken <rhyme label="b" type="m">bekent</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Hoe groot end sonder <rhyme label="b" type="m" corresp="#s3:l2">eyndt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Is v' wysheit <rhyme label="a" type="f" corresp="#s3:l1">ghepresen</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">Dit en kan niet <rhyme label="c" type="f">betrachten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">De mensche dwaes end <rhyme label="d" type="m">bot</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Een onuerstandigh <rhyme label="d" type="m" corresp="#s3:l6">sot</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Kan dit te recht niet <rhyme label="c" type="f" corresp="#s3:l5">achten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dat de godtloose <rhyme label="a" type="f">groeyen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Als t'gras doet op dat <rhyme label="b" type="m">veldt</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Die quaet doen met <rhyme label="b" type="m" corresp="#s4:l2">ghewelt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In voorspoedt t'saemen <rhyme label="a" type="f" corresp="#s4:l1">bloeyen</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Op dat sy daernae <rhyme label="c" type="f">vallen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">End eewighlick <rhyme label="d" type="m">vergaen</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Maer ghy Heer sijt <rhyme label="d" type="m" corresp="#s4:l6">voortaen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Godt gheeert bouen <rhyme label="c" type="f" corresp="#s4:l5">allen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Want siet Heer v' <rhyme label="a" type="f">vianden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sullen verderuen <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die boosdoenders ten <rhyme label="b" type="m" corresp="#s5:l2">val</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sullen kommen met <rhyme label="a" type="f" corresp="#s5:l1">schanden</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Myn hoorne daer en <rhyme label="c" type="f">teghen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sal seer verhooghet <rhyme label="d" type="m">sijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ghy sult my doen gaen <rhyme label="d" type="m" corresp="#s5:l6">fyn</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Also d'Eenhoornen <rhyme label="c" type="f" corresp="#s5:l5">pleghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Heer met oly vol <rhyme label="a" type="f">trauwen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Werd' ick ghesalft seer <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Aen myn vianden <rhyme label="b" type="m" corresp="#s6:l2">daer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Sal ick myn lust <rhyme label="a" type="f" corresp="#s6:l1">aenschauwen</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Myn ooren sullen <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Haeren luit haest end <rhyme label="d" type="m">snel</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Aen der boosen val <rhyme label="d" type="m" corresp="#s6:l6">fel</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Die my willen <rhyme label="c" type="f" corresp="#s6:l5">verstooren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dan sal wassen end <rhyme label="a" type="f">bloeyen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">De mensch' oprecht end <rhyme label="b" type="m">vroom</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">End als de <rhyme label="b" type="m" corresp="#s7:l2">Palmeboom</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">End Cederboom oock <rhyme label="a" type="f" corresp="#s7:l1">groeyen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Sy die de Heer wilt <rhyme label="c" type="f">planten</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">In sijn voorhouen <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Sullen al in't <rhyme label="d" type="m" corresp="#s7:l6">ghemeyn</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Groenen aen allen <rhyme label="c" type="f" corresp="#s7:l5">kanten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Iae oock oudt sijnd' al <rhyme label="a" type="f">t'saeme</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Sullen se sijn <rhyme label="b" type="m">vruchtbaer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">End groen sijn voor end <rhyme label="b" type="m" corresp="#s8:l2">naer</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Vol vruchten <rhyme label="a" type="f" corresp="#s8:l1">aenghenaeme</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Dat van hen sij <rhyme label="c" type="f">beleden</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat Godt myn toevlucht <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Suyuer end reyn <rhyme label="d" type="m" corresp="#s8:l6">ghewis</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Van <rhyme label="c" type="f" corresp="#s8:l5">ongherechticheden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
