  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Minlick in <rhyme label="a" type="f">saysoene</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Therteken mi <rhyme label="b" type="m">verfraeyt</rhyme>/</l>
        <l id="s1:l3" met="3" type="-+">De veldekens staen <rhyme label="a" type="f" corresp="#s1:l1">groene</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met bloemkens <rhyme label="b" type="m" corresp="#s1:l2">besaeyt</rhyme>/</l>
        <l id="s1:l5" met="3" type="-+">Hoe soudick <rhyme label="c" type="f">ghetrueren</rhyme>/</l>
        <l id="s1:l6" met="3" type="-+">Van herten zijn <rhyme label="b" type="m" corresp="#s1:l2">verdraeyt</rhyme>/</l>
        <l id="s1:l7" met="3" type="-+">Vliedt wech alle <rhyme label="c" type="f" corresp="#s1:l5">rancueren</rhyme>/</l>
        <l id="s1:l8" met="3" type="-+">Wel ben ick <rhyme label="b" type="m" corresp="#s1:l2">ghepaeyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die suyuer claer <rhyme label="a" type="f">fonteynen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat louelick <rhyme label="b" type="m">graen</rhyme>/</l>
        <l id="s2:l3" met="3" type="-+">Vloeyt hier inden <rhyme label="a" type="f" corresp="#s2:l1">pleyne</rhyme>/</l>
        <l id="s2:l4" met="3" type="-+">Met aderkens <rhyme label="b" type="m" corresp="#s2:l2">ontdaen</rhyme>/</l>
        <l id="s2:l5" met="3" type="-+">Schoon springhen haer <rhyme label="c" type="f">conduyten</rhyme>/</l>
        <l id="s2:l6" met="3" type="-+">Lof moet zijs <rhyme label="b" type="m" corresp="#s2:l2">ontfaen</rhyme>/</l>
        <l id="s2:l7" met="3" type="-+">Danst nv wter <rhyme label="c" type="f" corresp="#s2:l5">muyten</rhyme>/</l>
        <l id="s2:l8" met="3" type="-+">Die wilt solaes <rhyme label="b" type="m" corresp="#s2:l2">aengaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dedel Ghendtsche <rhyme label="a" type="f">maecht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In eeren verre <rhyme label="b" type="m">vernaemt</rhyme>/</l>
        <l id="s3:l3" met="3" type="-+">Die mi wel <rhyme label="a" type="f" corresp="#s3:l1">behaecht</rhyme>/</l>
        <l id="s3:l4" met="3" type="-+">Heeft nv een feeste <rhyme label="b" type="m" corresp="#s3:l2">gheraemt</rhyme>/</l>
        <l id="s3:l5" met="3" type="-+">Eere reyn met <rhyme label="c" type="f">liefden</rhyme>/</l>
        <l id="s3:l6" met="3" type="-+">Sendick haer <rhyme label="b" type="m" corresp="#s3:l2">ongheschent</rhyme>/</l>
        <l id="s3:l7" met="3" type="-+">Haer lof sy gheerne <rhyme label="c" type="f" corresp="#s3:l5">briefden</rhyme>/</l>
        <l id="s3:l8" met="3" type="-+">So zijnse <rhyme label="b" type="m" corresp="#s3:l2">versaemt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
