  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O schoone glans van myn <rhyme label="a" type="m">Vrindin</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Ghy zyt de toe-sicht van myn soete <rhyme label="a" type="m" corresp="#s1:l1">min</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghy bint my in de liefdens <rhyme label="b" type="m">bandt</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Want myn jongh <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s1:l5" met="1" type="-+">Vol <rhyme label="c" type="m" corresp="#s1:l4">smert</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Is aen u schoon <rhyme label="b" type="m" corresp="#s1:l3">verpandt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vw' ooghskens klaerder als <rhyme label="a" type="m">Robyn</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">In dit gheval myn soete vangers <rhyme label="a" type="m" corresp="#s2:l1">zyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">U witte wanghe, ronde <rhyme label="b" type="m">borst</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">En lipkens <rhyme label="c" type="m">roodt</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">O <rhyme label="c" type="m" corresp="#s2:l4">doodt</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Is daer myn hert naer <rhyme label="b" type="m" corresp="#s2:l3">dorst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Noyt sagh-men schoonder marmer <rhyme label="a" type="m">beldt</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Oft met een soet penceel op doeck <rhyme label="a" type="m" corresp="#s3:l1">ghestelt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Vw' tanden witter als <rhyme label="b" type="m">abast</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">En poesel <rhyme label="c" type="m">kin</rhyme>,</l>
        <l id="s3:l5" met="1" type="-+">Den <rhyme label="c" type="m" corresp="#s3:l4">sin</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Houdt van myn vryheyt <rhyme label="b" type="m" corresp="#s3:l3">vast</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vw' witte wanghen, seedbaer <rhyme label="a" type="m">ganck</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">En ronde armkens, uw' stems soet <rhyme label="a" type="m" corresp="#s4:l1">gheblanck</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Beroert myn hert, myn ziel, en <rhyme label="b" type="m">bryn</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">V vrindt schoon <rhyme label="c" type="m">maeght</rhyme>,</l>
        <l id="s4:l5" met="1" type="-+">Die <rhyme label="c" type="m" corresp="#s4:l4">vraeght</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Om in u gonst te <rhyme label="b" type="m" corresp="#s4:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Al boodt my Jupiter syn <rhyme label="a" type="m">ryck</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">En wou Diana met my zyn <rhyme label="a" type="m" corresp="#s5:l1">ghelyck</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">U liet ick niet, Ã´ suyver <rhyme label="b" type="m">beelt</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Want myn <rhyme label="c" type="m">geneught</rhyme>,</l>
        <l id="s5:l5" met="1" type="-+">En <rhyme label="c" type="m" corresp="#s5:l4">vreught</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Is heel in u <rhyme label="b" type="m" corresp="#s5:l3">ghewelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
