<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>111246</id>
    <date>
      <year-start>1785</year-start>
      <year-end>1785</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="children's song">kinderlied</tag>
    </tags>
    <melody>Soldaat voor wie hebt ge u geweerd</melody>
    <categories>
      <category translation="children">kinderen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Triomph; ô Volk van <rhyme label="a" type="m">Nederland</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">Triomph; ô Boer en <rhyme label="b" type="f">Burger</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Gy zyt hier van verheevener <rhyme label="a" type="m" corresp="#s1:l1">stand</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">'t Sa zingt dan op een hoogeren <rhyme label="a" type="m" corresp="#s1:l1">trant</rhyme>;</l>
        <l id="s1:l5" met="5" type="+-">Vivat in Neêrland de Burger ende <rhyme label="c" type="m">Boer</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Vivat de Boer en de <rhyme label="d" type="f">Burger</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Elk Neêrlands Heer, een Prins, een <rhyme label="a" type="m">Graaf</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is hier of Boer of <rhyme label="b" type="f">Burger</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Hy doe zyn pligt dan is hy <rhyme label="a" type="m" corresp="#s2:l1">braaf</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Geen Boer of Burger is zyn <rhyme label="a" type="m" corresp="#s2:l1">slaaf</rhyme>;</l>
        <l id="s2:l5" met="5" type="+-">Vivat in Neêrland de Burger ende <rhyme label="c" type="m">Boer</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Vivat de Boer en de <rhyme label="d" type="f">Burger</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In Neêrland kan de <rhyme label="a" type="m">nyverheid</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Naar d'eer van vorsten <rhyme label="b" type="f">dingen</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">De Boer en Burger door zyn <rhyme label="a" type="m" corresp="#s3:l1">vleit</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Hier naar het Hoogste Gestoelte <rhyme label="a" type="m" corresp="#s3:l1">geleid</rhyme></l>
        <l id="s3:l5" met="5" type="+-">Vivat in Neêrland de Burger ende <rhyme label="c" type="m">Boer</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Vivat de Boer en de <rhyme label="d" type="f">Burger</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Neen Krygsman, gy dient hier geen <rhyme label="a" type="m">Vorst</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Gy dient hier vrye <rhyme label="b" type="f">staaten</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Gy dient het Volk dat lasten <rhyme label="a" type="m" corresp="#s4:l1">torscht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Hier is de boer en burger uw <rhyme label="a" type="m" corresp="#s4:l1">vorst</rhyme></l>
        <l id="s4:l5" met="5" type="+-">Vivat in Neêrland de burger en de <rhyme label="c" type="m">boer</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Vivat de boer en de <rhyme label="d" type="f">burger</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Het volk van Neêrland torscht geen <rhyme label="a" type="m">last</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die geen bevraag zou <rhyme label="b" type="f">dulden</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Met d'Overheid, die daar op <rhyme label="a" type="m" corresp="#s5:l1">past</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Stelt boer en burger zelf dien <rhyme label="a" type="m" corresp="#s5:l1">vast</rhyme>.</l>
        <l id="s5:l5" met="5" type="+-">Vivat in Neêrland de burger en de <rhyme label="c" type="m">boer</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Vivat de boer en de <rhyme label="d" type="f">burger</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Triomph; o vrye <rhyme label="a" type="m">batavier</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Ras word u heil <rhyme label="b" type="f">voldongen</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Ras kiest de boer en burger <rhyme label="a" type="m" corresp="#s6:l1">hier</rhyme></l>
        <l id="s6:l4" met="4" type="-+">De braafste zelf in 't Hoogst <rhyme label="a" type="m" corresp="#s6:l1">bestier</rhyme>.</l>
        <l id="s6:l5" met="5" type="+-">Vivat in Neêrland de burger en de <rhyme label="c" type="m">boer</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Vivat de boer en de <rhyme label="d" type="f">burger</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hoe wordt ons Neêrland weer <rhyme label="a" type="m">gered</rhyme>?</l>
        <l id="s7:l2" met="3" type="-+">Hoe kan het beter <rhyme label="b" type="f">bloeijen</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Als de boer en burger zyne <rhyme label="a" type="m" corresp="#s7:l1">Wet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Zelf op den Troon der vryheid <rhyme label="a" type="m" corresp="#s7:l1">zet</rhyme>;</l>
        <l id="s7:l5" met="5" type="+-">Vivat in Neêrland de burger en de <rhyme label="c" type="m">boer</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Vivat de boer en de <rhyme label="d" type="f">burger</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dan zal in Neêrland Deugd en <rhyme label="a" type="m">Pligt</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">Door scepter weder <rhyme label="b" type="f">zwaaijen</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Dan blyft de staat goed <rhyme label="a" type="m" corresp="#s8:l1">ingericht</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Als boer en burger blyft <rhyme label="a" type="m" corresp="#s8:l1">verlicht</rhyme>.</l>
        <l id="s8:l5" met="5" type="+-">Vivat in Neêrland de burger en de <rhyme label="c" type="m">boer</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Vivat de Boer en <rhyme label="d" type="f">burger</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
