      <category translation="narratives">verhalen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Liefde die is <rhyme label="a" type="m">blint</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gelijck als hier sal <rhyme label="b" type="f">blijcken</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Van de <rhyme label="b" type="f" corresp="#s1:l2">practijken</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">van een snoode <rhyme label="c" type="m">quant</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Die met een Edel Daem trekt uyt het <rhyme label="c" type="m" corresp="#s1:l4">lant</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ia heeft met zijn praet <rhyme label="d" type="m">geleyt</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Dat sy hem toont alle <rhyme label="d" type="m" corresp="#s1:l6">genegentheyt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Van <rhyme label="e" type="f">wederminne</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Sonder <rhyme label="e" type="f" corresp="#s1:l8">versinne</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Al in <rhyme label="d" type="m" corresp="#s1:l6">eerbaerheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sy was stil heen <rhyme label="a" type="m">gegaen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van Vader en van <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Suster en <rhyme label="b" type="f" corresp="#s2:l2">Broeder</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En de Vrienden <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Gaet sy verlaten tot haer <rhyme label="c" type="m" corresp="#s2:l4">ongeval</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En trecken samen uyt het <rhyme label="d" type="m">Landt</rhyme>,</l>
        <l id="s2:l7" met="5" type="-+">Tot haer oneer en door de vrienden <rhyme label="d" type="m" corresp="#s2:l6">schant</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En daer <rhyme label="e" type="f">beneeve</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">So neemt sy <rhyme label="e" type="f" corresp="#s2:l8">meede</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Gout en <rhyme label="d" type="m" corresp="#s2:l6">Diamant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy komen in <rhyme label="a" type="m">Zeelant</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">By Domburg wilt <rhyme label="b" type="f">weten</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Al na den <rhyme label="b" type="f" corresp="#s3:l2">eeten</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Seyt hy tot de <rhyme label="c" type="m">Weert</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Is hier geen plaets daer gy ons bey <rhyme label="c" type="m" corresp="#s3:l4">Logeert</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Op dat wy rusten van de <rhyme label="d" type="m">reys</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">Beschickt ons wel gy sult hebben u <rhyme label="d" type="m" corresp="#s3:l6">prijs</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Men sal u <rhyme label="e" type="f">loonen</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Met goude <rhyme label="e" type="f" corresp="#s3:l8">Kroonen</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">En wat gy ons <rhyme label="d" type="m" corresp="#s3:l6">eyst</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sijn Lief was in haer <rhyme label="a" type="m">rust</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Heeft haer tot slaep <rhyme label="b" type="f">begeven</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">En daer <rhyme label="b" type="f" corresp="#s4:l2">beneven</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Heeft sy haer <rhyme label="c" type="m">betrout</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Op hem waer in de Liefde was <rhyme label="c" type="m" corresp="#s4:l4">verflout</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Want in sijn hert was <rhyme label="d" type="m">valsigheyt</rhyme>,</l>
        <l id="s4:l7" met="5" type="-+">En heeft het op een Moort dog <rhyme label="d" type="m" corresp="#s4:l6">toegeleyt</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Als is <rhyme label="e" type="f">gebleken</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Maer van het <rhyme label="e" type="f" corresp="#s4:l8">teecken</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Tot den Hemel <rhyme label="d" type="m" corresp="#s4:l6">schreyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Als sy was in den <rhyme label="a" type="m">slaep</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">In ’t midden van haer <rhyme label="b" type="f">rusten</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Niet haer <rhyme label="b" type="f" corresp="#s5:l2">ontlusten</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Als dien vals <rhyme label="c" type="m">Minnaer</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Die sig met wreedtheyt toonde tegen <rhyme label="c" type="m" corresp="#s5:l4">haer</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">En neemt een Mes seer <rhyme label="d" type="m">opstinaet</rhyme>,</l>
        <l id="s5:l7" met="5" type="-+">Ende vermoorden haer op heter <rhyme label="d" type="m" corresp="#s5:l6">daet</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">In plaats van <rhyme label="e" type="f">Minnen</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Sonder <rhyme label="e" type="f" corresp="#s5:l8">versinnen</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Is ’t een <rhyme label="d" type="m" corresp="#s5:l6">Moordenaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy gaet dan haestig <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Pakt Gout ende <rhyme label="b" type="f">Iuweelen</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Neemt tot sijn <rhyme label="b" type="f" corresp="#s6:l2">deele</rhyme>.</l>
        <l id="s6:l4" met="3" type="+-">Alles van haer <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Dat in het kleyn voor hem lag op die <rhyme label="c" type="m" corresp="#s6:l4">stee</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Verlaet dan Zeelant metter <rhyme label="d" type="m">haest</rhyme>,</l>
        <l id="s6:l7" met="5" type="-+">Die Moordenaer en schijnt niet eens <rhyme label="d" type="m" corresp="#s6:l6">verbaest</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Maer gaet aen ’t <rhyme label="e" type="f">vlugten</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Sonder <rhyme label="e" type="f" corresp="#s6:l8">genugten</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Sport hem op het <rhyme label="d" type="m" corresp="#s6:l6">naest</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daer leyt dat Edel <rhyme label="a" type="m">bloet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ontrooft van goet en <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">En daer <rhyme label="b" type="f" corresp="#s7:l2">beneven</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Van niemant <rhyme label="c" type="m">bekent</rhyme>,</l>
        <l id="s7:l5" met="5" type="-+">Die haer aensiet in desen groot <rhyme label="c" type="m" corresp="#s7:l4">elent</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Sy leyt hier in haer bloet <rhyme label="d" type="m">versmoort</rhyme>,</l>
        <l id="s7:l7" met="5" type="-+">Siet wat de dwalende liefde brengt <rhyme label="d" type="m" corresp="#s7:l6">voort</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Niet als dan <rhyme label="e" type="f">klagten</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Die noyt <rhyme label="e" type="f" corresp="#s7:l8">versagten</rhyme>,</l>
        <l id="s7:l10" met="3" type="+-">En saeck noyt meer <rhyme label="d" type="m" corresp="#s7:l6">gehoort</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof gy Iuffers <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Laet dit u gaen ter <rhyme label="b" type="f">herten</rhyme>,</l>
        <l id="s8:l3" met="2" type="+-">Dees droeve <rhyme label="b" type="f" corresp="#s8:l2">smerte</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Van de <rhyme label="c" type="m">Minne-gloet</rhyme>,</l>
        <l id="s8:l5" met="5" type="-+">Die hier int eynt so bitter heeft <rhyme label="c" type="m" corresp="#s8:l4">gevoet</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Maer siet de liefde die is <rhyme label="d" type="m">blint</rhyme>,</l>
        <l id="s8:l7" met="5" type="-+">Als is gebleken aen dit Edel <rhyme label="d" type="m" corresp="#s8:l6">kint</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Gy meugt wel <rhyme label="e" type="f">minne</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Maer eerst <rhyme label="e" type="f" corresp="#s8:l8">versinne</rhyme>,</l>
        <l id="s8:l10" met="3" type="+-">Voor dat gy <rhyme label="d" type="m" corresp="#s8:l6">begint</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
