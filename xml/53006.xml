<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>53006</id>
    <date>
      <year-start>1826</year-start>
      <year-end>1837</year-end>
    </date>
    <location>
      <orginal-name>rotterdam (korte pannekoekstraat bij de nieuwe markt)</orginal-name>
      <standard-name>Binnerotte, Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9220344</lon>
        <lat>4.4778012</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>God save the king</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">God spaar d' Oranje Vorst!</l>
        <l id="s1:l2">God spaar dien ed'len Vorst!</l>
        <l id="s1:l3">God spaar den Vorst!</l>
        <l id="s1:l4">Hy steld uw dienst op prys!</l>
        <l id="s1:l5">Regeert ons goed en wys!</l>
        <l id="s1:l6">Maakt Willem oud en grys.</l>
        <l id="s1:l7">God spaar den Vorst!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">God spaar d' Oranje Vorst!</l>
        <l id="s2:l2">God spaar dien ed'len Vorst!</l>
        <l id="s2:l3">O spaar dien Vorst!</l>
        <l id="s2:l4">Alom de buurt in brand,</l>
        <l id="s2:l5">Alarm aan alle kant,</l>
        <l id="s2:l6">Maar rust in Nederland.</l>
        <l id="s2:l7">God spaar den Vorst!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">God spaar de Koningin!</l>
        <l id="s3:l2">Die ed'le Ryks Vorstin!</l>
        <l id="s3:l3">God spaar die Vrouw!</l>
        <l id="s3:l4">z' Is Willems Echtvriendin,</l>
        <l id="s3:l5">Mint Hem met ziel en zin,</l>
        <l id="s3:l6">Heeft ook zyn hart zyn min.</l>
        <l id="s3:l7">God spaar die Vrouw!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1">God spaar Oranjes Prins!</l>
        <l id="s4:l2">Spaar ons dien ed'len Prins!</l>
        <l id="s4:l3">O spaar de Prins!</l>
        <l id="s4:l4">Hy vocht met Leeuwenmoed,</l>
        <l id="s4:l5">Vergoot zyn Heldenbloed,</l>
        <l id="s4:l6">Weêrstond het Fransch gebroed.</l>
        <l id="s4:l7">O spaar die Prins!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1">God spaar Oranjes Prins!</l>
        <l id="s5:l2">Spaart ons dien ed'len Prins!</l>
        <l id="s5:l3">O spaar de Prins!</l>
        <l id="s5:l4">Schenkt Hem als 's Koningszoon,</l>
        <l id="s5:l5">Zyn moed en deugd ten loon,</l>
        <l id="s5:l6">Ook eens de Konings kroon.</l>
        <l id="s5:l7">Lang leef de Prins!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1">Schenkt Frederik ook uw gunst,</l>
        <l id="s6:l2">Schenkt aan dien Prins uw gunst,</l>
        <l id="s6:l3">God spaar die Prins,</l>
        <l id="s6:l4">Hy 's konings tweede kind,</l>
        <l id="s6:l5">Zoo goed, zoo welgezind,</l>
        <l id="s6:l6">Geacht, geëerd, bemind.</l>
        <l id="s6:l7">God spaar die Prins.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1">God spaar d' Oranje Stam,</l>
        <l id="s7:l2">God spaar ons Nassauws Stam,</l>
        <l id="s7:l3">O spaar die Stam,</l>
        <l id="s7:l4">Wat wierd van Nederland?</l>
        <l id="s7:l5">Hield Nassauws Stam geen Stand,</l>
        <l id="s7:l6">Wee dan ons Vaderland,</l>
        <l id="s7:l7">God spaar die Stam.</l>
      </lg>
    </lg>
  </text>
</song>
