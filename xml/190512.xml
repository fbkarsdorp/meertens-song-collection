<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190512</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Wy danken Neêrlands goede God, <rhyme label="a" type="m">Hoezée</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Die ons verlost heeft van dat Rot, <rhyme label="a" type="m" corresp="#s1:l1">Hoezée</rhyme></l>
        <l id="s1:l3" met="4" type="-+">De God van Neêrland is <rhyme label="b" type="m">getrouw</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En ook aan Willem van <rhyme label="b" type="m" corresp="#s1:l3">Nassouw</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Vivat Oranje <rhyme label="a" type="m" corresp="#s1:l1">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Van harte welkom ô Vorst, <rhyme label="a" type="m">Hoezée</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Wy betuige uit een zuivere borst, <rhyme label="a" type="m" corresp="#s2:l1">Hoezée</rhyme></l>
        <l id="s2:l3" met="4" type="-+">U komst is ons hoogst <rhyme label="b" type="m">aangenaam</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Wy moogen zingen nu <rhyme label="b" type="m" corresp="#s2:l3">voortaan</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Vivat Oranje <rhyme label="a" type="m" corresp="#s2:l1">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Hy die Oranje heeft geplant, <rhyme label="a" type="m">Hoezée</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Die U bewaard heeft door zyn hand, <rhyme label="a" type="m" corresp="#s3:l1">Hoezée</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Die schenkt U Heil in <rhyme label="b" type="m">overvloed</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Bewaar U in zyn gunste <rhyme label="b" type="m" corresp="#s3:l3">goed</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Vivat Oranje <rhyme label="a" type="m" corresp="#s3:l1">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Wy strooijen Bloemen op U neer, <rhyme label="a" type="m">Hoezée</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Wy zingen nu tot uwer Eer, <rhyme label="a" type="m" corresp="#s4:l1">Hoezée</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Gedankt zy God van <rhyme label="b" type="m">Nederland</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die U en ook den <rhyme label="b" type="m" corresp="#s4:l3">Burgerstand</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Herstelt heeft nu <rhyme label="a" type="m" corresp="#s4:l1">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Gezegent zy U Gemalin, <rhyme label="a" type="m">Hoezée</rhyme></l>
        <l id="s5:l2" met="5" type="-+">Ons lief en waardige Vorstin, <rhyme label="a" type="m" corresp="#s5:l1">Hoezée</rhyme></l>
        <l id="s5:l3" met="4" type="-+">U dierbaar Spruiten met <rhyme label="b" type="m">malkaar</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die werde Gezeegend Jaar op <rhyme label="b" type="m" corresp="#s5:l3">Jaar</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Vivat Oranje <rhyme label="a" type="m" corresp="#s5:l1">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Neemt dit in gunste van ons aan, <rhyme label="a" type="m">Hoezée</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Dees wensch van ons Zevenen gedaan, <rhyme label="a" type="m" corresp="#s6:l1">Hoezée</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Wy zyn wel laag en <rhyme label="b" type="m">onbekwaam</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dog uit een oprecht hart <rhyme label="b" type="m" corresp="#s6:l3">gedaan</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Vivat Oranje <rhyme label="a" type="m" corresp="#s6:l1">Hoezée</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
