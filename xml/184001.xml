<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184001</id>
    <date>
      <year-start>1806</year-start>
      <year-end>1806</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Frères et Compagnons</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Aimables <rhyme label="a" type="m">nourissons</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dont la tendre <rhyme label="b" type="f">paupière</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Grace â nos <rhyme label="a" type="m" corresp="#s1:l1">leçons</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Contemple la <rhyme label="b" type="f" corresp="#s1:l2">lumière</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Voyez votre <rhyme label="c" type="m">bonheur</rhyme>:</l>
        <l id="s1:l6" met="5" type="-+">A la vertu nous accordons <rhyme label="d" type="f">éstime</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Et notre coeur est pour le <rhyme label="c" type="m" corresp="#s1:l5">coeur</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Un tribut <rhyme label="d" type="f" corresp="#s1:l6">légitime</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ici le prince <rhyme label="a" type="m">admis</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Est un homme <rhyme label="b" type="f">ordinaire</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Qui trouve des <rhyme label="a" type="m" corresp="#s2:l1">amis</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Que son rang ne peut <rhyme label="b" type="f" corresp="#s2:l2">faire</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Vous êtes ses <rhyme label="c" type="m">égaux</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Et le passez, selon notre <rhyme label="d" type="f">prudence</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Si vous montrez dans nos <rhyme label="c" type="m" corresp="#s2:l5">travaux</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Plus vive <rhyme label="d" type="f" corresp="#s2:l6">intelligence</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Honorez vos <rhyme label="a" type="m">mentors</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Pratiquez leur <rhyme label="b" type="f">doctrine</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Méritez les <rhyme label="a" type="m" corresp="#s3:l1">trésors</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Que l'Ordre vous <rhyme label="b" type="f" corresp="#s3:l2">destine</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Soyez soumis, <rhyme label="c" type="m">discrets</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">C'est le chemin qui mène à la <rhyme label="d" type="f">victoire</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Chez nous les lauriers sont tout <rhyme label="c" type="m" corresp="#s3:l5">prèts</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Pour qui chérit la <rhyme label="d" type="f" corresp="#s3:l6">gloire</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">D'une tendre <rhyme label="a" type="m">union</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Venez ferrer la <rhyme label="b" type="f">chaine</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Joignez <rhyme label="a" type="m" corresp="#s4:l1">l'intention</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Au gout qui nous <rhyme label="b" type="f" corresp="#s4:l2">entraine</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Et que notre <rhyme label="c" type="m">unité</rhyme></l>
        <l id="s4:l6" met="5" type="-+">Soit en tout tems le flansbeau de votre <rhyme label="d" type="f">ame</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Nous accroitrons votre <rhyme label="c" type="m" corresp="#s4:l5">beauté</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Sans redouter le <rhyme label="d" type="f" corresp="#s4:l6">blame</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
