  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">LEst gonck ick <rhyme label="a" type="f">slapen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Om rust te <rhyme label="a" type="f" corresp="#s1:l1">rapen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En lagh int <rhyme label="b" type="m">bedt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Daer ick haest wert <rhyme label="b" type="m" corresp="#s1:l3">uyt-geset</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Die cleyne swarte <rhyme label="c" type="f">dieren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die op de bedden <rhyme label="c" type="f" corresp="#s1:l5">swieren</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Daer soetiens inde <rhyme label="d" type="m">wol</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Van mijn sargie hielen <rhyme label="d" type="m" corresp="#s1:l7">hol</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Sy staecken,, En sy <rhyme label="e" type="f">braecken</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Om op mijn vlees te <rhyme label="e" type="f" corresp="#s1:l9">raecken</rhyme>,</l>
        <l id="s1:l11" met="2" type="-+">Met macht sy <rhyme label="f" type="f">dronghen</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Op dat vel my <rhyme label="f" type="f" corresp="#s1:l11">spronghen</rhyme>,</l>
        <l id="s1:l13" met="2" type="-+">Aten met <rhyme label="g" type="f">vreughden</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Soo veel sy van my <rhyme label="g" type="f" corresp="#s1:l13">meughden</rhyme>,</l>
        <l id="s1:l15" met="3" type="-+">Soo dat ick door <rhyme label="h" type="m">t'ghebyt</rhyme></l>
        <l id="s1:l16" met="3" type="-+">Niet cost slapen gans den <rhyme label="h" type="m" corresp="#s1:l15">tijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Dees diefsche <rhyme label="a" type="f">proeyen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">En dulle <rhyme label="a" type="f" corresp="#s2:l1">vloeyen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">VVaeren seer <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">t Scheen sy droeghen op my <rhyme label="b" type="m" corresp="#s2:l3">haet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En ick begonst te <rhyme label="c" type="f">crouwen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Te vrijven en te <rhyme label="c" type="f" corresp="#s2:l5">douwen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maer staecken even <rhyme label="d" type="m">wel</rhyme></l>
        <l id="s2:l8" met="3" type="-+">VVacker door mijn huyt en <rhyme label="d" type="m" corresp="#s2:l7">vel</rhyme>;</l>
        <l id="s2:l9" met="3" type="+-">Sy beten,, In het <rhyme label="e" type="f">eten</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Malcanderen oock <rhyme label="e" type="f" corresp="#s2:l9">smeten</rhyme>,</l>
        <l id="s2:l11" met="2" type="-+">En ick vvou <rhyme label="f" type="f">nijpen</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Om een te <rhyme label="f" type="f" corresp="#s2:l11">grijpen</rhyme>,</l>
        <l id="s2:l13" met="2" type="-+">t'VVas maer om te <rhyme label="g" type="f">scheyden</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Den tvvist soo van hun <rhyme label="g" type="f" corresp="#s2:l13">beyden</rhyme>,</l>
        <l id="s2:l15" met="3" type="-+">Maer saegen stracx mijn <rhyme label="h" type="m">hant</rhyme></l>
        <l id="s2:l16" met="3" type="-+">En sy liepen toens van <rhyme label="h" type="m" corresp="#s2:l15">cant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Hier een <rhyme label="a" type="f">gheseten</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Heeft vveer <rhyme label="a" type="f" corresp="#s3:l1">ghebeten</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Seer doencker <rhyme label="b" type="m">vvast</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daerom vvert sy niet <rhyme label="b" type="m" corresp="#s3:l3">verrast</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Maer ick dacht goey <rhyme label="c" type="f">helden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ick sal't u vvel <rhyme label="c" type="f" corresp="#s3:l5">vergelden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Quamp den dagh maer <rhyme label="d" type="m">aen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Svveir ick sou u soecken <rhyme label="d" type="m" corresp="#s3:l7">gaen</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">K'vvou vangen,, Hun en <rhyme label="e" type="f">hangen</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Maer costse niet <rhyme label="e" type="f" corresp="#s3:l9">ghelanghen</rhyme>,</l>
        <l id="s3:l11" met="2" type="-+">VVant uyt mijn <rhyme label="f" type="f">duymen</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Sprongen sy als <rhyme label="f" type="f" corresp="#s3:l11">pluymen</rhyme>,</l>
        <l id="s3:l13" met="2" type="-+">Ick vongh noch een <rhyme label="g" type="f">ionghe</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">Maer die heeft het haest <rhyme label="g" type="f" corresp="#s3:l13">onspronghe</rhyme>,</l>
        <l id="s3:l15" met="3" type="-+">En naus was ickse <rhyme label="h" type="m">quijt</rhyme></l>
        <l id="s3:l16" met="3" type="-+">Oft sy maeckten weer <rhyme label="h" type="m" corresp="#s3:l15">ghebijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ick moest wel <rhyme label="a" type="f">voelen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Hun spitsche <rhyme label="a" type="f" corresp="#s4:l1">smoelen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Daer als een <rhyme label="b" type="m">els</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Sy me staecken in mijn <rhyme label="b" type="m" corresp="#s4:l3">pels</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Oft ick hun toens wou <rhyme label="c" type="f">smijten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Sy bleven my noch <rhyme label="c" type="f" corresp="#s4:l5">bijten</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Een sat daer op mijn <rhyme label="d" type="m">rugh</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">En die stack gelijck een <rhyme label="d" type="m" corresp="#s4:l7">mugh</rhyme>.</l>
        <l id="s4:l9" met="3" type="+-">Ick vaeghden,, En ick <rhyme label="e" type="f">claegden</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Maer sy daer niet naer <rhyme label="e" type="f" corresp="#s4:l9">vraegden</rhyme>,</l>
        <l id="s4:l11" met="2" type="-+">Ten helpt gheen <rhyme label="f" type="f">schreeuwen</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+">Sy bijten ons als <rhyme label="f" type="f" corresp="#s4:l11">Leeuwen</rhyme>,</l>
        <l id="s4:l13" met="2" type="-+">Cnaeghen ons <rhyme label="g" type="f">billen</rhyme></l>
        <l id="s4:l14" met="3" type="-+">En doen dat vlees <rhyme label="g" type="f" corresp="#s4:l13">onsswillen</rhyme>,</l>
        <l id="s4:l15" met="3" type="-+">Ia maecken gans ons <rhyme label="h" type="m">lijf</rhyme></l>
        <l id="s4:l16" met="3" type="-+">Door t'gebijt met bobbels <rhyme label="h" type="m" corresp="#s4:l15">stijf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">VVacker aen <rhyme label="a" type="f">t'crouwen</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Moest ick my toens <rhyme label="a" type="f" corresp="#s5:l1">gouwen</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">VVant sonder <rhyme label="b" type="m">vrees</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Liepen over al mijn <rhyme label="b" type="m" corresp="#s5:l3">vlees</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Met al-te-bey mijn <rhyme label="c" type="f">veren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Moest ickse van my <rhyme label="c" type="f" corresp="#s5:l5">vveren</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">VVant crabden ick mijn <rhyme label="d" type="m">teen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">d'Ander beet weer op mijn <rhyme label="d" type="m" corresp="#s5:l7">been</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Vant laecken,, Op mijn <rhyme label="e" type="f">kaeken</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Soo voelden ick hun <rhyme label="e" type="f" corresp="#s5:l9">raecken</rhyme>,</l>
        <l id="s5:l11" met="2" type="-+">K'cost niet <rhyme label="f" type="f">ghedueren</rhyme>,</l>
        <l id="s5:l12" met="2" type="-+">K'moest dat <rhyme label="f" type="f" corresp="#s5:l11">besueren</rhyme>,</l>
        <l id="s5:l13" met="2" type="-+">Noch van die <rhyme label="g" type="f">grooten</rhyme></l>
        <l id="s5:l14" met="3" type="-+">Een met die lange <rhyme label="g" type="f" corresp="#s5:l13">pooten</rhyme></l>
        <l id="s5:l15" met="3" type="-+">Die stack my in mijn <rhyme label="h" type="m">sy</rhyme></l>
        <l id="s5:l16" met="3" type="-+">K'meynden sy vermoorden <rhyme label="h" type="m" corresp="#s5:l15">my</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ontrent den <rhyme label="a" type="f">morgen</rhyme></l>
        <l id="s6:l2" met="2" type="-+">VVast al <rhyme label="a" type="f" corresp="#s6:l1">gheborghen</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">En t'saem <rhyme label="b" type="m">ghevoet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met een pint schier van mijn <rhyme label="b" type="m" corresp="#s6:l3">bloet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Phoebus die quamp <rhyme label="c" type="f">straelen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Ick vvou hun <rhyme label="c" type="f" corresp="#s6:l5">betaelen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Spelden hier <rhyme label="d" type="m">crick-crack</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En hun al die beenen <rhyme label="d" type="m" corresp="#s6:l7">brack</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Dees proeyen,, Gonc ic <rhyme label="e" type="f">doeyen</rhyme></l>
        <l id="s6:l10" met="3" type="+-">En al dees dulle <rhyme label="e" type="f" corresp="#s6:l9">vloeyen</rhyme>,</l>
        <l id="s6:l11" met="2" type="-+">Die ick cost <rhyme label="f" type="f">vinden</rhyme></l>
        <l id="s6:l12" met="2" type="-+">Mijn hant <rhyme label="f" type="f" corresp="#s6:l11">verslinden</rhyme>,</l>
        <l id="s6:l13" met="2" type="-+">K'gaf geen <rhyme label="g" type="f">quartieren</rhyme></l>
        <l id="s6:l14" met="3" type="-+">Aen dees vervvoede <rhyme label="g" type="f" corresp="#s6:l13">dieren</rhyme>.</l>
        <l id="s6:l15" met="3" type="-+">Maer ginckse al <rhyme label="h" type="m">gelat</rhyme></l>
        <l id="s6:l16" met="3" type="-+">Met mijn nagels aen hun <rhyme label="h" type="m" corresp="#s6:l15">gat</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Hoort eens int <rhyme label="a" type="f">soecken</rhyme></l>
        <l id="s7:l2" met="2" type="-+">In alle <rhyme label="a" type="f" corresp="#s7:l1">hoecken</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Heb icker <rhyme label="b" type="m">g'hadt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Die bescheelijck vvaeren <rhyme label="b" type="m" corresp="#s7:l3">sadt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En ick sal t vvel <rhyme label="c" type="f">onthouvven</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Vier ander vvouvven <rhyme label="c" type="f" corresp="#s7:l5">spouvven</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En die t vvater <rhyme label="d" type="m">had'</rhyme></l>
        <l id="s7:l8" met="3" type="-+">VVas soo dick gelijc een <rhyme label="d" type="m" corresp="#s7:l7">pad'</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">Seer droencken (soo sy <rhyme label="e" type="f">cloenken</rhyme>)</l>
        <l id="s7:l10" met="3" type="+-">En vvaeren al <rhyme label="e" type="f" corresp="#s7:l9">beschoencken</rhyme>,</l>
        <l id="s7:l11" met="2" type="-+">Ses sieck van <rhyme label="f" type="f">brommen</rhyme></l>
        <l id="s7:l12" met="2" type="-+">Sagh ick daer <rhyme label="f" type="f" corresp="#s7:l11">commen</rhyme>,</l>
        <l id="s7:l13" met="2" type="-+">Die my <rhyme label="g" type="f">ghebeten</rhyme>;</l>
        <l id="s7:l14" met="3" type="-+">Te veel hadden <rhyme label="g" type="f" corresp="#s7:l13">ghegheten</rhyme>,</l>
        <l id="s7:l15" met="3" type="-+">De rest van mijnen <rhyme label="h" type="m">vvijn</rhyme></l>
        <l id="s7:l16" met="3" type="-+">Creghen al-te-mael <rhyme label="h" type="m" corresp="#s7:l15">t'flescijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Princen uyt <rhyme label="a" type="f">vaeten</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Drinckt dan by <rhyme label="a" type="f" corresp="#s8:l1">maeten</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Dat ghy niet <rhyme label="b" type="m">sadt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Als een vloey loopt lancx de <rhyme label="b" type="m" corresp="#s8:l3">Stadt</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">En vvilt ghy dan gaen <rhyme label="c" type="f">slaepen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Soo vanght die svvarte <rhyme label="c" type="f" corresp="#s8:l5">schaepen</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Die ons t <rhyme label="d" type="m">somers-tijt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Maecken sulcken fel <rhyme label="d" type="m" corresp="#s8:l7">gebijt</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">VVilt jaeghen Dan dees <rhyme label="e" type="f">daeghen</rhyme></l>
        <l id="s8:l10" met="3" type="+-">De vloeyen uyt u <rhyme label="e" type="f" corresp="#s8:l9">craeghen</rhyme>,</l>
        <l id="s8:l11" met="2" type="-+">Sa doet hun <rhyme label="f" type="f">smyten</rhyme></l>
        <l id="s8:l12" met="2" type="-+">Eer sy u <rhyme label="f" type="f" corresp="#s8:l11">byten</rhyme>,</l>
        <l id="s8:l13" met="2" type="-+">VVant dees <rhyme label="g" type="f">ghesellen</rhyme></l>
        <l id="s8:l14" met="3" type="-+">Ons s'nachts het lijf seer <rhyme label="g" type="f" corresp="#s8:l13">quellen</rhyme>,</l>
        <l id="s8:l15" met="3" type="-+">Vanght een Vloeyken <rhyme label="h" type="m">mé</rhyme></l>
        <l id="s8:l16" met="3" type="-+">VVilt ghy leven s'nachts in <rhyme label="h" type="m" corresp="#s8:l15">vré</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
