<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157836</id>
    <date>
      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon liefje jent zeer excellent</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ignati <rhyme label="a" type="m">vroet</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Van edele <rhyme label="a" type="m" corresp="#s1:l1">bloet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ghy zijt Kers-nacht <rhyme label="b" type="f">gheboren</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Als Iesus <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">In een Stall’ van <rhyme label="a" type="m" corresp="#s1:l1">ootmoet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die u met liefde <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wt duysent heeft <rhyme label="b" type="f" corresp="#s1:l3">vercoren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Na ‘swerelts <rhyme label="a" type="m">raet</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Die pooght om <rhyme label="a" type="m" corresp="#s2:l1">baet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Socht ghy <rhyme label="b" type="f">Krijchs-handel</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">In desen <rhyme label="a" type="m" corresp="#s2:l1">staet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Vol laghen ghy nu <rhyme label="a" type="m" corresp="#s2:l1">gaet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En loopt in doodes <rhyme label="a" type="m" corresp="#s2:l1">graet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Na vrede keert u <rhyme label="b" type="f" corresp="#s2:l3">wandel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Petrus <rhyme label="a" type="m">verscheen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Ghenas u <rhyme label="a" type="m" corresp="#s3:l1">been</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maria Godes <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Verkreegh voor <rhyme label="a" type="m" corresp="#s3:l1">heen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">De suyverheyt met <rhyme label="a" type="m" corresp="#s3:l1">een</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En toonde u met <rhyme label="a" type="m" corresp="#s3:l1">vreen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Haer kint voor een <rhyme label="b" type="f" corresp="#s3:l3">behoeder</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">’t Crijghs <rhyme label="a" type="m">ghewaet</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Ghy haest <rhyme label="a" type="m" corresp="#s4:l1">verlaet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En hinght ten <rhyme label="b" type="f">thoone</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Te <rhyme label="a" type="m" corresp="#s4:l1">Montserraet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Met een vrolijck <rhyme label="a" type="m" corresp="#s4:l1">ghelaet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Liet varen ‘smenschen <rhyme label="a" type="m" corresp="#s4:l1">praet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Om de hemelsche <rhyme label="b" type="f" corresp="#s4:l3">croone</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Nae ‘theylich <rhyme label="a" type="m">Landt</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Liept ghy ter <rhyme label="a" type="m" corresp="#s5:l1">hant</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">In arm Pelgrims <rhyme label="b" type="f">cleeren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Christus <rhyme label="a" type="m" corresp="#s5:l1">playsant</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Verscheen u <rhyme label="a" type="m" corresp="#s5:l1">triumphant</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Als men u sloech ter <rhyme label="a" type="m" corresp="#s5:l1">kant</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En wilde doen <rhyme label="b" type="f" corresp="#s5:l3">weer-keeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Buyten <rhyme label="a" type="m">Parijs</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Vol eer en <rhyme label="a" type="m" corresp="#s6:l1">prijs</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hebt ghy Godt trouw <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Als seer <rhyme label="a" type="m" corresp="#s6:l1">onwijs</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Den  Achtsten <rhyme label="a" type="m" corresp="#s6:l1">ouden-grijs</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Weer stont het claer <rhyme label="a" type="m" corresp="#s6:l1">bewijs</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Want Godes Kerck <rhyme label="b" type="f" corresp="#s6:l3">verheven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Aen Tibers <rhyme label="a" type="m">Zoom</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Quaemt ghy te <rhyme label="a" type="m" corresp="#s7:l1">Room</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Om mannen uyt te <rhyme label="b" type="f">lesen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Tot haet en <rhyme label="a" type="m" corresp="#s7:l1">schroom</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Vander hellen <rhyme label="a" type="m" corresp="#s7:l1">vloed-stroom</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Maer Iesus, seyde, <rhyme label="a" type="m" corresp="#s7:l1">vroom</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ick sal u hulper <rhyme label="b" type="f" corresp="#s7:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Verre <rhyme label="a" type="m">bekent</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Seer <rhyme label="a" type="m" corresp="#s8:l1">excellent</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">De Societeyt <rhyme label="b" type="f">Iesu</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Haer <rhyme label="a" type="m" corresp="#s8:l1">fondament</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">In veelen <rhyme label="a" type="m" corresp="#s8:l1">in-geprent</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Hebt ghy in Godt <rhyme label="a" type="m" corresp="#s8:l1">vol-endt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">En Satan seyt, ick vrees’ <rhyme label="b" type="f" corresp="#s8:l3">u</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Den <rhyme label="a" type="m">Aposstaet</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Luther vol <rhyme label="a" type="m" corresp="#s9:l1">haet</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En vleeschelijcke <rhyme label="b" type="f">lusten</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Die boos van <rhyme label="a" type="m" corresp="#s9:l1">daet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Verliet sijn hoghen <rhyme label="a" type="m" corresp="#s9:l1">staet</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En stroyde Ketters <rhyme label="a" type="m" corresp="#s9:l1">zaey</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Alles bracht in <rhyme label="b" type="f" corresp="#s9:l3">onrusten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Als oock <rhyme label="a" type="m">Calvijn</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Den vos seer <rhyme label="a" type="m" corresp="#s10:l1">fijn</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Die door loose <rhyme label="b" type="f">kroken</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Op dien <rhyme label="a" type="m" corresp="#s10:l1">termijn</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Quam spouwen ‘thelsch <rhyme label="a" type="m" corresp="#s10:l1">fenijn</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Onder Gods leeringhs <rhyme label="a" type="m" corresp="#s10:l1">schijn</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Hebt ghy den neck <rhyme label="b" type="f" corresp="#s10:l3">ghebroken</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Francois <rhyme label="a" type="m">Xavier</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Den eed’len <rhyme label="a" type="m" corresp="#s11:l1">saphier</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Is door u <rhyme label="b" type="f">ghesonden</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Vol Hemels <rhyme label="a" type="m" corresp="#s11:l1">vier</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Na Indien van <rhyme label="a" type="m" corresp="#s11:l1">hier</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">O door des Gheests <rhyme label="a" type="m" corresp="#s11:l1">bestier</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">’tEuangeli te <rhyme label="b" type="f" corresp="#s11:l3">verkonden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">V soet <rhyme label="a" type="m">advijs</rhyme>,</l>
        <l id="s12:l2" met="2" type="-+">Was al tot <rhyme label="a" type="m" corresp="#s12:l1">prijs</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En meerdere Godts <rhyme label="b" type="f">eere</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Een licht <rhyme label="a" type="m" corresp="#s12:l1">exquijs</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Met der stralen <rhyme label="a" type="m" corresp="#s12:l1">uyt-rijs</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Na ‘sHeylicheyts <rhyme label="a" type="m" corresp="#s12:l1">bewijs</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Scheen uyt u aensicht <rhyme label="b" type="f" corresp="#s12:l3">veere</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="2" type="-+">D’Engelen <rhyme label="a" type="m">bly</rhyme>.</l>
        <l id="s13:l2" met="2" type="-+">Met <rhyme label="a" type="m" corresp="#s13:l1">melody</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Heeftmen daer hooren <rhyme label="b" type="f">singhen</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">Twee daghen <rhyme label="a" type="m" corresp="#s13:l1">vry</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Als u tombe ter <rhyme label="a" type="m" corresp="#s13:l1">zy</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Werde ghedraghen <rhyme label="a" type="m" corresp="#s13:l1">by</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Den Autaer, daer zy <rhyme label="b" type="f" corresp="#s13:l3">gingen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="2" type="-+">Princelijck <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s14:l2" met="2" type="-+">Die’t quaet <rhyme label="a" type="m" corresp="#s14:l1">verwan</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Om het goet te <rhyme label="b" type="f">verwerven</rhyme>,</l>
        <l id="s14:l4" met="2" type="-+">Als wy hier <rhyme label="a" type="m" corresp="#s14:l1">van</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Moeten elders komen <rhyme label="a" type="m" corresp="#s14:l1">an</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Wilt ons toch helpen <rhyme label="a" type="m" corresp="#s14:l1">dan</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Roomsch Katholijck te <rhyme label="b" type="f" corresp="#s14:l3">sterven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
