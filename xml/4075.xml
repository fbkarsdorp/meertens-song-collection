<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4075</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Puer nobis nascitur</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MEt rechte singhen wij nieuwen <rhyme label="a" type="m">sanck</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Heer Iesus is <rhyme label="b" type="f">gheboren</rhyme></l>
        <l id="s1:l3" met="4" type="-+">De mensceyt heeft hi <rhyme label="c" type="m">aengenomen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat sal ons allen <rhyme label="b" type="f" corresp="#s1:l2">vromen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al dat is / ende wesen <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Heeft God in sijnder <rhyme label="b" type="f">machte</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Al sonder wee was hi <rhyme label="c" type="m">gheboren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ter couder <rhyme label="b" type="f" corresp="#s2:l2">middernachte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Segt mi lieue heer <rhyme label="a" type="m">Ioseph</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Was daer eenighe <rhyme label="b" type="f">feeste</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Daer dat kint gheboren <rhyme label="c" type="m">was</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tusscen twee stomme <rhyme label="b" type="f" corresp="#s3:l2">beesten</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daer en was bont noch <rhyme label="a" type="m">grau</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Noch pelsen noch <rhyme label="b" type="f">samnijt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Oreten was sijn <rhyme label="c" type="m">wieghestroo</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat is ons een groot <rhyme label="b" type="f" corresp="#s4:l2">iolijt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die osse ende die <rhyme label="a" type="m">ezel</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Bedreuen daer groot <rhyme label="b" type="f">wonder</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Daer dat kint gheboren <rhyme label="c" type="m">was</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In doeceklkijns <rhyme label="b" type="f" corresp="#s5:l2">ghewonden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O o o o <rhyme label="a" type="m">gloria</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat soetste vander <rhyme label="b" type="f">minnen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Dat is Iesus Marien <rhyme label="c" type="m">soon</rhyme></l>
        <l id="s6:l4" met="3" type="-+">God laet ons hem <rhyme label="b" type="f" corresp="#s6:l2">ghewinnen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
