      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Psalm 100 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Iesu! dijne soete <rhyme label="a" type="m">Naem</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Is alle mijn vreuchd' te <rhyme label="a" type="m" corresp="#s1:l1">saem</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">O Iesu! mijne <rhyme label="b" type="m">soetigheyt</rhyme></l>
        <l id="s1:l4" met="4" type="-+">In uwe Naem is <rhyme label="b" type="m" corresp="#s1:l3">op-geleyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat ick van allen heb <rhyme label="a" type="m">bedacht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Wat ick in allen heb <rhyme label="a" type="m" corresp="#s2:l1">betracht</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">O Iesu! Godes eygen <rhyme label="b" type="m">Soon</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ghy spant van alle dingh de <rhyme label="b" type="m" corresp="#s2:l3">Kroon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O Iesu! mijn bekeert <rhyme label="a" type="m">gemoet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Van uwen Name werdt <rhyme label="a" type="m" corresp="#s3:l1">gegroet</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">O Iesu! mijn bedroefde <rhyme label="b" type="m">Ziel</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In uwen Name, Godt <rhyme label="b" type="m" corresp="#s3:l3">geviel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Iesu! Heerscher <rhyme label="a" type="m">over-al</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hoe soet is uwe Naem <rhyme label="a" type="m" corresp="#s4:l1">gheval</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gelijck den Honigh in de <rhyme label="b" type="m">mondt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die Medicijn het hert <rhyme label="b" type="m" corresp="#s4:l3">gesondt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Soecker, isse <rhyme label="a" type="m">vriendelijck</rhyme>;</l>
        <l id="s5:l2" met="4" type="-+">De Vinder, isser <rhyme label="a" type="m" corresp="#s5:l1">vrede-rijck</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Den Hoper, isse <rhyme label="b" type="m">deuchdelijck</rhyme>;</l>
        <l id="s5:l4" met="4" type="-+">Den Hebber, isse <rhyme label="b" type="m" corresp="#s5:l3">vreuchde-rijck</rhyme>;</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Vluchter, isse sijnen <rhyme label="a" type="m">wijck</rhyme>;</l>
        <l id="s6:l2" met="4" type="-+">De Duchter, eenen <rhyme label="a" type="m" corresp="#s6:l1">onbeswijck</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Den Armen, eene soete <rhyme label="b" type="m">troost</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">In het bedroefde West en <rhyme label="b" type="m" corresp="#s6:l3">Oost</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Geluckich is de vrome <rhyme label="a" type="m">tongh</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die vrolijck uwen Name <rhyme label="a" type="m" corresp="#s7:l1">songh</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Geluckigh het geloovigh <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Tot u, dat op-getrocke <rhyme label="b" type="m" corresp="#s7:l3">wert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wel aen, o soete Iesu! <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Gedreven mijne Ziele <rhyme label="a" type="m" corresp="#s8:l1">zy</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">In alle swarigheyt tot <rhyme label="b" type="m">dijn</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Om allesins gevrijt te <rhyme label="b" type="m" corresp="#s8:l3">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
