    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Weest gegroet <rhyme label="a" type="f">Coninginne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">moeder der <rhyme label="b" type="m">bermherticheijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">den bedructen <rhyme label="a" type="f" corresp="#s1:l1">vrindinne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">ons hoep, leuen, <rhyme label="b" type="m" corresp="#s1:l2">soeticheijt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Tot v roepen wij <rhyme label="a" type="f">armen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ballingen van Eua <rhyme label="b" type="m">wreet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Tot v suchten en <rhyme label="a" type="f" corresp="#s2:l1">kermen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">inden dal van alle <rhyme label="b" type="m" corresp="#s2:l2">leet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Och onse <rhyme label="a" type="f">voorsprakersse</rhyme></l>
        <l id="s3:l2" met="3" type="-+">wilt sien goedertieren <rhyme label="b" type="m">neer</rhyme></l>
        <l id="s3:l3" met="3" type="-+">op ons o <rhyme label="a" type="f" corresp="#s3:l1">middelerse</rhyme></l>
        <l id="s3:l4" met="3" type="-+">met Jesvm ons lieuen <rhyme label="b" type="m" corresp="#s3:l2">heer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghij nae deesen <rhyme label="a" type="f">allende</rhyme></l>
        <l id="s4:l2" met="3" type="-+">thoent ons uwen sone <rhyme label="b" type="m">blijdt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">int hemelsche <rhyme label="a" type="f" corresp="#s4:l1">belende</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Vrou, mooder, <rhyme label="b" type="m" corresp="#s4:l2">gebenedijt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O genaedijge <rhyme label="a" type="f">vrouwe</rhyme></l>
        <l id="s5:l2" met="3" type="-+">o mooder <rhyme label="b" type="m">goodertieren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Zoeticheijt sonder <rhyme label="a" type="f" corresp="#s5:l1">rouwe</rhyme></l>
        <l id="s5:l4" met="3" type="-+">reijn maecht wilt ons <rhyme label="b" type="m" corresp="#s5:l2">regieren</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Onsser ballingen <rhyme label="a" type="f">ontfermt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">helpt ons ras vuijt dit <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">oeck van alle quaet <rhyme label="a" type="f" corresp="#s6:l1">bescermt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">door v ons kan deren <rhyme label="b" type="m" corresp="#s6:l2">nijet</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Op dat wij moogen <rhyme label="a" type="f">waerdich</rhyme></l>
        <l id="s7:l2" met="3" type="-+">der beloften christi <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s7:l3" met="3" type="-+">wilt voor ons bidden <rhyme label="a" type="f" corresp="#s7:l1">vaerdich</rhyme></l>
        <l id="s7:l4" met="3" type="-+">uwen soon reijn <rhyme label="b" type="m" corresp="#s7:l2">maechdekijn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
