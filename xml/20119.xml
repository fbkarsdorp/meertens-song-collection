<song>
  <meta>
    <id>20119</id>
    <date>
      <year-start>1654</year-start>
      <year-end>1654</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kluchtlied" translation="humorous song">kluchtlied</tag>
    </tags>
    <melody>Laburette</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IK mach dat gebruy niet <rhyme label="a" type="f">lyen</rhyme>.</l>
        <l id="s1:l2" met="2" type="+-">Hou je <rhyme label="b" type="m">rust</rhyme>.</l>
        <l id="s1:l3" met="2" type="+-">Hebje <rhyme label="b" type="m" corresp="#s1:l2">lust</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Om wat tegen my te <rhyme label="a" type="f" corresp="#s1:l1">stryen</rhyme>?</l>
        <l id="s1:l5" met="2" type="+-">Wacht een <rhyme label="c" type="f">beetje</rhyme>.</l>
        <l id="s1:l6" met="2" type="+-">Wakker <rhyme label="c" type="f" corresp="#s1:l5">Peetje</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Kom nou <rhyme label="d" type="m">aan</rhyme>.</l>
        <l id="s1:l8" met="2" type="+-">Wilje <rhyme label="d" type="m" corresp="#s1:l7">slaan</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">2O moer! ick mach jou wel <rhyme label="a" type="f">over</rhyme>.</l>
        <l id="s2:l2" met="2" type="+-">Lustigh, <rhyme label="b" type="m">knap</rhyme>:</l>
        <l id="s2:l3" met="2" type="+-">Zet je <rhyme label="b" type="m" corresp="#s2:l2">schrap</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Al je krachten zijn te <rhyme label="a" type="f" corresp="#s2:l1">pover</rhyme>.</l>
        <l id="s2:l5" met="2" type="+-">Kijk hoe het s’ <rhyme label="c" type="f">er</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">En hoe zets’ <rhyme label="c" type="f" corresp="#s2:l5">er</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Om van <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Baas te <rhyme label="d" type="m" corresp="#s2:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Neen! je moet, hoe wel ’t je <rhyme label="a" type="f">wonder</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Dunckt te <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Onder <rhyme label="b" type="m" corresp="#s3:l2">mijn</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">’t Gaat hoe ’t gaet, je moet ‘er <rhyme label="a" type="f" corresp="#s3:l1">onder</rhyme>.</l>
        <l id="s3:l5" met="2" type="+-">Al je <rhyme label="c" type="f">praten</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Zel niet <rhyme label="c" type="f" corresp="#s3:l5">baten</rhyme>.</l>
        <l id="s3:l7" met="2" type="+-">Houje <rhyme label="d" type="m">stil</rhyme>.</l>
        <l id="s3:l8" met="2" type="+-">Doe mijn <rhyme label="d" type="m" corresp="#s3:l7">wil</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Nouje mijn het hart dus <rhyme label="a" type="f">gaande</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Hebt <rhyme label="b" type="m">gemaakt</rhyme></l>
        <l id="s4:l3" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s4:l2">geschaakt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Of je blijf niet langer <rhyme label="a" type="f" corresp="#s4:l1">staande</rhyme>.</l>
        <l id="s4:l5" met="2" type="+-">Kom mijn <rhyme label="c" type="f">soetje</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Nou ik <rhyme label="c" type="f" corresp="#s4:l5">moetje</rhyme></l>
        <l id="s4:l7" met="2" type="+-">’t Eensjes <rhyme label="d" type="m">doen</rhyme></l>
        <l id="s4:l8" met="2" type="+-">Met een <rhyme label="d" type="m" corresp="#s4:l7">zoen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wel de droes mach jou niet <rhyme label="a" type="f">willen</rhyme>!</l>
        <l id="s5:l2" met="2" type="+-">Krab je <rhyme label="b" type="m">my</rhyme>!</l>
        <l id="s5:l3" met="2" type="+-">Of je <rhyme label="b" type="m" corresp="#s5:l2">pry</rhyme>!</l>
        <l id="s5:l4" met="4" type="+-">‘k Smijtje strak wat voor je <rhyme label="a" type="f" corresp="#s5:l1">billen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Hoe zel ’t <rhyme label="c" type="f">wezen</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Zeg <rhyme label="c" type="f" corresp="#s5:l5">geprezen</rhyme>?</l>
        <l id="s5:l7" met="2" type="+-">Strecktje <rhyme label="d" type="m">lust</rhyme></l>
        <l id="s5:l8" met="2" type="+-">Tot geen <rhyme label="d" type="m" corresp="#s5:l7">rust</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Kom ik moet jou anders <rhyme label="a" type="f">dwingen</rhyme>.</l>
        <l id="s6:l2" met="2" type="+-">’t Is al <rhyme label="b" type="m">wel</rhyme>!</l>
        <l id="s6:l3" met="2" type="+-">Maar ik <rhyme label="b" type="m" corresp="#s6:l2">zel</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Iou wel kleynder leeren <rhyme label="a" type="f" corresp="#s6:l1">zingen</rhyme>.</l>
        <l id="s6:l5" met="2" type="+-">Kom jou <rhyme label="c" type="f">platje</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Waar ’s jou <rhyme label="c" type="f" corresp="#s6:l5">gatje</rhyme>?</l>
        <l id="s6:l7" met="2" type="+-">Dat ik <rhyme label="d" type="m">jou</rhyme></l>
        <l id="s6:l8" met="2" type="+-">Eensjes <rhyme label="d" type="m" corresp="#s6:l7">douw</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">O de drommel! is dat <rhyme label="a" type="f">knypen</rhyme></l>
        <l id="s7:l2" met="2" type="+-">In mijn <rhyme label="b" type="m">huyt</rhyme>?</l>
        <l id="s7:l3" met="2" type="+-">‘k Scheyer <rhyme label="b" type="m" corresp="#s7:l2">uyt</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">‘k Heb de bruy van sullik <rhyme label="a" type="f" corresp="#s7:l1">grypen</rhyme>.</l>
        <l id="s7:l5" met="2" type="+-">Dies staat <rhyme label="c" type="f">vanme</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Raakje <rhyme label="c" type="f" corresp="#s7:l5">anme</rhyme>,</l>
        <l id="s7:l7" met="2" type="+-">‘k Geef j’ een <rhyme label="d" type="m">trek</rhyme></l>
        <l id="s7:l8" met="2" type="+-">In je <rhyme label="d" type="m" corresp="#s7:l7">bek</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Iy bint maar een olik <rhyme label="a" type="f">fleersje</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Die al ’t <rhyme label="b" type="m">leet</rhyme></l>
        <l id="s8:l3" met="2" type="+-">Datje <rhyme label="b" type="m" corresp="#s8:l2">weet</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Vyt te broen met neus en <rhyme label="a" type="f" corresp="#s8:l1">neersje</rhyme></l>
        <l id="s8:l5" met="2" type="+-">My komt <rhyme label="c" type="f">schaffen</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">Maar je <rhyme label="c" type="f" corresp="#s8:l5">blaffen</rhyme></l>
        <l id="s8:l7" met="2" type="+-">Kan <rhyme label="d" type="m">voortaan</rhyme></l>
        <l id="s8:l8" met="2" type="+-">My niet <rhyme label="d" type="m" corresp="#s8:l7">schaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
