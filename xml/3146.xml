    <tags>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
    </tags>
    <melody>Die mint die lijdt veel pijn</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh sprakeloos <rhyme label="a" type="m">Geboomt</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Tgeen van 't Wintje <rhyme label="b" type="m">zwoel</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Wert gedreven gins en <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Waer onder ruysent <rhyme label="a" type="m" corresp="#s1:l1">stroomt</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Eene Beeckje <rhyme label="b" type="m" corresp="#s1:l2">koel</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Van de hooge Clippen <rhyme label="c" type="m" corresp="#s1:l3">neer</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Hoort mijn <rhyme label="d" type="m">geklagh</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Hoe ick treur nacht en <rhyme label="d" type="m" corresp="#s1:l7">dagh</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Om een schoon' <rhyme label="e" type="m">Herderin</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Die ick als mijn Ziel <rhyme label="e" type="m" corresp="#s1:l9">bemin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haer Oogjens als <rhyme label="a" type="m">Cristal</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Die de gulde <rhyme label="b" type="m">glans</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van de Son te boven <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Haer voor-hooft met <rhyme label="a" type="m" corresp="#s2:l1">gebral</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Ciert een blonde <rhyme label="b" type="m" corresp="#s2:l2">Krans</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Waer veel duysent krullen <rhyme label="c" type="m" corresp="#s2:l3">staen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Haer roode <rhyme label="d" type="m">mondt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Haer witte Borsjes <rhyme label="d" type="m" corresp="#s2:l7">rondt</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Haer vriendelijck <rhyme label="e" type="m">gelaet</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Steets in mijn gedachten <rhyme label="e" type="m" corresp="#s2:l9">staet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ach! Cypria, <rhyme label="a" type="m">Goddin</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Laet het blinde <rhyme label="b" type="m">Wicht</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Eens door-schieten mijn Liefs <rhyme label="c" type="m">hert</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Met stralen van de <rhyme label="a" type="m" corresp="#s3:l1">Min</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Dat dien gulde <rhyme label="b" type="m" corresp="#s3:l2">schicht</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Sal verlichten doen mijn <rhyme label="c" type="m" corresp="#s3:l3">smert</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Soo sal ick <rhyme label="d" type="m">gaen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Met Myrrth' en, <rhyme label="d" type="m" corresp="#s3:l7">wieroocx-blaen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">U Offerhande <rhyme label="e" type="m">doen</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Met veel Lauwer-tackjens <rhyme label="e" type="m" corresp="#s3:l9">groen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ach! stroompjes, Boompjes, <rhyme label="a" type="m">Windt</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Vee, en Vogels, <rhyme label="b" type="m">vraeght</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Aen Lerinde, mijn schoon! <rhyme label="c" type="m">Son</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Of sy my weer <rhyme label="a" type="m" corresp="#s4:l1">bemindt</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Want sy my <rhyme label="b" type="m" corresp="#s4:l2">behaeght</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Meer als eenigh <rhyme label="c" type="m" corresp="#s4:l3">Paragon</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Bootst Echo <rhyme label="d" type="m">na</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Aen mijn Lerinde <rhyme label="d" type="m" corresp="#s4:l7">dra</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En maeckt door 't Bos <rhyme label="e" type="m">gedruys</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Brengt mijn Lief dees klachtjes t' <rhyme label="e" type="m" corresp="#s4:l9">huys</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">My dunckt dat Phoebus <rhyme label="a" type="m">daelt</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Kom mijn Schaepjens <rhyme label="b" type="m">treet</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Langhs des groene <rhyme label="c" type="m">Klaver-laen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De Son niet meer en <rhyme label="a" type="m" corresp="#s5:l1">straelt</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Komt, vermits ick <rhyme label="b" type="m" corresp="#s5:l2">weet</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Waer veel nieuwe Clavers <rhyme label="c" type="m" corresp="#s5:l3">staen</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Mijn Ram en <rhyme label="d" type="m">Oy</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Komt weydet na u <rhyme label="d" type="m" corresp="#s5:l7">Koy</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Tot dat de Son weer <rhyme label="e" type="m">rijst</rhyme></l>
        <l id="s5:l10" met="4" type="+-">En sijn soetigheyt <rhyme label="e" type="m" corresp="#s5:l9">bewijst</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
