  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MARIA deughdigh // MARIA <rhyme label="a" type="f">vreughdigh</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Gaet uyt den <rhyme label="b" type="m">stal</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Met haeren Soone // Haer <rhyme label="c" type="f">kroone</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">En haeren <rhyme label="b" type="m" corresp="#s1:l2">al</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Na Godes Tempel sy <rhyme label="d" type="m">vertreckt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Al is sy reyn en <rhyme label="d" type="m" corresp="#s1:l5">onbevleckt</rhyme>.</l>
        <l id="s1:l7" met="6" type="+-">Wilt op dese feest // IESUM in den <rhyme label="e" type="m">gheest</rhyme></l>
        <l id="s1:l8" met="2" type="+-"><rhyme label="f" type="f">Ghebenedijden</rhyme>;</l>
        <l id="s1:l9" met="6" type="+-">Met MARIA soet // Die haer kerck-gangh <rhyme label="g" type="m">doet</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Laet ons <rhyme label="f" type="f" corresp="#s1:l8">verblijden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">SimÃ«on rechtveirdigh // Dat Kindt <rhyme label="a" type="f">eerweirdigh</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Dat soete <rhyme label="b" type="m">Lam</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sijn Godt, sijn Heere // Met <rhyme label="c" type="f">eere</rhyme></l>
        <l id="s2:l4" met="2" type="-+">In handen <rhyme label="b" type="m" corresp="#s2:l2">nam</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Hy was verheught tot in de <rhyme label="d" type="m">ziel</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Als hy Godt in zijn aerems <rhyme label="d" type="m" corresp="#s2:l5">hiel</rhyme>.</l>
        <l id="s2:l7" met="6" type="+-">Wilt op dese feest // IESUM in den <rhyme label="e" type="m">gheest</rhyme></l>
        <l id="s2:l8" met="2" type="+-"><rhyme label="f" type="f">Ghebenedijden</rhyme>;</l>
        <l id="s2:l9" met="6" type="+-">Met MARIA soet // Die haer kerck-gangh <rhyme label="g" type="m">doet</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Laet ons <rhyme label="f" type="f" corresp="#s2:l8">verblijden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Naer sijn betrauwen // Heeft hy <rhyme label="a" type="f">aenschauwen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Godts <rhyme label="b" type="m">Saligheyt</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Want van Godts wesen // Voor <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Hem was <rhyme label="b" type="m" corresp="#s3:l2">voorseyt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat hem de doodt niet sou <rhyme label="d" type="m">gheschien</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Eer dat hy Christum soude <rhyme label="d" type="m" corresp="#s3:l5">sien</rhyme>.</l>
        <l id="s3:l7" met="6" type="+-">Wilt op dese feest, IESUM in den <rhyme label="e" type="m">gheest</rhyme></l>
        <l id="s3:l8" met="2" type="+-"><rhyme label="f" type="f">Ghebenedijden</rhyme>;</l>
        <l id="s3:l9" met="6" type="+-">Met MARIA soet // Die haer kerck-gangh <rhyme label="g" type="m">doet</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Laet ons <rhyme label="f" type="f" corresp="#s3:l8">verblijden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy heeft als heden // Omhelst, <rhyme label="a" type="f">aenbeden</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Dat <rhyme label="b" type="m">Hemels-Licht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Het welck'  zijn stralen // Liet <rhyme label="c" type="f">dalen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">In sijn <rhyme label="b" type="m" corresp="#s4:l2">ghesicht</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Het welcke vierighlijck <rhyme label="d" type="m">doorstraelt</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Alwaer noyt Sonn', noch Maen' en <rhyme label="d" type="m" corresp="#s4:l5">daelt</rhyme>.</l>
        <l id="s4:l7" met="6" type="+-">Wilt op dese feest // IESUM in den <rhyme label="e" type="m">gheest</rhyme></l>
        <l id="s4:l8" met="2" type="+-"><rhyme label="f" type="f">Ghebenedijden</rhyme>;</l>
        <l id="s4:l9" met="6" type="+-">Met MARIA soet // Die haer kerck-gangh <rhyme label="g" type="m">doet</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Laet ons <rhyme label="f" type="f" corresp="#s4:l8">verblijden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Van alle menschen // Die vierigh <rhyme label="a" type="f">wenschen</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Naer uw' <rhyme label="b" type="m">aenschijn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">In d'eeuwigheden // <rhyme label="c" type="f">beleden</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Lof moet u <rhyme label="b" type="m" corresp="#s5:l2">zijn</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Lof zy de Moeder altijdt <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Die u van daegh' ten Offer <rhyme label="d" type="m" corresp="#s5:l5">draeght</rhyme>.</l>
        <l id="s5:l7" met="6" type="+-">Wilt op dese feest // IESUM in den <rhyme label="e" type="m">gheest</rhyme></l>
        <l id="s5:l8" met="2" type="+-"><rhyme label="f" type="f">Ghebenedijden</rhyme>;</l>
        <l id="s5:l9" met="6" type="+-">Met MARIA soet // Die haer kerck-gangh <rhyme label="g" type="m">doet</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">Laet ons <rhyme label="f" type="f" corresp="#s5:l8">verblijden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
