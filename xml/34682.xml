      <tag>praise</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Dulces cunæ</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HErders met onnoosel <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Herders die Godt weet te <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Siet, ick komme hier <rhyme label="b" type="f">vertolcken</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Groote vreught voor alle <rhyme label="b" type="f" corresp="#s1:l3">volcken</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Want u is, ten langhen <rhyme label="c" type="f">lesten</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">By de Bethleëmsche <rhyme label="c" type="f" corresp="#s1:l5">vesten</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Versch gheboren den <rhyme label="d" type="f">Messias</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Langh' voorseyt door <rhyme label="d" type="f" corresp="#s1:l7">Isaias</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ende flus, na dese <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ons verschenen in <rhyme label="a" type="f" corresp="#s2:l1">slagh-oorden</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Godes Heyligh' <rhyme label="b" type="f">Enghel-schaeren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die ons brochten goede <rhyme label="b" type="f" corresp="#s2:l3">maeren</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En die blijdelijck <rhyme label="c" type="f">uyt-songhen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Met hun-lieder soetste <rhyme label="c" type="f" corresp="#s2:l5">tonghen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Lof zy Godt in d'hooghste <rhyme label="d" type="f">stede</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">En oock aen de menschen <rhyme label="d" type="f" corresp="#s2:l7">vrede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Jesse-blomme versch <rhyme label="a" type="f">ghesproten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wt een roede noyt <rhyme label="a" type="f" corresp="#s3:l1">ontsloten</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Wilt ons in ghenaed' <rhyme label="b" type="f">ontfanghen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Op dat wy de natte <rhyme label="b" type="f" corresp="#s3:l3">wanghen</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Van den traenen-vloet <rhyme label="c" type="f">af-vaghen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En u teere liefde <rhyme label="c" type="f" corresp="#s3:l5">draghen</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Op dat wy u dienst <rhyme label="d" type="f">bewijsen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Eeuwigh loven, ende <rhyme label="d" type="f" corresp="#s3:l7">prijsen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laet ons altijdt u <rhyme label="a" type="f">verheffen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Die de saligheyt komt <rhyme label="a" type="f" corresp="#s4:l1">treffen</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Laet u vrindtschap ons <rhyme label="b" type="f">ghenieten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die voor ons u bloedt komt <rhyme label="b" type="f" corresp="#s4:l3">ghieten</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Laet ons' shemels vreught <rhyme label="c" type="f">be-erven</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die voor ons de doodt komt <rhyme label="c" type="f" corresp="#s4:l5">sterven</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">Laet ons hert met u <rhyme label="d" type="f">vereenen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die voor ons u wilt <rhyme label="d" type="f" corresp="#s4:l7">verkleenen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
