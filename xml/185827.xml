<song>
  <meta>
    <id>185827</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nu zijt wellekome</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">Nv zijt wellecome Heere <rhyme label="a" type="m">Christ</rhyme></l>
        <l id="s1:l2" met="5" type="+-">Want ghy onser alder Heere <rhyme label="a" type="m" corresp="#s1:l1">bist</rhyme></l>
        <l id="s1:l3" met="5" type="+-">Nu zijt wellecome lieve <rhyme label="b" type="f">Heere</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Hier in aertrijcke ghy zijt so <rhyme label="c" type="m">schone</rhyme></l>
        <l id="s1:l5" met="2" type="-+"><rhyme label="d" type="m">Kyrieleys</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Kyrieleyson Christeleyson willen wy <rhyme label="a" type="m">singen</rhyme></l>
        <l id="s2:l2" met="5" type="+-">Daer me die schone Leyssen <rhyme label="a" type="m" corresp="#s2:l1">beginnen</rhyme></l>
        <l id="s2:l3" met="5" type="+-">Iesus is geboren op die heylige <rhyme label="b" type="f">Korsnacht</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Van eene Maget reyne sy blevet en sy <rhyme label="c" type="m">wast</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+"><rhyme label="d" type="m">Kyrieleys</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Die Herders op die velde songen een nieu <rhyme label="a" type="m">Liet</rhyme></l>
        <l id="s3:l2" met="5" type="+-">Dat Iesus was geboren wy en wistens <rhyme label="a" type="m" corresp="#s3:l1">niet</rhyme></l>
        <l id="s3:l3" met="5" type="+-">Gaet aen geender strate men salt u seggen <rhyme label="b" type="f">claer</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Tot Bethleem inder stede daer ist geschiet <rhyme label="c" type="m">voorwaer</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+"><rhyme label="d" type="m">Kyrieleys</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Die heylige drie Koningen wt verre <rhyme label="a" type="m">landen</rhyme></l>
        <l id="s4:l2" met="5" type="+-">Sochten onsen Heere met <rhyme label="a" type="m" corresp="#s4:l1">Offerhanden</rhyme></l>
        <l id="s4:l3" met="5" type="+-">Die eene offerde Myrthe die ander <rhyme label="b" type="f">wieroock</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Die derde louter gout, van dat alderpuerste <rhyme label="c" type="m">oock</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+"><rhyme label="d" type="m">Kyrieleys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
