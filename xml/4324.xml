<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4324</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Si vous ne voulez me guérir</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALs ik aan 't Zuierbeekje <rhyme label="a" type="m">kom</rhyme>;</l>
        <l id="s1:l2" met="4" type="-+">Alwaar Amintas my in <rhyme label="b" type="f">d'elzen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Vaak, met kriel <rhyme label="b" type="f" corresp="#s1:l2">omhelzen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Heten <rhyme label="a" type="m" corresp="#s1:l1">wellekom</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Schijnt lommer-groen en <rhyme label="c" type="m">roze-ruigt</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Het afzijn van mijn zoetert buigend' <rhyme label="c" type="m" corresp="#s1:l5">tuigt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ia bleikt haar purper <rhyme label="d" type="m">blos</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Als ik een zugje <rhyme label="d" type="m" corresp="#s1:l7">los</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ziel-rover die mijn Zieltje <rhyme label="a" type="m">schaakt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ai! voer mijn Zielloos rif je <rhyme label="b" type="f">mede</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">'t Geen, met grage <rhyme label="b" type="f" corresp="#s2:l2">schrede'</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Naar uw' bij-zijn <rhyme label="a" type="m" corresp="#s2:l1">haakt</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Mijn Gees je treurt, mijn <rhyme label="c" type="m">harzen-togt</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Slaat nare galmpjes in de <rhyme label="c" type="m" corresp="#s2:l5">logt</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Ik quijn en teer en <rhyme label="d" type="m">sterf</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Mits ick uw lipjes <rhyme label="d" type="m" corresp="#s2:l7">derf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">VVaar heen mijn zinnen buiten <rhyme label="a" type="m">reen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Op hol? zijn lunderagtig <rhyme label="b" type="f">toeven</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Is maar om te <rhyme label="b" type="f" corresp="#s3:l2">proeven</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Uw' <rhyme label="a" type="m" corresp="#s3:l1">getrouwighêen</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Kom, Zinnedief je! kom, ei! <rhyme label="c" type="m">kom</rhyme>;</l>
        <l id="s3:l6" met="5" type="-+">Besproei uw kuische <rhyme label="c" type="m" corresp="#s3:l5">Maagdeblom</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met daauw van uwe <rhyme label="d" type="m">mont</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En maakt mijn hart <rhyme label="d" type="m" corresp="#s3:l7">gezont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dan zal mijn Liefd' met Liefd' <rhyme label="a" type="m">gevoedt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Door Liefde in Liefde groeje' en <rhyme label="b" type="f">bloejen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En uw' Liefd' <rhyme label="b" type="f" corresp="#s4:l2">besproejen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Met weerliefdens <rhyme label="a" type="m" corresp="#s4:l1">zoet</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Als 't naakte Dwingelandtjen, <rhyme label="c" type="m">ons</rhyme></l>
        <l id="s4:l6" met="5" type="-+">Geleit na 't welig <rhyme label="c" type="m" corresp="#s4:l5">Swanen-dons</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Alwaar gewenste <rhyme label="d" type="m">weeld'</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Gewenste wellust <rhyme label="d" type="m" corresp="#s4:l7">teelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
