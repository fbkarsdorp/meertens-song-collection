    <melody>Laura zoet minnebeeld</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VVAer sijt ghy <rhyme label="a" type="m">Coridon</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Sal ick u <rhyme label="b" type="f">vinde</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Oft schuylt ghy voor de <rhyme label="a" type="m" corresp="#s1:l1">son</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Onder de <rhyme label="b" type="f" corresp="#s1:l2">Linde</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Zijt ghy voor u <rhyme label="c" type="m">plesier</rhyme></l>
        <l id="s1:l6" met="2" type="-+">V vee gaen <rhyme label="d" type="f">weyden</rhyme>?</l>
        <l id="s1:l7" met="3" type="-+">Had ick mijn Herder <rhyme label="c" type="m" corresp="#s1:l5">hier</rhyme></l>
        <l id="s1:l8" met="2" type="-+">k'Sou hem me <rhyme label="d" type="f" corresp="#s1:l6">leyden</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zijt ghy ten Boswaert <rhyme label="a" type="m">in</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Om coelt' te <rhyme label="b" type="f">raepen</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">En laet u <rhyme label="a" type="m" corresp="#s2:l1">Herderin</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Hier met u <rhyme label="b" type="f" corresp="#s2:l2">schaepen</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Die nu roept met <rhyme label="c" type="m">ghetier</rhyme></l>
        <l id="s2:l6" met="2" type="-+">VVaer mach hy <rhyme label="d" type="f">blijven</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">Had ick mijn Herder <rhyme label="c" type="m" corresp="#s2:l5">hier</rhyme></l>
        <l id="s2:l8" met="2" type="-+">k'Sou vreught <rhyme label="d" type="f" corresp="#s2:l6">bedrijven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zijt ghy by een <rhyme label="a" type="m">fonteyn</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Om dorst te <rhyme label="b" type="f">slissen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Oft op een ander <rhyme label="a" type="m" corresp="#s3:l1">pleyn</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Om my te <rhyme label="b" type="f" corresp="#s3:l2">missen</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">Heeft u een vreeslijck <rhyme label="c" type="m">dier</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Vyt t'velt <rhyme label="d" type="f">ghedreven</rhyme>?</l>
        <l id="s3:l7" met="3" type="-+">Had ick mijn Herder <rhyme label="c" type="m" corresp="#s3:l5">hier</rhyme></l>
        <l id="s3:l8" met="2" type="-+">k'Sou vreedich <rhyme label="d" type="f" corresp="#s3:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nochtans ghy sijt <rhyme label="a" type="m">bekent</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Voor een cloeck <rhyme label="b" type="f">Herder</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die felle dienen <rhyme label="a" type="m" corresp="#s4:l1">ment</rhyme></l>
        <l id="s4:l4" met="2" type="-+">En jaeght veel <rhyme label="b" type="f" corresp="#s4:l2">verder</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die voor Beer, wilden <rhyme label="c" type="m">Stier</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Nimmer sal <rhyme label="d" type="f">vresen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Had ick mijn Herder <rhyme label="c" type="m" corresp="#s4:l5">hier</rhyme></l>
        <l id="s4:l8" met="2" type="-+">k'Sou vrolijck <rhyme label="d" type="f" corresp="#s4:l6">wesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu roep ick aen de <rhyme label="a" type="m">Goôn</rhyme></l>
        <l id="s5:l2" met="2" type="-+">VVilt help my <rhyme label="b" type="f">thoonen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Comt grooten <rhyme label="a" type="m" corresp="#s5:l1">Venussoôn</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Ick sal t'u <rhyme label="b" type="f" corresp="#s5:l2">loonen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Gaet dan mijns minne <rhyme label="c" type="m">vier</rhyme></l>
        <l id="s5:l6" met="2" type="-+">De bootschap <rhyme label="d" type="f">draeghen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Had ick mijn Herder <rhyme label="c" type="m" corresp="#s5:l5">hier</rhyme></l>
        <l id="s5:l8" met="2" type="-+">k'Sou niet meer <rhyme label="d" type="f" corresp="#s5:l6">claeghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sa loopt dan naer het <rhyme label="a" type="m">dal</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Dat ghy siet <rhyme label="b" type="f">legghen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ghy kent mijns Herders <rhyme label="a" type="m" corresp="#s6:l1">stal</rhyme></l>
        <l id="s6:l4" met="2" type="-+">VVilt hem doch <rhyme label="b" type="f" corresp="#s6:l2">seggen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat ick in dit <rhyme label="c" type="m">quartier</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Noch wat sal <rhyme label="d" type="f">toeven</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">k'Sou (had ick hem maer <rhyme label="c" type="m" corresp="#s6:l5">hier</rhyme>)</l>
        <l id="s6:l8" met="2" type="-+">Gheen bood' <rhyme label="d" type="f" corresp="#s6:l6">behoeven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
