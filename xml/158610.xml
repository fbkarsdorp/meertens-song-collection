<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>158610</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="seasonal song">seizoenslied</tag>
    </tags>
    <melody>Wilhelmus</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe lieff'lijk staen de <rhyme label="a" type="f">Boomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe rijkelijk <rhyme label="b" type="m">gelaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Met Bladt en <rhyme label="a" type="f" corresp="#s1:l1">Boeisel-soomen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die al veel moyer <rhyme label="b" type="m" corresp="#s1:l2">staen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Als al die malle <rhyme label="c" type="f">Kleeren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Daer menich een sijn <rhyme label="d" type="m">lijf</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Meent fraytjes door te <rhyme label="c" type="f" corresp="#s1:l5">eeren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En 't is te slecht <rhyme label="d" type="m" corresp="#s1:l6">bedrijf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Trek aen den besten <rhyme label="a" type="f">Bouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Mey' Meysjes, met <rhyme label="b" type="m">verlof</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Neem Voor- en <rhyme label="a" type="f" corresp="#s2:l1">Achter-mouwen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En al den bras van 't <rhyme label="b" type="m" corresp="#s2:l2">Hof</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Vertreed u dan na <rhyme label="c" type="f">buiten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Kijk snedich in de <rhyme label="d" type="m">wint</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">'t Is als een Padd' op <rhyme label="c" type="f" corresp="#s2:l5">Kluiten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">By 't geen m' in 't Hofje <rhyme label="d" type="m" corresp="#s2:l6">vint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zoo veelerley <rhyme label="a" type="f">coleuren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Als 'g aen een Tulpje <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Doen immers uw' <rhyme label="a" type="f" corresp="#s3:l1">Faveuren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En Lintjens heel te <rhyme label="b" type="m" corresp="#s3:l2">niet</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">De Witt' en Roode <rhyme label="c" type="f">Roosen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Vol aengename <rhyme label="d" type="m">geur</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Doen al de <rhyme label="c" type="f" corresp="#s3:l5">Poeyer-doosen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Verwerpen als een <rhyme label="d" type="m" corresp="#s3:l6">leur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Noch hooger <rhyme label="a" type="f">Vrouwe-dieren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Geen soeter <rhyme label="b" type="m">Paerlen-kant</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Als aen mijn <rhyme label="a" type="f" corresp="#s4:l1">Anjelieren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Omringt uw hals of <rhyme label="b" type="m" corresp="#s4:l2">hant</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Waer vint gy rooder <rhyme label="c" type="f">Kralen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Als aen den <rhyme label="d" type="m">Peperboom</rhyme>?</l>
        <l id="s4:l7" met="3" type="-+">Laet vry uw roem-seil <rhyme label="c" type="f" corresp="#s4:l5">dalen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">'t Gaet hier al tegen <rhyme label="d" type="m" corresp="#s4:l6">stroom</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Al blijft gy op de <rhyme label="a" type="f">Wegen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met reuk en met <rhyme label="b" type="m">gesicht</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Noch ben ik niet <rhyme label="a" type="f" corresp="#s5:l1">verlegen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En gy valt weer te te <rhyme label="b" type="m" corresp="#s5:l2">licht</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">De groene <rhyme label="c" type="f">Maye-grasen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En 't Gout-geel <rhyme label="d" type="m">Paerde-blat</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Al 't geel en groen <rhyme label="c" type="f" corresp="#s5:l5">verbasen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat g' oit hebt aen <rhyme label="d" type="m" corresp="#s5:l6">gehat</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De Heerlijkheit des <rhyme label="a" type="f">Wijsen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En Rijken <rhyme label="b" type="m">Salomons</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Kon self zoo hoogh niet <rhyme label="a" type="f" corresp="#s6:l1">rijsen</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Zoo leert de Leeraer <rhyme label="b" type="m" corresp="#s6:l2">ons</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Die m' op zijn bloote <rhyme label="c" type="f">seggen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Gelooven mach, en <rhyme label="d" type="m">moet</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">'t Was ook licht uit te <rhyme label="c" type="f" corresp="#s6:l5">leggen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Maer al te veel, <rhyme label="d" type="m" corresp="#s6:l6">onzoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
