  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HEere zijt ons hender // <rhyme label="a" type="m">by</rhyme></l>
        <l id="s1:l2" met="3" type="+-">En die u geven // <rhyme label="b" type="m">danck</rhyme></l>
        <l id="s1:l3" met="4" type="+-">dat wy van die schender // <rhyme label="a" type="m" corresp="#s1:l1">vry</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Doch sijn ons leven // <rhyme label="b" type="m" corresp="#s1:l2">lanck</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Dat hy niet // wat hy <rhyme label="c" type="m">ziet</rhyme></l>
        <l id="s1:l6" met="4" type="+-">en bespiet // int <rhyme label="c" type="m" corresp="#s1:l5">verdriet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Of houdt in zynen <rhyme label="b" type="m" corresp="#s1:l2">dwanck</rhyme>.</l>
        <l id="s1:l8" met="4" type="+-">Dat wy Vader // vol <rhyme label="d" type="f">genaden</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Altydt nader // in u <rhyme label="d" type="f" corresp="#s1:l8">paden</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Sijn bevryt // van den <rhyme label="e" type="m">nyt</rhyme></l>
        <l id="s1:l11" met="4" type="+-">En den stryt // worden <rhyme label="e" type="m" corresp="#s1:l10">quyt</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Dat naer mach sijn <rhyme label="e" type="m" corresp="#s1:l10">jolyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Den mensch van God geschapen // <rhyme label="a" type="m">is</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Heylich goet groot // <rhyme label="b" type="m">geacht</rhyme></l>
        <l id="s2:l3" met="4" type="+-">door sDuyvels betrapen // <rhyme label="a" type="m" corresp="#s2:l1">wis</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Die heeft de doodt // <rhyme label="b" type="m" corresp="#s2:l2">bewracht</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Doen hy plat // <rhyme label="c" type="m">overtrat</rhyme></l>
        <l id="s2:l6" met="4" type="+-">En oock wat // daer van <rhyme label="c" type="m" corresp="#s2:l5">at</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En hielt zy geen goe <rhyme label="b" type="m" corresp="#s2:l2">wacht</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Want een bevel // Gods der <rhyme label="d" type="f">goden</rhyme></l>
        <l id="s2:l9" met="4" type="-+">Zy wistet wel // twas <rhyme label="d" type="f" corresp="#s2:l8">verboden</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Dat zy wan // alleen <rhyme label="e" type="m">van</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Den Satan // was den <rhyme label="e" type="m" corresp="#s2:l10">ban</rhyme></l>
        <l id="s2:l12" met="3" type="-+">De doot die quam haer <rhyme label="e" type="m" corresp="#s2:l10">an</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Alzoo in doots gewelde // <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s3:l2" met="3" type="+-">De Menschen alle // <rhyme label="b" type="m">gaer</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Den Helt alder Helden // <rhyme label="a" type="m" corresp="#s3:l1">nam</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Ons uy den valle // <rhyme label="b" type="m" corresp="#s3:l2">zwaer</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Dien Heylant // heeft <rhyme label="c" type="m">vermant</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Den Vyandt // met der <rhyme label="c" type="m" corresp="#s3:l5">hant</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Deylt uyt zijn rooff <rhyme label="b" type="m" corresp="#s3:l2">aldaer</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Den Leeu Juda // heeft <rhyme label="d" type="f">verwonnen</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Dat goed' noch qua // doen en <rhyme label="d" type="f" corresp="#s3:l8">connen</rhyme></l>
        <l id="s3:l10" met="4" type="+-">In ootmoet // quam hy <rhyme label="e" type="m">soet</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Wat hy doet // tis al <rhyme label="e" type="m" corresp="#s3:l10">goet</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Verlost ons met zijn <rhyme label="e" type="m" corresp="#s3:l10">bloet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dits den rechten Herder // <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Die sijne Schapen // <rhyme label="b" type="m">voedt</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Daer en kan geen werder // <rhyme label="a" type="m" corresp="#s4:l1">sijn</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Die ons vant slapen // <rhyme label="b" type="m" corresp="#s4:l2">hoedt</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Hy kan leen // dat niet <rhyme label="c" type="m">een</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Groot noch kleen // en moet <rhyme label="c" type="m" corresp="#s4:l5">scheen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Tot sWolfs gapen // <rhyme label="b" type="m" corresp="#s4:l2">verwoet</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Dan alleen dat // kint <rhyme label="d" type="f">verloren</rhyme></l>
        <l id="s4:l9" met="4" type="-+">Daer David wat // seyt te <rhyme label="d" type="f" corresp="#s4:l8">voren</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Twas Judaes // dat boos <rhyme label="e" type="m">aes</rhyme></l>
        <l id="s4:l11" met="4" type="+-">Dat eylaes // als een <rhyme label="e" type="m" corresp="#s4:l10">dwaes</rhyme>:</l>
        <l id="s4:l12" met="3" type="-+">Verriet sijn eygen <rhyme label="e" type="m" corresp="#s4:l10">baes</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Prins alder Princieren // <rhyme label="a" type="m">stam</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Van hoogher weerden // <rhyme label="b" type="m">fier</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Als een goedertieren // <rhyme label="a" type="m" corresp="#s5:l1">Lam</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Quaemt ghy op Eerden // <rhyme label="b" type="m" corresp="#s5:l2">hier</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Hebt gehaelt // dat <rhyme label="c" type="m">gefaelt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Was verdwaelt // en <rhyme label="c" type="m" corresp="#s5:l5">betaelt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Met uwen bloede <rhyme label="b" type="m" corresp="#s5:l2">dier</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Den ingang tot // d'eeuwigh <rhyme label="d" type="f">ruste</rhyme></l>
        <l id="s5:l9" met="4" type="-+">Is van Godt // gemaeckt tot <rhyme label="d" type="f" corresp="#s5:l8">luste</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Hy wil schoon // elck <rhyme label="e" type="m">persoon</rhyme></l>
        <l id="s5:l11" met="4" type="+-">Seer ydoon // tot haer <rhyme label="e" type="m" corresp="#s5:l10">loon</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Schincken des levens <rhyme label="e" type="m" corresp="#s5:l10">Croon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
