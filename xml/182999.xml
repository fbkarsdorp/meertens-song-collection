<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182999</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Adieu schone Rosalinde</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom, waarde <rhyme label="a" type="f">Landgenooten</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Die uit Bataafsche <rhyme label="b" type="m">bloed</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Der Helden zyt <rhyme label="a" type="f" corresp="#s1:l1">gesprooten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wien Nassau's liefde <rhyme label="b" type="m" corresp="#s1:l2">voed</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Kom, braave <rhyme label="c" type="f">Prinsgezinden</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">Kom alle, wie gy <rhyme label="d" type="m">zyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Van Wilhelmus waardste <rhyme label="c" type="f" corresp="#s1:l5">Vrinden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Uw zy my Zang <rhyme label="d" type="m" corresp="#s1:l6">gewyd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dien dag komt weer te <rhyme label="a" type="f">vooren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Toen, na veel <rhyme label="b" type="m">smeekgebeên</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Een Prins ons wierd <rhyme label="a" type="f" corresp="#s2:l1">gebooren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wat vond men dubbele <rhyme label="b" type="m" corresp="#s2:l2">reên</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Om 's Heeren Naam te <rhyme label="c" type="f">looven</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Voor zulk een dierbaar <rhyme label="d" type="m">Pand</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">God schonk dit heil van <rhyme label="c" type="f" corresp="#s2:l5">boven</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Tot vreugd van <rhyme label="d" type="m" corresp="#s2:l6">Nederland</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Nu zyn de Prins zyn <rhyme label="a" type="f">jaaren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Reeds driemaal tien en <rhyme label="b" type="m">negen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Den Hemel wil hem <rhyme label="a" type="f" corresp="#s3:l1">spaaren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En kroone met veel <rhyme label="b" type="m" corresp="#s3:l2">zegen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">De Oranje-Stamboom <rhyme label="c" type="f">bloeijen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In Neêrlands Tuin <rhyme label="d" type="m">geplant</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">God wil hem steeds doen <rhyme label="c" type="f" corresp="#s3:l5">groeijen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Tot heil van 't <rhyme label="d" type="m" corresp="#s3:l6">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nu mogen wy ook <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">'Er is nog blyde <rhyme label="b" type="m">stof</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'Er zyn veel <rhyme label="a" type="f" corresp="#s4:l1">zegeningen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Gedaald van 't <rhyme label="b" type="m" corresp="#s4:l2">Hemel-Hof</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Laat vry de laster <rhyme label="c" type="f">vaaren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De vuile Snoodaarts <rhyme label="d" type="m">woên</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">God wil dien Vorst <rhyme label="c" type="f" corresp="#s4:l5">bewaaren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En zal hem voorts <rhyme label="d" type="m" corresp="#s4:l6">behoên</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Oranje-Stam zal <rhyme label="a" type="f">bloeijen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De Snoodaarts eens <rhyme label="b" type="m">vergaan</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Oranje-Telgen <rhyme label="a" type="f" corresp="#s5:l1">groeijen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Zoo lang de Zon en <rhyme label="b" type="m" corresp="#s5:l2">Maan</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dit Aardryk mag <rhyme label="c" type="f">bestraalen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Zoo lang zal Nassau's <rhyme label="d" type="m">Naam</rhyme></l>
        <l id="s5:l7" met="3" type="-+">In Neêrland <rhyme label="c" type="f" corresp="#s5:l5">zegenpraalen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En leeven op de <rhyme label="d" type="m" corresp="#s5:l6">Faam</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Moet 's Gravenhage <rhyme label="a" type="f">treuren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Om 't Vorstelyk <rhyme label="b" type="m">gemis</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Moet dit, helaas! <rhyme label="a" type="f" corresp="#s6:l1">gebeuren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">'t Is God die met hem <rhyme label="b" type="m" corresp="#s6:l2">is</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">'t Is God die hem <rhyme label="c" type="f">geleiden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">'t Is God van <rhyme label="d" type="m">Nederland</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Die voor zyn regt zal <rhyme label="c" type="f" corresp="#s6:l5">stryden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Hem redden door zyn <rhyme label="d" type="m" corresp="#s6:l6">hand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">God heeft in vroeger <rhyme label="a" type="f">tyden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">In Vryheit ons <rhyme label="b" type="m">gestelt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Door Prinsen hy <rhyme label="a" type="f" corresp="#s7:l1">bevryden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Ons van 't Spaansch <rhyme label="b" type="m" corresp="#s7:l2">geweld</rhyme>;</l>
        <l id="s7:l5" met="3" type="-+">God gaf ons Nassau's <rhyme label="c" type="f">Helden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die voor ons <rhyme label="d" type="m">Vaderland</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Hun goed en leven <rhyme label="c" type="f" corresp="#s7:l5">stelden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Gesterkt door 's Heeren <rhyme label="d" type="m" corresp="#s7:l6">hand</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hun Naam zal in <rhyme label="a" type="f">gedagten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met glorie zyn <rhyme label="b" type="m">bekend</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Van 't laatste <rhyme label="a" type="f" corresp="#s8:l1">Nageslagten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Tot aan des Waerelds <rhyme label="b" type="m" corresp="#s8:l2">end</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Haar daaden zullen <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Schoon zy gedaald in 't <rhyme label="d" type="m">stof</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Schoon zy ter sterve <rhyme label="c" type="f" corresp="#s8:l5">zweeven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">En ontfangen eeuwig <rhyme label="d" type="m" corresp="#s8:l6">lof</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Hoe kan de nyd <rhyme label="a" type="f">vergelden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Zoo snoode <rhyme label="b" type="m">ondankbaarheid</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Aan Neêrlands Nassau's <rhyme label="a" type="f" corresp="#s9:l1">Helden</rhyme>!</l>
        <l id="s9:l4" met="3" type="-+">ô Snoodaarts! wie gy <rhyme label="b" type="m" corresp="#s9:l2">zyt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">God zal dat kwaad <rhyme label="c" type="f">bezoeken</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Gy moogt, als <rhyme label="d" type="m">Simeï</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Dien vroomen David <rhyme label="c" type="f" corresp="#s9:l5">vloeken</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Gods wraak kwam ook op <rhyme label="d" type="m" corresp="#s9:l6">die</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Laat u tog <rhyme label="a" type="f">overwinnen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Wil Neêrlands <rhyme label="b" type="m">Liefdepand</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Vorst Willems Huis, <rhyme label="a" type="f" corresp="#s10:l1">beminnnen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dien roem van <rhyme label="b" type="m" corresp="#s10:l2">Nederland</rhyme>!</l>
        <l id="s10:l5" met="3" type="-+">Die Vorst is liefde <rhyme label="c" type="f">waardig</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Waarom tog zó <rhyme label="d" type="m">gehaat</rhyme>?</l>
        <l id="s10:l7" met="3" type="-+">Hy is tot heil steeds <rhyme label="c" type="f" corresp="#s10:l5">vaardig</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Van Kerk en <rhyme label="d" type="m" corresp="#s10:l6">Burgerstaat</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wilt gy in Vryheid <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">God zal door Nassau's <rhyme label="b" type="m">moed</rhyme></l>
        <l id="s11:l3" met="3" type="-+">U de eêlste Vryheid <rhyme label="a" type="f" corresp="#s11:l1">geeven</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Die ons voor dwang <rhyme label="b" type="m" corresp="#s11:l2">behoed</rhyme>;</l>
        <l id="s11:l5" met="3" type="-+">Ach! mogten wy eens <rhyme label="c" type="f">hooren</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Dat dit <rhyme label="d" type="m">Oranje-huis</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Mogt praalen als te <rhyme label="c" type="f" corresp="#s11:l5">vooren</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Bevryd van leed en <rhyme label="d" type="m" corresp="#s11:l6">kruis</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Mogt binnen onze <rhyme label="a" type="f">Steeden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">De Twist en Tweedragt <rhyme label="b" type="m">vliên</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">En de Eendragt en de <rhyme label="a" type="f" corresp="#s12:l1">Vreede</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Eens weer gevestigd <rhyme label="b" type="m" corresp="#s12:l2">zien</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Dan zou dit Land in <rhyme label="c" type="f">zegen</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Weer bloeijen, als <rhyme label="d" type="m">voorheen</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Dan wierd Gods heil <rhyme label="c" type="f" corresp="#s12:l5">verkreegen</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Op onze <rhyme label="d" type="m" corresp="#s12:l6">smeekgebeên</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">ô Hoorder der <rhyme label="a" type="f">Gebeden</rhyme>!</l>
        <l id="s13:l2" met="3" type="-+">Die Neêrlands Redder <rhyme label="b" type="m">zyt</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Schenk Eendragt, Liefde en <rhyme label="a" type="f" corresp="#s13:l1">Vreede</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Gelyk in vroeger <rhyme label="b" type="m" corresp="#s13:l2">tyd</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Doet uit het aaklig <rhyme label="c" type="f">duister</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Een vrolyk licht op <rhyme label="d" type="m">gaan</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Herstel de <rhyme label="c" type="f" corresp="#s13:l5">Oranje-luister</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Door uwe <rhyme label="d" type="m" corresp="#s13:l6">wonderdaên</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Weleer van God <rhyme label="b" type="m">gebeên</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Blyf op den Heer <rhyme label="a" type="f" corresp="#s14:l1">vertrouwen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">In al uw <rhyme label="b" type="m" corresp="#s14:l2">zwarigheên</rhyme>;</l>
        <l id="s14:l5" met="3" type="-+">Hy zal uw nooit <rhyme label="c" type="f">begeeven</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Hy is 't die u <rhyme label="d" type="m">bewaar</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Hy zorgt nog voor uw <rhyme label="c" type="f" corresp="#s14:l5">leeven</rhyme></l>
        <l id="s14:l8" met="3" type="-+">In 't allergrootst <rhyme label="d" type="m" corresp="#s14:l6">gevaar</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Die nog beveiligd <rhyme label="b" type="m">zyt</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Laat nooit uw moed <rhyme label="a" type="f" corresp="#s15:l1">verflaauwen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Wees gy ten alle <rhyme label="b" type="m" corresp="#s15:l2">tyd</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Een David, kloek in 't <rhyme label="c" type="f">stryden</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Een dappere <rhyme label="d" type="m">Gidion</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Een Job in al uw <rhyme label="c" type="f" corresp="#s15:l5">lyden</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Een wyze <rhyme label="d" type="m" corresp="#s15:l6">Salomon</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">By 's Heeren Volk <rhyme label="b" type="m">bemind</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">By die Gods lof <rhyme label="a" type="f" corresp="#s16:l1">ontvouwen</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">De beste <rhyme label="b" type="m" corresp="#s16:l2">Menschen-vrind</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">God wil uw Leidsman <rhyme label="c" type="f">weezen</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Uw Toevlugt in den <rhyme label="d" type="m">nood</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Hy zy, ach! wilt niet <rhyme label="c" type="f" corresp="#s16:l5">vreezen</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Uw Schilt en Loon zeer <rhyme label="d" type="m" corresp="#s16:l6">groot</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Die uw <rhyme label="b" type="m">Geboorte-dag</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Op heden mag <rhyme label="a" type="f" corresp="#s17:l1">aanschouwen</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">En staatlyk vieren <rhyme label="b" type="m" corresp="#s17:l2">mag</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">God wil nog veele <rhyme label="c" type="f">Jaaren</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">ô Vorst! ô <rhyme label="d" type="m">Liefde-pand</rhyme>!</l>
        <l id="s17:l7" met="3" type="-+">Uw dierbaar leeven <rhyme label="c" type="f" corresp="#s17:l5">spaaren</rhyme>,</l>
        <l id="s17:l8" met="3" type="-+">Tot heil van 't <rhyme label="d" type="m" corresp="#s17:l6">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Leef lang, bevryd van <rhyme label="b" type="m">kruis</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Leef zonder druk en <rhyme label="a" type="f" corresp="#s18:l1">rouwe</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">God wil uw Vorstelyk <rhyme label="b" type="m" corresp="#s18:l2">Huis</rhyme></l>
        <l id="s18:l5" met="3" type="-+">Mild zegenen, vroeg en <rhyme label="c" type="f">spaade</rhyme>;</l>
        <l id="s18:l6" met="3" type="-+">Uw dierbre <rhyme label="d" type="m">Gemaalin</rhyme></l>
        <l id="s18:l7" met="3" type="-+">En Telgen <rhyme label="c" type="f" corresp="#s18:l5">overlaaden</rhyme></l>
        <l id="s18:l8" met="3" type="-+">Met heil door 's Hemels <rhyme label="d" type="m" corresp="#s18:l6">min</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Nassouwen</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Blyf op Gods <rhyme label="b" type="m">Trouwverbond</rhyme></l>
        <l id="s19:l3" met="3" type="-+">Uw hoop en heil steeds <rhyme label="a" type="f" corresp="#s19:l1">bouwen</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Naar 't Heilwoord vast <rhyme label="b" type="m" corresp="#s19:l2">gegrond</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Tot gy in 's Hemels <rhyme label="c" type="f">Zaalen</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">By uwen Vader, <rhyme label="d" type="m">God</rhyme>,</l>
        <l id="s19:l7" met="3" type="-+">Zult eeuwig <rhyme label="c" type="f" corresp="#s19:l5">zegenpraalen</rhyme>,</l>
        <l id="s19:l8" met="3" type="-+">In 's Hemels <rhyme label="d" type="m" corresp="#s19:l6">heilgenot</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Zie daar de blyde <rhyme label="a" type="f">toonen</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Verheft uit het <rhyme label="b" type="m">gemoed</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Van Neêrlands echte <rhyme label="a" type="f" corresp="#s20:l1">Zoonen</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Van 't Vorstbeminnend <rhyme label="b" type="m" corresp="#s20:l2">bloed</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">Het Kroost van Neêrlands <rhyme label="c" type="f">Helden</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Die al den Prinsen <rhyme label="d" type="m">daên</rhyme></l>
        <l id="s20:l7" met="3" type="-+">Met lof van haar <rhyme label="c" type="f" corresp="#s20:l5">vertelden</rhyme>,</l>
        <l id="s20:l8" met="3" type="-+">Uit 's Lands <rhyme label="d" type="m" corresp="#s20:l6">Historie-blaên</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Zie daar de <rhyme label="a" type="f">liefde-toonen</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Gegalmt uit het <rhyme label="b" type="m">gemoed</rhyme></l>
        <l id="s21:l3" met="3" type="-+">Van die te Amsterdam <rhyme label="a" type="f" corresp="#s21:l1">woonen</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">En in dien schoot <rhyme label="b" type="m" corresp="#s21:l2">gevoed</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Geteeld uit zulk een <rhyme label="c" type="f">Vader</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">Die streed voor Nassau's <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s21:l7" met="3" type="-+">Wiens bloed nog speeld in de <rhyme label="c" type="f" corresp="#s21:l5">ader</rhyme>,</l>
        <l id="s21:l8" met="3" type="-+">Tot Willem's glorie <rhyme label="d" type="m" corresp="#s21:l6">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
