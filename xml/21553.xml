  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VErhuecht met <rhyme label="a" type="m">my</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die door Christum troost <rhyme label="b" type="m">begheert</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Weest vro en <rhyme label="a" type="m" corresp="#s1:l1">bly</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Uwen paeys is <rhyme label="b" type="m" corresp="#s1:l2">gheconcludeert</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Uerneert, <rhyme label="b" type="m" corresp="#s1:l2">casseert</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Den druck in allen <rhyme label="c" type="f">stonden</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Want Christus is <rhyme label="d" type="m">nv</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ghestoruen voor ons <rhyme label="c" type="f" corresp="#s1:l6">sonden</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Dus anxt en <rhyme label="d" type="m" corresp="#s1:l7">gru</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Weert verre van <rhyme label="d" type="m" corresp="#s1:l7">v</rhyme></l>
        <l id="s1:l11" met="3" type="-+">Maer weest der werelt <rhyme label="d" type="m" corresp="#s1:l7">schu</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Der basuynen <rhyme label="a" type="m">gheclanck</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is ghehoort in allen <rhyme label="b" type="m">landen</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Ury sonder <rhyme label="a" type="m" corresp="#s2:l1">bedwanck</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ontslaet hy onse <rhyme label="b" type="m" corresp="#s2:l2">banden</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Sijn bloet, seer <rhyme label="b" type="m" corresp="#s2:l2">soet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Heeft ghenesen ons <rhyme label="c" type="f">allen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Uan des duyuels <rhyme label="d" type="m">macht</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Beurijt voor weder <rhyme label="c" type="f" corresp="#s2:l6">vallen</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Sijn groote <rhyme label="d" type="m" corresp="#s2:l7">cracht</rhyme></l>
        <l id="s2:l10" met="2" type="+-">Heeft hy tonder <rhyme label="d" type="m" corresp="#s2:l7">bracht</rhyme></l>
        <l id="s2:l11" met="3" type="-+">Ons lijden gheheel <rhyme label="d" type="m" corresp="#s2:l7">versacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Siet dat ghy <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dancbaer van zijn <rhyme label="b" type="m">ghenade</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Aenmerct den <rhyme label="a" type="m" corresp="#s3:l1">strijt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Bidt waect vroech ende <rhyme label="b" type="m" corresp="#s3:l2">spade</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Als kint, <rhyme label="b" type="m" corresp="#s3:l2">bemint</rhyme></l>
        <l id="s3:l6" met="3" type="-+">U viant tallen <rhyme label="c" type="f">stonden</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Al doense v groot <rhyme label="d" type="m">leet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En zijt doch niet <rhyme label="c" type="f" corresp="#s3:l6">beuonden</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Wraeckghierich en <rhyme label="d" type="m" corresp="#s3:l7">wreet</rhyme></l>
        <l id="s3:l10" met="2" type="+-">Om wreken <rhyme label="d" type="m" corresp="#s3:l7">bereet</rhyme></l>
        <l id="s3:l11" met="3" type="-+">Haelt in Gods woort <rhyme label="d" type="m" corresp="#s3:l7">bescheet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Prince <rhyme label="a" type="m">valliant</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Christus die coemt <rhyme label="b" type="m">stellen</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Ter rechter <rhyme label="a" type="m" corresp="#s4:l1">hant</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ontsleghen vander <rhyme label="b" type="m" corresp="#s4:l2">hellen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Ons gheest, <rhyme label="b" type="m" corresp="#s4:l2">tempeest</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Om uwen name wilt <rhyme label="c" type="f">slaken</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Onse boosheyt <rhyme label="d" type="m">quaet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Een nieu herte wilt ons <rhyme label="c" type="f" corresp="#s4:l6">maken</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Uan mijn <rhyme label="d" type="m" corresp="#s4:l7">misdaet</rhyme></l>
        <l id="s4:l10" met="2" type="+-">My Heere <rhyme label="d" type="m" corresp="#s4:l7">ontslaet</rhyme></l>
        <l id="s4:l11" met="3" type="-+">Sterckt my God al come ick <rhyme label="d" type="m" corresp="#s4:l7">laet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
