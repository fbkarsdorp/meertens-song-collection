        <l id="s1:l7" met="2" type="-+">Mijn teÃªr <rhyme label="c" type="f" corresp="#s1:l3">geliefde</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Hoor 's minnaars <rhyme label="d" type="m">stem</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Hij zingt en weeklaagt, o troost gij <rhyme label="d" type="m" corresp="#s1:l8">hem</rhyme>.</l>
