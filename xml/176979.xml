    <melody>Boire à son tour</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'k Ging in deez' <rhyme label="a" type="m">Morgenstond</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met Hagenroosje <rhyme label="b" type="f">wandlen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Langs 't weelig <rhyme label="a" type="m" corresp="#s1:l1">klaverrond</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om van myn min te <rhyme label="b" type="f" corresp="#s1:l2">handlen</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">'k Vond haar niet <rhyme label="c" type="m">straf</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Des ik haar <rhyme label="c" type="m" corresp="#s1:l5">gaf</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Myn mooije <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Myn mooije <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Myn <rhyme label="c" type="m" corresp="#s1:l5">Herdersstaf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Myn stafje greep zy <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik drukten haar heel <rhyme label="b" type="f">teder</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wyl ik haar borst zag <rhyme label="a" type="m" corresp="#s2:l1">gaan</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al zachtjes op en <rhyme label="b" type="f" corresp="#s2:l2">neder</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Ik gaf haar <rhyme label="c" type="m">toen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">In 't jeugdig <rhyme label="c" type="m" corresp="#s2:l5">groen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Terstond een <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Terstond een <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Terstond een <rhyme label="c" type="m" corresp="#s2:l5">zoen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Toen kusten zy my <rhyme label="a" type="m">weêr</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met hare malsche <rhyme label="b" type="f">lipjes</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Terwyl ik meer en <rhyme label="a" type="m" corresp="#s3:l1">meer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Haar blanke <rhyme label="b" type="f" corresp="#s3:l2">toverklipjes</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Bewegen <rhyme label="c" type="m">zag</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">'k Zei Engel, <rhyme label="c" type="m" corresp="#s3:l5">ach</rhyme>!</l>
        <l id="s3:l7" met="4" type="-+">Hoor eens myn <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Hoor eens myn <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Hoor myn <rhyme label="c" type="m" corresp="#s3:l5">geklach</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik heb u <rhyme label="a" type="m">menigwerf</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Myn trouwe min <rhyme label="b" type="f">gezworen</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Een min, waarom ik <rhyme label="a" type="m" corresp="#s4:l1">sterf</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Zo gy die noch wilt <rhyme label="b" type="f" corresp="#s4:l2">smoren</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Hoe menig <rhyme label="c" type="m">lied</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Speelde ik u <rhyme label="c" type="m" corresp="#s4:l5">niet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Op 't Herders <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Op 't Herders <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Op 't Herders <rhyme label="c" type="m" corresp="#s4:l5">riet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hoe menig droeve <rhyme label="a" type="m">zucht</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Verzeld van bittre <rhyme label="b" type="f">klachten</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zyn wel myn ziel <rhyme label="a" type="m" corresp="#s5:l1">ontvlucht</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">By dagen en by <rhyme label="b" type="f" corresp="#s5:l2">nachten</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">'k Zach myn <rhyme label="c" type="m">getreur</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Gesteld te <rhyme label="c" type="m" corresp="#s5:l5">leur</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Vlak voor uw <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Vlak voor uw <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Vlak voor uw <rhyme label="c" type="m" corresp="#s5:l5">deur</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hoe dikwyls, <rhyme label="a" type="m">Hagenroos</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Gaf ik u de eêlste <rhyme label="b" type="f">bloemen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Uit Flora's marmre <rhyme label="a" type="m" corresp="#s6:l1">doos</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Waar op Natuur kon <rhyme label="b" type="f" corresp="#s6:l2">roemen</rhyme>?</l>
        <l id="s6:l5" met="2" type="-+">Ja, noch bood <rhyme label="c" type="m">ik</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Aan u met <rhyme label="c" type="m" corresp="#s6:l5">schik</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Myn beste <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Myn beste <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Myn beste <rhyme label="c" type="m" corresp="#s6:l5">strik</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Zy zei, 't is waar 'k beken <rhyme label="a" type="m">'t</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">'k Heb u lang <rhyme label="b" type="f">opgehouwen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Schoon gy 't onwaardig <rhyme label="a" type="m" corresp="#s7:l1">bent</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Men moet niet licht <rhyme label="b" type="f" corresp="#s7:l2">vertrouwen</rhyme>;</l>
        <l id="s7:l5" met="2" type="-+">Die spreuk is <rhyme label="c" type="m">nut</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Zo ik niet <rhyme label="c" type="m" corresp="#s7:l5">dut</rhyme>;</l>
        <l id="s7:l7" met="4" type="-+">Kom in myn <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Kom in myn <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Kom in myn <rhyme label="c" type="m" corresp="#s7:l5">hut</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wy gingen zoetjes <rhyme label="a" type="m">heen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En traden t'saam' naar <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wyl zy my wel te <rhyme label="a" type="m" corresp="#s8:l1">vreên</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Beloofde weêr te <rhyme label="b" type="f" corresp="#s8:l2">minnen</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">Zy kreeg ter <rhyme label="c" type="m">sluik</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Van onder 't <rhyme label="c" type="m" corresp="#s8:l5">luik</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Een fraaije <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Een fraaije <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Een fraaije <rhyme label="c" type="m" corresp="#s8:l5">Kruik</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wy dronken met <rhyme label="a" type="m">malkaêr</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Op ons aanstaande <rhyme label="b" type="f">paren</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Ik overlei met <rhyme label="a" type="m" corresp="#s9:l1">haar</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Daar ras meê voort te <rhyme label="b" type="f" corresp="#s9:l2">varen</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">'k Kreeg 't in myn <rhyme label="c" type="m">hul</rhyme>;</l>
        <l id="s9:l6" met="2" type="-+">Zy greep heel <rhyme label="c" type="m" corresp="#s9:l5">gul</rhyme></l>
        <l id="s9:l7" met="4" type="-+">Weêr naar de <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Weêr naar de <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s9:l9" met="2" type="-+">Weêr naar de <rhyme label="c" type="m" corresp="#s9:l5">Pul</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Toen zyn wy wel te <rhyme label="a" type="m">vree'</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Al kussende <rhyme label="b" type="f">gescheiden</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Om elk ons wollig <rhyme label="a" type="m" corresp="#s10:l1">vee</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">In 't grazig dal te <rhyme label="b" type="f" corresp="#s10:l2">weiden</rhyme>.</l>
        <l id="s10:l5" met="2" type="-+">Weg <rhyme label="c" type="m">minnepyn</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Weg, weg <rhyme label="c" type="m" corresp="#s10:l5">verdwyn</rhyme>:</l>
        <l id="s10:l7" met="4" type="-+">'k Zal nu haast <rhyme label="d" type="m">tierelierelier</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">'k Zal nu haast <rhyme label="e" type="m">toereloereloer</rhyme>,</l>
        <l id="s10:l9" met="2" type="-+">Haast Bruîgom <rhyme label="c" type="m" corresp="#s10:l5">zyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
