    <melody>Een lied eerbaar</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MY dunckt die mint, is dick <rhyme label="a" type="m">ontsint</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En dat deur Venus <rhyme label="b" type="f">trecken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Eylaes ick kint, want ick <rhyme label="a" type="m" corresp="#s1:l1">bevint</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Al sou elck met my <rhyme label="b" type="f" corresp="#s1:l2">gecken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al ist dat claer, int <rhyme label="a" type="m">openbaer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat ick gheenen troost mach <rhyme label="b" type="f">crijghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ken kan voorwaer daeromme <rhyme label="a" type="m" corresp="#s2:l1">haer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Van ontrou noch <rhyme label="b" type="f" corresp="#s2:l2">betijghen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy houdt haer och, seer koelkens <rhyme label="a" type="m">noch</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ick merckt wel aen haer <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maer lacy doch, een troostelick <rhyme label="a" type="m" corresp="#s3:l1">soch</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sou my moghen <rhyme label="b" type="f" corresp="#s3:l2">ghenesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Princen ghelijck, gheen <rhyme label="a" type="m">rethorijck</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Sou my moghen <rhyme label="b" type="f">verheughen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Doch geen musijck, hoe <rhyme label="a" type="m" corresp="#s4:l1">autentijck</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dan sy alleen sout <rhyme label="b" type="f" corresp="#s4:l2">meughen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
