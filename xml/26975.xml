        <l id="s2:l9" met="4" type="-+">dar mb die katz nach ma senn <rhyme label="d" type="m" corresp="#s2:l7">trachtt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Noch hoff ich <rhyme label="a" type="m">des</rhyme> /</l>
        <l id="s3:l2" met="2" type="-+">sei nitt <rhyme label="a" type="m" corresp="#s3:l1">vergeß</rhyme> /</l>
        <l id="s3:l3" met="2" type="-+">der alter <rhyme label="b" type="m">trew</rhyme></l>
        <l id="s3:l4" met="2" type="-+">vnnd mich nitt <rhyme label="b" type="m" corresp="#s3:l3">schew</rhyme> /</l>
        <l id="s3:l5" met="2" type="-+">Jn rechter <rhyme label="c" type="m">lieb</rhyme> /</l>
