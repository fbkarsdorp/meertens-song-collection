        <l id="s1:l4" met="2" type="-+">vergessenn <rhyme label="b" type="m" corresp="#s1:l3">gar</rhyme> /</l>
        <l id="s1:l5" met="2" type="-+">iß worden <rhyme label="c" type="m">mein</rhyme></l>
        <l id="s1:l6" met="2" type="-+">es ist woll <rhyme label="c" type="m" corresp="#s1:l5">schein</rhyme> /</l>
