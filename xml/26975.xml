        <l id="s2:l4" met="2" type="-+">nu ist <rhyme label="b" type="m" corresp="#s2:l3">vmbkertt</rhyme> /</l>
        <l id="s2:l5" met="2" type="-+">als vur <rhyme label="c" type="m">was</rhyme></l>
        <l id="s2:l6" met="2" type="-+">n  merck ich <rhyme label="c" type="m" corresp="#s2:l5">das</rhyme></l>
        <l id="s2:l7" met="2" type="-+">man meiner nitt en <rhyme label="d" type="m">achtt</rhyme> /</l>
