        <l id="s1:l8" met="2" type="-+">will han kein <rhyme label="d" type="m" corresp="#s1:l7">bitt</rhyme> /</l>
        <l id="s1:l9" met="4" type="-+">Dr mb lest die katz irs mausens <rhyme label="d" type="m" corresp="#s1:l7">nitt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Vor zeitenn ich s <rhyme label="a" type="m">chtt</rhyme></l>
        <l id="s2:l2" met="2" type="-+">auch die <rhyme label="a" type="m" corresp="#s2:l1">streig</rhyme>/</l>
