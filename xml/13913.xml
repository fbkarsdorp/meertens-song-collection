<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13913</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
    </tags>
    <melody>Fietje Floris mijn speelmeisje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEl wat zijn dit voor <rhyme label="a" type="f">manieren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dat ‘et hier zoo is te <rhyme label="b" type="m">doen</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Dit’s al wat op ‘t nieuw <rhyme label="b" type="m" corresp="#s1:l2">Fatsoen</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Salmen Bachus moeten <rhyme label="a" type="f" corresp="#s1:l1">vyeren</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat staet my niet qualijck <rhyme label="c" type="m">an</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Want ick houwer lustigh <rhyme label="c" type="m" corresp="#s1:l5">van</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt dan ras de Tafel <rhyme label="a" type="f">decken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Haeldt ghesooden en <rhyme label="b" type="m">ghebraen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want de Klock gaet duysendt <rhyme label="b" type="m" corresp="#s2:l2">slaen</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">‘t Is nou tijdt te <rhyme label="a" type="f" corresp="#s2:l1">Pannelecken</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Elck die toont nu als een <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat hy Bachus eeren <rhyme label="c" type="m" corresp="#s2:l5">kan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer noch moet een yder <rhyme label="a" type="f">weten</rhyme></l>
        <l id="s3:l2" met="4" type="+-">(Dat hy dit doch wel <rhyme label="b" type="m">bedickt</rhyme>)</l>
        <l id="s3:l3" met="4" type="+-">Alle Glaesjes die men <rhyme label="b" type="m" corresp="#s3:l2">drinckt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">(‘t Is dan voor of nae den <rhyme label="a" type="f" corresp="#s3:l1">eeten</rhyme>)</l>
        <l id="s3:l5" met="4" type="+-">Moeten suyver ledigh <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">‘t Zy van Bier of klare <rhyme label="c" type="m" corresp="#s3:l5">Wijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zoo, soo, dat mach ick <rhyme label="a" type="f">verdragen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dit’s voor eerst een goedt <rhyme label="b" type="m">begin</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Leghter soo de Glaesjes <rhyme label="b" type="m" corresp="#s4:l2">in</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Anders mach ‘t my niet <rhyme label="a" type="f" corresp="#s4:l1">behaghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dat ‘et soo niet waer <rhyme label="c" type="m">ghesteldt</rhyme>;</l>
        <l id="s4:l6" met="4" type="+-">Maer een yder is een <rhyme label="c" type="m" corresp="#s4:l5">Heldt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hey! soo moetmen <rhyme label="a" type="f">bancketeeren</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Soeffen, Fressen al den <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Zoo langh als ‘t maer dueren <rhyme label="b" type="m" corresp="#s5:l2">mach</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Want wy moeten <rhyme label="a" type="f" corresp="#s5:l1">domineeren</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">‘t Mach ‘er doch ghenoegh op <rhyme label="c" type="m">staen</rhyme>:</l>
        <l id="s5:l6" met="4" type="+-">‘t Is met eens in ‘t Iaer <rhyme label="c" type="m" corresp="#s5:l5">ghedaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
