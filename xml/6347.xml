    <melody>Windje daar het bos af drilt</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MUchos negros de <rhyme label="a" type="m">gînea</rhyme></l>
        <l id="s1:l2" met="4" type="+-">I lamen pero bucha <rhyme label="a" type="m" corresp="#s1:l1">mea</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Deangole, <rhyme label="b" type="f">mariole</rhyme></l>
        <l id="s1:l4" met="4" type="+-">si quisieras comeer <rhyme label="c" type="m">pan</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Nolla maeys a negro <rhyme label="c" type="m" corresp="#s1:l4">can</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">In el chiefes del <rhyme label="a" type="m">ry</rhyme></l>
        <l id="s2:l2" met="4" type="+-">los negros no tien en <rhyme label="a" type="m" corresp="#s2:l1">ly</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Ny con blanco, ciego <rhyme label="b" type="f">manco</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Ny una pago seles <rhyme label="c" type="m">dan</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Todos los cantaros que <rhyme label="c" type="m" corresp="#s2:l4">braran</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Quando el negro tien <rhyme label="a" type="m">manteo</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Luego en bico su <rhyme label="a" type="m" corresp="#s3:l1">chapeo</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Capa y es spado, como <rhyme label="b" type="f">onrado</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Tenne y bayle mucho <rhyme label="c" type="m">bien</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Negro piente quel <rhyme label="c" type="m" corresp="#s3:l4">algen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Quando dan en <rhyme label="a" type="m">balentons</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Manco vados y <rhyme label="a" type="m" corresp="#s4:l1">ladrons</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Guardo, guardo, gento <rhyme label="b" type="f">pardo</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Gugadores <rhyme label="c" type="m">bebarons</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Grandisimus <rhyme label="c" type="m" corresp="#s4:l4">villacons</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Des muy cru como <rhyme label="a" type="m">bosio</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Entro u can con <rhyme label="a" type="m" corresp="#s5:l1">alegrio</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Enlacama, de <rhyme label="b" type="f">suama</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Callando com un <rhyme label="c" type="m">traeydor</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Hiz traisiona su <rhyme label="c" type="m" corresp="#s5:l4">señor</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ry niego detal canalla <rhyme label="a" type="m">mal</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Dallas concordas y <rhyme label="a" type="m" corresp="#s6:l1">pal</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Raucalla el cabella, y a <rhyme label="b" type="f">ella</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Para james <rhyme label="c" type="m">cousintiera</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Los braso que su <rhyme label="c" type="m" corresp="#s6:l4">negroda</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Quien vau â indis ô <rhyme label="a" type="m">Brasyl</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Trayga sucar ô <rhyme label="a" type="m" corresp="#s7:l1">anyl</rhyme></l>
        <l id="s7:l3" met="4" type="+-">o primente, que <rhyme label="b" type="f">allente</rhyme></l>
        <l id="s7:l4" met="4" type="+-">los u y pas y <rhyme label="c" type="m">caracon</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Not ray negro bu <rhyme label="c" type="m" corresp="#s7:l4">charon</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">ô principe negro suese <rhyme label="a" type="m">la</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Mas que nora <rhyme label="a" type="m" corresp="#s8:l1">malava</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Por mondongo, <rhyme label="b" type="f">aminicongo</rhyme></l>
        <l id="s8:l4" met="4" type="+-">señor si vos es <rhyme label="c" type="m">panrays</rhyme></l>
        <l id="s8:l5" met="4" type="+-">A caccays alla ven <rhyme label="c" type="m" corresp="#s8:l4">mays</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
