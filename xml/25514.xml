<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25514</id>
    <date>
      <year-start>1400</year-start>
      <year-end>1400</year-end>
    </date>
    <tags>
      <tag translation="reflective song">bespiegelend lied</tag>
      <tag translation="">geestelijk</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Et yn ys neyt al wege <rhyme label="a" type="f">vastauent</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Der doet der kompt ende brenget den <rhyme label="a" type="f" corresp="#s1:l1">auent</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Ende bynt vns myt eyme so vasten <rhyme label="b" type="m">bant</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Dat he ons trecket yn eyn ander <rhyme label="b" type="m" corresp="#s1:l3">lant</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ouch eyn ys yt neit al wege <rhyme label="a" type="f">mey</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Wyr moissen danssen an den <rhyme label="a" type="f" corresp="#s2:l1">rey</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Da vns dat meyen wyrt <rhyme label="b" type="m">yntwant</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Ind syngen voirt na eyn ander <rhyme label="b" type="m" corresp="#s2:l3">lant</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Alwege yn mogen wyr hyr neit <rhyme label="a" type="f">blyuen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Der doet wylt vns van hynne <rhyme label="a" type="f" corresp="#s3:l1">dryuen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Noch schyr morn off alrzo <rhyme label="b" type="m">hant</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Got der weis yt wyr moessen yn eyn ander <rhyme label="b" type="m" corresp="#s3:l3">lant</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wyr hoffen alle tzyt lange tzo <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ende begern dat yt vns gae <rhyme label="a" type="f" corresp="#s4:l1">euen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Also bleuen wyr gerne an dussen <rhyme label="b" type="m">kant</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Tzwair neyn wyr moessen yn eyn ander <rhyme label="b" type="m" corresp="#s4:l3">lant</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wie scone wyr vns tzieren ofte <rhyme label="a" type="f">weschen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Wyr synt alle eyrst comen van <rhyme label="a" type="f" corresp="#s5:l1">eschen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Dat eyrste pair volkes dat men <rhyme label="b" type="m">vant</rhyme></l>
        <l id="s5:l4" met="4" type="-+">De synt och vort yn eyn ander <rhyme label="b" type="m" corresp="#s5:l3">lant</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Och wat ys soessers dan dat <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Wyr moissen steruen ynd allet <rhyme label="a" type="f" corresp="#s6:l1">begheuen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Der doet der kumpt sunder wedder <rhyme label="b" type="m">stant</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Ende sleyft vns yn eyn ander <rhyme label="b" type="m" corresp="#s6:l3">lant</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ich wach ych brech yc heue yc <rhyme label="a" type="f">krude</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Vmb goet dat ys doch ander <rhyme label="a" type="f" corresp="#s7:l1">lude</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Yt was oucg hyr do yck yt <rhyme label="b" type="m">vant</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Ich laissent hyr ende varen eyn ander <rhyme label="b" type="m" corresp="#s7:l3">lant</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ich gaen hyr scheren ynde <rhyme label="a" type="f">scorgen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Vmb goet off ych mych solde <rhyme label="a" type="f" corresp="#s8:l1">erworgen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Got en hait mych daer vmb neyt heyr <rhyme label="b" type="m">gesant</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Want naket inde bloet varen ych yn eyn ander <rhyme label="b" type="m" corresp="#s8:l3">lant</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ich solde hyr got tzo allen <rhyme label="a" type="f">tzyden</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Louen dancken ind <rhyme label="a" type="f" corresp="#s9:l1">benediden</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Dat were myn schult myn mure myn <rhyme label="b" type="m">want</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Voir sathanas in dat ander <rhyme label="b" type="m" corresp="#s9:l3">lant</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Her gijr her gyr wat moecht yr <rhyme label="a" type="f">krigen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">It mois doch allet hyr blyuen <rhyme label="a" type="f" corresp="#s10:l1">lyggen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Myt vns moest yr vnder den <rhyme label="b" type="m">sant</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Ind varen hyn yn eyn ander <rhyme label="b" type="m" corresp="#s10:l3">lant</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Niemantz goet off eere to <rhyme label="a" type="f">nemen</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Vrunt des salstu dych <rhyme label="a" type="f" corresp="#s11:l1">yntwennen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">De dat daden de woerden <rhyme label="b" type="m">geschant</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Al hyr ynd ouch yn dat ander <rhyme label="b" type="m" corresp="#s11:l3">lant</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Wair ys karl hector myt <rhyme label="a" type="f">allexander</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Julius artes ynd mannych <rhyme label="a" type="f" corresp="#s12:l1">ander</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Rytter knecht inde amnnych <rhyme label="b" type="m">scharyant</rhyme></l>
        <l id="s12:l4" met="4" type="-+">War ander dan yn eyn ander <rhyme label="b" type="m" corresp="#s12:l3">lant</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Pais keyser hertzogen ynd <rhyme label="a" type="f">greuen</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Geistlich werentlich nichten ind <rhyme label="a" type="f" corresp="#s13:l1">neuen</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Dysser ys mannyger oer <rhyme label="b" type="m">gesant</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Sunder wedder keren yn eyn ander <rhyme label="b" type="m" corresp="#s13:l3">lant</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Were enych keyser van <rhyme label="a" type="f">rome</rhyme></l>
        <l id="s14:l2" met="4" type="-+">So wert so edel ynd so <rhyme label="a" type="f" corresp="#s14:l1">schone</rhyme></l>
        <l id="s14:l3" met="4" type="-+">Als eyn karbunkel off <rhyme label="b" type="m">dyemant</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Hie mois dan noch yn eyn ander <rhyme label="b" type="m" corresp="#s14:l3">lant</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Profficialen yr <rhyme label="a" type="f">aduocaten</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Rychter sceffen altzo <rhyme label="a" type="f" corresp="#s15:l1">malen</rhyme></l>
        <l id="s15:l3" met="4" type="-+">Ir bleuet gerne hijr hed yr <rhyme label="b" type="m">hant</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Auer neyn yr moest yn eyn ander <rhyme label="b" type="m" corresp="#s15:l3">lant</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Al synt wyr hyr vngelyck van <rhyme label="a" type="f">iaren</rhyme></l>
        <l id="s16:l2" met="4" type="-+">It ys myslych van my van <rhyme label="a" type="f" corresp="#s16:l1">hynenvaren</rhyme></l>
        <l id="s16:l3" met="4" type="-+">Alt ionck starch off wal <rhyme label="b" type="m">bewant</rhyme></l>
        <l id="s16:l4" met="4" type="-+">Wyr moessen al yn dat ander <rhyme label="b" type="m" corresp="#s16:l3">lant</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Der dach der mach tzom auende <rhyme label="a" type="f">kamen</rhyme></l>
        <l id="s17:l2" met="4" type="-+">Yt sy tzo schaden off tzo <rhyme label="a" type="f" corresp="#s17:l1">vromen</rhyme></l>
        <l id="s17:l3" met="4" type="-+">Na dem leuen kompt der doet <rhyme label="b" type="m">gerant</rhyme></l>
        <l id="s17:l4" met="4" type="-+">In driuet vns yn eyn ander <rhyme label="b" type="m" corresp="#s17:l3">lant</rhyme></l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">As wyr synt doet wyr magen <rhyme label="a" type="f">krygen</rhyme></l>
        <l id="s18:l2" met="4" type="-+">Eyn alt lachen da wyr yn <rhyme label="a" type="f" corresp="#s18:l1">lygen</rhyme></l>
        <l id="s18:l3" met="4" type="-+">Off eyn boisse kyste sunder <rhyme label="b" type="m">gewant</rhyme></l>
        <l id="s18:l4" met="4" type="-+">Dus varen ye arm yn eyn ander <rhyme label="b" type="m" corresp="#s18:l3">lant</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Och wat geselschaff vynden wy <rhyme label="a" type="f">dair</rhyme></l>
        <l id="s19:l2" met="4" type="-+">Kraden slangen wormen <rhyme label="a" type="f" corresp="#s19:l1">voirwair</rhyme></l>
        <l id="s19:l3" met="4" type="-+">Eyn kalt graff ys vnse bedde altzo <rhyme label="b" type="m">hant</rhyme></l>
        <l id="s19:l4" met="4" type="-+">Aldus sent men vns eyn ander <rhyme label="b" type="m" corresp="#s19:l3">lant</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Wy werden alle naket <rhyme label="a" type="f">gebaeren</rhyme></l>
        <l id="s20:l2" met="4" type="-+">Geyn eygen goet yn haynt wyr <rhyme label="a" type="f" corresp="#s20:l1">tzwaren</rhyme></l>
        <l id="s20:l3" met="4" type="-+">Dn vnse siele de ys eyn vnder <rhyme label="b" type="m">pant</rhyme></l>
        <l id="s20:l4" met="4" type="-+">Ir wercke vynt sy yn eyn ander <rhyme label="b" type="m" corresp="#s20:l3">lant</rhyme></l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">So wat der lycham haet <rhyme label="a" type="f">mysdaen</rhyme></l>
        <l id="s21:l2" met="4" type="-+">De seile yn mach des neit <rhyme label="a" type="f" corresp="#s21:l1">yntgaen</rhyme></l>
        <l id="s21:l3" met="4" type="-+">Se moys lyden pynen off den <rhyme label="b" type="m">brant</rhyme></l>
        <l id="s21:l4" met="4" type="-+">Hyr nae al yn dat ander <rhyme label="b" type="m" corresp="#s21:l3">lant</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Och lieue mynsche verstant wal <rhyme label="a" type="f">dat</rhyme></l>
        <l id="s22:l2" met="4" type="-+">Wyr yn wyssen hyr geyn blyuende <rhyme label="a" type="f" corresp="#s22:l1">stat</rhyme></l>
        <l id="s22:l3" met="4" type="-+">Stunde alle de werlt an vns <rhyme label="b" type="m">bewant</rhyme></l>
        <l id="s22:l4" met="4" type="-+">Wyr moessen tzom lesten yn eyn ander <rhyme label="b" type="m" corresp="#s22:l3">lant</rhyme></l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Got hait vns vnse tzyt <rhyme label="a" type="f">gegeuen</rhyme></l>
        <l id="s23:l2" met="4" type="-+">Dair wy neyt mogen ouer <rhyme label="a" type="f" corresp="#s23:l1">leuen</rhyme></l>
        <l id="s23:l3" met="4" type="-+">Al synt wyr nu starck tzo <rhyme label="b" type="m">hant</rhyme></l>
        <l id="s23:l4" met="4" type="-+">Doch moissen wy balden yndert ander <rhyme label="b" type="m" corresp="#s23:l3">lant</rhyme></l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Vns yn hylfet kruyt noch <rhyme label="a" type="f">wyn</rhyme></l>
        <l id="s24:l2" met="4" type="-+">Wyr moissen smachen des dodes <rhyme label="a" type="f" corresp="#s24:l1">pijn</rhyme></l>
        <l id="s24:l3" met="4" type="-+">Daer lucifer eirst deb vont aff <rhyme label="b" type="m">vant</rhyme></l>
        <l id="s24:l4" met="4" type="-+">Der moist he sceyden van den hilligen <rhyme label="b" type="m" corresp="#s24:l3">lant</rhyme></l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Intgayn den doet eyn ys geyn <rhyme label="a" type="f">raet</rhyme></l>
        <l id="s25:l2" met="4" type="-+">Dan wal tzo doen ynde laissen dat <rhyme label="a" type="f" corresp="#s25:l1">quait</rhyme></l>
        <l id="s25:l3" met="4" type="-+">Der doet yn wylt borge noch <rhyme label="b" type="m">pant</rhyme></l>
        <l id="s25:l4" met="4" type="-+">Wyr moissen vmmer yn eyn ander <rhyme label="b" type="m" corresp="#s25:l3">lant</rhyme></l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Dat beste dat ych mych kan <rhyme label="a" type="f">versynnen</rhyme></l>
        <l id="s26:l2" met="4" type="-+">Dat ys got vorten ynd altzijt <rhyme label="a" type="f" corresp="#s26:l1">mynnen</rhyme></l>
        <l id="s26:l3" met="4" type="-+">Dat sal syn vnser sielen <rhyme label="b" type="m">gewant</rhyme></l>
        <l id="s26:l4" met="4" type="-+">So varen wyr sycher yn dat ander <rhyme label="b" type="m" corresp="#s26:l3">lant</rhyme></l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Als wy werden alt kranck ynde vns <rhyme label="a" type="f">krummen</rhyme></l>
        <l id="s27:l2" met="4" type="-+">So were yt tzyt dat wyr besegen <rhyme label="a" type="f" corresp="#s27:l1">vmme</rhyme></l>
        <l id="s27:l3" met="4" type="-+">Als vns vntfellet der lecker <rhyme label="b" type="m">tzant</rhyme></l>
        <l id="s27:l4" met="4" type="-+">So wyllen wyr balde yn eyn ander <rhyme label="b" type="m" corresp="#s27:l3">lant</rhyme></l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Geduldicheyt were rechte <rhyme label="a" type="f">boesse</rhyme></l>
        <l id="s28:l2" met="4" type="-+">Tzo lyden wat men lyden <rhyme label="a" type="f" corresp="#s28:l1">moesse</rhyme></l>
        <l id="s28:l3" met="4" type="-+">As der doet brycht des leuens <rhyme label="b" type="m">bant</rhyme></l>
        <l id="s28:l4" met="4" type="-+">Vnde voert vns yn eyn ander <rhyme label="b" type="m" corresp="#s28:l3">lant</rhyme></l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Och got wie sal vnse leytz man <rhyme label="a" type="f">syn</rhyme></l>
        <l id="s29:l2" met="4" type="-+">Wyr yn wyssen neit dan vnse <rhyme label="a" type="f" corresp="#s29:l1">pyn</rhyme></l>
        <l id="s29:l3" met="4" type="-+">Der wech ys veer ynd <rhyme label="b" type="m">vnbekant</rhyme></l>
        <l id="s29:l4" met="4" type="-+">Daer wyr hynne varen yn eyn ander <rhyme label="b" type="m" corresp="#s29:l3">lant</rhyme></l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="4" type="-+">Och lieue vrunt bedencke den <rhyme label="a" type="f">grunt</rhyme></l>
        <l id="s30:l2" met="4" type="-+">We snel we kortz de doet <rhyme label="a" type="f" corresp="#s30:l1">tzerstunt</rhyme></l>
        <l id="s30:l3" met="4" type="-+">Nympt den mynchen van dyssen <rhyme label="b" type="m">bant</rhyme></l>
        <l id="s30:l4" met="4" type="-+">Ind voert yn hen yn eyn ander <rhyme label="b" type="m" corresp="#s30:l3">lant</rhyme></l>
      </lg>
      <lg id="s31" type="stanza">
        <l id="s31:l1" met="4" type="-+">Na dem as men bescreuen <rhyme label="a" type="f">vynt</rhyme></l>
        <l id="s31:l2" met="4" type="-+">So ys vnse leuen als eyn <rhyme label="a" type="f" corresp="#s31:l1">wynt</rhyme></l>
        <l id="s31:l3" met="4" type="-+">De der flueget ouer den <rhyme label="b" type="m">sant</rhyme></l>
        <l id="s31:l4" met="4" type="-+">So snel varen wyr yn eyn ander <rhyme label="b" type="m" corresp="#s31:l3">lant</rhyme></l>
      </lg>
      <lg id="s32" type="stanza">
        <l id="s32:l1" met="4" type="-+">Och dat ych ye wart <rhyme label="a" type="f">gebaeren</rhyme></l>
        <l id="s32:l2" met="4" type="-+">Dat ych myne ionge tzyt hayn <rhyme label="a" type="f" corresp="#s32:l1">verloeren</rhyme></l>
        <l id="s32:l3" met="4" type="-+">Doch here myn siele setze ych yn dyn <rhyme label="b" type="m">hant</rhyme></l>
        <l id="s32:l4" met="4" type="-+">As ych sal varen yn dat ander <rhyme label="b" type="m" corresp="#s32:l3">lant</rhyme></l>
      </lg>
      <lg id="s33" type="stanza">
        <l id="s33:l1" met="4" type="-+">Wyr wyllen vmmer des besten <rhyme label="a" type="f">hoffen</rhyme></l>
        <l id="s33:l2" met="4" type="-+">Die goitz genaide steyt altzyt <rhyme label="a" type="f" corresp="#s33:l1">offen</rhyme></l>
        <l id="s33:l3" met="4" type="-+">We wal vns got hait heer <rhyme label="b" type="m">gesant</rhyme></l>
        <l id="s33:l4" met="4" type="-+">Doch moissen wyr yn eyn ander <rhyme label="b" type="m" corresp="#s33:l3">lant</rhyme></l>
      </lg>
      <lg id="s34" type="stanza">
        <l id="s34:l1" met="4" type="-+">Bydden wyr marien de ioncfrau <rhyme label="a" type="f">fyn</rhyme></l>
        <l id="s34:l2" met="4" type="-+">Dat sy vnse troesterynne wyl <rhyme label="a" type="f" corresp="#s34:l1">syn</rhyme></l>
        <l id="s34:l3" met="4" type="-+">Ind blyuen doch vmmer vnse <rhyme label="b" type="m">vurstant</rhyme></l>
        <l id="s34:l4" met="4" type="-+">As wyr varen yn dat ander <rhyme label="b" type="m" corresp="#s34:l3">lant</rhyme></l>
      </lg>
      <lg id="s35" type="stanza">
        <l id="s35:l1" met="4" type="-+">Vnse here ihesus der wyl vns <rhyme label="a" type="f">geuen</rhyme></l>
        <l id="s35:l2" met="4" type="-+">Int hemelrych syn ewych <rhyme label="a" type="f" corresp="#s35:l1">leuen</rhyme></l>
        <l id="s35:l3" met="4" type="-+">Ind behoeden vns voir den boesen <rhyme label="b" type="m">vyant</rhyme></l>
        <l id="s35:l4" met="4" type="-+">Dat wy neyt inkomen yn dat helsche <rhyme label="b" type="m" corresp="#s35:l3">lant</rhyme></l>
      </lg>
      <lg id="s36" type="stanza">
        <l id="s36:l1" met="4" type="-+">Dyt ys vys yn hayns neit me <rhyme label="a" type="f">bescreuen</rhyme></l>
        <l id="s36:l2" met="4" type="-+">Got de brenge vns yn dat ewyge <rhyme label="a" type="f" corresp="#s36:l1">leuen</rhyme></l>
        <l id="s36:l3" met="4" type="-+">Dat wyr dair moissen werden <rhyme label="b" type="m">bekant</rhyme></l>
        <l id="s36:l4" met="4" type="-+">Myt alle hylligen yn dat hemelsche <rhyme label="b" type="m" corresp="#s36:l3">lant</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
