    <tags>
      <tag translation="drinking song">drinklied</tag>
    </tags>
    <melody>Laburette</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">NAbuyr ‘k wensch u goeden <rhyme label="a" type="m">dagh</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Laborette <rhyme label="b" type="m">Domino</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">‘k Brengh het u eens soe ’t wesen <rhyme label="a" type="m" corresp="#s1:l1">mach</rhyme>,</l>
        <l id="s1:l4" met="1" type="-+">Laet <rhyme label="c" type="f">varen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Laborette, ‘k gader mee <rhyme label="d" type="f">decken</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Mijn Schuytje is <rhyme label="d" type="f" corresp="#s1:l5">lecke</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Ick kanje niet <rhyme label="e" type="f">setten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Laborette <rhyme label="b" type="m" corresp="#s1:l2">Domino</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nu set het aen u <rhyme label="a" type="m">mondt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Laborette <rhyme label="b" type="m">Domino</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En drinckt het uyt tot aen den <rhyme label="a" type="m" corresp="#s2:l1">grondt</rhyme>,</l>
        <l id="s2:l4" met="1" type="-+">Laet <rhyme label="c" type="f">varen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Laborette, ‘k gader mee <rhyme label="d" type="f">decken</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Mijn Schuytje is <rhyme label="d" type="f" corresp="#s2:l5">lecke</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Ick kanje niet <rhyme label="e" type="f">setten</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Laborette <rhyme label="b" type="m" corresp="#s2:l2">Domino</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Tsa Schencker, spoelt das <rhyme label="a" type="m">glas</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Laborette <rhyme label="b" type="m">Domino</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En schenckt het als ’t te vooren <rhyme label="a" type="m" corresp="#s3:l1">was</rhyme>,</l>
        <l id="s3:l4" met="1" type="-+">Laet <rhyme label="c" type="f">varen</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Laborette, ‘k gader mee <rhyme label="d" type="f">decken</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Mijn Schuytje is <rhyme label="d" type="f" corresp="#s3:l5">lecke</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ick kanje niet <rhyme label="e" type="f">setten</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Laborette <rhyme label="b" type="m" corresp="#s3:l2">Domino</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O neen, dat mach alsoo niet <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Laborette <rhyme label="b" type="m">Domino</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gheeft eerst malkaer de <rhyme label="a" type="m" corresp="#s4:l1">mondelijn</rhyme>,</l>
        <l id="s4:l4" met="1" type="-+">Laet <rhyme label="c" type="f">varen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Laborette, ‘k gader mee <rhyme label="d" type="f">decken</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Mijn Schuytje is <rhyme label="d" type="f" corresp="#s4:l5">lecke</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Ick kanje niet <rhyme label="e" type="f">setten</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Laborette <rhyme label="b" type="m" corresp="#s4:l2">Domino</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
