  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ic zie wel watter <rhyme label="a" type="m">scuult</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">Als goede ghezellen geen gelt en <rhyme label="b" type="f">hebben</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dan zit den weert en <rhyme label="a" type="m" corresp="#s1:l1">muult</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Ten es geleden geen langhe <rhyme label="c" type="f">jaren</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dat ic heb alzo <rhyme label="c" type="f" corresp="#s1:l4">gevaren</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ic hadde zo gerne <rhyme label="d" type="f">geruult</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Myn geldekin haddic al <rhyme label="e" type="m">verteert</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Ic zach onnoselic up den <rhyme label="e" type="m" corresp="#s1:l7">weert</rhyme></l>
        <l id="s1:l9" met="3" type="+-">En wart met my <rhyme label="a" type="m" corresp="#s1:l1">gedruult</rhyme>.</l>
        <l id="s1:l10" met="3" type="-+">Ic zie wel watter <rhyme label="a" type="m" corresp="#s1:l1">scuult</rhyme>.</l>
        <l id="s1:l11" met="4" type="-+">Als goede ghezellen geen gelt en <rhyme label="b" type="f" corresp="#s1:l2">hebben</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Dan zit den weert en <rhyme label="a" type="m" corresp="#s1:l1">muult</rhyme>.</l>
      </lg>
    </lg>
