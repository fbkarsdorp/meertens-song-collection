<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188116</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">t HEyligh Bid-huys van den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Was al <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Ons verlosser 't quam <rhyme label="b" type="f">betreden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Eenen Kraem van <rhyme label="c" type="m">Koopmanschap</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Daer de <rhyme label="c" type="m" corresp="#s1:l4">klap</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Hof hiel, vol van <rhyme label="b" type="f" corresp="#s1:l3">ydelheden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Gulden kunst-werck word <rhyme label="a" type="m">ontciert</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Door 't <rhyme label="a" type="m" corresp="#s2:l1">gediert</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Die 't betraden, en <rhyme label="b" type="f">bevuylden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En in plaets van Godes <rhyme label="c" type="m">Lof</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">('t Is te <rhyme label="c" type="m" corresp="#s2:l4">grof</rhyme>)</l>
        <l id="s2:l6" met="4" type="+-">In Godts Huys de Dieren <rhyme label="b" type="f" corresp="#s2:l3">huylden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer doe Jesus daer in <rhyme label="a" type="m">quam</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Strack hy <rhyme label="a" type="m" corresp="#s3:l1">nam</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Eenen Geessel-snoer in <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Daer mee hy al dat <rhyme label="c" type="m">gespuys</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Wt Godts <rhyme label="c" type="m" corresp="#s3:l4">Huys</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Tot de open Merckt-pleyn <rhyme label="b" type="f" corresp="#s3:l3">bande</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sulck een Koop-volck, set de <rhyme label="a" type="m">voet</rhyme></l>
        <l id="s4:l2" met="2" type="+-">In 't <rhyme label="a" type="m" corresp="#s4:l1">gemoet</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Van een yder Mensche <rhyme label="b" type="f">mede</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat is het veel-vuldigh <rhyme label="c" type="m">zaedt</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Van het <rhyme label="c" type="m" corresp="#s4:l4">quaedt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Woelende in boose <rhyme label="b" type="f" corresp="#s4:l3">seden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer als Jesus sulck <rhyme label="a" type="m">gemoet</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Eens <rhyme label="a" type="m" corresp="#s5:l1">begroet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Met een aenkomst tot <rhyme label="b" type="f">bekeeringh</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Vlucht dit Legher seer <rhyme label="c" type="m">verbaest</rhyme></l>
        <l id="s5:l5" met="2" type="+-">In der <rhyme label="c" type="m" corresp="#s5:l4">haest</rhyme>:</l>
        <l id="s5:l6" met="4" type="+-">En dan groeyt de heyl'ge <rhyme label="b" type="f" corresp="#s5:l3">leeringh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
