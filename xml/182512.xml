<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182512</id>
    <date>
      <year-start>1793</year-start>
      <year-end>1793</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb de groene straten</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik ben 'er de groene <rhyme label="a" type="f">straetjes</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zo dikmaels ten einde <rhyme label="b" type="m">gegaen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ik 't myn Liefje moeten <rhyme label="c" type="f">laten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat hebben myn Vrienden <rhyme label="b" type="m" corresp="#s1:l2">gedaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zal haar nooit <rhyme label="a" type="f">verlaaten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Al waaren zy nog zo <rhyme label="b" type="m">boos</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Ik zal haer gedagten <rhyme label="c" type="f">weesen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot dat ik sterf de <rhyme label="b" type="m" corresp="#s2:l2">dood</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Niet langer als gisteren <rhyme label="a" type="f">avond</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Kwam ik voor myn zoete liefs <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ik zei 'er wel Betje doet <rhyme label="c" type="f">open</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Doet open, ik sta 'er <rhyme label="b" type="m" corresp="#s3:l2">veur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik doe u voorwaer niet <rhyme label="a" type="f">open</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En ik laet u voorwaer niet <rhyme label="b" type="m">in</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Gaet na huis en legt u te <rhyme label="c" type="f">slaepen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daeris 'er een ander lief <rhyme label="b" type="m" corresp="#s4:l2">in</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Is daer een ander Lief <rhyme label="a" type="f">inne</rhyme>?</l>
        <l id="s5:l2" met="3" type="-+">Dat ik u niet spreeken <rhyme label="b" type="m">mag</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dan wensch ik u voor het <rhyme label="c" type="f">laatste</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Nog eens een vrolyke <rhyme label="b" type="m" corresp="#s5:l2">nagt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer Betje als gy komt te <rhyme label="a" type="f">trouwen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Schryf my dan maer eens een <rhyme label="b" type="m">Brief</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Als ik op u Bruiloft mag <rhyme label="c" type="f">komen</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">En kiesen een ander zoete <rhyme label="b" type="m" corresp="#s6:l2">lief</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Gy zult op myn Bruiloft niet <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Gy zult op myn Bruiloft niet <rhyme label="b" type="m">zyn</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Daer zyn wel andere <rhyme label="c" type="f">Ionkmans</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die in de gratie <rhyme label="b" type="m" corresp="#s7:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Het schuitje is myn <rhyme label="a" type="f">omdreeven</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">De ankers waren niet <rhyme label="b" type="m">vast</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Myn Liefje is myn <rhyme label="c" type="f">ontvreeden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ik heb 'er niet wel <rhyme label="b" type="m" corresp="#s8:l2">opgepast</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wel is 'er uw Liefje <rhyme label="a" type="f">ontvreden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Heb jy daer niet wel <rhyme label="b" type="m">opgepast</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Dan moet jy een ander Lief <rhyme label="c" type="f">zoeken</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En houden ze beter <rhyme label="b" type="m" corresp="#s9:l2">vast</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wel Betje als gy komt te <rhyme label="a" type="f">kramen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Wie zal 'er voor Vader <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Wel Iantje dat zal ik u <rhyme label="c" type="f">zeggen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Uit de dingen by my heeft <rhyme label="b" type="m" corresp="#s10:l2">gedaan</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wel Betje zult gy my dat <rhyme label="a" type="f">zeggen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Wie de dingen by u had <rhyme label="b" type="m">gedaen</rhyme>?</l>
        <l id="s11:l3" met="3" type="-+">Dan moet gy wel agtien <rhyme label="c" type="f">weeken</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Onder de Geboden <rhyme label="b" type="m" corresp="#s11:l2">staan</rhyme>:</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Wel Betje als gy komt te <rhyme label="a" type="f">sterven</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ik zal u gedatelijk <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Ik zal op u Grafsteen <rhyme label="c" type="f">schryven</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Hier legt een Hoertje van <rhyme label="b" type="m" corresp="#s12:l2">myn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
