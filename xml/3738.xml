    <melody>Psalm 135 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LOEFT nu vry onses Godts <rhyme label="a" type="m">naem</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Al ghy dienaers des <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Komt wilt hem prysen al <rhyme label="a" type="m" corresp="#s1:l1">t'saem</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ghy die daer woent met <rhyme label="b" type="f" corresp="#s1:l2">eeren</rhyme></l>
        <l id="s1:l5" met="3" type="-+">In sijn huys: Wilt hem <rhyme label="c" type="f">louen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In sijn schoone <rhyme label="c" type="f" corresp="#s1:l5">voorhouen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Loeft den Heer, want hy is <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Sijns naems eer laett nu <rhyme label="b" type="f">hooren</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Hy is lieflick ende <rhyme label="a" type="m" corresp="#s2:l1">soet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dies heeft hy hem <rhyme label="b" type="f" corresp="#s2:l2">verkoren</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Israel end Iacob <rhyme label="c" type="f">vrom</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tot een eewigh <rhyme label="c" type="f" corresp="#s2:l5">eyghendom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ick weet wt Godts <rhyme label="a" type="m">gheboden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat de Heer' hoogh' <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Meerder is dan <rhyme label="a" type="m" corresp="#s3:l1">d'afgoden</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Want wat hy wilt, moet <rhyme label="b" type="f" corresp="#s3:l2">wesen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">In hemel end d'ęerde <rhyme label="c" type="f">bloot</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In de Zee diep' ende <rhyme label="c" type="f" corresp="#s3:l5">groot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy doet die wolcken <rhyme label="a" type="m">opstaen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Van t'eynde des <rhyme label="b" type="f">ęerdtrycken</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Den blixem laett hy <rhyme label="a" type="m" corresp="#s4:l1">wtgaen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Den reghen <rhyme label="b" type="f" corresp="#s4:l2">desghelycken</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Hy bringht den windt voort vol <rhyme label="c" type="f">macht</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wt sijn schatkamer met <rhyme label="c" type="f" corresp="#s4:l5">kracht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">D'eerste gheboren' in't <rhyme label="a" type="m">landt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">AEgypti, sijn <rhyme label="b" type="f">ghestoruen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Mensch' end vee kaemen ter <rhyme label="a" type="m" corresp="#s5:l1">schand'</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">End waeren t'saem <rhyme label="b" type="f" corresp="#s5:l2">verdoruen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">In AEgypten sagh men <rhyme label="c" type="f">klaer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Godts teekenen <rhyme label="c" type="f" corresp="#s5:l5">wonderbaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Pharao was <rhyme label="a" type="m">verslaeghen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met sijn knechten al <rhyme label="b" type="f">t'saemen</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Koninghen tot dien <rhyme label="a" type="m" corresp="#s6:l1">daeghen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">End volckeren <rhyme label="b" type="f" corresp="#s6:l2">omkaemen</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Sihon end Og tot <rhyme label="c" type="f">Basan</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Verghinghen in <rhyme label="c" type="f" corresp="#s6:l5">Chanaan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hy ghaf sijn volck <rhyme label="a" type="m">Israel</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Haer landt tot eener <rhyme label="b" type="f">eruen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Om dat te besitten <rhyme label="a" type="m" corresp="#s7:l1">wel</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Altydt, sonder <rhyme label="b" type="f" corresp="#s7:l2">verderuen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Heer uwes naems <rhyme label="c" type="f">heerlickheit</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Gheduert in der <rhyme label="c" type="f" corresp="#s7:l5">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Godts ghedachtenisse <rhyme label="a" type="m">fyn</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gheduert tot allen <rhyme label="b" type="f">tyden</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">De Heer' sal oock dat volck <rhyme label="a" type="m" corresp="#s8:l1">syn</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Richten, end doen <rhyme label="b" type="f" corresp="#s8:l2">verblyden</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Hy sal sijnen knechten <rhyme label="c" type="f">goet</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Vriendlick wesen ende <rhyme label="c" type="f" corresp="#s8:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Der heydenen beelden <rhyme label="a" type="m">slecht</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Siluer end goudt steedts <rhyme label="b" type="f">blyuen</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">T'maecksel der menschen <rhyme label="a" type="m" corresp="#s9:l1">onrecht</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Om boosheit te <rhyme label="b" type="f" corresp="#s9:l2">bedryuen</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Haer mondt kan gantsch spreken <rhyme label="c" type="f">niet</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">heen harer ooghen jet <rhyme label="c" type="f" corresp="#s9:l5">siet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Haer ooren niet hooren <rhyme label="a" type="m">gaer</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Gheen Gheest komt wt den <rhyme label="b" type="f">monden</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Die die maeken sijn <rhyme label="a" type="m" corresp="#s10:l1">voorwaer</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Also: Sy oock die <rhyme label="b" type="f" corresp="#s10:l2">gronden</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Vast daer op haer <rhyme label="c" type="f">vertrauwen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Ende haer hope <rhyme label="c" type="f" corresp="#s10:l5">bauwen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">O ghy huys van <rhyme label="a" type="m">Israel</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Singht, wilt Godt eer' <rhyme label="b" type="f">bewysen</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Loeft hen Aarons huys <rhyme label="a" type="m" corresp="#s11:l1">snel</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">T'huys Leui met hem <rhyme label="b" type="f" corresp="#s11:l2">prysen</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Loeft den Heer', hem oock nu <rhyme label="c" type="f">vreest</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Pryst hem, danckt hem in de <rhyme label="c" type="f" corresp="#s11:l5">gheest</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Gheloeft end hoogh' <rhyme label="a" type="m">verheuen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Sij Godt wt Zion <rhyme label="b" type="f">krachtigh</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Die hem 'gantsch heeft <rhyme label="a" type="m" corresp="#s12:l1">begheuen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Om te blyuen <rhyme label="b" type="f" corresp="#s12:l2">wonachtigh</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">In die stadt <rhyme label="c" type="f">Ierusalem</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Loeft hem met herten end <rhyme label="c" type="f" corresp="#s12:l5">stem</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
