    <melody>De tijd is hier</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Laet ons Godt louen / en nu de gracy <rhyme label="a" type="f">singhen</rhyme></l>
        <l id="s1:l2" met="5" type="-+">den vader hier bouen / hy ist diet al <rhyme label="b" type="m">versiet</rhyme></l>
        <l id="s1:l3" met="5" type="-+">wat wy behoeuen / hy geeft ons alle <rhyme label="a" type="f" corresp="#s1:l1">dingen</rhyme></l>
        <l id="s1:l4" met="5" type="-+">syn goetheyt wy proeuen / syn soetheyt hy ons <rhyme label="b" type="m" corresp="#s1:l2">biet</rhyme></l>
        <l id="s1:l5" met="6" type="-+">met barmhertighe oogen hy nederwaerts na ons <rhyme label="b" type="m" corresp="#s1:l2">siet</rhyme></l>
        <l id="s1:l6" met="3" type="+-">al wt syn hauen / <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">wel vloeyen alle gauen / <rhyme label="c" type="m" corresp="#s1:l6">soet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">hy is die ons lauen / <rhyme label="c" type="m" corresp="#s1:l6">doet</rhyme></l>
        <l id="s1:l9" met="3" type="-+">met melck en wyn om <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Om ons nateur / te stercken met <rhyme label="a" type="f">gesontheyt</rhyme></l>
        <l id="s2:l2" met="5" type="-+">wel op deser uer / met spys en dranck ons <rhyme label="b" type="m">voet</rhyme></l>
        <l id="s2:l3" met="5" type="-+">alle createur / alhier in swerelts <rhyme label="a" type="f" corresp="#s2:l1">rontheyt</rhyme></l>
        <l id="s2:l4" met="5" type="-+">al synse stuer / voor ons hy bringhen <rhyme label="b" type="m" corresp="#s2:l2">doet</rhyme></l>
        <l id="s2:l5" met="6" type="-+">hy heeft geschapen al die lyf en siel <rhyme label="b" type="m" corresp="#s2:l2">behoet</rhyme></l>
        <l id="s2:l6" met="3" type="+-">in onser feesten / <rhyme label="c" type="m">schier</rhyme></l>
        <l id="s2:l7" met="3" type="-+">weest danckbaer minst en meesten / <rhyme label="c" type="m" corresp="#s2:l6">hier</rhyme></l>
        <l id="s2:l8" met="3" type="-+">hy maeckt ons van geesten / <rhyme label="c" type="m" corresp="#s2:l6">fier</rhyme></l>
        <l id="s2:l9" met="3" type="-+">door weldaet <rhyme label="b" type="m" corresp="#s2:l2">oueruloet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Visschen in see / voor ons sy moeten <rhyme label="a" type="f">swemmen</rhyme></l>
        <l id="s3:l2" met="5" type="-+">dieren en vee / die op berch en dal <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s3:l3" met="5" type="-+">vogelkens mee / voor ons haer laten <rhyme label="a" type="f" corresp="#s3:l1">temmen</rhyme></l>
        <l id="s3:l4" met="5" type="-+">cruyden hoer ree / cryghen haer <rhyme label="b" type="m" corresp="#s3:l2">sonneschyn</rhyme></l>
        <l id="s3:l5" met="6" type="-+">boomen haer reeghen het dient voor ons al <rhyme label="b" type="m" corresp="#s3:l2">fyn</rhyme></l>
        <l id="s3:l6" met="3" type="+-">al wat op aerde / <rhyme label="c" type="m">groeyt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">en hier seer fray in weerde / <rhyme label="c" type="m" corresp="#s3:l6">bloeyt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">tot ons te voet te paerde / <rhyme label="c" type="m" corresp="#s3:l6">vloeyt</rhyme></l>
        <l id="s3:l9" met="3" type="-+">te scheep in menichten <rhyme label="b" type="m" corresp="#s3:l2">fyn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ons gerechticheyt noch onser niet behoeft <rhyme label="a" type="f">hy</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Om syn maiestyt wil hy oock syn ons <rhyme label="b" type="m">Godt</rhyme></l>
        <l id="s4:l3" met="5" type="-+">En is oock bereyt dies hem van herten looft <rhyme label="a" type="f" corresp="#s4:l1">vry</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Om ons salicheyt te geuen tot en <rhyme label="b" type="m" corresp="#s4:l2">lot</rhyme></l>
        <l id="s4:l5" met="6" type="-+">Singt danckelyck dan ende hout syn <rhyme label="b" type="m" corresp="#s4:l2">gebodt</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Hy is een <rhyme label="c" type="m">wonderheer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Hy can ons helpen sonder <rhyme label="c" type="m" corresp="#s4:l6">seer</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Geuen ouer en onder <rhyme label="c" type="m" corresp="#s4:l6">meer</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Van als dat isset <rhyme label="b" type="m" corresp="#s4:l2">slot</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Prince versint laet ons den vaeder <rhyme label="a" type="f">heerlyck</rhyme></l>
        <l id="s5:l2" met="5" type="-+">als kinderen bemindt wel soecken nu <rhyme label="b" type="m">voortaen</rhyme></l>
        <l id="s5:l3" met="5" type="-+">Die hem niet vindt hy doolt eylacy <rhyme label="a" type="f" corresp="#s5:l1">deerlyck</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Inde werelt blint noch blyftse heel <rhyme label="b" type="m" corresp="#s5:l2">belaen</rhyme></l>
        <l id="s5:l5" met="6" type="-+">Wilt dit wel verstaen die clopt die sal <rhyme label="b" type="m" corresp="#s5:l2">ontfaen</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Want het niet te deegen <rhyme label="c" type="m">is</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Hebben wy niet syn seegen <rhyme label="c" type="m" corresp="#s5:l6">fris</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En wandelen in syn weegen <rhyme label="c" type="m" corresp="#s5:l6">wis</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Tsal ons qualyck <rhyme label="b" type="m" corresp="#s5:l2">vergaen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
