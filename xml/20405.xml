<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20405</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Waar schuilt mijn nimfje in het groen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe treurt mijn zieltje vol <rhyme label="a" type="m">smert</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Om de schoone <rhyme label="b" type="m">Rosemondt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die mijn verlieven- de <rhyme label="a" type="m" corresp="#s1:l1">hart</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Heeft geprickelt en <rhyme label="b" type="m" corresp="#s1:l2">gewont</rhyme>,</l>
        <l id="s1:l5" met="6" type="+-">Door haer glans // die <rhyme label="c" type="m">bykans</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Aen den dans // lock alle de <rhyme label="c" type="m" corresp="#s1:l5">Jonghmans</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe was mijn hartje <rhyme label="a" type="m">verheught</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Doen ick eerstmael haer <rhyme label="b" type="m">aensach</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Maer ach! die schielijcke <rhyme label="a" type="m" corresp="#s2:l1">vreught</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Is verandert <rhyme label="b" type="m" corresp="#s2:l2">ingheklach</rhyme>,</l>
        <l id="s2:l5" met="6" type="+-">Nu dat sy // geen gevry // wil van <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Maer schimp en smaedt ick <rhyme label="c" type="m" corresp="#s2:l5">ly</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doen sy in't Bosch, aende <rhyme label="a" type="m">kant</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Van de klare <rhyme label="b" type="m">Water-beeck</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Haer Wooningh hadde <rhyme label="a" type="m" corresp="#s3:l1">gheplant</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Satse meninghmael en <rhyme label="b" type="m" corresp="#s3:l2">keeck</rhyme></l>
        <l id="s3:l5" met="6" type="+-">In de vloet // wel gemoet // vlocht een <rhyme label="c" type="m">hoet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van Kruydt, en Blomtjes <rhyme label="c" type="m" corresp="#s3:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daer heb ick 't aerdige <rhyme label="a" type="m">beelt</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Menighmael ghesproken <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En op mijn Pijpje <rhyme label="a" type="m" corresp="#s4:l1">ghespeelt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Als ick quam langhs 't Water <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>,</l>
        <l id="s4:l5" met="6" type="+-">Haer ter eer // maer veel meer // neemt sy <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van my nu een <rhyme label="c" type="m" corresp="#s4:l5">afkeer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ny sy haer Schaepjes en <rhyme label="a" type="m">vee</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En het Boersche leven <rhyme label="b" type="m">laet</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">En neemt haer Wooningh in <rhyme label="a" type="m" corresp="#s5:l1">stee</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Schijntet datse my oock <rhyme label="b" type="m" corresp="#s5:l2">haet</rhyme>.</l>
        <l id="s5:l5" met="6" type="+-">Steetse pracht // sy meer acht // dan mijn <rhyme label="c" type="m">klacht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In droefheydt ick <rhyme label="c" type="m" corresp="#s5:l5">versmacht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Het krenckt mijn jeughdighe <rhyme label="a" type="m">bloedt</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Als ick denck dat sy met <rhyme label="b" type="m">vreught</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Aen Thyrsis offeren <rhyme label="a" type="m" corresp="#s6:l1">moet</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">'t Aldereelste van haer <rhyme label="b" type="m" corresp="#s6:l2">jeught</rhyme>:</l>
        <l id="s6:l5" met="6" type="+-">Hayrtjes blondt // Ooghjes, Mondt // Borsjes <rhyme label="c" type="m">rondt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En, laes! ick blijf <rhyme label="c" type="m" corresp="#s6:l5">ghewondt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O Rosemondt: ach! <rhyme label="a" type="m">vriendin</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Waerder noch wat hoop voor <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick knield' en badt om u <rhyme label="a" type="m" corresp="#s7:l1">Min</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Tot verlichtingh van mijn <rhyme label="b" type="m" corresp="#s7:l2">pijn</rhyme>;</l>
        <l id="s7:l5" met="6" type="+-">Ick leyd' af // en vergaf // d'Harders <rhyme label="c" type="m">staf</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En koos de stadt, voort <rhyme label="c" type="m" corresp="#s7:l5">Graf</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
