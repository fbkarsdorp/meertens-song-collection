<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3231</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Van een meisje fier het was</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">DE tierlijcke <rhyme label="a" type="m">Mey</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Brengt bloempjes <rhyme label="a" type="m" corresp="#s1:l1">veelderley</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En zaeytse in de <rhyme label="b" type="f">Hoven</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Van vriendelijcke <rhyme label="c" type="m">geur</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En seer aenghename <rhyme label="c" type="m" corresp="#s1:l4">kleur</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Alles haeltse dat van <rhyme label="b" type="f" corresp="#s1:l3">boven</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Laet ons Godt den Gever <rhyme label="b" type="f" corresp="#s1:l3">loven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Yder Beesje <rhyme label="a" type="m">queelt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ende juyghende <rhyme label="a" type="m" corresp="#s2:l1">speelt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met een soo vriendlijck <rhyme label="b" type="f">swieren</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Het is soo soeten <rhyme label="c" type="m">saeck</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">'t Is soo lieffelijck <rhyme label="c" type="m" corresp="#s2:l4">vermaeck</rhyme></l>
        <l id="s2:l6" met="4" type="+-">In 'et veldt de dertle <rhyme label="b" type="f" corresp="#s2:l3">dieren</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Soet te hooren <rhyme label="b" type="f" corresp="#s2:l3">tierelieren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Hier schatert de <rhyme label="a" type="m">tael</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van de soete <rhyme label="a" type="m" corresp="#s3:l1">Nachtegael</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daer we'er de stem der <rhyme label="b" type="f">Cijsen</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Elck die wil met <rhyme label="c" type="m">gheluyt</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Roepen Godes goetheydt <rhyme label="c" type="m" corresp="#s3:l4">uyt</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Yder wil den Schepper <rhyme label="b" type="f" corresp="#s3:l3">prijsen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En met zangh hem eer <rhyme label="b" type="f" corresp="#s3:l3">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Godt die is soo <rhyme label="a" type="m">goedt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In rijcken <rhyme label="a" type="m" corresp="#s4:l1">overvloedt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat hy dit lieflijck <rhyme label="b" type="f">queelen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Beneffens waerder <rhyme label="c" type="m">pandt</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Wil door zijn gaef-rijcke <rhyme label="c" type="m" corresp="#s4:l4">handt</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Ons heur soete vreucht en <rhyme label="b" type="f" corresp="#s4:l3">speelen</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Vyt ghenae en gunst <rhyme label="b" type="f" corresp="#s4:l3">mee-deelen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Laten wy de <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Gheduerigh meer en <rhyme label="a" type="m" corresp="#s5:l1">meer</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Voor allerhande <rhyme label="b" type="f">dinghen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ter eeren sijnen <rhyme label="c" type="m">Naem</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Staegh danckbaerheydt al te <rhyme label="c" type="m" corresp="#s5:l4">saem</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Tot sijn Naems verhoogingh <rhyme label="b" type="f" corresp="#s5:l3">bringhen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">En voor al de Wereldt <rhyme label="b" type="f" corresp="#s5:l3">singhen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
