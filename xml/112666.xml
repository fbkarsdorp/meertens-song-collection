<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>112666</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Ik lig somtijds gehele nachten</melody>
    <categories>
      <category translation="children">kinderen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AArdig stafje. teder <rhyme label="a" type="f">Wayertje</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Klein en dartel <rhyme label="a" type="f" corresp="#s1:l1">windezwayertje</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ach! was ik in uwe <rhyme label="b" type="m">plaats</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Gy vermoogt de <rhyme label="c" type="f">roozetipjes</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Raken van myn kloris <rhyme label="c" type="f" corresp="#s1:l4">lipjes</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Want zy denkt van u niets <rhyme label="b" type="m" corresp="#s1:l3">quaats</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als de Zon haar gulde <rhyme label="a" type="f">stralen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Laat in haar schoone oogjes <rhyme label="a" type="f" corresp="#s2:l1">Daalen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Met een schitt'rend <rhyme label="b" type="m">flikkerlicht</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Neemt zy u om zig te <rhyme label="c" type="f">weeren</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">En dien roover af te <rhyme label="c" type="f" corresp="#s2:l4">keeren</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van haar Hemels <rhyme label="b" type="m" corresp="#s2:l3">aangezigt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waait het wintje wat <rhyme label="a" type="f">vrymoedig</rhyme></l>
        <l id="s3:l2" met="4" type="+-">In haar lokjes, zy neemt <rhyme label="a" type="f" corresp="#s3:l1">spoedig</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Voor dien snoeper u te <rhyme label="b" type="m">baat</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Schiet een Minnaar op haar <rhyme label="c" type="f">ligtjes</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Uyt zyn oogen <rhyme label="c" type="f" corresp="#s3:l4">minneschigtjes</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Gy zyt weer haar <rhyme label="b" type="m" corresp="#s3:l3">toeverlaat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wil zy eens, uit zoete <rhyme label="a" type="f">kuuren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Iemant van ter zy <rhyme label="a" type="f" corresp="#s4:l1">begluuren</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Zonder dat zyn oog het <rhyme label="b" type="m">merk</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Zy verschuilt zig in de <rhyme label="c" type="f">kniekjes</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Van uw net gevouwen <rhyme label="c" type="f" corresp="#s4:l4">wiekjes</rhyme>:</l>
        <l id="s4:l6" met="4" type="+-">En begluurt hem in de <rhyme label="b" type="m" corresp="#s4:l3">kerk</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wil zy weer aandachtig <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Luisterende na het <rhyme label="a" type="f" corresp="#s5:l1">leezen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Zy ontplooit uw <rhyme label="b" type="m">hallefrond</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">En van aandacht <rhyme label="c" type="f">ingenoomen</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Houdt u' zonder iets te <rhyme label="c" type="f" corresp="#s5:l4">schroomen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Voor haar lieven <rhyme label="b" type="m" corresp="#s5:l3">Rozemond</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maar wat's dit? klein <rhyme label="a" type="f">Windezwayertje</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Olyk Guitje, dartel <rhyme label="a" type="f" corresp="#s6:l1">wayertje</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Gy besmet het <rhyme label="b" type="m">Leliwit</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Gy besmet het geurig <rhyme label="c" type="f">bloosje</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Van haar toegedrukte <rhyme label="c" type="f" corresp="#s6:l4">roosje</rhyme>?</l>
        <l id="s6:l6" met="4" type="+-">Haar teer montje met uw <rhyme label="b" type="m" corresp="#s6:l3">git</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Foei onwaarde, foei <rhyme label="a" type="f">trouwlooze</rhyme>:</l>
        <l id="s7:l2" met="4" type="+-">Heeft zy daarom u <rhyme label="a" type="f" corresp="#s7:l1">gekoozen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Foei, dat partjen is te <rhyme label="b" type="m">slegt</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">'t Wayertje, van vrees <rhyme label="c" type="f">bekroopen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Doet zyn zachte vouwtjes <rhyme label="c" type="f" corresp="#s7:l4">oopen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Op myn scherp verwyt, en <rhyme label="b" type="m" corresp="#s7:l3">zegt</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Ik besmette slegts de <rhyme label="a" type="f">tipjes</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Van uw kloris zuiv're <rhyme label="a" type="f" corresp="#s8:l1">lipjes</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Met wat zwart, op dat gy <rhyme label="b" type="m">kond</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Onder 't zagte kusjes <rhyme label="c" type="f">drukken</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Onder 't Nectar-roosjes <rhyme label="c" type="f" corresp="#s8:l4">plukken</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Die weer zuiv'ren met uw <rhyme label="b" type="m" corresp="#s8:l3">mond</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wayertje myn hart gaat <rhyme label="a" type="f">open</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">'k Voel myn ziel van vreugd <rhyme label="a" type="f" corresp="#s9:l1">bekroopen</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Smet, my smet haar lipjes <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Met uw gitzwart 't aller <rhyme label="c" type="f">uuren</rhyme>:</l>
        <l id="s9:l5" met="4" type="+-">'k prys die zoete <rhyme label="c" type="f" corresp="#s9:l4">minnekuuren</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Mits het zuiv'ren sta aan <rhyme label="b" type="m" corresp="#s9:l3">my</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Aardig strafje, teder <rhyme label="a" type="f">wayertje</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Klein en dartel <rhyme label="a" type="f" corresp="#s10:l1">windezwayertje</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Blaas de minnevonkjes <rhyme label="b" type="m">aan</rhyme></l>
        <l id="s10:l4" met="4" type="+-">In haar hartje, die de <rhyme label="c" type="f">Liefde</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Misschien in haar boezem <rhyme label="c" type="f" corresp="#s10:l4">griefde</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Tot zy ligter laage <rhyme label="b" type="m" corresp="#s10:l3">staan</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">'k Sal u dan een autaer <rhyme label="a" type="f">stigten</rhyme></l>
        <l id="s11:l2" met="4" type="+-">In myn hart, en dat <rhyme label="a" type="f" corresp="#s11:l1">verligten</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Met de vlammen myner <rhyme label="b" type="m">min</rhyme>.</l>
        <l id="s11:l4" met="4" type="+-">'k Zal Poley en mirthe <rhyme label="c" type="f">rooken</rhyme>:</l>
        <l id="s11:l5" met="4" type="+-">'k Zal u Musschen hartjes <rhyme label="c" type="f" corresp="#s11:l4">kooken</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Doen daar duyven oogjes <rhyme label="b" type="m" corresp="#s11:l3">in</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Maar zo gy die lieve <rhyme label="a" type="f">vonkjes</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Niet doet vlammen, dat haar <rhyme label="a" type="f" corresp="#s12:l1">lonkjes</rhyme></l>
        <l id="s12:l3" met="4" type="+-">My uit Liefde schieten <rhyme label="b" type="m">toe</rhyme>;</l>
        <l id="s12:l4" met="4" type="+-">Zal ik u in stukjes <rhyme label="c" type="f">breeken</rhyme></l>
        <l id="s12:l5" met="4" type="+-">En u in het vuurtje <rhyme label="c" type="f" corresp="#s12:l4">steeken</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Dat ik in myn boezem <rhyme label="b" type="m" corresp="#s12:l3">v√¥e</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
