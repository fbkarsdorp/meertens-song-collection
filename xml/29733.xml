  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Loeft mijn siele den <rhyme label="a" type="f">Heere</rhyme>,,</l>
        <l id="s1:l2" met="3" type="-+">Wat in my is sulcx <rhyme label="b" type="m">doet</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Geeft doch prijs ende <rhyme label="a" type="f" corresp="#s1:l1">eere</rhyme>,,</l>
        <l id="s1:l4" met="3" type="-+">Den Naem heylich en <rhyme label="b" type="m" corresp="#s1:l2">soet</rhyme>,,</l>
        <l id="s1:l5" met="3" type="-+">Loeft hem vergheet doch <rhyme label="c" type="m">niet</rhyme>,,</l>
        <l id="s1:l6" met="3" type="-+">Sijn groote <rhyme label="d" type="f">wonderdaden</rhyme>,,</l>
        <l id="s1:l7" met="3" type="-+">Het goet dat u <rhyme label="c" type="m" corresp="#s1:l5">gheschiet</rhyme>,,</l>
        <l id="s1:l8" met="3" type="-+">En t'gheestelick <rhyme label="d" type="f" corresp="#s1:l6">versaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die u allen u <rhyme label="a" type="f">sonden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Daer ghy in hebt <rhyme label="b" type="m">gheweest</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ghebreken ende <rhyme label="a" type="f" corresp="#s2:l1">wonden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ghenadichlijck <rhyme label="b" type="m" corresp="#s2:l2">gheneest</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">T'leven verlost, <rhyme label="c" type="m">verschoont</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van het eewich <rhyme label="d" type="f">verderven</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En met ghenade <rhyme label="c" type="m" corresp="#s2:l5">croont</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Als een kint zijnder <rhyme label="d" type="f" corresp="#s2:l6">Erven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Heer is seer <rhyme label="a" type="f">ghenadich</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Langmoedich, soet daer <rhyme label="b" type="m">by</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Sijn goetheyt duert <rhyme label="a" type="f" corresp="#s3:l1">ghestadich</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En maeckt de siele <rhyme label="b" type="m" corresp="#s3:l2">bly</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Na ons verdiende <rhyme label="c" type="m">quaet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Handelt hy met ons <rhyme label="d" type="f">selden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ende na ons <rhyme label="c" type="m" corresp="#s3:l5">misdaet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Sietmen hem niet <rhyme label="d" type="f" corresp="#s3:l6">verghelden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Want soo hoogh' als is <rhyme label="a" type="f">staende</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Den Hemel van <rhyme label="b" type="m">Aerdtrijck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Is zijn ghenade <rhyme label="a" type="f" corresp="#s4:l1">gaende</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Over ons all' <rhyme label="b" type="m" corresp="#s4:l2">ghelijck</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Soo wijdt den Opganck <rhyme label="c" type="m">streckt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Der Sonnen hier <rhyme label="d" type="f">beneden</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Tot daer sy onder <rhyme label="c" type="m" corresp="#s4:l5">treckt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Doet hy wech <rhyme label="d" type="f" corresp="#s4:l6">d'overtreden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghelijck hem hier een <rhyme label="a" type="f">Vader</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Zijn kinderen <rhyme label="b" type="m">ontfermt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gheschiet ons <rhyme label="a" type="f" corresp="#s5:l1">allegader</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die hem vreest, wort <rhyme label="b" type="m" corresp="#s5:l2">beschermt</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Siet zijnder liefden <rhyme label="c" type="m">aert</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">In Christo synen <rhyme label="d" type="f">Sone</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die hy niet heeft <rhyme label="c" type="m" corresp="#s5:l5">ghespaert</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Maer sondt uut synen <rhyme label="d" type="f" corresp="#s5:l6">Throne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy is <rhyme label="a" type="f">overghegheven</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Voor ons hier inder <rhyme label="b" type="m">doot</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Op dat wy souden <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Door zijn verdiensten <rhyme label="b" type="m" corresp="#s6:l2">groot</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Siet deser liefden <rhyme label="c" type="m">brant</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Laet sulcks dijn hert <rhyme label="d" type="f">ontfoncken</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Want met dit hooghe <rhyme label="c" type="m" corresp="#s6:l5">pant</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ist dy alles <rhyme label="d" type="f" corresp="#s6:l6">gheschoncken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Is hy voor ons <rhyme label="a" type="f">ghestorven</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Doe elck noch sondich <rhyme label="b" type="m">was</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Maer wort van ons <rhyme label="a" type="f" corresp="#s7:l1">verworven</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Door hem nu op dit <rhyme label="b" type="m" corresp="#s7:l2">pas</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Nu wy door t'dierbaer <rhyme label="c" type="m">bloet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Zijns Soons rechtvaerdich <rhyme label="d" type="f">werden</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En van toorne <rhyme label="c" type="m" corresp="#s7:l5">behoet</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Die in gheloof <rhyme label="d" type="f" corresp="#s7:l6">volherden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Soo wy Godt der <rhyme label="a" type="f">Heyrscharen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Door zijn doot versoent <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Doe wy vyanden <rhyme label="a" type="f" corresp="#s8:l1">waren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Voor zijn Godtlick <rhyme label="b" type="m" corresp="#s8:l2">aenschijn</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Hoe veel meer sullen <rhyme label="c" type="m">wy</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Door t'leven salich <rhyme label="d" type="f">wesen</rhyme>?</l>
        <l id="s8:l7" met="3" type="-+">Want die ons maecte <rhyme label="c" type="m" corresp="#s8:l5">vry</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Die leeft, end' is <rhyme label="d" type="f" corresp="#s8:l6">verresen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dit nu zijnde <rhyme label="a" type="f">ghedachtich</rhyme></l>
        <l id="s9:l2" met="3" type="-+">In dit u <rhyme label="b" type="m">Avontmael</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Dancken wy u <rhyme label="a" type="f" corresp="#s9:l1">eendrachtich</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Christe int' <rhyme label="b" type="m" corresp="#s9:l2">generael</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Mont ende hert u <rhyme label="c" type="m">prijst</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Den Gheest wilt ons <rhyme label="d" type="f">inlijven</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Daer ghy de siel med' <rhyme label="c" type="m" corresp="#s9:l5">spijst</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Ende wilt in ons <rhyme label="d" type="f" corresp="#s9:l6">blijven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
