  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Des vaders licht geheel <rhyme label="a" type="m">gelijck</rhyme></l>
        <l id="s1:l2" met="4" type="-+">ghij licht des lichts ende dach <rhyme label="a" type="m" corresp="#s1:l1">rijck</rhyme></l>
        <l id="s1:l3" met="4" type="-+">den nacht wij breken met <rhyme label="b" type="m">gesang</rhyme></l>
        <l id="s1:l4" met="4" type="-+">weest bij tot ons gebets <rhyme label="b" type="m" corresp="#s1:l3">aenuang</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Neempt wech des herten duijster <rhyme label="a" type="m">boes</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Viants gesweren godde <rhyme label="a" type="m" corresp="#s2:l1">loes</rhyme></l>
        <l id="s2:l3" met="4" type="-+">verdrijft alle traecheijts <rhyme label="b" type="m">gewalt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">op dat die ons niet <rhyme label="b" type="m" corresp="#s2:l3">ouerualt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dus christe vergeeft te <rhyme label="a" type="m">zaen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">ons die gelouen uwen <rhyme label="a" type="m" corresp="#s3:l1">naem</rhyme></l>
        <l id="s3:l3" met="4" type="-+">maect dat den bidder helpen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s3:l4" met="4" type="-+">den loff die hij met sange <rhyme label="b" type="m" corresp="#s3:l3">doet</rhyme></l>
      </lg>
    </lg>
  </text>
