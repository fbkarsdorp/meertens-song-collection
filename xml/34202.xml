    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Rosbol wou laatst buizen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Petrus Suetmans, Eustachius de Meyer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Soo haest als Phebus <rhyme label="a" type="m">glans</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Lest van de <rhyme label="b" type="f">aerde</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verdreef den duysteren <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hiel ick voor myn vee de <rhyme label="c" type="m" corresp="#s1:l3">wacht</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">'kvlocht voor my lief een <rhyme label="a" type="m" corresp="#s1:l1">krans</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Niet groot van <rhyme label="b" type="f" corresp="#s1:l2">weerde</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Maer van gheblomt, en <rhyme label="d" type="m">kruyt</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">d'Welck daer uyt der aerde <rhyme label="d" type="m" corresp="#s1:l7">spruyt</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Ververscht van den <rhyme label="e" type="m">douw</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Roos, <rhyme label="e" type="m" corresp="#s1:l9">karsouw</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Myrtus <rhyme label="f" type="m">kruyt</rhyme>,</l>
        <l id="s1:l12" met="2" type="+-">Als een <rhyme label="f" type="m" corresp="#s1:l11">Bruyt</rhyme>,</l>
        <l id="s1:l13" met="2" type="-+">Uyt suyver <rhyme label="g" type="m">min</rhyme>,</l>
        <l id="s1:l14" met="4" type="+-">Wou ick kroone myn <rhyme label="g" type="m" corresp="#s1:l13">vrindin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick sagh myn <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Het Wout <rhyme label="b" type="f">ghenaecken</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Verselt met haere <rhyme label="c" type="m">vee</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die haer volghden, twee, en <rhyme label="c" type="m" corresp="#s2:l3">twee</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ick docht, Ã´ schoon <rhyme label="a" type="m" corresp="#s2:l1">vrindin</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Hoe sal ick't <rhyme label="b" type="f" corresp="#s2:l2">maecken</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Om u naer desen <rhyme label="d" type="m">groet</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Te ont-decken myn <rhyme label="d" type="m" corresp="#s2:l7">ghemoet</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Ick boodt haer de <rhyme label="e" type="m">kroon</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">En tot <rhyme label="e" type="m" corresp="#s2:l9">loon</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Sprack sy <rhyme label="f" type="m">vrint</rhyme>,</l>
        <l id="s2:l12" met="2" type="+-">Soo ghy <rhyme label="f" type="m" corresp="#s2:l11">mint</rhyme>,</l>
        <l id="s2:l13" met="2" type="-+">Ick min u <rhyme label="g" type="m">weer</rhyme>,</l>
        <l id="s2:l14" met="4" type="+-">Sitten wy een weynigh <rhyme label="g" type="m" corresp="#s2:l13">neer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O suyver soete <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">U soete <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Verbleyden myn jongh <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">d'Welck eerst was vol druck, en <rhyme label="c" type="m" corresp="#s3:l3">smert</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ghy zyt die my <rhyme label="a" type="m" corresp="#s3:l1">behaeght</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Aen-hoort myn <rhyme label="b" type="f" corresp="#s3:l2">beden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Vergont my uwe <rhyme label="d" type="m">trouw</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Soo verdwynt myn druck, en <rhyme label="d" type="m" corresp="#s3:l7">rouw</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Op dat uwen <rhyme label="e" type="m">glans</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Met een <rhyme label="e" type="m" corresp="#s3:l9">krans</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Desen <rhyme label="f" type="m">dagh</rhyme>,</l>
        <l id="s3:l12" met="2" type="+-">Wesen <rhyme label="f" type="m" corresp="#s3:l11">magh</rhyme>,</l>
        <l id="s3:l13" met="2" type="-+">Verciert myn <rhyme label="g" type="m">Bruyt</rhyme>,</l>
        <l id="s3:l14" met="4" type="+-">Ach! het ja-woord was <rhyme label="g" type="m" corresp="#s3:l13">'tbesluyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy hadden gheene <rhyme label="a" type="m">jeught</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Die ons daer <rhyme label="b" type="f">dienden</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Want alle Hofsche <rhyme label="c" type="m">pracht</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Wirt daer van ons bey <rhyme label="c" type="m" corresp="#s4:l3">veracht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Maer wel een soete <rhyme label="a" type="m" corresp="#s4:l1">vreucht</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Kost ick daer <rhyme label="b" type="f" corresp="#s4:l2">vinden</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">In't voghels soet <rhyme label="d" type="m">gheluyt</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die daer groette myn <rhyme label="d" type="m" corresp="#s4:l7">Bruyt</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Ick speelde een <rhyme label="e" type="m">liet</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">Op myn <rhyme label="e" type="m" corresp="#s4:l9">riet</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Onse <rhyme label="f" type="m">vee</rhyme>,</l>
        <l id="s4:l12" met="2" type="+-">Twee, en <rhyme label="f" type="m" corresp="#s4:l11">twee</rhyme>,</l>
        <l id="s4:l13" met="2" type="-+">Maeckten den <rhyme label="g" type="m">dans</rhyme>,</l>
        <l id="s4:l14" met="4" type="+-">d'Echte trouw die was myn <rhyme label="g" type="m" corresp="#s4:l13">kans</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
