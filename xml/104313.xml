  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">POrden al myn <rhyme label="a" type="f">kragten</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Aan haar te <rhyme label="a" type="f" corresp="#s1:l1">pachten</rhyme>:</l>
        <l id="s1:l3" met="2" type="-+">En k'lietme <rhyme label="b" type="f">glippen</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">In de <rhyme label="b" type="f" corresp="#s1:l3">knippen</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Van'er volle <rhyme label="b" type="f" corresp="#s1:l3">Lippen</rhyme>.</l>
        <l id="s1:l6" met="2" type="-+">Soo haast de <rhyme label="b" type="f" corresp="#s1:l3">tippen</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">Mijn <rhyme label="b" type="f" corresp="#s1:l3">lippen</rhyme>,</l>
        <l id="s1:l8" met="1" type="-+">Quam, <rhyme label="b" type="f" corresp="#s1:l3">kippen</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Schoot up'ter Montje een amb're <rhyme label="c" type="m">geur</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">t'Drong deur mijn gantse Geestje <rhyme label="c" type="m" corresp="#s1:l9">deur</rhyme>.</l>
      </lg>
    </lg>
  </text>
