  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh Liefste weest <rhyme label="a" type="m">begroet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Door u begaeftheyt <rhyme label="b" type="f">schoone</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die my dees torment <rhyme label="a" type="m" corresp="#s1:l1">aendoet</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Door dat <rhyme label="c" type="m">Beelt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dat soo <rhyme label="c" type="m" corresp="#s1:l4">speelt</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">In mijnen <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Om u soetste <rhyme label="d" type="m" corresp="#s1:l6">Herderin</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Mocht ick eens troost <rhyme label="e" type="m">ontfaen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Van u mijn <rhyme label="f" type="f">welbeminde</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Al mijn quelling was <rhyme label="e" type="m" corresp="#s1:l8">gedaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ionghman die u <rhyme label="a" type="m">gelooft</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met al soete <rhyme label="b" type="f">woorden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Is van zyn verstant <rhyme label="a" type="m" corresp="#s2:l1">berooft</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Al u <rhyme label="c" type="m">gevrey</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">En u <rhyme label="c" type="m" corresp="#s2:l4">geschrey</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Iongmans <rhyme label="d" type="m">loos</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Acht ik voor <rhyme label="d" type="m" corresp="#s2:l6">bedriegery</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Als gy my had in ’t <rhyme label="e" type="m">net</rhyme></l>
        <l id="s2:l9" met="3" type="-+">soud gy mijn laten <rhyme label="f" type="f">zitten</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Ick was van mijn Eer <rhyme label="e" type="m" corresp="#s2:l8">ontset</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Och schoone <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Staeckt alde wreede <rhyme label="b" type="f">woorden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En stil u tens door de <rhyme label="a" type="m" corresp="#s3:l1">min</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Want gy <rhyme label="c" type="m">zijt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">daer ick na <rhyme label="c" type="m" corresp="#s3:l4">tracht</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dag ende <rhyme label="d" type="m">nacht</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Staet gy vast in mijnen <rhyme label="d" type="m" corresp="#s3:l6">sin</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Mogt ick eens troost <rhyme label="e" type="m">ontfaen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Van u myn <rhyme label="f" type="f">welbeminde</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Al mijn quelling was <rhyme label="e" type="m" corresp="#s3:l8">gedaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Iongman mijn hert <rhyme label="a" type="m">beswijkt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Sou ik het mogen <rhyme label="b" type="f">geloven</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Al de woorden die gy <rhyme label="a" type="m" corresp="#s4:l1">spreekt</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Want gy <rhyme label="c" type="m">zyt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Die den <rhyme label="c" type="m" corresp="#s4:l4">brant</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Door de <rhyme label="d" type="m">Min</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Groeyen doet aen alle <rhyme label="d" type="m" corresp="#s4:l6">kant</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Daerom schencje ick u mijn <rhyme label="e" type="m">Trouw</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Schoon Lief in bey myn <rhyme label="f" type="f">armen</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Op geloften van ons <rhyme label="e" type="m" corresp="#s4:l8">Trouw</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Och alderschoonste <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ick neem u in bey mijn <rhyme label="b" type="f">armen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Op beloften van ons <rhyme label="a" type="m" corresp="#s5:l1">Trouw</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Nu is de tijdt <rhyme label="c" type="m">vervult</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">En de <rhyme label="c" type="m" corresp="#s5:l4">nut</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">En de <rhyme label="d" type="m">fleur</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En van droefheyt met een <rhyme label="d" type="m" corresp="#s5:l6">sier</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Nu is de uur en <rhyme label="e" type="m">dag</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">En ook den tijdt <rhyme label="f" type="f">gekomen</rhyme></l>
        <l id="s5:l10" met="4" type="-+">Dat ick u lief omhelsen <rhyme label="e" type="m" corresp="#s5:l8">magh</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sy dee doen haer <rhyme label="a" type="m">beklag</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met twee beweende <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Doe sy voor sijn voeten <rhyme label="a" type="m" corresp="#s6:l1">lag</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">En sy <rhyme label="c" type="m">sey</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">
          <rhyme label="c" type="m" corresp="#s6:l4">Meenigmael</rhyme>
        </l>
        <l id="s6:l6" met="2" type="-+">met een <rhyme label="d" type="m">tael</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Iongman kiest mijn voor u <rhyme label="d" type="m" corresp="#s6:l6">Slaef</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Dochter had gy daer <rhyme label="e" type="m">opgelet</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Want al mijn schoon <rhyme label="f" type="f">beloften</rhyme></l>
        <l id="s6:l10" met="4" type="-+">Sijn vergaen op het Pluymen <rhyme label="e" type="m" corresp="#s6:l8">Bedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nu hebt ghy uwen <rhyme label="a" type="m">wil</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Verkregen door schoone <rhyme label="b" type="f">woorden</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Maer ick bid u zwygt dog <rhyme label="a" type="m" corresp="#s7:l1">stil</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Wat hebt gy <rhyme label="c" type="m">dan</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Als gy mijn <rhyme label="c" type="m" corresp="#s7:l4">Eer</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Hebt <rhyme label="d" type="m">gebrocht</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">In ’t gemeen voor alle <rhyme label="d" type="m" corresp="#s7:l6">Man</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Daer zijnder meer als <rhyme label="e" type="m">gy</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Die soo worden <rhyme label="f" type="f">bedroogen</rhyme>,</l>
        <l id="s7:l10" met="4" type="-+">Soete Lief voegt u daer <rhyme label="e" type="m" corresp="#s7:l8">by</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O gy bedrieger <rhyme label="a" type="m">vals</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gy en hebt soo niet <rhyme label="b" type="f">gesprooken</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Doe gy my viel om den <rhyme label="a" type="m" corresp="#s8:l1">hals</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Maer het was <rhyme label="c" type="m">altijdt</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Lief ick <rhyme label="c" type="m" corresp="#s8:l4">bezwijck</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Door de <rhyme label="d" type="m">min</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">So ick geenen troost en <rhyme label="d" type="m" corresp="#s8:l6">win</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Maer het is te laet <rhyme label="e" type="m">beklaegt</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Gy hebt mijn eer en <rhyme label="f" type="f">schanden</rhyme>,</l>
        <l id="s8:l10" met="4" type="-+">Onder uwen voet <rhyme label="e" type="m" corresp="#s8:l8">gebragt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Staet op en krijt niet <rhyme label="a" type="m">meer</rhyme>:</l>
        <l id="s9:l2" met="3" type="-+">Ick sal u weer <rhyme label="b" type="f">ontfermen</rhyme>;</l>
        <l id="s9:l3" met="4" type="+-">En weer setten in u <rhyme label="a" type="m" corresp="#s9:l1">eer</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Daer is mijn <rhyme label="c" type="m">hant</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Tot <rhyme label="c" type="m" corresp="#s9:l4">onderpant</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Treckt daer <rhyme label="d" type="m">af</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Eenen Rinck van <rhyme label="d" type="m" corresp="#s9:l6">Diamant</rhyme>;</l>
        <l id="s9:l8" met="3" type="+-">Verblijd u Ionge <rhyme label="e" type="m">Maegt</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Van dees geluckige <rhyme label="f" type="f">uren</rhyme>,</l>
        <l id="s9:l10" met="4" type="-+">Dat wy samen zijn <rhyme label="e" type="m" corresp="#s9:l8">gepaert</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Oorlof dogters <rhyme label="a" type="m">gelijck</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Wilt op u eer wel <rhyme label="b" type="f">letten</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">En gy Iongmans wie gy <rhyme label="a" type="m" corresp="#s10:l1">zijt</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Vry <rhyme label="c" type="m">versint</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Eer gy <rhyme label="c" type="m" corresp="#s10:l4">begint</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">En neemt geen <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Gaet daer gy het open <rhyme label="d" type="m" corresp="#s10:l6">vint</rhyme>:</l>
        <l id="s10:l8" met="3" type="+-">Want als het is te <rhyme label="e" type="m">laet</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Gy zijt ’er aen <rhyme label="f" type="f">verbonden</rhyme>,</l>
        <l id="s10:l10" met="4" type="-+">’t Zy van hoog of lage <rhyme label="e" type="m" corresp="#s10:l8">staet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
