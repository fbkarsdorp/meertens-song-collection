<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9174</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach schone Cariclea</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Prachtighe <rhyme label="a" type="m">Velt-Godin</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Die 'tbinnenste murw' mijns harte treuren <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sal ick gheen <rhyme label="a" type="m" corresp="#s1:l1">wedermin</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Schoonste ontsaen daer ick u met <rhyme label="b" type="m" corresp="#s1:l2">ootmoet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Aenbie uyt suyv're liefd' mijn <rhyme label="c" type="m">trou</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Ontfanght my doch roem-waerde <rhyme label="c" type="m" corresp="#s1:l5">Vrou</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Eens doch verhoort <rhyme label="a" type="m">'tgheclach</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Van een die met ghesmeeck versoeckt <rhyme label="b" type="m">weermin</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Noyt had ick blyden <rhyme label="a" type="m" corresp="#s2:l1">dach</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Van doen ick eerst te lieven nam int <rhyme label="b" type="m" corresp="#s2:l2">sin</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">V die mijn lieven loont met <rhyme label="c" type="m">quaest</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En niet weerliefd' maer haetlijck <rhyme label="c" type="m" corresp="#s2:l5">haet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laest doen ick was <rhyme label="a" type="m">verselt</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Neven u zy met al mijn Lam'ren <rhyme label="b" type="m">teer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Int Calverijcke <rhyme label="a" type="m" corresp="#s3:l1">velt</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Wert mijn hart verheucht om dat my de <rhyme label="b" type="m" corresp="#s3:l2">eer</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Gheschiede dat ick by u <rhyme label="c" type="m">was</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Gheseeten in het malsis <rhyme label="c" type="m" corresp="#s3:l5">gras</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sult ghy u staegh dus <rhyme label="a" type="m">wreet</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Toonen aen my die u van harten <rhyme label="b" type="m">mint</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Voorwaer soo is mijn <rhyme label="a" type="m" corresp="#s4:l1">leet</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Dat ick tot een soo wreede was <rhyme label="b" type="m" corresp="#s4:l2">ghesint</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En dat ick lief die gheen liefd' <rhyme label="c" type="m">toont</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Maer mijn lieven met quaetheyt <rhyme label="c" type="m" corresp="#s4:l5">loont</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
