  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom vrienden in het <rhyme label="a" type="f">ronde</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En luisterd na dit <rhyme label="b" type="m">lied</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik zal een Klucht <rhyme label="a" type="f" corresp="#s1:l1">verkonden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wat onlangs is <rhyme label="b" type="m" corresp="#s1:l2">geschied</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Hoedat twee Meisjes met een <rhyme label="c" type="m">list</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Hadden in één Flesch <rhyme label="c" type="m" corresp="#s1:l5">gepist</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">O! Jannetje <rhyme label="d" type="f">Styffel</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wat hadden die twee een <rhyme label="e" type="m">pret</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haar naam was beide, <rhyme label="a" type="f">Bethje</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En waren by de <rhyme label="b" type="m">zaak</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Alle Avond' een ander <rhyme label="a" type="f" corresp="#s2:l1">Pretje</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat was hun grootst <rhyme label="b" type="m" corresp="#s2:l2">vermaak</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Zy hadden een Flesch Ponsch <rhyme label="c" type="m">gekogt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En daar twee Vreijers op <rhyme label="c" type="m" corresp="#s2:l5">verzogt</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">O! Jannetje <rhyme label="d" type="f">Styffel</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wat hadden die twee een <rhyme label="e" type="m">pret</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy zaten met haar <rhyme label="a" type="f">vieren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En dronken een goede <rhyme label="b" type="m">graan</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Vol lusten en <rhyme label="a" type="f" corresp="#s3:l1">Plaizieren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zy hoorden 't Acht uren <rhyme label="b" type="m" corresp="#s3:l2">staan</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Haar Vreijers moesten na 't <rhyme label="c" type="m">Appél</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En zouden weer komen, verstaat 't <rhyme label="c" type="m" corresp="#s3:l5">wel</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">O! Jannetje <rhyme label="d" type="f">Styffel</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wat hadden die twee een <rhyme label="e" type="m">pret</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Meisjes wilden niet <rhyme label="a" type="f">wachten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Zy leegde gaauw de <rhyme label="b" type="m">Flesch</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Het was kwartier na <rhyme label="a" type="f" corresp="#s4:l1">achten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Toen dronken zy de <rhyme label="b" type="m" corresp="#s4:l2">rest</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Zy hadden het zamen wel <rhyme label="c" type="m">beslist</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">En weer wat in de Flesch <rhyme label="c" type="m" corresp="#s4:l5">gepist</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">O! Jannetje <rhyme label="d" type="f">Styffel</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Wat hadden die twee een <rhyme label="e" type="m">pret</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Haar Vreijers kwamen <rhyme label="a" type="f">weder</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En hadden geen kwaad in 't <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zy zaten een weinig <rhyme label="a" type="f" corresp="#s5:l1">neder</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En zeide, kom, schenk eens <rhyme label="b" type="m" corresp="#s5:l2">in</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">De Meisjes ligt hen by de <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Avous, Santée! den <rhyme label="c" type="m" corresp="#s5:l5">Sergeant</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">O! Jannetje <rhyme label="d" type="f">Styffel</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Wat hadden die twee een <rhyme label="e" type="m">pret</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Het Glas wierd vol <rhyme label="a" type="f">geschonken</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Een Duitscher nam het <rhyme label="b" type="m">aan</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Hy heeft daar uit <rhyme label="a" type="f" corresp="#s6:l1">gedronken</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Begon met zyn Kling te <rhyme label="b" type="m" corresp="#s6:l2">slaan</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Sprak tot zyn Kammeraad, heel <rhyme label="c" type="m">vies</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Zy hebben in de Flesch <rhyme label="c" type="m" corresp="#s6:l5">gepiest</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Hetgeen de Meisjes nu wel <rhyme label="d" type="f">spyt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Zy zyn haar Vreijers <rhyme label="e" type="m">kwyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dus Meisjes maakt gy <rhyme label="a" type="f">Pretjes</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Onthoudt vooral deez <rhyme label="b" type="m">les</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Doet niet als deze <rhyme label="a" type="f" corresp="#s7:l1">Bethjes</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En pist toch in geen <rhyme label="b" type="m" corresp="#s7:l2">Flesch</rhyme>;</l>
        <l id="s7:l5" met="4" type="-+">Want als ge uw Vreijers zoo <rhyme label="c" type="m">tracteerd</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Dan raakt op 't laast de zaak <rhyme label="c" type="m" corresp="#s7:l5">verkeerd</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">O! Jannetje <rhyme label="d" type="f">Styffel</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wagt uw toch voor die <rhyme label="e" type="m">pret</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof gy <rhyme label="a" type="f">Jonggezellen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Onthoud vooral deez' <rhyme label="b" type="m">raad</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En past toch op u <rhyme label="a" type="f" corresp="#s8:l1">tellen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Opdat het u zoo niet <rhyme label="b" type="m" corresp="#s8:l2">gaat</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Zoodra gy met een Meid <rhyme label="c" type="m">verkeerd</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Die uw in het geheim <rhyme label="c" type="m" corresp="#s8:l5">tracteerd</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Laat haar dan eerst <rhyme label="d" type="f">proeven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Ik heb er door <rhyme label="e" type="m">geleerd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
