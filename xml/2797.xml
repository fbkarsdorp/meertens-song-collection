    <melody>Naar oostland wil ik varen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ick wil een hulsie <rhyme label="a" type="f">timmeren</rhyme></l>
        <l id="s1:l2" met="6" type="-+">van pieter seelye van pieter <rhyme label="b" type="m">seelye</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Daer op soe wil ick <rhyme label="c" type="f">sette</rhyme></l>
        <l id="s1:l4" met="6" type="-+">Drie roese leelye drie roese <rhyme label="b" type="m" corresp="#s1:l2">leelye</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als Ick myn huis getimmert <rhyme label="a" type="f">heb</rhyme></l>
        <l id="s2:l2" met="6" type="-+">soe helpter myn godt in <rhyme label="b" type="m">soe</rhyme></l>
        <l id="s2:l3" met="3" type="-+">met een soe moeyen <rhyme label="c" type="f">meyse</rhyme></l>
        <l id="s2:l4" met="6" type="-+">Als Ick een Jongelinck <rhyme label="b" type="m" corresp="#s2:l2">ben</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick wil met deese <rhyme label="a" type="f">Jongelinck</rhyme></l>
        <l id="s3:l2" met="6" type="-+">gaen trecken uit het lant / <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">een packie wil Ick <rhyme label="c" type="f">draegen</rhyme></l>
        <l id="s3:l4" met="6" type="-+">en vaeren nae engelant / <rhyme label="b" type="m" corresp="#s3:l2">en</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doen sy In engelant <rhyme label="a" type="f">qusemen</rhyme></l>
        <l id="s4:l2" met="6" type="-+">en daer ginck eenen dans / <rhyme label="b" type="m">en</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Daer quam dat goelicke <rhyme label="c" type="f">vrijertge</rhyme></l>
        <l id="s4:l4" met="6" type="-+">tq(u)am dansen aen myn <rhyme label="b" type="m" corresp="#s4:l2">hant</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">myn vaeder en myn <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s5:l2" met="6" type="-+">Die stonde daer beyegaer bey <rhyme label="b" type="m">die</rhyme></l>
        <l id="s5:l3" met="3" type="-+">sij vraechde soe meenich <rhyme label="c" type="f">weruee</rhyme></l>
        <l id="s5:l4" met="6" type="-+">Wat Jongelinck mach dat sijn <rhyme label="b" type="m" corresp="#s5:l2">wat</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Och moeder seyde Ick <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s6:l2" met="6" type="-+">Dat Is de liefste myn <rhyme label="b" type="m">dat</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Hy kan die meisies <rhyme label="c" type="f">weincken</rhyme></l>
        <l id="s6:l4" met="6" type="-+">Al Inder maeneschyn <rhyme label="b" type="m" corresp="#s6:l2">al</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">ken hy die meisies <rhyme label="a" type="f">weincken</rhyme></l>
        <l id="s7:l2" met="6" type="-+">al Inder maeneschyn <rhyme label="b" type="m">al</rhyme></l>
        <l id="s7:l3" met="3" type="-+">soe laet hy u niet <rhyme label="c" type="f">bedriegen</rhyme></l>
        <l id="s7:l4" met="6" type="-+">myn goelycke dochterkyn / <rhyme label="b" type="m" corresp="#s7:l2">myn</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Als hy myn dan bedroegen <rhyme label="a" type="f">hadt</rhyme></l>
        <l id="s8:l2" met="6" type="-+">gy prinse wadt hadt Ick dan / <rhyme label="b" type="m">gy</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Soe most Ick de weeuer <rhyme label="c" type="f">trouwe</rhyme></l>
        <l id="s8:l4" met="6" type="-+">en houde myn voor myn man / <rhyme label="b" type="m" corresp="#s8:l2">en</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
