    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">All mein <rhyme label="a" type="m">gedenck</rhyme> /</l>
        <l id="s1:l2" met="2" type="-+">ker ich vnnd <rhyme label="a" type="m" corresp="#s1:l1">wendt</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">nach einer zartt is s  <rhyme label="b" type="m">erlich</rhyme> /</l>
        <l id="s1:l4" met="2" type="-+">wan ich sei <rhyme label="c" type="m">angesehenn</rhyme></l>
        <l id="s1:l5" met="2" type="-+">so erfrewett sei <rhyme label="c" type="m" corresp="#s1:l4">mich</rhyme> /</l>
        <l id="s1:l6" met="2" type="-+">jn stettig <rhyme label="d" type="m">boger</rhyme> /</l>
        <l id="s1:l7" met="2" type="-+">sie liebett mir <rhyme label="d" type="m" corresp="#s1:l6">sehr</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Jch s chtt vnnd <rhyme label="a" type="m">tracht</rhyme> /</l>
        <l id="s2:l2" met="2" type="-+">bei tag vnnd <rhyme label="a" type="m" corresp="#s2:l1">nachtt</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">das ich nitt stetz bei jr mag <rhyme label="b" type="m">sein</rhyme> /</l>
        <l id="s2:l4" met="2" type="-+">Sei ligtt mir jn dem <rhyme label="c" type="m">Syn</rhyme> /</l>
        <l id="s2:l5" met="2" type="-+">stetz wor ich <rhyme label="c" type="m" corresp="#s2:l4">bin</rhyme> /</l>
        <l id="s2:l6" met="2" type="-+">noch ist bei <rhyme label="d" type="m">jr</rhyme> /</l>
        <l id="s2:l7" met="2" type="-+">all meins herzen <rhyme label="d" type="m" corresp="#s2:l6">boger</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">O wee vnnd <rhyme label="a" type="m">wach</rhyme> /</l>
        <l id="s3:l2" met="2" type="-+">der schwarer <rhyme label="a" type="m" corresp="#s3:l1">clagh</rhyme> /</l>
        <l id="s3:l3" met="4" type="-+">daß ich moß meidenn ihr m ndlein <rhyme label="b" type="m">roth</rhyme> /</l>
        <l id="s3:l4" met="2" type="-+">n  schwigh / leidt vnnd <rhyme label="c" type="m">meidt</rhyme> /</l>
        <l id="s3:l5" met="2" type="-+">verbeidtt der <rhyme label="c" type="m" corresp="#s3:l4">zeitt</rhyme> /</l>
        <l id="s3:l6" met="2" type="-+">jn stattiger h <rhyme label="d" type="m">itt</rhyme> /</l>
        <l id="s3:l7" met="2" type="-+">mein will ist g <rhyme label="d" type="m" corresp="#s3:l6">itt</rhyme>.</l>
      </lg>
