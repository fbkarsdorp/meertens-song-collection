<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100183</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1760</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>In de min vind ik geneugten</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GElykerwys een <rhyme label="a" type="m">Hazenwind</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Door Bosschen en door <rhyme label="b" type="f">Landen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Zoo lange rent tot dat hy <rhyme label="a" type="m" corresp="#s1:l1">vind</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Een prooje voor zyn <rhyme label="b" type="f" corresp="#s1:l2">tanden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Aldus zoo vliegt myn wak're <rhyme label="a" type="m">geest</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ook staag na myn <rhyme label="b" type="f">beminde</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Een roove daar myn Ziele <rhyme label="a" type="m" corresp="#s2:l1">meest</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Haar voedzel in kan <rhyme label="b" type="f" corresp="#s2:l2">vinden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maar schoon ik op de <rhyme label="a" type="m">Liefdejagt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">My spoedig ga <rhyme label="b" type="f">begeven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En dat myn ziel niet anders <rhyme label="a" type="m" corresp="#s3:l1">tragt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dan myn Lief, myn <rhyme label="b" type="f" corresp="#s3:l2">leeven</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zoo zien, ik dat ik na de <rhyme label="a" type="m">loop</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van myn verdrietig <rhyme label="b" type="f">jagen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">My vinde met een yd'le <rhyme label="a" type="m" corresp="#s4:l1">hoop</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In myn gemoed <rhyme label="b" type="f" corresp="#s4:l2">geslagen</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ja in de plaats dat 't vlugtig <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Is in het Lyf <rhyme label="b" type="f">geslooten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Zoo voel ik, dat myn eigen <rhyme label="a" type="m" corresp="#s5:l1">hert</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Is met een pyl <rhyme label="b" type="f" corresp="#s5:l2">doorschooten</rhyme>:</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">En eindelyk naar al 't <rhyme label="a" type="m">gesloof</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van Rennen en en van <rhyme label="b" type="f">Jagen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Zoo word ik zelfs als eene <rhyme label="a" type="m" corresp="#s6:l1">roof</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Na 't Offervuur <rhyme label="b" type="f" corresp="#s6:l2">gedragen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
