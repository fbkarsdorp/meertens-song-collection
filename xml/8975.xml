<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8975</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Wir glauben all an einen Gott</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">VVY ghelooven all' aen eenen <rhyme label="a" type="m">GOdt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Schepper Hemels en der <rhyme label="b" type="f">Eerden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die hem ten Vader geven <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat wy syne Kinder <rhyme label="b" type="f" corresp="#s1:l2">weerden</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Hy wil ons altijdt <rhyme label="d" type="f">gheneeren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Lijff en Ziel oock wel <rhyme label="e" type="f">bewaren</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Allen Onval wil hy <rhyme label="d" type="f" corresp="#s1:l5">weeren</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Gheen Leedt sal ons <rhyme label="e" type="f" corresp="#s1:l6">wedervaren</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Want hy sorghet voor <rhyme label="f" type="m">ons</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Hoedet ende <rhyme label="g" type="m">wacht</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Het staet al in syner <rhyme label="g" type="m" corresp="#s1:l10">Macht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Wy gelooven ooc aen JEsum <rhyme label="a" type="m">Christ</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Synen Soon end onsen <rhyme label="b" type="f">HEEre</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die Eewich by den Vader <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">G'lijcker Godt van Macht end <rhyme label="b" type="f" corresp="#s2:l2">Eere</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Van Maria <rhyme label="d" type="f">wtvercoren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Is waerachtich Mensch <rhyme label="e" type="f">gheboren</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Door den heyligen Geest int <rhyme label="d" type="f" corresp="#s2:l5">Geloove</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Voor ons wy die waren <rhyme label="e" type="f" corresp="#s2:l6">verloren</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Aent Cruyce <rhyme label="f" type="m">ghestorven</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Ende van den <rhyme label="g" type="m">Doot</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Weder Opghestanden is door <rhyme label="g" type="m" corresp="#s2:l10">GOdt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Wy gelooven aen den heyligen <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Godt met Vader en den <rhyme label="b" type="f">Soone</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die aller bedruckter Trooster <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">End met gaven Cieret <rhyme label="b" type="f" corresp="#s3:l2">schoone</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Die heele Christenheyt op <rhyme label="d" type="f">Eerden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Houdt in eenen sinn' gaer <rhyme label="e" type="f">even</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Hier alle Sond' vergeven <rhyme label="d" type="f" corresp="#s3:l5">weerden</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Dat Vleesch sal oock weder <rhyme label="e" type="f" corresp="#s3:l6">Leven</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Nae dees <rhyme label="f" type="m">Allendicheyt</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Is <rhyme label="g" type="m">bereydt</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Ons een Leven in <rhyme label="g" type="m" corresp="#s3:l10">Eewicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
