        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Paden,</l>
        <l id="s1:l2">Bladen,</l>
        <l id="s1:l3">Groen en digt,</l>
        <l id="s1:l4">Weeren,</l>
        <l id="s1:l5">Keeren</l>
        <l id="s1:l6">'t Zonnelicht:</l>
        <l id="s1:l7">Zoeter</l>
        <l id="s1:l8">Groet 'er</l>
        <l id="s1:l9">'t Bladgesuis,</l>
        <l id="s1:l10">'t Woelend,</l>
        <l id="s1:l11">Koelend</l>
        <l id="s1:l12">Beekgeruisch.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">Geurig-</l>
        <l id="s2:l2">Fleurig</l>
        <l id="s2:l3">Is het bosch,</l>
        <l id="s2:l4">Digter</l>
        <l id="s2:l5">Ligt 'er</l>
        <l id="s2:l6">'t Welig mos;</l>
        <l id="s2:l7">Achtloos,</l>
        <l id="s2:l8">Magtloos</l>
        <l id="s2:l9">Uitgespreid,</l>
        <l id="s2:l10">Vindt gij,</l>
        <l id="s2:l11">Mint gij,</l>
        <l id="s2:l12">Eenzaamheid!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">Paden,</l>
        <l id="s3:l2">Bladen,</l>
        <l id="s3:l3">Groen en breed,</l>
        <l id="s3:l4">Weert 'er,</l>
        <l id="s3:l5">Keert 'er</l>
        <l id="s3:l6">Zorg en leed!</l>
        <l id="s3:l7">Buiten</l>
        <l id="s3:l8">Sluiten</l>
        <l id="s3:l9">We alle smart:</l>
        <l id="s3:l10">Rustig-</l>
        <l id="s3:l11">Lustig:</l>
        <l id="s3:l12">Vrede in 't hart!</l>
      </lg>
    </lg>
  </text>
</song>
