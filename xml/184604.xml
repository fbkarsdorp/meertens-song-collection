<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184604</id>
    <date>
      <year-start>1755</year-start>
      <year-end>1755</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Edele kersouw</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Adieu <rhyme label="a" type="m">Maas-Sluys</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wy hoopen nu te <rhyme label="b" type="f">scheyden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Al na ’t Rijk <rhyme label="c" type="m">Asia</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Ik hoop zeer <rhyme label="a" type="m" corresp="#s1:l1">kuys</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat ons Godt zal <rhyme label="b" type="f" corresp="#s1:l2">geleyden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In zijn groote <rhyme label="c" type="m" corresp="#s1:l3">gena</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Om kort en <rhyme label="c" type="m" corresp="#s1:l3">dra</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Al in het Land te <rhyme label="d" type="f">komen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">In de Stad <rhyme label="c" type="m" corresp="#s1:l3">Batavia</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Als Boots-gezellen <rhyme label="d" type="f" corresp="#s1:l8">vroomen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+"><rhyme label="a" type="m">Sumatera</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoopen wy te <rhyme label="b" type="f">aanschouwen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met gezondheyd en <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Ambon, <rhyme label="a" type="m" corresp="#s2:l1">Banda</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Combodia zeer <rhyme label="b" type="f" corresp="#s2:l2">trouwe</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Taiwan vroeg ende <rhyme label="c" type="m" corresp="#s2:l3">spa</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Met ons <rhyme label="c" type="m" corresp="#s2:l3">gewa</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Zullen wy <rhyme label="d" type="f">compareeren</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Wy zullen op een ander <rhyme label="c" type="m" corresp="#s2:l3">Ba</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Een ander Wout <rhyme label="d" type="f" corresp="#s2:l8">passeeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Catzu met <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dien ik niet te <rhyme label="b" type="f">vergeeten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Om te bezien <rhyme label="c" type="m">voorwaar</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Conpuyn <rhyme label="a" type="m" corresp="#s3:l1">certeen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Als in een Bogt wild <rhyme label="b" type="f" corresp="#s3:l2">weeten</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Om de Noorder-zee <rhyme label="c" type="m" corresp="#s3:l3">klaar</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ik hoop <rhyme label="c" type="m" corresp="#s3:l3">eenpaar</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dit alles te <rhyme label="d" type="f">bespeure</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Hier mee gaan wy met ons <rhyme label="c" type="m" corresp="#s3:l3">Schaar</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Op Godts genade <rhyme label="d" type="f" corresp="#s3:l8">deure</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Adieu mijn <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En Kinderen te <rhyme label="b" type="f">zame</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ik schey nu met <rhyme label="c" type="m">voordagt</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Mijn Moeder <rhyme label="a" type="m" corresp="#s4:l1">trouw</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En Zwager ook by <rhyme label="b" type="f" corresp="#s4:l2">Name</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ik zeg u t’zaam goe <rhyme label="c" type="m" corresp="#s4:l3">Nagt</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">God die heeft <rhyme label="c" type="m" corresp="#s4:l3">magt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Om ons zaam te <rhyme label="d" type="f">bewaaren</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">In zijn hand is groote <rhyme label="c" type="m" corresp="#s4:l3">kragt</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Om ons weer te <rhyme label="d" type="f" corresp="#s4:l8">vergaaren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Nu Vrienden <rhyme label="a" type="m">ook</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hier op Maas-Sluys <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die hier nu woonen <rhyme label="c" type="m">vroom</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">In Liefde <rhyme label="a" type="m" corresp="#s5:l1">kloek</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Zullen wy u <rhyme label="b" type="f" corresp="#s5:l2">begeven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met vreugde zonder <rhyme label="c" type="m" corresp="#s5:l3">schroom</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">‘k Zal u een <rhyme label="c" type="m" corresp="#s5:l3">Droom</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Nu uyt het Hoofd gaan <rhyme label="d" type="f">halen</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Te bezien daar Bosch en <rhyme label="c" type="m" corresp="#s5:l3">Boom</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Goe-nagt nu <rhyme label="d" type="f" corresp="#s5:l8">al-te-malen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Mijn Moeye <rhyme label="a" type="m">vroet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die noem ik hier nog <rhyme label="b" type="f">nader</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">By na mijn naaste <rhyme label="c" type="m">Vriend</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Ik zal in ’t <rhyme label="a" type="m" corresp="#s6:l1">goed</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Van u scheyden te <rhyme label="b" type="f" corresp="#s6:l2">gader</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Na Asia zo ’t <rhyme label="c" type="m" corresp="#s6:l3">diend</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Wy zijn <rhyme label="c" type="m" corresp="#s6:l3">verkliend</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Op dees plaats door ’t <rhyme label="d" type="f">blameeren</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Moeten nu een ander <rhyme label="c" type="m" corresp="#s6:l3">Grient</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">In een nieuwt Wouwt <rhyme label="d" type="f" corresp="#s6:l8">passeeren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Mijn Peet en <rhyme label="a" type="m">Moey</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ik zeg u zonder <rhyme label="b" type="f">falen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Adieu tot dezer <rhyme label="c" type="m">stond</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Ik zal in ’t <rhyme label="a" type="m" corresp="#s7:l1">groey</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Gaan zoeken ander <rhyme label="b" type="f" corresp="#s7:l2">palen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Al met een goede <rhyme label="c" type="m" corresp="#s7:l3">vond</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Met Lip en <rhyme label="c" type="m" corresp="#s7:l3">Mond</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Mijn Nigten, Neven <rhyme label="d" type="f">schoon</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Houd nu agt, die by u <rhyme label="c" type="m" corresp="#s7:l3">zagt</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Eeuwiglijk vast zal <rhyme label="d" type="f" corresp="#s7:l8">woonen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Goe-nagt voor <rhyme label="a" type="m">eerst</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gy Visschers al te <rhyme label="b" type="f">zame</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Die hier nu woonagtig <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Minst en <rhyme label="a" type="m" corresp="#s8:l1">meest</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Adieu in Liefd’ <rhyme label="b" type="f" corresp="#s8:l2">bekwame</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Tot wederziens zeer <rhyme label="c" type="m" corresp="#s8:l3">fijn</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Gezond in <rhyme label="c" type="m" corresp="#s8:l3">schijn</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Hoopen wy aan te <rhyme label="d" type="f">bouwen</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Met kloekheyd en zonder <rhyme label="c" type="m" corresp="#s8:l3">pijn</rhyme>,</l>
        <l id="s8:l10" met="3" type="-+">Goe-nagt zonder <rhyme label="d" type="f" corresp="#s8:l8">benouwen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Gy Burgers <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Die hier nu zijn <rhyme label="b" type="f">woonagtig</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Al op Maas-Sluys <rhyme label="c" type="m">bekend</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Veel in ’t <rhyme label="a" type="m" corresp="#s9:l1">getal</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Van Visschers zijnder <rhyme label="b" type="f" corresp="#s9:l2">magtig</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">In weelden en <rhyme label="c" type="m" corresp="#s9:l3">elend</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Als met <rhyme label="c" type="m" corresp="#s9:l3">pasjent</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Wy zullen nu <rhyme label="d" type="f">waaragtig</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Van u scheyden <rhyme label="c" type="m" corresp="#s9:l3">pertinent</rhyme>,</l>
        <l id="s9:l10" met="3" type="-+">Gerust en schoon <rhyme label="d" type="f" corresp="#s9:l8">voordagtig</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Oorlof <rhyme label="a" type="m">Vriende</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En Maagden na <rhyme label="b" type="f">behooren</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Die hier nu woonen <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Ik ga ter <rhyme label="a" type="m" corresp="#s10:l1">Zee</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Al met vreugd mijn <rhyme label="b" type="f" corresp="#s10:l2">herbooren</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En dat u Godt <rhyme label="c" type="m" corresp="#s10:l3">gelee</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Wy zijn <rhyme label="c" type="m" corresp="#s10:l3">geree</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Om te gaan haastig <rhyme label="d" type="f">Zeylen</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Zullen nu al met ons <rhyme label="c" type="m" corresp="#s10:l3">twee</rhyme>,</l>
        <l id="s10:l10" met="3" type="-+">Gerustig t’zamen <rhyme label="d" type="f" corresp="#s10:l8">peylen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Prinssen, met <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Zo neem ik <rhyme label="b" type="f">audientie</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Onder ons Vrienden <rhyme label="c" type="m">hier</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Dit is het <rhyme label="a" type="m" corresp="#s11:l1">slot</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Ik scheyde van deze <rhyme label="b" type="f" corresp="#s11:l2">Provintie</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En neem in mijn armen <rhyme label="c" type="m" corresp="#s11:l3">Godt</rhyme>,</l>
        <l id="s11:l7" met="2" type="-+">Wy tot een <rhyme label="c" type="m" corresp="#s11:l3">Kot</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Hoopen weer te <rhyme label="d" type="f">vergaaren</rhyme>,</l>
        <l id="s11:l9" met="3" type="-+">Ik bid Vrienden tot een <rhyme label="c" type="m" corresp="#s11:l3">Lot</rhyme>,</l>
        <l id="s11:l10" met="3" type="-+">Dat wy weer mogen <rhyme label="d" type="f" corresp="#s11:l8">paaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
