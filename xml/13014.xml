    <tags/>
    <melody>Ik voer al over zee</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEy, tza! wie wil meê aan de <rhyme label="a" type="m">wind</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Wie wil meê op den <rhyme label="b" type="m">tril</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Wie wilder meê om een <rhyme label="c" type="f">Slentertje</rhyme>?</l>
        <l id="s1:l4" met="3" type="+-">Slentertje, Slentertje, <rhyme label="c" type="f" corresp="#s1:l3">Slentertje</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Wy zitten hier al te <rhyme label="b" type="m" corresp="#s1:l2">stil</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Kom lustig met de neus op <rhyme label="a" type="m">straat</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Voort, voort, verluchje <rhyme label="b" type="m">wat</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Wie wilder meê om een <rhyme label="c" type="f">Slentertje</rhyme>?</l>
        <l id="s2:l4" met="3" type="+-">Slentertje, Slentertje, <rhyme label="c" type="f" corresp="#s2:l3">Slentertje</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Wie wilder meê op een <rhyme label="b" type="m" corresp="#s2:l2">pad</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Stap weg. Die niet meê Slenteren <rhyme label="a" type="m">wil</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die blijft vry in der <rhyme label="b" type="m">muit</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Wie wilder meê om een <rhyme label="c" type="f">Slentertje</rhyme>?</l>
        <l id="s3:l4" met="3" type="+-">Slentertje, Slentertje, <rhyme label="c" type="f" corresp="#s3:l3">Slentertje</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">'t Moy weertje lokt ons- <rhyme label="b" type="m" corresp="#s3:l2">UIT</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
