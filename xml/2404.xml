<song>
  <meta>
    <id>2404</id>
    <date>
      <year-start>1597</year-start>
      <year-end>1597</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>O Venus der minne godin</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Venus <rhyme label="a" type="f">strale</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghy zijt die <rhyme label="a" type="f" corresp="#s1:l1">principale</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die my houdt <rhyme label="a" type="f" corresp="#s1:l1">t'eenemale</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">In droefheyts <rhyme label="a" type="f" corresp="#s1:l1">sale</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Laet my <rhyme label="a" type="f">ghebeuren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die bloeme der <rhyme label="a" type="f" corresp="#s2:l1">figueren</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ghedencke mijn bitter <rhyme label="a" type="f" corresp="#s2:l1">treuren</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Tot allen <rhyme label="a" type="f" corresp="#s2:l1">uren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Laet my <rhyme label="a" type="f">verwerven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Troostelicke <rhyme label="a" type="f" corresp="#s3:l1">conserven</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Moet ickse langer <rhyme label="a" type="f" corresp="#s3:l1">derven</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ick salt <rhyme label="a" type="f" corresp="#s3:l1">besterven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Princers <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wilt my die liefste <rhyme label="a" type="f" corresp="#s4:l1">geven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Sy is mijn vreucht en <rhyme label="a" type="f" corresp="#s4:l1">leven</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Int herte <rhyme label="a" type="f" corresp="#s4:l1">gheschreven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
