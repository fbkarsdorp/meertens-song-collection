<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25103</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag>holy song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Scoender maeght voer noch <rhyme label="a" type="m">na</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En was noeyt <rhyme label="b" type="f">geboren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Dan die edel <rhyme label="a" type="m" corresp="#s1:l1">barbara</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wonderlijc <rhyme label="b" type="f" corresp="#s1:l2">vercoren</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sy was vanden <rhyme label="a" type="m">geslechte</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Der coninginne <rhyme label="b" type="f">marien</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Wy moetense wel met <rhyme label="a" type="m" corresp="#s2:l1">rechte</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Alle <rhyme label="b" type="f" corresp="#s2:l2">gebenedien</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy was van allen consten <rhyme label="a" type="m">wijs</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wel geleert al <rhyme label="b" type="f">openbare</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van allen doeghden heeft sy <rhyme label="a" type="m" corresp="#s3:l1">prijs</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Geen gebrec en was in <rhyme label="b" type="f" corresp="#s3:l2">hare</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Bouen al die heilige <rhyme label="a" type="m">geest</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hy leerese van <rhyme label="b" type="f">binnen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Met sinen lichte alder <rhyme label="a" type="m" corresp="#s4:l1">meest</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die godheit claer <rhyme label="b" type="f" corresp="#s4:l2">bekinnen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Haer was gesonden herde <rhyme label="a" type="m">snel</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Een ingel uten <rhyme label="b" type="f">troone</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Diese int geloue stercte <rhyme label="a" type="m" corresp="#s5:l1">wel</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Der hoger drie <rhyme label="b" type="f" corresp="#s5:l2">persone</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ende sy sach ter seluer <rhyme label="a" type="m">stont</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Enen edelen <rhyme label="b" type="f">iongelinc</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Die aent cruce was doer <rhyme label="a" type="m" corresp="#s6:l1">wont</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat haer diep int herte <rhyme label="b" type="f" corresp="#s6:l2">ginc</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Barbara die edel <rhyme label="a" type="m">ioncfrouwe</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Heeft gode bouen al <rhyme label="b" type="f">gemint</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Ende hem gehouden vaste <rhyme label="a" type="m" corresp="#s7:l1">trouwe</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Als men in haer leuen <rhyme label="b" type="f" corresp="#s7:l2">vint</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Die minne was soe ouer <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Als sy wel dede in <rhyme label="b" type="f">scijne</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Sy en ontsach torment noch <rhyme label="a" type="m" corresp="#s8:l1">doot</rhyme></l>
        <l id="s8:l4" met="3" type="-+">noch geenderande <rhyme label="b" type="f" corresp="#s8:l2">pine</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Gheen mertelarse en was soe <rhyme label="a" type="m">gelijc</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Der passien ons liefs <rhyme label="b" type="f">heeren</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Die nie quam op <rhyme label="a" type="m" corresp="#s9:l1">eertrijc</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Wine connense niet vol <rhyme label="b" type="f" corresp="#s9:l2">eeren</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Barbara rose ende lelie <rhyme label="a" type="m">wijt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">alder sielen <rhyme label="b" type="f">vrouwe</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Waer ghy onsen heere voer <rhyme label="a" type="m" corresp="#s10:l1">bit</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Gecrijcht van sonden <rhyme label="b" type="f" corresp="#s10:l2">rouwe</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ende sijn heilich <rhyme label="a" type="m">sacrament</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Ons heeren lichame ende sijn <rhyme label="b" type="f">bloet</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Daer men ewich leuen in <rhyme label="a" type="m" corresp="#s11:l1">vint</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Ende oec oueruloedich <rhyme label="b" type="f" corresp="#s11:l2">goet</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">O dierbaer sceen van dooden <rhyme label="a" type="m">rijch</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Wilt ons te gode <rhyme label="b" type="f">leiden</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Der maget marien meest <rhyme label="a" type="m" corresp="#s12:l1">gelijch</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Van <rhyme label="b" type="f" corresp="#s12:l2">genedicheiden</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Xpristus sprac in sijnder <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Vergeeft des sonde vader <rhyme label="b" type="f">mijn</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Also dat die in uwer <rhyme label="a" type="m" corresp="#s13:l1">noot</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Voer alle die in sonden <rhyme label="b" type="f" corresp="#s13:l2">sijn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
