<song>
  <meta>
    <id>100087</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Petite royale</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Lief Dorinde waerste <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">U schoonheyt sal ick <rhyme label="b" type="f">prysen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">En steets eer <rhyme label="b" type="f" corresp="#s1:l2">bewysen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Mits ghy my <rhyme label="a" type="m" corresp="#s1:l1">behaeght</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Volmaeckten <rhyme label="c" type="f">Engel</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Hoort mijn klachten <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En siet hoe dat ick <rhyme label="c" type="f" corresp="#s1:l5">hengel</rhyme></l>
        <l id="s1:l8" met="3" type="+-">[...] u gunst 't <rhyme label="d" type="m" corresp="#s1:l6">ontfaen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Kunt my van smert <rhyme label="d" type="m" corresp="#s1:l6">ontslaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'k En heb geen koude wint <rhyme label="a" type="m">ontsien</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Noch oock geen Noorde <rhyme label="b" type="f">buyen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Van het koude <rhyme label="b" type="f" corresp="#s2:l2">suyen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Maer op u <rhyme label="a" type="m" corresp="#s2:l1">gebien</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Heb ick gaen <rhyme label="c" type="f">dryve</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Al u ruysch <rhyme label="d" type="m">gevacht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Om in u stal te <rhyme label="c" type="f" corresp="#s2:l5">blyve</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Jae ick hiel de <rhyme label="d" type="m" corresp="#s2:l6">wacht</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Vertoefde inde <rhyme label="d" type="m" corresp="#s2:l6">nacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Weet ghy niet wat de Liefde <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Heb ick niet uwe <rhyme label="b" type="f">Schapen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Als ghy lach te <rhyme label="b" type="f" corresp="#s3:l2">slapen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Voor de Wolf <rhyme label="a" type="m" corresp="#s3:l1">behoet</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">En voort <rhyme label="c" type="f">verkrachten</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Van den <rhyme label="d" type="m">Bos-Godtpan</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die naer u eens ginck <rhyme label="c" type="f" corresp="#s3:l5">trachten</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Ick hielp u daer <rhyme label="d" type="m" corresp="#s3:l6">van</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Veriaegden den <rhyme label="d" type="m" corresp="#s3:l6">Tiran</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'k En eys geen loon als <rhyme label="a" type="m">wedermin</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Voor al mijn trouwe <rhyme label="b" type="f">daden</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Isser geen <rhyme label="b" type="f" corresp="#s4:l2">genaden</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Schoonste <rhyme label="a" type="m" corresp="#s4:l1">Harderin</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Dees dach ist <rhyme label="c" type="f">leste</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Van mijns levens <rhyme label="d" type="m">draet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ten sy my yemant <rhyme label="c" type="f" corresp="#s4:l5">queste</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">My wederom <rhyme label="d" type="m" corresp="#s4:l6">verlaet</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Ta volgen Liefdens <rhyme label="d" type="m" corresp="#s4:l6">raet</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick ken u gunst 'k weet wat u <rhyme label="a" type="m">deert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ick moet u trouheyt <rhyme label="b" type="f">loonen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">En u Liefde <rhyme label="b" type="f" corresp="#s5:l2">toonen</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Mits ghy swijgen <rhyme label="a" type="m" corresp="#s5:l1">leert</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Vlucht dan <rhyme label="c" type="f">Dorinde</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Hier int Elf <rhyme label="d" type="m">Bos</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Daer sal ons niemant <rhyme label="c" type="f" corresp="#s5:l5">vinde</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Amon niet te <rhyme label="d" type="m" corresp="#s5:l6">grof</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Mijn eer verdwijnt tot <rhyme label="d" type="m" corresp="#s5:l6">stof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
