<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14003</id>
    <date>
      <year-start>1566</year-start>
      <year-end>1566</year-end>
    </date>
    <location>
      <orginal-name>emden</orginal-name>
      <standard-name>Emden, Germany</standard-name>
      <coordinates>
        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag>sung Lord's Prayer</tag>
    </tags>
    <melody>Onze vader in hemelrijk</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VAder onser in <rhyme label="a" type="m">Hemelrijck</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Die ons heet Broeders al <rhyme label="a" type="m" corresp="#s1:l1">gelijck</rhyme></l>
        <l id="s1:l3" met="4" type="-+">En wilt dat wy dy roepen <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Als wy met node zijn <rhyme label="b" type="m" corresp="#s1:l3">beuaen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Gheeft dat niet bidt alleyn de <rhyme label="c" type="m">mont</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Helpt dat het gae van herten <rhyme label="c" type="m" corresp="#s1:l5">gront</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Geheylicht uwen name <rhyme label="a" type="m">si</rhyme></l>
        <l id="s2:l2" met="4" type="-+">V Woort ongeualst ons bliue <rhyme label="a" type="m" corresp="#s2:l1">bi</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dat wi oock leuen <rhyme label="b" type="m">Heylichlick</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Nae uwen name <rhyme label="b" type="m" corresp="#s2:l3">weerdichlick</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Behoet ons Heer voor valsche <rhyme label="c" type="m">leer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Dat arm verdoolde volck <rhyme label="c" type="m" corresp="#s2:l5">bekeer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">V Rijcke toecomen Vader <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Hier ende hier na uwen trooster <rhyme label="a" type="m" corresp="#s3:l1">soet</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Gheeft ons so Christus ons <rhyme label="b" type="m">toesey</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Met sinen gaue <rhyme label="b" type="m" corresp="#s3:l3">menigherley</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Breect Sathans tooren en groot <rhyme label="c" type="m">ghewelt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Voor zijnercheyt v Kerck <rhyme label="c" type="m" corresp="#s3:l5">verhelt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vwen wil gheschiet o Heer <rhyme label="a" type="m">ghelijck</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Op aerden als in <rhyme label="a" type="m" corresp="#s4:l1">Hemelrijck</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Gheeft ons gedult in lijdens <rhyme label="b" type="m">pijn</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Ghehoorsaem allesins te <rhyme label="b" type="m" corresp="#s4:l3">zijn</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Neemt van ons wech vleesch ende <rhyme label="c" type="m">bloet</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Dat teghens uwen wille <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gheeft ons huden ons daghelicx <rhyme label="a" type="m">broot</rhyme></l>
        <l id="s5:l2" met="4" type="-+">En wat wy behoeuen ins lijfs <rhyme label="a" type="m" corresp="#s5:l1">noot</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Behoet ons Heer voor twist en <rhyme label="b" type="m">strijt</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Voor Pest en oock voor dieren <rhyme label="b" type="m" corresp="#s5:l3">tijt</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Dat wi in goede vrede <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Doet sorge ende Giericheyt van ons <rhyme label="c" type="m" corresp="#s5:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Al onse schult vergheeft ons <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Dat si ons niet bedroeuen <rhyme label="a" type="m" corresp="#s6:l1">meer</rhyme></l>
        <l id="s6:l3" met="4" type="-+">So wi oock die ons schuldich <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Haer schult vergheuen op dit <rhyme label="b" type="m" corresp="#s6:l3">termijn</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Tot haren dienst maect ons <rhyme label="c" type="m">bereyt</rhyme></l>
        <l id="s6:l6" met="4" type="-+">In rechter liefde en <rhyme label="c" type="m" corresp="#s6:l5">eenicheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Leyt ons Heer in becoring <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Als ons die boose Gheest strijt <rhyme label="a" type="m" corresp="#s7:l1">hiet</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Ter rechter oft ter <rhyme label="b" type="m">slinckerhant</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Helpt ons dat wi doen <rhyme label="b" type="m" corresp="#s7:l3">wederstant</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Int recht betrouwen <rhyme label="c" type="m">onbeureest</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Door uwen trooster den Heylighen <rhyme label="c" type="m" corresp="#s7:l5">geest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Van alle quaden verlost ons <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s8:l2" met="4" type="-+">In dese bedroefde tijden o <rhyme label="a" type="m" corresp="#s8:l1">Heer</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Beurijt ons vander eewigher <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s8:l4" met="4" type="-+">En troost ons inder lester <rhyme label="b" type="m" corresp="#s8:l3">noot</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Doet ons altijt goet <rhyme label="c" type="m">onderstant</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Neempt onse sielen in v <rhyme label="c" type="m" corresp="#s8:l5">handt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Amen dat is het werde <rhyme label="a" type="m">waer</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Sterct ons ghelooue <rhyme label="a" type="m" corresp="#s9:l1">wanckelbaer</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Op dat wi niet en twifelen daer <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Of wi sullen dit al <rhyme label="b" type="m" corresp="#s9:l3">ontfaen</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Naer uwen wille om Christus <rhyme label="c" type="m">naem</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Door welcken ons bede is <rhyme label="c" type="m" corresp="#s9:l5">ghedaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
