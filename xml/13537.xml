    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Von Gott will ich nicht lassen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GOds eer niet zy <rhyme label="a" type="f">verswegen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want Hy van my niet <rhyme label="b" type="m">keert</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hy leidt my op Sijn <rhyme label="a" type="f" corresp="#s1:l1">wegen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daer ik schier was <rhyme label="b" type="m" corresp="#s1:l2">verheert</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">God reikt tot my Sijn <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Den avond en den <rhyme label="d" type="f">morgen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Doet Hy my wel <rhyme label="d" type="f" corresp="#s1:l6">versorgen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ter plaetse daer ik <rhyme label="c" type="m" corresp="#s1:l5">land'</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wanneer de mensch <rhyme label="a" type="f">mismoedigh</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Op aerd en weet geen <rhyme label="b" type="m">raed</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">So sent de Heere <rhyme label="a" type="f" corresp="#s2:l1">spoedigh</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sijn macht en Sijn <rhyme label="b" type="m" corresp="#s2:l2">genaed</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">My helpt uit alle <rhyme label="c" type="m">nood</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Verlost van sond en <rhyme label="d" type="f">schanden</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Van ketten en van <rhyme label="d" type="f" corresp="#s2:l6">banden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ook voor den helschen <rhyme label="c" type="m" corresp="#s2:l5">dood</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Op God wil ik <rhyme label="a" type="f">vertrouwen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In mijne swaere <rhyme label="b" type="m">tyd</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">'t En sal my niet <rhyme label="a" type="f" corresp="#s3:l1">berouwen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hy weeret alle <rhyme label="b" type="m" corresp="#s3:l2">stryd</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">De Heer mijn sorge <rhyme label="c" type="m">draeght</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Mijn lyf, mijn ziel, mijn <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Wil ik Hem <rhyme label="d" type="f" corresp="#s3:l6">overgeven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hy maek, soo 't Hem <rhyme label="c" type="m" corresp="#s3:l5">behaeght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy geeft nae <rhyme label="a" type="f">wel-gevallen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wat ons ten besten <rhyme label="b" type="m">dient</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy handelt by ons <rhyme label="a" type="f" corresp="#s4:l1">allen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">So Hy 't wel raedsaem <rhyme label="b" type="m" corresp="#s4:l2">mient</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Hy schenkt Sijn lieven <rhyme label="c" type="m">Soon</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En heeft genoegh te <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Voor lyf en ziel <rhyme label="d" type="f" corresp="#s4:l6">gegheven</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">Hem loof in 's Hemels <rhyme label="c" type="m" corresp="#s4:l5">Throon</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En pryst met hert en <rhyme label="a" type="f">monde</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die Hy u beide <rhyme label="b" type="m">schenkt</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Wel saeligh is de <rhyme label="a" type="f" corresp="#s5:l1">stonde</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Als men aen Hem <rhyme label="b" type="m" corresp="#s5:l2">gedenkt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die tyd is uw <rhyme label="c" type="m">gewin</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die gy dus gaet <rhyme label="d" type="f">besteden</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">De Heere sal uw <rhyme label="d" type="f" corresp="#s5:l6">schreden</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Ten Hemel leiden <rhyme label="c" type="m" corresp="#s5:l5">in</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Als d'ongestuime <rhyme label="a" type="f">werelt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Vergaet met prael en <rhyme label="b" type="m">pracht</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">All eer en goed heen <rhyme label="a" type="f" corresp="#s6:l1">dwerelt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Te vooren groot <rhyme label="b" type="m" corresp="#s6:l2">geacht</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Als in den aerdsche <rhyme label="c" type="m">dal</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Begraeven zijn ons <rhyme label="d" type="f">beenen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">God salse weÃªr <rhyme label="d" type="f" corresp="#s6:l6">vereenen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Met een <rhyme label="c" type="m" corresp="#s6:l5">basuin-geschal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
