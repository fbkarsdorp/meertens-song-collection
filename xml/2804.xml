  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">En soude <rhyme label="a" type="f">baeren</rhyme></l>
        <l id="s1:l2" met="2" type="-+">nae schrifts <rhyme label="a" type="f" corresp="#s1:l1">voerklaeren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">tot betteleJaem een <rhyme label="b" type="f">soene</rhyme></l>
        <l id="s1:l4" met="2" type="-+">die ons <rhyme label="a" type="f" corresp="#s1:l1">beswaeren</rhyme></l>
        <l id="s1:l5" met="2" type="-+">sou doen <rhyme label="a" type="f" corresp="#s1:l1">voerhaeren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">en stellen hem ten <rhyme label="b" type="f" corresp="#s1:l3">toone</rhyme></l>
        <l id="s1:l7" met="3" type="-+">aen den kruise seer <rhyme label="c" type="m">snoet</rhyme></l>
        <l id="s1:l8" met="2" type="-+">smaedich <rhyme label="d" type="f">gebonden</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Daer hy den bitteren <rhyme label="c" type="m" corresp="#s1:l7">doet</rhyme></l>
        <l id="s1:l10" met="3" type="-+">voer ons kleyn en <rhyme label="c" type="m" corresp="#s1:l7">groet</rhyme></l>
        <l id="s1:l11" met="3" type="+-">sou steruen met anchst en <rhyme label="c" type="m" corresp="#s1:l7">noet</rhyme></l>
        <l id="s1:l12" met="2" type="-+">voer onse <rhyme label="d" type="f" corresp="#s1:l8">sonde</rhyme></l>
        <l id="s1:l13" met="2" type="-+">en met syn <rhyme label="d" type="f" corresp="#s1:l8">wonde</rhyme></l>
        <l id="s1:l14" met="3" type="-+">ten graeue sy (n) <rhyme label="e" type="m">gespreit</rhyme></l>
        <l id="s1:l15" met="3" type="-+">en voerrysen tot <rhyme label="e" type="m" corresp="#s1:l14">leyt</rhyme></l>
        <l id="s1:l16" met="3" type="-+">der Ioden wyt en <rhyme label="e" type="m" corresp="#s1:l14">breyt</rhyme></l>
        <l id="s1:l17" met="2" type="-+">diet selfs <rhyme label="d" type="f" corresp="#s1:l8">oerkonden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Lof <rhyme label="a" type="f">proefysyen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">tot ons <rhyme label="a" type="f" corresp="#s2:l1">voerblyen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">met swserder loften <rhyme label="b" type="f">hoegelick</rhyme></l>
        <l id="s2:l4" met="2" type="-+">zyn sonder <rhyme label="a" type="f" corresp="#s2:l1">myen</rhyme></l>
        <l id="s2:l5" met="2" type="-+">van syn <rhyme label="a" type="f" corresp="#s2:l1">partyen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">voldaen eensdeels seer <rhyme label="b" type="f" corresp="#s2:l3">oegelyck</rhyme></l>
        <l id="s2:l7" met="3" type="-+">want de doebters <rhyme label="c" type="m">onuleckt</rhyme></l>
        <l id="s2:l8" met="2" type="-+">van elye <rhyme label="d" type="f">goedich</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Wt Hannae voert <rhyme label="c" type="m" corresp="#s2:l7">gewrocht</rhyme></l>
        <l id="s2:l10" met="3" type="-+">maryse seer <rhyme label="c" type="m" corresp="#s2:l7">coereckt</rhyme></l>
        <l id="s2:l11" met="3" type="+-">baerde' dat soet <rhyme label="c" type="m" corresp="#s2:l7">perfeckt</rhyme></l>
        <l id="s2:l12" met="2" type="-+">den geest <rhyme label="d" type="f" corresp="#s2:l8">spoedich</rhyme></l>
        <l id="s2:l13" met="2" type="-+">die haer <rhyme label="d" type="f" corresp="#s2:l8">behoedich</rhyme></l>
        <l id="s2:l14" met="3" type="-+">doer godt was in <rhyme label="e" type="m">gestoert</rhyme></l>
        <l id="s2:l15" met="3" type="-+">en heeft naetuer <rhyme label="e" type="m" corresp="#s2:l14">begort</rhyme></l>
        <l id="s2:l16" met="3" type="-+">dat sy in daegen <rhyme label="e" type="m" corresp="#s2:l14">kort</rhyme></l>
        <l id="s2:l17" met="2" type="-+">haer vruecht was <rhyme label="d" type="f" corresp="#s2:l8">voedich</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Syn naem was <rhyme label="a" type="f">eerelick</rhyme></l>
        <l id="s3:l2" met="2" type="-+">soe groet en <rhyme label="a" type="f" corresp="#s3:l1">heerelick</rhyme></l>
        <l id="s3:l3" met="3" type="-+">dat engelen hier <rhyme label="b" type="f">verhuechde</rhyme></l>
        <l id="s3:l4" met="2" type="-+">die selfs haer <rhyme label="a" type="f" corresp="#s3:l1">keerelick</rhyme></l>
        <l id="s3:l5" met="2" type="-+">en seer <rhyme label="a" type="f" corresp="#s3:l1">begeerelick</rhyme></l>
        <l id="s3:l6" met="3" type="-+">waeren met groeter <rhyme label="b" type="f" corresp="#s3:l3">vruechtDe</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Jesum taanschouwen <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s3:l8" met="2" type="-+">doen eerst <rhyme label="d" type="f">geboeren</rhyme></l>
        <l id="s3:l9" met="3" type="-+">wiens kracht de engelsche <rhyme label="c" type="m" corresp="#s3:l7">schaer</rhyme></l>
        <l id="s3:l10" met="3" type="-+">elck een maeken <rhyme label="c" type="m" corresp="#s3:l7">gewaer</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Ock den wysen <rhyme label="c" type="m" corresp="#s3:l7">eenpaer</rhyme></l>
        <l id="s3:l12" met="2" type="-+">soemen on <rhyme label="d" type="f" corresp="#s3:l8">sporen</rhyme></l>
        <l id="s3:l13" met="2" type="-+">want sy al <rhyme label="d" type="f" corresp="#s3:l8">voeren</rhyme></l>
        <l id="s3:l14" met="3" type="-+">hem sochten met <rhyme label="e" type="m">oetmoet</rhyme></l>
        <l id="s3:l15" met="3" type="-+">en vielen hem te <rhyme label="e" type="m" corresp="#s3:l14">voet</rhyme></l>
        <l id="s3:l16" met="3" type="-+">met offer hant en <rhyme label="e" type="m" corresp="#s3:l14">goet</rhyme></l>
        <l id="s3:l17" met="2" type="-+">nae het <rhyme label="d" type="f" corresp="#s3:l8">behoeren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Prinse <rhyme label="a" type="f">bequaeme</rhyme></l>
        <l id="s4:l2" met="2" type="-+">lof synen <rhyme label="a" type="f" corresp="#s4:l1">naeme</rhyme></l>
        <l id="s4:l3" met="3" type="-+">gy heb door hem het <rhyme label="b" type="f">leuen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">syn woert <rhyme label="a" type="f" corresp="#s4:l1">heilsaeme</rhyme></l>
        <l id="s4:l5" met="2" type="-+">wilt u niet <rhyme label="a" type="f" corresp="#s4:l1">schaemen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">voerstandich te aen <rhyme label="b" type="f" corresp="#s4:l3">kleeuen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">want hy maeckte <rhyme label="c" type="m">bekent</rhyme></l>
        <l id="s4:l8" met="2" type="-+">als Is <rhyme label="d" type="f">waerachtich</rhyme></l>
        <l id="s4:l9" met="3" type="-+">dat liefde ist <rhyme label="c" type="m" corresp="#s4:l7">tfondaement</rhyme></l>
        <l id="s4:l10" met="3" type="-+">synde aent kruis <rhyme label="c" type="m" corresp="#s4:l7">gepent</rhyme></l>
        <l id="s4:l11" met="3" type="+-">syn vleys gedoet <rhyme label="c" type="m" corresp="#s4:l7">geschent</rhyme></l>
        <l id="s4:l12" met="2" type="-+">dees koeninck <rhyme label="d" type="f" corresp="#s4:l8">machtich</rhyme></l>
        <l id="s4:l13" met="2" type="-+">syt dit <rhyme label="d" type="f" corresp="#s4:l8">gedachtich</rhyme></l>
        <l id="s4:l14" met="3" type="-+">Int nieuwe Iaer met <rhyme label="e" type="m">vlit</rhyme></l>
        <l id="s4:l15" met="3" type="-+">Dat hy ons haest <rhyme label="e" type="m" corresp="#s4:l14">beuryt</rhyme></l>
        <l id="s4:l16" met="3" type="-+">dies maeck u sonde <rhyme label="e" type="m" corresp="#s4:l14">quit</rhyme></l>
        <l id="s4:l17" met="2" type="-+">en syt neer <rhyme label="d" type="f" corresp="#s4:l8">slachtich</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
