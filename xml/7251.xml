<song>
  <meta>
    <id>7251</id>
    <date>
      <year-start>1630</year-start>
      <year-end>1640</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="lament">klaaglied</tag>
    </tags>
    <melody>Toen ik was een jonge dochter</melody>
    <categories>
      <category translation="emotions">emoties</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Doen ick was een ionghe <rhyme label="a" type="f">dochter</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Vrijde my twee jongmans <rhyme label="b" type="m">fiet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Tot vryen kosten sy my niet <rhyme label="c" type="f">krijghen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Noch in gheender ey <rhyme label="b" type="m" corresp="#s1:l2">manier</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Nu hebben sy my also veer <rhyme label="d" type="f">ghekreghen</rhyme></l>
        <l id="s1:l6" met="6" type="+-">Over de Wilde Zee, Gevoert in vreemde <rhyme label="d" type="f" corresp="#s1:l5">wegen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Doe ic quam in vremde <rhyme label="a" type="f">stede</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Daer ick niet ghewoon en <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daer vondt ick mijn lief <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En ick boot hem goeden <rhyme label="b" type="m" corresp="#s2:l2">dagh</rhyme>:</l>
        <l id="s2:l5" met="5" type="+-">Zijt gy daer mijn liefje uyt <rhyme label="d" type="f">vercoren</rhyme></l>
        <l id="s2:l6" met="6" type="+-">‘k Heb v’staen dat gy met ruyters hebt <rhyme label="d" type="f" corresp="#s2:l5">gaen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zijn hier gheen boden te <rhyme label="a" type="f">krijghen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Dee my voeren nae mijn <rhyme label="b" type="m">Landt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Voor mijn Vader niet en <rhyme label="c" type="f">swijghen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Dat ick ben gheraeckt in <rhyme label="b" type="m" corresp="#s3:l2">schandt</rhyme>,</l>
        <l id="s3:l5" met="5" type="+-">Dat zy segghen teghen mijnen <rhyme label="d" type="f">Vader</rhyme></l>
        <l id="s3:l6" met="6" type="+-">Dat ic in lange tijt hem niet sal komen <rhyme label="d" type="f" corresp="#s3:l5">nader</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Doen ick quam al voor de <rhyme label="a" type="f">Poorte</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Voor een bossen daer ick <rhyme label="b" type="m">sagh</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Mijne lieve jonghste <rhyme label="c" type="f">Suster</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En ick boodt haer goeden <rhyme label="b" type="m" corresp="#s4:l2">dagh</rhyme>:</l>
        <l id="s4:l5" met="5" type="+-">Zyde gy daer mijn Suster <rhyme label="d" type="f">uytverkoren</rhyme>?</l>
        <l id="s4:l6" met="6" type="+-">‘k Heb so menigen traen om uwent wil <rhyme label="d" type="f" corresp="#s4:l5">v’loren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Doen ick quam al in de <rhyme label="a" type="f">Keucken</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Daer ick wel ghewoone <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daer vondt ick mijn beminde <rhyme label="c" type="f">Moeder</rhyme></l>
        <l id="s5:l4" met="4" type="+-">En ick boodt haer goeden <rhyme label="b" type="m" corresp="#s5:l2">dagh</rhyme>:</l>
        <l id="s5:l5" met="5" type="+-">Zyde gy daer mijn dochter <rhyme label="d" type="f">uytverkoren</rhyme>,</l>
        <l id="s5:l6" met="6" type="+-">‘k Heb v’staen dat gy achter lande hebt gaen <rhyme label="d" type="f" corresp="#s5:l5">dolen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Doe ic quam op de <rhyme label="a" type="f">slaepkamer</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Daer ick wel te slapen <rhyme label="b" type="m">plagh</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Daer vondt ick mijn straffen <rhyme label="c" type="f">Vader</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En ick boodt hem goeden <rhyme label="b" type="m" corresp="#s6:l2">dagh</rhyme>:</l>
        <l id="s6:l5" met="5" type="+-">Zyde gy daer Lantloopster achter <rhyme label="d" type="f">landen</rhyme></l>
        <l id="s6:l6" met="6" type="+-">Packt u terstondt van hier, Of ick brengh u in <rhyme label="d" type="f" corresp="#s6:l5">schande</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ick en ben voorwaer geen <rhyme label="a" type="f">Schooyster</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Noch oock gheen Landt-loopster <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">‘k Heb soo dickmael wesen <rhyme label="c" type="f">doolen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">In soo meenighe schoone <rhyme label="b" type="m" corresp="#s7:l2">stee</rhyme>,</l>
        <l id="s7:l5" met="5" type="+-">Daer heb ick mijn op alle vremde <rhyme label="d" type="f">wegen</rhyme></l>
        <l id="s7:l6" met="6" type="+-">Eerelijck en vroom by alle luy <rhyme label="d" type="f" corresp="#s7:l5">gedregen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Adieu Vader, adieu <rhyme label="a" type="f">Moeder</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Adieu lieve Broeders <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Godt den Heer zy u <rhyme label="c" type="f">behoeder</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Nu gae ick weer uytter <rhyme label="b" type="m" corresp="#s8:l2">stee</rhyme>,</l>
        <l id="s8:l5" met="5" type="+-">En ic wil mijn achter lande gaen <rhyme label="d" type="f">begeven</rhyme></l>
        <l id="s8:l6" met="6" type="+-">Over de woeste zee en enden mijn jonc <rhyme label="d" type="f" corresp="#s8:l5">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
