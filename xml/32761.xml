  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">COmt en laet ons <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den groenen Mey in <rhyme label="b" type="f">draghen</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Daer den <rhyme label="a" type="m" corresp="#s1:l1">Pellicaen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Iesus heeft op <rhyme label="c" type="m">gherust</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">En voor ons <rhyme label="a" type="m" corresp="#s1:l1">voldaen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Naer sijn goedt <rhyme label="b" type="f" corresp="#s1:l2">wel-behaghen</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Onse sonden <rhyme label="a" type="m" corresp="#s1:l1">saen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Door sijn bloedt <rhyme label="c" type="m" corresp="#s1:l4">uytgheblust</rhyme>.</l>
        <l id="s1:l9" met="3" type="+-">Desen Mey't <rhyme label="a" type="m" corresp="#s1:l1">ontfaen</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Had' Christus grooten <rhyme label="c" type="m" corresp="#s1:l4">lust</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Om dat hy soude doen te <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Die Eva door den boom <rhyme label="d" type="m" corresp="#s1:l11">verriet</rhyme>:</l>
        <l id="s1:l13" met="4" type="-+">Triumph' en <rhyme label="e" type="m">accoort</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Brenght desen Meyboom <rhyme label="e" type="m" corresp="#s1:l13">voort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Menschen wilt u <rhyme label="a" type="m">spoen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Desen Mey in te <rhyme label="b" type="f">halen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Want hy can ons <rhyme label="a" type="m" corresp="#s2:l1">voen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met allen weelden <rhyme label="c" type="m">fijn</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Ons schult en <rhyme label="a" type="m" corresp="#s2:l1">rantsoen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Wou Christus oock <rhyme label="b" type="f" corresp="#s2:l2">betalen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Aen het Cruyce <rhyme label="a" type="m" corresp="#s2:l1">coen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Daer was den wille <rhyme label="c" type="m" corresp="#s2:l4">sijn</rhyme>:</l>
        <l id="s2:l9" met="3" type="+-">Desen Meyboom <rhyme label="a" type="m" corresp="#s2:l1">groen</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Is ons een <rhyme label="c" type="m" corresp="#s2:l4">Medecijn</rhyme>:</l>
        <l id="s2:l11" met="3" type="+-">Zijn costelijcke vruchten <rhyme label="d" type="m">goet</rhyme></l>
        <l id="s2:l12" met="4" type="-+">Des vyants machten vluchten <rhyme label="d" type="m" corresp="#s2:l11">doet</rhyme></l>
        <l id="s2:l13" met="4" type="-+">Triumph' en <rhyme label="e" type="m">accort</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Brenght desen Meyboom <rhyme label="e" type="m" corresp="#s2:l13">voort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">En wordt niet <rhyme label="a" type="m">vermoeyt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van desen Mey <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Want uyt hem soo <rhyme label="a" type="m" corresp="#s3:l1">vloeyt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Alle ghenuchte <rhyme label="c" type="m">soet</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Lieffelijck hy <rhyme label="a" type="m" corresp="#s3:l1">bloeyt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van blomkens <rhyme label="b" type="f" corresp="#s3:l2">uytghelesen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Want hy is <rhyme label="a" type="m" corresp="#s3:l1">besproeyt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Met Christus dierbaer <rhyme label="c" type="m" corresp="#s3:l4">bloet</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Dus niet en <rhyme label="a" type="m" corresp="#s3:l1">verfoeyt</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Den Mey in uwen <rhyme label="c" type="m" corresp="#s3:l4">moet</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Die voor ons is <rhyme label="d" type="m">victorieus</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">Triumphant ende <rhyme label="d" type="m" corresp="#s3:l11">glorieus</rhyme>.</l>
        <l id="s3:l13" met="4" type="-+">Triumph' en <rhyme label="e" type="m">accoort</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Brenght desen Meyboom <rhyme label="e" type="m" corresp="#s3:l13">voort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
