<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179639</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAer is het Fortuin nu <rhyme label="a" type="m">ras</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En haestig weg <rhyme label="b" type="f">gevlogen</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Dat myn laest gunstig <rhyme label="a" type="m" corresp="#s1:l1">was</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Heeft myn tot min <rhyme label="b" type="f" corresp="#s1:l2">bewogen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">maer als het is <rhyme label="c" type="m">voorby</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">En in de hoogste <rhyme label="d" type="m">graet</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">maer ziet dan is het <rhyme label="c" type="m" corresp="#s1:l5">fy</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Het is helaes te <rhyme label="d" type="m" corresp="#s1:l6">laet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Iuffrouw <rhyme label="a" type="m">principael</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die myn haer trouw quam <rhyme label="b" type="f">vlye</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">het scheen wel een plaets <rhyme label="a" type="m" corresp="#s2:l1">cosael</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">maer ach het zyn maer <rhyme label="b" type="f" corresp="#s2:l2">lye</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Om myn te krygen tot haer <rhyme label="c" type="m">minteneur</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Of voor haer <rhyme label="d" type="m">Serviteur</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">maer ach het is <rhyme label="c" type="m" corresp="#s2:l5">voorby</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Daer ik zoo lang om <rhyme label="d" type="m" corresp="#s2:l6">treur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
