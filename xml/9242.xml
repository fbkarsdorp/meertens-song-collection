  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Beweent nu t Nederlant// <rhyme label="a" type="m">want</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Zijn straff is noch voor d' <rhyme label="a" type="m" corresp="#s1:l1">hant</rhyme>/</l>
        <l id="s1:l3" met="3" type="-+">Want al heeft Godt <rhyme label="b" type="f">ghegheven</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Veel waerschouwinghen <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Naer vleesch en bloet/ den raet noch <rhyme label="c" type="m" corresp="#s1:l4">doet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Maer na Gods Raet <rhyme label="b" type="f" corresp="#s1:l3">verheven</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Settense niet een <rhyme label="c" type="m" corresp="#s1:l4">voet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ghelijck t' volck Israel// <rhyme label="a" type="m">snel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Eyschsten van <rhyme label="a" type="m" corresp="#s2:l1">Samuel</rhyme>/</l>
        <l id="s2:l3" met="3" type="-+">Een Coninck om te <rhyme label="b" type="f">Richten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Over hen vroech en <rhyme label="c" type="m">laet</rhyme>/</l>
        <l id="s2:l5" met="4" type="-+">Soo is oock t'quaet// sonder Gods <rhyme label="c" type="m" corresp="#s2:l4">Raet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ghebracht in dees <rhyme label="b" type="f" corresp="#s2:l3">ghestichten</rhyme>/</l>
        <l id="s2:l7" met="3" type="-+">Tot straff' van ons <rhyme label="c" type="m" corresp="#s2:l4">misdaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want men wil tvolck met vliet// <rhyme label="a" type="m">siet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Brenghen in groot <rhyme label="a" type="m" corresp="#s3:l1">verdriet</rhyme>/</l>
        <l id="s3:l3" met="3" type="-+">Als wast eens vande <rhyme label="b" type="f">clauwen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Des Leeus verlost seer <rhyme label="c" type="m">wel</rhyme>/</l>
        <l id="s3:l5" met="4" type="-+">Deur t'Vos opste//  met zijn <rhyme label="c" type="m" corresp="#s3:l4">ghesel</rhyme>/</l>
        <l id="s3:l6" met="3" type="-+">Commet met swaer <rhyme label="b" type="f" corresp="#s3:l3">benauwen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Onder Swolfs clauwen <rhyme label="c" type="m" corresp="#s3:l4">fel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Het gaet met dit lant <rhyme label="a" type="m">hier//schiet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghelijck met Troyen <rhyme label="a" type="m" corresp="#s4:l1">fier</rhyme></l>
        <l id="s4:l3" met="3" type="-+">De Griecken hadden <rhyme label="b" type="f">langhe</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Bekrijcht Troyen de <rhyme label="c" type="m">Stadt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Op elcken padt// maecten zijt <rhyme label="c" type="m" corresp="#s4:l4">pladt</rhyme>/</l>
        <l id="s4:l6" met="3" type="-+">Maer die Troyanen <rhyme label="b" type="f" corresp="#s4:l3">stranghe</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Maeckten de Griecken <rhyme label="c" type="m" corresp="#s4:l4">madt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer dees Griecken noch wreet// <rhyme label="a" type="m">heet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Wilden wreken haer <rhyme label="a" type="m" corresp="#s5:l1">leet</rhyme>/</l>
        <l id="s5:l3" met="3" type="-+">Maeckten een Peert van <rhyme label="b" type="f">houte</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Seer hooch en breet <rhyme label="c" type="m">voorwaer</rhyme>/</l>
        <l id="s5:l5" met="4" type="-+">Daer in dat claer// ghinghen al <rhyme label="c" type="m" corresp="#s5:l4">daer</rhyme>/</l>
        <l id="s5:l6" met="3" type="-+">Veel krijchslien als de <rhyme label="b" type="f" corresp="#s5:l3">stoute</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Vrijmoedich sonder <rhyme label="c" type="m" corresp="#s5:l4">baer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Deur Symons schoon op smaek// <rhyme label="a" type="m">faek</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Sijns' uut Troyen <rhyme label="a" type="m" corresp="#s6:l1">ghegaen</rhyme>/</l>
        <l id="s6:l3" met="3" type="-+">Braken haer poort en <rhyme label="b" type="f">mueren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Om in te krijghen <rhyme label="c" type="m">handt</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Dat daer opt' landt// fijn was <rhyme label="c" type="m" corresp="#s6:l4">gheplandt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Maer Troyen moest <rhyme label="b" type="f" corresp="#s6:l3">besueren</rhyme>/</l>
        <l id="s6:l7" met="3" type="-+">Want daer deur wiert <rhyme label="c" type="m" corresp="#s6:l4">verbrandt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Sulcx hebben wy ghesien// <rhyme label="a" type="m">lien</rhyme></l>
        <l id="s7:l2" met="3" type="-+">In onsen tijt <rhyme label="a" type="m" corresp="#s7:l1">gheschien</rhyme>/</l>
        <l id="s7:l3" met="3" type="-+">Den Franschen konnick <rhyme label="b" type="f">machtich</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Heeft deur gheweldt <rhyme label="c" type="m">versocht</rhyme></l>
        <l id="s7:l5" met="4" type="-+">En sijn ghedrocht// aent landt <rhyme label="c" type="m" corresp="#s7:l4">gebrocht</rhyme>/</l>
        <l id="s7:l6" met="3" type="-+">Om in te nemen <rhyme label="b" type="f" corresp="#s7:l3">krachtich</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Maer hy heeft niets <rhyme label="c" type="m" corresp="#s7:l4">vermocht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Doch Symons raet altoos// <rhyme label="a" type="m">loos</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Bracht hier l' homme du <rhyme label="a" type="m" corresp="#s8:l1">bois</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Men deed 't volck te <rhyme label="b" type="f">verstane</rhyme></l>
        <l id="s8:l4" met="3" type="-+">t Was een de beste <rhyme label="c" type="m">man</rhyme>/</l>
        <l id="s8:l5" met="4" type="-+">Veel beter dan// men vinden <rhyme label="c" type="m" corresp="#s8:l4">kan</rhyme>/</l>
        <l id="s8:l6" met="3" type="-+">Daerom men hoort te <rhyme label="b" type="f" corresp="#s8:l3">gane</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Om hem te nemen <rhyme label="c" type="m" corresp="#s8:l4">an</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Hier deur wert tvolck ghesindt// <rhyme label="a" type="m">blindt</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Om t'hebben dit goet <rhyme label="a" type="m" corresp="#s9:l1">kindt</rhyme>/</l>
        <l id="s9:l3" met="3" type="-+">men sandt hem <rhyme label="b" type="f">Ambassaden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">En haeld' hem eerlick <rhyme label="c" type="m">in</rhyme></l>
        <l id="s9:l5" met="4" type="-+">t Was groot ghewin// naer des volcx <rhyme label="c" type="m" corresp="#s9:l4">sin</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Maer hy wilt ons <rhyme label="b" type="f" corresp="#s9:l3">verraden</rhyme>/</l>
        <l id="s9:l7" met="3" type="-+">Ten was gheen goet <rhyme label="c" type="m" corresp="#s9:l4">beghin</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Want soo dat Griesche Peert// <rhyme label="a" type="m">weert</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Hadt eenen quaden <rhyme label="a" type="m" corresp="#s10:l1">steert</rhyme>/</l>
        <l id="s10:l3" met="3" type="-+">Hy wou terstondt de <rhyme label="b" type="f">Misse</rhyme></l>
        <l id="s10:l4" met="3" type="-+">In plaetse van Gods <rhyme label="c" type="m">Woort</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Gaen stellen voort// hy wiert <rhyme label="c" type="m" corresp="#s10:l4">verhoort</rhyme>/</l>
        <l id="s10:l6" met="3" type="-+">Want teghen zijn <rhyme label="b" type="f" corresp="#s10:l3">Altesse</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Mocht niemant zijn <rhyme label="c" type="m" corresp="#s10:l4">ghestoort</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Voorts in zijn erste Jaer// <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Heeft hy ghetoont dats <rhyme label="a" type="m" corresp="#s11:l1">claer</rhyme>/</l>
        <l id="s11:l3" met="3" type="-+">t' Antwerpen, elders <rhyme label="b" type="f">mede</rhyme>/</l>
        <l id="s11:l4" met="3" type="-+">Wat daer stack in zijn <rhyme label="c" type="m">vel</rhyme>/</l>
        <l id="s11:l5" met="4" type="-+">Raetsen en Spel// hoereren <rhyme label="c" type="m" corresp="#s11:l4">snel</rhyme>/</l>
        <l id="s11:l6" met="3" type="-+">Maer Oudenaerd' die <rhyme label="b" type="f" corresp="#s11:l3">Stede</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Verliet hy int <rhyme label="c" type="m" corresp="#s11:l4">ghequel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Daer na quam tvolck by een// <rhyme label="a" type="m">tscheen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Dat hyse woud' op <rhyme label="a" type="m" corresp="#s12:l1">leen</rhyme>/</l>
        <l id="s12:l3" met="3" type="-+">Om monsteringh' te <rhyme label="b" type="f">doene</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Daer van was veel <rhyme label="c" type="m">gheschals</rhyme>/</l>
        <l id="s12:l5" met="4" type="-+">Maer hy sprack wals// al enen <rhyme label="c" type="m" corresp="#s12:l4">vals</rhyme>/</l>
        <l id="s12:l6" met="3" type="-+">T'Was om de Burghers <rhyme label="b" type="f" corresp="#s12:l3">koene</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Te brenghen om den <rhyme label="c" type="m" corresp="#s12:l4">hals</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Als hy was buyten t' Stadt// <rhyme label="a" type="m">wat</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Stelden hy voort zijn <rhyme label="a" type="m" corresp="#s13:l1">pradt</rhyme>/</l>
        <l id="s13:l3" met="3" type="-+">Hy dreef zijn Eel lien <rhyme label="b" type="f">binnen</rhyme>/</l>
        <l id="s13:l4" met="3" type="-+">Meest Moorders van <rhyme label="c" type="m">Parijs</rhyme>/</l>
        <l id="s13:l5" met="4" type="-+">Die Daer propijs// quamen opt' <rhyme label="c" type="m" corresp="#s13:l4">ijs</rhyme>/</l>
        <l id="s13:l6" met="3" type="-+">Die ghinghen tspel <rhyme label="b" type="f" corresp="#s13:l3">beghinnen</rhyme>/</l>
        <l id="s13:l7" met="3" type="-+">Maer Godt gaf vander <rhyme label="c" type="m" corresp="#s13:l4">prijs</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Sijn volck viel dapper aen// <rhyme label="a" type="m">saen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Met schieten/ moorden/ <rhyme label="a" type="m" corresp="#s14:l1">slaen</rhyme>/</l>
        <l id="s14:l3" met="3" type="-+">Al roepen/ schreewen/ <rhyme label="b" type="f">tieren</rhyme>/</l>
        <l id="s14:l4" met="3" type="-+">Vive la Mess'tue// Ville <rhyme label="c" type="m">gaigne</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Daer weelck <rhyme label="c" type="m" corresp="#s14:l4">de</rhyme>/</l>
        <l id="s14:l6" met="3" type="-+">Sijn best om te <rhyme label="b" type="f" corresp="#s14:l3">schoffieren</rhyme>/</l>
        <l id="s14:l7" met="3" type="-+">Al datter was in <rhyme label="c" type="m" corresp="#s14:l4">ste</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Maer een seer cleyn ghetal// <rhyme label="a" type="m">smal</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Deden se loopen <rhyme label="a" type="m" corresp="#s15:l1">al</rhyme>/</l>
        <l id="s15:l3" met="3" type="-+">En de sterckten <rhyme label="b" type="f">verlaten</rhyme>/</l>
        <l id="s15:l4" met="3" type="-+">Veel spronghen inde vest//droncken om <rhyme label="c" type="m">best</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Al voor haer <rhyme label="c" type="m" corresp="#s15:l4">lest</rhyme>/</l>
        <l id="s15:l6" met="3" type="-+">Voorts wierden langs der <rhyme label="b" type="f" corresp="#s15:l3">straten</rhyme>/</l>
        <l id="s15:l7" met="3" type="-+">Doot ghesmeten de <rhyme label="c" type="m" corresp="#s15:l4">rest</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Als dat sach d'Hertogh' jonc// <rhyme label="a" type="m">clonck</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Dat t'volck int water <rhyme label="a" type="m" corresp="#s16:l1">spronck</rhyme>/</l>
        <l id="s16:l3" met="3" type="-+">Riep hy siet die <rhyme label="b" type="f">Canaille</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Hoe datse daer springhen <rhyme label="c" type="m">uut</rhyme>/</l>
        <l id="s16:l5" met="4" type="-+">Sy laten haer muyt// ons is de <rhyme label="c" type="m" corresp="#s16:l4">bruyt</rhyme>/</l>
        <l id="s16:l6" met="3" type="-+">Maer voor deynd' der <rhyme label="b" type="f" corresp="#s16:l3">bataille</rhyme>/</l>
        <l id="s16:l7" met="3" type="-+">Soo sach  hy langhs zijn <rhyme label="c" type="m" corresp="#s16:l4">snuyt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Want tgroft gheschut seer straf// <rhyme label="a" type="m">gaf</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Vier/ Clooten boven <rhyme label="a" type="m" corresp="#s17:l1">af</rhyme>/</l>
        <l id="s17:l3" met="3" type="-+">In zijn volck dat <rhyme label="b" type="f">marcheerde</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Om in te commen <rhyme label="c" type="m">koen</rhyme>/</l>
        <l id="s17:l5" met="4" type="-+">Daerom hy doen// scheen te <rhyme label="c" type="m" corresp="#s17:l4">verween</rhyme>/</l>
        <l id="s17:l6" met="3" type="-+">Oft hy eerst <rhyme label="b" type="f" corresp="#s17:l3">triumpheerde</rhyme>/</l>
        <l id="s17:l7" met="3" type="-+">t' Hert sonck nu in zijn <rhyme label="c" type="m" corresp="#s17:l4">schoen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Monsieur most schampen voort// <rhyme label="a" type="m">hoort</rhyme></l>
        <l id="s18:l2" met="3" type="-+">En hadd' daer zijn <rhyme label="a" type="m" corresp="#s18:l1">paspoort</rhyme>/</l>
        <l id="s18:l3" met="3" type="-+">Want zijn <rhyme label="b" type="f">verraderije</rhyme></l>
        <l id="s18:l4" met="3" type="-+">En snoo <rhyme label="c" type="m">meyneedicheyt</rhyme></l>
        <l id="s18:l5" met="4" type="-+">Is deur dit feyt// seer claer <rhyme label="c" type="m" corresp="#s18:l4">verbreyt</rhyme>/</l>
        <l id="s18:l6" met="3" type="-+">Die hem tot allen <rhyme label="b" type="f" corresp="#s18:l3">tijt</rhyme></l>
        <l id="s18:l7" met="3" type="-+">Bedeckt int herte <rhyme label="c" type="m" corresp="#s18:l4">leyt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Desen die deur verraet// <rhyme label="a" type="m">quaet</rhyme></l>
        <l id="s19:l2" met="3" type="-+">tLant maeckt heel <rhyme label="a" type="m" corresp="#s19:l1">desolaet</rhyme>/</l>
        <l id="s19:l3" met="3" type="-+">Die wilt men woderom <rhyme label="b" type="f">stellen</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Als een Heer over <rhyme label="c" type="m">al</rhyme>/</l>
        <l id="s19:l5" met="4" type="-+">Dies hy noch sal/ dit volck seer <rhyme label="c" type="m" corresp="#s19:l4">mal</rhyme></l>
        <l id="s19:l6" met="3" type="-+">Deur zijn listicheyt <rhyme label="b" type="f" corresp="#s19:l3">quellen</rhyme>/</l>
        <l id="s19:l7" met="3" type="-+">Keert Godt hem niet den <rhyme label="c" type="m" corresp="#s19:l4">val</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Hoe kan t'eenich termijn// <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Ooft hy en schiet t' <rhyme label="a" type="m" corresp="#s20:l1">fenijn</rhyme>/</l>
        <l id="s20:l3" met="3" type="-+">Dat hy langh' heeft <rhyme label="b" type="f">ghesoghen</rhyme>/</l>
        <l id="s20:l4" met="3" type="-+">Uit Royne Meres <rhyme label="c" type="m">burst</rhyme>/</l>
        <l id="s20:l5" met="4" type="-+">Die hy met lust// langh' heeft <rhyme label="c" type="m" corresp="#s20:l4">ghekust</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Daerom zijn wy <rhyme label="b" type="f" corresp="#s20:l3">bedroghen</rhyme></l>
        <l id="s20:l7" met="3" type="-+">Hebben wy sulcken <rhyme label="c" type="m" corresp="#s20:l4">Durst</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Meynt niet dat ons Godt milt// <rhyme label="a" type="m">wilt</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Helpen/ deur s'weert of <rhyme label="a" type="m" corresp="#s21:l1">schilt</rhyme>/</l>
        <l id="s21:l3" met="3" type="-+">Der Franschen seer <rhyme label="b" type="f">ghebeten</rhyme>/</l>
        <l id="s21:l4" met="3" type="-+">Om stelen/ moorden/ <rhyme label="c" type="m">slaen</rhyme></l>
        <l id="s21:l5" met="4" type="-+">Den vromen saen// oock te <rhyme label="c" type="m" corresp="#s21:l4">betraen</rhyme>/</l>
        <l id="s21:l6" met="3" type="-+">En tgansche landt op <rhyme label="b" type="f" corresp="#s21:l3">t'eten</rhyme>/</l>
        <l id="s21:l7" met="3" type="-+">Daerom wast hen <rhyme label="c" type="m" corresp="#s21:l4">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Doen Leyden was in noot// <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s22:l2" met="3" type="-+">En Hollandt hadd' <rhyme label="a" type="m" corresp="#s22:l1">aenstoot</rhyme>/</l>
        <l id="s22:l3" met="3" type="-+">Vande Spaengiaerts <rhyme label="b" type="f">bloetgierich</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Thien zijnde teghen <rhyme label="c" type="m">een</rhyme>/</l>
        <l id="s22:l5" met="4" type="-+">Der Franschen reen// en holp hen <rhyme label="c" type="m" corresp="#s22:l4">gheen</rhyme>/</l>
        <l id="s22:l6" met="3" type="-+">Maer sy baden Godt <rhyme label="b" type="f" corresp="#s22:l3">vierich</rhyme></l>
        <l id="s22:l7" met="3" type="-+">Die brachts al uut <rhyme label="c" type="m" corresp="#s22:l4">tgheween</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Daerom/ och vrienden goet// <rhyme label="a" type="m">vroet</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Bidt Godt oock met <rhyme label="a" type="m" corresp="#s23:l1">ootmoet</rhyme></l>
        <l id="s23:l3" met="3" type="-+">Dat hy doch der <rhyme label="b" type="f">Raetsheeren</rhyme></l>
        <l id="s23:l4" met="3" type="-+">Herten regeer' <rhyme label="c" type="m">eenpaer</rhyme>/</l>
        <l id="s23:l5" met="4" type="-+">Datse hier naer// dit nemen <rhyme label="c" type="m" corresp="#s23:l4">waer</rhyme>/</l>
        <l id="s23:l6" met="3" type="-+">En ons regeeren <rhyme label="b" type="f" corresp="#s23:l3">leeren</rhyme>/</l>
        <l id="s23:l7" met="3" type="-+">Sonder dien quaden <rhyme label="c" type="m" corresp="#s23:l4">vaer</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Och Prince maeckt Gods wet// <rhyme label="a" type="m">vet</rhyme></l>
        <l id="s24:l2" met="3" type="-+">Brenght ons niet meer int <rhyme label="a" type="m" corresp="#s24:l1">net</rhyme>/</l>
        <l id="s24:l3" met="3" type="-+">Van dien Franschen <rhyme label="b" type="f">gheselle</rhyme>/</l>
        <l id="s24:l4" met="3" type="-+">Die doch is een <rhyme label="c" type="m">Tyran</rhyme>/</l>
        <l id="s24:l5" met="4" type="-+">Met zijn ghespan// ghelijck Don <rhyme label="c" type="m" corresp="#s24:l4">Jan</rhyme>/</l>
        <l id="s24:l6" met="3" type="-+">En denckt dat uut die <rhyme label="b" type="f" corresp="#s24:l3">Helle</rhyme></l>
        <l id="s24:l7" met="3" type="-+">Niet goedt ghecomen en <rhyme label="c" type="m" corresp="#s24:l4">kan</rhyme>:</l>
      </lg>
    </lg>
  </text>
</song>
