<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187026</id>
    <date>
      <year-start>1708</year-start>
      <year-end>1708</year-end>
    </date>
    <location>
      <orginal-name>gouda</orginal-name>
      <standard-name>Gouda, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114922</lon>
        <lat>4.7106094</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Point de chagrin dans notre vie</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wat wil alom dit <rhyme label="a" type="f">zegevieren</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">Hoe klimt de vreugd dus na de <rhyme label="b" type="m">lugt</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Hoe juichen Brit, en <rhyme label="a" type="f" corresp="#s1:l1">Batavieren</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">ô Langgewenste <rhyme label="b" type="m" corresp="#s1:l2">krygsgerugt</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De magt van Vrankrijk legt <rhyme label="a" type="f">verslagen</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">De Beyervorst is op de <rhyme label="b" type="m">vlugt</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">De zee, en alle kusten <rhyme label="a" type="f" corresp="#s2:l1">wagen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Van dit verwonderlijk <rhyme label="b" type="m" corresp="#s2:l2">gerugt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nu droog de tranen van uw <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">ô Breede Donauw, edle <rhyme label="b" type="m">vloed</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Uw Rijksverlosser <rhyme label="a" type="f" corresp="#s3:l1">aangetogen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Treed al uw onheil met de <rhyme label="b" type="m" corresp="#s3:l2">voet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoogduitse maagden vlegt <rhyme label="a" type="f">Laurieren</rhyme></l>
        <l id="s4:l2" met="4" type="-+">(Uw, overwinnaar houd het <rhyme label="b" type="m">veld</rhyme>.)</l>
        <l id="s4:l3" met="4" type="-+">Om 't hooft van Marlboroug te <rhyme label="a" type="f" corresp="#s4:l1">sieren</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Den onverwinnelijksten <rhyme label="b" type="m" corresp="#s4:l2">Held</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maar gy, op uwe magt te <rhyme label="a" type="f">moedig</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Als of geen magt u dempen <rhyme label="b" type="m">kon</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">ô Trotse vorst zie eens hoe <rhyme label="a" type="f" corresp="#s5:l1">bloedig</rhyme></l>
        <l id="s5:l4" met="4" type="-+">De glans hier zinkt van uwe <rhyme label="b" type="m" corresp="#s5:l2">zon</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zoo storte uit zyn verheven <rhyme label="a" type="f">zetel</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Verwaande Lucifer ter <rhyme label="b" type="m">neer</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Zoo valt de hoogmoed, die <rhyme label="a" type="f" corresp="#s6:l1">vermetel</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Haar eygent meer dan menslijke <rhyme label="b" type="m" corresp="#s6:l2">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
