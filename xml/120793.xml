    <tags>
      <tag translation="reflective song">bespiegelend lied</tag>
      <tag translation="">wereldlijk</tag>
    </tags>
    <melody>Achter in de Ridderstraat</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Luisterd na dit aardig <rhyme label="a" type="m">lied</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van nieuwe modes <rhyme label="b" type="m">trant</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ik u zal gaan <rhyme label="c" type="f">zingen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Ja, ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wie zag ooit vreemde <rhyme label="c" type="f" corresp="#s1:l3">dingen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Al in ons <rhyme label="b" type="m" corresp="#s1:l2">Nederland</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laastmaal kwam ik de Stad <rhyme label="a" type="m">doorgaan</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wat zag ik daar een <rhyme label="b" type="m">zwier</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Al kruissen langs de <rhyme label="c" type="f">straaten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Ja, Ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Door hoeken en door <rhyme label="c" type="f" corresp="#s2:l3">gaten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van Meisjes van <rhyme label="b" type="m" corresp="#s2:l2">plaizier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De een haar borsten <rhyme label="a" type="m">opgezet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Of ook gemaakt in <rhyme label="b" type="m">schyn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Haar buik of zy moest <rhyme label="c" type="f">kraamen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ja, ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ik kan ze wel by <rhyme label="c" type="f" corresp="#s3:l3">namen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die daar woonachtig <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'k Sag Naatje met gespleeten <rhyme label="a" type="m">hair</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al onder haar <rhyme label="b" type="m">cornet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Sy had een hoed met <rhyme label="c" type="f">hokken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Ja, ja van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En op haar lange <rhyme label="c" type="f" corresp="#s4:l3">rokken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Een breede band <rhyme label="b" type="m" corresp="#s4:l2">gezet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Haar schoentjes waren spits en <rhyme label="a" type="m">plat</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met doffen <rhyme label="b" type="m">opgecierd</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Sy deed niet aars als <rhyme label="c" type="f">lonken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ja, ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Het Meisje scheen half <rhyme label="c" type="f" corresp="#s5:l3">dronken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Want zy had braaf <rhyme label="b" type="m" corresp="#s5:l2">gezwierd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sy had een Losbol op haar <rhyme label="a" type="m">zy</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Een Jougman na de <rhyme label="b" type="m">smaak</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met ringen in zyn <rhyme label="c" type="f">ooren</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Ja, ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Het Meisje was te <rhyme label="c" type="f" corresp="#s6:l3">vooren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Al van hem zwaar <rhyme label="b" type="m" corresp="#s6:l2">geraakt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy met de Dame na de <rhyme label="a" type="m">trant</rhyme>:</l>
        <l id="s7:l2" met="3" type="-+">Liep speelhuis wit en <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En deed niet aÃªrs als <rhyme label="c" type="f">schuiven</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Ja, ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Maar meende toe te <rhyme label="c" type="f" corresp="#s7:l3">kluiven</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Verward door liefde en <rhyme label="b" type="m" corresp="#s7:l2">min</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ik keek byna myn oogen <rhyme label="a" type="m">uit</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En dacht dat is een <rhyme label="b" type="m">klucht</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wie kan zulks ooit <rhyme label="c" type="f">gelooven</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Ja, ja, van  <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Zy ging met hem na <rhyme label="c" type="f" corresp="#s8:l3">boven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En plukte van de <rhyme label="b" type="m" corresp="#s8:l2">vrugt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Zo zyn de Meisjens <rhyme label="a" type="m">hedendaags</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Hoe nee ook <rhyme label="b" type="m">opgetooid</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Zy maken menig <rhyme label="c" type="f">ploertje</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Ja, ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Zo wel als 't slegtste <rhyme label="c" type="f" corresp="#s9:l3">hoertje</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Zyn kop geheel <rhyme label="b" type="m" corresp="#s9:l2">berooid</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Kom Dochters volg d'een <rhyme label="a" type="m">vouwigheid</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En zoek een deugdzaam <rhyme label="b" type="m">Man</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Om in u jonge <rhyme label="c" type="f">jaaren</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Ja, ja, van <rhyme label="d" type="m">falderala</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Gelukkig mee te <rhyme label="c" type="f" corresp="#s10:l3">paaren</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Dan zyt gy 't best 'er <rhyme label="b" type="m" corresp="#s10:l2">an</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
