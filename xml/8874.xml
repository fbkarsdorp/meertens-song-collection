      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">COemt doch hier by, zijt alle <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Singht vrolijck God den <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Want ons Heylant en troost is <rhyme label="a" type="m" corresp="#s1:l1">hy</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dies laet ons <rhyme label="b" type="f" corresp="#s1:l2">Jubileren</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Met Dancken voor syn Aensicht <rhyme label="c" type="m">gaen</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">En Juychen t'syner <rhyme label="d" type="f">Eeren</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Met Psalmen goet end' wel <rhyme label="c" type="m" corresp="#s1:l5">ghedaen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Syn Loff altijt <rhyme label="d" type="f" corresp="#s1:l6">vermeeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De HEEr een Godt en Coninck <rhyme label="a" type="m">is</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Der Goden All' <rhyme label="b" type="f">ghelijcke</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want t'is in syner hant <rhyme label="a" type="m" corresp="#s2:l1">ghewis</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al wat voort brengt <rhyme label="b" type="f" corresp="#s2:l2">t'Aertrijcke</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die hoochsten aller Berghen <rhyme label="c" type="m">fijn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">(Als yeder can <rhyme label="d" type="f">bemercken</rhyme>)</l>
        <l id="s2:l7" met="4" type="-+">Die Zeede en de Droochte <rhyme label="c" type="m" corresp="#s2:l5">zijn</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Al zijnder handen <rhyme label="d" type="f" corresp="#s2:l6">wercken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dies coemt, laet ons aenbidden <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En knielen voor den <rhyme label="b" type="f">HEERE</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Diet al (Wat leven heeft <rhyme label="a" type="m" corresp="#s3:l1">ontfaen</rhyme>)</l>
        <l id="s3:l4" met="3" type="-+">Ghemaeckt heeft end' <rhyme label="b" type="f" corresp="#s3:l2">gheneeret</rhyme>//</l>
        <l id="s3:l5" met="4" type="-+">Want hy is onsen Godt end' <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Barmhertich end' <rhyme label="d" type="f">ghenadich</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">End' wy zijn Volck en Schapen <rhyme label="c" type="m" corresp="#s3:l5">teer</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hy weydet ons <rhyme label="d" type="f" corresp="#s3:l6">ghestaedich</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Soo huyden ghy aenhoort syn <rhyme label="a" type="m">stem</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Verstockt niet uwe <rhyme label="b" type="f">herten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ghelijck te Massa schieden <rhyme label="a" type="m" corresp="#s4:l1">hem</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dijn Vaders sonder <rhyme label="b" type="f" corresp="#s4:l2">scherten</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">My tarchden, end' versochten <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">End' myne wercke <rhyme label="d" type="f">saghen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Doch doolend', op haer Loeden <rhyme label="c" type="m" corresp="#s4:l5">meer</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Straff, Onghenaed' en <rhyme label="d" type="f" corresp="#s4:l6">Plaghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick heb ghehadt wel veertich <rhyme label="a" type="m">Jaer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Veel Arbeydt, Moeyt en <rhyme label="b" type="f">smerte</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Al met dit volck, dat ick seyd' <rhyme label="a" type="m" corresp="#s5:l1">daer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">T'zijn Luyden dien haer <rhyme label="b" type="f" corresp="#s5:l2">herte</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Staech dwalen wil, dies swoer ick, <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">In mynen thoorn, sy <rhyme label="d" type="f">souden</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Tot myner Ruste comen <rhyme label="c" type="m" corresp="#s5:l5">niet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Want sy my niet <rhyme label="d" type="f" corresp="#s5:l6">betrouden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
