  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een droevich liet <rhyme label="a" type="m">sertein</rhyme></l>
        <l id="s1:l2" met="3" type="-+">heb Ick in mijnen <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">van een die Ick <rhyme label="a" type="m" corresp="#s1:l1">mein</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Duer liefde niet <rhyme label="a" type="m" corresp="#s1:l1">gemein</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Wilt dit <rhyme label="c" type="m">voorstaen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">heeft mijn voerlaeten <rhyme label="c" type="m" corresp="#s1:l5">zaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als Ick met haer <rhyme label="a" type="m">voorwaer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">mainde mijn vruecht te <rhyme label="b" type="f">useeren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Alsdan valt sij mijn <rhyme label="a" type="m" corresp="#s2:l1">swaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die Ick bemin <rhyme label="a" type="m" corresp="#s2:l1">eenpaer</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Dees fraey <rhyme label="c" type="m">pijlaer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">wilt mij <rhyme label="c" type="m" corresp="#s2:l5">aebondoeneeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als venes <rhyme label="a" type="m">aemoerues</rhyme></l>
        <l id="s3:l2" met="3" type="-+">So kan sij haer <rhyme label="b" type="f">voorsieren</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Als Junoe <rhyme label="a" type="m" corresp="#s3:l1">graesyues</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Is sij <rhyme label="a" type="m" corresp="#s3:l1">familyues</rhyme></l>
        <l id="s3:l5" met="2" type="-+">haer lipkens <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Soe root als een <rhyme label="c" type="m" corresp="#s3:l5">roebijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">hier toe van leeden <rhyme label="a" type="m">vast</rhyme></l>
        <l id="s4:l2" met="3" type="-+">wie zout hier op <rhyme label="b" type="f">letten</rhyme></l>
        <l id="s4:l3" met="3" type="-+">gelijck als den <rhyme label="a" type="m" corresp="#s4:l1">albast</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Sijn haar tandekens <rhyme label="a" type="m" corresp="#s4:l1">gepast</rhyme></l>
        <l id="s4:l5" met="2" type="-+">haer aesom <rhyme label="c" type="m">ruick</rhyme></l>
        <l id="s4:l6" met="3" type="-+">gelijck als <rhyme label="c" type="m" corresp="#s4:l5">fyoeletten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Terwijl Ick dit <rhyme label="a" type="m">voorhael</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En peins om haerder <rhyme label="b" type="f">taelen</rhyme>?</l>
        <l id="s5:l3" met="3" type="-+">Soe val Ick Int <rhyme label="a" type="m" corresp="#s5:l1">gequel</rhyme></l>
        <l id="s5:l4" met="3" type="-+">want tis mijn groete <rhyme label="a" type="m" corresp="#s5:l1">pijn</rhyme></l>
        <l id="s5:l5" met="2" type="-+">van haer te <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dus Ick bij na <rhyme label="c" type="m" corresp="#s5:l5">voordwin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">haer liefde merckt het <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s6:l2" met="3" type="-+">heeft mijn Ionckhert <rhyme label="b" type="f">bevangen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Dat als sij mij vat <rhyme label="a" type="m" corresp="#s6:l1">rebel</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ick met een fel <rhyme label="a" type="m" corresp="#s6:l1">opstel</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Leef Int <rhyme label="c" type="m">gequel</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Tsal mijn eewich aen <rhyme label="c" type="m" corresp="#s6:l5">hangen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Oerlof prinsesse <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wilt mij genaede <rhyme label="b" type="f">bewijeen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Ick sal sin en <rhyme label="a" type="m" corresp="#s7:l1">gemoet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">stellen in u <rhyme label="a" type="m" corresp="#s7:l1">behoet</rhyme></l>
        <l id="s7:l5" met="2" type="-+">weest dit wel <rhyme label="c" type="m">vroet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dus doet mijn <rhyme label="c" type="m" corresp="#s7:l5">verioelijsen</rhyme>.</l>
      </lg>
    </lg>
  </text>
