<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21458</id>
    <date>
      <year-start>1558</year-start>
      <year-end>1558</year-end>
    </date>
    <location>
      <orginal-name>emden]</orginal-name>
      <standard-name>Emden, Germany</standard-name>
      <coordinates>
        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="passielied" translation="passion song">passielied</tag>
    </tags>
    <melody>Hilf Gott dass mir es gelinge</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HElp God my mach <rhyme label="a" type="f">ghelinghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Du edelen schepper <rhyme label="b" type="m">mijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Den selfsten te <rhyme label="a" type="f" corresp="#s1:l1">volbringhen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ende loue den Heere <rhyme label="b" type="m" corresp="#s1:l2">fijn</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Dat ick mach vrolick heffen <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Uan dynen woorden en lijden te <rhyme label="d" type="f">singen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">O Heere wilt ghy my <rhyme label="c" type="m" corresp="#s1:l5">bystaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">By v woort willen wy <rhyme label="a" type="f">blijuen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Na Esaias doet <rhyme label="b" type="m">vermaen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">In zijn boeck doet <rhyme label="a" type="f" corresp="#s2:l1">schrijuen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Eer die weerelt soude <rhyme label="b" type="m" corresp="#s2:l2">vergaen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Ende wat God oyt voor ons <rhyme label="c" type="m">gewracht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Soude hy dat laten <rhyme label="d" type="f">bederuen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Het waer bouen natuer en <rhyme label="c" type="m" corresp="#s2:l5">kracht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Iesus dat woort des <rhyme label="a" type="f">Uaders</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Is komen sy v <rhyme label="b" type="m">vertelt</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Met grooter wonder <rhyme label="a" type="f" corresp="#s3:l1">daders</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Uerkocht om snoode <rhyme label="b" type="m" corresp="#s3:l2">ghelt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Door Iudam synen Iongheren <rhyme label="c" type="m">een</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wert hy verkocht <rhyme label="d" type="f">verraden</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Iesus dat Lam Gods <rhyme label="c" type="m" corresp="#s3:l5">reen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Na dat sy hadden <rhyme label="a" type="f">ghegheten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met namen dat <rhyme label="b" type="m">Paeschelam</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Christus en wilde niet <rhyme label="a" type="f" corresp="#s4:l1">vergheten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat broot in zijnder hant <rhyme label="b" type="m" corresp="#s4:l2">nam</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Sprack etet het is dat lichaem <rhyme label="c" type="m">mijn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat voor v wert <rhyme label="d" type="f">ghegheuen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Tot verghiffenisse der sonden <rhyme label="c" type="m" corresp="#s4:l5">dijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gaf hen daer oock te <rhyme label="a" type="f">drincken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In wijn zijn bloet so <rhyme label="b" type="m">root</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Sijn doot daer by te <rhyme label="a" type="f" corresp="#s5:l1">ghedincken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Soot is beschreuen <rhyme label="b" type="m" corresp="#s5:l2">bloot</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Wie weerdich etet van dit <rhyme label="c" type="m">broot</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ende van desen kelck mach <rhyme label="d" type="f">drincken</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En sal niet sien den <rhyme label="c" type="m" corresp="#s5:l5">doot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Iesus die wiesch haer <rhyme label="a" type="f">voeten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Als doen ter seluer <rhyme label="b" type="m">stont</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Leerdese niet om <rhyme label="a" type="f" corresp="#s6:l1">versoeten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ut synen Godlicken <rhyme label="b" type="m" corresp="#s6:l2">mont</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Hebt lief malcanderen tot alder <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Daer by salmen <rhyme label="d" type="f">bekennen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dat ghy mijn kinderen <rhyme label="c" type="m" corresp="#s6:l5">zijt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Christus al inder <rhyme label="a" type="f">gaerden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Doen hy ghebeden <rhyme label="b" type="m">hadt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Die Ioden hem daer <rhyme label="a" type="f" corresp="#s7:l1">waerden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Al op den seluen <rhyme label="b" type="m" corresp="#s7:l2">padt</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Ende brachten hem voor den rechter <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Daer hy ghegheesselt ende <rhyme label="d" type="f">ghecroonet</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Ende totter doot veroordeelt <rhyme label="c" type="m" corresp="#s7:l5">waert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Noch aenden cruyce <rhyme label="a" type="f">ghehanghen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die hooghe gheboren <rhyme label="b" type="m">Uorst</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Na ons dede hem <rhyme label="a" type="f" corresp="#s8:l1">verlanghen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Daeromme sprack hy my <rhyme label="b" type="m" corresp="#s8:l2">dorst</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Met name nae onse <rhyme label="c" type="m">salicheyt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Daerom heeft hy <rhyme label="d" type="f">ontfanghen</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Hier opter eerden die <rhyme label="c" type="m" corresp="#s8:l5">menscheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Met synen hoofde <rhyme label="a" type="f">gheneghen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Hy synen gheest op <rhyme label="b" type="m">gaf</rhyme></l>
        <l id="s9:l3" met="3" type="-+">So Ioannes heeft <rhyme label="a" type="f" corresp="#s9:l1">beschreuen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ende wert ghenomen <rhyme label="b" type="m" corresp="#s9:l2">af</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Uanden cruyce int graf wert hy <rhyme label="c" type="m">gheleyt</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Aenden derden dach <rhyme label="d" type="f">opstanden</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Als hy voor hadde <rhyme label="c" type="m" corresp="#s9:l5">gheseyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ende na den seluen <rhyme label="a" type="f">daghen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Christus zijn iongheren heeft <rhyme label="b" type="m">gheleert</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Alleen zijn woort te <rhyme label="a" type="f" corresp="#s10:l1">draghen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Te preken in de weerelt <rhyme label="b" type="m" corresp="#s10:l2">vermeert</rhyme></l>
        <l id="s10:l5" met="4" type="-+">So wie ghelooft, en is <rhyme label="c" type="m">ghedoopt</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Die heeft dat eeuwighe <rhyme label="d" type="f">leuen</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Tis hem door Christum <rhyme label="c" type="m" corresp="#s10:l5">ghecocht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Recht laet ons allen <rhyme label="a" type="f">bidden</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Christus voor <rhyme label="b" type="m">douerheyt</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Wen wy schoon van hen <rhyme label="a" type="f" corresp="#s11:l1">lijden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Ghewalt <rhyme label="b" type="m" corresp="#s11:l2">ellendicheyt</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Dat haer God wil ghenadich <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Heeft Henyrick Molenaer <rhyme label="d" type="f">ghesonghen</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Inder gheuancnis <rhyme label="c" type="m" corresp="#s11:l5">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
