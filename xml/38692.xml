  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">T'is blijschap waer dat Bacchus <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Sijt well-com vrienden al aen desen <rhyme label="a" type="m" corresp="#s1:l1">dis</rhyme>?</l>
        <l id="s1:l3" met="5" type="-+">Want t'is nu heden onse <rhyme label="a" type="m" corresp="#s1:l1">kerremis</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">En wilt niet lanck staen <rhyme label="b" type="f">proncken</rhyme>?</l>
        <l id="s1:l5" met="5" type="-+">Sa lustigh vat den vollen roomer <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die uyt <rhyme label="b" type="f" corresp="#s1:l4">ghedroncken</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Weer vol <rhyme label="b" type="f" corresp="#s1:l4">gheschoncken</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Laet die rondom <rhyme label="c" type="m" corresp="#s1:l5">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
