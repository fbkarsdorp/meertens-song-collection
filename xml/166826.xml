      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Heer t' gulden Iaer, doet toch <rhyme label="a" type="f">verblyden</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die uwen naem, op recht <rhyme label="a" type="f" corresp="#s1:l1">belyden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Breekt hun raetslagen, die met <rhyme label="b" type="m">list</rhyme></l>
        <l id="s1:l4" met="4" type="-+">t' lant soucken te maecken <rhyme label="b" type="m" corresp="#s1:l3">Papist</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Doet haer harten, alzo <rhyme label="c" type="f">verschricken</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Als herodes, die <rhyme label="c" type="f" corresp="#s1:l5">heymelycken</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Leert, hoe de ster verschynd en <rhyme label="d" type="m">waer</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Gheleden zestienhondert <rhyme label="d" type="m" corresp="#s1:l7">Iaer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In Babel dus langhe <rhyme label="a" type="f">geseeten</rhyme></l>
        <l id="s2:l2" met="4" type="-+">O heer verlost, en doet nijt <rhyme label="a" type="f" corresp="#s2:l1">meeten</rhyme></l>
        <l id="s2:l3" met="4" type="-+">u groote wonderlijcke <rhyme label="b" type="m">cracht</rhyme></l>
        <l id="s2:l4" met="4" type="-+">tot Bethliem geschiet <rhyme label="b" type="m" corresp="#s2:l3">kersnacht</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Doen de harders al opten <rhyme label="c" type="f">velde</rhyme></l>
        <l id="s2:l6" met="4" type="-+">u daden groot, den volk <rhyme label="c" type="f" corresp="#s2:l5">vertelden</rhyme></l>
        <l id="s2:l7" met="4" type="-+">hoe t' licht gods scheen, op daerde <rhyme label="d" type="m">claer</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Gheleeden zestien hondert <rhyme label="d" type="m" corresp="#s2:l7">Iaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghepropheteert was lang te <rhyme label="a" type="f">vooren</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Een tyt vervult, gods zoon <rhyme label="a" type="f" corresp="#s3:l1">gebooren</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Tot Bethliem, Int Iootsche <rhyme label="b" type="m">lant</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Daer de wijze, met <rhyme label="b" type="m" corresp="#s3:l3">offerhandt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Quamen om Christi te <rhyme label="c" type="f">aenbeden</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Daer hun de ster van oosten <rhyme label="c" type="f" corresp="#s3:l5">heden</rhyme></l>
        <l id="s3:l7" met="4" type="-+">waren verblyt, bij t' kint <rhyme label="d" type="m">aldaer</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Gheleden zestienhondert <rhyme label="d" type="m" corresp="#s3:l7">Iaer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Engel deed, t' bevel des <rhyme label="a" type="f">heeren</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Liet Ioseph na, Egipten <rhyme label="a" type="f" corresp="#s4:l1">keeren</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Met Maria, en t' godtlyck <rhyme label="b" type="m">zaet</rhyme></l>
        <l id="s4:l4" met="4" type="-+">waer op herodes, had den <rhyme label="b" type="m" corresp="#s4:l3">haet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">syn In Egipten lant <rhyme label="c" type="f">gebleven</rhyme></l>
        <l id="s4:l6" met="4" type="-+">tot was, vervult dat staet <rhyme label="c" type="f" corresp="#s4:l5">geschreven</rhyme></l>
        <l id="s4:l7" met="4" type="-+">dits mynen zoon, en <rhyme label="d" type="m">middelaer</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Gheleden zestien hondert <rhyme label="d" type="m" corresp="#s4:l7">Iaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wilt desen prins, dancken en <rhyme label="a" type="f">loven</rhyme></l>
        <l id="s5:l2" met="4" type="-+">die deur zyn cracht, alleen van <rhyme label="a" type="f" corresp="#s5:l1">boven</rhyme></l>
        <l id="s5:l3" met="4" type="-+">zijnen zoon heeft, neder <rhyme label="b" type="m">gesent</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Waer In dat Lieffd'es <rhyme label="b" type="m" corresp="#s5:l3">tfondament</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Tot Danckbaerheyt wilt u <rhyme label="c" type="f">bereijen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">zo wit, en schoon, als <rhyme label="c" type="f" corresp="#s5:l5">accoleijen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">pryst syn geboorte voor en <rhyme label="d" type="m">naer</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Gheleden zestien hondert <rhyme label="d" type="m" corresp="#s5:l7">Iaer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
