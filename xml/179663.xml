<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179663</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <tags/>
    <melody>Laatstmaal zat ik op een hek</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ACh Moeder onze <rhyme label="a" type="m">Turfboer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die van Nieuwerker <rhyme label="a" type="m" corresp="#s1:l1">voer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In Zeeland kwam hy te <rhyme label="b" type="f">landen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En daer dronk hy zyn gat zo <rhyme label="c" type="m">vol</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Het was voor hem geen <rhyme label="b" type="f" corresp="#s1:l3">schanden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als hy in Zeeland binnen <rhyme label="a" type="m">kwam</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Sprak hy de Waerdinne <rhyme label="a" type="m" corresp="#s2:l1">an</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Hy sprak myn lief <rhyme label="b" type="f">geprezen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Laten wy spelen ’t spel der <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ik schop u met myn <rhyme label="b" type="f" corresp="#s2:l3">voeten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Waerdin sprak heel <rhyme label="a" type="m">verstoord</rhyme>;</l>
        <l id="s3:l2" met="4" type="-+">’k Had liever dat u stak de <rhyme label="a" type="m" corresp="#s3:l1">moord</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Eer ik u lust zou <rhyme label="b" type="f">boeten</rhyme>;</l>
        <l id="s3:l4" met="4" type="-+">En dat voor een zak met <rhyme label="c" type="m">Geld</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Ik schop u met myn <rhyme label="b" type="f" corresp="#s3:l3">voeten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Deze Boer kreeg quaed <rhyme label="a" type="m">bescheid</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En ging aenstonds naer de <rhyme label="a" type="m" corresp="#s4:l1">Meid</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Hy ging het Meisje <rhyme label="b" type="f">vragen</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Of zy voor een zak met <rhyme label="c" type="m">Geld</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Haer Magedom wou <rhyme label="b" type="f" corresp="#s4:l3">wagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Deze Meid sprak heel <rhyme label="a" type="m">geswind</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Als ik dan eens raek met <rhyme label="a" type="m" corresp="#s5:l1">kind</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Sult gy myn kraem <rhyme label="b" type="f">uithouwen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Dan zal ik van stonden <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Een Turf boer laten <rhyme label="b" type="f" corresp="#s5:l3">bouwen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Deze boer die ging zyn <rhyme label="a" type="m">gank</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Van ’t bed op de <rhyme label="a" type="m" corresp="#s6:l1">voetebank</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hy toonden zyn <rhyme label="b" type="f">kordaten</rhyme></l>
        <l id="s6:l4" met="4" type="-+">So lang tot hy niet meer en <rhyme label="c" type="m">kon</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">De voetebank die <rhyme label="b" type="f" corresp="#s6:l3">kraekten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Negen maenden en een <rhyme label="a" type="m">dag</rhyme>;</l>
        <l id="s7:l2" met="4" type="-+">Het Meisje in het kraem bed <rhyme label="a" type="m" corresp="#s7:l1">lag</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Wat is ’er voort <rhyme label="b" type="f">gekomen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Eenen jonge <rhyme label="c" type="m">Turfboer</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Sy heeft hem <rhyme label="b" type="f" corresp="#s7:l3">waergenomen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Oorlof Boeren tot <rhyme label="a" type="m">besluyt</rhyme>;</l>
        <l id="s8:l2" met="4" type="-+">Als gy vaert met de <rhyme label="a" type="m" corresp="#s8:l1">Turfschuyt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En laet u zoo niet <rhyme label="b" type="f">loere</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Of anders raekt u geld en <rhyme label="c" type="m">Schip</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Na de duyvelze <rhyme label="b" type="f" corresp="#s8:l3">hoeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
