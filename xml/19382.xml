    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Naar oostland wil ik varen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SIet en smaect en wilt Gods cracht <rhyme label="a" type="f">beseffen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Die na zijn beelde gescapen <rhyme label="b" type="m">zijt</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Wilt v swaermoedich hooft op <rhyme label="a" type="f" corresp="#s1:l1">heffen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Waect op want tis nv meer dan <rhyme label="b" type="m" corresp="#s1:l2">tijt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">En hoort toe alle <rhyme label="c" type="f">gader</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Nae een blyde maer <rhyme label="d" type="m">playsant</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die God den hemelschen <rhyme label="c" type="f" corresp="#s1:l5">Uader</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Door Christum ons eenich <rhyme label="c" type="f" corresp="#s1:l5">berader</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Uan bouen heeft neder <rhyme label="d" type="m" corresp="#s1:l6">ghesant</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Hier in dit eertsche <rhyme label="d" type="m" corresp="#s1:l6">lant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy ouden Adam ghy moet <rhyme label="a" type="f">steruen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Uerloren hebt ghy v eerste <rhyme label="b" type="m">rijck</rhyme></l>
        <l id="s2:l3" met="4" type="-+">U overtreden dede v dat <rhyme label="a" type="f" corresp="#s2:l1">verweruen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Ghy zijt eertsch al vander eerden <rhyme label="b" type="m" corresp="#s2:l2">slijck</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Den nieuwen Adam is <rhyme label="c" type="f">ghecomen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Hemelsch den Heere selfs dit <rhyme label="d" type="m">ghelooft</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Den ouden Adam heeft hy <rhyme label="c" type="f" corresp="#s2:l5">benomen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Sijn cracht, ende als den <rhyme label="c" type="f" corresp="#s2:l5">vromen</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Uertreden der slanghen <rhyme label="d" type="m" corresp="#s2:l6">hooft</rhyme></l>
        <l id="s2:l10" met="3" type="-+">En al haer macht <rhyme label="d" type="m" corresp="#s2:l6">berooft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den waren Noach is comen <rhyme label="a" type="f">bouwen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Sijn Arcke, den inganck en es niet <rhyme label="b" type="m">wijt</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Trect tot hem met een vast <rhyme label="a" type="f" corresp="#s3:l1">betrouwen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Ghy wert van allen drue en noot <rhyme label="b" type="m" corresp="#s3:l2">beurijt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Ghy moet treden sonder <rhyme label="c" type="f">sneuen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">In dees arcke wilt ghy <rhyme label="d" type="m">ontgaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Den straf, want daer en es <rhyme label="c" type="f" corresp="#s3:l5">ghegeuen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Gheen salicheyt, dan die met Noe <rhyme label="c" type="f" corresp="#s3:l5">leuen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">In die arcke, nae Petrus <rhyme label="d" type="m" corresp="#s3:l6">vermaen</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Wilt dit voorbeelt wel <rhyme label="d" type="m" corresp="#s3:l6">verstaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den waren Isaac <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Is ghecomen, zijn offer heeft hy <rhyme label="b" type="m">ghedaen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Uerworpen es Esaus rouwe <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Iacob die heeft den zeghen <rhyme label="b" type="m" corresp="#s4:l2">ontfaen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Uan Silo is ghecomen wilt <rhyme label="c" type="f">hooren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Den Hertoghe soot was <rhyme label="d" type="m">vermelt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Iuda heeft sinen septer <rhyme label="c" type="f" corresp="#s4:l5">verloren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ontwaect en merct ghy <rhyme label="c" type="f" corresp="#s4:l5">wtuercoren</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Den dach is nv <rhyme label="d" type="m" corresp="#s4:l6">bestelt</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Die een yeghelijck nv <rhyme label="d" type="m" corresp="#s4:l6">ghelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Israel en leeft niet meer soo <rhyme label="a" type="f">banghe</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Uerladighet v met hemels <rhyme label="b" type="m">broot</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Siet aen de metalen <rhyme label="a" type="f" corresp="#s5:l1">Slanghe</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Sy heelt v van den bete en <rhyme label="b" type="m" corresp="#s5:l2">doot</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Wilt oock vanden water <rhyme label="c" type="f">drincken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dwelck springt wt Christum den <rhyme label="d" type="m">steen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Aenschout sonder <rhyme label="c" type="f" corresp="#s5:l5">vermincken</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die druyue die v God wil <rhyme label="c" type="f" corresp="#s5:l5">schincken</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Sy coempt wt beloofde lant <rhyme label="d" type="m" corresp="#s5:l6">reen</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Costelijcker en vintmen <rhyme label="d" type="m" corresp="#s5:l6">gheen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Israel wilt met neersticheyt <rhyme label="a" type="f">treden</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Door die enghe poorte int beloofde <rhyme label="b" type="m">lant</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Daer toe sterck ons die Gods des <rhyme label="a" type="f" corresp="#s6:l1">vreden</rhyme></l>
        <l id="s6:l4" met="4" type="-+">En leyde ons met zijn stercke <rhyme label="b" type="m" corresp="#s6:l2">hant</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Roept voor Iericho sonder <rhyme label="c" type="f">trueren</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dwelcke Babilon is <rhyme label="d" type="m">bekent</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Huer macht die sal heel <rhyme label="c" type="f" corresp="#s6:l5">schueren</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Uallen sullen oock haer <rhyme label="c" type="f" corresp="#s6:l5">mueren</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Quaet is haer <rhyme label="d" type="m" corresp="#s6:l6">fondament</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Haest soo sal comen haer <rhyme label="d" type="m" corresp="#s6:l6">ent</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Den ghesalfden Coninck en <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Is ghecomen, Saul heeft verloren zijn <rhyme label="b" type="m">croon</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Golias is berooft van zijn <rhyme label="a" type="f" corresp="#s7:l1">eere</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Met zijn selfs sweert gaf Dauid hem sinen <rhyme label="b" type="m" corresp="#s7:l2">loon</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Die Philisteen moeten <rhyme label="c" type="f">wijcken</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Wt dat beloofde lant <rhyme label="d" type="m">fijn</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Des Lams macht en sal niet <rhyme label="c" type="f" corresp="#s7:l5">beswijcken</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Uerwinnen salt alle <rhyme label="c" type="f" corresp="#s7:l5">rijcken</rhyme></l>
        <l id="s7:l9" met="3" type="-+">En verlossen wt alle <rhyme label="d" type="m" corresp="#s7:l6">ghepijn</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Die in hem gheloouende <rhyme label="d" type="m" corresp="#s7:l6">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
