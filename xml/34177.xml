  <author>Petrus Suetmans, Eustachius de Meyer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Sult ghy myn <rhyme label="a" type="f">klachten</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Kleywicht noot <rhyme label="a" type="f" corresp="#s1:l1">versachten</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Wilt ghy't noch <rhyme label="a" type="f" corresp="#s1:l1">verachten</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Wreeden minnen <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hoe strangh is u <rhyme label="b" type="m" corresp="#s1:l4">ghebodt</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Ghy doet my <rhyme label="c" type="f">suchten</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">En myn blyschap <rhyme label="c" type="f" corresp="#s1:l6">vluchten</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Als ghy <rhyme label="d" type="m">verwoet</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">My sulcken on-recht <rhyme label="d" type="m" corresp="#s1:l8">doet</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Als ghy my, en meer <rhyme label="e" type="f">menschen</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Naer weder-min doet <rhyme label="e" type="f" corresp="#s1:l10">wenschen</rhyme>.</l>
        <l id="s1:l12" met="3" type="-+">Dan perst gy uyt ons <rhyme label="d" type="m" corresp="#s1:l8">bloet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wat wil ick <rhyme label="a" type="f">suchten</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Dat zyn u <rhyme label="a" type="f" corresp="#s2:l1">ghenuchten</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En de beste <rhyme label="a" type="f" corresp="#s2:l1">vruchten</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Die ghy 'tmeeste <rhyme label="b" type="m">acht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Te spotten met ons <rhyme label="b" type="m" corresp="#s2:l4">klacht</rhyme></l>
        <l id="s2:l6" met="2" type="-+">d'Welck tuyght de <rhyme label="c" type="f">Sonne</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Die ghy hebt <rhyme label="c" type="f" corresp="#s2:l6">verwonne</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En teghen <rhyme label="d" type="m">recht</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Hem achte als u <rhyme label="d" type="m" corresp="#s2:l8">knecht</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Haeter van mensch, en <rhyme label="e" type="f">Goden</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Door u wree <rhyme label="e" type="f" corresp="#s2:l10">gheboden</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Is uwen lof seer <rhyme label="d" type="m" corresp="#s2:l8">slecht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">'tIs waer myn <rhyme label="a" type="f">schelden</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Can geen voor-spoet <rhyme label="a" type="f" corresp="#s3:l1">melden</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Maer myn hert <rhyme label="a" type="f" corresp="#s3:l1">ontstelden</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Als het <rhyme label="b" type="m">over-denckt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hoe dat ghy ieder <rhyme label="b" type="m" corresp="#s3:l4">krenckt</rhyme></l>
        <l id="s3:l6" met="2" type="-+">U eyghen <rhyme label="c" type="f">moeder</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Waer voor ghy <rhyme label="c" type="f" corresp="#s3:l6">behoeder</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">'bHoorden te <rhyme label="d" type="m">zyn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">'tGheniet oock van u <rhyme label="d" type="m" corresp="#s3:l8">pyn</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Met Mars wort sy <rhyme label="e" type="f">bevonnen</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Door uwen brant <rhyme label="e" type="f" corresp="#s3:l10">verwonnen</rhyme></l>
        <l id="s3:l12" met="3" type="-+">d'Welck Phaebus broght in <rhyme label="d" type="m" corresp="#s3:l8">scheyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Hebt ghy <rhyme label="a" type="f">verkregen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Sulcken amp te <rhyme label="a" type="f" corresp="#s4:l1">pleghen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Ieder is u <rhyme label="a" type="f" corresp="#s4:l1">teghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Jonckheyt niet <rhyme label="b" type="m">verstaet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hoe 'tmet een Minnaer <rhyme label="b" type="m" corresp="#s4:l4">gaet</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Waer't noch te <rhyme label="c" type="f">kiesen</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Licht soud' ghy't <rhyme label="c" type="f" corresp="#s4:l6">verliesen</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Want sulck <rhyme label="d" type="m">bevel</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En is gheen <rhyme label="d" type="m" corresp="#s4:l8">kinder-spel</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Een mensch syn hert te <rhyme label="e" type="f">stelen</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Waer door hy moet <rhyme label="e" type="f" corresp="#s4:l10">quelen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Voeght meerder wysheyt <rhyme label="d" type="m" corresp="#s4:l8">wel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Men sou u <rhyme label="a" type="f">stellen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Aen u sluyder <rhyme label="a" type="f" corresp="#s5:l1">bellen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Loopt dan ieder <rhyme label="a" type="f" corresp="#s5:l1">quellen</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">U boogh met een <rhyme label="b" type="m">blaes</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En maeckt daer mé <rhyme label="b" type="m" corresp="#s5:l4">geraes</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Daer ghy sult <rhyme label="c" type="f">kommen</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Kunt ghy u <rhyme label="c" type="f" corresp="#s5:l6">berommen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">O, weldigh <rhyme label="d" type="m">wicht</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Van eenigh liedt, oft <rhyme label="d" type="m" corresp="#s5:l8">dicht</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Wilt u daer mé <rhyme label="e" type="f">genoegen</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Dat sal u beter <rhyme label="e" type="f" corresp="#s5:l10">voegen</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Laet vaeren uwen <rhyme label="d" type="m" corresp="#s5:l8">schicht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
