    <melody>O zalig heilig Bethlehem</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Salich Heylich <rhyme label="a" type="m">Bethleem</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Onder duysent uyt <rhyme label="b" type="f">verkoren</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Vereert boven <rhyme label="a" type="m" corresp="#s1:l1">Hierusalem</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Want Jesus is in u <rhyme label="b" type="f" corresp="#s1:l2">gheboren</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Vereert boven <rhyme label="a" type="m" corresp="#s1:l1">Hierusalem</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Want Jesus is in u <rhyme label="b" type="f" corresp="#s1:l2">gheboren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Bethleem cleyn groote <rhyme label="a" type="m">stadt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Kleyn van begrijp, maer groot van <rhyme label="b" type="f">weerden</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Ghy zijt dat alder-eelste <rhyme label="a" type="m" corresp="#s2:l1">vat</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En d'alder rijckste stadt der <rhyme label="b" type="f" corresp="#s2:l2">eerden</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Ghy zijt dat alder-eelste <rhyme label="a" type="m" corresp="#s2:l1">vat</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En d'alder rijckste stadt der <rhyme label="b" type="f" corresp="#s2:l2">eerden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Verheught u dan o <rhyme label="a" type="m">Israel</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hoe mochtmen u blijder bootschap <rhyme label="b" type="f">bringhen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Tot u soo komt <rhyme label="a" type="m" corresp="#s3:l1">Emanuel</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wilt uyt der sonden slaep <rhyme label="b" type="f" corresp="#s3:l2">ontspringen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Tot u soo komt <rhyme label="a" type="m" corresp="#s3:l1">Emanuel</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Wilt uyt der sonden slaep <rhyme label="b" type="f" corresp="#s3:l2">ontspringen</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Koninck Christe Prince <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hoe wort ghy hier aldus <rhyme label="b" type="f">ghebonden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">In hoy, in stroo in sulcker <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">In arme doecxkens teer <rhyme label="b" type="f" corresp="#s4:l2">ghewonden</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">In hoy, in stroo in sulcker <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">In arme doecxkens teer <rhyme label="b" type="f" corresp="#s4:l2">ghewonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy hebt het firmament <rhyme label="a" type="m">gemaeckt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Al waer u loven s'hemels <rhyme label="b" type="f">gheesten</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Maer nu gheheel bloot ende <rhyme label="a" type="m" corresp="#s5:l1">naeckt</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Light ghy in't midden vande <rhyme label="b" type="f" corresp="#s5:l2">beesten</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Maer nu gheheel bloot ende <rhyme label="a" type="m" corresp="#s5:l1">naeckt</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Light ghy in't midden vande <rhyme label="b" type="f" corresp="#s5:l2">beesten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy wordt gheboren in een <rhyme label="a" type="m">stal</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Niemant bekent inden nacht <rhyme label="b" type="f">stille</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Maer d'Enghels singhen over <rhyme label="a" type="m" corresp="#s6:l1">al</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Peys zy den mensch van goeden <rhyme label="b" type="f" corresp="#s6:l2">wille</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Maer d'Enghels singhen over <rhyme label="a" type="m" corresp="#s6:l1">al</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Peys zy den mensch van goeden <rhyme label="b" type="f" corresp="#s6:l2">wille</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O machtich Godt, o Jesu <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Wat liefd' heeft u daer <rhyme label="b" type="f">ghetrocken</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Dat ghy aenneemt ons vleesch en <rhyme label="a" type="m" corresp="#s7:l1">bloet</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Om ons tot u alsoo te <rhyme label="b" type="f" corresp="#s7:l2">locken</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Dat ghy aenneemt ons vleesch en <rhyme label="a" type="m" corresp="#s7:l1">bloet</rhyme>.</l>
        <l id="s7:l6" met="4" type="-+">Om ons tot u alsoo te <rhyme label="b" type="f" corresp="#s7:l2">locken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Komt tot dit kint ghy Adams <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Hoe kont ghy noch de werelt <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Siet hoe Jesus hem met u <rhyme label="a" type="m" corresp="#s8:l1">bint</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Offert hem heel u hert en <rhyme label="b" type="f" corresp="#s8:l2">sinnen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Siet hoe Jesus hem met u <rhyme label="a" type="m" corresp="#s8:l1">bint</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Offert hem heel u hert en <rhyme label="b" type="f" corresp="#s8:l2">sinnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
