    <melody>Lass nur der Politiker sprechen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Hoe dan zou gy myn <rhyme label="a" type="f">verlaaten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die tot u zo ben <rhyme label="b" type="m">gezind</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Hoe zou gy myn kunne <rhyme label="a" type="f" corresp="#s1:l1">haaten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dien gy altoos heb <rhyme label="b" type="m" corresp="#s1:l2">bemind</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ziet uw droevig meisje <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Ziet my weenend voor u <rhyme label="c" type="m" corresp="#s1:l5">staan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ach hoe zoet hoe menig <rhyme label="a" type="f">werven</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Hebt gy met een slim <rhyme label="b" type="m">beleid</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Op uw leeven en uw <rhyme label="a" type="f" corresp="#s2:l1">sterven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Eeuwig liefde <rhyme label="b" type="m" corresp="#s2:l2">toegezeid</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Geef myn dan uw hart en <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Voor myn eer tot <rhyme label="c" type="m" corresp="#s2:l5">onderpand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Eeuwig hebt gy trouw <rhyme label="a" type="f">gezwooren</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Hooger als ik zeggen <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Toen heb ik u <rhyme label="a" type="f" corresp="#s3:l1">uitverkooren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Voor myn beste vriend en <rhyme label="b" type="m" corresp="#s3:l2">man</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ach laat my niet in de <rhyme label="c" type="m">rouw</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Maakt my tot uw echten <rhyme label="c" type="m" corresp="#s3:l5">Vrouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Neen ik kon het niet <rhyme label="a" type="f">gelooven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Toen myn hart zat in de <rhyme label="b" type="m">rouw</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat als gy myn eer zou <rhyme label="a" type="f" corresp="#s4:l1">rooven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Gy myn dan verlaaten <rhyme label="b" type="m" corresp="#s4:l2">zouw</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Heb nu <rhyme label="c" type="m">medendogendheid</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Met myn dien gy hebt <rhyme label="c" type="m" corresp="#s4:l5">verleid</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Kan myn klagt u niet <rhyme label="a" type="f">beweegen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Laat dan myn vrugt u eigen <rhyme label="b" type="m">kind</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Dien ik by u hebt <rhyme label="a" type="f" corresp="#s5:l1">gekreegen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Toen gy myn nog waart <rhyme label="b" type="m" corresp="#s5:l2">gesint</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Eert my nu ik zit in <rhyme label="c" type="m">druk</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Red myn uit myn <rhyme label="c" type="m" corresp="#s5:l5">ongeluk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ja dit kind heb ik <rhyme label="a" type="f">ontfangen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Van u uit liefde valsche <rhyme label="b" type="m">vriend</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Toen gy met een groot <rhyme label="a" type="f" corresp="#s6:l1">verlangen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Van myn eer moest zyn <rhyme label="b" type="m" corresp="#s6:l2">gediend</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Welk met myn thans teder <rhyme label="c" type="m">schreid</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Om u <rhyme label="c" type="m" corresp="#s6:l5">ontstandvastigheid</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ik zie myn klagt kan u niet <rhyme label="a" type="f">breken</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Vaart dan wel in <rhyme label="b" type="m">eeuwigheid</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Den hemel zal myn onschuld <rhyme label="a" type="f" corresp="#s7:l1">wreeken</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Kom lief kind hier niet <rhyme label="b" type="m" corresp="#s7:l2">verblyd</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">U vader heeft een hart van <rhyme label="c" type="m">steen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Is doof voor zugten en <rhyme label="c" type="m" corresp="#s7:l5">geween</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
