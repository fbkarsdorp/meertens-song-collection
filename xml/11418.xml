    <melody>Posso cor mio partire</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GOddin u bly'e <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die hebben my <rhyme label="a" type="f" corresp="#s1:l1">bedroogen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Mijn hart, mijn hart hebt ghy <rhyme label="b" type="f">ontdragen</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">En door een soete toovery <rhyme label="b" type="f" corresp="#s1:l3">verslagen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">u bly'e soete <rhyme label="c" type="f">lonken</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Doen, doen myn ziel, doen, doen myn <rhyme label="d" type="m">ziel</rhyme></l>
        <l id="s1:l7" met="2" type="+-">gloeyend' <rhyme label="c" type="f" corresp="#s1:l5">voncken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Goddin! u klare <rhyme label="a" type="f">stralen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die doen mijn lust <rhyme label="a" type="f" corresp="#s2:l1">verdwalen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Mijn jeughd, mijn jeughd houd ghy <rhyme label="b" type="f">gebonden</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">En in Cupidoos net omstrickt, <rhyme label="b" type="f" corresp="#s2:l3">omwonden</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">O bly'e <rhyme label="c" type="f">slavernye</rhyme>!</l>
        <l id="s2:l6" met="4" type="-+">Die, Die de Go'on, die, die de <rhyme label="d" type="m">Go'on</rhyme></l>
        <l id="s2:l7" met="2" type="+-">My <rhyme label="c" type="f" corresp="#s2:l5">beny'en</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Geluckigh is 't om <rhyme label="a" type="f">branden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">V tot een <rhyme label="a" type="f" corresp="#s3:l1">offerhande</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die ghy, die ghy selfs gaet <rhyme label="b" type="f">ontsteken</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Met het vyer dat uyt u ooghjens <rhyme label="b" type="f" corresp="#s3:l3">breken</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">O wel geluckigh <rhyme label="c" type="f">sterven</rhyme>!</l>
        <l id="s3:l6" met="4" type="-+">Wie, wie het mach, wie, wie het <rhyme label="d" type="m">mach</rhyme></l>
        <l id="s3:l7" met="2" type="+-">Maer <rhyme label="c" type="f" corresp="#s3:l5">be-erven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wie souw de doodt doch <rhyme label="a" type="f">vreesen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Om staegh by u te <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">O Schoon! o schoone <rhyme label="b" type="f">zonne-lichten</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Die in myn Nymph' als twee sterren <rhyme label="b" type="f" corresp="#s4:l3">lichten</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Kom laet u strael my <rhyme label="c" type="f">dooden</rhyme></l>
        <l id="s4:l6" met="4" type="-+">So, so ben ick, so, so ben <rhyme label="d" type="m">ick</rhyme></l>
        <l id="s4:l7" met="2" type="+-">Als de <rhyme label="c" type="f" corresp="#s4:l5">Goden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
