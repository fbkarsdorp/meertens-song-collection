  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Nu luystert <rhyme label="a" type="f">Papouwen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Die 't Speck eten <rhyme label="a" type="f" corresp="#s1:l1">schouwen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Op uwen verboden <rhyme label="b" type="m">tijd</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Het Speck dat wort <rhyme label="c" type="f">geslagen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">'T is nu in de <rhyme label="c" type="f" corresp="#s1:l4">dagen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat hem de Geus-neef <rhyme label="b" type="m" corresp="#s1:l3">verblijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Anthony d' <rhyme label="a" type="f">Oquende</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Een heel <rhyme label="a" type="f" corresp="#s2:l1">onbekende</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Quamen uyt het Speckse <rhyme label="b" type="m">Lande</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Na pelsen en <rhyme label="c" type="f">poorten</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Al sulcke <rhyme label="c" type="f" corresp="#s2:l4">soorten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die waren ons <rhyme label="b" type="m" corresp="#s2:l3">onbekant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Jst niet om te <rhyme label="a" type="f">gecken</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">By t'seventigh <rhyme label="a" type="f" corresp="#s3:l1">Specken</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Soo schepen als <rhyme label="b" type="m">Galjoens</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Die mosten gaen <rhyme label="c" type="f">loopen</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Wat schande met <rhyme label="c" type="f" corresp="#s3:l4">hoopen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">'t Js niet met al <rhyme label="b" type="m" corresp="#s3:l3">fatsoens</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wat dunckt u <rhyme label="a" type="f">Jnfante</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Van dese <rhyme label="a" type="f" corresp="#s4:l1">Gesante</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Js dit niet wat te <rhyme label="b" type="m">snoo</rhyme>?</l>
        <l id="s4:l4" met="2" type="-+">Dat sy gingen <rhyme label="c" type="f">strijcken</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Soo lichtelijck <rhyme label="c" type="f" corresp="#s4:l4">wijcken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Heeft dit wel by <rhyme label="b" type="m" corresp="#s4:l3">Kallo</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Gaet klaeght nu in <rhyme label="a" type="f">Spanje</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Hoe dat u <rhyme label="a" type="f" corresp="#s5:l1">Oraenje</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Soo veel nu geeft te <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Gaet klaeghet jou <rhyme label="c" type="f">Broertje</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">En loopt na jou <rhyme label="c" type="f" corresp="#s5:l4">Moertje</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Laet haer u scha <rhyme label="b" type="m" corresp="#s5:l3">vergoen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">U <rhyme label="a" type="f">Jnfanterijen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Sijn niet te <rhyme label="a" type="f" corresp="#s6:l1">bedijen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">'t Js wel maer ydel <rhyme label="b" type="m">wint</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Of laet haer <rhyme label="c" type="f">gewennen</rhyme></l>
        <l id="s6:l5" met="2" type="-+">De zee eerst te <rhyme label="c" type="f" corresp="#s6:l4">kennen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Versint eer ghy <rhyme label="b" type="m" corresp="#s6:l3">begint</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Den Paus uwen <rhyme label="a" type="f">Vader</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Meer roemer dan <rhyme label="a" type="f" corresp="#s7:l1">dader</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Aenroept hem tot <rhyme label="b" type="m">bystant</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Laet hem vry mee <rhyme label="c" type="f">komen</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Ontbiet hem van <rhyme label="c" type="f" corresp="#s7:l4">Roomen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Wy brengen hem in ons <rhyme label="b" type="m" corresp="#s7:l3">Lant</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Wat dunckt u <rhyme label="a" type="f">Papisten</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Van der Geusen <rhyme label="a" type="f" corresp="#s8:l1">listen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sy breken 't hout <rhyme label="b" type="m">Geloof</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Wanneer ghy gaet <rhyme label="c" type="f">mercken</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Door Godt onse <rhyme label="c" type="f" corresp="#s8:l4">wercken</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Den Paus is maer een <rhyme label="b" type="m" corresp="#s8:l3">Sloof</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Oorlof Mans en <rhyme label="a" type="f">Vrouwen</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Op God gaet <rhyme label="a" type="f" corresp="#s9:l1">betrouwen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Geen bemodderde <rhyme label="b" type="m">Kerck</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Wy sien <rhyme label="c" type="f">openbare</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">En al de dagh <rhyme label="c" type="f" corresp="#s9:l4">klare</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dat God is met ons <rhyme label="b" type="m" corresp="#s9:l3">werck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
