    <melody>WÃ¤r Gott nicht mit uns diese Zeit</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SO Godt de HEER niet by ons <rhyme label="a" type="m">halt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Als onse Vyandt <rhyme label="b" type="f">woedet</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">End onser Saecken niet toe <rhyme label="a" type="m" corresp="#s1:l1">valt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Van Hemel hoogh <rhyme label="b" type="f" corresp="#s1:l2">behoedet</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">So hy Israel Schutz niet <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">End selver breeckt des Vyandts <rhyme label="c" type="m" corresp="#s1:l5">List</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">So ist met ons <rhyme label="d" type="f">verlooren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat Menschen Macht end sin <rhyme label="a" type="m">Aenvaert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Sal ons billick niet <rhyme label="b" type="f">schrecken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy sitt so hooch al boven <rhyme label="a" type="m" corresp="#s2:l1">d'Aerd</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die sal haer Raedt Op <rhyme label="b" type="f" corresp="#s2:l2">decken</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wann' zijt opt Cloeckste grijpen <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">So gaet doch Godt een ander <rhyme label="c" type="m" corresp="#s2:l5">Baen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Het staet in synen <rhyme label="d" type="f">Handen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy Woeden vast end Tieren <rhyme label="a" type="m">seer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Als wouden sy ons <rhyme label="b" type="f">Eeten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Te Worghen staet al haer <rhyme label="a" type="m" corresp="#s3:l1">begeer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Godt is by haer <rhyme label="b" type="f" corresp="#s3:l2">vergeeten</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Als Meyres Baren Onstuyr <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Nae Lijff end Leven sy ons <rhyme label="c" type="m" corresp="#s3:l5">staen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Des sal hem Godt <rhyme label="d" type="f">Ontfarmen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy stellen ons als Ketters <rhyme label="a" type="m">nae</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Nae onsen Bloet sy <rhyme label="b" type="f">trachten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Noch Roemen sy haer Christen, <rhyme label="a" type="m" corresp="#s4:l1">Jae</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die Godt alleen groot <rhyme label="b" type="f" corresp="#s4:l2">Achten</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Ach Godt die duyre Name <rhyme label="c" type="m">dijn</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Moet haerder schalcheyts Decksel <rhyme label="c" type="m" corresp="#s4:l5">zijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ghy sult eenmael op <rhyme label="d" type="f">Waecken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Op spalcken sy die Kaecken <rhyme label="a" type="m">wijt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">End willen ons <rhyme label="b" type="f">opslucken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Loff end Danck sy GOdt all <rhyme label="a" type="m" corresp="#s5:l1">tijt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Het sal haer niet <rhyme label="b" type="f" corresp="#s5:l2">ghelucken</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Hy sal haer strick verscheuren <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">En storten hare valsche <rhyme label="c" type="m" corresp="#s5:l5">Leer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Sy sullent Godt niet <rhyme label="d" type="f">weeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ach HEEre Godt hoe Rijck troost <rhyme label="a" type="m">ghy</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die gantslijck syn <rhyme label="b" type="f">verlaten</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Der g'naden Deur staet altoos <rhyme label="a" type="m" corresp="#s6:l1">vry</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vernufft can dat niet <rhyme label="b" type="f" corresp="#s6:l2">Vaten</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Sy spreeckt: Het is nu al <rhyme label="c" type="m">verloor'n</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Daer doch dat Cruys heeft Nieuw <rhyme label="c" type="m" corresp="#s6:l5">gheboorn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die dijner Hulp <rhyme label="d" type="f">verwachten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die Vyand' syn in dijner <rhyme label="a" type="m">Handt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Daer toe all' haer <rhyme label="b" type="f">Ghedancken</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Haer Aenslach is dy wel <rhyme label="a" type="m" corresp="#s7:l1">bekandt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Helpt ons dat wy niet <rhyme label="b" type="f" corresp="#s7:l2">wancken</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Vernufft teghen t'Gheloove <rhyme label="c" type="m">vicht</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Opt coemstich Wil sy Trouwen <rhyme label="c" type="m" corresp="#s7:l5">nicht</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Daer ghy sult selver <rhyme label="d" type="f">Troosten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Den HEmel end die Aerde <rhyme label="a" type="m">wijt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Hebt ghy HEER Godt <rhyme label="b" type="f">ghegrondet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dijn Licht laet ons Claer zijn <rhyme label="a" type="m" corresp="#s8:l1">altijt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">T'hert wort in ons <rhyme label="b" type="f" corresp="#s8:l2">bevonden</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Vol rechter Lieffd' des G'loovens <rhyme label="c" type="m">dijn</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Tot aent Eynde standtvastich <rhyme label="c" type="m" corresp="#s8:l5">zijn</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Die Wer'lt laet ymmer <rhyme label="d" type="f">Murren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
