  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Een droevich en jammer <rhyme label="a" type="f">scheyden</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Is t'Geertruydenberch <rhyme label="b" type="m">gheschiet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Doen Wolven by schapen <rhyme label="a" type="f" corresp="#s1:l1">weyden</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Ten cost langher dueren <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Deur der Wolven <rhyme label="c" type="f">practijcken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Mosten de Herders <rhyme label="c" type="f" corresp="#s1:l5">wijcken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die Schapen hier ende <rhyme label="d" type="m">daer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Verstroijden <rhyme label="c" type="f" corresp="#s1:l5">desghelijcken</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">O Godt wilt laten <rhyme label="c" type="f" corresp="#s1:l5">blijcken</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Dat ghy syt t'hooft van <rhyme label="d" type="m" corresp="#s1:l7">haer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">O opperste hooft <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Wilt die Schapen nu <rhyme label="b" type="m">bystaen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die noch met suchten en <rhyme label="a" type="f" corresp="#s2:l1">beven</rhyme></l>
        <l id="s2:l4" met="4" type="+-">In Egipten weyden <rhyme label="b" type="m" corresp="#s2:l2">gaen</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Door distelen en <rhyme label="c" type="f">bramen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Wiltse weder <rhyme label="c" type="f" corresp="#s2:l5">versamen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">In een weije wel <rhyme label="d" type="m">vruchtbaer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En Pharao <rhyme label="c" type="f" corresp="#s2:l5">beschamen</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Die haer alle te <rhyme label="c" type="f" corresp="#s2:l5">samen</rhyme>:</l>
        <l id="s2:l10" met="3" type="-+">Valt hart, wreet ende <rhyme label="d" type="m" corresp="#s2:l7">swaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wilt oock die verstroijde <rhyme label="a" type="f">lichten</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Alsoo ghy die Wijsen <rhyme label="b" type="m">deet</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Blijven in haer <rhyme label="a" type="f" corresp="#s3:l1">ghesichten</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">In droeffheyt en oock in <rhyme label="b" type="m" corresp="#s3:l2">vreet</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Soo ghy d'Enghelen <rhyme label="c" type="f">stelde</rhyme></l>
        <l id="s3:l6" met="3" type="-+">By d'Herders op den <rhyme label="c" type="f" corresp="#s3:l5">velde</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die u glorie seer <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En heerlijcheyt <rhyme label="c" type="f" corresp="#s3:l5">vertelden</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Daer in hem die tijt <rhyme label="c" type="f" corresp="#s3:l5">quelde</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Herodes seer <rhyme label="d" type="m" corresp="#s3:l7">verwoet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sijnen bloetgierighen <rhyme label="a" type="f">toren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Die en is niet al <rhyme label="b" type="m">ghedaen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want hy sou geerne als <rhyme label="a" type="f" corresp="#s4:l1">voren</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Die onschuldighe <rhyme label="b" type="m" corresp="#s4:l2">dootslaen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Cost hy haer slechs <rhyme label="c" type="f">betrapen</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">Maer wacht u wel ghy <rhyme label="c" type="f" corresp="#s4:l5">Schapen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Voor den Wolff loos ende <rhyme label="d" type="m">vals</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die door sijn boose <rhyme label="c" type="f" corresp="#s4:l5">knapen</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Heeft moordadich met <rhyme label="c" type="f" corresp="#s4:l5">wapen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">d'Herder ghebracht om <rhyme label="d" type="m" corresp="#s4:l7">hals</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Noch sijnse niet al <rhyme label="a" type="f">verslonden</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Godt stiert sijn Edel saet <rhyme label="b" type="m">voort</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Heeft hem in Breda <rhyme label="a" type="f" corresp="#s5:l1">ghesonden</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Tot een Herder om zijn <rhyme label="b" type="m" corresp="#s5:l2">woort</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">En Schapen te <rhyme label="c" type="f">bevrijden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En die daer te <rhyme label="c" type="f" corresp="#s5:l5">bestrijden</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Die haer haest hadde <rhyme label="d" type="m">gheschent</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dus wilt met groot <rhyme label="c" type="f" corresp="#s5:l5">verblijden</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Godt loven t'allen <rhyme label="c" type="f" corresp="#s5:l5">tijdent</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Die dees gratie <rhyme label="d" type="m" corresp="#s5:l7">sent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wilts oock eer en danck <rhyme label="a" type="f">bewijsen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Al die daer waren <rhyme label="b" type="m">ontrent</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">T'hooft was Maurits weert om <rhyme label="a" type="f" corresp="#s6:l1">prijsen</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Den gheboren Prince <rhyme label="b" type="m" corresp="#s6:l2">jent</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Hohenlo den Graeff <rhyme label="c" type="f">eele</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Oock Nassauwen met <rhyme label="c" type="f" corresp="#s6:l5">vele</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Capiteynen vroom <rhyme label="d" type="m">bekant</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En die sonder <rhyme label="c" type="f" corresp="#s6:l5">gheschele</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Het Schip brocht int <rhyme label="c" type="f" corresp="#s6:l5">Castele</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Met torff en volck <rhyme label="d" type="m" corresp="#s6:l7">vailljant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">De vyant heeft dit <rhyme label="a" type="f">vernomen</rhyme>;</l>
        <l id="s7:l2" met="4" type="+-">Carel van Mansvelt synde <rhyme label="b" type="m">gram</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Is met een armaed' <rhyme label="a" type="f" corresp="#s7:l1">ghecomen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Voor die schansse van <rhyme label="b" type="m" corresp="#s7:l2">Noortdam</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dwelck syn volck meest <rhyme label="c" type="f">beschreyden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Grobbendonck weeck <rhyme label="c" type="f" corresp="#s7:l5">Ter-Heyden</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Off in die Schansse daer <rhyme label="d" type="m">an</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Daer hy oock uyt most <rhyme label="c" type="f" corresp="#s7:l5">scheyden</rhyme>:</l>
        <l id="s7:l9" met="3" type="-+">Maurits quam indie <rhyme label="c" type="f" corresp="#s7:l5">weyden</rhyme></l>
        <l id="s7:l10" met="3" type="-+">En lichte hem daer <rhyme label="d" type="m" corresp="#s7:l7">van</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Noch is den Prince der <rhyme label="a" type="f">vromen</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Met syn heyrcracht <rhyme label="b" type="m">triomphant</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Binnen Steenberghen <rhyme label="a" type="f" corresp="#s8:l1">ghecomen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Schansschen oock in <rhyme label="b" type="m" corresp="#s8:l2">gelderlandt</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">En Sutphen <rhyme label="c" type="f">daerbeneven</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Is gheen vyant <rhyme label="c" type="f" corresp="#s8:l5">ghebleven</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Die van Deventer wel <rhyme label="d" type="m">wijs</rhyme>:</l>
        <l id="s8:l8" met="3" type="-+">Hebbent oock <rhyme label="c" type="f" corresp="#s8:l5">opghegheven</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Behouden goet en <rhyme label="c" type="f" corresp="#s8:l5">d'leven</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Godt sy loff eer en <rhyme label="d" type="m" corresp="#s8:l7">prijs</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Daer nae heeft hy hem <rhyme label="a" type="f">begheven</rhyme></l>
        <l id="s9:l2" met="4" type="+-">By Graeff Wilhelm vroom int <rhyme label="b" type="m">werck</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En den vyant daer <rhyme label="a" type="f" corresp="#s9:l1">verdreven</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Uut veel Schanschen hooch en <rhyme label="b" type="m" corresp="#s9:l2">sterck</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Comende heeft <rhyme label="c" type="f">verslagen</rhyme>:</l>
        <l id="s9:l6" met="3" type="-+">Die op de Velu <rhyme label="c" type="f" corresp="#s9:l5">laghen</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Te Peert al onder de <rhyme label="d" type="m">voet</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Om Knodsenborch te <rhyme label="c" type="f" corresp="#s9:l5">plaghen</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Dit gaff Godt tot <rhyme label="c" type="f" corresp="#s9:l5">behaghen</rhyme></l>
        <l id="s9:l10" met="3" type="-+">Den Prince wel <rhyme label="d" type="m" corresp="#s9:l7">ghemoet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Godt heeft door synen arm <rhyme label="a" type="f">krachtich</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Noch gevoert den vromen <rhyme label="b" type="m">Helt</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Dat hy is gheworden <rhyme label="a" type="f" corresp="#s10:l1">machtich</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Hulst en tlant Waes met <rhyme label="b" type="m" corresp="#s10:l2">ghewelt</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Nae Nimmeghen <rhyme label="c" type="f">ghevaren</rhyme>:</l>
        <l id="s10:l6" met="3" type="-+">Daer hy sonder <rhyme label="c" type="f" corresp="#s10:l5">beswaren</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Met veraccoordeerde <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Godt wil hem voorts <rhyme label="c" type="f" corresp="#s10:l5">bewaren</rhyme></l>
        <l id="s10:l9" met="3" type="-+">In toecomende <rhyme label="c" type="f" corresp="#s10:l5">Jaren</rhyme></l>
        <l id="s10:l10" met="3" type="-+">Laten soo spoedich <rhyme label="d" type="m" corresp="#s10:l7">syn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Voorspoet is weder <rhyme label="a" type="f">begonnen</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Tot Godts lof en s'Princen <rhyme label="b" type="m">want</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Steenwijck heeft hy doen <rhyme label="a" type="f" corresp="#s11:l1">gewonnen</rhyme></l>
        <l id="s11:l4" met="4" type="+-">En Coevoerden sterck <rhyme label="b" type="m" corresp="#s11:l2">gheplant</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Die ghespreyt hadden <rhyme label="c" type="f">netten</rhyme>:</l>
        <l id="s11:l6" met="3" type="-+">Om t'selve te <rhyme label="c" type="f" corresp="#s11:l5">ontsetten</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Werden dapper <rhyme label="d" type="m">aengheroert</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Eer sy diet haer <rhyme label="c" type="f" corresp="#s11:l5">beletten</rhyme></l>
        <l id="s11:l9" met="3" type="-+">En die Godts woort en <rhyme label="c" type="f" corresp="#s11:l5">Wetten</rhyme></l>
        <l id="s11:l10" met="3" type="-+">In Bels plaetsen <rhyme label="d" type="m" corresp="#s11:l7">invoert</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Den winter ghepasseert <rhyme label="a" type="f">zijnde</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Doenmen drie-tneghentich <rhyme label="b" type="m">schreef</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">d'Excelentie <rhyme label="a" type="f" corresp="#s12:l1">verschijnde</rhyme></l>
        <l id="s12:l4" met="4" type="+-">t'Geertenberch wonder <rhyme label="b" type="m" corresp="#s12:l2">bedreeff</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Met Hohenloo <rhyme label="c" type="f">beleghen</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Die Schans seer haest <rhyme label="c" type="f" corresp="#s12:l5">vercregen</rhyme>:</l>
        <l id="s12:l7" met="3" type="-+">Die Stadt hieldet noch seer <rhyme label="d" type="m">vast</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Mansvelt quam daer <rhyme label="c" type="f" corresp="#s12:l5">teghen</rhyme>:</l>
        <l id="s12:l9" met="3" type="-+">Maer den Prins <rhyme label="c" type="f" corresp="#s12:l5">onversleghen</rhyme></l>
        <l id="s12:l10" met="3" type="-+">Had op zijn compst <rhyme label="d" type="m" corresp="#s12:l7">ghepast</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Den vyant lach daer <rhyme label="a" type="f">gheweldich</rhyme></l>
        <l id="s13:l2" met="4" type="+-">Tontsetten dorfd' noyt <rhyme label="b" type="m">bestaen</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">D'ander mosten treurich <rhyme label="a" type="f" corresp="#s13:l1">scheldich</rhyme></l>
        <l id="s13:l4" met="4" type="+-">d'Stadt verlaten daer uut <rhyme label="b" type="m" corresp="#s13:l2">gaan</rhyme>:</l>
        <l id="s13:l5" met="3" type="-+">Met t'geweer t'haren <rhyme label="c" type="f">Lijven</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Haer vendels mosten <rhyme label="c" type="f" corresp="#s13:l5">blijven</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Aen dat eel Nassauwen <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Looft Godt Mans ende <rhyme label="c" type="f" corresp="#s13:l5">Wijven</rhyme>:</l>
        <l id="s13:l9" met="3" type="-+">Eertse die d'werck hier <rhyme label="c" type="f" corresp="#s13:l5">drijven</rhyme></l>
        <l id="s13:l10" met="3" type="-+">Als instrumenten <rhyme label="d" type="m" corresp="#s13:l7">goet</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Die blijschap dient te <rhyme label="a" type="f">verhalen</rhyme></l>
        <l id="s14:l2" met="4" type="+-">Die daer schiede int <rhyme label="b" type="m">ghemeyn</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Lovende Godt in veel <rhyme label="a" type="f" corresp="#s14:l1">talen</rhyme></l>
        <l id="s14:l4" met="4" type="+-">Die vroom Borgers groot en <rhyme label="b" type="m" corresp="#s14:l2">cleyn</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Die daer weder in <rhyme label="c" type="f">quamen</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Spraken nae trechts <rhyme label="c" type="f" corresp="#s14:l5">betamen</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Met grooten verheuchden <rhyme label="d" type="m">gheest</rhyme>:</l>
        <l id="s14:l8" met="3" type="-+">Wilcom broeders by <rhyme label="c" type="f" corresp="#s14:l5">namen</rhyme></l>
        <l id="s14:l9" met="3" type="-+">Godt loff die brengt te <rhyme label="c" type="f" corresp="#s14:l5">samen</rhyme></l>
        <l id="s14:l10" met="3" type="-+">Die verstroyt sijn <rhyme label="d" type="m" corresp="#s14:l7">gheweest</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Siet hoe Godt straft die <rhyme label="a" type="f">vergheten</rhyme>:</l>
        <l id="s15:l2" met="4" type="+-">Sijn woort en heylighen <rhyme label="b" type="m">naem</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Mansvelt wou t'Crevekeur <rhyme label="a" type="f" corresp="#s15:l1">eten</rhyme></l>
        <l id="s15:l4" met="4" type="+-">Eer Prins Maurits creech de <rhyme label="b" type="m" corresp="#s15:l2">faem</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Listich ginck hijt <rhyme label="c" type="f">becruypen</rhyme></l>
        <l id="s15:l6" met="3" type="-+">En meyndet op te <rhyme label="c" type="f" corresp="#s15:l5">suypen</rhyme>:</l>
        <l id="s15:l7" met="3" type="-+">Met syn groff gheschut seer <rhyme label="d" type="m">ras</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Maer hy ginck daer van <rhyme label="c" type="f" corresp="#s15:l5">druypen</rhyme>:</l>
        <l id="s15:l9" met="3" type="-+">Den selven wech <rhyme label="c" type="f" corresp="#s15:l5">insluypen</rhyme></l>
        <l id="s15:l10" met="3" type="-+">Van daer hij comen <rhyme label="d" type="m" corresp="#s15:l7">was</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Groote victorijen <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s16:l2" met="4" type="+-">Gheeft Godt den Prince <rhyme label="b" type="m">vermaert</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Hy en spaert niet zijns <rhyme label="a" type="f" corresp="#s16:l1">persoone</rhyme>:</l>
        <l id="s16:l4" met="4" type="+-">D'werck hevet <rhyme label="b" type="m" corresp="#s16:l2">gheopenbaert</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Gheen moeyten hem <rhyme label="c" type="f">vernoeijen</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Soomen hem noch sach <rhyme label="c" type="f" corresp="#s16:l5">spoeijen</rhyme>:</l>
        <l id="s16:l7" met="3" type="-+">Nae Coevoerden lanck <rhyme label="d" type="m">benout</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Godt loff diet gaff ten <rhyme label="c" type="f" corresp="#s16:l5">goeijen</rhyme>:</l>
        <l id="s16:l9" met="3" type="-+">Want sonder slach te <rhyme label="c" type="f" corresp="#s16:l5">moeijen</rhyme></l>
        <l id="s16:l10" met="3" type="-+">Weeck den vyant <rhyme label="d" type="m" corresp="#s16:l7">verflout</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Schantlijck is hij daer <rhyme label="a" type="f">gheweecken</rhyme></l>
        <l id="s17:l2" met="4" type="+-">Prins Maurits vroom wijs <rhyme label="b" type="m">bedacht</rhyme>:</l>
        <l id="s17:l3" met="4" type="+-">Is de winning <rhyme label="a" type="f" corresp="#s17:l1">naegestreken</rhyme></l>
        <l id="s17:l4" met="4" type="+-">Van Groeninghen sterck <rhyme label="b" type="m" corresp="#s17:l2">gheacht</rhyme></l>
        <l id="s17:l5" met="3" type="-+">Onwinbaer soo sy <rhyme label="c" type="f">meende</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Godt haren trots <rhyme label="c" type="f" corresp="#s17:l5">vercleende</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Ende gaff Nassauwen <rhyme label="d" type="m">cracht</rhyme>,</l>
        <l id="s17:l8" met="3" type="-+">Auwerzeel dat <rhyme label="c" type="f" corresp="#s17:l5">beweende</rhyme>,</l>
        <l id="s17:l9" met="3" type="-+">Groeninghen Godt <rhyme label="c" type="f" corresp="#s17:l5">verleende</rhyme></l>
        <l id="s17:l10" met="3" type="-+">Oock in Prins Maurits <rhyme label="d" type="m" corresp="#s17:l7">macht</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">God bleeck noch sijn wonder <rhyme label="a" type="f">wercken</rhyme></l>
        <l id="s18:l2" met="4" type="+-">Door dat Nassauwen bloet <rhyme label="b" type="m">eel</rhyme>,</l>
        <l id="s18:l3" met="4" type="+-">Hier grootelick tot s'Lants <rhyme label="a" type="f" corresp="#s18:l1">verstercken</rhyme></l>
        <l id="s18:l4" met="4" type="+-">En des vyants <rhyme label="b" type="m" corresp="#s18:l2">achterdeel</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Doen den Prince <rhyme label="c" type="f">cloeckmoedich</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Met Graven Heeren <rhyme label="c" type="f" corresp="#s18:l5">goedich</rhyme></l>
        <l id="s18:l7" met="3" type="-+">Binnen Geertrudenberch <rhyme label="d" type="m">quamp</rhyme>,</l>
        <l id="s18:l8" met="3" type="-+">Met t'Crijchsvolck <rhyme label="c" type="f" corresp="#s18:l5">overvloedich</rhyme></l>
        <l id="s18:l9" met="3" type="-+">En van daer seer <rhyme label="c" type="f" corresp="#s18:l5">voorspoedich</rhyme></l>
        <l id="s18:l10" met="3" type="-+">Nae Turnhout den tocht <rhyme label="d" type="m" corresp="#s18:l7">namp</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Daerontrent ghecomen <rhyme label="a" type="f">sijnde</rhyme></l>
        <l id="s19:l2" met="4" type="+-">Den vyant werdet <rhyme label="b" type="m">ghewaer</rhyme>,</l>
        <l id="s19:l3" met="4" type="+-">Te vertrecken hy hem <rhyme label="a" type="f" corresp="#s19:l1">pijnde</rhyme>:</l>
        <l id="s19:l4" met="4" type="+-">Prins Maurits die volchde <rhyme label="b" type="m" corresp="#s19:l2">naer</rhyme></l>
        <l id="s19:l5" met="3" type="-+">Seer dapperlijck <rhyme label="c" type="f">merceerden</rhyme></l>
        <l id="s19:l6" met="3" type="-+">Op T'hieler-Hey hem <rhyme label="c" type="f" corresp="#s19:l5">keerden</rhyme>,</l>
        <l id="s19:l7" met="3" type="-+">Den Waracx sach gheen <rhyme label="d" type="m">verhael</rhyme>,</l>
        <l id="s19:l8" met="3" type="-+">Hohenloo den <rhyme label="c" type="f" corresp="#s19:l5">onverveerden</rhyme></l>
        <l id="s19:l9" met="3" type="-+">Graeff Solms met haer troep <rhyme label="c" type="f" corresp="#s19:l5">peerden</rhyme></l>
        <l id="s19:l10" met="3" type="-+">Treften den <rhyme label="d" type="m" corresp="#s19:l7">generael</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Nassaus Heeren en <rhyme label="a" type="f">Collonellen</rhyme>,</l>
        <l id="s20:l2" met="4" type="+-">Capiteyns en ons crijchsvolck <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s20:l3" met="4" type="+-">Sachmen oock aent vechten <rhyme label="a" type="f" corresp="#s20:l1">stellen</rhyme></l>
        <l id="s20:l4" met="4" type="+-">Met een vroom en cloeck <rhyme label="b" type="m" corresp="#s20:l2">ghemoet</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">Twee duysent daer neer <rhyme label="c" type="f">vielen</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Die reste sachmen <rhyme label="c" type="f" corresp="#s20:l5">knielen</rhyme>:</l>
        <l id="s20:l7" met="3" type="-+">Roepende met luyder <rhyme label="d" type="m">stem</rhyme>,</l>
        <l id="s20:l8" met="3" type="-+">O geus spaert onse <rhyme label="c" type="f" corresp="#s20:l5">sielen</rhyme>:</l>
        <l id="s20:l9" met="3" type="-+">Siet hoe laet Godt <rhyme label="c" type="f" corresp="#s20:l5">vernielen</rhyme></l>
        <l id="s20:l10" met="3" type="-+">Die strijden teghens <rhyme label="d" type="m" corresp="#s20:l7">hem</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Noch vierhondert daer <rhyme label="a" type="f">ghevanghen</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Achtendertich vendels <rhyme label="b" type="m">met</rhyme></l>
        <l id="s21:l3" met="4" type="+-">Die inden HAECH op d'Sael <rhyme label="a" type="f" corresp="#s21:l1">hanghen</rhyme>,</l>
        <l id="s21:l4" met="4" type="+-">Met een Ruyters vaen <rhyme label="b" type="m" corresp="#s21:l2">Cornet</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Tot deser landen <rhyme label="c" type="f">glori</rhyme></l>
        <l id="s21:l6" met="3" type="-+">En eeuwighe <rhyme label="c" type="f" corresp="#s21:l5">memori</rhyme>,</l>
        <l id="s21:l7" met="3" type="-+">Turnhouts Slot door t'schieten <rhyme label="d" type="m">bloot</rhyme></l>
        <l id="s21:l8" met="3" type="-+">Gaft op (leest die <rhyme label="c" type="f" corresp="#s21:l5">Histori</rhyme>,)</l>
        <l id="s21:l9" met="3" type="-+">Looft Godt van dees <rhyme label="c" type="f" corresp="#s21:l5">victori</rhyme></l>
        <l id="s21:l10" met="3" type="-+">En sijn weldaden <rhyme label="d" type="m" corresp="#s21:l7">groot</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Maurits door Gods <rhyme label="a" type="f">ghenaden</rhyme></l>
        <l id="s22:l2" met="4" type="+-">Kreech noch int merceren <rhyme label="b" type="m">sterck</rhyme>:</l>
        <l id="s22:l3" met="4" type="+-">Die Stadt Alpen sonder <rhyme label="a" type="f" corresp="#s22:l1">schaden</rhyme>,</l>
        <l id="s22:l4" met="4" type="+-">Corts daer nae die Stadt <rhyme label="b" type="m" corresp="#s22:l2">Rhijnberck</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Oock Muers ende Grol <rhyme label="c" type="f">mede</rhyme>,</l>
        <l id="s22:l6" met="3" type="-+">Brevoirt die stercke <rhyme label="c" type="f" corresp="#s22:l5">Stede</rhyme></l>
        <l id="s22:l7" met="3" type="-+">Namp hy met ghewelder <rhyme label="d" type="m">hant</rhyme>,</l>
        <l id="s22:l8" met="3" type="-+">Otmars, Old'zeel, <rhyme label="c" type="f" corresp="#s22:l5">Enschede</rhyme>,</l>
        <l id="s22:l9" met="3" type="-+">Linghen en brocht in <rhyme label="c" type="f" corresp="#s22:l5">vrede</rhyme></l>
        <l id="s22:l10" met="3" type="-+">T'heel Overijssel <rhyme label="d" type="m" corresp="#s22:l7">Landt</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Looft Godt, singt, schrijft in <rhyme label="a" type="f">Chronijcken</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Dat Prins Maurits soo <rhyme label="b" type="m">subijt</rhyme></l>
        <l id="s23:l3" met="4" type="+-">Met ghewelt en crijchs <rhyme label="a" type="f" corresp="#s23:l1">practijcken</rhyme>:</l>
        <l id="s23:l4" met="4" type="+-">Dese Steden heeft <rhyme label="b" type="m" corresp="#s23:l2">bevrijt</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Met haer Casteelen <rhyme label="c" type="f">meere</rhyme>,,</l>
        <l id="s23:l6" met="3" type="-+">Liepen uut schanschen <rhyme label="c" type="f" corresp="#s23:l5">veere</rhyme></l>
        <l id="s23:l7" met="3" type="-+">Vreesende t'Nassauwen <rhyme label="d" type="m">saet</rhyme>,</l>
        <l id="s23:l8" met="3" type="-+">Een vroom Prins graeff en <rhyme label="c" type="f" corresp="#s23:l5">Heere</rhyme></l>
        <l id="s23:l9" met="3" type="-+">Naest God, gheeft hem die <rhyme label="c" type="f" corresp="#s23:l5">eere</rhyme>:</l>
        <l id="s23:l10" met="3" type="-+">En deser Landen <rhyme label="d" type="m" corresp="#s23:l7">Raet</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Doent met Linghen soo wel <rhyme label="a" type="f">eynden</rhyme></l>
        <l id="s24:l2" met="4" type="+-">Den vyant zijnde <rhyme label="b" type="m">ontstelt</rhyme>,</l>
        <l id="s24:l3" met="4" type="+-">Grobbendonck, Barlotte <rhyme label="a" type="f" corresp="#s24:l1">meynden</rhyme></l>
        <l id="s24:l4" met="4" type="+-">Met verraet ende <rhyme label="b" type="m" corresp="#s24:l2">ghewelt</rhyme>:</l>
        <l id="s24:l5" met="3" type="-+">Geertrudenberch te <rhyme label="c" type="f">strijcken</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">Berch-op-Zoom <rhyme label="c" type="f" corresp="#s24:l5">desghelijcken</rhyme>,</l>
        <l id="s24:l7" met="3" type="-+">Maer God heeft voor haer <rhyme label="d" type="m">ghewaeckt</rhyme>:</l>
        <l id="s24:l8" met="3" type="-+">Want sy moesten weer <rhyme label="c" type="f" corresp="#s24:l5">wijcken</rhyme></l>
        <l id="s24:l9" met="3" type="-+">En haer valsche <rhyme label="c" type="f" corresp="#s24:l5">practijcken</rhyme></l>
        <l id="s24:l10" met="3" type="-+">Heeft hy tot niet <rhyme label="d" type="m" corresp="#s24:l7">ghemaeckt</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">God die dit al heeft <rhyme label="a" type="f">ghegheven</rhyme>:</l>
        <l id="s25:l2" met="4" type="+-">En dat eel Nassauwen <rhyme label="b" type="m">bloet</rhyme></l>
        <l id="s25:l3" met="4" type="+-">Soo victorieus <rhyme label="a" type="f" corresp="#s25:l1">verheven</rhyme></l>
        <l id="s25:l4" met="4" type="+-">Neem hem voorts in sijn <rhyme label="b" type="m" corresp="#s25:l2">behoet</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">Met al dees Lants <rhyme label="c" type="f">Staten</rhyme></l>
        <l id="s25:l6" met="3" type="-+">En die om eer te <rhyme label="c" type="f" corresp="#s25:l5">vaten</rhyme></l>
        <l id="s25:l7" met="3" type="-+">Ghewillich gaen totten <rhyme label="d" type="m">strijt</rhyme>,</l>
        <l id="s25:l8" met="3" type="-+">Oock die vroom <rhyme label="c" type="f" corresp="#s25:l5">ondersaten</rhyme></l>
        <l id="s25:l9" met="3" type="-+">Wil Godt comen te <rhyme label="c" type="f" corresp="#s25:l5">baten</rhyme></l>
        <l id="s25:l10" met="3" type="-+">T'sijnder eer, t'slants <rhyme label="d" type="m" corresp="#s25:l7">proffijt</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">O PRINCE GODT wilt <rhyme label="a" type="f">vermeeren</rhyme></l>
        <l id="s26:l2" met="4" type="+-">En stercken, dat Edel <rhyme label="b" type="m">zaet</rhyme></l>
        <l id="s26:l3" met="4" type="+-">Van Vorsten, Princen en <rhyme label="a" type="f" corresp="#s26:l1">Heeren</rhyme>,</l>
        <l id="s26:l4" met="4" type="+-">Die u Schapen vroech en <rhyme label="b" type="m" corresp="#s26:l2">laet</rhyme>:</l>
        <l id="s26:l5" met="3" type="-+">Hier hebben <rhyme label="c" type="f">voor-ghestanden</rhyme></l>
        <l id="s26:l6" met="3" type="-+">En noch als den <rhyme label="c" type="f" corresp="#s26:l5">vaelianden</rhyme></l>
        <l id="s26:l7" met="3" type="-+">Wederstaen den Wolff <rhyme label="d" type="m">verwoet</rhyme>,</l>
        <l id="s26:l8" met="3" type="-+">Met alle die <rhyme label="c" type="f" corresp="#s26:l5">vyanden</rhyme></l>
        <l id="s26:l9" met="3" type="-+">Van dese <rhyme label="c" type="f" corresp="#s26:l5">Nederlanden</rhyme></l>
        <l id="s26:l10" met="3" type="-+">En t'vals Babels <rhyme label="d" type="m" corresp="#s26:l7">ghebroet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
