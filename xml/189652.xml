<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>189652</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Allez où le sort vous conduit</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ANnaatje, myn lief, myn schoone <rhyme label="a" type="m">zon</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Wyl my de nood nu dwingt van u te <rhyme label="b" type="f">schyden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Zoo moet ik door u of zyn <rhyme label="b" type="f" corresp="#s1:l2">lyden</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">De swaarste pyn, die men ooit lyden <rhyme label="a" type="m" corresp="#s1:l1">kon</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">De Hemel schynt my zonder <rhyme label="c" type="m">licht</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Wyl dat ik missen moet uw zoet <rhyme label="c" type="m" corresp="#s1:l5">gezicht</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Wiens gunst dat my weleer plag te <rhyme label="d" type="f">bestralen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Maar nu myn <rhyme label="e" type="m">hert</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Geen aam kan <rhyme label="d" type="f" corresp="#s1:l7">halen</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Leef ik steets in <rhyme label="e" type="m" corresp="#s1:l8">smert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Myn oogen, die weleer haar <rhyme label="a" type="m">vrucht</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Ja 't leven zelf uit uw gezicht <rhyme label="b" type="f">ontfongen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Zyn van verdriet nu zoo <rhyme label="b" type="f" corresp="#s2:l2">gedrongen</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Dat zy versmelten in al de <rhyme label="a" type="m" corresp="#s2:l1">ongeneugt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Myn tranen smoren gras en <rhyme label="c" type="m">kruyt</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Op 't vruchtbaar lant, tot aan den wortel <rhyme label="c" type="m" corresp="#s2:l5">uit</rhyme>,</l>
        <l id="s2:l7" met="5" type="-+">Ja d'aarde zelf die schynt met ons te <rhyme label="d" type="f">weenen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Mits myn <rhyme label="e" type="m">elent</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">De harde <rhyme label="d" type="f" corresp="#s2:l7">steenen</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Zelfs tot tranen <rhyme label="e" type="m" corresp="#s2:l8">wendt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'s Nags als men 't al in rusten <rhyme label="a" type="m">vint</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Soo woelt myn hart, 'k meen zomtyds met myn <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Uw gints te zien, maar ik ben <rhyme label="b" type="f" corresp="#s3:l2">bedrogen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Want in u plaas omhels ik niet dan <rhyme label="a" type="m" corresp="#s3:l1">wint</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">'k Ben levent doot, want 'k mis myn <rhyme label="c" type="m">ziel</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Mits gy, ô Schoone! die tot pant <rhyme label="c" type="m" corresp="#s3:l5">behiel</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">Maar Echter kan ik deze doot niet <rhyme label="d" type="f">haten</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Wyl ik tot <rhyme label="e" type="m">pant</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Myn ziel zal <rhyme label="d" type="f" corresp="#s3:l7">laten</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">In zoo waarde <rhyme label="e" type="m" corresp="#s3:l8">hant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
