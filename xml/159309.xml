<song>
  <meta>
    <id>159309</id>
    <date>
      <year-start>1749</year-start>
      <year-end>1749</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
