<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190514</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">Als het Gezang de zorg verpoost, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s1:l2" met="6" type="+-">Dan Zingt de Bloem van Bato's kroost, ha, <rhyme label="a" type="m" corresp="#s1:l1">ha</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Oranje houd nu weêr de <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Aan 't wreed gefolterd <rhyme label="b" type="m" corresp="#s1:l3">Nederland</rhyme>....</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">ô Patriotten tot u spyt, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s2:l2" met="6" type="+-">Veschimmeld nu door wrok en nyd, ha, <rhyme label="a" type="m" corresp="#s2:l1">ha</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wy lachen met uw snood <rhyme label="b" type="m">gebroed</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En juichen by den <rhyme label="b" type="m" corresp="#s2:l3">Vryheidshoed</rhyme>....</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Ach Heeren van de Societeit, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">De Staat vereischt een Achtbaarheid, ha, <rhyme label="a" type="m" corresp="#s3:l1">ha</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maar niet zo als uw leden <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Verzoopen in den Drank en <rhyme label="b" type="m" corresp="#s3:l3">Wyn</rhyme>...</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">De Maffe Jonker van 't Salet, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s4:l2" met="6" type="+-">Te Zwak voor Roosje op het Bed, ha, <rhyme label="a" type="m" corresp="#s4:l1">ha</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Speelde als een twede Don <rhyme label="b" type="m">Quichot</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">'t Verwaande Gekje <rhyme label="b" type="m" corresp="#s4:l3">Patriot</rhyme>....</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Had nog zoo'n kaale sobre bloed, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Een Pluimpje op zyn Smousse Hoed, ha, <rhyme label="a" type="m" corresp="#s5:l1">ha</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Schoon hy leezen nog schryven <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dog was hy een Weldenkend <rhyme label="b" type="m" corresp="#s5:l3">Man</rhyme>....</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="+-">De Snyder speelde de Advocaat, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s6:l2" met="6" type="+-">De Schoes droeg zig als Magistraat, ha, <rhyme label="a" type="m" corresp="#s6:l1">ha</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Kruijer scheen een 's Lands <rhyme label="b" type="m">Fiscaal</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">De Hospes was een <rhyme label="b" type="m" corresp="#s6:l3">Generaal</rhyme>....</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="+-">Geconstitueerde 't gaat verkeert, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s7:l2" met="6" type="+-">Want als een Aap zyn Meester leert, ha, <rhyme label="a" type="m" corresp="#s7:l1">ha</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dan komt het dier in <rhyme label="b" type="m">ongena</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En zyt gy niet zyn <rhyme label="b" type="m" corresp="#s7:l3">wederga</rhyme>....</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="6" type="+-">Defensie Heeren wat een spyt. ha. <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s8:l2" met="6" type="+-">Gy zyt uw Magt en Helden kwyt, ha, <rhyme label="a" type="m" corresp="#s8:l1">ha</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Genaakt de Doelen <rhyme label="b" type="m">nimmermeer</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ach uitgediend Defensie <rhyme label="b" type="m" corresp="#s8:l3">Heer</rhyme>....</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="6" type="+-">Gy hebt voor eeuwig afgedaan, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s9:l2" met="6" type="+-">Nu kunt gy wel op Sloffen gaan, ha, <rhyme label="a" type="m" corresp="#s9:l1">ha</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Hoe zeer uw gedrag <rhyme label="b" type="m">berouwd</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Uw word geen Staatlyk Ampt <rhyme label="b" type="m" corresp="#s9:l3">betrouwt</rhyme>...</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="6" type="+-">Ach Held tot nut der Schuttery, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s10:l2" met="6" type="+-">Wat staat gy 'er nu ook lastig by, ha, <rhyme label="a" type="m" corresp="#s10:l1">ha</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Gy leek waaragtig wel een <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Met Matz en Lubbens in 't <rhyme label="b" type="m" corresp="#s10:l3">Geweer</rhyme>....</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="6" type="+-">Maar ach wat is die kans verkeerd, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s11:l2" met="6" type="+-">Van al uw Mooys geremoveerd, ha, <rhyme label="a" type="m" corresp="#s11:l1">ha</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Is 't sprookje niet van ouds <rhyme label="b" type="m">geweest</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Ach Schoesje houd je by je <rhyme label="b" type="m" corresp="#s11:l3">Leest</rhyme>....</l>
      </lg>
    </lg>
  </text>
</song>
