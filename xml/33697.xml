<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>33697</id>
    <date>
      <year-start>1654</year-start>
      <year-end>1654</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>biblical history song</tag>
    </tags>
    <melody>Wilhelmus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WY groeten u <rhyme label="a" type="f">Gehouwde</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wy wenschen u <rhyme label="b" type="m">geluk</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">U Eghtelijk <rhyme label="a" type="f" corresp="#s1:l1">Getrouwde</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Godt wil des harten <rhyme label="b" type="m" corresp="#s1:l2">druk</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Veer wegh van u <rhyme label="c" type="f">verjagen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En gunnen 't leeven <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">In alle uwe <rhyme label="c" type="f" corresp="#s1:l5">dagen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Met grati <rhyme label="d" type="m" corresp="#s1:l6">overvloet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Godt heeft in den <rhyme label="a" type="f">beginne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Des Weerelts soo <rhyme label="b" type="m">verklaert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat Man en Vrouw met <rhyme label="a" type="f" corresp="#s2:l1">minne</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dus werden souw <rhyme label="b" type="m" corresp="#s2:l2">gepaert</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Als dat sy alsoo <rhyme label="c" type="f">souden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sijn in een vleysche <rhyme label="d" type="m">twee</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">De Eghtelijk <rhyme label="c" type="f" corresp="#s2:l5">Getrouden</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Van Godt gevoeght in <rhyme label="d" type="m" corresp="#s2:l6">Vree</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Uw' staet wilt wel <rhyme label="a" type="f">bemerke</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'t Is een groot <rhyme label="b" type="m">Sacrament</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">In Christo en sijn <rhyme label="a" type="f" corresp="#s3:l1">Kerke</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Als Paulus maekt <rhyme label="b" type="m" corresp="#s3:l2">bekent</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat Godt tot <rhyme label="c" type="f">eeuwigheede</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Sijn Kerke heeft <rhyme label="d" type="m">getrouwt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Aldus moet gy-lie <rhyme label="c" type="f" corresp="#s3:l5">meede</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Ter doodt toe sijn <rhyme label="d" type="m" corresp="#s3:l6">gehouwt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dus die werden <rhyme label="a" type="f">gevonden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In <rhyme label="b" type="m">d'Houwelijken-staet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Soek niet te sijn <rhyme label="a" type="f" corresp="#s4:l1">ontbonden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Nogh geenighsins <rhyme label="b" type="m" corresp="#s4:l2">verlaet</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Eerwaerdigh is het <rhyme label="c" type="f">Houwlijk</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Een bedde <rhyme label="d" type="m">onbesmet</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Dus onderhoudt het <rhyme label="c" type="f" corresp="#s4:l5">trouwlijk</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Als 't Godt heeft <rhyme label="d" type="m" corresp="#s4:l6">ingeset</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een sonderlinge <rhyme label="a" type="f">eere</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Heeft Christus u <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als hy om dit te <rhyme label="a" type="f" corresp="#s5:l1">leere</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ter Bruyloft is <rhyme label="b" type="m" corresp="#s5:l2">gegaen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Met sijn Lief Moeder <rhyme label="c" type="f">meede</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En sijne <rhyme label="d" type="m">Jongeren</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Alwaer hy 't eerste <rhyme label="c" type="f" corresp="#s5:l5">deede</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Van sijn <rhyme label="d" type="m" corresp="#s5:l6">Miraculen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
