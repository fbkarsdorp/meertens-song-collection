    <melody>Gezwinde bode van de min</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Trots al het lyden van mijn <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Blaak ik van u <rhyme label="b" type="m">min</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Hoe seer my uwe strafheid <rhyme label="a" type="m" corresp="#s1:l1">smert</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Wrede <rhyme label="b" type="m" corresp="#s1:l2">vyandin</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Mijn <rhyme label="c" type="m">gemoed</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Eerd de <rhyme label="c" type="m" corresp="#s1:l5">gloed</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Daar mijn <rhyme label="d" type="m">Siel</rhyme></l>
        <l id="s1:l8" met="2" type="+-">In <rhyme label="d" type="m" corresp="#s1:l7">verviel</rhyme></l>
        <l id="s1:l9" met="1" type="-+">Als <rhyme label="e" type="m">ik</rhyme></l>
        <l id="s1:l10" met="4" type="+-">In het eerste <rhyme label="e" type="m" corresp="#s1:l9">oogenblik</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Sag u lief <rhyme label="f" type="m">gezigt</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">Soo vol heerlijk <rhyme label="f" type="m" corresp="#s1:l11">ligt</rhyme></l>
        <l id="s1:l13" met="3" type="+-">Dat ik, fel <rhyme label="g" type="m">gewond</rhyme>,</l>
        <l id="s1:l14" met="4" type="+-">My haast   in u boeijen <rhyme label="g" type="m" corresp="#s1:l13">vond</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En of gy schoon u sinnen <rhyme label="a" type="m">vleid</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Met mijn ziels <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">En nimmer met <rhyme label="a" type="m" corresp="#s2:l1">medogendheit</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Mijne smerten <rhyme label="b" type="m" corresp="#s2:l2">siet</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Ik <rhyme label="c" type="m">vertraag</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Nog <rhyme label="c" type="m" corresp="#s2:l5">verstaag</rhyme></l>
        <l id="s2:l7" met="2" type="+-">
          <rhyme label="d" type="m">Nimmermeer</rhyme>
        </l>
        <l id="s2:l8" met="2" type="+-">Maar ik <rhyme label="d" type="m" corresp="#s2:l7">eer</rhyme></l>
        <l id="s2:l9" met="1" type="-+">Uw <rhyme label="e" type="m">min</rhyme></l>
        <l id="s2:l10" met="4" type="+-">En soek daar mijn vernoegen <rhyme label="e" type="m" corresp="#s2:l9">in</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Wyl de hoop my <rhyme label="f" type="m">streeld</rhyme>,</l>
        <l id="s2:l12" met="3" type="+-">En altoos <rhyme label="f" type="m" corresp="#s2:l11">verbeeld</rhyme></l>
        <l id="s2:l13" met="3" type="+-">Dat uw <rhyme label="g" type="m">straffigheid</rhyme></l>
        <l id="s2:l14" met="4" type="+-">Eens vergaan   sal door de <rhyme label="g" type="m" corresp="#s2:l13">tyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
