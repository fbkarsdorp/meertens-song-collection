<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183004</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Adieu dan Roselinde</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wie zou niet vrolyk <rhyme label="a" type="f">zingen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Op deezen blyden <rhyme label="b" type="m">stond</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En hart en stemmen <rhyme label="a" type="f" corresp="#s1:l1">dwingen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Op onzen vryjen <rhyme label="b" type="m" corresp="#s1:l2">grond</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">De blyde <rhyme label="c" type="f">omstandigheden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Waar in elk nu <rhyme label="d" type="m">verkeerd</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Stuurd onze <rhyme label="c" type="f" corresp="#s1:l5">vreugdetreeden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">De rampen zyn <rhyme label="d" type="m" corresp="#s1:l6">geweerd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Son aan d'ochtend <rhyme label="a" type="f">kimmen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die langs den Heemel <rhyme label="b" type="m">spoed</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Doed zyne glansen <rhyme label="a" type="f" corresp="#s2:l1">klimmen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met goude Oranje <rhyme label="b" type="m" corresp="#s2:l2">gloed</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Men ziet zyn schoone <rhyme label="c" type="f">straalen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Aan 't blaauwe <rhyme label="d" type="m">Firmament</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">In al zyn luister <rhyme label="c" type="f" corresp="#s2:l5">praalen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Die nimmer weergae <rhyme label="d" type="m" corresp="#s2:l6">kend</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De ontrolde Vlaggen <rhyme label="a" type="f">waaijen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van Huis en <rhyme label="b" type="m">Toorentrans</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">De Oranje Wimpels <rhyme label="a" type="f" corresp="#s3:l1">draaijen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En ieder gaat ten <rhyme label="b" type="m" corresp="#s3:l2">dans</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Het Knegtje met zyn <rhyme label="c" type="f">Meisje</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gaat hup'lend twee aan <rhyme label="d" type="m">twee</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Zy zoenen reis op <rhyme label="c" type="f" corresp="#s3:l5">reisje</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Matroosje roept <rhyme label="d" type="m" corresp="#s3:l6">Hoezée</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men hoord de golfjes <rhyme label="a" type="f">bruisschen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Daar 't kab'lend stroompje <rhyme label="b" type="m">vloeid</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En 't Zuiderwindje <rhyme label="a" type="f" corresp="#s4:l1">ruisschen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat ieders ooren <rhyme label="b" type="m" corresp="#s4:l2">boeid</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Men hoord de Klokken <rhyme label="c" type="f">speelen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Met vreugde hoord men <rhyme label="d" type="m">an</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Men duizend deuntjes <rhyme label="c" type="f" corresp="#s4:l5">kweelen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">De Stad weergalmt 'er <rhyme label="d" type="m" corresp="#s4:l6">van</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'t Is byna veertig <rhyme label="a" type="f">jaaren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat men 't <rhyme label="b" type="m">Stadhouderschap</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zig Erffelyk <rhyme label="a" type="f" corresp="#s5:l1">verklaaren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Met juichend <rhyme label="b" type="m" corresp="#s5:l2">handgeklap</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En wie, 't zy kleine of <rhyme label="c" type="f">grooten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Zal dulden dat m' een <rhyme label="d" type="m">Vorst</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Zou van den Zetel <rhyme label="c" type="f" corresp="#s5:l5">stooten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Die 's Lands belangen <rhyme label="d" type="m" corresp="#s5:l6">torst</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Neen, 't zy Oranje <rhyme label="a" type="f">boven</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Door 't gantsche <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Men zal steeds Holland <rhyme label="a" type="f" corresp="#s6:l1">looven</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van hier, van Strand tot <rhyme label="b" type="m" corresp="#s6:l2">Strand</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Men zal den naam dier <rhyme label="c" type="f">Helden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">By ons zo zeer <rhyme label="d" type="m">geächt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Met roem en eer <rhyme label="c" type="f" corresp="#s6:l5">vermelden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Tot aan het <rhyme label="d" type="m" corresp="#s6:l6">Nageslacht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ziet elk zyn wensch <rhyme label="a" type="f">beklyven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Word ieders hart <rhyme label="b" type="m">verkwikt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Men mag dan vreugd <rhyme label="a" type="f" corresp="#s7:l1">bedryven</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Een ieder zy <rhyme label="b" type="m" corresp="#s7:l2">gestrikt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En tooije zig met <rhyme label="c" type="f">Lindjes</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Aan hoed, en op de <rhyme label="d" type="m">borst</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Als zo veel liefde <rhyme label="c" type="f" corresp="#s7:l5">bindjes</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Aan onzen lieven <rhyme label="d" type="m" corresp="#s7:l6">Vorst</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Prins Willem van <rhyme label="a" type="f">Nassouwe</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Blyft Stadhouder van 't <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Om Neêrland op te <rhyme label="a" type="f" corresp="#s8:l1">bouwe</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Uit zyn vervallen <rhyme label="b" type="m" corresp="#s8:l2">stand</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Men ziet in weinig <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Hem in zyn Eer <rhyme label="d" type="m">hersteld</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">En 's Vyands list en <rhyme label="c" type="f" corresp="#s8:l5">laagen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Voor eeuwig <rhyme label="d" type="m" corresp="#s8:l6">neêrgeveld</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Kom laaten w' in deez' <rhyme label="a" type="f">Muuren</rhyme>!</l>
        <l id="s9:l2" met="3" type="-+">Om Willem blyde <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En drinken met de <rhyme label="a" type="f" corresp="#s9:l1">Buuren</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Een vrolyk glaasje <rhyme label="b" type="m" corresp="#s9:l2">wyn</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">De borrel voor <rhyme label="c" type="f">gemeenen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Men brengt wel eens een <rhyme label="d" type="m">roes</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Aan 't fiere <rhyme label="c" type="f" corresp="#s9:l5">Water-leeuwjen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Avoes, avoes, <rhyme label="d" type="m" corresp="#s9:l6">avoes</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Hoezée! Oranje <rhyme label="a" type="f">boven</rhyme>!</l>
        <l id="s10:l2" met="3" type="-+">Lang leeve Nassauws <rhyme label="b" type="m">Telg</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En bloeije in Neêrlands <rhyme label="a" type="f" corresp="#s10:l1">Hooven</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Als een roemruchte <rhyme label="b" type="m" corresp="#s10:l2">Belg</rhyme>;</l>
        <l id="s10:l5" met="3" type="-+">Haast hoord m' op blyder <rhyme label="c" type="f">daagje</rhyme>;</l>
        <l id="s10:l6" met="3" type="-+">Den galm van <rhyme label="d" type="m">Hollands-rhee</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">In 't Princelyke <rhyme label="c" type="f" corresp="#s10:l5">Haagje</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Hoezée Oranje, <rhyme label="d" type="m" corresp="#s10:l6">Hoezée</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
