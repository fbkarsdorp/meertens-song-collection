        <l id="s2:l4" met="4" type="-+">Gwendt w rtt in trost vnnd <rhyme label="b" type="m" corresp="#s2:l2">freitt</rhyme>  /</l>
        <l id="s2:l5" met="2" type="-+">Das will nitt <rhyme label="c" type="m">sein</rhyme> /</l>
