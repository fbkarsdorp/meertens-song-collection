    <melody>Ik lijd in het hart pijn ongewoon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ick lijd' In thart Pijn <rhyme label="a" type="m">ongewoon</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Ick en cans <rhyme label="b" type="f">verdragen</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Wie zal Icks <rhyme label="b" type="f" corresp="#s1:l2">clagen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">die schoonheyt van myn lieffs <rhyme label="a" type="m" corresp="#s1:l1">persoon</rhyme></l>
        <l id="s1:l5" met="2" type="-+">die doet mij <rhyme label="b" type="f" corresp="#s1:l2">Jagen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">In Venus <rhyme label="b" type="f" corresp="#s1:l2">lagen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">zij die mijn t hart <rhyme label="c" type="m">doorschoot</rhyme></l>
        <l id="s1:l8" met="2" type="-+">blijft In <rhyme label="d" type="f">secreten</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Eijlaes noijt meerder <rhyme label="c" type="m" corresp="#s1:l7">noot</rhyme></l>
        <l id="s1:l10" met="3" type="-+">die t hart uijt Jonsten <rhyme label="c" type="m" corresp="#s1:l7">boot</rhyme></l>
        <l id="s1:l11" met="2" type="+-">en wilt nyet <rhyme label="d" type="f" corresp="#s1:l8">weten</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Waren van myn tot haer <rhyme label="a" type="m">gevlucht</rhyme></l>
        <l id="s2:l2" met="2" type="-+">al myn <rhyme label="b" type="f">droeffheden</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Om haer <rhyme label="b" type="f" corresp="#s2:l2">geleden</rhyme></l>
        <l id="s2:l4" met="4" type="-+">zo soud' zy weten met wat <rhyme label="a" type="m" corresp="#s2:l1">zucht</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Ic waer <rhyme label="b" type="f" corresp="#s2:l2">t'onvreden</rhyme></l>
        <l id="s2:l6" met="2" type="-+">en dat met <rhyme label="b" type="f" corresp="#s2:l2">reden</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Och woud Cupidoos <rhyme label="c" type="m">strael</rhyme></l>
        <l id="s2:l8" met="2" type="-+">haer so deur <rhyme label="d" type="f">schieten</rhyme></l>
        <l id="s2:l9" met="3" type="-+">twaeer mijn hart <rhyme label="c" type="m" corresp="#s2:l7">principael</rhyme></l>
        <l id="s2:l10" met="3" type="-+">dat Ick haer lijeff <rhyme label="c" type="m" corresp="#s2:l7">loyael</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Eens mocht <rhyme label="d" type="f" corresp="#s2:l8">genijeten</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ooch mocht Ick met <rhyme label="a" type="m">ootmoedicheijt</rhyme></l>
        <l id="s3:l2" met="2" type="-+">en laech <rhyme label="b" type="f">verneren</rhyme></l>
        <l id="s3:l3" met="2" type="-+">haer hart <rhyme label="b" type="f" corresp="#s3:l2">moveren</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dat sy als Ick me waer <rhyme label="a" type="m" corresp="#s3:l1">bereyt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">te <rhyme label="b" type="f" corresp="#s3:l2">consenteren</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Myn soet <rhyme label="b" type="f" corresp="#s3:l2">begeren</rhyme></l>
        <l id="s3:l7" met="3" type="-+">zo waer myn hart <rhyme label="c" type="m">verfraeyt</rhyme></l>
        <l id="s3:l8" met="2" type="-+">vol <rhyme label="d" type="f">melodyen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Eylaes tis al <rhyme label="c" type="m" corresp="#s3:l7">verdraeyt</rhyme></l>
        <l id="s3:l10" met="3" type="-+">mijn lust blyft <rhyme label="c" type="m" corresp="#s3:l7">ongepaeyt</rhyme></l>
        <l id="s3:l11" met="2" type="+-">t syn <rhyme label="d" type="f" corresp="#s3:l8">fantasijen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Nochtans princes gryp Ick den <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Ick <rhyme label="b" type="f">ongesonde</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Jae heel <rhyme label="b" type="f" corresp="#s4:l2">deurwonde</rhyme></l>
        <l id="s4:l4" met="4" type="-+">op hoop oft zij tot mijn <rhyme label="a" type="m" corresp="#s4:l1">voorspoet</rhyme></l>
        <l id="s4:l5" met="2" type="-+">teniger <rhyme label="b" type="f" corresp="#s4:l2">stonde</rhyme></l>
        <l id="s4:l6" met="2" type="-+">haer lieff my <rhyme label="b" type="f" corresp="#s4:l2">Jonde</rhyme></l>
        <l id="s4:l7" met="3" type="-+">In lieffd' perfeckt <rhyme label="c" type="m">volmaeckt</rhyme></l>
        <l id="s4:l8" met="2" type="-+">zal Ick <rhyme label="d" type="f">geduijren</rhyme></l>
        <l id="s4:l9" met="3" type="-+">maer zij mij thart deur <rhyme label="c" type="m" corresp="#s4:l7">blaeckt</rhyme></l>
        <l id="s4:l10" met="3" type="-+">t'soet wert selden <rhyme label="c" type="m" corresp="#s4:l7">gesmaeckt</rhyme></l>
        <l id="s4:l11" met="2" type="+-">dan met <rhyme label="d" type="f" corresp="#s4:l8">besuijren</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
