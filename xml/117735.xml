  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Op myn Fluytje wil ik <rhyme label="a" type="f">spelen</rhyme>;</l>
        <l id="s1:l2" met="4" type="+-">Vrolyk van myn <rhyme label="b" type="m">Harderin</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ik moet eens een Deuntje <rhyme label="a" type="f" corresp="#s1:l1">kwelen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Want ik haar op Trou <rhyme label="b" type="m" corresp="#s1:l2">bemin</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Echo bost myn stem dog <rhyme label="c" type="m">voord</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Dat myn Sylvia het <rhyme label="c" type="m" corresp="#s1:l5">hoord</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sylvia aanhoord myn <rhyme label="a" type="f">keeltje</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Want het tot u lof nu <rhyme label="b" type="m">zingt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat zoo klaar gelyk een <rhyme label="a" type="f" corresp="#s2:l1">Veeltje</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Door het groene Bosje <rhyme label="b" type="m" corresp="#s2:l2">klinkt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Komt dog aan op het <rhyme label="c" type="m">geluyt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Van myn zoete <rhyme label="c" type="m" corresp="#s2:l5">Harders-Fluyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ik helaas! bedroefden <rhyme label="a" type="f">Harder</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Moet alleen myn Schaapjes <rhyme label="b" type="m">hoen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En myn Sylvia gaat <rhyme label="a" type="f" corresp="#s3:l1">verder</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Wyden in het Bosjen <rhyme label="b" type="m" corresp="#s3:l2">groen</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Of ik haar dryf agter <rhyme label="c" type="m">an</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ik haar dog niet krygen <rhyme label="c" type="m" corresp="#s3:l5">kan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Harderin wilt my dog <rhyme label="a" type="f">horen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Ik bid u blyft een wynig <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Nygt tot my u klagt en <rhyme label="a" type="f" corresp="#s4:l1">oren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En ziet dog myn lyden <rhyme label="b" type="m" corresp="#s4:l2">aan</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Ziet hoe dat ik in myn <rhyme label="c" type="m">Hert</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Lyd' onlydelyke <rhyme label="c" type="m" corresp="#s4:l5">smert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Mogt ik eens myn Liefje <rhyme label="a" type="f">spreken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Daar zy Wyd al aan de <rhyme label="b" type="m">kant</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Van de klare <rhyme label="a" type="f" corresp="#s5:l1">Water-beken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Daar veel Bloempjes staan <rhyme label="b" type="m" corresp="#s5:l2">geplant</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Dat ik haar daar vinden <rhyme label="c" type="m">zou</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Verdreven was al myn <rhyme label="c" type="m" corresp="#s5:l5">rou</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dat ik haar maar wist te <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Daadelyk zou ik my <rhyme label="b" type="m">spoen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Door de digt beblade <rhyme label="a" type="f" corresp="#s6:l1">Linden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Om te komen in het <rhyme label="b" type="m" corresp="#s6:l2">Groen</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Ras myn Schaapjes treed wat <rhyme label="c" type="m">voord</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Want haar Stem heb ik <rhyme label="c" type="m" corresp="#s6:l5">gehoord</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Sylvia hoor ik daar <rhyme label="a" type="f">Zingen</rhyme>;</l>
        <l id="s7:l2" met="4" type="+-">Hemel wat een zoeten <rhyme label="b" type="m">Thoon</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Al myn Aderen die <rhyme label="a" type="f" corresp="#s7:l1">springen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Daar is geen Snare-spel zo <rhyme label="b" type="m" corresp="#s7:l2">schoon</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Want de klare Beeke <rhyme label="c" type="m">springt</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Wanneer dat myn Liefste <rhyme label="c" type="m" corresp="#s7:l5">zingt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hoe zal ik u dan met <rhyme label="a" type="f">kusjes</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">O myn zoeten <rhyme label="b" type="m">Sylvia</rhyme>;</l>
        <l id="s8:l3" met="4" type="+-">En met lieffelyke <rhyme label="a" type="f" corresp="#s8:l1">lusjes</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">In myn Armen nemen <rhyme label="b" type="m" corresp="#s8:l2">dra</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Want u zoete <rhyme label="c" type="m">Rooder-mond</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Kon myn Hart maken <rhyme label="c" type="m" corresp="#s8:l5">gezond</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Prinçes, Harderin ik <rhyme label="a" type="f">haake</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Na de uure, dat ik <rhyme label="b" type="m">jou</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Zal in d'Egten-Staat <rhyme label="a" type="f" corresp="#s9:l1">genake</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dan zult gy zyn myne <rhyme label="b" type="m" corresp="#s9:l2">Vrouw</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">En ons Schaapjes zullen <rhyme label="c" type="m">meê</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Zyn verëenigt op die <rhyme label="c" type="m" corresp="#s9:l5">steê</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
