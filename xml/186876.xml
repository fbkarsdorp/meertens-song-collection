<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186876</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>O zielsverrukte avondstond</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Kom Caetje! gaa je met me <rhyme label="a" type="m">meê</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Daer kan een glas op <rhyme label="b" type="m">staen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Ik moet eerlang alweer in <rhyme label="a" type="m" corresp="#s1:l1">Zee</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ora den Brit te <rhyme label="b" type="m" corresp="#s1:l2">slaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gy vegten, Jan, zie wat je <rhyme label="a" type="m">doet</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">Gy zyt zoo jong en <rhyme label="b" type="m">teêr</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">Jaa Meisje, maer myn hart is <rhyme label="a" type="m" corresp="#s2:l1">goed</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat kent geen jonkheid <rhyme label="b" type="m" corresp="#s2:l2">meer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gy zyt nog zulk een jonge <rhyme label="a" type="m">bloem</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Die past in Neêrlands <rhyme label="b" type="m">Tuin</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Ik vegt voor onzer Staaten <rhyme label="a" type="m" corresp="#s3:l1">roem</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al gold het kop en <rhyme label="b" type="m" corresp="#s3:l2">kruin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die Groot is wordt het ligtst <rhyme label="a" type="m">geraekt</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">De kleine schiet ook <rhyme label="b" type="m">mêe</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En staet het vastt' als alles <rhyme label="a" type="m" corresp="#s4:l1">kraekt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Door 't slingren van de <rhyme label="b" type="m" corresp="#s4:l2">Zee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een Dispens deugt niet tot den <rhyme label="a" type="m">stryd</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die kan geen Vyand <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die haekt alleen maer naer den <rhyme label="a" type="m" corresp="#s5:l1">tyd</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Om aen den dis te <rhyme label="b" type="m" corresp="#s5:l2">gaen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doch zyn vermack is <rhyme label="a" type="m">slaverny</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Maer ik waeg goed en <rhyme label="b" type="m">bloed</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Voor onze braeve <rhyme label="a" type="m" corresp="#s6:l1">Koopvaerdy</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En Neêrlands vryen <rhyme label="b" type="m" corresp="#s6:l2">Hoed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
