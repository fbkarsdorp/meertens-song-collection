<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35090</id>
    <date>
      <year-start>1600</year-start>
      <year-end>1700</year-end>
    </date>
    <tags>
      <tag standard-name="herderslied" translation="pastoral song">herderslied</tag>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">k'sagh lest een herderin <rhyme label="a" type="f">geseten</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die sacht in slaep viel in het <rhyme label="b" type="m">groen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">k'ghinck bij haer, en sonder haer <rhyme label="a" type="f" corresp="#s1:l1">weten</rhyme></l>
        <l id="s1:l4" met="2" type="-+">k'stal haer een <rhyme label="b" type="m" corresp="#s1:l2">soen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">maei goden ah, hoe dier stout <rhyme label="c" type="m">mij</rhyme></l>
        <l id="s1:l6" met="2" type="-+">die <rhyme label="c" type="m" corresp="#s1:l5">dieverij</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">zij die nouw aengeraeckt <rhyme label="a" type="f">ontweckte</rhyme></l>
        <l id="s2:l2" met="4" type="-+">sagh m' over dweers grammoedigh <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">vraeghde waer heen de stoutheid <rhyme label="a" type="f" corresp="#s2:l1">streckte</rhyme></l>
        <l id="s2:l4" met="2" type="-+">van dit <rhyme label="b" type="m" corresp="#s2:l2">bestaen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">en tot mijn druck en <rhyme label="c" type="m">ongenucht</rhyme></l>
        <l id="s2:l6" met="2" type="-+">steegh op de <rhyme label="c" type="m" corresp="#s2:l5">vlucht</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">ick dacht haer in haer loop te <rhyme label="a" type="f">stooren</rhyme></l>
        <l id="s3:l2" met="4" type="-+">en die te stutten door <rhyme label="b" type="m">gebÃªen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">dogh sij wou na geen ontschuld <rhyme label="a" type="f" corresp="#s3:l1">hooren</rhyme></l>
        <l id="s3:l4" met="2" type="-+">en sij <rhyme label="b" type="m" corresp="#s3:l2">verdween</rhyme></l>
        <l id="s3:l5" met="4" type="-+">en liet mij met spijt <rhyme label="c" type="m">overlaen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">alleenigh <rhyme label="c" type="m" corresp="#s3:l5">staen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">als ick nu sagh dat al mijn <rhyme label="a" type="f">poogen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">vergeefs was en dat ick niet <rhyme label="b" type="m">wou</rhyme></l>
        <l id="s4:l3" met="4" type="-+">volghd'ick haer eenigh op met <rhyme label="a" type="f" corresp="#s4:l1">d'oogen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">soo ver ick <rhyme label="b" type="m" corresp="#s4:l2">kon</rhyme></l>
        <l id="s4:l5" met="4" type="-+">maer voeld haest al mijn hoop <rhyme label="c" type="m">misluckt</rhyme></l>
        <l id="s4:l6" met="2" type="-+">en t'hert <rhyme label="c" type="m" corresp="#s4:l5">ontruckt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">nu kan ick anders niet als <rhyme label="a" type="f">klagen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">en sal het al mijn leven <rhyme label="b" type="m">doen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">dat ick mij voeld het hert <rhyme label="a" type="f" corresp="#s5:l1">ontdragen</rhyme></l>
        <l id="s5:l4" met="2" type="-+">door eenen <rhyme label="b" type="m" corresp="#s5:l2">soen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">die mij berooft van alle <rhyme label="c" type="m">rust</rhyme></l>
        <l id="s5:l6" met="2" type="-+">o diere <rhyme label="c" type="m" corresp="#s5:l5">lust</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Eert maer tergt noijt de min <rhyme label="a" type="f">godinne</rhyme></l>
        <l id="s6:l2" met="4" type="-+">die hier wild spotten wort <rhyme label="b" type="m">bespot</rhyme></l>
        <l id="s6:l3" met="4" type="-+">list noch bedrogh dient noijt int <rhyme label="a" type="f" corresp="#s6:l1">minne</rhyme></l>
        <l id="s6:l4" met="2" type="-+">den <rhyme label="b" type="m" corresp="#s6:l2">minnegodt</rhyme></l>
        <l id="s6:l5" met="4" type="-+">die en laet <rhyme label="c" type="m">liefde-snoeperij</rhyme></l>
        <l id="s6:l6" met="2" type="-+">noijt straffe <rhyme label="c" type="m" corresp="#s6:l5">vrij</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
