<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3178</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>spiritual song</tag>
    </tags>
    <melody>Wel Daphne dus versuft</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL die door grootsche <rhyme label="a" type="m">pracht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zijn gheeren hoogh <rhyme label="a" type="m" corresp="#s1:l1">gheacht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die worden meest <rhyme label="b" type="f">bedroghen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Al is hy schoon al tot den <rhyme label="c" type="m">top</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En boven alle sterren <rhyme label="c" type="m" corresp="#s1:l4">op</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hy wort haest <rhyme label="b" type="f" corresp="#s1:l3">neergetoghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De trotse moedt en <rhyme label="a" type="m">waen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En kan niet langh <rhyme label="a" type="m" corresp="#s2:l1">bestaen</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Die veel al waent te <rhyme label="b" type="f">weten</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En meent daer deur te zijn <rhyme label="c" type="m">ghe-eert</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">'t Is waen, en oock in waen <rhyme label="c" type="m" corresp="#s2:l4">verkeert</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Hy wordt om-veer <rhyme label="b" type="f" corresp="#s2:l3">ghesmeten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een trotse Paeuw die <rhyme label="a" type="m">swaeyt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die spieghelt en die <rhyme label="a" type="m" corresp="#s3:l1">draeyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Met hoogh-verwaende <rhyme label="b" type="f">kueren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Een yder lacht 'er met, en <rhyme label="c" type="m">spot</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Wie sou 'et by een malle <rhyme label="c" type="m" corresp="#s3:l4">Sot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Langh sonder lacchen <rhyme label="b" type="f" corresp="#s3:l3">dueren</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Is yemandt totten <rhyme label="a" type="m">trap</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van brave <rhyme label="a" type="m" corresp="#s4:l1">wetenschap</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Door konsten <rhyme label="b" type="f">opgheheven</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Indien hy hoogh van herten <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Al stondt hy noch en noch soo <rhyme label="c" type="m" corresp="#s4:l4">wis</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hy wordt'er van <rhyme label="b" type="f" corresp="#s4:l3">verdreven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gheen hooghmoedt houdt'er <rhyme label="a" type="m">stee</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Gheen trotse preutsheydt <rhyme label="a" type="m" corresp="#s5:l1">mee</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Maer alles sal <rhyme label="b" type="f">verdwijnen</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">De Heer, een LIEVERvan 'et <rhyme label="c" type="m">laegh</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Die sendt ghemeenelijck een <rhyme label="c" type="m" corresp="#s5:l4">plaegh</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Als trotse heerlijck <rhyme label="b" type="f" corresp="#s5:l3">schijnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Haman, hoe hoogh hy <rhyme label="a" type="m">was</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hy viel als in de <rhyme label="a" type="m" corresp="#s6:l1">ass'</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Hoe naeder aen sijn <rhyme label="b" type="f">machten</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Hoe meer hy totte Wolcken <rhyme label="c" type="m">klam</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Hoe meer hy voor den Heere <rhyme label="c" type="m" corresp="#s6:l4">quam</rhyme></l>
        <l id="s6:l6" met="3" type="-+">In leelijcker <rhyme label="b" type="f" corresp="#s6:l3">verachten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De waen is yd'le <rhyme label="a" type="m">windt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die lichtelijck <rhyme label="a" type="m" corresp="#s7:l1">verswindt</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Soo oock 't hooghmoedigh <rhyme label="b" type="f">brallen</rhyme>;</l>
        <l id="s7:l4" met="4" type="-+">Ghenoeght u met de laghe <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En houdt u leeghjes achter 't <rhyme label="c" type="m" corresp="#s7:l4">landt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Soo sulje niet hoogh <rhyme label="b" type="f" corresp="#s7:l3">vallen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
