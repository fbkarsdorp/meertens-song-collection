  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KOmt Parnassus Zang <rhyme label="a" type="f">Godinnen</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Voegt uw stemmen by de <rhyme label="b" type="m">myn</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Hoort uw lof met vreugt <rhyme label="a" type="f" corresp="#s1:l1">beginnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Echo zal getuigen <rhyme label="b" type="m" corresp="#s1:l2">zyn</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Staat Musica zeer hoog <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">U zy goon en menschen <rhyme label="d" type="m">gunst</rhyme>!</l>
        <l id="s1:l7" met="4" type="+-">Wy zyn u altyd <rhyme label="c" type="f" corresp="#s1:l5">genegen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Wilt ons hert tot Zang <rhyme label="c" type="f" corresp="#s1:l5">beweegen</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Zeegent ook ons zoete <rhyme label="d" type="m" corresp="#s1:l6">kunst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Konst Liefhebbers wilt <rhyme label="a" type="f">vergaaren</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Speelt op Bas Phiool en <rhyme label="b" type="m">Fluit</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Toont Parnassus blyde <rhyme label="a" type="f" corresp="#s2:l1">maaren</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Laat ons maken zoet <rhyme label="b" type="m" corresp="#s2:l2">geluit</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Staat Musica zeer hoog <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">U zy goon en menschen <rhyme label="d" type="m">gunst</rhyme>!</l>
        <l id="s2:l7" met="4" type="+-">Wy zyn u altyd <rhyme label="c" type="f" corresp="#s2:l5">genegen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Wilt ons hert tot Zang <rhyme label="c" type="f" corresp="#s2:l5">beweegen</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Zeegent ook ons zoete <rhyme label="d" type="m" corresp="#s2:l6">kunst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gelaurierde konst <rhyme label="a" type="f">Godinnen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Schoone Maagt, <rhyme label="b" type="m">Retorica</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">God Apollo moet u <rhyme label="a" type="f" corresp="#s3:l1">minnen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Nu verselt met <rhyme label="b" type="m" corresp="#s3:l2">Musica</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Staat Musica zeer hoog <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">U zy goon en menschen <rhyme label="d" type="m">gunst</rhyme>!</l>
        <l id="s3:l7" met="4" type="+-">Wy zyn u altyd <rhyme label="c" type="f" corresp="#s3:l5">genegen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Wilt ons hert tot Zang <rhyme label="c" type="f" corresp="#s3:l5">beweegen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Zeegent ook ons zoete <rhyme label="d" type="m" corresp="#s3:l6">kunst</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wilt ons blyde blydschap <rhyme label="a" type="f">tuygen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Zoet en aardig pluym <rhyme label="b" type="m">gediert</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Wilt met Zang en Speeltuig <rhyme label="a" type="f" corresp="#s4:l1">juygen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Zoete Jeugt Parnassus <rhyme label="b" type="m" corresp="#s4:l2">viert</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Staat Musica, zeer hoog <rhyme label="c" type="f">verheeven</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">U zy goon en menschen <rhyme label="d" type="m">gunst</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">Wy zyn u altyd <rhyme label="c" type="f" corresp="#s4:l5">genegen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Wilt ons hert tot Zang <rhyme label="c" type="f" corresp="#s4:l5">bewegen</rhyme>;</l>
        <l id="s4:l9" met="4" type="+-">Zeegent ook ons zoete <rhyme label="d" type="m" corresp="#s4:l6">kunst</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
