  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Het moet nu klingen over <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Met lof en oock met rijcken <rhyme label="a" type="m" corresp="#s1:l1">schal</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Syon met der vroomen <rhyme label="a" type="m" corresp="#s1:l1">ghetal</rhyme>,</l>
        <l id="s1:l4" met="6" type="-+">Hy is erschenen, den ons gebooren <rhyme label="b" type="m">Maria</rhyme>,</l>
        <l id="s1:l5" met="6" type="-+">Het is vervult, dat ons verkondicht heeft <rhyme label="c" type="m">Gabriel</rhyme>,</l>
        <l id="s1:l6" met="6" type="-+">Eia, Eia, die Ionckfrou Godt geboren <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s1:l7" met="6" type="+-">Als die Godtlicke wijsheyt sich verwilliget <rhyme label="d" type="m" corresp="#s1:l6">heeft</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het is gebooren <rhyme label="a" type="m">Emanuel</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Den ons verkondigt <rhyme label="a" type="m" corresp="#s2:l1">Gabriel</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat betuygt <rhyme label="a" type="m" corresp="#s2:l1">Ezechiel</rhyme>,</l>
        <l id="s2:l4" met="6" type="-+">Hy is erschenen, den ons gebooren <rhyme label="b" type="m">Maria</rhyme>,</l>
        <l id="s2:l5" met="6" type="-+">Het is vervult, dat ons verkondicht heeft <rhyme label="c" type="m">Gabriel</rhyme>,</l>
        <l id="s2:l6" met="6" type="-+">Eia, Eia, die Ionckfrou Godt geboren <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s2:l7" met="6" type="+-">Als die Godtlicke wijsheyt sich verwilliget <rhyme label="d" type="m" corresp="#s2:l6">heeft</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy jonge kinder singt den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Den nyen Koninck brengt die <rhyme label="a" type="m" corresp="#s3:l1">eer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met aendacht lovet God den <rhyme label="a" type="m" corresp="#s3:l1">Heer</rhyme>,</l>
        <l id="s3:l4" met="6" type="-+">Hy is erschenen, den ons gebooren <rhyme label="b" type="m">Maria</rhyme>,</l>
        <l id="s3:l5" met="6" type="-+">Het is vervult, dat ons verkondicht heeft <rhyme label="c" type="m">Gabriel</rhyme>,</l>
        <l id="s3:l6" met="6" type="-+">Eia, Eia, die Ionckfrou Godt geboren <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s3:l7" met="6" type="+-">Als die Godtlicke wijsheyt sich verwilliget <rhyme label="d" type="m" corresp="#s3:l6">heeft</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sion prijst dit kindeken <rhyme label="a" type="m">cleyn</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Den heylant welcker ons <rhyme label="a" type="m" corresp="#s4:l1">alleyn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Maeckt van allen sonden <rhyme label="a" type="m" corresp="#s4:l1">reyn</rhyme></l>
        <l id="s4:l4" met="6" type="-+">Hy is erschenen, den ons gebooren <rhyme label="b" type="m">Maria</rhyme>,</l>
        <l id="s4:l5" met="6" type="-+">Het is vervult, dat ons verkondicht heeft <rhyme label="c" type="m">Gabriel</rhyme>,</l>
        <l id="s4:l6" met="6" type="-+">Eia, Eia, die Ionckfrou Godt geboren <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s4:l7" met="6" type="+-">Als die Godtlicke wijsheyt sich verwilliget <rhyme label="d" type="m" corresp="#s4:l6">heeft</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daerom so laet ons singen <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Met aendacht en met frewden <rhyme label="a" type="m" corresp="#s5:l1">schal</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Den nyen Koninck <rhyme label="a" type="m" corresp="#s5:l1">altemael</rhyme>,</l>
        <l id="s5:l4" met="6" type="-+">Hy is erschenen, den ons gebooren <rhyme label="b" type="m">Maria</rhyme>,</l>
        <l id="s5:l5" met="6" type="-+">Het is vervult, dat ons verkondicht heeft <rhyme label="c" type="m">Gabriel</rhyme>,</l>
        <l id="s5:l6" met="6" type="-+">Eia, Eia, die Ionckfrou Godt geboren <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s5:l7" met="6" type="+-">Als die Godtlicke wijsheyt sich verwilliget <rhyme label="d" type="m" corresp="#s5:l6">heeft</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Singt en looft den Heere <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die ons gevoert heeft wt den <rhyme label="a" type="m" corresp="#s6:l1">doot</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En deylt ons met dat eewig <rhyme label="a" type="m" corresp="#s6:l1">goet</rhyme>.</l>
        <l id="s6:l4" met="6" type="-+">Hy is erschenen, den ons gebooren <rhyme label="b" type="m">Maria</rhyme>,</l>
        <l id="s6:l5" met="6" type="-+">Het is vervult, dat ons verkondicht heeft <rhyme label="c" type="m">Gabriel</rhyme>,</l>
        <l id="s6:l6" met="6" type="-+">Eia, Eia, die Ionckfrou Godt geboren <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s6:l7" met="6" type="+-">Als die Godtlicke wijsheyt sich verwilliget <rhyme label="d" type="m" corresp="#s6:l6">heeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
