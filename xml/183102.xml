<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183102</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Volzalig God en <rhyme label="a" type="m">Opperheer</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">Ei! zie tog uit Uw Sion <rhyme label="a" type="m" corresp="#s1:l1">neêr</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Op onze <rhyme label="b" type="f">Erfstadhouder</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Neem Gy tog zyne zaak ter <rhyme label="c" type="m">hand</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Ach! schenk hem Geestes <rhyme label="c" type="m" corresp="#s1:l4">onderstand</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Door Uwe sterke <rhyme label="b" type="f" corresp="#s1:l3">schouder</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laat hem niet langer zyn <rhyme label="a" type="m">bedroeft</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Toon dat Gy weet wat hem <rhyme label="a" type="m" corresp="#s2:l1">behoeft</rhyme></l>
        <l id="s2:l3" met="3" type="-+">In deez' bedroefde <rhyme label="b" type="f">daagen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Aan U klaag ik zyn <rhyme label="c" type="m">harteleed</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Zyn Vyanden staan steeds <rhyme label="c" type="m" corresp="#s2:l4">gereed</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met leugens, list en <rhyme label="b" type="f" corresp="#s2:l3">laagen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ei! laat Gy hem eens zien het <rhyme label="a" type="m">regt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Dat Gy Uw Volk hebt <rhyme label="a" type="m" corresp="#s3:l1">toegezegd</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Tot spyt van zyn <rhyme label="b" type="f">Vyanden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wees Gy hem tot een <rhyme label="c" type="m">Toeverlaat</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Toon dat Gy nimmer vaaren <rhyme label="c" type="m" corresp="#s3:l4">laat</rhyme></l>
        <l id="s3:l6" met="3" type="-+">De werken Uwer <rhyme label="b" type="f" corresp="#s3:l3">handen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'t Heeft immers U welëer <rhyme label="a" type="m">behaagt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Als Hy van U wierd <rhyme label="a" type="m" corresp="#s4:l1">afgevraagd</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Aan ons een Prins te <rhyme label="b" type="f">geeven</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En, zoud Gy dan gedoogen, <rhyme label="c" type="m">Heer</rhyme>!</l>
        <l id="s4:l5" met="4" type="-+">Dat zulk een Vorst zou vallen <rhyme label="c" type="m" corresp="#s4:l4">neêr</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In 't bloeijen van zyn <rhyme label="b" type="f" corresp="#s4:l3">leeven</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Allerhoogste <rhyme label="a" type="m">Majesteit</rhyme>!</l>
        <l id="s5:l2" met="4" type="-+">O Koning, vol van <rhyme label="a" type="m" corresp="#s5:l1">Heerlykheid</rhyme>!</l>
        <l id="s5:l3" met="3" type="-+">Spaar Willem van <rhyme label="b" type="f">Oranje</rhyme>!</l>
        <l id="s5:l4" met="4" type="-+">Wees Gy steeds zyne hulp en <rhyme label="c" type="m">trouw</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Leer ons bedenken hoe <rhyme label="c" type="m" corresp="#s5:l4">Nassouw</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ons heeft verlost van <rhyme label="b" type="f" corresp="#s5:l3">Spanje</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zyn Vaders stelde tot een <rhyme label="a" type="m">pand</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Haar goed en bloed voor 't <rhyme label="a" type="m" corresp="#s6:l1">Vaderland</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Zy gingen voor ons <rhyme label="b" type="f">stryden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Wat eer krygt hier voor Friso's <rhyme label="c" type="m">Zoon</rhyme>!</l>
        <l id="s6:l5" met="4" type="-+">Niet anders dan als smaad en <rhyme label="c" type="m" corresp="#s6:l4">hoon</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ei! wil hem tog <rhyme label="b" type="f" corresp="#s6:l3">verblyden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ach! ziet hem aan in <rhyme label="a" type="m">liefdemin</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Uw oog zy op zyn <rhyme label="a" type="m" corresp="#s7:l1">Gemaalin</rhyme></l>
        <l id="s7:l3" met="3" type="-+">En Vorstelyke <rhyme label="b" type="f">Kinderen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Neem Gy eens alle ramp en <rhyme label="c" type="m">kruis</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Van 't Vorstelyk <rhyme label="c" type="m" corresp="#s7:l4">Oranje-huis</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Op dat hem niets meer <rhyme label="b" type="f" corresp="#s7:l3">hinderen</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ootmoedig smeek ik van hem <rhyme label="a" type="m">af</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Wees Gy hem tot een stok en <rhyme label="a" type="m" corresp="#s8:l1">staf</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Om hem te <rhyme label="b" type="f">ondersteunen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Schenk hem Uw gunst uit Uwen <rhyme label="c" type="m">Troon</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Laat hem op Jezus, Uwen <rhyme label="c" type="m" corresp="#s8:l4">Zoon</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En Zyne Sterkte, <rhyme label="b" type="f" corresp="#s8:l3">leunen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ach Heer! schenk Willem kragt na <rhyme label="a" type="m">kruis</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Al kwam 'er dan een helsch <rhyme label="a" type="m" corresp="#s9:l1">gespuis</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Dat hem zou willen <rhyme label="b" type="f">krenken</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Al dryft men staâg met hem den <rhyme label="c" type="m">spot</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Wees Gy zyn Redder, groote <rhyme label="c" type="m" corresp="#s9:l4">God</rhyme>!</l>
        <l id="s9:l6" met="3" type="-+">Ach! wil hem tog <rhyme label="b" type="f" corresp="#s9:l3">gedenken</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hier breng ik my eens in de <rhyme label="a" type="m">zin</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Hoe Jezus op een <rhyme label="a" type="m" corresp="#s10:l1">Ezelin</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Jeruzalem kwam <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">De Schaaren hadden zeer <rhyme label="c" type="m">verblyd</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Hun kleed'ren op den weg <rhyme label="c" type="m" corresp="#s10:l4">gespreid</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Zy wilde Jezus <rhyme label="b" type="f" corresp="#s10:l3">minnen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">De Schaaren riepen <rhyme label="a" type="m">overluid</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Met groot geroep, Hosanna! <rhyme label="a" type="m" corresp="#s11:l1">uit</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Tot in de hoogste <rhyme label="b" type="f">Hemel</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">'t Geluid klonk zelf tot in Gods <rhyme label="c" type="m">Troon</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Men riep Hosanna, Davids <rhyme label="c" type="m" corresp="#s11:l4">Zoon</rhyme>!</l>
        <l id="s11:l6" met="3" type="-+">Het was 'er vol <rhyme label="b" type="f" corresp="#s11:l3">gewemel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">De Dogteren Sions, vol van <rhyme label="a" type="m">vreugd</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Die zagen daar de waare <rhyme label="a" type="m" corresp="#s12:l1">Deugd</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Haar hoog verheven <rhyme label="b" type="f">Koning</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Men zag hoe het Beloofde <rhyme label="c" type="m">Lam</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Met vreugde daar zyn intrek <rhyme label="c" type="m" corresp="#s12:l4">nam</rhyme></l>
        <l id="s12:l6" met="3" type="-+">In haare <rhyme label="b" type="f" corresp="#s12:l3">Tempelwooning</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Maar ziet, zeer kort na deeze <rhyme label="a" type="m">tyd</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Was weg al deeze <rhyme label="a" type="m" corresp="#s13:l1">vrolykheid</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">De Dogteren Sions <rhyme label="b" type="f">treurden</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">De Jooden met een luider <rhyme label="c" type="m">stem</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Riepen te zaam, kruist Hem! kruist <rhyme label="c" type="m" corresp="#s13:l4">Hem</rhyme>!</l>
        <l id="s13:l6" met="3" type="-+">'t Geen ook weldra <rhyme label="b" type="f" corresp="#s13:l3">gebeurde</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Het was ruim agt-en-dertig <rhyme label="a" type="m">jaar</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Toen kwam het Neêrlands Volk te <rhyme label="a" type="m" corresp="#s14:l1">gaar</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Aan U ootmoedig <rhyme label="b" type="f">vraagen</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Zy smeekten voor de Oranje <rhyme label="c" type="m">stam</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Dat 'er een Manlyk' Spruite <rhyme label="c" type="m" corresp="#s14:l4">kwam</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Het was Uw <rhyme label="b" type="f" corresp="#s14:l3">welbehaagen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">O Hemel-Koning! groote <rhyme label="a" type="m">God</rhyme>!</l>
        <l id="s15:l2" met="4" type="-+">Gy schonk aan Neêrlands Volk dat <rhyme label="a" type="m" corresp="#s15:l1">lot</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Gy kwam een Prins te <rhyme label="b" type="f">geeven</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Men riep met stemme <rhyme label="c" type="m">overluid</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Wy hebben een <rhyme label="c" type="m" corresp="#s15:l4">Oranje-Spruit</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">God spaar hem lang in 't <rhyme label="b" type="f" corresp="#s15:l3">leeven</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Het Neêrlands Volk was zeer <rhyme label="a" type="m">verblyd</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Maar wat beleeft men nu een <rhyme label="a" type="m" corresp="#s16:l1">tyd</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Vol angst, vol schrik en <rhyme label="b" type="f">vreezen</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Veel Nederlanders zyn van <rhyme label="c" type="m">zins</rhyme></l>
        <l id="s16:l5" met="4" type="-+">Om uitteroeijen deeze <rhyme label="c" type="m" corresp="#s16:l4">Prins</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Zy zeggen, weg met <rhyme label="b" type="f" corresp="#s16:l3">deezen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">O Alvermogend <rhyme label="a" type="m">Hemelheer</rhyme>!</l>
        <l id="s17:l2" met="4" type="-+">Zie op onz' Erfstadhouder <rhyme label="a" type="m" corresp="#s17:l1">neêr</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Met oogen van <rhyme label="b" type="f">genade</rhyme>!</l>
        <l id="s17:l4" met="4" type="-+">Ach! sterk hem tog in al zyn <rhyme label="c" type="m">druk</rhyme>,</l>
        <l id="s17:l5" met="4" type="-+">Verlos hem van dat zwaare <rhyme label="c" type="m" corresp="#s17:l4">juk</rhyme></l>
        <l id="s17:l6" met="3" type="-+">Waarmeê hy word <rhyme label="b" type="f" corresp="#s17:l3">belaaden</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Versterk Gy hem door Uwen <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">En laat nem tog voor 't <rhyme label="a" type="m" corresp="#s18:l1">allermeest</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Looven Uw Heilige <rhyme label="b" type="f">Naamen</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">Laat hem hierna in <rhyme label="c" type="m">Heerlykheid</rhyme></l>
        <l id="s18:l5" met="4" type="-+">By al Uw Engelen zyn <rhyme label="c" type="m" corresp="#s18:l4">verblyd</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Met Koning Jezus! <rhyme label="b" type="f" corresp="#s18:l3">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
