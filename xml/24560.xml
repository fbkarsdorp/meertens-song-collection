<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24560</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag>christmas song</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IN alle der werlt en vyndic <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">noch in den hemel hier <rhyme label="b" type="f">bauen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">dat gades mynne bauen <rhyme label="a" type="m" corresp="#s1:l1">gaet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">wie cans toe volle <rhyme label="b" type="f" corresp="#s1:l2">gelauen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat wille wy nv voertbrengen <rhyme label="a" type="m">yet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">van deser hoger <rhyme label="b" type="f">mynnen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">want sy recht wt toekundigen <rhyme label="a" type="m" corresp="#s2:l1">is</rhyme></l>
        <l id="s2:l4" met="3" type="-+">ten kan geen man <rhyme label="b" type="f" corresp="#s2:l2">besynnen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nochtans en hoert ons te <rhyme label="a" type="m">maele</rhyme></l>
        <l id="s3:l2" met="3" type="-+">van deser mynnen te <rhyme label="b" type="f">swygen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">van soe wille wy syngen <rhyme label="a" type="m" corresp="#s3:l1">yet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">op dat wi se moegen <rhyme label="b" type="f" corresp="#s3:l2">vercrygen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Des enghels nyt was hem te <rhyme label="a" type="m">swaer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">hi stont in groten <rhyme label="b" type="f">stryden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">hi gaf consent hi voel van <rhyme label="a" type="m" corresp="#s4:l1">daer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">des moste wi alle <rhyme label="b" type="f" corresp="#s4:l2">liden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die tyt volquam ende god <rhyme label="a" type="m">ontboet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Marien der Jonger <rhyme label="b" type="f">maget</rhyme></l>
        <l id="s5:l3" met="4" type="-+">eene nye bosscop blyscap <rhyme label="a" type="m" corresp="#s5:l1">groet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">soe dat sy waert <rhyme label="b" type="f" corresp="#s5:l2">versaget</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghegruet systu sprack <rhyme label="a" type="m">gabryel</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Maria vol <rhyme label="b" type="f">genaden</rhyme></l>
        <l id="s6:l3" met="4" type="-+">du salste baren <rhyme label="a" type="m" corresp="#s6:l1">emanuel</rhyme></l>
        <l id="s6:l4" met="3" type="-+">soe heeft sich god <rhyme label="b" type="f" corresp="#s6:l2">beraden</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maria vrouwe ontvrucht v <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">ontfanget den sone <rhyme label="b" type="f">gaedes</rhyme></l>
        <l id="s7:l3" met="4" type="-+">hi is almechtich diet <rhyme label="a" type="m" corresp="#s7:l1">gebiet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">wes dancber syns <rhyme label="b" type="f" corresp="#s7:l2">gebodes</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Doe sprack die weerde <rhyme label="a" type="m">Conynginne</rhyme></l>
        <l id="s8:l2" met="3" type="-+">wt al oers hertsen <rhyme label="b" type="f">gronde</rhyme></l>
        <l id="s8:l3" met="4" type="-+">ic byn syn snoede <rhyme label="a" type="m" corresp="#s8:l1">deernekijn</rhyme></l>
        <l id="s8:l4" met="3" type="-+">my geschie nae dinen <rhyme label="b" type="f" corresp="#s8:l2">woerden</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Doe quam die hele godheit <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Jhesus marien <rhyme label="b" type="f">soene</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Och laet ons alle vrolic <rhyme label="a" type="m" corresp="#s9:l1">syn</rhyme></l>
        <l id="s9:l4" met="3" type="-+">die mystroest is <rhyme label="b" type="f" corresp="#s9:l2">verdreuen</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ghebaren is dat <rhyme label="a" type="m">kyndekyn</rhyme></l>
        <l id="s10:l2" met="3" type="-+">dat sal ons tleuen <rhyme label="b" type="f">geuen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">dat heeft gedaen doe mynne <rhyme label="a" type="m" corresp="#s10:l1">syn</rhyme></l>
        <l id="s10:l4" met="3" type="-+">gelaeuet soe moet hi <rhyme label="b" type="f" corresp="#s10:l2">wesen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
