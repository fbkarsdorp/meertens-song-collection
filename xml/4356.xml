<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4356</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laura zat laatst bij de beek</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EVen als de nugt're <rhyme label="a" type="m">Zon</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In het oost begon te <rhyme label="b" type="f">blozen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Hoorden ik, aan <rhyme label="a" type="m" corresp="#s1:l1">d'Ooster-bron</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Mijn Korinne, in schaauw van <rhyme label="b" type="f" corresp="#s1:l2">rozen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">In haar droom een zugje <rhyme label="b" type="f" corresp="#s1:l2">lozen</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Naar het zugje een lief <rhyme label="c" type="m">geluit</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Uit haar lieve lippen, <rhyme label="d" type="f">glipte'</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">'t Geen mijn lievend' harte <rhyme label="d" type="f" corresp="#s1:l7">kipte'</rhyme>:</l>
        <l id="s1:l9" met="4" type="+-">Titer! Titer! hier 's u <rhyme label="c" type="m" corresp="#s1:l6">Bruit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Lieve Titer! die mijn <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Boeit met diamante <rhyme label="b" type="f">koorden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die mijn jonge ziel <rhyme label="a" type="m" corresp="#s2:l1">(verwart</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">In een net van zoete <rhyme label="b" type="f" corresp="#s2:l2">woorden</rhyme>,)</l>
        <l id="s2:l5" met="4" type="+-">Stranden dee aan Cipris <rhyme label="b" type="f" corresp="#s2:l2">Oorden</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Kom verzel uw' <rhyme label="c" type="m">harderin</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En verzaat uw' grage <rhyme label="d" type="f">lusjes</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Met de keur van keur'ge <rhyme label="d" type="f" corresp="#s2:l7">kusjes</rhyme>;</l>
        <l id="s2:l9" met="4" type="+-">Viert de tomen van uw' <rhyme label="c" type="m" corresp="#s2:l6">Min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Al de lusjes, al 't <rhyme label="a" type="m">gemal</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Is hier vailig in de <rhyme label="b" type="f">blaatjes</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Boom, en rots, en berg en <rhyme label="a" type="m" corresp="#s3:l1">dal</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Klappen noit de kriele <rhyme label="b" type="f" corresp="#s3:l2">daatjes</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Die, van de verliefde <rhyme label="b" type="f" corresp="#s3:l2">maatjes</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">In het vrijen, vaak <rhyme label="c" type="m">geschiên</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Titer mag Korinne, in de <rhyme label="d" type="f">elzen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Vailig strelen en <rhyme label="d" type="f" corresp="#s3:l7">omhelzen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">'t Lofje schut; wie zal 't <rhyme label="c" type="m" corresp="#s3:l6">verspiên</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik (tot lieve lust <rhyme label="a" type="m">getergt</rhyme>)</l>
        <l id="s4:l2" met="4" type="+-">Drukte duizentmaal de <rhyme label="b" type="f">tippen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Van het koralijn <rhyme label="a" type="m" corresp="#s4:l1">gebergt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">VVaar, door alabaste <rhyme label="b" type="f" corresp="#s4:l2">klippen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Ambrozijne geurtjes <rhyme label="b" type="f" corresp="#s4:l2">slippen</rhyme>:</l>
        <l id="s4:l6" met="4" type="+-">VVijl mijn hart in lusjes <rhyme label="c" type="m">swom</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Liet mijn Dageraat de <rhyme label="d" type="f">dekken</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Van haar Zonnetjes <rhyme label="d" type="f" corresp="#s4:l7">vertrekken</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">En riep, vriend'lijk, <rhyme label="c" type="m" corresp="#s4:l6">wellekom</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
