    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SUjo, Sujo, Kintje <rhyme label="a" type="m">doe</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Uw oogjes <rhyme label="a" type="m" corresp="#s1:l1">toe</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Leg uw kleine hooftjen <rhyme label="b" type="m">neêr</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Al dat schreyen doet geen <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Myn lieve <rhyme label="c" type="m" corresp="#s1:l4">bloet</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Want uw oogjes zyn te <rhyme label="b" type="m" corresp="#s1:l3">teêr</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">En de rust u meerder <rhyme label="d" type="m">voed</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Dan spyze <rhyme label="d" type="m" corresp="#s1:l7">doet</rhyme>:</l>
        <l id="s1:l9" met="4" type="-+">Daarom slaap toch <rhyme label="e" type="m">gerust</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Toch gerust, Myn lieve <rhyme label="e" type="m" corresp="#s1:l9">lust</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Al die snikjes <rhyme label="f" type="m">zyn</rhyme></l>
        <l id="s1:l12" met="3" type="-+">My d'allerzwaarste <rhyme label="f" type="m" corresp="#s1:l11">pyn</rhyme></l>
        <l id="s1:l13" met="4" type="+-">En het maak my zeer <rhyme label="e" type="m" corresp="#s1:l9">ongerust</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sluit, myn alderliefste <rhyme label="a" type="m">guit</rhyme>.</l>
        <l id="s2:l2" met="2" type="-+">Ey lieve, <rhyme label="a" type="m" corresp="#s2:l1">sluit</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Ras uw duiven oogjes <rhyme label="b" type="m">digt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hoe of hy zo geem'lik <rhyme label="c" type="m">is</rhyme>?</l>
        <l id="s2:l5" met="2" type="-+">Ik loof <rhyme label="c" type="m" corresp="#s2:l4">gewis</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat hier is te sterken <rhyme label="b" type="m" corresp="#s2:l3">ligt</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Daaerom breng my hier <rhyme label="d" type="m">gezwint</rhyme></l>
        <l id="s2:l8" met="2" type="-+">De wieg en 't <rhyme label="d" type="m" corresp="#s2:l7">kint</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">Liefje slaap, sujo <rhyme label="e" type="m">soe</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Sujo soe, zyt wel te <rhyme label="e" type="m" corresp="#s2:l9">moe</rhyme>.</l>
        <l id="s2:l11" met="3" type="+-">Wieg wat met <rhyme label="f" type="m">gemak</rhyme>:</l>
        <l id="s2:l12" met="3" type="-+">Zyn oogjes staan al <rhyme label="f" type="m" corresp="#s2:l11">strak</rhyme>,</l>
        <l id="s2:l13" met="4" type="+-">En zo datelyk zyn zy <rhyme label="e" type="m" corresp="#s2:l9">toe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zoele weste-wintje, <rhyme label="a" type="m">ruis</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Met geen <rhyme label="a" type="m" corresp="#s3:l1">gedruis</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Nu myn lieve kintje <rhyme label="b" type="m">slaapt</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Zoete Papegaitje, <rhyme label="c" type="m">maak</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Toch geen <rhyme label="c" type="m" corresp="#s3:l4">gekaak</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Daar myn Poesje <rhyme label="b" type="m" corresp="#s3:l3">raapt</rhyme>.</l>
        <l id="s3:l7" met="4" type="+-">Gae hier langs toch zagtjes <rhyme label="d" type="m">heen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">En op je <rhyme label="d" type="m" corresp="#s3:l7">teen</rhyme>:</l>
        <l id="s3:l9" met="4" type="-+">Als hy slaapt, groeyt'er <rhyme label="e" type="m">van</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Groeyt'er van, zyn hertjen <rhyme label="e" type="m" corresp="#s3:l9">an</rhyme>.</l>
        <l id="s3:l11" met="3" type="+-">En zyn oogjes <rhyme label="f" type="m">zyn</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Dan vry van al de <rhyme label="f" type="m" corresp="#s3:l11">pyn</rhyme>,</l>
        <l id="s3:l13" met="4" type="+-">Die hy krygt van het <rhyme label="e" type="m" corresp="#s3:l9">getraan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
