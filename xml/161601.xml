<song>
  <meta>
    <id>161601</id>
    <date>
      <year-start>1690</year-start>
      <year-end>1690</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Lofzang van Maria</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn innigh Ziels <rhyme label="a" type="m">begeer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Rijckhalst tot u, o <rhyme label="a" type="m" corresp="#s1:l1">Heer</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Ick smeeck om <rhyme label="b" type="f">Zegeningen</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Om Wijsheydt, Deught, en <rhyme label="c" type="m">Kracht</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Soo ghy 't soo zalighst <rhyme label="c" type="m" corresp="#s1:l4">acht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Schenckt die die <rhyme label="b" type="f" corresp="#s1:l3">Iongelingen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die in geboorte <rhyme label="a" type="m">staen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">U dienst te vangen <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Tot uws Naems Lof en <rhyme label="b" type="f">Eere</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Gun haer uw Geest en <rhyme label="c" type="m">Woordt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Op dat sy voort en <rhyme label="c" type="m" corresp="#s2:l4">voort</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Veel Zielen <rhyme label="b" type="f" corresp="#s2:l3">reformeeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dat uw gezeegent <rhyme label="a" type="m">Rijck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vermeerd're, en klaer <rhyme label="a" type="m" corresp="#s3:l1">blijck</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Uws Zions, licht en <rhyme label="b" type="f">deughden</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Dat haren dienst magh <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Gelijck dien soeten <rhyme label="c" type="m" corresp="#s3:l4">Wijn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die Godt en Mensch <rhyme label="b" type="f" corresp="#s3:l3">verheughden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
