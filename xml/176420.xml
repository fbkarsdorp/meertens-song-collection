  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Altoos lustig zingen <rhyme label="a" type="f">springen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">En heel vrolyk bly van <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Nimmer knorren maar vrolyk <rhyme label="a" type="f" corresp="#s1:l1">weezen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Moet ’t Meisje zyn dat ik <rhyme label="b" type="m" corresp="#s1:l2">min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zy die altyd knorrig <rhyme label="a" type="f">kyken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Als ’t Mamaatje dreigen <rhyme label="b" type="m">ziet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Schertzen, zang en dans <rhyme label="a" type="f" corresp="#s2:l1">ontwyken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Zulke Meisjes mag ik <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">En die de kat in donker <rhyme label="a" type="f">knypen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Vallen daar zo vies niet <rhyme label="b" type="m">van</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En te dansen naar haar <rhyme label="a" type="f" corresp="#s3:l1">pypen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Leeren zy de goede <rhyme label="b" type="m" corresp="#s3:l2">Man</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maar altoos zingen lustig <rhyme label="a" type="f">springen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En heel vrolyk zyn van <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Nimmer knorren maar vrolyk <rhyme label="a" type="f" corresp="#s4:l1">weezen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Moet ’t Meisje zyn dat ik <rhyme label="b" type="m" corresp="#s4:l2">min</rhyme>.</l>
      </lg>
    </lg>
  </text>
