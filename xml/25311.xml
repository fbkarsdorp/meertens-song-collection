<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25311</id>
    <date>
      <year-start>1451</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Mijn hart is ervuld met vrolijkheid</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mijn hertz is ervullet mit <rhyme label="a" type="m">vrolicheit</rhyme></l>
        <l id="s1:l2" met="4" type="-+">van vreuden moiss ich <rhyme label="b" type="f">syngen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Vnss ist vpgegangen dat licht der <rhyme label="a" type="m" corresp="#s1:l1">selicheit</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Laist vns mit suyssen stymmen <rhyme label="b" type="f" corresp="#s1:l2">clyngen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Laist vnss suyssen sanck <rhyme label="c" type="f">anheuen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">want vns ist <rhyme label="c" type="f" corresp="#s1:l5">gegeuen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">gar wieder dat ewige <rhyme label="c" type="f" corresp="#s1:l5">leuen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">durch marien der vil reynen <rhyme label="d" type="f">juncffrauwen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vss dauidtes stamme is <rhyme label="a" type="m">vpgegangen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">eyn blomechijn der <rhyme label="b" type="f">reynicheit</rhyme></l>
        <l id="s2:l3" met="4" type="-+">van schoenden groiss mit suyssen roech <rhyme label="a" type="m" corresp="#s2:l1">vmbfangen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">maria eyn fonteyn der <rhyme label="b" type="f" corresp="#s2:l2">suyssicheit</rhyme></l>
        <l id="s2:l5" met="3" type="-+">vnss ist in ir wal <rhyme label="c" type="f">erlungen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In den hymmel ist ir ruch <rhyme label="c" type="f" corresp="#s2:l5">gedrungen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Ind hait zo sich <rhyme label="c" type="f" corresp="#s2:l5">gargedwungen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">den oeuersten konynck in guoeder <rhyme label="d" type="f">trouwen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maria du bist dat vill lustliche <rhyme label="a" type="m">preill</rhyme></l>
        <l id="s3:l2" met="4" type="-+">van allen suyssen bloemen ind <rhyme label="b" type="f">kruden</rhyme></l>
        <l id="s3:l3" met="4" type="-+">der heilgen drieuoldicheit alleyn geworden zo <rhyme label="a" type="m" corresp="#s3:l1">deil</rhyme></l>
        <l id="s3:l4" met="4" type="-+">geplantzet gesteuelt in dat <rhyme label="b" type="f" corresp="#s3:l2">suden</rhyme></l>
        <l id="s3:l5" met="3" type="-+">vss dir ist vnss zo troiste <rhyme label="c" type="f">gewassen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">sonder allen mynschen <rhyme label="c" type="f" corresp="#s3:l5">saissen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">bouen all natuyrlicher <rhyme label="c" type="f" corresp="#s3:l5">maissen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">eyn violier der <rhyme label="d" type="f">vrolicheit</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O edel iunffrauwe we wal was dir zo <rhyme label="a" type="m">mode</rhyme></l>
        <l id="s4:l2" met="4" type="-+">do ihesus in dymne schoisse <rhyme label="b" type="f">lach</rhyme></l>
        <l id="s4:l3" met="4" type="-+">dijn leue kyndelijn in dynre moderlicher <rhyme label="a" type="m" corresp="#s4:l1">hoede</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Ind dich gar vruntlichen an <rhyme label="b" type="f" corresp="#s4:l2">sach</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ind viss dynen <rhyme label="c" type="f">burstelijn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">soich dat vil reyne <rhyme label="c" type="f" corresp="#s4:l5">mylchelijn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Ind he wieder in dyn <rhyme label="c" type="f" corresp="#s4:l5">hertzelijn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">goiss syn gotliche <rhyme label="d" type="f">suyssicheit</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">In diesen vill schoenen throen hait <rhyme label="a" type="m">gesessen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">der oeuerste konynck gotz son <rhyme label="b" type="f">alweldich</rhyme></l>
        <l id="s5:l3" met="4" type="-+">In synre gotheit vnbegrijfflich <rhyme label="a" type="m" corresp="#s5:l1">vngemessen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">He hait van ir gar <rhyme label="b" type="f" corresp="#s5:l2">mynnentlich</rhyme></l>
        <l id="s5:l5" met="3" type="-+">angenoemen de mynsliche <rhyme label="c" type="f">natuyr</rhyme></l>
        <l id="s5:l6" met="3" type="-+">van allen sunden reyn ind <rhyme label="c" type="f" corresp="#s5:l5">puyr</rhyme></l>
        <l id="s5:l7" met="3" type="-+">he hait vnssgegulden <rhyme label="c" type="f" corresp="#s5:l5">garduyr</rhyme></l>
        <l id="s5:l8" met="3" type="-+">mit synes bitteren doides <rhyme label="d" type="f">dracht</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maria du vil werdige <rhyme label="a" type="m">konyncgynne</rhyme></l>
        <l id="s6:l2" met="4" type="-+">dis hymmelrichs ind ouch der <rhyme label="b" type="f">eirden</rhyme></l>
        <l id="s6:l3" met="4" type="-+">sijs vns eyn vnderstant dar zo eyn guyt <rhyme label="a" type="m" corresp="#s6:l1">begyn</rhyme></l>
        <l id="s6:l4" met="4" type="-+">dat wir selich moigen <rhyme label="b" type="f" corresp="#s6:l2">werden</rhyme></l>
        <l id="s6:l5" met="3" type="-+">als wir sullen varen van <rhyme label="c" type="f">hynne</rhyme></l>
        <l id="s6:l6" met="3" type="-+">so verlucht vns vnsse <rhyme label="c" type="f" corresp="#s6:l5">syne</rhyme></l>
        <l id="s6:l7" met="3" type="-+">van buyssen ind ouch van <rhyme label="c" type="f" corresp="#s6:l5">bynnen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">zo schouwen de heilge <rhyme label="d" type="f">dryualdicheit</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O oeuerste konynck van <rhyme label="a" type="m">hymmelrich</rhyme></l>
        <l id="s7:l2" met="4" type="-+">cristus ihesus vnss verloiser ind <rhyme label="b" type="f">here</rhyme></l>
        <l id="s7:l3" met="4" type="-+">durch dinen bitteren doit biden wir dich <rhyme label="a" type="m" corresp="#s7:l1">ynnentlich</rhyme></l>
        <l id="s7:l4" met="4" type="-+">hilff vns durch dynre moder <rhyme label="b" type="f" corresp="#s7:l2">ere</rhyme></l>
        <l id="s7:l5" met="3" type="-+">dat wir werden vrij van <rhyme label="c" type="f">sunden</rhyme></l>
        <l id="s7:l6" met="3" type="-+">van den helschen <rhyme label="c" type="f" corresp="#s7:l5">hunden</rhyme></l>
        <l id="s7:l7" met="3" type="-+">In vnss ellendigen doitz <rhyme label="c" type="f" corresp="#s7:l5">stunden</rhyme></l>
        <l id="s7:l8" met="3" type="-+">breng vns in de ewige <rhyme label="d" type="f">selicheit</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
