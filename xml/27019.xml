  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Der Heger  das iß ein sparwer <rhyme label="a" type="m">vogell</rhyme> /</l>
        <l id="s1:l2" met="4" type="-+">er spott allen andernn vogelein an der <rhyme label="b" type="f">heidenn</rhyme> /</l>
        <l id="s1:l3" met="2" type="-+">Jch hab gehortt vnnd iß <rhyme label="c" type="m">also</rhyme> /</l>
        <l id="s1:l4" met="2" type="-+">ein Weidemann g <rhyme label="c" type="m" corresp="#s1:l3">ett</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">reitt a ß nach vogelein <rhyme label="d" type="f">kleine</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Tarq artt a ff den klawenn <rhyme label="a" type="m">tratt</rhyme> /</l>
        <l id="s2:l2" met="4" type="-+">was helffen im sein falsche n <rhyme label="b" type="f">cke</rhyme>  /</l>
        <l id="s2:l3" met="2" type="-+">Ein weidemann g ett jn z  gasten <rhyme label="c" type="m">batt</rhyme> /</l>
        <l id="s2:l4" met="2" type="-+">er stach in / in ein <rhyme label="c" type="m" corresp="#s2:l3">sacke</rhyme></l>
        <l id="s2:l5" met="3" type="-+">vnnd hengede jn a ff den r <rhyme label="d" type="f">gge</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Des werenn alle Vogelein <rhyme label="a" type="m">fro</rhyme> /</l>
        <l id="s3:l2" met="4" type="-+">Das dem Heger also was <rhyme label="b" type="f">ergangenn</rhyme> /</l>
        <l id="s3:l3" met="2" type="-+">Sommer potz Velten ich g nnes jm <rhyme label="c" type="m">woll</rhyme></l>
        <l id="s3:l4" met="2" type="-+">er leschett doch <rhyme label="c" type="m" corresp="#s3:l3">viell</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">das jn doch nitt kan <rhyme label="d" type="f">brennenn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Der Heger sprach ein th mmer <rhyme label="a" type="m">gast</rhyme> /</l>
        <l id="s4:l2" met="4" type="-+">war ich vnnd flog jn <rhyme label="b" type="f">Hagedornn</rhyme> /</l>
        <l id="s4:l3" met="2" type="-+">Jch weiß nitt wie ichs versehenn <rhyme label="c" type="m">hab</rhyme> /</l>
        <l id="s4:l4" met="2" type="-+">Das mir so <rhyme label="c" type="m" corresp="#s4:l3">viell</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">a ß meinem nest sein <rhyme label="d" type="f">getragen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die Eier die ich darjnne <rhyme label="a" type="m">hatt</rhyme> /</l>
        <l id="s5:l2" met="4" type="-+">Die sein dara ß diefflich <rhyme label="b" type="f">gestolenn</rhyme> /</l>
        <l id="s5:l3" met="2" type="-+">N  woll hen / es iß woll ehr <rhyme label="c" type="m">geschehenn</rhyme> /</l>
        <l id="s5:l4" met="2" type="-+">Das des spotters ha <rhyme label="c" type="m" corresp="#s5:l3">ß</rhyme></l>
        <l id="s5:l5" met="3" type="-+">st ndt jn heischen <rhyme label="d" type="f">kolenn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">So sei Jn woll gezemmett <rhyme label="a" type="m">hatte</rhyme> /</l>
        <l id="s6:l2" met="4" type="-+">Vnnd hatt Jn so liebliche a <rhyme label="b" type="f">sgezogenn</rhyme> /</l>
        <l id="s6:l3" met="2" type="-+">K ckuck ka wa  so sagtt <rhyme label="c" type="m">ehr</rhyme> /</l>
        <l id="s6:l4" met="2" type="-+">vnnd flog <rhyme label="c" type="m" corresp="#s6:l3">hinweg</rhyme> /</l>
        <l id="s6:l5" met="3" type="-+">Das megdlein hatt ehr <rhyme label="d" type="f">bedrogenn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hett ich dich ebenn <rhyme label="a" type="m">angesehenn</rhyme> /</l>
        <l id="s7:l2" met="4" type="-+">Deinenn wortenn hett ich nitt gegle <rhyme label="b" type="f">bett</rhyme> /</l>
        <l id="s7:l3" met="2" type="-+">N  woll hin eß ist woll ehr <rhyme label="c" type="m">geschehenn</rhyme> /</l>
        <l id="s7:l4" met="2" type="-+">sich eben <rhyme label="c" type="m" corresp="#s7:l3">z</rhyme>  /</l>
        <l id="s7:l5" met="3" type="-+">So wirst  nitt <rhyme label="d" type="f">betrogenn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Eß woltt ein her spatzierenn <rhyme label="a" type="m">reittenn</rhyme> /</l>
        <l id="s8:l2" met="4" type="-+">ein so k rtze kleine <rhyme label="b" type="f">weile</rhyme>:</l>
        <l id="s8:l3" met="2" type="-+">Do wardt ehr einer tannenn <rhyme label="c" type="m">gewar</rhyme> /</l>
        <l id="s8:l4" met="2" type="-+">dara ff dar lieff ein <rhyme label="c" type="m" corresp="#s8:l3">appelein</rhyme> /</l>
        <l id="s8:l5" met="3" type="-+">vmb die <rhyme label="d" type="f">zweiche</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ehr lieff die tannenn a ff vnnd <rhyme label="a" type="m">nyeder</rhyme> /</l>
        <l id="s9:l2" met="4" type="-+">Das sei die zweig nitt mehr kh ndtt <rhyme label="b" type="f">tragen</rhyme></l>
        <l id="s9:l3" met="2" type="-+">sei fiell einenn vbergrossenn <rhyme label="c" type="m">fall</rhyme></l>
        <l id="s9:l4" met="2" type="-+">ein bein <rhyme label="c" type="m" corresp="#s9:l3">enzwei</rhyme> /</l>
        <l id="s9:l5" met="3" type="-+">Sie d rfft das nemantz <rhyme label="d" type="f">clagenn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O wee ich armes <rhyme label="a" type="m">appelein</rhyme> /</l>
        <l id="s10:l2" met="4" type="-+">Daß ich diesenn vberm tt m s <rhyme label="b" type="f">leidenn</rhyme> /</l>
        <l id="s10:l3" met="2" type="-+">Des m s ich n  ein kruppell <rhyme label="c" type="m">sein</rhyme> /</l>
        <l id="s10:l4" met="2" type="-+">Steig nitt z  <rhyme label="c" type="m" corresp="#s10:l3">hoch</rhyme> /</l>
        <l id="s10:l5" met="3" type="-+">so wirst  on schadenn <rhyme label="d" type="f">pleibenn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Deiß lyedt habenn vnß die weisenn <rhyme label="a" type="m">bedachtt</rhyme> /</l>
        <l id="s11:l2" met="4" type="-+">vonn einer a ssermassenn schoner J <rhyme label="b" type="f">nckfrawenn</rhyme> /</l>
        <l id="s11:l3" met="2" type="-+">Vnnd der nemantt z  maß <rhyme label="c" type="m">was</rhyme> /</l>
        <l id="s11:l4" met="2" type="-+">Des m ß ein <rhyme label="c" type="m" corresp="#s11:l3">Nar</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Jn Jrem armlein ra <rhyme label="d" type="f">wen</rhyme>.</l>
      </lg>
    </lg>
  </text>
