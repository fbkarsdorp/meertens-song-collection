<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187502</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zijn kost te kunnen winnen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Zyn kost te kunnen <rhyme label="a" type="f">winne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En vol verliefde <rhyme label="a" type="f" corresp="#s1:l1">zinnen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Géen eerbre Meid te <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Is dit ook wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">En elders gaan te <rhyme label="c" type="f">stoeyen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Den braaven Dochters <rhyme label="c" type="f" corresp="#s1:l5">moeyen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Nu duizend doen <rhyme label="c" type="f" corresp="#s1:l5">verbloeyen</rhyme>!...</l>
        <l id="s1:l8" met="3" type="-+">Neen, dit moet anders <rhyme label="b" type="m" corresp="#s1:l4">gaan</rhyme>;</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Wyf om ’t Geld te <rhyme label="a" type="f">trouwen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Syn Nimfje <rhyme label="a" type="f" corresp="#s2:l1">t’onderhouwen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En zo naar heen te <rhyme label="a" type="f" corresp="#s2:l1">sjouwen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Is dit ook wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Of erger dan die <rhyme label="c" type="f">Guiten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Eens anders Hof <rhyme label="c" type="f" corresp="#s2:l5">t’ontsluiten</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En daar te gaan vry <rhyme label="c" type="f" corresp="#s2:l5">buiten</rhyme>!...</l>
        <l id="s2:l8" met="3" type="-+">Neen, dit moet anders <rhyme label="b" type="m" corresp="#s2:l4">gaan</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat dunkt U, jonge <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Slecht leeven, weinig <rhyme label="a" type="f" corresp="#s3:l1">leeren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">So zyn Geslagt <rhyme label="a" type="f" corresp="#s3:l1">t’ontëeren</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">Is dit ook wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">En toch als <rhyme label="c" type="f">Hoog-gebooren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al droeg men <rhyme label="c" type="f" corresp="#s3:l5">Ezels-ooren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Te worden <rhyme label="c" type="f" corresp="#s3:l5">uitgekooren</rhyme>!...</l>
        <l id="s3:l8" met="3" type="-+">Neen dit moet anders <rhyme label="b" type="m" corresp="#s3:l4">gaan</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sich s’avons t’huis <rhyme label="a" type="f">verveelen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Sich Wyf en Kind <rhyme label="a" type="f" corresp="#s4:l1">t’ontsteelen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Voor Kaart of <rhyme label="a" type="f" corresp="#s4:l1">Dobbelspeelen</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Is dit ook wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s4:l5" met="3" type="-+">Dan nog als trotsche <rhyme label="c" type="f">zotten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Met Eer en Pligt te <rhyme label="c" type="f" corresp="#s4:l5">spotten</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">O waare <rhyme label="c" type="f" corresp="#s4:l5">Patriotte</rhyme>;...</l>
        <l id="s4:l8" met="3" type="-+">Moet dit niet anders <rhyme label="b" type="m" corresp="#s4:l4">gaan</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een braave Koopmans <rhyme label="a" type="f">natie</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Te noemen vrek en <rhyme label="a" type="f" corresp="#s5:l1">lacie</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ei zeg eens, Heer van <rhyme label="a" type="f" corresp="#s5:l1">statie</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Is dit ook wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">In ondeugt gantsch <rhyme label="c" type="f">verzoopen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Syn Rang te kunnen <rhyme label="c" type="f" corresp="#s5:l5">koopen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">De Deugd te laaten <rhyme label="c" type="f" corresp="#s5:l5">loopen</rhyme>;...</l>
        <l id="s5:l8" met="3" type="-+">Neen dit moet anders <rhyme label="b" type="m" corresp="#s5:l4">gaan</rhyme>;</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Door kuipen of door <rhyme label="a" type="f">dreigen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Te doen de Vryheid <rhyme label="a" type="f" corresp="#s6:l1">zwygen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Om zoo zyn wil te <rhyme label="a" type="f" corresp="#s6:l1">krygen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Is dit ook wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s6:l5" met="3" type="-+">Of tegen zulke <rhyme label="c" type="f">Bazen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Te doen de Vryheid <rhyme label="c" type="f" corresp="#s6:l5">raazen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Het vuur dus aan te <rhyme label="c" type="f" corresp="#s6:l5">blaazen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Neen dit moet anders <rhyme label="b" type="m" corresp="#s6:l4">gaan</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Door eigen haat <rhyme label="a" type="f">gedreeven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Voo zich alleen te <rhyme label="a" type="f" corresp="#s7:l1">leeven</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En nergens om te <rhyme label="a" type="f" corresp="#s7:l1">geeven</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Is dit ook wel <rhyme label="b" type="m">gedaan</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">Te denken in deez’ <rhyme label="c" type="f">uuren</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Het Land zal my wel <rhyme label="c" type="f" corresp="#s7:l5">duuren</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">O Neêrlands <rhyme label="c" type="f" corresp="#s7:l5">Palinuuren</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Moet dit niet anders <rhyme label="b" type="m" corresp="#s7:l4">gaan</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
