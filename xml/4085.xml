    </location>
    <tags>
      <tag standard-name="mystiek lied" translation="mystical song">mystiek lied</tag>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Nu hoor wat ik u schenk</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">1.Nu hoort wat ic v <rhyme label="a" type="f">schincke</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met desen nieuwen <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van een soe schonen <rhyme label="a" type="f" corresp="#s1:l1">vincke</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sij brengt ons groot <rhyme label="b" type="m" corresp="#s1:l2">iolijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die vader vanden <rhyme label="a" type="f">throone</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Heeft ons die vincke <rhyme label="b" type="m">ghesent</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Het is sijn eenich <rhyme label="a" type="f" corresp="#s2:l1">sone</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dats ons een lief <rhyme label="b" type="m" corresp="#s2:l2">present</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Het vincse een teer <rhyme label="a" type="f">roede</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Al binnen <rhyme label="b" type="m">Nazareth</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Beuaen al met <rhyme label="a" type="f" corresp="#s3:l1">armoeden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met duechden wel <rhyme label="b" type="m" corresp="#s3:l2">omset</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die muyte en was niet <rhyme label="a" type="f">gebroken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die dese vincke aen <rhyme label="b" type="m">nam</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Maer si bleef wel <rhyme label="a" type="f" corresp="#s4:l1">ghesloten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Als hi hier tot <rhyme label="b" type="m" corresp="#s4:l2">quam</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maer dat vernamen een <rhyme label="a" type="f">hauic</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met crommen clauwen <rhyme label="b" type="m">stijf</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Hi wilde dat vincxken <rhyme label="a" type="f" corresp="#s5:l1">doden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Het was hem een <rhyme label="b" type="m" corresp="#s5:l2">ontblijf</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Als hi hadde <rhyme label="a" type="f">verloren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die edel vincke <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Doen wrack hi sinen <rhyme label="a" type="f" corresp="#s6:l1">thoren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Aen so menich cleyn <rhyme label="b" type="m" corresp="#s6:l2">vogelkijn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die vinck quam haer <rhyme label="a" type="f">vermeyden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Tot Bethleem int <rhyme label="b" type="m">prijeel</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Daer mocht mense horen <rhyme label="a" type="f" corresp="#s7:l1">screyden</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Om onser alder <rhyme label="b" type="m" corresp="#s7:l2">seer</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu bidden wij der <rhyme label="a" type="f">vincken</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Metter ghyolen <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dat hi ons niet en laet <rhyme label="a" type="f" corresp="#s8:l1">versincken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Al inder hellen <rhyme label="b" type="m" corresp="#s8:l2">pijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
