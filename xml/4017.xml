<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4017</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Klaas Molenaar</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">In vruechden is alle die werelt <rhyme label="a" type="m">wijt</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Die engelen bringen ons groot <rhyme label="a" type="m" corresp="#s1:l1">iolijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Met vruechden hier <rhyme label="b" type="f">beneeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want aen een maghet <rhyme label="a" type="m">gebenedijt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">So is volcomen nu ter <rhyme label="a" type="m" corresp="#s2:l1">tijt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Onser alder <rhyme label="b" type="f">salichede</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nature is bouen den loop <rhyme label="a" type="m">ghegaen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Een suuer maget heeft <rhyme label="a" type="m" corresp="#s3:l1">ontfaen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Een kint in <rhyme label="b" type="f">aertrike</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Seer cortelijc yst al <rhyme label="a" type="m">voldaen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Dat Esayas dede <rhyme label="a" type="m" corresp="#s4:l1">verstaen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dwelc was seer <rhyme label="b" type="f">wonderlike</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dat licht der sonnen is ons <rhyme label="a" type="m">gheboren</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Die moeder is maghet / ende bleef te <rhyme label="a" type="m" corresp="#s5:l1">voren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Ende bleef in <rhyme label="b" type="f">reynicheden</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Inder scrifturen machmen <rhyme label="a" type="m">horen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Hoe dat bi Adam was <rhyme label="a" type="m" corresp="#s6:l1">verloren</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Tgheslachte der <rhyme label="b" type="f">menschelicheden</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Kinder nu laet ons blide <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Ende louen dat soete <rhyme label="a" type="m" corresp="#s7:l1">kindekijn</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Gheboren <rhyme label="b" type="f">menschelijcke</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Want hi is comen so ic <rhyme label="a" type="m">beuroede</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Om te verlossen met sinen <rhyme label="a" type="m" corresp="#s8:l1">bloede</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Ons allen <rhyme label="b" type="f">ghemeenlijcke</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Iesus so is die name <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Hi weende als een ander <rhyme label="a" type="m" corresp="#s9:l1">kindekijn</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Nae sijnder <rhyme label="b" type="f">menschelicheden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Twee stomme beesten bekenden <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Hem / haren scepper / ende niemant <rhyme label="a" type="m" corresp="#s10:l1">el</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Bi sijnder <rhyme label="b" type="f">goddelicheden</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Een cribbeken stont dair <rhyme label="a" type="m">ghemaect</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Daer wert hi inne gheleyt al <rhyme label="a" type="m" corresp="#s11:l1">naect</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Die heere van <rhyme label="b" type="f">hemelrijcke</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Drie coninghen quamen wt verren <rhyme label="a" type="m">lande</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Ende si brochten daer haer <rhyme label="a" type="m" corresp="#s12:l1">offerhande</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Seer blide ende <rhyme label="b" type="f">ootmoedelijcke</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Nu laet ons bidden dit <rhyme label="a" type="m">kindekijn</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Met Mariam die suyver maghet <rhyme label="a" type="m" corresp="#s13:l1">fijn</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Dat si onser willen <rhyme label="b" type="f">ontfermen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Als wi hier niet langher en moghen <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Dat si ons dan van die eewighe <rhyme label="a" type="m" corresp="#s14:l1">pijn</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Wil hoeden ende <rhyme label="b" type="f">beschermen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
