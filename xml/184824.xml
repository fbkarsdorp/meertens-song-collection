<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184824</id>
    <date>
      <year-start>1791</year-start>
      <year-end>1791</year-end>
    </date>
    <tags/>
    <melody>Voor het vaderland en vrijheid</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Neen! of schoon een aaklig <rhyme label="a" type="f">duister</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Thans het Vryheidslicht <rhyme label="b" type="m">verdooft</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En ons van haar heldre <rhyme label="a" type="f" corresp="#s1:l1">luister</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Door een zwarte wolk <rhyme label="b" type="m" corresp="#s1:l2">berooft</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Rasch verdwynt die <rhyme label="c" type="m">wolk</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Zinkt in 's afgronds <rhyme label="c" type="m" corresp="#s1:l5">kolk</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En 't verdrukte <rhyme label="d" type="m">Leeuwendaal</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Smaakt een grootsche <rhyme label="d" type="m" corresp="#s1:l7">zegepraal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schoon de woede van <rhyme label="a" type="f">Oranje</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Dan ten hoogsten toppunt <rhyme label="b" type="m">steig'</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Schoon ook Pruissen met <rhyme label="a" type="f" corresp="#s2:l1">Brittanje</rhyme></l>
        <l id="s2:l4" met="4" type="+-">De edle Vryheid vloeke en <rhyme label="b" type="m" corresp="#s2:l2">dreig'</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Een geduchter <rhyme label="c" type="m">kracht</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Dan hun trotsche <rhyme label="c" type="m" corresp="#s2:l5">magt</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Stryd voor die <rhyme label="d" type="m">Gemeenebest</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Zoo blyft Vryheid steeds <rhyme label="d" type="m" corresp="#s2:l7">gevest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
