    <melody>Wij liepen laatstmaal</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt was een Herderinne <rhyme label="a" type="m">stout</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Al met haer schaepkens in dat <rhyme label="a" type="m" corresp="#s1:l1">wout</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">En sy had vogels in haer <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om haer wat te <rhyme label="c" type="f">vermaken</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sy leydse by haar borsjes <rhyme label="b" type="m" corresp="#s1:l3">bloot</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat deed' de Iongmans <rhyme label="c" type="f" corresp="#s1:l4">haken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Doen wasser noch een <rhyme label="a" type="m">Celadon</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En die stont achter eenen <rhyme label="a" type="m" corresp="#s2:l1">boom</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy heeft de overschoone <rhyme label="b" type="m">maeght</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Haer Vogeltjes sien <rhyme label="c" type="f">strijcken</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dat heeft de Iongman wel <rhyme label="b" type="m" corresp="#s2:l3">behaegt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hy bleef'er na staen <rhyme label="c" type="f" corresp="#s2:l4">kijcken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Herder seyde hy <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En gy staet immer in mijn <rhyme label="a" type="m" corresp="#s3:l1">sin</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ick wou dat ick een Vogel <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">By jou is wel te <rhyme label="c" type="f">rusten</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Tusschen bey uw' borsjes <rhyme label="b" type="m" corresp="#s3:l3">blanck</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">En dat is al mijn <rhyme label="c" type="f" corresp="#s3:l4">lusten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Herder dat en doen ick <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Gy sult mijn brengen in't <rhyme label="a" type="m" corresp="#s4:l1">verdriet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ick heb mijn Vogels veel te <rhyme label="b" type="m">lief</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ick wil haer niet <rhyme label="c" type="f">quiteeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Gaet heen en soekt een ander <rhyme label="b" type="m" corresp="#s4:l3">lief</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ick sal haer niet <rhyme label="c" type="f" corresp="#s4:l4">blameeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Och wie heeft desen <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gegeven dese wijsheyt <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme>?</l>
        <l id="s5:l3" met="4" type="-+">Ick meende haer van t' eerst tot <rhyme label="b" type="m">lest</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Tot mijnen lust te <rhyme label="c" type="f">krijgen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Maer sy was al te kloeck van <rhyme label="b" type="m" corresp="#s5:l3">geest</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sy deed' mijn voor haer <rhyme label="c" type="f" corresp="#s5:l4">zwijgen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
