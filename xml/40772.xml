  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O HEER! loyts' ljeaflijck op m' <rhyme label="a" type="m">om-leeg</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">Mijn hert, mijn eagen sjea naet <rhyme label="a" type="m" corresp="#s1:l1">haeg</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ick wandelje nogh breck mijn <rhyme label="b" type="m">sin</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Yn 't jinge' ick grijppje oef fetg je <rhyme label="b" type="m" corresp="#s1:l3">kin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ja sijcker, 'k hab mijn siels <rhyme label="a" type="m">jin-wil</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Twang teamd, dol sot, ijn hadden <rhyme label="a" type="m" corresp="#s2:l1">stil</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Az 't suwgh-bern luwckjend', ney sijn <rhyme label="b" type="m">aad</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Mijn siel 's lijck, 't suwgh-soontje' ijn <rhyme label="b" type="m" corresp="#s2:l3">yenfaad</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O Isr'el, sillig foltsen, <rhyme label="a" type="m">komm'</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Lizze' Hoope' op God, Ã´ Goadz <rhyme label="a" type="m" corresp="#s3:l1">eyndomm'</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">O sjiette' hoops anckre-klimm', ijn <rhyme label="b" type="m">lest</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Op, in ijn de HEER' God, yvig <rhyme label="b" type="m" corresp="#s3:l3">fest</rhyme>.</l>
      </lg>
    </lg>
  </text>
