    <melody>Ik zal u eeuwig minnen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Waar blijft gij, mijn <rhyme label="a" type="f">beminden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ik ben hier in <rhyme label="b" type="m">ontrust</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik dacht u hier te <rhyme label="a" type="f" corresp="#s1:l1">vinden</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Te smaken <rhyme label="b" type="m" corresp="#s1:l2">minne-lust</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Ach bloem vol <rhyme label="c" type="m">waard</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Mijn schat op <rhyme label="c" type="m" corresp="#s1:l5">aard</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wilt u tot mijn <rhyme label="d" type="f">begeeven</rhyme>;</l>
        <l id="s1:l8" met="4" type="-+">Want ziet ik steek vol minne <rhyme label="e" type="m">pijn</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Waar zijt gij, ach, waar mag gij <rhyme label="e" type="m" corresp="#s1:l8">zijn</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Ziet mijn jonge <rhyme label="f" type="m">hart</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Wagt u met <rhyme label="f" type="m" corresp="#s1:l10">smart</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Waar blijft gij mijn <rhyme label="a" type="f" corresp="#s1:l1">beminde</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wie doet u schoonheid <rhyme label="a" type="f">smaaken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat ik u hier niet <rhyme label="b" type="m">vind</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wie mag u lipjes <rhyme label="a" type="f" corresp="#s2:l1">raaken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wat minnaar gij <rhyme label="b" type="m" corresp="#s2:l2">bemint</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Zijt gij <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Al met die <rhyme label="c" type="m" corresp="#s2:l5">geen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Laat u in stil <rhyme label="d" type="f">genaken</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">In u vertrek, daar gij nu <rhyme label="e" type="m">blijft</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Daar gij de venus lust <rhyme label="e" type="m" corresp="#s2:l8">bedrijft</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Ach, denkt gij <rhyme label="f" type="m">niet</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Op mijn <rhyme label="f" type="m" corresp="#s2:l10">verdriet</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Maar wel op u <rhyme label="a" type="f" corresp="#s2:l1">vermaken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laat uw geluid nu <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Verheugd mij, waarde <rhyme label="b" type="m">pand</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Laat ik u rust doch <rhyme label="a" type="f" corresp="#s3:l1">stooren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Bindt u in geen <rhyme label="b" type="m" corresp="#s3:l2">verband</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Opent met <rhyme label="c" type="m">spoed</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Mijn liefste <rhyme label="c" type="m" corresp="#s3:l5">zoet</rhyme>!</l>
        <l id="s3:l7" met="3" type="-+">Mijn lief, mijn <rhyme label="d" type="f">uitverkooren</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Laat mijn niet langer in deez' <rhyme label="e" type="m">pijn</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Laat ik alleen uw Minnaar <rhyme label="e" type="m" corresp="#s3:l8">zijn</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Ik bemin u <rhyme label="f" type="m">zeer</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Bemind mij <rhyme label="f" type="m" corresp="#s3:l10">weÃªr</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Wilt mij trouw liefde <rhyme label="a" type="f" corresp="#s3:l1">toonen</rhyme>.</l>
      </lg>
    </lg>
  </text>
