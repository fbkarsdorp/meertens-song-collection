  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IEsus soete <rhyme label="a" type="f">minne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sie was wel alsoe <rhyme label="b" type="m">groot</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Dat hi om onsen <rhyme label="a" type="f" corresp="#s1:l1">wille</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Woude steruen die bitter <rhyme label="b" type="m" corresp="#s1:l2">doot</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daer nae is hi <rhyme label="c" type="f">verresen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ende heeft hem dicke <rhyme label="d" type="m">vertoont</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ten hemel op <rhyme label="c" type="f" corresp="#s1:l5">gheuaren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Inden alder hoochsten <rhyme label="d" type="m" corresp="#s1:l6">troon</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Daer sijn die engelsche <rhyme label="a" type="f">choren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Cherubin ende <rhyme label="b" type="m">seraphin</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Daer machmen wonder <rhyme label="a" type="f" corresp="#s2:l1">horen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van meniger stemmen <rhyme label="b" type="m" corresp="#s2:l2">sijn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Sij spelen die <rhyme label="c" type="f">melodien</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In so menigerhande <rhyme label="d" type="m">wijs</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Voer Iesum ende <rhyme label="c" type="f" corresp="#s2:l5">Marien</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat niet te begripen en <rhyme label="d" type="m" corresp="#s2:l6">is</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die. xxiiij. ouden <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Staen voort met haesten <rhyme label="b" type="m">seer</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sij offeren daer die <rhyme label="a" type="f" corresp="#s3:l1">croone</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Voer Iesum onsen <rhyme label="b" type="m" corresp="#s3:l2">heer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Si hebben ooc den rechten <rhyme label="c" type="f">trede</rhyme></l>
        <l id="s3:l6" met="3" type="-+">So menich iaer lanc <rhyme label="d" type="m">gheleert</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Sy singen en springen <rhyme label="c" type="f" corresp="#s3:l5">mede</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Op dat hi sy vol <rhyme label="d" type="m" corresp="#s3:l6">eert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die vier <rhyme label="a" type="f">euangelisten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sijn bouen gout scoon <rhyme label="b" type="m">gecleet</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">In soe wonderlijcke <rhyme label="a" type="f" corresp="#s4:l1">listen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Als daer van ghescreuen <rhyme label="b" type="m" corresp="#s4:l2">steet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Veruruechden hem also <rhyme label="c" type="f">seere</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ende die baptiste sint <rhyme label="d" type="m">Ian</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dat nu noch <rhyme label="c" type="f" corresp="#s4:l5">nemmermeere</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Gheen man volgronden en <rhyme label="d" type="m" corresp="#s4:l6">can</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dauid met sijnder <rhyme label="a" type="f">herpen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die maect so groten <rhyme label="b" type="m">ghescal</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dat steden ende oock <rhyme label="a" type="f" corresp="#s5:l1">dorpen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Moghent hooren ouer <rhyme label="b" type="m" corresp="#s5:l2">al</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Hi roert die soete <rhyme label="c" type="f">snaren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Voer Iesum alsoe hi <rhyme label="d" type="m">plach</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dat hondert duysent <rhyme label="c" type="f" corresp="#s5:l5">iaren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Daer en scijnt niet eenen <rhyme label="d" type="m" corresp="#s5:l6">dach</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Propheten en <rhyme label="a" type="f">patriarcken</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mit purpur bysse <rhyme label="b" type="m">ghecleet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ende Noe mitter <rhyme label="a" type="f" corresp="#s6:l1">arcken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Inden rinc al mede <rhyme label="b" type="m" corresp="#s6:l2">gheet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Sij verbliden hem sonder <rhyme label="c" type="f">beyden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Voer Ihesum haren <rhyme label="d" type="m">troost</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Die inder hellen <rhyme label="c" type="f" corresp="#s6:l5">screyden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Daerse Iesus wt heeft <rhyme label="d" type="m" corresp="#s6:l6">verlost</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De apostolen blincken <rhyme label="a" type="f">groene</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Veel schoonder dan <rhyme label="b" type="m">samnijt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dat mengt haer vruecht <rhyme label="a" type="f" corresp="#s7:l1">scone</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Daer si nemmermeer wort <rhyme label="b" type="m" corresp="#s7:l2">besmit</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Sij maken daer <rhyme label="c" type="f">melodie</rhyme></l>
        <l id="s7:l6" met="3" type="-+">In soe soeten <rhyme label="d" type="m">musica</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat Iesus ende <rhyme label="c" type="f" corresp="#s7:l5">Marie</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Daer seer gaerne hooren <rhyme label="d" type="m" corresp="#s7:l6">na</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ridders ende <rhyme label="a" type="f">martelaren</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Als eenighe roose <rhyme label="b" type="m">root</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Bloeyen in haer <rhyme label="a" type="f" corresp="#s8:l1">vierscharen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ende driuen bliscap <rhyme label="b" type="m" corresp="#s8:l2">groot</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Sij spelen in vieriger <rhyme label="c" type="f">minnen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Voer Iesum tot alder <rhyme label="d" type="m">tijt</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Dat alle dat hemels <rhyme label="c" type="f" corresp="#s8:l5">ghesinne</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Wel seer daer af is <rhyme label="d" type="m" corresp="#s8:l6">verblijt</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Die heylighe <rhyme label="a" type="f">confessoren</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Die hebben der vruechten <rhyme label="b" type="m">veel</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Sij blincken in haer <rhyme label="a" type="f" corresp="#s9:l1">choren</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Veel scoonder dan blau <rhyme label="b" type="m" corresp="#s9:l2">fluweel</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Wt blauwe so suldy <rhyme label="c" type="f">mercken</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Haren saechten stadigen <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s9:l7" met="3" type="-+">In woerden ende in <rhyme label="c" type="f" corresp="#s9:l5">wercken</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Soe sijn si voer Gode <rhyme label="d" type="m" corresp="#s9:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Heremiten ende <rhyme label="a" type="f">monicken</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">Hooren mede in dit <rhyme label="b" type="m">ghetal</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Oock priesters ende <rhyme label="a" type="f" corresp="#s10:l1">canonicken</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Die singhen te gader <rhyme label="b" type="m" corresp="#s10:l2">al</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Sij dienen soe <rhyme label="c" type="f">eersaemlijcke</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Ende singhen voer Iesum <rhyme label="d" type="m">lof</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Alle die sijn in dat <rhyme label="c" type="f" corresp="#s10:l5">rijcke</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Vernieuwen hen seer daer <rhyme label="d" type="m" corresp="#s10:l6">of</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ioncfrouwen reyn van <rhyme label="a" type="f">seden</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Die climmen den hoochsten <rhyme label="b" type="m">graet</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Sie gaen al na Iesum <rhyme label="a" type="f" corresp="#s11:l1">treden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Waer dat hi henen <rhyme label="b" type="m" corresp="#s11:l2">gaet</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Si blincken in haren <rhyme label="c" type="f">troone</rhyme></l>
        <l id="s11:l6" met="3" type="-+">So reyne ende so <rhyme label="d" type="m">claer</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Noyt lelie en was so <rhyme label="c" type="f" corresp="#s11:l5">schoone</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Si en wert besmet bi <rhyme label="d" type="m" corresp="#s11:l6">haer</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Si connen <rhyme label="a" type="f">iubileren</rhyme></l>
        <l id="s12:l2" met="3" type="-+">In also <rhyme label="b" type="m">menigherwijs</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Oock singen ende <rhyme label="a" type="f" corresp="#s12:l1">discanteren</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Dattet niet te dencken en <rhyme label="b" type="m" corresp="#s12:l2">is</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Och wat soeter <rhyme label="c" type="f">melodye</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Hoortmen inder maechden <rhyme label="d" type="m">choor</rhyme>.</l>
        <l id="s12:l7" met="3" type="-+">Want Iesus ende <rhyme label="c" type="f" corresp="#s12:l5">Marie</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Die singhen daer selue <rhyme label="d" type="m" corresp="#s12:l6">voor</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Dat ghetal is sonder <rhyme label="a" type="f">mate</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Die daar alle singhen <rhyme label="b" type="m">claer</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Van so menigherhande <rhyme label="a" type="f" corresp="#s13:l1">state</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Dattet niet te scriuen en <rhyme label="b" type="m" corresp="#s13:l2">waer</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Want sy sijn al int <rhyme label="c" type="f">ghemeyne</rhyme>.</l>
        <l id="s13:l6" met="3" type="-+">Van allen consten <rhyme label="d" type="m">gheleert</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Mer Iesus eest <rhyme label="c" type="f" corresp="#s13:l5">alleyne</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Die alle dit ghesinne <rhyme label="d" type="m" corresp="#s13:l6">regeert</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wat mach dan daer <rhyme label="a" type="f">ontbreken</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Daer Iesus dat selue <rhyme label="b" type="m">gheeft</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Datmen dencken mach oft <rhyme label="a" type="f" corresp="#s14:l1">spreken</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Elck dat gheuoelen <rhyme label="b" type="m" corresp="#s14:l2">heeft</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Al woude sy inder <rhyme label="c" type="f">hellen</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Rechteuoort so sijn sy <rhyme label="d" type="m">daer</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Ende souden sonder <rhyme label="c" type="f" corresp="#s14:l5">quellen</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Iesum scouwen puer ende <rhyme label="d" type="m" corresp="#s14:l6">claer</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Elcken dunct int <rhyme label="a" type="f">ghemeyne</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Dat Iesus hem mint so <rhyme label="b" type="m">seere</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Recht oft hijt waer <rhyme label="a" type="f" corresp="#s15:l1">alleyne</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Och wat is dit wonders <rhyme label="b" type="m" corresp="#s15:l2">meere</rhyme>.</l>
        <l id="s15:l5" met="3" type="-+">Elck mint die vruechde <rhyme label="c" type="f">clare</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Die hi aen den anderen <rhyme label="d" type="m">siet</rhyme>.</l>
        <l id="s15:l7" met="3" type="-+">Recht oft sy sijns selfs <rhyme label="c" type="f" corresp="#s15:l5">ware</rhyme></l>
        <l id="s15:l8" met="3" type="-+">Also is hi daer in <rhyme label="d" type="m" corresp="#s15:l6">verblijt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Alle dat van <rhyme label="a" type="f">beghinne</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Oyt van Adam ghecomen <rhyme label="b" type="m">is</rhyme></l>
        <l id="s16:l3" met="3" type="-+">En soude in gheenen <rhyme label="a" type="f" corresp="#s16:l1">sinne</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Volscriuen in gheender <rhyme label="b" type="m" corresp="#s16:l2">wijs</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Een graet der minster <rhyme label="c" type="f">vruechden</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Die der nedersten daer in <rhyme label="d" type="m">plien</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Nochtans niemant en <rhyme label="c" type="f" corresp="#s16:l5">soude</rhyme></l>
        <l id="s16:l8" met="3" type="-+">Al veel lieuer Iesum <rhyme label="d" type="m" corresp="#s16:l6">sien</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Die Iesum siet <rhyme label="a" type="f">alleene</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Die mach hem spieghelen <rhyme label="b" type="m">claer</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Die dinghen al int <rhyme label="a" type="f" corresp="#s17:l1">ghemeene</rhyme></l>
        <l id="s17:l4" met="3" type="-+">So roepen sy alle <rhyme label="b" type="m" corresp="#s17:l2">daer</rhyme>.</l>
        <l id="s17:l5" met="3" type="-+">O wel lieue soete <rhyme label="c" type="f">Iesus</rhyme></l>
        <l id="s17:l6" met="3" type="-+">Nv weest <rhyme label="d" type="m">ghebenedijt</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Sanctus sanctus en <rhyme label="c" type="f" corresp="#s17:l5">sanctus</rhyme></l>
        <l id="s17:l8" met="3" type="-+">Nv ende in alder <rhyme label="d" type="m" corresp="#s17:l6">tijt</rhyme></l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Mijn machte wil mi <rhyme label="a" type="f">breken</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Mijn herte van binnen <rhyme label="b" type="m">beeft</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Als ic te gronde wil <rhyme label="a" type="f" corresp="#s18:l1">dencken</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Hoe Iesus mi hier in <rhyme label="b" type="m" corresp="#s18:l2">heeft</rhyme></l>
        <l id="s18:l5" met="3" type="-+">Met so wonderlijcke <rhyme label="c" type="f">goede</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Al na hem seluen <rhyme label="d" type="m">ghewrocht</rhyme>.</l>
        <l id="s18:l7" met="3" type="-+">Ende met sinen heyligen <rhyme label="c" type="f" corresp="#s18:l5">bloede</rhyme></l>
        <l id="s18:l8" met="3" type="-+">Tot deser vruechden <rhyme label="d" type="m" corresp="#s18:l6">ghecocht</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Als ick dit <rhyme label="a" type="f">ouerdochte</rhyme></l>
        <l id="s19:l2" met="3" type="-+">So gheuoelde ick wat in <rhyme label="b" type="m">mi</rhyme></l>
        <l id="s19:l3" met="3" type="-+">Dat Iesus dien ick seer <rhyme label="a" type="f" corresp="#s19:l1">sochte</rhyme>.</l>
        <l id="s19:l4" met="3" type="-+">Met sijnder gracien is <rhyme label="b" type="m" corresp="#s19:l2">bi</rhyme></l>
        <l id="s19:l5" met="3" type="-+">Dat herte wil mi <rhyme label="c" type="f">ontsincken</rhyme></l>
        <l id="s19:l6" met="3" type="-+">Ick en weet waer henen <rhyme label="d" type="m">gaen</rhyme></l>
        <l id="s19:l7" met="3" type="-+">Want hi mi meynt te <rhyme label="c" type="f" corresp="#s19:l5">scincken</rhyme></l>
        <l id="s19:l8" met="3" type="-+">Den soeten hemelschen <rhyme label="d" type="m" corresp="#s19:l6">traen</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Mi dunct ick heb <rhyme label="a" type="f">vernomen</rhyme></l>
        <l id="s20:l2" met="3" type="-+">In minen sinne <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Dat ons te hulpe sijn <rhyme label="a" type="f" corresp="#s20:l1">comen</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Veel duysent enghelen <rhyme label="b" type="m" corresp="#s20:l2">claer</rhyme></l>
        <l id="s20:l5" met="3" type="-+">Welop ende laet ons <rhyme label="c" type="f">springen</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Ende Iesum minnelic <rhyme label="d" type="m">ontfaen</rhyme></l>
        <l id="s20:l7" met="3" type="-+">Ende met vruechden <rhyme label="c" type="f" corresp="#s20:l5">singhen</rhyme>.</l>
        <l id="s20:l8" met="3" type="-+">Droeue werlt ic wil v <rhyme label="d" type="m" corresp="#s20:l6">versmaen</rhyme></l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Haelt ons wt dese <rhyme label="a" type="f">alleynde</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Wi willen ons pinen <rhyme label="b" type="m">voort</rhyme></l>
        <l id="s21:l3" met="3" type="-+">So moghen wi sonder <rhyme label="a" type="f" corresp="#s21:l1">eynde</rhyme>.</l>
        <l id="s21:l4" met="3" type="-+">Iesum dat minnelijck <rhyme label="b" type="m" corresp="#s21:l2">woort</rhyme></l>
        <l id="s21:l5" met="3" type="-+">In so grooter <rhyme label="c" type="f">melodyen</rhyme>.</l>
        <l id="s21:l6" met="3" type="-+">Met al den enghelen <rhyme label="d" type="m">schoon</rhyme></l>
        <l id="s21:l7" met="3" type="-+">Altoos <rhyme label="c" type="f" corresp="#s21:l5">ghebenedyen</rhyme></l>
        <l id="s21:l8" met="3" type="-+">In dinen hemelschen <rhyme label="d" type="m" corresp="#s21:l6">troon</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">IESVS ende <rhyme label="a" type="f">MARIA</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Wi bidden v ouer <rhyme label="b" type="m">al</rhyme></l>
        <l id="s22:l3" met="3" type="-+">Wilt ons arme siele dan <rhyme label="a" type="f" corresp="#s22:l1">vriden</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Wanneer sy scheyden <rhyme label="b" type="m" corresp="#s22:l2">sal</rhyme></l>
        <l id="s22:l5" met="3" type="-+">Ende als dat herte sal <rhyme label="c" type="f">breken</rhyme></l>
        <l id="s22:l6" met="3" type="-+">So helpt ons dan <rhyme label="d" type="m">aldus</rhyme></l>
        <l id="s22:l7" met="3" type="-+">Dat wi met vruechden <rhyme label="c" type="f" corresp="#s22:l5">spreken</rhyme>.</l>
        <l id="s22:l8" met="3" type="-+">Ghelooft Maria <rhyme label="d" type="m" corresp="#s22:l6">Iesus</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
