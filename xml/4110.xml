  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NU mensche v <rhyme label="a" type="m">ontwect</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Stelt v in God <rhyme label="a" type="m" corresp="#s1:l1">perfect</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Al in dit dal van <rhyme label="b" type="f">weene</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Gods moeder <rhyme label="a" type="m" corresp="#s1:l1">onbeulect</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Heeft ons als nu <rhyme label="a" type="m" corresp="#s1:l1">ontdect</rhyme></l>
        <l id="s1:l6" met="3" type="-+">So soeten kindeken <rhyme label="b" type="f" corresp="#s1:l3">cleene</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Stelt v om louen <rhyme label="a" type="m">al</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die sijt int swerelts <rhyme label="a" type="m" corresp="#s2:l1">dal</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Gods moeder <rhyme label="b" type="f">wtuercoren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Want van der sonden <rhyme label="a" type="m" corresp="#s2:l1">val</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Heeft si sonder <rhyme label="a" type="m" corresp="#s2:l1">ghetal</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Verlost die waren <rhyme label="b" type="f" corresp="#s2:l3">verloren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Want doer Adams <rhyme label="a" type="m">misdaet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Waren wij allen <rhyme label="a" type="m" corresp="#s3:l1">beschaet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Den hemel wert <rhyme label="b" type="f">ghesloten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Onser en was gheenen <rhyme label="a" type="m" corresp="#s3:l1">raet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Niet voer dat suyuer <rhyme label="a" type="m" corresp="#s3:l1">saet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wt Iesse was <rhyme label="b" type="f" corresp="#s3:l3">ghesproten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die soete suyuer <rhyme label="a" type="m">iuecht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wies op in alder <rhyme label="a" type="m" corresp="#s4:l1">duecht</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Met een ootmoedich <rhyme label="b" type="f">leuen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Och mensche sijt <rhyme label="a" type="m" corresp="#s4:l1">verhuecht</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wel ghi verbliden <rhyme label="a" type="m" corresp="#s4:l1">muecht</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat serpent heeft si <rhyme label="b" type="f" corresp="#s4:l3">verdreuen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Aue was Gabriels <rhyme label="a" type="m">woort</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Om onser alder <rhyme label="a" type="m" corresp="#s5:l1">confoort</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Maria ontfinck die <rhyme label="b" type="f">groete</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ick stelle mijn <rhyme label="a" type="m" corresp="#s5:l1">accoort</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Al in Gods wille <rhyme label="a" type="m" corresp="#s5:l1">voort</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Sprack si met soeten <rhyme label="b" type="f" corresp="#s5:l3">woorden</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gods gramschappe heeft si <rhyme label="a" type="m">gheslaect</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ende onsen pays <rhyme label="a" type="m" corresp="#s6:l1">ghemaect</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ghebenedijt moet si <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die godheyt is <rhyme label="a" type="m" corresp="#s6:l1">ghedaect</rhyme></l>
        <l id="s6:l5" met="3" type="-+">In haren lichaem <rhyme label="a" type="m" corresp="#s6:l1">naect</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dies sijn wij alle <rhyme label="b" type="f" corresp="#s6:l3">ghenesen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Princesse ick soecke <rhyme label="a" type="m">ootmoet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Aen v Gods moeder <rhyme label="a" type="m" corresp="#s7:l1">soet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Die maghet sijt <rhyme label="b" type="f">ghebleuen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Wanneer ic steruen <rhyme label="a" type="m" corresp="#s7:l1">moet</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Dat ghi mijn siele <rhyme label="a" type="m" corresp="#s7:l1">behoet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Bi v int eewich <rhyme label="b" type="f" corresp="#s7:l3">leuen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
