<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181912</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het ging een meisje aan de kant</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DAer ging een meisje aen de <rhyme label="a" type="m">kant</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Van de klare <rhyme label="b" type="f">Vlisselingen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Sou gaen Melken op ’t <rhyme label="a" type="m" corresp="#s1:l1">Land</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Daer haer Meesters koetjes <rhyme label="b" type="f" corresp="#s1:l2">gingen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Hoord eens hoe het haer <rhyme label="c" type="m">verging</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Met soo’n fraey fris <rhyme label="c" type="m" corresp="#s1:l5">Jongeling</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Deesen Jonkman kwam <rhyme label="a" type="m">aldaer</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Daer sy in de wey ging <rhyme label="b" type="f">treeden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Goeden morgen boot hy <rhyme label="a" type="m" corresp="#s2:l1">haer</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Sy hem van gelyke <rhyme label="b" type="f" corresp="#s2:l2">deede</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Meisje sy hy soete <rhyme label="c" type="m">dier</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Wat maekt gy soo vroeg <rhyme label="c" type="m" corresp="#s2:l5">alhier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doen antwoorde hem de <rhyme label="a" type="m">maegt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ik ga de melk Fonteyn <rhyme label="b" type="f">aftappen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">’t Heeft de Jonkman wel <rhyme label="a" type="m" corresp="#s3:l1">behaegt</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Datse kon soo aerdig <rhyme label="b" type="f" corresp="#s3:l2">klappen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dus zo deed hy haar <rhyme label="c" type="m">geley</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">By haar Koetjes in de <rhyme label="c" type="m" corresp="#s3:l5">wei</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy verleende haer een <rhyme label="a" type="m">praat</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Daar zy haar Koetjes sat en <rhyme label="b" type="f">melkte</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Jonge dogter waar het <rhyme label="a" type="m" corresp="#s4:l1">kwaed</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Datje u Jeugt alsoo <rhyme label="b" type="f" corresp="#s4:l2">versterkten</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Sonder myn of myns <rhyme label="c" type="m">gevoel</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Maar zu hielt ’er eeven <rhyme label="c" type="m" corresp="#s4:l5">koel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dog hy liet ’t daar niet <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Maer hy kwam nog verder <rhyme label="b" type="f">tragten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Meisje seid’ hy zegt het <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Wat hebt gy al voor <rhyme label="b" type="f" corresp="#s5:l2">gedagten</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Alsje de Uyer van de <rhyme label="c" type="m">Koe</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Nypt met u ving’ren <rhyme label="c" type="m" corresp="#s5:l5">toe</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">’t Meisje dat eenvoudig <rhyme label="a" type="m">was</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Gaf hem tot weder <rhyme label="b" type="f">spraeken</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Ik soek vrugten van het <rhyme label="a" type="m" corresp="#s6:l1">Gras</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Daar men Boter af kan <rhyme label="b" type="f" corresp="#s6:l2">maaken</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Koetjes denk ik geef ik <rhyme label="c" type="m">veel</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat myn Vrouw maakt geen <rhyme label="c" type="m" corresp="#s6:l5">krakeel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Doen sprak daar den Jonge <rhyme label="a" type="m">kwant</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Dat zyn niet al u <rhyme label="b" type="f">gepeinzen</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Want gy zyt van vlees en <rhyme label="a" type="m" corresp="#s7:l1">Bloed</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Waarom wilt gy voor mij <rhyme label="b" type="f" corresp="#s7:l2">veinzen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Denkje niet op d’ Uyer <rhyme label="c" type="m">van</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Meenig, fris, jong, jeugdig <rhyme label="c" type="m" corresp="#s7:l5">Man</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Doen sprak daar het Meisje <rhyme label="a" type="m">slegt</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Segt mij u meening van de <rhyme label="b" type="f">uijer</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Segt u meening eens te <rhyme label="a" type="m" corresp="#s8:l1">regt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Het een Manmens ook een <rhyme label="b" type="f" corresp="#s8:l2">uyer</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Daer men room of melk <rhyme label="c" type="m">uittrekt</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Maar men denkt gy met my <rhyme label="c" type="m" corresp="#s8:l5">gekt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Doen sprak daer het meisje <rhyme label="a" type="m">dit</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Wonder wou ik wel eens <rhyme label="b" type="f">speuren</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Wel schoon kind hier <rhyme label="a" type="m" corresp="#s9:l1">nederzit</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Zeiden ’t zal u wel <rhyme label="b" type="f" corresp="#s9:l2">gebeuren</rhyme>:</l>
        <l id="s9:l5" met="4" type="+-">En hy zmeetze met een <rhyme label="c" type="m">zoen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Agter over in ’t <rhyme label="c" type="m" corresp="#s9:l5">groen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Hy haalden den uyer voor den <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">En hy lietze het meisje <rhyme label="b" type="f">kyken</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Zy sprak doen zonder <rhyme label="a" type="m" corresp="#s10:l1">geklag</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Mag ik de hand daar wel aan <rhyme label="b" type="f" corresp="#s10:l2">stryken</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Ja sprak doe de jonge <rhyme label="c" type="m">kwant</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">En hy gafze haar in de <rhyme label="c" type="m" corresp="#s10:l5">hand</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Doen zy den uyer na haar <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Had beknuffelt en <rhyme label="b" type="f">bestreeken</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Wel sprak doen het kind van <rhyme label="a" type="m" corresp="#s11:l1">min</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Ik heb hem lang genoeg <rhyme label="b" type="f" corresp="#s11:l2">bekeeken</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Toon my nu eens met ’er <rhyme label="c" type="m">daad</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Hoe ’et room en melk <rhyme label="c" type="m" corresp="#s11:l5">uitgaat</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Gaet ’er leggen in het <rhyme label="a" type="m">groen</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Uit ’t gezigt van de een of <rhyme label="b" type="f">ander</rhyme>:</l>
        <l id="s12:l3" met="4" type="+-">Den Emmer hebje niet van <rhyme label="a" type="m" corresp="#s12:l1">doen</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">Want ik zie hier wel een <rhyme label="b" type="f" corresp="#s12:l2">ander</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">En al is ze wat <rhyme label="c" type="m">bejaart</rhyme>;</l>
        <l id="s12:l6" met="4" type="+-">De melk is daar te beter <rhyme label="c" type="m" corresp="#s12:l5">bewaart</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Zy ging leggen en <rhyme label="a" type="m">ontfing</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Eerst met smart en daar na met <rhyme label="b" type="f">lusten</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Den uyer van den <rhyme label="a" type="m" corresp="#s13:l1">jongeling</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Die zy altemet eens <rhyme label="b" type="f" corresp="#s13:l2">kusten</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Den uyer ging dan heen en <rhyme label="c" type="m">weer</rhyme>:</l>
        <l id="s13:l6" met="4" type="+-">En den Emmer op en <rhyme label="c" type="m" corresp="#s13:l5">neer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">’t Melken kwam in ’t eind soo <rhyme label="a" type="m">veer</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Dat den Emmer is <rhyme label="b" type="f">overgeloopen</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Wel sprak daer het meisje <rhyme label="a" type="m" corresp="#s14:l1">teer</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Hoe kom ik soo nat <rhyme label="b" type="f" corresp="#s14:l2">bedroopen</rhyme>:</l>
        <l id="s14:l5" met="4" type="+-">Meisje zeid hy met een <rhyme label="c" type="m">lag</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Die niet in den Emmer en <rhyme label="c" type="m" corresp="#s14:l5">mag</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Hy haelden den uyer weer tot <rhyme label="a" type="m">hem</rhyme>,</l>
        <l id="s15:l2" met="4" type="+-">’t Melken meenden hy <rhyme label="b" type="f">staeken</rhyme>:</l>
        <l id="s15:l3" met="4" type="+-">Wel seid sy ’t het nu eerst <rhyme label="a" type="m" corresp="#s15:l1">klem</rhyme></l>
        <l id="s15:l4" met="4" type="+-">Souje soo een slipper <rhyme label="b" type="f" corresp="#s15:l2">maken</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Fraey fris als een Jeugdig <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">’t Sa je moet ’er nog eens <rhyme label="c" type="m" corresp="#s15:l5">an</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Hy en dorst ’er deese <rhyme label="a" type="m">bee</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Om syn eere wil niet <rhyme label="b" type="f">weigeren</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Dus hy hem vast maekten <rhyme label="a" type="m" corresp="#s16:l1">ree</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">En hy kwam daer weer op <rhyme label="b" type="f" corresp="#s16:l2">steygeren</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Dat hem ’t zweet brak uyt soo <rhyme label="c" type="m">sterk</rhyme>:</l>
        <l id="s16:l6" met="4" type="+-">In het eind van ’t soete <rhyme label="c" type="m" corresp="#s16:l5">werk</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Hy nam het meisjen by de <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">En hy steldese op haer <rhyme label="b" type="f">Bienen</rhyme>:</l>
        <l id="s17:l3" met="4" type="+-">Nu sei sy heb ik het <rhyme label="a" type="m" corresp="#s17:l1">verstand</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Van dees Jonkman melk te <rhyme label="b" type="f" corresp="#s17:l2">spiene</rhyme>:</l>
        <l id="s17:l5" met="4" type="+-">Ik bidje als ik uyt melke <rhyme label="c" type="m">ga</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">Volgt my dan dog altyd <rhyme label="c" type="m" corresp="#s17:l5">na</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Wel seyd hy en is met een <rhyme label="a" type="m">kus</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">Van dees melk-meyd <rhyme label="b" type="f">gescheiden</rhyme>,</l>
        <l id="s18:l3" met="4" type="+-">Gy raet ook niet wie dat ’t <rhyme label="a" type="m" corresp="#s18:l1">is</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Ik sal de naem ook niet <rhyme label="b" type="f" corresp="#s18:l2">verbreiden</rhyme>,</l>
        <l id="s18:l5" met="4" type="+-">Want aan ’t melken u niet <rhyme label="c" type="m">leid</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">Van dees Jonkman en <rhyme label="c" type="m" corresp="#s18:l5">meid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
