    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Io vivat</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Komt, vat de <rhyme label="a" type="m">flesch</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ter eer' van 't jonge <rhyme label="b" type="m">paar</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Waarvan men nu de bruiloft <rhyme label="c" type="m">viert</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En dat deez' disch met vrienden <rhyme label="c" type="m" corresp="#s1:l3">siert</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Komt, vat de <rhyme label="a" type="m" corresp="#s1:l1">flesch</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Schenkt vol uw <rhyme label="a" type="m">glas</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En neemt het in de <rhyme label="b" type="m">hand</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">En dan met eenen blijden <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Hen heil gewenscht uit 's harten <rhyme label="c" type="m" corresp="#s2:l3">grond</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Schenkt vol uw <rhyme label="a" type="m" corresp="#s2:l1">glas</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dat zegen <rhyme label="a" type="m">daal'</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Op uwe hoofden <rhyme label="b" type="m">neer</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">Geen tegenspoed treffe ooit uw <rhyme label="c" type="m">huis</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Leeft lang bevrijd van ramp en <rhyme label="c" type="m" corresp="#s3:l3">kruis</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Dat zegen <rhyme label="a" type="m" corresp="#s3:l1">daal'</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Een <rhyme label="a" type="m">kindertal</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Bekroone uw <rhyme label="b" type="m">huwlijkskoets</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Gezond van lijf en rein van <rhyme label="c" type="m">geest</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Siert het uw zilvre of gouden <rhyme label="c" type="m" corresp="#s4:l3">feest</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Een <rhyme label="a" type="m" corresp="#s4:l1">kindertal</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Leegt thans uw <rhyme label="a" type="m">glas</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Daar kan een dronk op <rhyme label="b" type="m">staan</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">De bruigom drukt zijn' dank u <rhyme label="c" type="m">uit</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En kust vol vreugd zijn lieve <rhyme label="c" type="m" corresp="#s5:l3">bruid</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Leegt thans uw <rhyme label="a" type="m" corresp="#s5:l1">glas</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
