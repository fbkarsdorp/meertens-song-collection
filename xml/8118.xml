<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8118</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Een aardig vrouwtje heeft mij bedrogen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">O Christe God, Heere, van grooter <rhyme label="a" type="f">waerden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Suyver Lammeken <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Ghy zijt in 't vleesch verschenen op der <rhyme label="a" type="f" corresp="#s1:l1">aerden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met also groot <rhyme label="b" type="m" corresp="#s1:l2">ootmoet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ghy hebt dat eeuwigh <rhyme label="b" type="m" corresp="#s1:l2">goet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">V heerlijckheyt <rhyme label="c" type="f">verlaten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">V volck selve <rhyme label="d" type="m">besocht</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die in 'sdoodts schadu <rhyme label="c" type="f" corresp="#s1:l6">saten</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Wt liefde boven <rhyme label="c" type="f" corresp="#s1:l6">maten</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Den mensche dier <rhyme label="d" type="m" corresp="#s1:l7">ghekocht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Ghy waert geboren o Coning groot van <rhyme label="a" type="f">machte</rhyme></l>
        <l id="s2:l2" met="3" type="-+">In Bethlehem <rhyme label="b" type="m">Iuda</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Van eender Maeght ootmoedigh van <rhyme label="a" type="f" corresp="#s2:l1">ghedachte</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die voor Godt vandt <rhyme label="b" type="m" corresp="#s2:l2">ghena</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Gheheten <rhyme label="b" type="m" corresp="#s2:l2">Maria</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Al in een stal van <rhyme label="c" type="f">beesten</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">In armoede <rhyme label="d" type="m">aldaer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">O Godt Vader der <rhyme label="c" type="f" corresp="#s2:l6">Gheesten</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Men maeckte niet veel <rhyme label="c" type="f" corresp="#s2:l6">feesten</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Met uwen Soon <rhyme label="d" type="m" corresp="#s2:l7">eerbaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Want daer en was gheen plaets in de <rhyme label="a" type="f">herberge</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Om den Heere te <rhyme label="b" type="m">ontfaen</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Ootmoedigh Lam, sachtmoedigh vry van <rhyme label="a" type="f" corresp="#s3:l1">erghe</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hoe zijt ghy <rhyme label="b" type="m" corresp="#s3:l2">voorgegaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die bootschap werdt <rhyme label="b" type="m" corresp="#s3:l2">gedaen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Niet aen seer groote <rhyme label="c" type="f">Heeren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maer aen Herderen <rhyme label="d" type="m">slecht</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">O menschen wilt u <rhyme label="c" type="f" corresp="#s3:l6">keeren</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Tot Christum, ende <rhyme label="c" type="f" corresp="#s3:l6">leeren</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Ootmoedigh zijn <rhyme label="d" type="m" corresp="#s3:l7">oprecht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Simpel end' slecht quam oock uwen <rhyme label="a" type="f">voorbode</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Niet in kleederen <rhyme label="b" type="m">sacht</rhyme>.</l>
        <l id="s4:l3" met="5" type="-+">Coningh ghy, die ghelijck te sijne <rhyme label="a" type="f" corresp="#s4:l1">Gode</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Gheen roof en hebt <rhyme label="b" type="m" corresp="#s4:l2">gheacht</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Tot Nazareth <rhyme label="b" type="m" corresp="#s4:l2">volbracht</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In u heyligh <rhyme label="c" type="f">lichame</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Hebt ghy menighen <rhyme label="d" type="m">dagh</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">V Ouders <rhyme label="c" type="f" corresp="#s4:l6">ghehoorsame</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Daer doch in uwen <rhyme label="c" type="f" corresp="#s4:l6">name</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Al haer saligheyt <rhyme label="d" type="m" corresp="#s4:l7">lagh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Gheen hooghe, wijse, noch edel <rhyme label="a" type="f">gheboren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van der werelt <rhyme label="b" type="m">ghe-eert</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">En hebt ghy tot Apostelen <rhyme label="a" type="f" corresp="#s5:l1">vercoren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maer menschen <rhyme label="b" type="m" corresp="#s5:l2">ongheleert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Al daer ghy hebt <rhyme label="b" type="m" corresp="#s5:l2">verkeert</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hebt ghy Visschers <rhyme label="c" type="f">ghevonden</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Ter Galileetscher <rhyme label="d" type="m">zee</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Die u ten selven <rhyme label="c" type="f" corresp="#s5:l6">stonden</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Volghden na dijn <rhyme label="c" type="f" corresp="#s5:l6">vermonden</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">En lieten haer <rhyme label="d" type="m" corresp="#s5:l7">gheree</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">V wandelinghe was vry <rhyme label="a" type="f">onverbolgen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Al in <rhyme label="b" type="m">vernedertheyt</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Al waer ghy gaet (sprack een) wil ick u <rhyme label="a" type="f" corresp="#s6:l1">volgen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Maer ghy hebt hem <rhyme label="b" type="m" corresp="#s6:l2">gheseyt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Vossen hebben <rhyme label="b" type="m" corresp="#s6:l2">planteyt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hoolen, die voghelen <rhyme label="c" type="f">nesten</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">O Coningh <rhyme label="d" type="m">goedertier</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">In dees aerdtsche <rhyme label="c" type="f" corresp="#s6:l6">ghewesten</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En hadt ghy niet ten <rhyme label="c" type="f" corresp="#s6:l6">besten</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Om 't hooft te rusten <rhyme label="d" type="m" corresp="#s6:l7">hier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Sathan gingh u al sijn rijcken <rhyme label="a" type="f">vertoonen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Maer ghy en achtes <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">Als u de menschen Coningh wilden <rhyme label="a" type="f" corresp="#s7:l1">kroonen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Ghy zijt gevloden, <rhyme label="b" type="m" corresp="#s7:l2">siet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En alst oock is <rhyme label="b" type="m" corresp="#s7:l2">geschiet</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Dat u Iongeren <rhyme label="c" type="f">keven</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Om de meeste te <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Ghy hebt hen lien <rhyme label="c" type="f" corresp="#s7:l6">ghegheven</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Aen u ootmoedigh <rhyme label="c" type="f" corresp="#s7:l6">leven</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Een goedt exempel <rhyme label="d" type="m" corresp="#s7:l7">fijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">De wereltlijcke Coningen <rhyme label="a" type="f">heerschappen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Heeren zijn sy <rhyme label="b" type="m">ghenaemt</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">Maer al die willen volghen u <rhyme label="a" type="f" corresp="#s8:l1">voetstappen</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Al sulcks niet en <rhyme label="b" type="m" corresp="#s8:l2">betaemt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Want ghy om dienen <rhyme label="b" type="m" corresp="#s8:l2">quaemt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Niet om gedient te <rhyme label="c" type="f">wesen</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Noch is dit u <rhyme label="d" type="m">vermaen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Wordt niet om hoogh <rhyme label="c" type="f" corresp="#s8:l6">gheresen</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">De Heydenen na <rhyme label="c" type="f" corresp="#s8:l6">desen</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Seer inde werelt <rhyme label="d" type="m" corresp="#s8:l7">staen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">Ghy loofdet den Vader dat hy liet <rhyme label="a" type="f">weten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Kleynen, die ghy <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s9:l3" met="5" type="-+">Van 't Hemelrijcke die hooghe <rhyme label="a" type="f" corresp="#s9:l1">secreten</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Ghy seght wie als een <rhyme label="b" type="m" corresp="#s9:l2">kindt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Ootmoedigh is <rhyme label="b" type="m" corresp="#s9:l2">gesint</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">In 't Hemelrijck de <rhyme label="c" type="f">meeste</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Is desen <rhyme label="d" type="m">excellent</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Die arm zijn van <rhyme label="c" type="f" corresp="#s9:l6">gheeste</rhyme></l>
        <l id="s9:l9" met="3" type="-+">In de Hemelsche <rhyme label="c" type="f" corresp="#s9:l6">feeste</rhyme></l>
        <l id="s9:l10" met="3" type="-+">Sijn erfghenamen <rhyme label="d" type="m" corresp="#s9:l7">bekent</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">Coningh van Syon, ootmoedigh van <rhyme label="a" type="f">zeden</rhyme></l>
        <l id="s10:l2" met="3" type="-+">En sachtmoedigh <rhyme label="b" type="m">vermaert</rhyme>,</l>
        <l id="s10:l3" met="5" type="-+">Ghy quaemt op een Ezelinne <rhyme label="a" type="f" corresp="#s10:l1">ghereden</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Te <rhyme label="b" type="m" corresp="#s10:l2">Ierusalem-waert</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Wt ootmoedigher <rhyme label="b" type="m" corresp="#s10:l2">aert</rhyme></l>
        <l id="s10:l6" met="3" type="-+">D'Apostelen haer <rhyme label="c" type="f">voeten</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Ghewasschen boven <rhyme label="d" type="m">dien</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Daer doch in uwen <rhyme label="c" type="f" corresp="#s10:l6">soeten</rhyme></l>
        <l id="s10:l9" met="3" type="-+">Heylighen name <rhyme label="c" type="f" corresp="#s10:l6">moeten</rhyme></l>
        <l id="s10:l10" met="3" type="-+">Haer buyghen alle <rhyme label="d" type="m" corresp="#s10:l7">knyen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="-+">Doe u wel vreught hadde mogen <rhyme label="a" type="f">gebeuren</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Hebt ghy verdraghen <rhyme label="b" type="m">smaet</rhyme>,</l>
        <l id="s11:l3" met="5" type="-+">Gheesselen, slaghen, smerten en <rhyme label="a" type="f" corresp="#s11:l1">doleuren</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Al voor onse <rhyme label="b" type="m" corresp="#s11:l2">misdaet</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">De krijghslien <rhyme label="b" type="m" corresp="#s11:l2">opstinaet</rhyme></l>
        <l id="s11:l6" met="3" type="-+">V een kroone <rhyme label="c" type="f">opstelden</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Van doornen seer <rhyme label="d" type="m">fel</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">V beschimpten en <rhyme label="c" type="f" corresp="#s11:l6">quelden</rhyme>,</l>
        <l id="s11:l9" met="3" type="-+">Haer lasteren en <rhyme label="c" type="f" corresp="#s11:l6">schelden</rhyme>,</l>
        <l id="s11:l10" met="3" type="-+">Hebt ghy verdraghen <rhyme label="d" type="m" corresp="#s11:l7">wel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="5" type="-+">Ia tot de doot, de doot des cruyces <rhyme label="a" type="f">schandigh</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ghy u ootmoet <rhyme label="b" type="m">beweest</rhyme></l>
        <l id="s12:l3" met="5" type="-+">O Heer der heerlijckheyt seer <rhyme label="a" type="f" corresp="#s12:l1">goederhandigh</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Ghy storft ende <rhyme label="b" type="m" corresp="#s12:l2">verreest</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Daer door, alsoomen <rhyme label="b" type="m" corresp="#s12:l2">leest</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Sit ghy vry van <rhyme label="c" type="f">verseere</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Nu aen Godts <rhyme label="d" type="m">rechterhandt</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Ghekroont met prijs end' <rhyme label="c" type="f" corresp="#s12:l6">eere</rhyme>,</l>
        <l id="s12:l9" met="3" type="-+">Ghy hebt met sulcken <rhyme label="c" type="f" corresp="#s12:l6">keere</rhyme></l>
        <l id="s12:l10" met="3" type="-+">Verwonnen <rhyme label="d" type="m" corresp="#s12:l7">triumphant</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="5" type="-+">Prince gront-steen van den heyligen <rhyme label="a" type="f">Tempel</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Vast in den gheest <rhyme label="b" type="m">gheplant</rhyme>,</l>
        <l id="s13:l3" met="5" type="-+">Ghy zijt in ootmoet, ons een recht <rhyme label="a" type="f" corresp="#s13:l1">exempel</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Maer ghy doet <rhyme label="b" type="m" corresp="#s13:l2">wederstant</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Den hoovaerdigen, <rhyme label="b" type="m" corresp="#s13:l2">want</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Ghy haet al sulcke <rhyme label="c" type="f">quade</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Maer die ootmoedigh <rhyme label="d" type="m">blijft</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Die geeft ghy u <rhyme label="c" type="f" corresp="#s13:l6">genade</rhyme></l>
        <l id="s13:l9" met="3" type="-+">Dat sijn ziele <rhyme label="c" type="f" corresp="#s13:l6">ghestade</rhyme></l>
        <l id="s13:l10" met="3" type="-+">Vol rusten wel <rhyme label="d" type="m" corresp="#s13:l7">beklijft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
