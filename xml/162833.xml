        <l id="s1:l1" met="4" type="+-">Reys,  ik hou van nu af <rhyme label="a" type="m">aan</rhyme></l>
        <l id="s1:l2" met="2" type="+-">My <rhyme label="a" type="m" corresp="#s1:l1">voldaan</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wild slegts uyt u daden <rhyme label="b" type="f">tonen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat, met al wat gy <rhyme label="c" type="m">vermoogd</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Steedts uw hert in waarheit <rhyme label="c" type="m" corresp="#s1:l4">poogd</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Mijne liefde te <rhyme label="b" type="f" corresp="#s1:l3">belonen</rhyme>.</l>
