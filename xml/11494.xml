<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11494</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>shepherd's song</tag>
    </tags>
    <melody>Kom Venus uit uw hoge troon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DRijft Galath√©, mijn dartele <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hier u schaepjes uyt de <rhyme label="a" type="m" corresp="#s1:l1">wint</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Set u by my <rhyme label="b" type="f">neder</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Laetse grasen <rhyme label="c" type="m">hier</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Onder 't loof des <rhyme label="c" type="m" corresp="#s1:l4">Eglentier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'t Is nu van daegh den heughlijcken <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Ons God Bascho <rhyme label="a" type="m" corresp="#s2:l1">toe-gewijt</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Die men plaght te <rhyme label="b" type="f">vyeren</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Door ons groene <rhyme label="c" type="m">lant</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Hem met wil'ge <rhyme label="c" type="m" corresp="#s2:l4">offerhand'</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy, Lief! sult aen den dans <rhyme label="a" type="m">beneen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Dees mijn groene sy <rhyme label="a" type="m" corresp="#s3:l1">bekleen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Strickt u wackre <rhyme label="b" type="f">sooltjes</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Schort op 't dun <rhyme label="c" type="m">ghewaet</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">'t Welck u aers in 't dansen <rhyme label="c" type="m" corresp="#s3:l4">schaed'</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ionge Hiacint', het onnoosele <rhyme label="a" type="m">dier</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sal ons vee wel hoeden <rhyme label="a" type="m" corresp="#s4:l1">hier</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Want de rotz is <rhyme label="b" type="f">slibbrigh</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">En in 't vlack om <rhyme label="c" type="m">leegh</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Sijn de Geytjens in de <rhyme label="c" type="m" corresp="#s4:l4">weegh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Of, lief den schalken Tytirus <rhyme label="a" type="m">vlus</rhyme></l>
        <l id="s5:l2" met="4" type="+-">U mocht smeken om een <rhyme label="a" type="m" corresp="#s5:l1">kus</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Voeght u by y <rhyme label="b" type="f">Harder</rhyme>:</l>
        <l id="s5:l4" met="3" type="+-">Want hy, <rhyme label="c" type="m">Galathee</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Heeft te veel verkeert in <rhyme label="c" type="m" corresp="#s5:l4">stee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ick slecht', eenvoudige <rhyme label="a" type="m">Coridon</rhyme>!</l>
        <l id="s6:l2" met="4" type="+-">Noyt een Maeght bedriegen <rhyme label="a" type="m" corresp="#s6:l1">kon</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Daer op gaeft ghy <rhyme label="b" type="f">gist'ren</rhyme></l>
        <l id="s6:l4" met="3" type="+-">My dat syden <rhyme label="c" type="m">snoer</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Doen ick u mijn trouwe <rhyme label="c" type="m" corresp="#s6:l4">swoer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Van kints been af by Ellebrus <rhyme label="a" type="m">vloet</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Sijn wy t'samen <rhyme label="a" type="m" corresp="#s7:l1">opgevoet</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Daer wy dan als <rhyme label="b" type="f">kind'ren</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Aen de vlacke <rhyme label="c" type="m">strant</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Sochten schulpjes in het <rhyme label="c" type="m" corresp="#s7:l4">zandt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">En midd'ler tijt heeft des <rhyme label="a" type="m">minnens-brant</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Vast genomen <rhyme label="a" type="m" corresp="#s8:l1">d'overhant</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">En ons soete <rhyme label="b" type="f">zieltjes</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Soo ver-eenight <rhyme label="c" type="m">beyd</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Dat d'een niet van d'ander <rhyme label="c" type="m" corresp="#s8:l4">scheyd</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Komt, treet mijn Nimph, dees heuvelen <rhyme label="a" type="m">neer</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Want ick sie vast ginder <rhyme label="a" type="m" corresp="#s9:l1">veer</rhyme></l>
        <l id="s9:l3" met="3" type="+-">'t Harder-gilt <rhyme label="b" type="f">vergad'ren</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Bacchus met de <rhyme label="c" type="m">wijn</rhyme>;</l>
        <l id="s9:l5" met="4" type="+-">Laet ons niet de leste <rhyme label="c" type="m" corresp="#s9:l4">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
