  <author>Petrus Suetmans, Eustachius de Meyer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">Nederlanders gheeft u stemmen <rhyme label="a" type="m">klanck</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Looft den Oostenryckschen <rhyme label="b" type="m">helt</rhyme>,</l>
        <l id="s1:l3" met="5" type="+-">Croont hem met den weerden <rhyme label="a" type="m" corresp="#s1:l1">Louwer-ranck</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Want hy dryft den Frans uyt <rhyme label="b" type="m" corresp="#s1:l2">'tvelt</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Binnen <rhyme label="c" type="m">Valencyn</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Wou het haentjen <rhyme label="c" type="m" corresp="#s1:l5">zyn</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Hy versocht den Swaenen <rhyme label="d" type="m">nest</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Maer den Spaenschen <rhyme label="e" type="m">Leeuw</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Acht gheen Haen <rhyme label="e" type="m" corresp="#s1:l8">gheschreeuw</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Maer verjaeght hem uyt <rhyme label="d" type="m" corresp="#s1:l7">'tghewest</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Mazarin sont al de Fransche <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Tot verderf van't <rhyme label="b" type="m">Nederlant</rhyme>,</l>
        <l id="s2:l3" met="5" type="+-">Vyftigh duysent van dat licht <rhyme label="a" type="m" corresp="#s2:l1">gheslacht</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Stiert hy teghen onsen <rhyme label="b" type="m" corresp="#s2:l2">kant</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Graef Turene <rhyme label="c" type="m">straf</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Voert den Legher <rhyme label="c" type="m" corresp="#s2:l5">staf</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Van de gantsche Fransche <rhyme label="d" type="m">tocht</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Met Mons de <rhyme label="e" type="m">Grand-pré</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Ende la <rhyme label="e" type="m" corresp="#s2:l8">Fertè</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Hebben Valencyn <rhyme label="d" type="m" corresp="#s2:l7">besocht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Maer Don Ian heeft hun soo wel <rhyme label="a" type="m">ont-haelt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">In de Henegouwsche <rhyme label="b" type="m">feest</rhyme>,</l>
        <l id="s3:l3" met="5" type="+-">Daer den Frans-man heeft 'tghelagh <rhyme label="a" type="m" corresp="#s3:l1">betaelt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En seer wel ghesmeert <rhyme label="b" type="m" corresp="#s3:l2">gheweest</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Den Tureynschen <rhyme label="c" type="m">hoop</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Gaf hem tot den <rhyme label="c" type="m" corresp="#s3:l5">loop</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Maer de rest bleef in de <rhyme label="d" type="m">val</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Al wat niet en <rhyme label="e" type="m">vloot</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Dat is vast, oft <rhyme label="e" type="m" corresp="#s3:l8">doot</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Menigh duysen in't <rhyme label="d" type="m" corresp="#s3:l7">ghetal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Amonitie, kruyt, loot, en <rhyme label="a" type="m">canon</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En met duysent waghens <rhyme label="b" type="m">meel</rhyme>,</l>
        <l id="s4:l3" met="5" type="+-">Vendels, ende Standaerts men daer <rhyme label="a" type="m" corresp="#s4:l1">won</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Speys, en dranck, oock voor de <rhyme label="b" type="m" corresp="#s4:l2">keel</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Menigh peert, en <rhyme label="c" type="m">man</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Zyn hier tuyghnis <rhyme label="c" type="m" corresp="#s4:l5">van</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">La Eerte, ende <rhyme label="d" type="m">Grand-pré</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Die zyn <rhyme label="e" type="m">aen-ghetast</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">En sitten hier <rhyme label="e" type="m" corresp="#s4:l8">vast</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Met noch menigh Frans <rhyme label="d" type="m" corresp="#s4:l7">Cadé</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="+-">Staeckt nu Frans-man, staeckt u trotsche <rhyme label="a" type="m">pracht</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Treckt weer naer u <rhyme label="b" type="m">Haenen-kot</rhyme>,</l>
        <l id="s5:l3" met="5" type="+-">Soo ghy langer onsen Prins <rhyme label="a" type="m" corresp="#s5:l1">verwacht</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Soo schut ghy noch meer den <rhyme label="b" type="m" corresp="#s5:l2">bot</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">d'Oosten-rycksche <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">U dat leeren <rhyme label="c" type="m" corresp="#s5:l5">moet</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Dat hy niet en quam te <rhyme label="d" type="m">laet</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Die al 'tFrans <rhyme label="e" type="m">ghewelt</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Soo kloeck neder <rhyme label="e" type="m" corresp="#s5:l8">velt</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Ende met den sweerde <rhyme label="d" type="m" corresp="#s5:l7">slaet</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="+-">Lof syn Hoogheyt, met Prins de <rhyme label="a" type="m">Condé</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Caraceen die oock <rhyme label="b" type="m">verwon</rhyme>,</l>
        <l id="s6:l3" met="5" type="+-">Prins de Ligne, en Duc d'Aerschot <rhyme label="a" type="m" corresp="#s6:l1">mé</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Wittenbergh, Hennin <rhyme label="b" type="m" corresp="#s6:l2">Trelon</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">Vromen Hans <rhyme label="c" type="m">Verkest</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Die daer vocht om <rhyme label="c" type="m" corresp="#s6:l5">best</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">En met eeren waeght syn <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Gheeft dan Godt den <rhyme label="e" type="m">Heer</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Lof, preys, en al <rhyme label="e" type="m" corresp="#s6:l8">d'eer</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Die het suer soo keert in't <rhyme label="d" type="m" corresp="#s6:l7">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
