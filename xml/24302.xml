        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="rederijkerslied" translation="rhetorician song">rederijkerslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>O triomfante vrouw</melody>
    <categories>
      <category translation="groups">groepen</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O God vader verheuen <rhyme label="a" type="f">ghi</rhyme></l>
        <l id="s1:l2" met="3" type="-+">die ons sijt <rhyme label="b" type="f">genadich</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Hoe wort van ons <rhyme label="a" type="f" corresp="#s1:l1">bedreven</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sulcken boosheyt <rhyme label="b" type="f" corresp="#s1:l2">feldadich</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Nochtans ghi ons <rhyme label="b" type="f" corresp="#s1:l2">beradich</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Bevrijt vanden vyant <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ende ons hier gheeft <rhyme label="b" type="f" corresp="#s1:l2">gestadich</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ons daghelicx broot <rhyme label="c" type="m" corresp="#s1:l6">voorwaer</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Ons lichtende schoon en <rhyme label="c" type="m" corresp="#s1:l6">claer</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Met sonne en mane <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Hier om loeft godt mensch een <rhyme label="c" type="m" corresp="#s1:l6">paer</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Om sijn Elementen <rhyme label="d" type="m" corresp="#s1:l10">goet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hier om voor alle <rhyme label="a" type="f">saken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wilt Godt/ louende <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Sijn woort/ te recht wilt <rhyme label="a" type="f" corresp="#s2:l1">smaken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Aenmerckt/ reyn wt <rhyme label="b" type="f" corresp="#s2:l2">ghelesen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Sijn goetheyt groeyt <rhyme label="b" type="f" corresp="#s2:l2">ghepresen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die hy ons hier/ iont <rhyme label="c" type="m">ghemeyn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Den hemel/ claer midts <rhyme label="b" type="f" corresp="#s2:l2">desen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Bespoeyt/ ons met dauwe <rhyme label="c" type="m" corresp="#s2:l6">reyn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Die eerde/ in tswereldts <rhyme label="c" type="m" corresp="#s2:l6">pleyn</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Bringht vruchten voort metter <rhyme label="d" type="m">spoet</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Hier om looft Godt/ groot en <rhyme label="c" type="m" corresp="#s2:l6">cleyn</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Om sijn Elementen <rhyme label="d" type="m" corresp="#s2:l10">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die Elementen <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Godt selue <rhyme label="b" type="f">obedieren</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dus broeders/ dan <rhyme label="a" type="f" corresp="#s3:l1">idoone</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wilt tsheeren lof <rhyme label="b" type="f" corresp="#s3:l2">vermeeren</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En wilt niet elders <rhyme label="b" type="f" corresp="#s3:l2">keeren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dan tot Godt sijt niet <rhyme label="c" type="m">rebel</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die u hier anders <rhyme label="b" type="f" corresp="#s3:l2">leeren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Voorwaer sy en doen niet <rhyme label="c" type="m" corresp="#s3:l6">wel</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Dies doet al Christus <rhyme label="c" type="m" corresp="#s3:l6">beuel</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Aenmerckt/ sijn weldaden <rhyme label="d" type="m">vroet</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Looft/ danckt godt/ en niemant <rhyme label="c" type="m" corresp="#s3:l6">el</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Om sijn Elementen <rhyme label="d" type="m" corresp="#s3:l10">goedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hoe schoon claer/ int <rhyme label="a" type="f">verchieren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sijn hier des Heeren <rhyme label="b" type="f">wercken</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Des Elements <rhyme label="a" type="f" corresp="#s4:l1">mannieren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die sijn in alle <rhyme label="b" type="f" corresp="#s4:l2">percken</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Niet stil soo men mach <rhyme label="b" type="f" corresp="#s4:l2">mercken</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Godt subiect/ met gantscher <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dus ghy leecke/ en <rhyme label="b" type="f" corresp="#s4:l2">clercken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Slaet op Godts weldaden <rhyme label="c" type="m" corresp="#s4:l6">acht</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Tis voor ons dat godt oyt <rhyme label="c" type="m" corresp="#s4:l6">wracht</rhyme></l>
        <l id="s4:l10" met="3" type="-+">En noch is in volder <rhyme label="d" type="m">vloet</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Hier om/ looft godt dach en <rhyme label="c" type="m" corresp="#s4:l6">nacht</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Om sijn Elementen <rhyme label="d" type="m" corresp="#s4:l10">goet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Prince wilt godt/ <rhyme label="a" type="f">betrouwen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Altoos/ in alle <rhyme label="b" type="f">wijcken</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Ten sal u niet <rhyme label="a" type="f" corresp="#s5:l1">berouwen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Laet liefde aen u <rhyme label="b" type="f" corresp="#s5:l2">blijcken</rhyme></l>
        <l id="s5:l5" met="3" type="-+">En wilt u niet <rhyme label="b" type="f" corresp="#s5:l2">ghelijcken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Den onuerstandighen <rhyme label="c" type="m">quaet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Maer wilt sonder <rhyme label="b" type="f" corresp="#s5:l2">beswijcken</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Toch volghen des Heeren <rhyme label="c" type="m" corresp="#s5:l6">raedt</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Merckt sijn duecht seer <rhyme label="c" type="m" corresp="#s5:l6">delicaet</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Die hy ons daghelijcx/ <rhyme label="d" type="m">doet</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Hier om looft godt vroech en <rhyme label="c" type="m" corresp="#s5:l6">laet</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Om sijn Elementen <rhyme label="d" type="m" corresp="#s5:l10">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
