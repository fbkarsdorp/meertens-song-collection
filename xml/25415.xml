    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="meilied" translation="May song">meilied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Meyn <rhyme label="a" type="m">tsijt</rhyme></l>
        <l id="s1:l2" met="1" type="+-"><rhyme label="a" type="m" corresp="#s1:l1">ghijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">das ionghen hertzen wal <rhyme label="b" type="f">bevellet</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">zint der angher <rhyme label="a" type="m" corresp="#s1:l1">lijd</rhyme></l>
        <l id="s1:l5" met="3" type="-+">gaer wonnenclijch <rhyme label="b" type="f" corresp="#s1:l3">ghestellet</rhyme>.</l>
        <l id="s1:l6" met="2" type="+-">
          <rhyme label="c" type="m">Vogheliin</rhyme>
        </l>
        <l id="s1:l7" met="1" type="+-">
          <rhyme label="c" type="m" corresp="#s1:l6">ziin</rhyme>
        </l>
        <l id="s1:l8" met="4" type="-+">mit zanghe vrolich, das <rhyme label="b" type="f" corresp="#s1:l3">hellet</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Gheel vaer bloemen <rhyme label="c" type="m" corresp="#s1:l6">sciin</rhyme></l>
        <l id="s1:l10" met="3" type="-+">ziet men <rhyme label="b" type="f" corresp="#s1:l3">onghetellet</rhyme>.</l>
        <l id="s1:l11" met="4" type="+-">Des moet <rhyme label="d" type="f">zwachen</rhyme></l>
        <l id="s1:l12" met="4" type="+-">gayr yr <rhyme label="d" type="f" corresp="#s1:l11">leide</rhyme>.</l>
        <l id="s1:l13" met="2" type="+-">Vro can <rhyme label="e" type="m">machen</rhyme></l>
        <l id="s1:l14" met="1" type="+-">mir die lieve <rhyme label="e" type="m" corresp="#s1:l13">heyde</rhyme>.</l>
        <l id="s1:l15" met="4" type="-+">Das kan ich mich vro <rhyme label="b" type="f" corresp="#s1:l3">machen</rhyme>,</l>
        <l id="s1:l16" met="3" type="+-">dyr ich byn <rhyme label="e" type="m" corresp="#s1:l13">verzellet</rhyme></l>
        <l id="s1:l17" met="3" type="-+">gaer <rhyme label="b" type="f" corresp="#s1:l3">innentlijch</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">daer van miin hertz <rhyme label="a" type="m">quellet</rhyme>.</l>
        <l id="s2:l2" met="1" type="+-">Sal miin <rhyme label="a" type="m" corresp="#s2:l1">zanch</rhyme></l>
        <l id="s2:l3" met="4" type="-+">
          <rhyme label="b" type="f">lanch</rhyme>
        </l>
        <l id="s2:l4" met="3" type="+-">der hertzen lieven nich <rhyme label="a" type="m" corresp="#s2:l1">behaghen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">daer ich ye na <rhyme label="b" type="f" corresp="#s2:l3">ranch</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">wie mÜch ich das <rhyme label="c" type="m">verdraghen</rhyme>?</l>
        <l id="s2:l7" met="1" type="+-">Zal eyn <rhyme label="c" type="m" corresp="#s2:l6">man</rhyme></l>
        <l id="s2:l8" met="4" type="-+">
          <rhyme label="b" type="f" corresp="#s2:l3">van</rhyme>
        </l>
        <l id="s2:l9" met="3" type="-+">den vrouwen has eer prijs <rhyme label="c" type="m" corresp="#s2:l6">beiaghen</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">waer om dien ich yr <rhyme label="b" type="f" corresp="#s2:l3">dan</rhyme>?</l>
        <l id="s2:l11" met="4" type="+-">Das mach men mir wael <rhyme label="d" type="f">vraghen</rhyme>.</l>
        <l id="s2:l12" met="4" type="+-">Miin ghemuete ist hofens <rhyme label="d" type="f" corresp="#s2:l11">riiche</rhyme>.</l>
        <l id="s2:l13" met="2" type="+-">Wolde die lieve, het gynghe mir <rhyme label="e" type="m">ghelike</rhyme>.</l>
        <l id="s2:l14" met="1" type="+-">Was ich <rhyme label="e" type="m" corresp="#s2:l13">zol</rhyme></l>
        <l id="s2:l15" met="4" type="-+">
          <rhyme label="b" type="f" corresp="#s2:l3">wol</rhyme>
        </l>
        <l id="s2:l16" met="3" type="+-">den vrouwen zinghen odyr <rhyme label="e" type="m" corresp="#s2:l13">zaghen</rhyme>:</l>
        <l id="s2:l17" met="3" type="-+">was ich lidens <rhyme label="b" type="f" corresp="#s2:l3">dol</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">des moys ich ummer <rhyme label="a" type="m">claghen</rhyme>.</l>
        <l id="s3:l2" met="1" type="+-">
          <rhyme label="a" type="m" corresp="#s3:l1">Wan</rhyme>
        </l>
        <l id="s3:l3" met="4" type="-+">ich leve in den <rhyme label="b" type="f">waen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">das zi mijns <rhyme label="a" type="m" corresp="#s3:l1">beghere</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">zo steyt mir mijn <rhyme label="b" type="f" corresp="#s3:l3">moyt</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">oph ich der keyzer <rhyme label="c" type="m">were</rhyme>.</l>
        <l id="s3:l7" met="1" type="+-">Wan ich come an die <rhyme label="c" type="m" corresp="#s3:l6">stat</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">daer zi mich wal <rhyme label="b" type="f" corresp="#s3:l3">ontbere</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">zo wert mich ever <rhyme label="c" type="m" corresp="#s3:l6">wee</rhyme></l>
        <l id="s3:l10" met="3" type="-+">van commerlijcher <rhyme label="b" type="f" corresp="#s3:l3">zwere</rhyme>.</l>
        <l id="s3:l11" met="4" type="+-">twyderhande machs stut <rhyme label="d" type="f">wenden</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">scÜene fyeye, mach es mir een <rhyme label="d" type="f" corresp="#s3:l11">eynden</rhyme>!</l>
        <l id="s3:l13" met="2" type="+-">Vrouwe <rhyme label="e" type="m">vroet</rhyme>,</l>
        <l id="s3:l14" met="1" type="+-">
          <rhyme label="e" type="m" corresp="#s3:l13">doet</rhyme>
        </l>
        <l id="s3:l15" met="4" type="-+">mir wal, des zaltu haven <rhyme label="b" type="f" corresp="#s3:l3">ere</rhyme>!</l>
        <l id="s3:l16" met="3" type="+-">Verworve ich zulich <rhyme label="e" type="m" corresp="#s3:l13">goet</rhyme>,</l>
        <l id="s3:l17" met="3" type="-+">jo wensche ich nych <rhyme label="b" type="f" corresp="#s3:l3">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
