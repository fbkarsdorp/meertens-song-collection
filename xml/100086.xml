    <tags/>
    <melody>Les Feuillantines</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LIeve Iasper noch een <rhyme label="a" type="m">reys</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Dits mijn <rhyme label="a" type="m" corresp="#s1:l1">eys</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Aeltje ick en magh niet <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Sulje mijn dan doen <rhyme label="c" type="f">versmagten</rhyme>?</l>
        <l id="s1:l5" met="2" type="+-">Daer ick <rhyme label="d" type="m">troost</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Daer ick <rhyme label="d" type="m" corresp="#s1:l5">troost</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Van u moet <rhyme label="c" type="f" corresp="#s1:l4">wagten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Aeltje houd jou back-huys <rhyme label="a" type="m">toe</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">'k Ben te <rhyme label="a" type="m" corresp="#s2:l1">moe</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want ick mijn nau roeren <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Daerom Aeltje weest <rhyme label="c" type="f">gerusjes</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">'k Scheyer <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">'k scheyer <rhyme label="d" type="m" corresp="#s2:l5">uyt</rhyme></l>
        <l id="s2:l7" met="2" type="-+">En wacht tot <rhyme label="c" type="f" corresp="#s2:l4">flusjes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Iasper dat is noch soo <rhyme label="a" type="m">langh</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Ick ben <rhyme label="a" type="m" corresp="#s3:l1">bangh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Datjet van vergeten <rhyme label="b" type="m">selt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Aeltje ghy bruyt mijn wel <rhyme label="c" type="f">lustig</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">En je <rhyme label="d" type="m">bent</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">En je <rhyme label="d" type="m" corresp="#s3:l5">bent</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Wel <rhyme label="c" type="f" corresp="#s3:l4">ongerustig</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Mit soo scheet sy louter <rhyme label="a" type="m">hart</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Docht met <rhyme label="a" type="m" corresp="#s4:l1">smart</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Och, was sy de wint niet <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Men had seker haer zien <rhyme label="c" type="f">barsten</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Van 't <rhyme label="d" type="m">gedruys</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Van 't <rhyme label="d" type="m" corresp="#s4:l5">gedruys</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Want sy hart <rhyme label="c" type="f" corresp="#s4:l4">parste</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
