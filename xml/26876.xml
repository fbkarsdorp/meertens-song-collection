  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Singe ich nitt woll / das ist mir <rhyme label="a" type="m">leidt</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">von hertzen thette ichs <rhyme label="b" type="f">gerne</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">vnnd wer nach meinenn singenn nitt en <rhyme label="c" type="m">fragtt</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">der mag es woll <rhyme label="b" type="f" corresp="#s1:l2">entberenn</rhyme> /</l>
        <l id="s1:l5" met="4" type="-+">Das magtt allein / mein arm <rhyme label="d" type="m">gesang</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">darz  mein bose <rhyme label="e" type="f">schalle</rhyme> /</l>
        <l id="s1:l7" met="4" type="-+">mein wiese iß nemantz z  <rhyme label="d" type="m" corresp="#s1:l5">danck</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">vnnd th itt mir well <rhyme label="e" type="f" corresp="#s1:l6">gefallenn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nochtans will ich einen g etten modtt <rhyme label="a" type="m">han</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">Vmb nemantt will ich dieß <rhyme label="b" type="f">lassen</rhyme> /</l>
        <l id="s2:l3" met="4" type="-+">Mein narren will mir nitt <rhyme label="c" type="m">vergahenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">ich dreibes jn rechter <rhyme label="b" type="f" corresp="#s2:l2">massenn</rhyme> /</l>
        <l id="s2:l5" met="4" type="-+">n n wie die heist / ich achtz nitt <rhyme label="d" type="m">vill</rhyme></l>
        <l id="s2:l6" met="3" type="-+">ich schri e mir gar vn <rhyme label="e" type="f">erborgenn</rhyme> /</l>
        <l id="s2:l7" met="4" type="-+">das is  allein mein alte <rhyme label="d" type="m" corresp="#s2:l5">gesetz</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">vor mir darff nemantt <rhyme label="e" type="f" corresp="#s2:l6">sorgenn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Och sorgenn th itt meinem he ffte <rhyme label="a" type="m">wee</rhyme></l>
        <l id="s3:l2" met="3" type="-+">vnnd krenckt mir mein gem <rhyme label="b" type="f">tt</rhyme> /</l>
        <l id="s3:l3" met="4" type="-+">nh n tra renn will ich farhenn <rhyme label="c" type="m">lan</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">vnnd grundenn z  der rechter <rhyme label="b" type="f" corresp="#s3:l2">massenn</rhyme> /</l>
        <l id="s3:l5" met="4" type="-+">dans nemantz en radett vnnd mir <rhyme label="d" type="m">gesellet</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">das mag man mir woll g <rhyme label="e" type="f">nnen</rhyme> /</l>
        <l id="s3:l7" met="4" type="-+">darmitt fartt mir all mein tra ren <rhyme label="d" type="m" corresp="#s3:l5">daher</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">ich singe gern wan ichs k <rhyme label="e" type="f" corresp="#s3:l6">nne</rhyme>.</l>
      </lg>
    </lg>
  </text>
