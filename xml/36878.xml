  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LAet ons weer naer 't stal'ken <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">'t Kindt wat draeghen naer ons <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Eerder bringhen groote <rhyme label="a" type="f" corresp="#s1:l1">Heeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hun present met grooten <rhyme label="b" type="m" corresp="#s1:l2">pracht</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Ick hop' Godt ons ghift <rhyme label="c" type="m">behaeght</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Alsoo wel, als die veel <rhyme label="c" type="m" corresp="#s1:l5">draeght</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ick zal draeghen stoelen, <rhyme label="a" type="f">bancken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Stroo om wel te decken 't <rhyme label="b" type="m">dack</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Een schapraey, en stijlen, <rhyme label="a" type="f" corresp="#s2:l1">plancken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maecken een koets, 't hun <rhyme label="b" type="m" corresp="#s2:l2">ghemack</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Draeghen me een vlies van't <rhyme label="c" type="m">schaep</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Op dat 't Kintjen daer in <rhyme label="c" type="m" corresp="#s2:l5">slaep</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ick zal een vet schaepken <rhyme label="a" type="f">gheven</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En een lammeken daer <rhyme label="b" type="m">by</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Noch een kalfken nemen 't <rhyme label="a" type="f" corresp="#s3:l1">leven</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En oock kiekens twee ofte <rhyme label="b" type="m" corresp="#s3:l2">dry</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Gh'en meughe't niet draeghen <rhyme label="c" type="m">rauw</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Daer en is noch vier, noch <rhyme label="c" type="m" corresp="#s3:l5">schauw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">En ick zal een korfken <rhyme label="a" type="f">draeghen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Met boter, eyers, en <rhyme label="b" type="m">vis</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Voeren oock op eenen <rhyme label="a" type="f" corresp="#s4:l1">waeghen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Van't ghen' in ons huysken <rhyme label="b" type="m" corresp="#s4:l2">is</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Gh'en meught't niet draeghen <rhyme label="c" type="m">rauw</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Daer en is noch vier, noch <rhyme label="c" type="m" corresp="#s4:l5">schauw</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ick zal draeghen boonen, <rhyme label="a" type="f">raepen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En een kan' met soete <rhyme label="b" type="m">melck</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Om 't Kindt een papken te <rhyme label="a" type="f" corresp="#s5:l1">maeken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">'k Gheev' daer by een koecxken <rhyme label="b" type="m" corresp="#s5:l2">elck</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Gh'en meughe't niet draeghen <rhyme label="c" type="m">rauw</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Daer en is noch vier, noch <rhyme label="c" type="m" corresp="#s5:l5">schauw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ick zal my voor 't Kindt <rhyme label="a" type="f">verneren</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Vallen hem heel neer te <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Hem my selven <rhyme label="a" type="f" corresp="#s6:l1">presenteren</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Gheven hem sweet, vleesch, en <rhyme label="b" type="m" corresp="#s6:l2">bloet</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">En wesen slaef <rhyme label="c" type="m">bovendien</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Van dees weerdigh Edel <rhyme label="c" type="m" corresp="#s6:l5">Lien</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ick zal draeghen hem mijn <rhyme label="a" type="f">herte</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Onbevleckt dat gheven <rhyme label="b" type="m">g'heel</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Mijnen wil, druck, ende <rhyme label="a" type="f" corresp="#s7:l1">smerte</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">'t Is hem 't aenghenaemst' <rhyme label="b" type="m" corresp="#s7:l2">juweel</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">In het hert 't Kindt gheren <rhyme label="c" type="m">rust</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Daer beroerten zijn <rhyme label="c" type="m" corresp="#s7:l5">ghesust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
