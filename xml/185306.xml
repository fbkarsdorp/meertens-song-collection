<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185306</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Minzaam beeldje amoureus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Hoe inwendich, wel <rhyme label="a" type="m">gemaniert</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Syon vriendinne, <rhyme label="b" type="m">ghy</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Staet behendich, en schoon <rhyme label="a" type="m" corresp="#s1:l1">verciert</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Soo dat deur minne, <rhyme label="b" type="m" corresp="#s1:l2">my</rhyme></l>
        <l id="s1:l5" met="4" type="-+">‘t Hert sy, tot dy, <rhyme label="c" type="f">ontsteken</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">‘t Vytbreken, daer of <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">‘t Verrijcken, can <rhyme label="e" type="f">blijcken</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">‘t Afwijcken, niet <rhyme label="d" type="m" corresp="#s1:l6">min</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Gheresen, in <rhyme label="f" type="f">desen</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Laet wesen <rhyme label="d" type="m" corresp="#s1:l6">vriendin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vliet van stonden, het aertsche <rhyme label="a" type="m">slijck</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Soo dat u herte, <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer verbonden, en sy <rhyme label="a" type="m" corresp="#s2:l1">ghelijck</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Men veel tot smerte, <rhyme label="b" type="m" corresp="#s2:l2">siet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Verdriet, geschiet, ten <rhyme label="c" type="f">laetsten</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Die haesten, veel <rhyme label="d" type="m">hierom</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">En baren, van <rhyme label="e" type="f">haren</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Dierbaren, <rhyme label="d" type="m" corresp="#s2:l6">rijckdom</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Ia sterven, <rhyme label="f" type="f">bederven</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Om erven, kleyn <rhyme label="d" type="m" corresp="#s2:l6">rom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wert voor ooghen, u hier <rhyme label="a" type="m">gestelt</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Deur het ingeven, <rhyme label="b" type="m">van</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Sathans tooghen, goet of <rhyme label="a" type="m" corresp="#s3:l1">gewelt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wilt my lief kleven, <rhyme label="b" type="m" corresp="#s3:l2">an</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ick dan, u can, <rhyme label="c" type="f">verstercken</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Laet mercken, dat ghy <rhyme label="d" type="m">zijt</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Ghetrouwe, <rhyme label="e" type="f">jonckvrouwe</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Na rouwe, den <rhyme label="d" type="m" corresp="#s3:l6">strijdt</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Ghestreden, in <rhyme label="f" type="f">vreden</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Ghy heden, <rhyme label="d" type="m" corresp="#s3:l6">verblijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hebt ghy grondich, wel <rhyme label="a" type="m">aengemerckt</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Mijn liefd’ in allen, <rhyme label="b" type="m">noot</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Doen ghy sondich, noch waerdt <rhyme label="a" type="m" corresp="#s4:l1">ontvlerckt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ia laeght gevallen, <rhyme label="b" type="m" corresp="#s4:l2">doot</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Gans doot, deur noot, van <rhyme label="c" type="f">sneven</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">V leven, en <rhyme label="d" type="m">welvaert</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Seer milde, ick <rhyme label="e" type="f">wilde</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Dus stilde, ick <rhyme label="d" type="m" corresp="#s4:l6">daert</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Godts tooren, te <rhyme label="f" type="f">vooren</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Verloren, <rhyme label="d" type="m" corresp="#s4:l6">verklaert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">So in ‘t lyden, ick voor u <rhyme label="a" type="m">ginck</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Mach u behagen, <rhyme label="b" type="m">dat</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wilt oock strijden, so ick <rhyme label="a" type="m" corresp="#s5:l1">aenvinck</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Mijn cruys te dragen, <rhyme label="b" type="m" corresp="#s5:l2">wat</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ick trat, en bat, mijn <rhyme label="c" type="f">Vader</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Te gader, voor die <rhyme label="d" type="m">hoort</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Ten syne na <rhyme label="e" type="f">mijne</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Doctrijne, gaet <rhyme label="d" type="m" corresp="#s5:l6">voort</rhyme>,</l>
        <l id="s5:l9" met="4" type="-+">Dat hyse, <rhyme label="f" type="f">bewyse</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Oock spyse, deur ‘t <rhyme label="d" type="m" corresp="#s5:l6">woort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daerom laten, sult ghy te <rhyme label="a" type="m">recht</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Dijn huys met eenen, <rhyme label="b" type="m">moet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En dat haten, wat ghy <rhyme label="a" type="m" corresp="#s6:l1">aenleght</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Met goeder meenen, <rhyme label="b" type="m" corresp="#s6:l2">doet</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Verwoet, den gloeyt sal <rhyme label="c" type="f">raeyen</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">En maeyen, af ‘t <rhyme label="d" type="m">oncruyt</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Manierich, en <rhyme label="e" type="f">vierich</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Vercierich, comt <rhyme label="d" type="m" corresp="#s6:l6">uyt</rhyme>,</l>
        <l id="s6:l9" met="4" type="-+">Dees sinnen, van <rhyme label="f" type="f">binnen</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Laet winnen, reyn <rhyme label="d" type="m" corresp="#s6:l6">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">O Princesse, van alle <rhyme label="a" type="m">deucht</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">V niet omkeeren, <rhyme label="b" type="m">laet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Als fortresse, seer sterck ghy <rhyme label="a" type="m" corresp="#s7:l1">meucht</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Van u afweren, ‘t <rhyme label="b" type="m" corresp="#s7:l2">quaet</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Maer gaet, na raet, <rhyme label="c" type="f">verstandich</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">In brandich, liefde <rhyme label="d" type="m">tot</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Die sochte, dy <rhyme label="e" type="f">kochte</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">En brochte, tot <rhyme label="d" type="m" corresp="#s7:l6">Godt</rhyme>,</l>
        <l id="s7:l9" met="4" type="-+">Laet spruyten, wilt <rhyme label="f" type="f">buyten</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Niet sluyten, ‘t <rhyme label="d" type="m" corresp="#s7:l6">gebodt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
