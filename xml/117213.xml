    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">JAn Kooptmen een <rhyme label="a" type="f">Kermis</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Moy Meysje ik heb ‘er geen <rhyme label="b" type="m">geld</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ik sou u wel een Kermis <rhyme label="c" type="f">kopen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Maer ’t geld is door myn broek <rhyme label="c" type="f" corresp="#s1:l3">gedropen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ian koopt men een <rhyme label="a" type="f" corresp="#s1:l1">Kermis</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ach Meysjen ik heb ‘er geen <rhyme label="b" type="m" corresp="#s1:l2">geld</rhyme>.</l>
      </lg>
