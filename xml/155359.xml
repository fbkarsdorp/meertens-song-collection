<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155359</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Si c'est pour mon pucelage</melody>
  <author>Aernout van Overbeke</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DEed ick qualijck dat 'k mijn <rhyme label="a" type="f">Hofstee</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met mijn Tuyn en Bloemen <rhyme label="b" type="m">perck</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Mijn part Erfs <rhyme label="b" type="m" corresp="#s1:l2">futselwerck</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Met al 't buyten leven <rhyme label="a" type="f" corresp="#s1:l1">afsnee</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Daer ick altoos eenigh <rhyme label="c" type="m">sat</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En quam wonen inde <rhyme label="c" type="m" corresp="#s1:l5">Stat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Niet in 't Landt der <rhyme label="a" type="f">Haerlemijten</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Daer soo meen'gen <rhyme label="b" type="m">Tulipaen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Duysent kronen quam te <rhyme label="b" type="m" corresp="#s2:l2">staen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dan sou my mijn Thuyn <rhyme label="a" type="f" corresp="#s2:l1">verwijten</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dat ick in een Bloem de <rhyme label="c" type="m">kleur</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Hadt genomen voor de <rhyme label="c" type="m" corresp="#s2:l5">geur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer in 't Haegje daer de <rhyme label="a" type="f">groten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Schijnen Luy van groote <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">In die vollickrijcke <rhyme label="b" type="m" corresp="#s3:l2">Stadt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Daer geen Poort en werdt <rhyme label="a" type="f" corresp="#s3:l1">gesloten</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Welckers grootheyt muyr noch <rhyme label="c" type="m">wal</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Nimmermeer begrijpen <rhyme label="c" type="m" corresp="#s3:l5">sal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Moest t'huys alle dingh <rhyme label="a" type="f">doorsnoff'len</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Mijn Cytroen-blat en <rhyme label="b" type="m">Jasmijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'k Badt dan eens om <rhyme label="b" type="m" corresp="#s4:l2">Sonneschijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dan om regen voor <rhyme label="a" type="f" corresp="#s4:l1">Grenoff'len</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">'k Vreesd' als daer een By op <rhyme label="c" type="m">sat</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat hy al den Reuck op <rhyme label="c" type="m" corresp="#s4:l5">at</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Geef een Scheyt-brief nu aen <rhyme label="a" type="f">Flore</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Schoon de Rosen cierlijck <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Men komt daer niet macklijck <rhyme label="b" type="m" corresp="#s5:l2">aen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Want men quetst sich aen de <rhyme label="a" type="f" corresp="#s5:l1">dore</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Hoe hoogh 't velt gepresen <rhyme label="c" type="m">wort</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Florás krans is haest <rhyme label="c" type="m" corresp="#s5:l5">verdort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Noch is Reuck niet te <rhyme label="a" type="f">versmaden</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Alsmen voeght de Smaeck daer <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">By exempel <rhyme label="b" type="m" corresp="#s6:l2">Cellery</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Of Agourkjes of <rhyme label="a" type="f" corresp="#s6:l1">Salade</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Mits dat al dit goetje <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Als een Schildtknaep by 't <rhyme label="c" type="m" corresp="#s6:l5">gebraet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'t Is of 't Spel sprack, siet hoe <rhyme label="a" type="f">aerdigh</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Is elck op sijn plaets <rhyme label="b" type="m">geset</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Meester Frans verstaet het <rhyme label="b" type="m" corresp="#s7:l2">net</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En hy is warachtigh <rhyme label="a" type="f" corresp="#s7:l1">waerdigh</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Dat hy niet maer eens in <rhyme label="c" type="m">Maert</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Maer wel twemael 's jaers <rhyme label="c" type="m" corresp="#s7:l5">verjaert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">'k Wouw mijn neus nu niet <rhyme label="a" type="f">verruylen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Voor de houte neus van <rhyme label="b" type="m">Jan</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Die hy met een kunsje <rhyme label="b" type="m" corresp="#s8:l2">kan</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Aerdigh in sijn gadt <rhyme label="a" type="f" corresp="#s8:l1">verschuylen</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Als hy bangh is dat de <rhyme label="c" type="m">kouw</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Aen sijn gevel schaê doen <rhyme label="c" type="m" corresp="#s8:l5">souw</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Geen Rabarber of het <rhyme label="a" type="f">haet'lijck</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Aposema set hy <rhyme label="b" type="m">op</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Maer wel een veel klaerder <rhyme label="b" type="m" corresp="#s9:l2">sop</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dat de hersens opent <rhyme label="a" type="f" corresp="#s9:l1">daet'lijck</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">En maeckt dat een menschen <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">In een tongh verandert <rhyme label="c" type="m" corresp="#s9:l5">wert</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Prince maeckt voortaen geen <rhyme label="a" type="f">mentci</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Van extract of <rhyme label="b" type="m">spiritus</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Prijst dat aen een <rhyme label="b" type="m" corresp="#s10:l2">tiribus</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">'k Sit hier by de <rhyme label="a" type="f" corresp="#s10:l1">quintessentci</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Van de Vischmarckt ende <rhyme label="c" type="m">Hal</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Daer 'k het oock mé houden <rhyme label="c" type="m" corresp="#s10:l5">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
