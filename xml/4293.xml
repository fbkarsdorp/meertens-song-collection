<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4293</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag translation="love song">liefdeslied</tag>
      <tag translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Gij heidenen allen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">NAer dien dat ick <rhyme label="a" type="m">moet</rhyme> //</l>
        <l id="s1:l2" met="3" type="-+">Subiect, en dienstbaer <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">En mijn vryheyt <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme> //</l>
        <l id="s1:l4" met="3" type="-+">Geheel te buyten <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Meer <rhyme label="b" type="m" corresp="#s1:l2">onderdaen</rhyme> //</l>
        <l id="s1:l6" met="3" type="-+">Dan my rebel // <rhyme label="c" type="m">verthoont</rhyme> //</l>
        <l id="s1:l7" met="2" type="-+">Heb ic, naer <rhyme label="d" type="m">recht</rhyme> //</l>
        <l id="s1:l8" met="2" type="-+">Alsulck een <rhyme label="d" type="m" corresp="#s1:l7">knecht</rhyme> //</l>
        <l id="s1:l9" met="3" type="+-">Wert namaels wel // <rhyme label="c" type="m" corresp="#s1:l6">geloont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Edel, dier // en // <rhyme label="a" type="m">weerdt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Is zy, die t'hert <rhyme label="b" type="m">aensiet</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Al dreyght vier, en // <rhyme label="a" type="m" corresp="#s2:l1">sweert</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ick stae tot haer <rhyme label="b" type="m" corresp="#s2:l2">gebiet</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Deed'- ick sulcx <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Soo waer ick fel // <rhyme label="c" type="m">gehoont</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Wie twijffelt <rhyme label="d" type="m">dies</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Sulck tijt <rhyme label="d" type="m" corresp="#s2:l7">verlies</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Wert namaels wel // <rhyme label="c" type="m" corresp="#s2:l6">geloont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Een alsulcken <rhyme label="a" type="m">ieught</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Blijf ick eygen <rhyme label="b" type="m">voortan</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Wert het eynde <rhyme label="a" type="m" corresp="#s3:l1">vreught</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Soo sit ick edel <rhyme label="b" type="m" corresp="#s3:l2">man</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Als Coninck <rhyme label="b" type="m" corresp="#s3:l2">van</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gans Israhel // <rhyme label="c" type="m">getroont</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">T'sy hoe dat <rhyme label="d" type="m">gaet</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">De minste <rhyme label="d" type="m" corresp="#s3:l7">daet</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Wert naermaels wel // <rhyme label="c" type="m" corresp="#s3:l6">geloont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Liever smert // en <rhyme label="a" type="m">leedt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">T'aenveerden, angstich <rhyme label="b" type="m">swaer</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En haer t'zijn <rhyme label="a" type="m" corresp="#s4:l1">gereet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Een knecht, drie seuen <rhyme label="b" type="m" corresp="#s4:l2">iaer</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Soo werd' ick <rhyme label="b" type="m" corresp="#s4:l2">naer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">T'gewonnen spel // <rhyme label="c" type="m">gecroont</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">Dat dunct my <rhyme label="d" type="m">sienst</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Getrouwen <rhyme label="d" type="m" corresp="#s4:l7">dienst</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Wert namaels wel // <rhyme label="c" type="m" corresp="#s4:l6">geloont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Keert sy mijn <rhyme label="a" type="m">labeur</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Te zijden, als yet <rhyme label="b" type="m">cleens</rhyme>:</l>
        <l id="s5:l3" met="3" type="+-">My arm <rhyme label="a" type="m" corresp="#s5:l1">serviteur</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Toelaet luttel <rhyme label="b" type="m" corresp="#s5:l2">gemeens</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">T'sal werden <rhyme label="b" type="m" corresp="#s5:l2">eens</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat mijn gequel // <rhyme label="c" type="m">verschoont</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Want die <rhyme label="d" type="m">altijt</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Verdraeght, en <rhyme label="d" type="m" corresp="#s5:l7">lijdt</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Wert namaels wel // <rhyme label="c" type="m" corresp="#s5:l6">geloont</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Es oock meerder <rhyme label="a" type="m">last</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Vervaerlick elcx <rhyme label="b" type="m">gepeys</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Dat een dorper <rhyme label="a" type="m" corresp="#s6:l1">gast</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Geniet der minnen <rhyme label="b" type="m" corresp="#s6:l2">cheys</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Venus <rhyme label="b" type="m" corresp="#s6:l2">paleys</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Sonder bevel // <rhyme label="c" type="m">bewoont</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Och! t'is te <rhyme label="d" type="m">cleyn</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Dat sulck <rhyme label="d" type="m" corresp="#s6:l7">vileyn</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Wert namaels wel // <rhyme label="c" type="m" corresp="#s6:l6">geloont</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Niet versagen // <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Mijn Princelick <rhyme label="b" type="m">gemoet</rhyme>:</l>
        <l id="s7:l3" met="3" type="+-">Liever dragen // <rhyme label="a" type="m" corresp="#s7:l1">al</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Des weerelts <rhyme label="b" type="m" corresp="#s7:l2">tegenspoet</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Want diet my <rhyme label="b" type="m" corresp="#s7:l2">doet</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Is so ydoonen <rhyme label="c" type="m">maeght</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Voor cleyn <rhyme label="d" type="m">geween</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Ghy namaels <rhyme label="d" type="m" corresp="#s7:l7">geen</rhyme></l>
        <l id="s7:l9" met="3" type="+-">Soo wel beloonen // <rhyme label="c" type="m" corresp="#s7:l6">saeght</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
