<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20356</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De stille nacht genaakt</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE blonde Phebus <rhyme label="a" type="m">schijnt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat Carileen haer <rhyme label="a" type="m" corresp="#s1:l1">pijnt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Hier in de groene <rhyme label="b" type="f">weyden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dicht by 't ghevlochten <rhyme label="c" type="m">woudt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer sy vast schildt-wacht <rhyme label="c" type="m" corresp="#s1:l4">houdt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Haer Harder te <rhyme label="b" type="f" corresp="#s1:l3">verbeyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'k Gae dan nae <rhyme label="a" type="m">Carileen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Door 't Else-bosjen <rhyme label="a" type="m" corresp="#s2:l1">heen</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Mijn Enghel sal <rhyme label="b" type="f">verlanghen</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Want eenigheydt <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Baert treuren en <rhyme label="c" type="m" corresp="#s2:l4">gheween</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Ick gae, en spoey mijn <rhyme label="b" type="f" corresp="#s2:l3">gangen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dit is de groene <rhyme label="a" type="m">wey</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Daer Carileen my <rhyme label="a" type="m" corresp="#s3:l1">sey</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat sy my souw <rhyme label="b" type="f">verwachten</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Maer laes! sy is 'er <rhyme label="c" type="m">niet</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Wat of daer is <rhyme label="c" type="m" corresp="#s3:l4">gheschiedt</rhyme>?</l>
        <l id="s3:l6" met="3" type="-+">Hoe woelen myn <rhyme label="b" type="f" corresp="#s3:l3">ghedachten</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Heeft u het wildt <rhyme label="a" type="m">Ghediert</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat door de Bossen <rhyme label="a" type="m" corresp="#s4:l1">swiert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Verscheurt aen duysendt <rhyme label="b" type="f">stucken</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Soo laet u Geestje <rhyme label="c" type="m">doch</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Eens by my komen <rhyme label="c" type="m" corresp="#s4:l4">noch</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dit Rif uyt 't Lichaem <rhyme label="b" type="f" corresp="#s4:l3">rucken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">My dunckt ick hoor <rhyme label="a" type="m">gherucht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van Schaepjens, die de <rhyme label="a" type="m" corresp="#s5:l1">lucht</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Vervullen met haer <rhyme label="b" type="f">blaten</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">'t Is mis, het is <rhyme label="c" type="m">ghedroomt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">'t Is 't rits'len van 't <rhyme label="c" type="m" corresp="#s5:l4">Gheboomt</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">Neen, 'k hoor daer yemandt <rhyme label="b" type="f" corresp="#s5:l3">praten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hou, Carileen, stae <rhyme label="a" type="m">vast</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Wat zijt ghy voor een <rhyme label="a" type="m" corresp="#s6:l1">gast</rhyme>?</l>
        <l id="s6:l3" met="3" type="-+">Ghy sult my niet <rhyme label="b" type="f">ontsnappen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Komt steldt u gheest te <rhyme label="c" type="m">vre'en</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En scheurt dees Romp van <rhyme label="c" type="m" corresp="#s6:l4">een</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">'k Wil mee nae Styx toe <rhyme label="b" type="f" corresp="#s6:l3">stappen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hoe, is 't maer <rhyme label="a" type="m">artseny</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">Wie staet daer aen mijn <rhyme label="a" type="m" corresp="#s7:l1">zy</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Ghy sult my niet <rhyme label="b" type="f">ontloopen</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Och! 't is een doodtse <rhyme label="c" type="m">Romp</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Een Boomeloose <rhyme label="c" type="m" corresp="#s7:l4">stomp</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Ghy soudt het aers <rhyme label="b" type="f" corresp="#s7:l3">bekoopen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hoe is de Lucht <rhyme label="a" type="m">besmoockt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Siet hoe 't daer gunder <rhyme label="a" type="m" corresp="#s8:l1">roockt</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">Ay! siet dat bosch eens <rhyme label="b" type="f">branden</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Daer 's Charon met sijn <rhyme label="c" type="m">Schuyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En loert vast op de <rhyme label="c" type="m" corresp="#s8:l4">buyt</rhyme>;</l>
        <l id="s8:l6" met="3" type="-+">Ey Veerman houd u <rhyme label="b" type="f" corresp="#s8:l3">handen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ach! hoe mijn ziele <rhyme label="a" type="m">woelt</rhyme>:</l>
        <l id="s9:l2" met="3" type="-+">Komt bracke tranen <rhyme label="a" type="m" corresp="#s9:l1">spoelt</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Langhs dees verbleeckte <rhyme label="b" type="f">kaecken</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Van daer rolt op de <rhyme label="c" type="m">aerdt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">En daer soo veel <rhyme label="c" type="m" corresp="#s9:l4">vergaert</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Tot ghy een beeck kondt <rhyme label="b" type="f" corresp="#s9:l3">maecken</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Daer ghy u selfs in <rhyme label="a" type="m">drenckt</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">Ach! Cloris <rhyme label="a" type="m" corresp="#s10:l1">overdenckt</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Wat dat ghy hebt <rhyme label="b" type="f">begonnen</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">V Carileena <rhyme label="c" type="m">doodt</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Wat vlock, O! snootste <rhyme label="c" type="m" corresp="#s10:l4">snoot</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Mach voor u zijn <rhyme label="b" type="f" corresp="#s10:l3">versonnen</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
