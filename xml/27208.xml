  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Gelyck als Tantalus begeerich <rhyme label="a" type="m">haect</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Naer waeter <rhyme label="a" type="m" corresp="#s1:l1">smaeckt</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Soo is myn hart schoon lieff tot v <rhyme label="a" type="m" corresp="#s1:l1">geraect</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Dwelck vierich <rhyme label="a" type="m" corresp="#s1:l1">blaeckt</rhyme></l>
        <l id="s1:l5" met="5" type="-+">Och lieff vermaeckt v bedruckte <rhyme label="b" type="m">dienaer</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Want tis myn <rhyme label="c" type="f">steruen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Soo ick moet <rhyme label="c" type="f" corresp="#s1:l6">deruen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">V aenschyn <rhyme label="b" type="m" corresp="#s1:l5">claer</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Ach off v hart myn thoonde een bly <rhyme label="a" type="m">gemoet</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Als tmyn v <rhyme label="a" type="m" corresp="#s2:l1">doet</rhyme></l>
        <l id="s2:l3" met="5" type="-+">Als leander soudt ick door swemmen <rhyme label="a" type="m" corresp="#s2:l1">goet</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Den zee <rhyme label="a" type="m" corresp="#s2:l1">onvroet</rhyme></l>
        <l id="s2:l5" met="5" type="-+">En metter spoet naer v tzuchten <rhyme label="b" type="m">certeyn</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Want schoon <rhyme label="c" type="f">godinne</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Niemant ick <rhyme label="c" type="f" corresp="#s2:l6">minne</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Dan v <rhyme label="b" type="m" corresp="#s2:l5">alleyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Dabsenty van v door cupidos <rhyme label="a" type="m">gewout</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Myn zeer <rhyme label="a" type="m" corresp="#s3:l1">benout</rhyme></l>
        <l id="s3:l3" met="5" type="-+">Want tfondament der lieffden <rhyme label="a" type="m" corresp="#s3:l1">meenichfout</rhyme></l>
        <l id="s3:l4" met="2" type="-+">heb ick <rhyme label="a" type="m" corresp="#s3:l1">gebout</rhyme></l>
        <l id="s3:l5" met="5" type="-+">Vry onberout tot v alleen <rhyme label="b" type="m">gekeert</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Soo Ick moet <rhyme label="c" type="f">deruen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">V reyn <rhyme label="c" type="f" corresp="#s3:l6">conserue</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Myn vruecht <rhyme label="b" type="m" corresp="#s3:l5">cesseert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Princesse mynjoot wilt boeten myn <rhyme label="a" type="m">verdriet</rhyme></l>
        <l id="s4:l2" met="2" type="-+">En] laet het <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme></l>
        <l id="s4:l3" met="5" type="-+">Al is door v absenty myn groot leet <rhyme label="a" type="m" corresp="#s4:l1">geschiet</rhyme></l>
        <l id="s4:l4" met="2" type="-+">V jonste <rhyme label="a" type="m" corresp="#s4:l1">biet</rhyme>]</l>
        <l id="s4:l5" met="5" type="-+">T]en zy wel siet dat ghy v tot my <rhyme label="b" type="m">went</rhyme>]</l>
        <l id="s4:l6" met="2" type="-+">Soo moet ick <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Sonder <rhyme label="c" type="f" corresp="#s4:l6">verblyden</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Brant sonder <rhyme label="b" type="m" corresp="#s4:l5">endt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
