<song>
  <meta>
    <id>151859</id>
    <date>
      <year-start>1682</year-start>
      <year-end>1682</year-end>
    </date>
    <location>
      <orginal-name>antwerpen [=amsterdam]</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Diei solemnia</melody>
  <author>Christianus de Placker</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Singht met eenen blijden <rhyme label="a" type="m">geest</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+"><rhyme label="b" type="m">Alleluya</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Op dit hooge <rhyme label="a" type="m" corresp="#s1:l1">Paessche-feest</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Alle ale <rhyme label="b" type="m" corresp="#s1:l2">luya</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Jesus is <rhyme label="a" type="m">victorieus</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">
          <rhyme label="b" type="m">Alleluja</rhyme>
        </l>
        <l id="s2:l3" met="4" type="+-">Verresen weerom <rhyme label="a" type="m" corresp="#s2:l1">glorieus</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Alle <rhyme label="b" type="m" corresp="#s2:l2">Alleluja</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hy besocht d' Oudt-vaders <rhyme label="a" type="m">bend'</rhyme>:</l>
        <l id="s3:l2" met="2" type="-+"><rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">En die verlost' uyt haer <rhyme label="a" type="m" corresp="#s3:l1">ellend'</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Alle <rhyme label="b" type="m" corresp="#s3:l2">Alleluja</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">De derde dagh verrees uyt 't <rhyme label="a" type="m">graf</rhyme>:</l>
        <l id="s4:l2" met="2" type="-+"><rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">En sijn Discipels vrede <rhyme label="a" type="m" corresp="#s4:l1">gaf</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Alle <rhyme label="b" type="m" corresp="#s4:l2">Alleluja</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Laet ons hem aenbidden <rhyme label="a" type="m">mee</rhyme>;</l>
        <l id="s5:l2" met="2" type="-+"><rhyme label="b" type="m">Alleluja</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">Dat wy verrijsen eens in <rhyme label="a" type="m" corresp="#s5:l1">vree</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Alle <rhyme label="b" type="m" corresp="#s5:l2">Alleluja</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
